    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d59b9392-adb2-3883-bdc3-462846a668bf",
                    "sense_definition": "Poruszać się, stawiając kroki; przemieszczać się pieszo z miejsca na miejsce (czasownik niedokonany, oznaczający czynność powtarzalną lub trwającą).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek codziennie rano <chodzi> na długi spacer do parku."
                        },
                        {
                            "text": "Małe dziecko dopiero uczy się <chodzić> i często się przewraca."
                        }
                    ],
                    "synonyms": [
                        "spacerować",
                        "stawiać kroki"
                    ],
                    "antonyms": [
                        "stać"
                    ],
                    "common_phrases": [
                        "chodzić pieszo",
                        "chodzić na palcach",
                        "chodzić spać",
                        "chodzić boso"
                    ]
                },
                {
                    "sense_id": "ae6a1926-c908-3e77-9a35-3cb140b197c1",
                    "sense_definition": "Regularnie udawać się w jakieś miejsce, zwykle w określonym celu, np. do pracy, szkoły lub na zajęcia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "attendance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie moje dzieci <chodzą> do tej samej szkoły podstawowej."
                        },
                        {
                            "text": "Kiedyś regularnie <chodziłem> na basen, ale teraz nie mam czasu."
                        }
                    ],
                    "synonyms": [
                        "uczęszczać",
                        "chadzać"
                    ],
                    "common_phrases": [
                        "chodzić do szkoły",
                        "chodzić do pracy",
                        "chodzić na zajęcia",
                        "chodzić do kościoła"
                    ]
                },
                {
                    "sense_id": "850c713e-dc73-360c-b3bc-dea0de97bb03",
                    "sense_definition": "Dotyczyć czegoś, stanowić istotę problemu lub sytuacji; mieć coś na myśli (używane głównie w konstrukcjach typu „o co chodzi”).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zupełnie nie rozumiem, o co ci teraz <chodzi>."
                        },
                        {
                            "text": "W tej grze <chodzi> o to, żeby zebrać jak najwięcej punktów."
                        }
                    ],
                    "synonyms": [
                        "rozchodzić się"
                    ],
                    "common_phrases": [
                        "o co chodzi",
                        "o to chodzi",
                        "nie chodzi o to"
                    ]
                },
                {
                    "sense_id": "7f6466d5-2faa-321e-a60d-cdcc2c6dceb4",
                    "sense_definition": "O urządzeniach i mechanizmach: być sprawnym, wykonywać pracę; o środkach transportu: kursować według rozkładu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "functioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary zegar na ścianie wciąż <chodzi> bardzo dokładnie."
                        },
                        {
                            "text": "Autobusy do centrum miasta <chodzą> co piętnaście minut."
                        }
                    ],
                    "synonyms": [
                        "działać",
                        "kursować"
                    ],
                    "antonyms": [
                        "stać"
                    ],
                    "common_phrases": [
                        "zegarek chodzi",
                        "silnik chodzi",
                        "winda nie chodzi"
                    ]
                },
                {
                    "sense_id": "c4bb32db-e4a5-3b11-8855-9de263d5e725",
                    "sense_definition": "Nosić na sobie określone ubranie; ubierać się w pewien sposób.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "wearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą zawsze <chodzę> w ciepłej czapce i wełnianym szaliku."
                        },
                        {
                            "text": "Ona bardzo lubi <chodzić> w eleganckich, czarnych sukienkach."
                        }
                    ],
                    "common_phrases": [
                        "chodzić w spodniach",
                        "chodzić na czarno",
                        "chodzić w czymś"
                    ]
                },
                {
                    "sense_id": "77f0fd8c-293d-3faa-be4c-5aa6038d68f8",
                    "sense_definition": "Być z kimś w romantycznej relacji, spotykać się z kimś regularnie jako para.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tomek <chodzi> z Kasią już od trzech lat i planują ślub."
                        },
                        {
                            "text": "Wszyscy w klasie wiedzą, że oni ze sobą <chodzą>."
                        }
                    ],
                    "synonyms": [
                        "spotykać się"
                    ],
                    "common_phrases": [
                        "chodzić z kimś",
                        "chodzić ze sobą"
                    ]
                },
                {
                    "sense_id": "c8d9acdf-3416-3f21-8483-25d60e8b4436",
                    "sense_definition": "Starać się coś załatwić, zabiegać o coś lub opiekować się kimś.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "care_solicit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musiałem długo <chodzić> za tym pozwoleniem w urzędzie."
                        },
                        {
                            "text": "Pielęgniarka <chodziła> koło chorego pacjenta przez całą noc."
                        }
                    ],
                    "synonyms": [
                        "zabiegać",
                        "troszczyć się",
                        "dbać"
                    ],
                    "common_phrases": [
                        "chodzić za pracą",
                        "chodzić koło kogoś",
                        "chodzić za czymś"
                    ]
                }
            ]
        }
    ]

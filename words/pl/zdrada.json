            "pos": "noun",
            "senses": [
                {
                    "sense_id": "552cfa7f-2cad-3f89-903c-1604d057eed2",
                    "sense_definition": "Niedochowanie wierności w związku małżeńskim lub partnerskim, zazwyczaj poprzez kontakty seksualne z inną osobą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "betrayal_trust",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej bolesna <zdrada> doprowadziła do szybkiego rozwodu.",
                            "target_lang_translations": {
                                "ru": "Её болезненная <измена> привела к быстрому разводу."
                            }
                        },
                        {
                            "text": "Mimo szczerych chęci, nie potrafił wybaczyć żonie <zdrady>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на искреннее желание, он не смог простить жене <измены>."
                            }
                        }
                    ],
                    "synonyms": [
                        "niewierność",
                        "cudzołóstwo",
                        "skok w bok",
                        "nielojalność"
                    ],
                    "antonyms": [
                        "wierność",
                        "wierność małżeńska"
                    ],
                    "common_phrases": [
                        "zdrada małżeńska",
                        "dopuścić się zdrady",
                        "wybaczyć zdradę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарушение верности в браке или отношениях, обычно через сексуальную связь с другим человеком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изме́на",
                                "target_lang_sense_clarification": "супружеская"
                            },
                            {
                                "target_lang_word": "неве́рность",
                                "target_lang_sense_clarification": "в отношениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82c5ca5b-ad29-3709-95e7-229b7739f884",
                    "sense_definition": "Działanie na szkodę własnego narodu lub państwa, często we współpracy z wrogiem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "betrayal_trust",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za <zdradę> stanu grozi w tym kraju dożywocie.",
                            "target_lang_translations": {
                                "ru": "За государственную <измену> в этой стране грозит пожизненное заключение."
                            }
                        },
                        {
                            "text": "Oficer został oskarżony o <zdradę> i współpracę z obcym wywiadem.",
                            "target_lang_translations": {
                                "ru": "Офицер был обвинен в <предательстве> и сотрудничестве с иностранной разведкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeniewierstwo",
                        "wiarołomstwo",
                        "nielojalność"
                    ],
                    "antonyms": [
                        "patriotyzm",
                        "wierność"
                    ],
                    "common_phrases": [
                        "zdrada stanu",
                        "zdrada ojczyzny",
                        "wysoka zdrada"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действия, наносящие ущерб собственному народу или государству, часто в сотрудничестве с врагом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изме́на",

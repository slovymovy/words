    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d9381aea-6a69-38d7-aa74-403eae56bee8",
                    "sense_definition": "Оценка ситуации или действий как соответствующих обстановке; о том, что сделано вовремя, кстати и подходит к данному случаю.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В начале собрания будет <w>уместно</w> представить новых сотрудников.",
                            "target_lang_translations": {
                                "en": "At the beginning of the meeting, it will be <w>appropriate</w> to introduce the new employees."
                            }
                        },
                        {
                            "text": "Здесь <w>уместно</w> вспомнить одну старую пословицу.",
                            "target_lang_translations": {
                                "en": "Here it is <w>fitting</w> to recall an old proverb."
                            }
                        }
                    ],
                    "synonyms": [
                        "кстати",
                        "стоит",
                        "следует"
                    ],
                    "antonyms": [
                        "неуместно",
                        "некстати"
                    ],
                    "common_phrases": [
                        "здесь уместно",
                        "будет уместно",
                        "вполне уместно"
                    ],
                    "target_lang_definitions": {
                        "en": "An assessment of a situation or action as suitable for the circumstances; referring to something done at the right time and fitting the occasion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appropriate",
                                "target_lang_sense_clarification": "suitable or proper in the circumstances"
                            },
                            {
                                "target_lang_word": "fitting",
                                "target_lang_sense_clarification": "right for the occasion"
                            },
                            {
                                "target_lang_word": "in place",
                                "target_lang_sense_clarification": "suitable or correct for the situation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c84006f-0a65-3db0-8e26-51166eb0b400",
                    "sense_definition": "Соответствующим обстановке образом; так, как полагается или ожидается в данной ситуации.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его шутка прозвучала очень <w>уместно</w>, и все рассмеялись.",
                            "target_lang_translations": {
                                "en": "His joke sounded very <w>appropriate</w>, and everyone laughed."
                            }
                        },
                        {
                            "text": "Она всегда одевается <w>уместно</w> и со вкусом.",
                            "target_lang_translations": {
                                "en": "She always dresses <w>appropriately</w> and with taste."
                            }
                        }
                    ],
                    "synonyms": [
                        "кстати",
                        "к месту",
                        "впору"
                    ],
                    "antonyms": [
                        "неуместно",
                        "некстати"
                    ],
                    "common_phrases": [
                        "звучать уместно",
                        "выглядеть уместно",
                        "поступить уместно"
                    ],
                    "target_lang_definitions": {
                        "en": "In a manner corresponding to the circumstances; as is proper or expected in a given situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appropriately",
                                "target_lang_sense_clarification": "in a suitable manner"
                            },
                            {
                                "target_lang_word": "fittingly",
                                "target_lang_sense_clarification": "in a way that is suitable or right"
                            },
                            {
                                "target_lang_word": "aptly",
                                "target_lang_sense_clarification": "in a manner that is appropriate or suitable in the circumstances"
                            }
                        ]
                    }
                }
            ]
        },
        {

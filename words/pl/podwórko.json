            "senses": [
                {
                    "sense_id": "a46a11b5-9e09-3da4-bd12-af7b6363dbc7",
                    "sense_definition": "Przestrzeń przydomowa, zwykle ogrodzona lub otoczona zabudowaniami, służąca do rekreacji lub celów gospodarczych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci przez cały dzień bawiły się na <podwórku>.",
                            "target_lang_translations": {
                                "ru": "Дети весь день играли во <w>дворе</w>."
                            }
                        },
                        {
                            "text": "Z okna kuchni mam widok na małe <podwórko>.",
                            "target_lang_translations": {
                                "ru": "Из окна кухни у меня вид на маленький <w>двор</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "podwórze",
                        "dziedziniec",
                        "obejście"
                    ],
                    "common_phrases": [
                        "bawić się na podwórku",
                        "wyjść na podwórko",
                        "szkolne podwórko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придомовая территория, обычно огороженная или окруженная зданиями, служащая для отдыха или хозяйственных целей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двор",
                                "target_lang_sense_clarification": "пространство возле дома"
                            },
                            {
                                "target_lang_word": "дворик",
                                "target_lang_sense_clarification": "небольшой двор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ae2c4c9-bb45-38fb-ae18-10bf4b7d2081",
                    "sense_definition": "Najbliższe otoczenie, własne środowisko oraz sprawy i problemy, które kogoś bezpośrednio dotyczą (znaczenie przenośne).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zanim zaczniesz krytykować innych, zrób porządek na własnym <podwórku>.",
                            "target_lang_translations": {
                                "ru": "Прежде чем критиковать других, наведи порядок в собственном <w>огороде</w>."
                            }
                        },
                        {
                            "text": "To są polityczne spory z naszego lokalnego podwórka.",
                            "target_lang_translations": {
                                "ru": "Это политические споры нашего местного <w>двора</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "na własnym podwórku",
                        "z własnego podwórka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ближайшее окружение, собственная среда, а также дела и проблемы, касающиеся кого-либо напрямую (переносное значение)."
                    },
                    "translations": {
                        "ru": [

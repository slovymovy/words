    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "563ad5f4-7faa-3bae-9299-a3d45fcd320c",
                    "sense_definition": "Behorend tot, afkomstig van of lijkend op de duivel; slecht of kwaadaardig.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "evil_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koning bedacht een <w>duivels</w> plan om de macht in het land te grijpen.",
                            "target_lang_translations": {
                                "ru": "Король придумал <w>дьявольский</w> план, чтобы захватить власть в стране.",
                                "en": "The king devised a <w>diabolical</w> plan to seize power in the country."
                            }
                        },
                        {
                            "text": "In het sprookje had de oude heks een angstaanjagende en <w>duivelse</w> lach.",
                            "target_lang_translations": {
                                "ru": "В сказке у старой ведьмы был пугающий и <w>дьявольский</w> смех.",
                                "en": "In the fairy tale, the old witch had a terrifying and <w>devilish</w> laugh."
                            }
                        }
                    ],
                    "synonyms": [
                        "demonisch",
                        "satanisch",
                        "kwaadaardig",
                        "hels"
                    ],
                    "antonyms": [
                        "hemels",
                        "goddelijk",
                        "engelachtig"
                    ],
                    "common_phrases": [
                        "een duivels plan",
                        "duivelse krachten",
                        "duivelse praktijken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к дьяволу, исходящий от него или похожий на него; злой или злобный.",
                        "en": "Belonging to, coming from, or resembling the devil; evil or malicious."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дьявольский",
                                "target_lang_sense_clarification": "относящийся к дьяволу или злу"
                            },
                            {
                                "target_lang_word": "бесовский",
                                "target_lang_sense_clarification": "свойственный бесам"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "devilish",
                                "target_lang_sense_clarification": "resembling or characteristic of a devil"
                            },
                            {
                                "target_lang_word": "diabolical",
                                "target_lang_sense_clarification": "disgracefully bad or unpleasant"
                            },
                            {
                                "target_lang_word": "demonic",
                                "target_lang_sense_clarification": "relating to evil spirits"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae450aa5-a549-3443-ae9c-f7a43e4117d4",
                    "sense_definition": "Heel erg boos, woedend of gefrustreerd.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "angry_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vader werd <w>duivels</w> toen hij zag dat ik een kras op zijn auto had gemaakt.",
                            "target_lang_translations": {
                                "ru": "Мой отец был <w>взбешён</w>, когда увидел, что я поцарапал его машину.",
                                "en": "My father became <w>furious</w> when he saw that I had scratched his car."
                            }
                        },
                        {
                            "text": "Je wordt er soms echt <w>duivels</w> van als de computer steeds vastloopt.",
                            "target_lang_translations": {
                                "ru": "Иногда становишься просто <w>разъярённым</w>, когда компьютер постоянно зависает.",
                                "en": "You sometimes get really <w>mad</w> when the computer keeps freezing."
                            }
                        }
                    ],
                    "synonyms": [
                        "woedend",
                        "razend",
                        "kwaad",
                        "furieus"
                    ],
                    "antonyms": [
                        "kalm",
                        "rustig",
                        "bedaard"
                    ],
                    "common_phrases": [
                        "iemand duivels maken",
                        "duivels worden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень злой, разъярённый или разочарованный.",
                        "en": "Very angry, furious, or frustrated."

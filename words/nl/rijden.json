                        "ru": "Движение самого транспортного средства (поезда, автобуса, машины)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "trains/buses"
                            },
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "general vehicles"
                            }
                        ],
                        "ru": [
                            {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "41a955cc-829e-3464-966d-8a08e64eedfd",
                    "sense_definition": "The process or time of gathering ripened crops from the fields.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_agri_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The constant rain made the <w>harvest</w> a nightmare this year.",
                            "target_lang_translations": {
                                "ru": "Из-за постоянных дождей <w>уборка урожая</w> в этом году превратилась в кошмар."
                            }
                        },
                        {
                            "text": "Farmers work from sunrise to sunset during the <w>harvest</w>.",
                            "target_lang_translations": {
                                "ru": "Во время <w>жатвы</w> фермеры работают от рассвета до заката."
                            }
                        }
                    ],
                    "common_phrases": [
                        "harvest time",
                        "harvest festival",
                        "during the harvest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс или время сбора созревшего урожая с полей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уборка урожая",
                                "target_lang_sense_clarification": "процесс сбора сельскохозяйственных культур"
                            },
                            {
                                "target_lang_word": "жатва",
                                "target_lang_sense_clarification": "сбор зерновых; время сбора урожая"
                            },
                            {
                                "target_lang_word": "сбор урожая",
                                "target_lang_sense_clarification": "общее название процесса"
                            },
                            {
                                "target_lang_word": "страда",
                                "target_lang_sense_clarification": "напряженная работа в сезон сбора урожая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "827b5156-b352-3667-ad79-9066ab169e74",
                    "sense_definition": "The amount of crops or fruit gathered in a season.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_agri_yield",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This year's cotton <w>harvest</w> was great compared to the last one.",
                            "target_lang_translations": {
                                "ru": "<w>Урожай</w> хлопка в этом году был отличным по сравнению с прошлым."
                            }
                        },
                        {
                            "text": "We are expecting a bumper <w>harvest</w> thanks to the good weather.",
                            "target_lang_translations": {
                                "ru": "Благодаря хорошей погоде мы ожидаем небывалый <w>урожай</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "crop",
                        "gather",
                        "fall"
                    ],
                    "common_phrases": [
                        "bumper harvest",
                        "good harvest",
                        "poor harvest",
                        "grain harvest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество сельскохозяйственных культур или плодов, собранных за сезон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "урожай",
                                "target_lang_sense_clarification": "собранное количество продукции"
                            },
                            {
                                "target_lang_word": "сбор",
                                "target_lang_sense_clarification": "результат сбора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e98a29d-92fa-3886-871a-48b7be306c94",
                    "sense_definition": "The result, consequence, or reward of an action or effort.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The surveillance mission yielded a healthy <w>harvest</w> of intel.",
                            "target_lang_translations": {
                                "ru": "Разведывательная миссия принесла богатый <w>урожай</w> разведданных."
                            }
                        },
                        {
                            "text": "His long career produced a rich <w>harvest</w> of awards and accolades.",
                            "target_lang_translations": {
                                "ru": "Его долгая карьера принесла богатые <w>плоды</w> в виде наград и премий."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Результат, последствие или награда за действие или усилие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плоды",
                                "target_lang_sense_clarification": "перен. результат усилий"
                            },
                            {
                                "target_lang_word": "урожай",
                                "target_lang_sense_clarification": "перен. большое количество чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb175e12-84b2-3a58-9668-31cb315fb12c",
                    "sense_definition": "The season of autumn.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_season",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Harvest</w> is usually very damp and rainy in this region.",
                            "target_lang_translations": {
                                "ru": "<w>Осень</w> в этом регионе обычно очень сырая и дождливая."
                            }
                        },
                        {
                            "text": "The leaves turn brown and gold when <w>harvest</w> arrives.",
                            "target_lang_translations": {
                                "ru": "Листья становятся коричневыми и золотыми, когда наступает <w>осень</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "autumn",
                        "fall"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "standard English uses autumn or fall"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "UK"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Осенний сезон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осень",
                                "target_lang_sense_clarification": "время года"
                            },
                            {
                                "target_lang_word": "пора жатвы",
                                "target_lang_sense_clarification": "время сбора урожая"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2c7dae4f-302b-39e9-a435-dcdfced581af",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c25a28ad-5bfc-32cd-abb7-69e75cc02c8e",
                    "sense_definition": "Een gevoel van lichamelijke of geestelijke uitputting en gebrek aan energie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de lange reis overviel hem een zware <w>vermoeidheid</w>.",
                            "target_lang_translations": {
                                "ru": "После долгого путешествия на него навалилась сильная <w>усталость</w>.",
                                "en": "After the long journey, a heavy <w>fatigue</w> overcame him."
                            }
                        },
                        {
                            "text": "Zij maakte fouten op haar werk door pure <w>vermoeidheid</w>.",
                            "target_lang_translations": {
                                "ru": "Она делала ошибки на работе исключительно из-за <w>усталости</w>.",
                                "en": "She made mistakes at work due to sheer <w>tiredness</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "moeheid",
                        "vermoeienis"
                    ],
                    "antonyms": [
                        "fitheid",
                        "energie"
                    ],
                    "common_phrases": [
                        "chronische vermoeidheid",
                        "tegen de vermoeidheid vechten",
                        "van vermoeidheid omvallen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство физического или умственного истощения и недостатка энергии.",
                        "en": "A feeling of physical or mental exhaustion and lack of energy."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усталость",
                                "target_lang_sense_clarification": "физическая или умственная"
                            },
                            {
                                "target_lang_word": "утомление",
                                "target_lang_sense_clarification": "состояние организма"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fatigue",
                                "target_lang_sense_clarification": "physical or mental exhaustion"
                            },
                            {
                                "target_lang_word": "tiredness",
                                "target_lang_sense_clarification": "general state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b881ddbb-193d-33b4-828d-810e42246d96",
                    "sense_definition": "Het zwakker worden van materiaal, zoals metaal, door herhaalde of wisselende belasting.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material_science",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het vliegtuigonderdeel brak af door <w>vermoeidheid</w> van het metaal.",
                            "target_lang_translations": {
                                "ru": "Деталь самолета сломалась из-за <w>усталости</w> металла.",
                                "en": "The airplane part broke off due to <w>fatigue</w> of the metal."
                            }
                        },
                        {
                            "text": "Ingenieurs controleren de constructie regelmatig op scheurtjes door <w>vermoeidheid</w>.",
                            "target_lang_translations": {
                                "ru": "Инженеры регулярно проверяют конструкцию на наличие трещин, вызванных <w>усталостью</w>.",
                                "en": "Engineers regularly check the structure for cracks caused by <w>fatigue</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "metaalmoeheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ослабление материала, например металла, из-за повторяющихся или переменных нагрузок.",
                        "en": "The weakening of material, such as metal, due to repeated or varying loads."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усталость",
                                "target_lang_sense_clarification": "материала"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fatigue",
                                "target_lang_sense_clarification": "of materials"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "ff0e3e24-8c41-3794-b7c5-dee9e53a67e3",
                    "sense_definition": "Het woord tot iemand richten om een gesprek te beginnen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik durfde de onbekende man op straat niet <w>aan</w> te <w>spreken</w>.",
                            "target_lang_translations": {
                                "ru": "Я не осмелился <w>заговорить</w> с незнакомым мужчиной на улице.",
                                "en": "I didn't dare to <w>speak to</w> the stranger on the street."
                            }
                        },
                        {
                            "text": "De docent <w>sprak</w> de nieuwe studenten vriendelijk <w>aan</w> in de gang.",
                            "target_lang_translations": {
                                "ru": "Преподаватель дружелюбно <w>обратился</w> к новым студентам в коридоре.",
                                "en": "The teacher kindly <w>addressed</w> the new students in the hallway."
                            }
                        }
                    ],
                    "synonyms": [
                        "toespreken",
                        "benaderen"
                    ],
                    "antonyms": [
                        "negeren"
                    ],
                    "common_phrases": [
                        "iemand aanspreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обратиться к кому-либо, чтобы начать разговор.",
                        "en": "To direct speech towards someone to start a conversation."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обращаться",
                                "target_lang_sense_clarification": "начинать разговор с кем-либо"
                            },
                            {
                                "target_lang_word": "заговорить",
                                "target_lang_sense_clarification": "вступить в разговор"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "to speak to",
                                "target_lang_sense_clarification": "to initiate a conversation with someone"
                            },
                            {
                                "target_lang_word": "to address",
                                "target_lang_sense_clarification": "formal way of speaking to someone"
                            },
                            {
                                "target_lang_word": "to approach",
                                "target_lang_sense_clarification": "to go up to someone to speak"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "520488bd-9517-3915-a79a-f60feabe96b2",
                    "sense_definition": "Een positief gevoel oproepen; leuk, mooi of interessant gevonden worden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit moderne schilderij <w>spreekt</w> mij totaal niet <w>aan</w>.",
                            "target_lang_translations": {
                                "ru": "Эта современная картина меня совершенно не <w>привлекает</w>.",
                                "en": "This modern painting does not <w>appeal</w> to me at all."
                            }
                        },
                        {
                            "text": "Zijn ideeën over duurzaamheid <w>spraken</w> het publiek erg <w>aan</w>.",
                            "target_lang_translations": {
                                "ru": "Его идеи об устойчивом развитии очень <w>понравились</w> публике.",
                                "en": "His ideas about sustainability really <w>appealed</w> to the audience."
                            }
                        }
                    ],
                    "synonyms": [
                        "bevallen",
                        "aanstaan",
                        "interesseren",
                        "boeien"
                    ],
                    "antonyms": [
                        "tegenstaan",
                        "vervelen"
                    ],
                    "common_phrases": [
                        "erg aanspreken",
                        "niet aanspreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать положительные эмоции; нравиться или казаться интересным.",
                        "en": "To evoke a positive feeling; to be liked or found interesting."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нравиться",
                                "target_lang_sense_clarification": "быть по душе"
                            },
                            {
                                "target_lang_word": "привлекать",
                                "target_lang_sense_clarification": "вызывать интерес"
                            },
                            {
                                "target_lang_word": "импонировать",
                                "target_lang_sense_clarification": "вызывать симпатию или уважение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "to appeal to",
                                "target_lang_sense_clarification": "to be attractive or interesting to someone"
                            },
                            {
                                "target_lang_word": "to attract",
                                "target_lang_sense_clarification": "to draw interest or liking"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c7d8c39-d113-397a-b10b-cbfe7b43115c",
                    "sense_definition": "Iemand vragen om uitleg of verantwoording af te leggen over zijn of haar gedrag.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politieagent <w>sprak</w> de jongeren <w>aan</w> op hun luidruchtige gedrag.",
                            "target_lang_translations": {
                                "ru": "Полицейский <w>сделал замечание</w> молодым людям по поводу их шумного поведения.",
                                "en": "The police officer <w>confronted</w> the youths about their noisy behavior."
                            }
                        },
                        {
                            "text": "Je mag mij er altijd op <w>aanspreken</w> als ik een afspraak vergeet.",
                            "target_lang_translations": {
                                "ru": "Ты всегда можешь <w>указать</w> мне на это, если я забуду о встрече.",
                                "en": "You can always <w>call</w> me <w>out</w> on it if I forget an appointment."
                            }
                        }
                    ],
                    "synonyms": [
                        "berispen",
                        "confronteren"
                    ],
                    "common_phrases": [
                        "aanspreken op gedrag",
                        "iemand ergens op aanspreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потребовать объяснений или призвать к ответственности за поведение.",
                        "en": "To ask someone for an explanation or justification regarding their behavior."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делать замечание",
                                "target_lang_sense_clarification": "указывать на неправильное поведение"
                            },
                            {
                                "target_lang_word": "призывать к ответу",
                                "target_lang_sense_clarification": "требовать объяснений"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "to call to account",
                                "target_lang_sense_clarification": "to demand an explanation for behavior"
                            },
                            {
                                "target_lang_word": "to confront",
                                "target_lang_sense_clarification": "to face someone about their actions"
                            },
                            {
                                "target_lang_word": "to hold accountable",
                                "target_lang_sense_clarification": "to make someone responsible for their actions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6486c9c4-462d-3a15-b7ac-b0dd4f345846",
                    "sense_definition": "Beginnen met het gebruik van een voorraad, reserve of bron.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Omdat de wasmachine kapot ging, moesten we ons spaargeld <w>aanspreken</w>.",
                            "target_lang_translations": {
                                "ru": "Так как стиральная машина сломалась, нам пришлось <w>воспользоваться</w> нашими сбережениями.",
                                "en": "Because the washing machine broke down, we had to <w>dip into</w> our savings."
                            }
                        },
                        {
                            "text": "De wielrenner moest in de laatste kilometers al zijn krachten <w>aanspreken</w>.",
                            "target_lang_translations": {
                                "ru": "Велогонщику пришлось <w>задействовать</w> все свои силы на последних километрах.",
                                "en": "The cyclist had to <w>draw on</w> all his strength in the final kilometers."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanbreken",
                        "benutten",
                        "gebruiken"
                    ],
                    "common_phrases": [
                        "een voorraad aanspreken",
                        "reserves aanspreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать использовать запасы, резервы или ресурсы.",
                        "en": "To start using a supply, reserve, or source."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задействовать",
                                "target_lang_sense_clarification": "пустить в ход ресурсы или силы"
                            },
                            {
                                "target_lang_word": "воспользоваться",
                                "target_lang_sense_clarification": "использовать имеющееся"
                            },
                            {
                                "target_lang_word": "прибегнуть к",
                                "target_lang_sense_clarification": "обратиться к средству или запасу"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "to draw on",
                                "target_lang_sense_clarification": "to utilize a resource or reserve"
                            },
                            {
                                "target_lang_word": "to dip into",
                                "target_lang_sense_clarification": "to use part of a supply (like savings)"
                            },
                            {
                                "target_lang_word": "to tap into",
                                "target_lang_sense_clarification": "to gain access to a resource"
                            }
                        ]
                    }
                }
            ]
        }

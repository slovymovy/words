                {
                    "sense_id": "0d6b6a92-be42-3b1e-9439-d9e329da5449",
                    "sense_definition": "dotyczący węgla (kopaliny), służący do jego wydobycia, transportu lub wykorzystujący go jako paliwo",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary piec <w>węglowy</w> został w końcu wymieniony na ogrzewanie gazowe.",
                            "target_lang_translations": {
                                "ru": "Старая <w>угольная</w> печь была наконец заменена на газовое отопление."
                            }
                        },
                        {
                            "text": "Do portu wpłynął duży statek <w>węglowy</w> z ładunkiem z zagranicy.",
                            "target_lang_translations": {
                                "ru": "В порт вошло большое <w>угольное</w> судно с грузом из-за границы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piec węglowy",
                        "pył węglowy",
                        "skład węglowy",
                        "kopalnia węglowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к углю (ископаемому), служащий для его добычи, транспортировки или использующий его как топливо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угольный",
                                "target_lang_sense_clarification": "относящийся к ископаемому углю"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08a8e8fc-d51b-36f5-9d44-e8384adf27ac",
                    "sense_definition": "dotyczący węgla jako pierwiastka chemicznego lub materiału z niego wytworzonego",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Włókna <w>węglowe</w> są bardzo lekkie i niezwykle wytrzymałe.",
                            "target_lang_translations": {
                                "ru": "<w>Углеродные</w> волокна очень легкие и чрезвычайно прочные."
                            }
                        },
                        {
                            "text": "Filtr <w>węglowy</w> w masce przeciwgazowej chroni przed szkodliwymi oparami.",
                            "target_lang_translations": {
                                "ru": "<w>Угольный</w> фильтр в противогазе защищает от вредных испарений."
                            }
                        }
                    ],
                    "common_phrases": [
                        "włókno węglowe",
                        "filtr węglowy",
                        "ślad węglowy",
                        "cykl węglowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к углероду как химическому элементу или материалу, изготовленному из него"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "углеродный",
                                "target_lang_sense_clarification": "относящийся к химическому элементу"
                            },
                            {
                                "target_lang_word": "угольный",
                                "target_lang_sense_clarification": "сделанный из угля (например, фильтр)"

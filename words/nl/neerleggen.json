                {
                    "sense_id": "853124f4-e7a6-3ca4-a979-6abcfa34a749",
                    "sense_definition": "Een officiële functie, taak of beroep beëindigen of opgeven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "work_status",
                    "name_type": "no",
                    "examples": [

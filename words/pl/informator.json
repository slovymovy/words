    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3ee22f0b-9cae-3880-b657-8431f4487e17",
                    "sense_definition": "Publikacja w formie książki, broszury lub ulotki, która zawiera zbiór praktycznych informacji z określonej dziedziny.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "publikacja",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem aktualny <w>informator</w> turystyczny, aby znaleźć ciekawe miejsca w mieście.",
                            "target_lang_translations": {
                                "ru": "Я купил актуальный туристический <w>справочник</w>, чтобы найти интересные места в городе."
                            }
                        },
                        {
                            "text": "W tym <w>informatorze</w> znajdziesz adresy wszystkich ważnych urzędów i instytucji.",
                            "target_lang_translations": {
                                "ru": "В этом <w>справочнике</w> ты найдешь адреса всех важных ведомств и учреждений."
                            }
                        }
                    ],
                    "synonyms": [
                        "przewodnik",
                        "broszura"
                    ],
                    "common_phrases": [
                        "informator turystyczny",
                        "informator miejski",
                        "informator handlowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публикация в форме книги, брошюры или листовки, содержащая сборник практической информации из определенной области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "справочник",
                                "target_lang_sense_clarification": "книга со сведениями"
                            },
                            {
                                "target_lang_word": "путеводитель",
                                "target_lang_sense_clarification": "туристический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4562d2f2-cab2-3e89-b247-d0b9de948ae6",
                    "sense_definition": "Osoba, której zadaniem zawodowym jest udzielanie informacji klientom lub interesantom.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "osoba_informujaca",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli nie wiesz, gdzie iść, zapytaj <w>informatora</w> przy głównym wejściu.",
                            "target_lang_translations": {
                                "ru": "Если не знаешь, куда идти, спроси у <w>сотрудника справочной</w> у главного входа."
                            }
                        },
                        {
                            "text": "Jako <w>informator</w> na dworcu, muszę znać rozkład jazdy pociągów na pamięć.",
                            "target_lang_translations": {
                                "ru": "Как <w>сотрудник справочной</w> на вокзале, я должен знать расписание поездов наизусть."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, в чьи профессиональные обязанности входит предоставление информации клиентам или посетителям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сотрудник справочной",
                                "target_lang_sense_clarification": "работник бюро информации"
                            },
                            {
                                "target_lang_word": "информатор",
                                "target_lang_sense_clarification": "должность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c6e3a35-ec83-3a59-a1e6-7cb53d568172",
                    "sense_definition": "Osoba, która potajemnie dostarcza poufne informacje, zazwyczaj policji, służbom specjalnym lub prasie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "osoba_informujaca",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policyjny <w>informator</w> pomógł detektywom rozbić groźny gang.",
                            "target_lang_translations": {
                                "ru": "Полицейский <w>информатор</w> помог детективам разбить опасную банду."
                            }
                        },
                        {
                            "text": "Dziennikarz spotkał się ze swoim <w>informatorem</w> w ciemnej uliczce.",
                            "target_lang_translations": {
                                "ru": "Журналист встретился со своим <w>информатором</w> в темном переулке."
                            }
                        }
                    ],
                    "synonyms": [
                        "donosiciel",
                        "wtyczka",
                        "agent"
                    ],
                    "common_phrases": [
                        "policyjny informator",
                        "tajny informator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, тайно предоставляющее конфиденциальную информацию, обычно полиции, спецслужбам или прессе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "информатор",
                                "target_lang_sense_clarification": "источник информации"
                            },
                            {
                                "target_lang_word": "осведомитель",
                                "target_lang_sense_clarification": "полицейский"
                            }
                        ]
                    }
                }
            ]
        }
    ]

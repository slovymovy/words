                            },
                            {
                                "target_lang_word": "инертность",
                                "target_lang_sense_clarification": "пассивность, отсутствие активности"
                            },
                            {
                                "target_lang_word": "косность",
                                "target_lang_sense_clarification": "застой в мышлении"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "inflexibility",

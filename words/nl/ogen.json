        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "25d5abb0-4eb6-38d6-8cc8-0ef85c6df10c",
                    "sense_definition": "Er op een bepaalde manier uitzien; een specifieke indruk wekken door het uiterlijk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De woonkamer <w>oogt</w> veel groter door de lichte kleuren op de muur.",
                            "target_lang_translations": {
                                "en": "The living room <w>looks</w> much bigger because of the light colors on the wall.",
                                "ru": "Гостиная <w>выглядит</w> намного больше благодаря светлым тонам на стене."
                            }
                        },
                        {
                            "text": "Hoewel het gebouw oud is, <w>oogt</w> het nog steeds heel modern.",
                            "target_lang_translations": {
                                "en": "Although the building is old, it still <w>looks</w> very modern.",
                                "ru": "Хотя здание старое, оно всё ещё <w>выглядит</w> очень современно."
                            }
                        }
                    ],
                    "synonyms": [
                        "eruitzien",
                        "lijken",
                        "overkomen"
                    ],
                    "common_phrases": [
                        "jonger ogen",
                        "fris ogen",
                        "ruim ogen",
                        "professioneel ogen"
                    ],
                    "target_lang_definitions": {
                        "en": "To look a certain way; to create a specific impression through appearance.",
                        "ru": "Выглядеть определенным образом; производить впечатление своим внешним видом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look",
                                "target_lang_sense_clarification": "to appear or seem"
                            },
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "to seem"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выглядеть",
                                "target_lang_sense_clarification": "иметь определенный вид"
                            },
                            {
                                "target_lang_word": "смотреться",
                                "target_lang_sense_clarification": "производить зрительное впечатление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e69daa1-1e27-381f-b342-c3a590215e97",
                    "sense_definition": "Aandachtig en gericht naar iets kijken of ergens op mikken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schutter <w>oogde</w> zorgvuldig op het doelwit in de verte.",
                            "target_lang_translations": {
                                "en": "The marksman <w>aimed</w> carefully at the target in the distance.",
                                "ru": "Стрелок тщательно <w>целился</w> в мишень вдалеке."
                            }
                        },
                        {
                            "text": "Hij <w>oogde</w> op een hoge positie binnen het bedrijf.",
                            "target_lang_translations": {
                                "en": "He <w>had his eye</w> on a high position within the company.",
                                "ru": "Он <w>метил</w> на высокую должность в компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "turen",
                        "mikken",
                        "staren"
                    ],
                    "target_lang_definitions": {
                        "en": "To look attentively at something or to aim for something.",
                        "ru": "Пристально смотреть или нацеливаться на что-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "aim",
                                "target_lang_sense_clarification": "to direct intent or action towards"
                            },
                            {
                                "target_lang_word": "have one's eye on",
                                "target_lang_sense_clarification": "to desire or intend to get"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "целиться",
                                "target_lang_sense_clarification": "наводить оружие или взгляд"
                            },
                            {
                                "target_lang_word": "метить",
                                "target_lang_sense_clarification": "претендовать на что-то (переносное)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

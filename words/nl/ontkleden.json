    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6f622263-4ed1-3c0b-ba4e-166a87ced7e8",
                    "sense_definition": "Zich ontdoen van kleding; de kleren uittrekken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_undress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De patiënt moest zich in de kleedkamer <w>ontkleden</w> voor het onderzoek.",
                            "target_lang_translations": {
                                "en": "The patient had to <w>undress</w> in the changing room for the examination.",
                                "ru": "Пациенту пришлось <w>раздеться</w> в раздевалке перед осмотром."
                            }
                        },
                        {
                            "text": "Hij <w>ontkleedde</w> zich snel en sprong in het koude water.",
                            "target_lang_translations": {
                                "en": "He <w>undressed</w> quickly and jumped into the cold water.",
                                "ru": "Он быстро <w>разделся</w> и прыгнул в холодную воду."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitkleden"
                    ],
                    "antonyms": [
                        "aankleden"
                    ],
                    "common_phrases": [
                        "zich volledig ontkleden",
                        "zich snel ontkleden"
                    ],
                    "target_lang_definitions": {
                        "en": "To remove one's clothes.",
                        "ru": "Снимать с себя одежду; раздеваться."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "undress",
                                "target_lang_sense_clarification": "to take off one's clothes"
                            },
                            {
                                "target_lang_word": "get undressed",
                                "target_lang_sense_clarification": "common phrase for taking off clothes"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "раздеваться",
                                "target_lang_sense_clarification": "снимать с себя одежду"
                            },
                            {
                                "target_lang_word": "снимать одежду",
                                "target_lang_sense_clarification": "фраза, означающая процесс раздевания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b68a43cd-b30d-3522-b3f4-3408557ee870",
                    "sense_definition": "Iemand anders van kleding ontdoen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_undress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verpleegkundige hielp de oudere man te <w>ontkleden</w>.",
                            "target_lang_translations": {
                                "en": "The nurse helped <w>undress</w> the elderly man.",
                                "ru": "Медсестра помогла <w>раздеть</w> пожилого мужчину."
                            }
                        },
                        {
                            "text": "De moeder <w>ontkleedde</w> haar baby voorzichtig voor het badje.",
                            "target_lang_translations": {
                                "en": "The mother carefully <w>undressed</w> her baby for the bath.",
                                "ru": "Мать осторожно <w>раздела</w> ребенка перед купанием."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitkleden"
                    ],
                    "antonyms": [
                        "aankleden"
                    ],
                    "common_phrases": [
                        "iemand helpen ontkleden"
                    ],
                    "target_lang_definitions": {
                        "en": "To remove someone else's clothes.",
                        "ru": "Снимать одежду с кого-то другого."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "undress",
                                "target_lang_sense_clarification": "to take clothes off someone else"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "раздевать",
                                "target_lang_sense_clarification": "снимать одежду с другого человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b071fc14-0b6c-32aa-88fb-b129ea5991ff",
                    "sense_definition": "Iets ontdoen van bedekking, opsmuk of franje; de essentie of waarheid blootleggen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_reveal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De journalist probeerde de feiten van alle geruchten te <w>ontkleden</w>.",
                            "target_lang_translations": {
                                "en": "The journalist tried to <w>strip</w> the facts of all rumors.",
                                "ru": "Журналист пытался <w>очистить</w> факты от всех слухов."
                            }
                        },
                        {
                            "text": "In zijn toespraak <w>ontkleedde</w> hij het voorstel van alle retoriek.",
                            "target_lang_translations": {
                                "en": "In his speech, he <w>stripped</w> the proposal of all rhetoric.",
                                "ru": "В своей речи он <w>очистил</w> предложение от всякой риторики."
                            }
                        }
                    ],
                    "synonyms": [
                        "onthullen",
                        "onthullen"
                    ],
                    "common_phrases": [
                        "van franje ontkleden",
                        "de waarheid ontkleden"
                    ],
                    "target_lang_definitions": {
                        "en": "To strip something of covering or embellishment; to expose the essence.",
                        "ru": "Лишать что-либо покровов, украшений или лишнего; обнажать суть."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strip",
                                "target_lang_sense_clarification": "to remove covering or non-essentials"
                            },
                            {
                                "target_lang_word": "bare",
                                "target_lang_sense_clarification": "to uncover or reveal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "очищать",
                                "target_lang_sense_clarification": "освобождать от наслоений или лишнего"
                            },
                            {
                                "target_lang_word": "лишать",
                                "target_lang_sense_clarification": "отнимать что-либо, оставлять без чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c2cf526-fbd0-3001-8c21-c85f07268037",
                    "sense_definition": "Zich ontdoen of bevrijden van iets abstracts, zoals een recht, eigenschap of vooroordeel.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "figurative_rid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een rechter moet zich van elke vorm van partijdigheid <w>ontkleden</w>.",
                            "target_lang_translations": {
                                "en": "A judge must <w>divest</w> himself of any form of partiality.",
                                "ru": "Судья должен <w>отрешиться</w> от любой формы предвзятости."
                            }
                        },
                        {
                            "text": "Hij probeerde zich van zijn verantwoordelijkheden te <w>ontkleden</w>.",
                            "target_lang_translations": {
                                "en": "He tried to <w>divest</w> himself of his responsibilities.",
                                "ru": "Он пытался <w>освободиться</w> от своих обязанностей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich van vooroordelen ontkleden"
                    ],
                    "target_lang_definitions": {
                        "en": "To free oneself from something abstract, such as a right or prejudice.",
                        "ru": "Освобождаться или избавляться от чего-либо абстрактного (прав, предубеждений)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "divest",
                                "target_lang_sense_clarification": "to rid oneself of something abstract (formal)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отрешаться",
                                "target_lang_sense_clarification": "отказываться или отвлекаться от чего-либо (книжн.)"
                            },
                            {
                                "target_lang_word": "освобождаться",
                                "target_lang_sense_clarification": "избавляться от обязанностей или бремени"
                            }
                        ]
                    }
                }
            ]
        }
    ]

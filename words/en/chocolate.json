{
    "word_family": [
        "chocolaty",
        "chocolatier"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "27a8c6fc-50e3-3181-8fa7-e4981a7c1b2c",
                    "sense_definition": "A sweet food made from ground roasted cocoa beans, usually brown in color and eaten as a treat.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I bought a large bar of <w>chocolate</w> to share with my friends.",
                            "target_lang_translations": {
                                "ru": "Я купил большую плитку <w>шоколада</w>, чтобы поделиться с друзьями."
                            }
                        },
                        {
                            "text": "Most children love the taste of milk <w>chocolate</w>.",
                            "target_lang_translations": {
                                "ru": "Большинство детей любят вкус молочного <w>шоколада</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "choc",
                        "choccy"
                    ],
                    "common_phrases": [
                        "milk chocolate",
                        "dark chocolate",
                        "white chocolate",
                        "bar of chocolate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкий продукт питания из перетертых жареных какао-бобов, обычно коричневого цвета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоколад",
                                "target_lang_sense_clarification": "продукт / вещество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37aadba7-2e10-3e15-b6e7-fb93223211b7",
                    "sense_definition": "A single, small piece of confectionery made from or covered in chocolate.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food_item",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She offered me a <w>chocolate</w> from the gift box.",
                            "target_lang_translations": {
                                "ru": "Она предложила мне <w>шоколадную конфету</w> из подарочной коробки."
                            }
                        },
                        {
                            "text": "He ate the last <w>chocolate</w> before anyone else could get it.",
                            "target_lang_translations": {
                                "ru": "Он съел последнюю <w>шоколадную конфету</w>, прежде чем кто-либо успел её взять."
                            }
                        }
                    ],
                    "synonyms": [
                        "choc"
                    ],
                    "common_phrases": [
                        "box of chocolates"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная небольшая конфета, сделанная из шоколада или покрытая им."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоколадная конфета",
                                "target_lang_sense_clarification": "штучное изделие"
                            },
                            {
                                "target_lang_word": "шоколадка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14876591-0d0d-3264-9eed-2932be21957c",
                    "sense_definition": "A hot drink made by mixing chocolate powder or melted chocolate with milk or water.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "drink",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A warm cup of <w>chocolate</w> is perfect for a cold winter evening.",
                            "target_lang_translations": {
                                "ru": "Чашка теплого <w>шоколада</w> идеальна для холодного зимнего вечера."
                            }
                        },
                        {
                            "text": "We sat by the fire drinking hot <w>chocolate</w>.",
                            "target_lang_translations": {
                                "ru": "Мы сидели у огня и пили горячий <w>шоколад</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hot chocolate",
                        "drinking chocolate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Горячий напиток, приготовленный путем смешивания шоколадного порошка или растопленного шоколада с молоком или водой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горячий шоколад",
                                "target_lang_sense_clarification": "напиток"
                            },
                            {
                                "target_lang_word": "какао",
                                "target_lang_sense_clarification": "напиток из порошка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7e343c2-ff30-3f7f-a4dd-fb1c4e599a17",
                    "sense_definition": "A dark, reddish-brown color resembling the food.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist used a deep <w>chocolate</w> to paint the shadows of the trees.",
                            "target_lang_translations": {
                                "ru": "Художник использовал насыщенный <w>шоколадный цвет</w>, чтобы нарисовать тени деревьев."
                            }
                        },
                        {
                            "text": "She chose a rich <w>chocolate</w> for the living room walls.",
                            "target_lang_translations": {
                                "ru": "Она выбрала богатый <w>шоколадный цвет</w> для стен гостиной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chocolate brown"
                    ],
                    "target_lang_definitions": {
                        "ru": "Темный красновато-коричневый цвет, напоминающий шоколад."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоколадный цвет",
                                "target_lang_sense_clarification": "оттенок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "eb22cfdd-22aa-3b1f-a2e0-a20306380c44",
                    "sense_definition": "Made of, containing, or flavored with chocolate.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother baked a delicious <w>chocolate</w> cake for my birthday.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка испекла вкусный <w>шоколадный</w> торт на мой день рождения."
                            }
                        },
                        {
                            "text": "I would like two scoops of <w>chocolate</w> ice cream, please.",
                            "target_lang_translations": {
                                "ru": "Я бы хотел два шарика <w>шоколадного</w> мороженого, пожалуйста."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chocolate cake",
                        "chocolate chip",
                        "chocolate sauce",
                        "chocolate bar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный из шоколада, содержащий шоколад или имеющий его вкус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоколадный",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2d6e0ba-3f2d-37d1-a3c9-aefe0e08ba29",
                    "sense_definition": "Having a dark reddish-brown color.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dog had a beautiful <w>chocolate</w> coat that shone in the sun.",
                            "target_lang_translations": {
                                "ru": "У собаки была красивая <w>шоколадная</w> шерсть, которая блестела на солнце."
                            }
                        },
                        {
                            "text": "He looked into her deep <w>chocolate</w> eyes and smiled.",
                            "target_lang_translations": {
                                "ru": "Он посмотрел в её глубокие <w>шоколадные</w> глаза и улыбнулся."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий темный красновато-коричневый цвет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоколадный",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "шоколадного цвета",
                                "target_lang_sense_clarification": "описательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0634e846-ca4a-359b-bb74-c590c32c8647",
                    "sense_definition": "To add chocolate to something or to cover food in chocolate.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cooking_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pastry chef <w>chocolated</w> the strawberries to serve with champagne.",
                            "target_lang_translations": {
                                "ru": "Кондитер <w>покрыл шоколадом</w> клубнику для подачи с шампанским."
                            }
                        },
                        {
                            "text": "They <w>chocolated</w> the biscuits to make them more appealing to children.",
                            "target_lang_translations": {
                                "ru": "Они <w>покрыли шоколадом</w> печенье, чтобы сделать его более привлекательным для детей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавлять шоколад во что-либо или покрывать еду шоколадом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрывать шоколадом",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "глазировать",
                                "target_lang_sense_clarification": "кулинарный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ec34c74-016c-3ae4-9d76-c2035e9b08b0",
                    "sense_definition": "To treat blood agar by heating it in order to break down red blood cells (used in microbiology).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "scientific_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lab technician <w>chocolated</w> the blood agar to prepare the medium.",
                            "target_lang_translations": {
                                "ru": "Лаборант <w>нагрел</w> кровяной агар, чтобы подготовить среду."
                            }
                        },
                        {
                            "text": "Heating the mixture effectively <w>chocolated</w> the sample for the experiment.",
                            "target_lang_translations": {
                                "ru": "Нагревание смеси эффективно <w>превратило</w> образец <w>в «шоколадный»</w> для эксперимента."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical (microbiology)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нагревать кровяной агар для разрушения эритроцитов (в микробиологии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нагревать",
                                "target_lang_sense_clarification": "до получения шоколадного агара"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
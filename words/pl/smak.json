    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c4978f2-a5fa-36b5-aae2-3b8d905ecf50",
                    "sense_definition": "Właściwość jedzenia lub picia odczuwana w ustach, np. słodkość, słoność, kwasowość lub gorycz.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta zupa ma bardzo delikatny <w>smak</w>.",
                            "target_lang_translations": {
                                "ru": "У этого супа очень нежный <w>вкус</w>."
                            }
                        },
                        {
                            "text": "Lody o <w>smaku</w> czekoladowym są moimi ulubionymi.",
                            "target_lang_translations": {
                                "ru": "Мороженое с шоколадным <w>вкусом</w> — мое любимое."
                            }
                        }
                    ],
                    "common_phrases": [
                        "o smaku owocowym",
                        "łagodny smak",
                        "ostry smak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство еды или напитка, ощущаемое во рту, например, сладость, соленость, кислота или горечь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкус",
                                "target_lang_sense_clarification": "свойство пищи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32e63ac3-2b36-379b-9f10-c704c29f1ee4",
                    "sense_definition": "Zmysł, którego narządy znajdują się w jamie ustnej (głównie na języku), służący do rozpoznawania właściwości pokarmu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas silnego przeziębienia często tracimy węch i <w>smak</w>.",
                            "target_lang_translations": {
                                "ru": "Во время сильной простуды мы часто теряем обоняние и <w>вкус</w>."
                            }
                        },
                        {
                            "text": "Człowiek posiada pięć podstawowych zmysłów, w tym <w>smak</w>.",
                            "target_lang_translations": {
                                "ru": "У человека пять основных чувств, включая <w>вкус</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "podniebienie"
                    ],
                    "common_phrases": [
                        "zmysł smaku",
                        "stracić smak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство, органы которого находятся в ротовой полости (в основном на языке), служащее для распознавания свойств пищи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкус",
                                "target_lang_sense_clarification": "одно из пяти чувств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bccd3f3d-80e1-3e25-9076-164c5dc1975a",
                    "sense_definition": "Chęć lub ochota na zjedzenie czegoś konkretnego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam dzisiaj wielki <w>smak</w> na gorącą czekoladę.",
                            "target_lang_translations": {
                                "ru": "У меня сегодня огромное <w>желание</w> выпить горячего шоколада."
                            }
                        },
                        {
                            "text": "Musiałem obejść się <w>smakiem</w>, bo ciasto się skończyło.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось <w>облизнуться</w>, так как пирог закончился."
                            }
                        }
                    ],
                    "synonyms": [
                        "apetyt",
                        "apetyt"
                    ],
                    "common_phrases": [
                        "mieć smak na coś",
                        "narobić smaku",
                        "obejść się smakiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Желание или охота съесть что-то конкретное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аппетит",
                                "target_lang_sense_clarification": "желание поесть"
                            },
                            {
                                "target_lang_word": "желание",
                                "target_lang_sense_clarification": "охота к чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c1d0ef5-2c68-3891-9aeb-8f80cd2899cc",
                    "sense_definition": "Umiejętność oceny tego, co jest estetyczne, eleganckie, artystyczne lub stosowne.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Urządziła swoje nowe mieszkanie z wielkim <w>smakiem</w>.",
                            "target_lang_translations": {
                                "ru": "Она обставила свою новую квартиру с большим <w>вкусом</w>."
                            }
                        },
                        {
                            "text": "Ten żart był w bardzo złym <w>smaku</w>.",
                            "target_lang_translations": {
                                "ru": "Эта шутка была в очень дурном <w>вкусе</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gust",
                        "upodobanie"
                    ],
                    "antonyms": [
                        "bezguście",
                        "kicz"
                    ],
                    "common_phrases": [
                        "w dobrym smaku",
                        "kwestia smaku",
                        "mieć dobry smak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность оценивать то, что эстетично, элегантно, художественно или уместно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкус",
                                "target_lang_sense_clarification": "чувство прекрасного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a599c43-5842-3760-ab36-52fbad150c79",
                    "sense_definition": "Specyficzny charakter, nastrój lub odczucie związane z jakimś doświadczeniem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sportowcy doskonale znają słodki <w>smak</w> zwycięstwa.",
                            "target_lang_translations": {
                                "ru": "Спортсмены прекрасно знают сладкий <w>вкус</w> победы."
                            }
                        },
                        {
                            "text": "Podróże pozwalają nam odkryć różne <w>smaki</w> życia.",
                            "target_lang_translations": {
                                "ru": "Путешествия позволяют нам открыть разные <w>вкусы</w> жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "doświadczenie"
                    ],
                    "common_phrases": [
                        "smak zwycięstwa",
                        "smak życia",
                        "smak przygody"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "metaphorical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфический характер, настроение или ощущение, связанное с каким-либо опытом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкус",
                                "target_lang_sense_clarification": "метафорическое значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e09a5c41-60ec-3b10-a819-f0ec9a0b23ae",
                    "sense_definition": "Wywar z mięsa, kości lub warzyw, służący jako baza do zup i sosów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zupa pomidorowa najlepiej smakuje ugotowana na mięsnym <w>smaku</w>.",
                            "target_lang_translations": {
                                "ru": "Томатный суп вкуснее всего, если сварен на мясном <w>бульоне</w>."
                            }
                        },
                        {
                            "text": "Do sosu trzeba dodać trochę <w>smaku</w> z pieczenia mięsa.",
                            "target_lang_translations": {
                                "ru": "В соус нужно добавить немного <w>сока</w> от жарки мяса."
                            }
                        }
                    ],
                    "synonyms": [
                        "bulion"
                    ],
                    "common_phrases": [
                        "smak z kości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отвар из мяса, костей или овощей, служащий основой для супов и соусов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бульон",
                                "target_lang_sense_clarification": "отвар"
                            },
                            {
                                "target_lang_word": "навар",
                                "target_lang_sense_clarification": "насыщенный бульон"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
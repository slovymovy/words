                        "slaperig",
                        "lodderig",
                        "bedwelmd"
                    ],
                    "antonyms": [
                        "helder",
                        "alert",
                        "wakker"
                    ],
                    "common_phrases": [
                        "versuft kijken",
                        "helemaal versuft",
                        "versuft raken"
                    ],
                    "target_lang_definitions": {

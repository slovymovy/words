            "senses": [
                {
                    "sense_id": "6c36eb7c-def0-3549-b601-44ca028fcddf",
                    "sense_definition": "Niet vaardig met de handen of in bewegingen; geneigd om dingen te laten vallen of tegen dingen aan te stoten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "clumsy_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>onhandige</w> ober liet per ongeluk het dienblad met glazen vallen.",
                            "target_lang_translations": {
                                "en": "The <w>clumsy</w> waiter accidentally dropped the tray of glasses."
                            }
                        },
                        {
                            "text": "Ik voel me altijd een beetje <w>onhandig</w> als ik moet dansen op een feestje.",
                            "target_lang_translations": {
                                "en": "I always feel a bit <w>awkward</w> when I have to dance at a party."
                            }
                        }
                    ],
                    "synonyms": [
                        "klunzig",
                        "stuntelig",
                        "onbeholpen",
                        "schutterig"
                    ],
                    "antonyms": [
                        "handig"
                    ],
                    "common_phrases": [
                        "onhandig zijn met",
                        "een onhandige beweging"
                    ],
                    "target_lang_definitions": {
                        "en": "Not skillful with hands or movements; prone to dropping things or bumping into things."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clumsy",
                                "target_lang_sense_clarification": "lacking physical coordination"
                            },
                            {
                                "target_lang_word": "awkward",
                                "target_lang_sense_clarification": "lacking grace in movement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4280eeb-39af-3f87-a9c9-1f776e7cc2f7",
                    "sense_definition": "Niet praktisch of makkelijk in gebruik; wat extra moeite, tijd of ongemak veroorzaakt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "inconvenient_situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is erg <w>onhandig</w> dat de supermarkt op zondag al zo vroeg sluit.",
                            "target_lang_translations": {
                                "en": "It is very <w>inconvenient</w> that the supermarket closes so early on Sundays."
                            }
                        },
                        {
                            "text": "Hij woont op een <w>onhandige</w> locatie waar geen bussen of treinen stoppen.",
                            "target_lang_translations": {
                                "en": "He lives in an <w>inconvenient</w> location where no buses or trains stop."
                            }
                        }

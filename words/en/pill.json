                },
                {
                    "sense_id": "b94f1bba-30b5-37be-875d-b8de07fba33e",
                    "sense_definition": "A contemptible, annoying, or unpleasant person.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_unpleasant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stop being such a <w>pill</w> and help us clean up the kitchen.",
                            "target_lang_translations": {
                                "ru": "Перестань быть такой <w>занудой</w> и помоги нам убраться на кухне."
                            }
                        },
                        {
                            "text": "He can be a real <w>pill</w> when he does not get his own way.",
                            "target_lang_translations": {
                                "ru": "Он может быть настоящей <w>язвой</w>, когда всё идет не по его плану."
                            }
                        }
                    ],
                    "common_phrases": [
                        "be a pill",
                        "such a pill"

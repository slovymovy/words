    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "46ab6c15-442b-367c-9fa2-3af55c463030",
                    "sense_definition": "Сравнительная степень наречия <w>скоро</w>; быстрее, в более быстром темпе (часто с оттенком желания ускорить событие).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "speed_comparative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <w>поскорее</w> закончить эту сложную работу.",
                            "target_lang_translations": {
                                "en": "I want to finish this difficult work <w>as soon as possible</w>."
                            }
                        },
                        {
                            "text": "Нам нужно <w>поскорее</w> добраться до вокзала.",
                            "target_lang_translations": {
                                "en": "We need to get to the station <w>faster</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "побыстрее",
                        "скорее"
                    ],
                    "antonyms": [
                        "помедленнее"
                    ],
                    "common_phrases": [
                        "вернуться поскорее",
                        "сделать поскорее",
                        "уйти поскорее"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "употребляется чаще, чем нейтральное «скорее», в значении «как можно быстрее»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Comparative degree of the adverb \"soon\"; faster, at a faster pace (often implying a desire to hasten an event)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "as soon as possible",
                                "target_lang_sense_clarification": "indicating a desire to complete something quickly"
                            },
                            {
                                "target_lang_word": "sooner",
                                "target_lang_sense_clarification": "comparative time"
                            },
                            {
                                "target_lang_word": "faster",
                                "target_lang_sense_clarification": "comparative speed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee08ae4d-73f8-3a7f-84e7-b9fa6a51c04c",
                    "sense_definition": "Употребляется как побуждение или приказ делать что-либо быстрее; живей, торопись.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "speed_comparative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Собирайся <w>поскорее</w>, мы уже опаздываем!",
                            "target_lang_translations": {
                                "en": "Get ready <w>quickly</w>, we are already late!"
                            }
                        },
                        {
                            "text": "Иди сюда <w>поскорее</w>, здесь что-то интересное.",
                            "target_lang_translations": {
                                "en": "Come here <w>quick</w>, there is something interesting here."
                            }
                        }
                    ],
                    "synonyms": [
                        "живей",
                        "побыстрее",
                        "бегом"
                    ],
                    "antonyms": [
                        "помедленнее",
                        "не спеши"
                    ],
                    "common_phrases": [
                        "давай поскорее",
                        "решай поскорее"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "используется в разговорной речи как призыв к действию"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used as an urging or command to do something faster; lively, hurry up."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quickly",
                                "target_lang_sense_clarification": "urging action"
                            },
                            {
                                "target_lang_word": "faster",
                                "target_lang_sense_clarification": "imperative context"
                            }
                        ]
                    }
                }
            ]
        }
    ]

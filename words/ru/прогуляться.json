    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "222d92ae-05e0-35c8-8a42-f652d5584947",
                    "sense_definition": "Совершить небольшую прогулку, пройтись пешком ради отдыха или удовольствия.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "walk_leisure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В такую прекрасную погоду нам обязательно нужно <w>прогуляться</w>.",
                            "target_lang_translations": {
                                "en": "In such beautiful weather, we definitely need to <w>take a walk</w>."
                            }
                        },
                        {
                            "text": "Отец предложил детям <w>прогуляться</w> перед сном по парку.",
                            "target_lang_translations": {
                                "en": "Father suggested the children <w>take a stroll</w> through the park before bed."
                            }
                        }
                    ],
                    "synonyms": [
                        "пройтись",
                        "погулять"
                    ],
                    "common_phrases": [
                        "пойти прогуляться",
                        "выйти прогуляться"
                    ],
                    "target_lang_definitions": {
                        "en": "To take a short walk or go on foot for relaxation or pleasure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take a walk",
                                "target_lang_sense_clarification": "general leisure activity"
                            },
                            {
                                "target_lang_word": "go for a stroll",
                                "target_lang_sense_clarification": "relaxed, leisurely walking"
                            },
                            {
                                "target_lang_word": "take a stroll",
                                "target_lang_sense_clarification": "casual walking"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec59935b-ee21-3528-a837-66d2ddfa580d",
                    "sense_definition": "Сходить куда-либо пешком, часто сопровождая кого-либо или за компанию.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "walk_destination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>прогуляюсь</w> до магазина, тебе купить что-нибудь?",
                            "target_lang_translations": {
                                "en": "I'll <w>walk over</w> to the store; do you need me to buy anything?"
                            }
                        },
                        {
                            "text": "Мы решили <w>прогуляться</w> до остановки автобуса пешком.",
                            "target_lang_translations": {
                                "en": "We decided to <w>walk</w> to the bus stop."
                            }
                        }
                    ],
                    "synonyms": [
                        "сходить",
                        "пройтись"
                    ],
                    "common_phrases": [
                        "прогуляться до магазина",
                        "прогуляться пешком"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies a casual walk to a specific place"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To go somewhere on foot, often casually or to accompany someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "walk",
                                "target_lang_sense_clarification": "movement to a destination"
                            },
                            {
                                "target_lang_word": "walk over",
                                "target_lang_sense_clarification": "short distance to a specific place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94777286-9727-3c66-8f1e-9780f6aba4ae",
                    "sense_definition": "Побывать где-либо вынужденно, не по своей воле (обычно с иронией).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "forced_visit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за ошибки в документах мне пришлось лишний раз <w>прогуляться</w> в налоговую.",
                            "target_lang_translations": {
                                "en": "Due to an error in the documents, I had to <w>make a trip</w> to the tax office again."
                            }
                        },
                        {
                            "text": "Хулиганам пришлось <w>прогуляться</w> в отделение полиции для выяснения личности.",
                            "target_lang_translations": {
                                "en": "The hooligans had to <w>take a walk</w> to the police station for identification."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used ironically"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To visit a place out of necessity rather than desire (often used ironically)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make a trip",
                                "target_lang_sense_clarification": "often implies an unwanted errand"
                            },
                            {
                                "target_lang_word": "take a hike",
                                "target_lang_sense_clarification": "slang/ironic for going somewhere"
                            },
                            {
                                "target_lang_word": "drag oneself",
                                "target_lang_sense_clarification": "reluctant movement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7317497-9070-3200-af7b-a764c3fd22a1",
                    "sense_definition": "Растратить все деньги на развлечения и кутежи.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "squander",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Купец знатно <w>прогулялся</w> на ярмарке и вернулся домой без гроша.",
                            "target_lang_translations": {
                                "en": "The merchant really <w>went on a spree</w> at the fair and returned home penniless."
                            }
                        },
                        {
                            "text": "Он боялся <w>прогуляться</w> до последней копейки и остаться ни с чем.",
                            "target_lang_translations": {
                                "en": "He was afraid to <w>blow all his money</w> down to the last kopeck and be left with nothing."
                            }
                        }
                    ],
                    "synonyms": [
                        "прокутиться",
                        "потратиться"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "refers to squandering money"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To spend all one's money on entertainment and partying."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go on a spree",
                                "target_lang_sense_clarification": "spending recklessly on fun"
                            },
                            {
                                "target_lang_word": "blow one's money",
                                "target_lang_sense_clarification": "wasting funds"
                            },
                            {
                                "target_lang_word": "squander",
                                "target_lang_sense_clarification": "wasteful spending"
                            }
                        ]
                    }
                }
            ]
        }
    ]

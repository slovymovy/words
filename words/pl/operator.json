            "senses": [
                {
                    "sense_id": "bc73ff23-ac7a-3834-abb7-96532f602cbc",
                    "sense_definition": "Osoba zawodowo zajmująca się obsługą maszyny lub urządzenia technicznego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person_job",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój wujek jest <w>operatorem</w> żurawia budowlanego.",
                            "target_lang_translations": {
                                "ru": "Мой дядя — <w>оператор</w> (крановщик) строительного крана."
                            }
                        },
                        {
                            "text": "Doświadczony <w>operator</w> wózka widłowego szybko załadował towar na ciężarówkę.",
                            "target_lang_translations": {
                                "ru": "Опытный <w>оператор</w> вилочного погрузчика быстро погрузил товар в грузовик."
                            }
                        }
                    ],
                    "common_phrases": [
                        "operator koparki",
                        "operator dźwigu",
                        "operator maszyny",
                        "operator wózka widłowego"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, профессионально управляющий машиной или техническим устройством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оператор",
                                "target_lang_sense_clarification": "машины или устройства"
                            },
                            {
                                "target_lang_word": "машинист",
                                "target_lang_sense_clarification": "тяжелой техники (крана, поезда)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68217338-c2fa-3248-ad30-16cfe48c7a36",
                    "sense_definition": "Przedsiębiorstwo świadczące usługi, najczęściej telekomunikacyjne, pocztowe lub medialne.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "company",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie jestem zadowolony z usług mojego <w>operatora</w>, bo przez ostatni miesiąc dwa razy podnieśli ceny.",
                            "target_lang_translations": {
                                "ru": "Я недоволен услугами моего <w>оператора</w>, потому что за последний месяц они дважды подняли цены."
                            }
                        },
                        {
                            "text": "Każdy <w>operator</w> sieci komórkowej ma obowiązek rejestrować karty SIM swoich klientów.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>оператор</w> мобильной связи обязан регистрировать SIM-карты своих клиентов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "operator sieci komórkowej",
                        "operator pocztowy",
                        "operator logistyczny",
                        "zmiana operatora"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компания, предоставляющая услуги, чаще всего телекоммуникационные, почтовые или медийные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оператор",
                                "target_lang_sense_clarification": "связи или услуг"
                            },
                            {
                                "target_lang_word": "провайдер",
                                "target_lang_sense_clarification": "интернет-услуг"
                            }
                        ]

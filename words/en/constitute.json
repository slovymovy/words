    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bdeb0bbb-dc67-3386-bc55-b2fb606b684a",
                    "sense_definition": "To make up, compose, or form something as a whole.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "composition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twelve months <w>constitute</w> a full calendar year.",
                            "target_lang_translations": {
                                "ru": "Двенадцать месяцев <w>составляют</w> полный календарный год."
                            }
                        },
                        {
                            "text": "Women <w>constitute</w> the majority of the workforce in this sector.",
                            "target_lang_translations": {
                                "ru": "Женщины <w>составляют</w> большинство рабочей силы в этом секторе."
                            }
                        },
                        {
                            "text": "Does this single error <w>constitute</w> a failure of the entire system?",
                            "target_lang_translations": {
                                "ru": "<w>Представляет</w> ли собой эта единственная ошибка сбой всей системы?"
                            }
                        }
                    ],
                    "synonyms": [
                        "make up",
                        "compose",
                        "form"
                    ],
                    "common_phrases": [
                        "constitute a threat",
                        "constitute a crime",
                        "constitute a violation",
                        "constitute a majority"
                    ],
                    "target_lang_definitions": {
                        "ru": "Составлять, образовывать или представлять собой что-либо как целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составлять",
                                "target_lang_sense_clarification": "образовывать целое"
                            },
                            {
                                "target_lang_word": "представлять собой",
                                "target_lang_sense_clarification": "являться чем-либо"
                            },
                            {
                                "target_lang_word": "являть собой",
                                "target_lang_sense_clarification": "книжн. быть, являться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80008408-9fd0-3a7b-aae3-eb07418ecf6a",
                    "sense_definition": "To set up, establish, or enact legally or formally.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "establishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A special committee was <w>constituted</w> to investigate the allegations.",
                            "target_lang_translations": {
                                "ru": "Для расследования обвинений был <w>учреждён</w> специальный комитет."
                            }
                        },
                        {
                            "text": "The tribunal was duly <w>constituted</w> under international law.",
                            "target_lang_translations": {
                                "ru": "Трибунал был должным образом <w>учреждён</w> в соответствии с международным правом."
                            }
                        }
                    ],
                    "synonyms": [
                        "establish",
                        "enact"
                    ],
                    "common_phrases": [
                        "duly constituted",
                        "legally constituted"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждать, основывать или вводить в действие законным или официальным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "учреждать",
                                "target_lang_sense_clarification": "основывать организацию"
                            },
                            {
                                "target_lang_word": "создавать",
                                "target_lang_sense_clarification": "формировать орган"
                            },
                            {
                                "target_lang_word": "устанавливать",
                                "target_lang_sense_clarification": "вводить порядок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ceeb94b-42bd-3ad2-9c47-6196b98c7eef",
                    "sense_definition": "To appoint, depute, or elect someone to an office or position.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "appointment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The board <w>constituted</w> him the legal guardian of the estate.",
                            "target_lang_translations": {
                                "ru": "Совет <w>назначил</w> его законным опекуном имущества."
                            }
                        },
                        {
                            "text": "They <w>constituted</w> her as their representative for the negotiations.",
                            "target_lang_translations": {
                                "ru": "Они <w>назначили</w> её своим представителем на переговорах."
                            }
                        }
                    ],
                    "synonyms": [
                        "establish"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Назначать, уполномочивать или избирать кого-либо на должность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "назначать",
                                "target_lang_sense_clarification": "на должность"
                            },
                            {
                                "target_lang_word": "делать",
                                "target_lang_sense_clarification": "кем-либо (напр. опекуном)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "00ae2a76-4871-3af2-8013-c9a1ddf75c95",
                    "sense_definition": "An established law or decree.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The citizens were expected to obey every royal <w>constitute</w> without question.",
                            "target_lang_translations": {
                                "ru": "Граждане должны были беспрекословно подчиняться каждому королевскому <w>указу</w>."
                            }
                        },
                        {
                            "text": "He was punished for violating an ancient <w>constitute</w> of the realm.",
                            "target_lang_translations": {
                                "ru": "Он был наказан за нарушение древнего <w>постановления</w> королевства."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Установленный закон или указ (устаревшее)."
                    },
                    "translations": {

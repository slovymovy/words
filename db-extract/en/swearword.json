        "de-extract.jsonl": [
            {
                "entry_id": "4fee95bf-98de-3471-9f79-d26671b7921a",
                "word": "swearword",
                "pos": "noun",
                "lang_code": "en",
                "forms": [],
                "senses": [
                    {
                        "sense_id": "d9f73f69-2c4d-3771-b408-591499d6c8e4",
                        "entry_id": "4fee95bf-98de-3471-9f79-d26671b7921a",
                        "glosses": [
                            "das Schimpfwort, das Fluchwort, das Kraftwort"
                        ],
                        "tags": [],
                        "examples": [],
                        "sense_index_json": "\"1\""
                    }
                ],
                "translations": [
                    {
                        "translation_id": "e88051a6-1284-4ddd-86b1-876fb9d6d3ff",
                        "source_entry_id": "4fee95bf-98de-3471-9f79-d26671b7921a",
                        "source_sense_id": "d9f73f69-2c4d-3771-b408-591499d6c8e4",
                        "source_sense_description": "das Schimpfwort, das Fluchwort, das Kraftwort",
                        "target_lang_word": "Schimpfwort",
                        "target_lang_code": "de",
                        "target_sense_ids": [],
                        "cross_references": [
                            {
                                "target_word": "Schimpfwort",
                                "target_lang_code": "de",
                                "target_senses": [
                                    {
                                        "sense_id": "23fb8c50-d35a-3743-b522-e5fe7153bb9b",
                                        "entry_id": "2ce28425-f4c9-3ac2-8ca3-683b1dfa097a",
                                        "glosses": [
                                            "ein abfälliges, herabwürdigendes und beleidigendes Wort"
                                        ],
                                        "tags": [],
                                        "examples": [
                                            {
                                                "example_id": "1ff5ee3a-08c9-4918-9780-5245f57bbf18",
                                                "sense_id": "23fb8c50-d35a-3743-b522-e5fe7153bb9b",
                                                "text": "„Schimpfwörter sind Substantive, mit denen Personen anstatt mit ihren Namen oder Titel in abfälliger Weise angeredet oder benannt werden…“"
                                            },
                                            {
                                                "example_id": "ec1ea06d-e4b9-459a-a67f-981ab6c94335",
                                                "sense_id": "23fb8c50-d35a-3743-b522-e5fe7153bb9b",
                                                "text": "Benutze bitte nicht solche Schimpfworte!"
                                            },
                                            {
                                                "example_id": "250f192c-5c31-4bc9-9eb6-a8c640ba8540",
                                                "sense_id": "23fb8c50-d35a-3743-b522-e5fe7153bb9b",
                                                "text": "„Bevor wir uns einigen Schimpfwörtern zuwenden, befassen wir uns kurz mit der Bedeutungsgeschichte des Wortfeldes ‚schimpfen‘ selbst und versetzen uns zu diesem Zweck ins 17. Jahrhundert.“"
                                            },
                                            {
                                                "example_id": "a804152e-171c-4f5d-aa96-ca35bd2b27ce",
                                                "sense_id": "23fb8c50-d35a-3743-b522-e5fe7153bb9b",
                                                "text": "„Gefühlswert besitzen auch diejenigen Wörter, die als Schimpf- und Scheltwörter verwendet werden.“"
                                            },
                                            {
                                                "example_id": "b6e798ad-36d2-4680-8d0e-4b45a04bd92d",
                                                "sense_id": "23fb8c50-d35a-3743-b522-e5fe7153bb9b",
                                                "text": "„Eigentlich ist ja der Gebrauch von Schimpfwörtern heutzutage verpönt.“"
                                            },
                                            {
                                                "example_id": "48ebf0b1-acbc-483b-a038-685c776c3da0",
                                                "sense_id": "23fb8c50-d35a-3743-b522-e5fe7153bb9b",
                                                "text": "„Auf Menschen bezogen ist er ein allgemeines Schimpfwort, die Bezeichnung für gewalttätige junge Burschen, überspannte Typen, Faulpelze, Schlaumeier oder bösartige Weiber.“"
                                            }
                                        ],
                                        "sense_index_json": "\"1\""
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "translation_id": "61901f00-9ead-46f6-a927-9d76673ac0b1",
                        "source_entry_id": "4fee95bf-98de-3471-9f79-d26671b7921a",
                        "source_sense_id": "d9f73f69-2c4d-3771-b408-591499d6c8e4",
                        "source_sense_description": "das Schimpfwort, das Fluchwort, das Kraftwort",
                        "target_lang_word": "Fluchwort",
                        "target_lang_code": "de",
                        "target_sense_ids": [],
                        "cross_references": [
                            {
                                "target_word": "Fluchwort",
                                "target_lang_code": "de",
                                "target_senses": [
                                    {
                                        "sense_id": "ddf1740c-fd24-37b4-b807-401ebef12ed5",
                                        "entry_id": "a850218b-b2a9-3b98-9404-553f62cb30d5",
                                        "glosses": [
                                            "Wort oder Wortgruppe, womit jemand einen Fluch äußert"
                                        ],
                                        "tags": [],
                                        "examples": [
                                            {
                                                "example_id": "8ae02231-d8ea-45e2-b28c-19733f866351",
                                                "sense_id": "ddf1740c-fd24-37b4-b807-401ebef12ed5",
                                                "text": "„Dann ist als Fluchwort auch die Hölle helvete wichtig, statt ›verpiss dich!‹ heißt es unter den Schweden ›Geh zur Hölle!‹ Dra åt helvete!“"
                                            },
                                            {
                                                "example_id": "f2647f3b-ba0c-4ea5-a318-0ad45cb36cc9",
                                                "sense_id": "ddf1740c-fd24-37b4-b807-401ebef12ed5",
                                                "text": "„Bereitwilligst verpflichtete ich mich hierzu mit den feierlichsten Fluchworten und Verwünschungen, die der Teufel und wir beide nur zu finden vermochten.“"
                                            },
                                            {
                                                "example_id": "53d40e54-6870-4161-9a0e-8336a94ca029",
                                                "sense_id": "ddf1740c-fd24-37b4-b807-401ebef12ed5",
                                                "text": "„Noch verstärkt werden konnte dieses Fluchwort durch die Zusammensetzung mit Schock: Schockschwerenot!“"
                                            },
                                            {
                                                "example_id": "be9d59f0-5d0e-4dfa-82e7-f03977c21825",
                                                "sense_id": "ddf1740c-fd24-37b4-b807-401ebef12ed5",
                                                "text": "„Ein Pferd wieherte, eine Peitsche klappte, ein Fluchwort kam hinterher.“"
                                            }
                                        ],
                                        "sense_index_json": "\"1\""
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "translation_id": "4bac8cd8-b1f6-4553-8490-c6a6de85b8cf",
                        "source_entry_id": "4fee95bf-98de-3471-9f79-d26671b7921a",
                        "source_sense_id": "d9f73f69-2c4d-3771-b408-591499d6c8e4",
                        "source_sense_description": "das Schimpfwort, das Fluchwort, das Kraftwort",
                        "target_lang_word": "Kraftwort",
                        "target_lang_code": "de",
                        "target_sense_ids": [],
                        "cross_references": [
                            {
                                "target_word": "Kraftwort",
                                "target_lang_code": "de",
                                "target_senses": [
                                    {
                                        "sense_id": "7cfcf222-293e-3581-afc8-d889b6ad5276",
                                        "entry_id": "191bc31d-986a-3163-9603-773748e30e0c",
                                        "glosses": [
                                            "Wort, mit dem die angesprochene Sache oder Person derb/ drastisch benannt wird"
                                        ],
                                        "tags": [],
                                        "examples": [
                                            {
                                                "example_id": "0d937a3b-4d4d-4e09-a71c-024730fac527",
                                                "sense_id": "7cfcf222-293e-3581-afc8-d889b6ad5276",
                                                "text": "Ladendorf (1906) nennt „Kadavergehorsam“ als Kraftwort: „Kadavergehorsam, … seit Ausgang der siebziger Jahre des 19. Jahrhunderts belegt… Das verächtliche Kraftwort gehört zu den Trümpfen der Sozialdemokraten und wird mit Vorliebe gegen die militärische Subordination gewandt.“"
                                            },
                                            {
                                                "example_id": "dc8a8ae9-ae31-49d2-8ce1-ac9802a2f963",
                                                "sense_id": "7cfcf222-293e-3581-afc8-d889b6ad5276",
                                                "text": "„Ein Kraftwort ist ein derber, übertreibender, bizarr-anschaulicher Ausdruck aus der niederen Umgangssprache, der eben dadurch imstande ist, ein Ding nicht nur zu nennen, sondern gleichzeitig die Gefühle des Sprechenden zu entladen.“"
                                            },
                                            {
                                                "example_id": "542e8c30-e213-4044-a77d-f21ebebd5642",
                                                "sense_id": "7cfcf222-293e-3581-afc8-d889b6ad5276",
                                                "text": "Besonders geläufige Kraftwörter sind heutzutage „Mist“, „Sauerei“, „Scheiße“ und viele mehr."
                                            },
                                            {
                                                "example_id": "23845c11-4247-4b91-aee5-ca1b243ca6c0",
                                                "sense_id": "7cfcf222-293e-3581-afc8-d889b6ad5276",
                                                "text": "„Besonders einem Kraftwort gesellen sich gerne andere Wörter hinzu.“"
                                            }
                                        ],
                                        "sense_index_json": "\"1\""
                                    },
                                    {
                                        "sense_id": "bf0bfec4-040c-3ec9-a40a-1559a9d4cfc1",
                                        "entry_id": "191bc31d-986a-3163-9603-773748e30e0c",
                                        "glosses": [
                                            "Ausspruch derben/ drastischen Inhalts"
                                        ],
                                        "tags": [],
                                        "examples": [
                                            {
                                                "example_id": "f740e283-cf6b-4518-865f-bd4bd5219e68",
                                                "sense_id": "bf0bfec4-040c-3ec9-a40a-1559a9d4cfc1",
                                                "text": "„‚Du bist verrückt‘ ist Kraftwort für ‚dein Vorschlag scheint mir unzweckmäßig‘…“"
                                            },
                                            {
                                                "example_id": "a6fda418-5f1d-40aa-9613-e1755a7e2eba",
                                                "sense_id": "bf0bfec4-040c-3ec9-a40a-1559a9d4cfc1",
                                                "text": "„Beim Ausgang der römischen Republik sind die Kraftworte des älteren Cato verschwunden, aber seine strenge Sittlichkeit mit ihnen.“"
                                            }
                                        ],
                                        "sense_index_json": "\"2\""
                                    }
                                ]
                            }
                        ]
                    }
                ],
                "word_linkages": []
            }
        ],

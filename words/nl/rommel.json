                {
                    "sense_id": "6de15214-5302-3094-8db9-314f05ef900d",
                    "sense_definition": "Waardeloze spullen of dingen van slechte kwaliteit.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "rommel_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gooi die <w>rommel</w> toch eens weg!",

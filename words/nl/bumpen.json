    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2b4c9ad9-3805-3d50-be45-b6fa4eebb769",
                    "sense_definition": "Een reactie plaatsen in een internetforum om het onderwerp weer bovenaan de lijst te krijgen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "internet_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je geen antwoord krijgt, mag je het bericht na 24 uur <w>bumpen</w>.",
                            "target_lang_translations": {
                                "ru": "Если не получишь ответа, можешь <w>поднять</w> сообщение через 24 часа."
                            }
                        },
                        {
                            "text": "Hij <w>bumpte</w> de oude discussie omdat het probleem nog steeds niet was opgelost.",
                            "target_lang_translations": {
                                "ru": "Он <w>поднял</w> старое обсуждение, так как проблема всё ещё не была решена."
                            }
                        }
                    ],
                    "synonyms": [
                        "omhooghalen"
                    ],
                    "common_phrases": [
                        "een topic bumpen",
                        "een draadje bumpen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опубликовать комментарий на интернет-форуме, чтобы вернуть тему в начало списка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поднимать",
                                "target_lang_sense_clarification": "тему на форуме"
                            },
                            {
                                "target_lang_word": "бампать",
                                "target_lang_sense_clarification": "интернет-сленг"
                            },
                            {
                                "target_lang_word": "апать",
                                "target_lang_sense_clarification": "интернет-сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5007127-49ed-3107-a159-cf69b61a2b08",
                    "sense_definition": "Tegen iets of iemand aan botsen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De auto <w>bumpte</w> zachtjes tegen het paaltje aan.",
                            "target_lang_translations": {
                                "ru": "Машина слегка <w>стукнулась</w> о столбик."
                            }
                        },
                        {
                            "text": "Ik <w>bumpte</w> per ongeluk tegen haar op in de drukke gang.",
                            "target_lang_translations": {
                                "ru": "Я случайно <w>столкнулся</w> с ней в переполненном коридоре."
                            }
                        }
                    ],
                    "synonyms": [
                        "botsen",
                        "stoten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удариться или столкнуться с кем-то или чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ударяться",
                                "target_lang_sense_clarification": "обо что-то"
                            },
                            {

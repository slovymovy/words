                {
                    "sense_id": "16c93781-2baf-39a4-b97f-660b1fe04ebd",
                    "sense_definition": "Vloeistof of lucht krachtig naar binnen trekken, meestal met de mond.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De baby <w>zoog</w> tevreden aan zijn duim.",
                            "target_lang_translations": {
                                "en": "The baby <w>sucked</w> his thumb contentedly.",
                                "ru": "Малыш довольно <w>сосал</w> свой палец."
                            }
                        },
                        {
                            "text": "Je moet niet aan de wond <w>zuigen</w> als het bloedt.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>suck</w> on the wound if it's bleeding.",
                                "ru": "Не стоит <w>сосать</w> рану, если она кровоточит."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan zijn duim zuigen",
                        "zich vast zuigen",
                        "aan een rietje zuigen"
                    ],
                    "target_lang_definitions": {

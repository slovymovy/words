    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b707924d-3dce-3588-a8c8-e97d2a637fcc",
                    "sense_definition": "Powodować, że coś przestaje być zamknięte, dając dostęp do wnętrza (np. drzwi, okno, pudełko).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <w>otwierać</w> okno tylko wtedy, gdy jest ciepło.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>открывайте</w> окно только тогда, когда тепло."
                            }
                        },
                        {
                            "text": "Musisz <w>otwierać</w> drzwi kluczem, bo zamek się zacina.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>открывать</w> дверь ключом, потому что замок заедает."
                            }
                        }
                    ],
                    "synonyms": [
                        "odmykać"
                    ],
                    "antonyms": [
                        "zamykać",
                        "przymykać"
                    ],
                    "common_phrases": [
                        "otwierać drzwi",
                        "otwierać okno",
                        "otwierać oczy",
                        "otwierać usta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-то незапертым, обеспечивая доступ внутрь (например, дверь, окно, коробку)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "физически (дверь, окно)"
                            },
                            {
                                "target_lang_word": "отворять",
                                "target_lang_sense_clarification": "створки, ворота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5394f940-bb20-3469-a377-30d3a6a27cfe",
                    "sense_definition": "Rozpoczynać działalność sklepu, firmy lub instytucji; udostępniać je dla klientów.",

{
    "word_family": [
        "router",
        "reroute",
        "subroute"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7334d63e-7eea-3092-96c9-1ccce27b2055",
                    "sense_definition": "A way, course, or road taken to get from one place to another.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_path",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to take the scenic <w>route</w> to the beach instead of the highway.",
                            "target_lang_translations": {
                                "ru": "Мы решили выбрать живописный <w>маршрут</w> до пляжа вместо шоссе."
                            }
                        },
                        {
                            "text": "The climbers carefully planned their <w>route</w> up the steep mountain.",
                            "target_lang_translations": {
                                "ru": "Альпинисты тщательно спланировали свой <w>маршрут</w> подъема на крутую гору."
                            }
                        }
                    ],
                    "synonyms": [
                        "path",
                        "way",
                        "course",
                        "road",
                        "track"
                    ],
                    "common_phrases": [
                        "en route",
                        "scenic route",
                        "escape route",
                        "direct route"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путь, курс или дорога, выбранные для перемещения из одного места в другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маршрут",
                                "target_lang_sense_clarification": "путь следования"
                            },
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "направление движения"
                            },
                            {
                                "target_lang_word": "дорога",
                                "target_lang_sense_clarification": "место для проезда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d4701a8-738a-3351-a707-da183c0d42eb",
                    "sense_definition": "A regular path or schedule of stops, often used for public transport or deliveries.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "regular_itinerary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bus <w>route</w> stops right in front of the library every hour.",
                            "target_lang_translations": {
                                "ru": "Автобус этого <w>маршрута</w> останавливается прямо перед библиотекой каждый час."
                            }
                        },
                        {
                            "text": "He has a morning paper <w>route</w> that covers the entire neighborhood.",
                            "target_lang_translations": {
                                "ru": "У него есть утренний <w>маршрут</w> развозки газет, который охватывает весь район."
                            }
                        }
                    ],
                    "synonyms": [
                        "itinerary",
                        "circuit",
                        "run",
                        "round"
                    ],
                    "common_phrases": [
                        "bus route",
                        "delivery route",
                        "paper route",
                        "trade route"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулярный путь или расписание остановок, часто используемые для общественного транспорта или доставки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маршрут",
                                "target_lang_sense_clarification": "транспортный рейс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "489b56ee-626f-340a-92d2-3c6f8830fe9e",
                    "sense_definition": "A method or approach used to achieve a specific result.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "method",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hard work and dedication are the surest <w>route</w> to success.",
                            "target_lang_translations": {
                                "ru": "Упорный труд и самоотверженность — самый верный <w>путь</w> к успеху."
                            }
                        },
                        {
                            "text": "They decided to take a legal <w>route</w> to resolve the contract dispute.",
                            "target_lang_translations": {
                                "ru": "Они решили выбрать законный <w>путь</w> для разрешения спора по контракту."
                            }
                        }
                    ],
                    "synonyms": [
                        "method",
                        "approach",
                        "way",
                        "course"
                    ],
                    "common_phrases": [
                        "route to success",
                        "route to market"
                    ],
                    "target_lang_definitions": {
                        "ru": "Метод или подход, используемый для достижения определенного результата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "способ достижения цели"
                            },
                            {
                                "target_lang_word": "курс",
                                "target_lang_sense_clarification": "направление деятельности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abb78a05-cdac-3224-ae9d-b9db78a3f00f",
                    "sense_definition": "A path for data transmission across a network.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The network administrator configured a static <w>route</w> for the secure server.",
                            "target_lang_translations": {
                                "ru": "Сетевой администратор настроил статический <w>маршрут</w> для защищенного сервера."
                            }
                        },
                        {
                            "text": "Data packets take the most efficient <w>route</w> available to reach their destination.",
                            "target_lang_translations": {
                                "ru": "Пакеты данных выбирают наиболее эффективный доступный <w>маршрут</w>, чтобы добраться до места назначения."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Путь передачи данных через сеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маршрут",
                                "target_lang_sense_clarification": "путь данных в сети"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bbffc8b7-1428-39ab-8343-22c2aece82a2",
                    "sense_definition": "To send or direct someone or something along a particular path or course.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "directing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police <w>routed</w> traffic away from the accident scene to prevent congestion.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>направила</w> движение в сторону от места аварии, чтобы предотвратить затор."
                            }
                        },
                        {
                            "text": "All customer service calls are <w>routed</w> to the call center in the main office.",
                            "target_lang_translations": {
                                "ru": "Все звонки в службу поддержки <w>направляются</w> в колл-центр в главном офисе."
                            }
                        }
                    ],
                    "synonyms": [
                        "direct",
                        "send",
                        "channel",
                        "steer",
                        "divert"
                    ],
                    "common_phrases": [
                        "route traffic",
                        "route calls",
                        "route through"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправлять или направлять кого-либо или что-либо по определенному пути или курсу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "направлять",
                                "target_lang_sense_clarification": "указывать путь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6087331e-4bff-365c-bd15-f52745c0a3b6",
                    "sense_definition": "To send data or information through a network or router.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The system automatically <w>routes</w> emails to the spam folder if they look suspicious.",
                            "target_lang_translations": {
                                "ru": "Система автоматически <w>направляет</w> электронные письма в папку спама, если они выглядят подозрительно."
                            }
                        },
                        {
                            "text": "Internet traffic is <w>routed</w> through various servers before reaching your computer.",
                            "target_lang_translations": {
                                "ru": "Интернет-трафик <w>маршрутизируется</w> через различные серверы, прежде чем попасть на ваш компьютер."
                            }
                        }
                    ],
                    "synonyms": [
                        "transmit",
                        "forward"
                    ],
                    "common_phrases": [
                        "route data",
                        "route packets"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправлять данные или информацию через сеть или маршрутизатор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маршрутизировать",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "направлять",
                                "target_lang_sense_clarification": "пересылать данные"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "29834f38-ecc0-3bb5-8552-4168032d8c24",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Route</w> signed the guestbook at the hotel reception.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Рут</w> расписался в гостевой книге на стойке регистрации отеля."
                            }
                        },
                        {
                            "text": "We looked up the history of the <w>Route</w> family in the local archives.",
                            "target_lang_translations": {
                                "ru": "Мы искали историю семьи <w>Рут</w> в местных архивах."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "from French"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Рут",
                                "target_lang_sense_clarification": "транслитерация имени"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
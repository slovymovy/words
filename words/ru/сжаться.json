    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c683a85c-42fc-3109-9b83-0e21833e3fca",
                    "sense_definition": "Плотно сомкнуться, соединиться (о пальцах, руках, зубах или губах)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "body_parts_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его кулаки непроизвольно <w>сжались</w>, когда он услышал эту наглую ложь.",
                            "target_lang_translations": {
                                "en": "His fists involuntarily <w>clenched</w> when he heard that brazen lie."
                            }
                        },
                        {
                            "text": "Губы девушки упрямо <w>сжались</w>, и она отказалась отвечать на вопросы.",
                            "target_lang_translations": {
                                "en": "The girl's lips <w>tightened</w> stubbornly, and she refused to answer the questions."
                            }
                        }
                    ],
                    "synonyms": [
                        "сомкнуться"
                    ],
                    "antonyms": [
                        "разжаться"
                    ],
                    "common_phrases": [
                        "кулаки сжались",
                        "зубы сжались",
                        "пальцы сжались",
                        "губы сжались"
                    ],
                    "target_lang_definitions": {
                        "en": "To close tightly or join together (of fingers, hands, teeth, or lips)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clench",
                                "target_lang_sense_clarification": "fists, teeth, hands"
                            },
                            {
                                "target_lang_word": "tighten",
                                "target_lang_sense_clarification": "lips, general tension"
                            },
                            {
                                "target_lang_word": "purse",
                                "target_lang_sense_clarification": "lips"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e000995-1f66-30d4-87a2-2a71fb211b23",
                    "sense_definition": "Уменьшиться в размере или объёме, стать короче или уже",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "general_reduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Шерстяной свитер после стирки в горячей воде сильно <w>сжался</w>.",
                            "target_lang_translations": {
                                "en": "The wool sweater <w>shrank</w> significantly after washing in hot water."
                            }
                        },
                        {
                            "text": "От низкой температуры металлическая деталь может немного <w>сжаться</w>.",
                            "target_lang_translations": {
                                "en": "Due to the low temperature, the metal part might <w>contract</w> a little."
                            }
                        }
                    ],
                    "synonyms": [
                        "уменьшиться",
                        "сократиться"
                    ],
                    "antonyms": [
                        "расшириться",
                        "увеличиться"
                    ],
                    "common_phrases": [
                        "сжаться в размерах",
                        "сжаться от холода"
                    ],
                    "target_lang_definitions": {
                        "en": "To decrease in size or volume, become shorter or narrower"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shrink",
                                "target_lang_sense_clarification": "clothes, general size"
                            },
                            {
                                "target_lang_word": "contract",
                                "target_lang_sense_clarification": "materials, temperature effects"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37dac339-6eab-39d2-8225-74f89793fa11",
                    "sense_definition": "Собраться в комок, прижав конечности к туловищу (обычно от страха, холода или боли)",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "body_posture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленький котёнок <w>сжался</w> в комок, пытаясь согреться на холодном ветру.",
                            "target_lang_translations": {
                                "en": "The little kitten <w>curled up</w> into a ball, trying to keep warm in the cold wind."
                            }
                        },
                        {
                            "text": "Ребёнок испугался громкого звука и <w>сжался</w> в углу дивана.",
                            "target_lang_translations": {
                                "en": "The child got scared of the loud sound and <w>huddled</w> in the corner of the sofa."
                            }
                        }
                    ],
                    "synonyms": [
                        "съёжиться"
                    ],
                    "antonyms": [
                        "выпрямиться"
                    ],
                    "common_phrases": [
                        "сжаться в комок",
                        "сжаться от страха",
                        "весь сжался"
                    ],
                    "target_lang_definitions": {
                        "en": "To gather into a ball, pressing limbs to the body (usually from fear, cold, or pain)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "curl up",
                                "target_lang_sense_clarification": "posture, into a ball"
                            },
                            {
                                "target_lang_word": "huddle",
                                "target_lang_sense_clarification": "from cold or fear"
                            },
                            {
                                "target_lang_word": "cower",
                                "target_lang_sense_clarification": "specifically from fear"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7227856c-3905-3529-938a-57c36a33beaa",
                    "sense_definition": "Болезненно сократиться, испытать тяжесть от сильного чувства (о сердце или душе)",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У матери <w>сжалось</w> сердце, когда она увидела слёзы своего сына.",
                            "target_lang_translations": {
                                "en": "The mother's heart <w>clenched</w> when she saw her son's tears."
                            }
                        },
                        {
                            "text": "В груди всё <w>сжалось</w> от плохого предчувствия перед долгой поездкой.",
                            "target_lang_translations": {
                                "en": "Everything inside the chest <w>tightened</w> from a bad premonition before the long trip."
                            }
                        }
                    ],
                    "synonyms": [
                        "замереть"
                    ],
                    "common_phrases": [
                        "сердце сжалось",
                        "сжаться от тоски",
                        "сжаться от боли"
                    ],
                    "target_lang_definitions": {
                        "en": "To contract painfully, to feel heaviness from strong emotion (of the heart or soul)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clench",
                                "target_lang_sense_clarification": "of the heart, emotional pain"
                            },
                            {
                                "target_lang_word": "tighten",
                                "target_lang_sense_clarification": "sensation in the chest"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c3bd3fd-7dbf-3df2-9ab3-05adc6f02574",
                    "sense_definition": "Уплотниться, уменьшить свой объём под воздействием внешнего давления (о газах, жидкостях, пружинах)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physics_compression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Под большим давлением газ в баллоне сильно <w>сжался</w>.",
                            "target_lang_translations": {
                                "en": "Under high pressure, the gas in the cylinder <w>compressed</w> significantly."
                            }
                        },
                        {
                            "text": "Пружина в механизме <w>сжалась</w> до предела, готовая выстрелить в любой момент.",
                            "target_lang_translations": {
                                "en": "The spring in the mechanism <w>compressed</w> to the limit, ready to shoot out at any moment."
                            }
                        }
                    ],
                    "synonyms": [
                        "уплотниться"
                    ],
                    "antonyms": [
                        "расшириться"
                    ],
                    "common_phrases": [
                        "пружина сжалась",
                        "газ сжался"
                    ],
                    "target_lang_definitions": {
                        "en": "To become denser, reduce volume under external pressure (gases, liquids, springs)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "compress",
                                "target_lang_sense_clarification": "physics, mechanics"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
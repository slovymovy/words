    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8033dd08-6b36-3c76-9986-11239c11f25a",
                    "sense_definition": "Capable of being moved to a new place or location.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "general_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company is looking for a manager who is <w>relocatable</w> to their overseas branch.",
                            "target_lang_translations": {
                                "ru": "Компания ищет менеджера, <w>готового к переезду</w> в их зарубежный филиал."
                            }
                        },
                        {
                            "text": "We purchased a <w>relocatable</w> office unit for the construction site.",
                            "target_lang_translations": {
                                "ru": "Мы приобрели <w>передвижной</w> офисный блок для строительной площадки."
                            }
                        }
                    ],
                    "synonyms": [
                        "movable",
                        "transportable"
                    ],
                    "common_phrases": [
                        "relocatable home",
                        "relocatable building",
                        "relocatable staff"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный быть перемещенным на новое место или локацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перемещаемый",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "передвижной",
                                "target_lang_sense_clarification": "о конструкциях или оборудовании"
                            },
                            {
                                "target_lang_word": "готовый к переезду",
                                "target_lang_sense_clarification": "о сотрудниках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7e3e160-4787-3c3c-8582-54152ae35573",
                    "sense_definition": "Capable of being loaded into and executed from different parts of computer memory without error.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The compiler generates <w>relocatable</w> code that the linker processes.",
                            "target_lang_translations": {
                                "ru": "Компилятор генерирует <w>перемещаемый</w> код, который обрабатывает компоновщик."
                            }
                        },
                        {
                            "text": "This software module is <w>relocatable</w> and can run anywhere in the system memory.",
                            "target_lang_translations": {
                                "ru": "Этот программный модуль является <w>перемещаемым</w> и может работать в любом месте системной памяти."
                            }
                        }
                    ],
                    "common_phrases": [
                        "relocatable code",
                        "relocatable address",
                        "relocatable object"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный быть загруженным в различные области памяти компьютера и выполняться без ошибок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перемещаемый",
                                "target_lang_sense_clarification": "компьютерный термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "73d57a06-262e-3092-b9e7-366a56240f5f",
                    "sense_definition": "A temporary building or structure that can be dismantled and set up in another location.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "general_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The school installed two <w>relocatables</w> to accommodate the extra students.",
                            "target_lang_translations": {
                                "ru": "Школа установила два <w>модульных здания</w> для размещения дополнительных учеников."
                            }
                        },
                        {
                            "text": "These <w>relocatables</w> serve as temporary offices during the renovation.",
                            "target_lang_translations": {
                                "ru": "Эти <w>передвижные конструкции</w> служат временными офисами во время ремонта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "school relocatable",
                        "temporary relocatable"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временное здание или сооружение, которое можно разобрать и установить в другом месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модульное здание",
                                "target_lang_sense_clarification": "тип строения"
                            },
                            {
                                "target_lang_word": "передвижное сооружение",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ccb2647a-54a3-3591-b3f1-2357b5be663d",
                    "sense_definition": "A bag or pouch inside a plant or animal that typically contains a fluid.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "biological_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The spider carefully wove a silk <w>sac</w> to protect its eggs from predators.",
                            "target_lang_translations": {
                                "ru": "Паук тщательно сплел шелковый <w>мешочек</w>, чтобы защитить свои яйца от хищников."
                            }
                        },
                        {
                            "text": "Doctors identified a small fluid-filled <w>sac</w> near the joint that was causing the inflammation.",
                            "target_lang_translations": {
                                "ru": "Врачи обнаружили небольшой заполненный жидкостью <w>мешочек</w> возле сустава, который вызывал воспаление."
                            }
                        }
                    ],
                    "synonyms": [
                        "pouch",
                        "vesicle",
                        "bladder",
                        "cyst"
                    ],
                    "common_phrases": [
                        "amniotic sac",
                        "yolk sac",
                        "egg sac",
                        "air sac",
                        "venom sac"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мешок или полость внутри растения или животного, обычно содержащая жидкость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мешок",
                                "target_lang_sense_clarification": "анатомический термин"
                            },
                            {
                                "target_lang_word": "мешочек",
                                "target_lang_sense_clarification": "небольшая полость, например, с ядом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffe86dde-f314-3757-8abe-8246718e1052",
                    "sense_definition": "A sacrifice, typically of a game piece or unit for strategic advantage.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "gaming_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The grandmaster's unexpected queen <w>sac</w> completely disrupted his opponent's defense.",
                            "target_lang_translations": {
                                "ru": "Неожиданная <w>жертва</w> ферзя гроссмейстером полностью разрушила защиту его противника."
                            }
                        },
                        {
                            "text": "Making a positional <w>sac</w> can be risky if you do not calculate the outcome precisely.",
                            "target_lang_translations": {
                                "ru": "Позиционная <w>жертва</w> может быть рискованной, если вы точно не просчитаете результат."
                            }
                        }
                    ],
                    "synonyms": [
                        "sacrifice"
                    ],
                    "common_phrases": [
                        "queen sac",
                        "pawn sac",
                        "exchange sac"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in chess and gaming"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "short for sacrifice"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жертва фигуры или юнита для получения стратегического преимущества (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жертва",
                                "target_lang_sense_clarification": "в шахматах или играх"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "559dd05d-ee66-3caf-8c36-afae5dfdc7e8",
                    "sense_definition": "The privilege, formerly enjoyed by the lord of a manor, of holding courts and imposing fines.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_law",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient charter granted the local lord the rights of <w>sac</w> and soc over the peasantry.",
                            "target_lang_translations": {
                                "ru": "Древняя грамота даровала местному лорду права <w>сака</w> и сока над крестьянством."
                            }
                        },
                        {
                            "text": "Historians study old legal texts to understand the full extent of jurisdiction implied by <w>sac</w>.",
                            "target_lang_translations": {
                                "ru": "Историки изучают старые юридические тексты, чтобы понять полный объем юрисдикции, подразумеваемый правом <w>сака</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sac and soc"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical legal term"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинное право феодала вершить суд и взимать штрафы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сак",
                                "target_lang_sense_clarification": "исторический юридический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cf8ada04-a59f-340d-94a0-92ca4f15b092",
                    "sense_definition": "To sacrifice a game piece or unit to gain a tactical advantage.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "gaming_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to <w>sac</w> his knight to open up the center of the board.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>пожертвовать</w> конем, чтобы вскрыть центр доски."
                            }
                        },
                        {
                            "text": "In this video game, you sometimes have to <w>sac</w> a weaker unit to save the rest of your team.",
                            "target_lang_translations": {
                                "ru": "В этой видеоигре иногда приходится <w>жертвовать</w> более слабым юнитом, чтобы спасти остальную команду."
                            }
                        }
                    ],
                    "synonyms": [
                        "sacrifice"
                    ],
                    "common_phrases": [
                        "sac a piece",
                        "sac for position"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in chess and gaming"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пожертвовать игровой фигурой или юнитом ради тактического преимущества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жертвовать",
                                "target_lang_sense_clarification": "фигурой или юнитом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "30c504ce-26d0-39b8-90a7-0363dabd727f",
                    "sense_definition": "The city of Sacramento, California, United States.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "place_name_sacramento",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We are planning a road trip to <w>Sac</w> to visit some friends this weekend.",
                            "target_lang_translations": {
                                "ru": "Мы планируем поездку в <w>Сак</w>, чтобы навестить друзей в эти выходные."
                            }
                        },
                        {
                            "text": "Locals often refer to the capital simply as <w>Sac</w> rather than using its full name.",
                            "target_lang_translations": {
                                "ru": "Местные жители часто называют столицу просто <w>Сак</w>, а не полным именем."
                            }
                        }

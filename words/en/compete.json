{
    "word_family": [
        "competition",
        "competitor",
        "competitive",
        "competitively"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dda86fc9-84b5-3dfa-a08a-a94288930b6b",
                    "sense_definition": "To try to be more successful or better than someone else who is trying to do the same thing.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rivalry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Several large companies <w>compete</w> for the government contract.",
                            "target_lang_translations": {
                                "ru": "Несколько крупных компаний <w>конкурируют</w> за правительственный контракт."
                            }
                        },
                        {
                            "text": "It is difficult for a small business to <w>compete</w> against a global corporation.",
                            "target_lang_translations": {
                                "ru": "Малому бизнесу трудно <w>конкурировать</w> с глобальной корпорацией."
                            }
                        }
                    ],
                    "synonyms": [
                        "contend",
                        "vie"
                    ],
                    "antonyms": [
                        "cooperate"
                    ],
                    "common_phrases": [
                        "compete for",
                        "compete against",
                        "compete with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пытаться быть успешнее или лучше других, стремящихся к той же цели"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конкурировать",
                                "target_lang_sense_clarification": "в бизнесе или борьбе за ресурсы"
                            },
                            {
                                "target_lang_word": "соперничать",
                                "target_lang_sense_clarification": "бороться за первенство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8cd1561-c5d6-3f2c-8ca9-d2481da5b2f5",
                    "sense_definition": "To take part in a contest, race, or game.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "participation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Athletes from all over the world will <w>compete</w> in the Olympic Games.",
                            "target_lang_translations": {
                                "ru": "Спортсмены со всего мира будут <w>соревноваться</w> на Олимпийских играх."
                            }
                        },
                        {
                            "text": "She trained very hard to <w>compete</w> in the marathon this year.",
                            "target_lang_translations": {
                                "ru": "Она очень усердно тренировалась, чтобы <w>соревноваться</w> в марафоне в этом году."
                            }
                        }
                    ],
                    "synonyms": [
                        "participate",
                        "take part"
                    ],
                    "common_phrases": [
                        "compete in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать участие в соревновании, гонке или игре"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соревноваться",
                                "target_lang_sense_clarification": "в спорте или игре"
                            },
                            {
                                "target_lang_word": "состязаться",
                                "target_lang_sense_clarification": "меряться силами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa13d2d4-7bad-3f64-a71d-6425f7b5ab82",
                    "sense_definition": "To be good enough or successful enough to exist alongside others in a market or activity.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rivalry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Local shops often struggle to <w>compete</w> on price with big supermarkets.",
                            "target_lang_translations": {
                                "ru": "Местным магазинам часто трудно <w>конкурировать</w> по цене с большими супермаркетами."
                            }
                        },
                        {
                            "text": "We need to innovate constantly if we want to <w>compete</w> globally.",
                            "target_lang_translations": {
                                "ru": "Нам нужно постоянно внедрять инновации, если мы хотим <w>конкурировать</w> на глобальном уровне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "compete on price",
                        "compete globally"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть достаточно хорошим или успешным, чтобы выдерживать сравнение с другими на рынке"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конкурировать",
                                "target_lang_sense_clarification": "выдерживать конкуренцию"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
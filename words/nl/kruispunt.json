            "senses": [
                {
                    "sense_id": "8eb30c27-f717-32c3-bc70-8d64371aa79d",
                    "sense_definition": "Een plaats waar twee of meer wegen elkaar kruisen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "traffic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als u bij een <w>kruispunt</w> komt, slaat u linksaf en volgt u de borden.",
                            "target_lang_translations": {
                                "en": "When you come to an <w>intersection</w>, turn left and follow the signs.",
                                "ru": "Когда вы доедете до <w>перекрёстка</w>, поверните налево и следуйте указателям."
                            }
                        },
                        {
                            "text": "Op dit drukke <w>kruispunt</w> gebeuren helaas vaak ongelukken.",
                            "target_lang_translations": {
                                "en": "Unfortunately, accidents often happen at this busy <w>intersection</w>.",
                                "ru": "К сожалению, на этом оживлённом <w>перекрёстке</w> часто случаются аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "kruising"
                    ],
                    "common_phrases": [
                        "op een kruispunt",
                        "gevaarlijk kruispunt",
                        "naderen van een kruispunt"
                    ],
                    "target_lang_definitions": {
                        "en": "A place where two or more roads cross each other.",
                        "ru": "Место, где пересекаются две или более дороги."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "intersection",
                                "target_lang_sense_clarification": "traffic"
                            },
                            {
                                "target_lang_word": "crossroads",
                                "target_lang_sense_clarification": "junction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "перекрёсток",
                                "target_lang_sense_clarification": "место пересечения дорог"
                            },
                            {
                                "target_lang_word": "пересечение",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b43754d4-8d49-3d7c-bfd0-26b418c1b89f",
                    "sense_definition": "Een moment waarop een belangrijke beslissing genomen moet worden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zijn studie stond hij op een <w>kruispunt</w> in zijn leven en wist niet wat te doen.",
                            "target_lang_translations": {
                                "en": "After his studies, he stood at a <w>crossroads</w> in his life and didn't know what to do.",
                                "ru": "После учебы он стоял на <w>перепутье</w> в своей жизни и не знал, что делать."
                            }
                        },
                        {
                            "text": "Het bedrijf bevindt zich op een <w>kruispunt</w> en moet kiezen voor vernieuwing of traditie.",
                            "target_lang_translations": {
                                "en": "The company is at a <w>crossroads</w> and must choose between innovation or tradition.",
                                "ru": "Компания находится на <w>перепутье</w> и должна выбирать между обновлением и традициями."
                            }
                        }
                    ],
                    "synonyms": [
                        "kruising"
                    ],
                    "common_phrases": [
                        "op een kruispunt staan"
                    ],
                    "target_lang_definitions": {
                        "en": "A moment when an important decision must be made.",
                        "ru": "Момент, когда необходимо принять важное решение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crossroads",
                                "target_lang_sense_clarification": "figurative"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "перепутье",
                                "target_lang_sense_clarification": "жизненный выбор"
                            },
                            {
                                "target_lang_word": "перекрёсток",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6dcecd6-b273-3dcc-bd6f-480f56484c0b",
                    "sense_definition": "Het punt waar twee lijnen elkaar snijden.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Teken een stip op het <w>kruispunt</w> van de horizontale en verticale lijn.",
                            "target_lang_translations": {
                                "en": "Draw a dot at the <w>intersection</w> of the horizontal and vertical lines.",
                                "ru": "Нарисуйте точку на <w>пересечении</w> горизонтальной и вертикальной линий."
                            }
                        },
                        {
                            "text": "Het <w>kruispunt</w> van de diagonalen ligt precies in het midden van het vierkant.",
                            "target_lang_translations": {
                                "en": "The <w>intersection</w> of the diagonals lies exactly in the center of the square.",
                                "ru": "<w>Пересечение</w> диагоналей находится точно в центре квадрата."
                            }
                        }
                    ],
                    "synonyms": [
                        "kruising"
                    ],
                    "target_lang_definitions": {
                        "en": "The point where two lines intersect.",
                        "ru": "Точка, где пересекаются две линии."
                    },
                    "translations": {
                        "en": [

            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "151aeb0a-915f-3c90-9001-480a85bc87a6",
                    "sense_definition": "Вводное слово, выражающее уверенность говорящего в истинности сообщаемого; само собой разумеется, без сомнения.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "certainty_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы, <w>конечно</w>, придём к тебе в гости в следующую субботу.",
                            "target_lang_translations": {
                                "en": "We will, <w>of course</w>, come visit you next Saturday."
                            }
                        },
                        {
                            "text": "<w>Конечно</w>, учить новый язык непросто, но это очень интересно.",
                            "target_lang_translations": {
                                "en": "<w>Of course</w>, learning a new language is not easy, but it is very interesting."
                            }
                        }
                    ],
                    "synonyms": [
                        "разумеется",
                        "естественно",
                        "несомненно",
                        "само собой",
                        "действительно",
                        "определённо",
                        "однозначно",
                        "само собой разумеется"
                    ],
                    "common_phrases": [
                        "ну конечно",
                        "конечно же",
                        "конечно нет"
                    ],
                    "target_lang_definitions": {
                        "en": "An introductory word expressing the speaker's certainty in the truth of the statement; naturally, without doubt."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of course",
                                "target_lang_sense_clarification": "indicating that something is obvious or expected"
                            },
                            {
                                "target_lang_word": "certainly",
                                "target_lang_sense_clarification": "emphasizing that something is true or without doubt"
                            },
                            {
                                "target_lang_word": "naturally",
                                "target_lang_sense_clarification": "as is normal or to be expected"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b909a773-b650-37ec-935a-bcd47806002d",
                    "sense_definition": "Утвердительный ответ на вопрос или просьбу, выражающий полное согласие.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "certainty_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты поможешь мне перевести этот текст? — <w>Конечно</w>!",
                            "target_lang_translations": {
                                "en": "Will you help me translate this text? — <w>Of course</w>!"
                            }
                        },
                        {
                            "text": "Можно мне взять твою ручку на минуту? — <w>Конечно</w>, бери.",
                            "target_lang_translations": {
                                "en": "May I borrow your pen for a minute? — <w>Sure</w>, go ahead."
                            }
                        }
                    ],
                    "synonyms": [
                        "да",
                        "разумеется",
                        "естественно"
                    ],
                    "common_phrases": [
                        "да конечно"
                    ],
                    "target_lang_definitions": {
                        "en": "An affirmative answer to a question or request, expressing full agreement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of course",
                                "target_lang_sense_clarification": "used to give permission or agree enthusiastically"
                            },
                            {
                                "target_lang_word": "sure",
                                "target_lang_sense_clarification": "informal expression of agreement"
                            },
                            {
                                "target_lang_word": "certainly",
                                "target_lang_sense_clarification": "formal or polite expression of agreement"
                            }
                        ]
                    }
                }
            ]
        },
        {

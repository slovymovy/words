{
    "word_family": [
        "bzykać",
        "bzyk",
        "bzykanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0c6bb961-1ae0-320c-ae58-230a53d7b280",
                    "sense_definition": "Odbyć z kimś stosunek seksualny (słowo potoczne, często sugerujące szybki akt bez głębszego uczucia).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sex_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek chwalił się kolegom, że udało mu się ją <bzyknąć> na imprezie.",
                            "target_lang_translations": {
                                "ru": "Марек хвастался коллегам, что ему удалось её <трахнуть> на вечеринке."
                            }
                        },
                        {
                            "text": "On nie szukał żony, chciał tylko kogoś szybko <bzyknąć>.",
                            "target_lang_translations": {
                                "ru": "Он не искал жену, хотел только кого-то по-быстрому <трахнуть>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przespać się",
                        "zaliczyć",
                        "przelecieć"
                    ],
                    "common_phrases": [
                        "bzyknąć kogoś",
                        "chcieć bzyknąć"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заняться сексом с кем-либо (разговорное слово, часто подразумевающее быстрый акт без глубоких чувств)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трахнуть",
                                "target_lang_sense_clarification": "грубое, сленг"
                            },
                            {
                                "target_lang_word": "чпокнуть",
                                "target_lang_sense_clarification": "сленг, часто ироничное"
                            },
                            {
                                "target_lang_word": "переспать",
                                "target_lang_sense_clarification": "более нейтральное, разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c517319f-86fe-309e-87ad-6fcfbdc3585b",
                    "sense_definition": "Wydać krótki, cichy i brzęczący dźwięk, typowy dla owadów lub szybko przelatujących małych obiektów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielka mucha <bzyknęła> głośno i usiadła na kuchennym stole.",
                            "target_lang_translations": {
                                "ru": "Большая муха громко <прожужжала> и села на кухонный стол."
                            }
                        },
                        {
                            "text": "Kula <bzyknęła> żołnierzowi tuż koło ucha, ostrzegając go przed niebezpieczeństwem.",
                            "target_lang_translations": {
                                "ru": "Пуля <вжикнула> у солдата прямо над ухом, предупреждая его об опасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "zabrzęczeć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издать короткий, тихий и жужжащий звук, типичный для насекомых или быстро пролетающих мелких объектов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прожужжать",
                                "target_lang_sense_clarification": "о насекомых"
                            },
                            {
                                "target_lang_word": "вжикнуть",
                                "target_lang_sense_clarification": "о пуле или быстром объекте"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
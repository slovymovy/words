            "senses": [
                {
                    "sense_id": "47089b1d-d4d9-3765-9234-7eb63e687fa6",
                    "sense_definition": "Wskazuje na to, że dany stan lub czynność trwa nadal i nie uległy zmianie w czasie mówienia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_continuity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat <w>jeszcze</w> śpi, więc musimy być cicho."
                        },
                        {
                            "text": "Czy sklep spożywczy jest <w>jeszcze</w> otwarty o tej godzinie?"
                        }
                    ],
                    "synonyms": [
                        "nadal",
                        "wciąż",
                        "w dalszym ciągu"
                    ],
                    "antonyms": [
                        "już"
                    ],
                    "common_phrases": [
                        "jeszcze chwilę",
                        "jeszcze teraz",
                        "jeszcze dziś"
                    ]
                },
                {
                    "sense_id": "1f55c44e-aa7f-36fa-b15b-701e85b0d243",
                    "sense_definition": "Wskazuje, że oczekiwana czynność lub stan nie nastąpiły do tej pory (używane z przeczeniem).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_continuity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety <w>jeszcze</w> nie zrobiłem tego trudnego zadania domowego."
                        },
                        {
                            "text": "Pociąg z Warszawy <w>jeszcze</w> nie przyjechał na stację."
                        }
                    ],
                    "synonyms": [
                        "do tej pory",
                        "dotąd"
                    ],
                    "antonyms": [
                        "już"
                    ],
                    "common_phrases": [
                        "jeszcze nie",
                        "jeszcze nigdy",
                        "a nie jeszcze"
                    ]
                },
                {
                    "sense_id": "00e2a418-8e04-329d-92a9-65f96cee8c3b",
                    "sense_definition": "Wskazuje na dodanie elementu do istniejącej całości, powtórzenie czynności lub zwiększenie liczby.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poproszę <w>jeszcze</w> jedną kawę i kawałek ciasta."
                        },
                        {
                            "text": "Muszę to sprawdzić <w>jeszcze</w> raz, żeby mieć pewność."
                        }
                    ],
                    "common_phrases": [

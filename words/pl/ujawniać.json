                },
                {
                    "sense_id": "a1e061b5-d6d6-3b15-bc0a-30e3cd29bb17",
                    "sense_definition": "Stawać się widocznym, wiadomym lub wychodzić na jaw (o cechach, faktach, zjawiskach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "becoming_known",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prawdziwy charakter człowieka często zaczyna się <ujawniać> w sytuacjach stresowych.",
                            "target_lang_translations": {
                                "ru": "Истинный характер человека часто начинает <проявляться> в стрессовых ситуациях."
                            }

                {
                    "sense_id": "ab294214-1a51-3eae-b1c6-28c7d1d5abc8",
                    "sense_definition": "Geestelijk niet helder of in de war; niet goed wetend wat men moet doen of denken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>verwarde</w> man wist niet meer waar hij woonde.",
                            "target_lang_translations": {
                                "en": "The <w>confused</w> man no longer knew where he lived.",
                                "ru": "<w>Растерянный</w> мужчина больше не знал, где он живёт."
                            }
                        },
                        {
                            "text": "Ze keek me <w>verward</w> aan toen ik haar een vraag in het Frans stelde.",
                            "target_lang_translations": {
                                "en": "She looked at me <w>confused</w> when I asked her a question in French.",
                                "ru": "Она посмотрела на меня <w>растерянно</w>, когда я задал ей вопрос по-французски."
                            }
                        }

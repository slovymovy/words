    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e549a2f8-f175-3e91-9f48-2f4271904a7e",
                    "sense_definition": "Zdecydować się na jedną rzecz, osobę lub możliwość spośród wielu dostępnych opcji.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "selection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <wybrać> jeden kolor, który najbardziej ci się podoba.",
                            "target_lang_translations": {
                                "ru": "Ты должен <выбрать> один цвет, который тебе больше всего нравится."
                            }
                        },
                        {
                            "text": "Nie wiem, co <wybrać> z menu na dzisiejszy obiad.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, что <выбрать> из меню на сегодняшний обед."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyselekcjonować",
                        "dobrać"
                    ],
                    "antonyms": [
                        "odrzucić"
                    ],
                    "common_phrases": [
                        "wybrać numer",
                        "wybrać zawód",
                        "wybrać drogę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решить взять одну вещь, человека или вариант из множества доступных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбрать",
                                "target_lang_sense_clarification": "из множества вариантов"
                            },
                            {
                                "target_lang_word": "подобрать",
                                "target_lang_sense_clarification": "найти подходящее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e85ff5bc-018a-3201-88c9-29eb40bf5773",
                    "sense_definition": "W połączeniu z zaimkiem zwrotnym „się”: udać się w jakieś miejsce, wyruszyć w podróż lub na spacer.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym tygodniu planujemy <wybrać> się na wycieczkę w góry.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе мы планируем <отправиться> на экскурсию в горы."
                            }
                        },
                        {
                            "text": "Czy chcesz <wybrać> się ze mną do kina w sobotę?",
                            "target_lang_translations": {
                                "ru": "Ты хочешь <пойти> со мной в кино в субботу?"
                            }
                        }
                    ],
                    "synonyms": [
                        "udać się",
                        "wyruszyć"
                    ],
                    "antonyms": [
                        "zostać",
                        "wrócić"
                    ],
                    "common_phrases": [
                        "wybrać się na spacer",
                        "wybrać się w podróż",
                        "wybrać się do miasta"
                    ],
                    "target_lang_definitions": {
                        "ru": "В сочетании с возвратным местоимением «się»: пойти или поехать куда-либо, отправиться в путешествие или на прогулку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправиться",
                                "target_lang_sense_clarification": "в путь, в поездку"
                            },
                            {
                                "target_lang_word": "пойти",
                                "target_lang_sense_clarification": "куда-либо (пешком или вообще)"
                            },
                            {
                                "target_lang_word": "собраться",
                                "target_lang_sense_clarification": "намереваться пойти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2b191dc-3111-306c-a627-2e3ee46a195b",
                    "sense_definition": "Powołać kogoś na stanowisko lub funkcję w drodze głosowania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "selection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkańcy miasta <wybrali> nowego burmistrza w pierwszej turze wyborów.",
                            "target_lang_translations": {
                                "ru": "Жители города <выбрали> нового мэра в первом туре выборов."
                            }
                        },
                        {
                            "text": "Na przewodniczącego klasy <wybrano> ucznia z najlepszymi ocenami.",
                            "target_lang_translations": {
                                "ru": "Председателем класса <выбрали> ученика с лучшими оценками."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyłonić"
                    ],
                    "antonyms": [
                        "odwołać"
                    ],
                    "common_phrases": [
                        "wybrać prezydenta",
                        "wybrać zarząd",
                        "wybrać władze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Назначить кого-либо на должность или функцию путем голосования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбрать",
                                "target_lang_sense_clarification": "голосованием"
                            },
                            {
                                "target_lang_word": "избрать",
                                "target_lang_sense_clarification": "официально/политически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6c9bce6-2fdc-3888-8bcf-1215558275dc",
                    "sense_definition": "Wydobyć zawartość czegoś, opróżnić miejsce z czegoś sypkiego lub płynnego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robotnicy musieli ręcznie <wybrać> gruz z zasypanej piwnicy.",
                            "target_lang_translations": {
                                "ru": "Рабочим пришлось вручную <выбрать> строительный мусор из засыпанного подвала."
                            }
                        },
                        {
                            "text": "Po ulewie trzeba było wiadrami <wybrać> wodę z zalanej łódki.",
                            "target_lang_translations": {
                                "ru": "После ливня нужно было ведрами <вычерпать> воду из залитой лодки."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydobyć",
                        "opróżnić",
                        "wyczerpać"
                    ],
                    "antonyms": [
                        "zasypać",
                        "napełnić"
                    ],
                    "common_phrases": [
                        "wybrać ziemię",
                        "wybrać wodę",
                        "wybrać piasek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Извлечь содержимое, освободить место от чего-либо сыпучего или жидкого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбрать",
                                "target_lang_sense_clarification": "извлечь полностью"
                            },
                            {
                                "target_lang_word": "вычерпать",
                                "target_lang_sense_clarification": "жидкость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3dc4f89b-c818-3a2e-81e2-097f84e44e52",
                    "sense_definition": "Pociągnąć linę, łańcuch lub sznur w celu ich naprężenia i zlikwidowania luzu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żeglarz musiał szybko <wybrać> linę, aby poprawić ustawienie żagla.",
                            "target_lang_translations": {
                                "ru": "Моряк должен был быстро <выбрать> трос, чтобы поправить положение паруса."
                            }
                        },
                        {
                            "text": "Należy powoli <wybrać> luz na łańcuchu kotwicznym.",
                            "target_lang_translations": {
                                "ru": "Нужно медленно <выбрать> слабину на якорной цепи."
                            }
                        }
                    ],
                    "synonyms": [
                        "naciągnąć",
                        "naprężyć"
                    ],
                    "antonyms": [
                        "poluzować",
                        "popuścić"
                    ],
                    "common_phrases": [
                        "wybrać luz",
                        "wybrać szoty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потянуть трос, цепь или веревку, чтобы натянуть их и убрать провисание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбрать",
                                "target_lang_sense_clarification": "морской термин (слабину)"
                            },
                            {
                                "target_lang_word": "натянуть",
                                "target_lang_sense_clarification": "сделать тугим"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
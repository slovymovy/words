{
    "word_family": [
        "yourself",
        "yourselves"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "606219a0-6d47-34fc-8fe2-cadbe545aa1f",
                    "sense_definition": "The person or people that the speaker is addressing.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "address_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Are <w>you</w> ready to go to the park?",
                            "target_lang_translations": {
                                "ru": "<w>Ты</w> готов пойти в парк?"
                            }
                        },
                        {
                            "text": "I bought a gift for <w>you</w>.",
                            "target_lang_translations": {
                                "ru": "Я купил <w>тебе</w> подарок."
                            }
                        },
                        {
                            "text": "Thank <w>you</w> for helping me yesterday.",
                            "target_lang_translations": {
                                "ru": "Спасибо <w>тебе</w> за то, что помог мне вчера."
                            }
                        }
                    ],
                    "synonyms": [
                        "y'all",
                        "thou"
                    ],
                    "common_phrases": [
                        "thank you",
                        "how are you",
                        "nice to meet you",
                        "see you",
                        "bless you"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или люди, к которым обращается говорящий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ты",
                                "target_lang_sense_clarification": "неформальное обращение к одному человеку"
                            },
                            {
                                "target_lang_word": "вы",
                                "target_lang_sense_clarification": "вежливое обращение или обращение к группе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb6a0063-41ff-39ed-9b4c-61ca33d95bac",
                    "sense_definition": "Any person in general; anyone.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "generic_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>You</w> learn something new every day.",
                            "target_lang_translations": {
                                "ru": "Каждый день <w>ты</w> узнаешь что-то новое."
                            }
                        },
                        {
                            "text": "In this city, <w>you</w> can find food from all over the world.",
                            "target_lang_translations": {
                                "ru": "В этом городе <w>ты</w> можешь найти еду со всего мира."
                            }
                        },
                        {
                            "text": "<w>You</w> never know what might happen in the future.",
                            "target_lang_translations": {
                                "ru": "<w>Ты</w> никогда не знаешь, что может случиться в будущем."
                            }
                        }
                    ],
                    "synonyms": [
                        "one",
                        "people"
                    ],
                    "common_phrases": [
                        "you never know",
                        "what can you do",
                        "if you like"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой человек вообще; кто угодно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ты",
                                "target_lang_sense_clarification": "в обобщенном значении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7fc1b00-c2b4-3fdd-b612-d05f28f6cfcd",
                    "sense_definition": "Yourself; for yourself (used as a reflexive or datival object).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reflexive_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You had better get <w>you</w> a coat before it starts raining.",
                            "target_lang_translations": {
                                "ru": "Лучше купи <w>себе</w> пальто, пока не пошел дождь."
                            }
                        },
                        {
                            "text": "Sit <w>you</w> down and rest for a while.",
                            "target_lang_translations": {
                                "ru": "Усади <w>себя</w> и отдохни немного."
                            }
                        }
                    ],
                    "synonyms": [
                        "yourself"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "common in some US dialects and archaic English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Себя; для себя (используется как возвратное или дательное местоимение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "себе",
                                "target_lang_sense_clarification": "возвратное значение"
                            },
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "возвратное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec4a2b38-1d50-3af7-bb02-327de3a26c15",
                    "sense_definition": "The sports team that the person being addressed supports.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sports_metonymy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I can't believe <w>you</w> beat us in the final minute of the game.",
                            "target_lang_translations": {
                                "ru": "Не могу поверить, что <w>вы</w> победили нас на последней минуте игры."
                            }
                        },
                        {
                            "text": "<w>You</w> have a good chance of winning the league this year.",
                            "target_lang_translations": {
                                "ru": "У <w>вас</w> хорошие шансы выиграть лигу в этом году."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in sports contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивная команда, за которую болеет собеседник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вы",
                                "target_lang_sense_clarification": "подразумевается команда"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "708ca5fa-ab27-3989-9064-79d68f31c735",
                    "sense_definition": "Used before a noun to specify the particular person or group being addressed.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "determiner_specifying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do <w>you</w> guys want to go to the cinema tonight?",
                            "target_lang_translations": {
                                "ru": "<w>Вы</w>, ребята, хотите пойти в кино сегодня вечером?"
                            }
                        },
                        {
                            "text": "<w>You</w> two should stop arguing and shake hands.",
                            "target_lang_translations": {
                                "ru": "<w>Вы</w> двое должны прекратить спорить и пожать друг другу руки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "you guys",
                        "you two",
                        "you lot",
                        "you people"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется перед существительным для уточнения конкретного лица или группы, к которым обращаются."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вы",
                                "target_lang_sense_clarification": "множественное число"
                            },
                            {
                                "target_lang_word": "ты",
                                "target_lang_sense_clarification": "единственное число"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5770896-23db-3cc4-a94e-87bcc6339a99",
                    "sense_definition": "Used before a description of a person to express strong emotion, such as anger, contempt, or admiration.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "determiner_emphatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>You</w> idiot, look what you have done to the car!",
                            "target_lang_translations": {
                                "ru": "<w>Ты</w>, идиот, посмотри, что ты наделал с машиной!"
                            }
                        },
                        {
                            "text": "<w>You</w> genius, that is the perfect solution.",
                            "target_lang_translations": {
                                "ru": "<w>Ты</w>, гений, это идеальное решение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "you idiot",
                        "you fool",
                        "you beauty"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "emphatic address"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется перед описанием человека для выражения сильных эмоций, таких как гнев, презрение или восхищение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ты",
                                "target_lang_sense_clarification": "эмфатическое обращение"
                            }
                        ]
                    }
                }
            ]
        },
        {

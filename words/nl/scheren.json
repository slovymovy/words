            "senses": [
                {
                    "sense_id": "38f549af-dfec-36a1-b691-70ae20fdec89",
                    "sense_definition": "Haar van de huid verwijderen met een mesje of scheerapparaat.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hair_removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <scheert> zich elke ochtend voor hij naar zijn werk gaat.",
                            "target_lang_translations": {
                                "en": "He <shaves> every morning before he goes to work.",
                                "ru": "Он <бреется> каждое утро перед уходом на работу."
                            }
                        },
                        {
                            "text": "In de zomer <scheren> veel mensen hun benen.",
                            "target_lang_translations": {
                                "en": "In the summer, many people <shave> their legs.",
                                "ru": "Летом многие люди <бреют> ноги."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontharen"
                    ],
                    "common_phrases": [
                        "zich scheren",
                        "glad scheren"
                    ],
                    "target_lang_definitions": {
                        "en": "To remove hair from the skin using a blade or shaver.",
                        "ru": "Удалять волосы с кожи с помощью лезвия или бритвы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shave",
                                "target_lang_sense_clarification": "remove body hair"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "брить",
                                "target_lang_sense_clarification": "удалять волосы"
                            },
                            {
                                "target_lang_word": "бриться",
                                "target_lang_sense_clarification": "удалять волосы у себя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d608e701-9caa-3f65-8255-cd61d5a2c61d",
                    "sense_definition": "De vacht of wol van een dier (meestal een schaap) afknippen of afsnijden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "hair_removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het voorjaar worden de schapen op de boerderij <geschoren>.",
                            "target_lang_translations": {
                                "en": "In the spring, the sheep on the farm are <sheared>.",
                                "ru": "Весной овец на ферме <стригут>."
                            }
                        },
                        {
                            "text": "De boer <scheert> de dikke laag wol van het dier.",
                            "target_lang_translations": {
                                "en": "The farmer <shears> the thick layer of wool off the animal.",
                                "ru": "Фермер <состригает> толстый слой шерсти с животного."
                            }
                        }
                    ],
                    "common_phrases": [
                        "schapen scheren"
                    ],
                    "target_lang_definitions": {
                        "en": "To cut or clip the fleece or wool off an animal (usually a sheep).",
                        "ru": "Срезать шерсть или мех с животного (обычно овцы)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shear",
                                "target_lang_sense_clarification": "cut wool from an animal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "стричь",
                                "target_lang_sense_clarification": "шерсть, животных"
                            },
                            {
                                "target_lang_word": "остригать",
                                "target_lang_sense_clarification": "срезать шерсть"
                            }
                        ]

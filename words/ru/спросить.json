{
    "word_family": [
        "вопрос",
        "спрос",
        "просьба",
        "спрашивать",
        "переспросить",
        "расспросить",
        "допросить",
        "выпросить",
        "запросить",
        "опросить"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "16602c74-a9db-361c-b7bd-8a5a7fff1b99",
                    "sense_definition": "Обратиться к кому-либо с вопросом, чтобы узнать или выяснить что-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Турист <w>спросил</w> у прохожего, как пройти к ближайшей станции метро.",
                            "target_lang_translations": {
                                "en": "The tourist <w>asked</w> a passerby how to get to the nearest metro station."
                            }
                        },
                        {
                            "text": "Я хочу <w>спросить</w> совета у своего старого друга.",
                            "target_lang_translations": {
                                "en": "I want to <w>ask</w> my old friend for advice."
                            }
                        }
                    ],
                    "synonyms": [
                        "поинтересоваться",
                        "осведомиться"
                    ],
                    "antonyms": [
                        "ответить"
                    ],
                    "common_phrases": [
                        "спросить совета",
                        "спросить дорогу",
                        "спросить разрешения",
                        "спросить время"
                    ],
                    "target_lang_definitions": {
                        "en": "To address someone with a question in order to learn or find out something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "a question"
                            },
                            {
                                "target_lang_word": "inquire",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e3882a8-f3a9-3b7a-8347-a5459f2eb4de",
                    "sense_definition": "Обратиться с просьбой дать или сделать что-либо.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Посетитель <w>спросил</w> в библиотеке новую книгу известного автора.",
                            "target_lang_translations": {
                                "en": "The visitor <w>asked for</w> a new book by a famous author at the library."
                            }
                        },
                        {
                            "text": "Мы зашли в кафе и <w>спросили</w> меню у официанта.",
                            "target_lang_translations": {
                                "en": "We went into a cafe and <w>asked for</w> the menu from the waiter."
                            }
                        }
                    ],
                    "synonyms": [
                        "попросить"
                    ],
                    "common_phrases": [
                        "спросить книгу",
                        "спросить воды"
                    ],
                    "target_lang_definitions": {
                        "en": "To request that something be given or done."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask for",
                                "target_lang_sense_clarification": "an item or service"
                            },
                            {
                                "target_lang_word": "request",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5ba25f5-dc3d-3243-9075-62cf1ea7df62",
                    "sense_definition": "Вызвать кого-либо, выразить желание видеть кого-либо.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Секретарь сообщила, что меня <w>спросил</w> директор компании.",
                            "target_lang_translations": {
                                "en": "The secretary reported that the company director had <w>asked for</w> me."
                            }
                        },
                        {
                            "text": "Кто-то позвонил в дверь и <w>спросил</w> хозяина квартиры.",
                            "target_lang_translations": {
                                "en": "Someone rang the doorbell and <w>asked for</w> the owner of the apartment."
                            }
                        }
                    ],
                    "synonyms": [
                        "попросить",
                        "потребовать",
                        "поинтересоваться"
                    ],
                    "common_phrases": [
                        "спросить начальника",
                        "спросить врача"
                    ],
                    "target_lang_definitions": {
                        "en": "To call for someone or express a desire to see someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask for",
                                "target_lang_sense_clarification": "someone's presence"
                            },
                            {
                                "target_lang_word": "ask to see",
                                "target_lang_sense_clarification": "someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44c16e39-f1ce-3d73-8843-8e8a6dfd8a61",
                    "sense_definition": "Назначить определённую цену за товар или услугу.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Продавец на рынке <w>спросил</w> за эти яблоки слишком высокую цену.",
                            "target_lang_translations": {
                                "en": "The seller at the market <w>asked</w> too high a price for these apples."
                            }
                        },
                        {
                            "text": "Мастер <w>спросил</w> с нас пять тысяч рублей за ремонт телевизора.",
                            "target_lang_translations": {
                                "en": "The repairman <w>charged</w> us five thousand rubles for the TV repair."
                            }
                        }
                    ],
                    "synonyms": [
                        "потребовать",
                        "попросить"
                    ],
                    "common_phrases": [
                        "спросить дорого",
                        "спросить цену",
                        "сколько спросить"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To set a specific price for a product or service."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "a price"
                            },
                            {
                                "target_lang_word": "charge",
                                "target_lang_sense_clarification": "money"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8211d00-a4ae-3391-8cf7-41d5a75c50c0",
                    "sense_definition": "Призвать к ответственности, потребовать отчёта или объяснений за чьи-либо действия.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник строго <w>спросит</w> с сотрудников за невыполнение плана.",
                            "target_lang_translations": {
                                "en": "The boss will strictly <w>hold</w> the employees <w>accountable</w> for failure to meet the plan."
                            }
                        },
                        {
                            "text": "Если мы допустим ошибку, с нас <w>спросят</w> по всей строгости.",
                            "target_lang_translations": {
                                "en": "If we make a mistake, we will be <w>held accountable</w> to the fullest extent."
                            }
                        }
                    ],
                    "synonyms": [
                        "потребовать"
                    ],
                    "common_phrases": [
                        "строго спросить",
                        "спросить за ошибки",
                        "спросить по всей строгости"
                    ],
                    "target_lang_definitions": {
                        "en": "To call to account or demand an explanation for someone's actions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold accountable",
                                "target_lang_sense_clarification": "for actions"
                            },
                            {
                                "target_lang_word": "call to account",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_id": "b18d0c4e-f162-3c7c-b29d-ff7fc73d65a3",
                    "sense_definition": "Present participle and gerund of <w>breathe</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The patient is <w>breathing</w> on his own now.",
                            "target_lang_translations": {
                                "ru": "Пациент теперь <w>дышит</w> самостоятельно."
                            }
                        },
                        {
                            "text": "She was <w>breathing</w> heavily after running up the stairs.",
                            "target_lang_translations": {
                                "ru": "Она тяжело <w>дышала</w>, взбежав по лестнице."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stopped breathing",
                        "difficulty breathing",
                        "barely breathing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола <w>breathe</w> (дышать)."

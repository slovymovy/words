            "senses": [
                {
                    "sense_id": "5ecdc121-3c20-34fc-bcb9-f5fd9b4fbc09",
                    "sense_definition": "Дать, вручить что-либо кому-либо в руки",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transfer_physical",
                    "name_type": "no",

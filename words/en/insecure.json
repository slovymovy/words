{
    "word_family": [
        "insecurity",
        "insecurely",
        "secure",
        "security"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "34eb52f6-fd48-37b8-b26a-9668ee5d1cd0",
                    "sense_definition": "Not comfortable or confident in oneself or in certain situations.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She often feels <w>insecure</w> about her appearance when she compares herself to others.",
                            "target_lang_translations": {
                                "ru": "Она часто чувствует себя <w>неуверенно</w> из-за своей внешности, когда сравнивает себя с другими."
                            }
                        },
                        {
                            "text": "It is normal to feel <w>insecure</w> when starting a new job with difficult tasks.",
                            "target_lang_translations": {
                                "ru": "Это нормально — чувствовать себя <w>неуверенно</w>, начиная новую работу со сложными задачами."
                            }
                        }
                    ],
                    "synonyms": [
                        "unconfident",
                        "uncertain",
                        "anxious"
                    ],
                    "antonyms": [
                        "confident",
                        "self-confident"
                    ],
                    "common_phrases": [
                        "feel insecure",
                        "insecure about",
                        "deeply insecure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не чувствующий уверенности в себе или испытывающий беспокойство в определенных ситуациях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неуверенный",
                                "target_lang_sense_clarification": "в себе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b477386-303a-3eec-90e5-7f8a658a23a6",
                    "sense_definition": "Not firmly fixed or likely to give way; not safe or protected.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old wooden bridge looked <w>insecure</w>, so we decided not to cross it.",
                            "target_lang_translations": {
                                "ru": "Старый деревянный мост выглядел <w>ненадёжным</w>, поэтому мы решили не переходить по нему."
                            }
                        },
                        {
                            "text": "You should never send passwords over an <w>insecure</w> internet connection.",
                            "target_lang_translations": {
                                "ru": "Никогда не следует отправлять пароли через <w>небезопасное</w> интернет-соединение."
                            }
                        }
                    ],
                    "synonyms": [
                        "unsafe",
                        "unstable",
                        "precarious"
                    ],
                    "antonyms": [
                        "confident",
                        "self-confident"
                    ],
                    "common_phrases": [
                        "insecure connection",
                        "insecure foundation",
                        "financially insecure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непрочно закреплённый, неустойчивый или не обеспечивающий безопасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ненадёжный",
                                "target_lang_sense_clarification": "шаткий, непрочный"
                            },
                            {
                                "target_lang_word": "небезопасный",
                                "target_lang_sense_clarification": "незащищённый"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
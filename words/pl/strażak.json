            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0c600ca7-0ab8-3002-a596-c58f8eb9a29b",
                    "sense_definition": "Osoba przeszkolona i zajmująca się zawodowo lub ochotniczo gaszeniem pożarów oraz ratownictwem w nagłych wypadkach.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "professions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzielny <w>strażak</w> wyniósł przestraszone dziecko z płonącego budynku.",
                            "target_lang_translations": {
                                "ru": "Отважный <w>пожарный</w> вынес испуганного ребёнка из горящего здания."
                            }
                        },
                        {
                            "text": "Strażakom udało się szybko ugasić pożar w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "<w>Пожарным</w> удалось быстро потушить пожар в центре города."
                            }
                        },
                        {
                            "text": "Święty Florian jest znanym patronem wszystkich <w>strażaków</w>.",
                            "target_lang_translations": {
                                "ru": "Святой Флориан — известный покровитель всех <w>пожарных</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pożarnik"
                    ],
                    "common_phrases": [
                        "zostać strażakiem",
                        "dzielny strażak",
                        "wezwać strażaków",
                        "interwencja strażaka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, профессионально или на добровольной основе занимающийся тушением пожаров и спасательными работами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожарный",
                                "target_lang_sense_clarification": "профессия"
                            },
                            {
                                "target_lang_word": "пожарник",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee110169-acfd-3f6c-8a08-58ace77e1f47",
                    "sense_definition": "Najniższy stopień służbowy w strukturze Państwowej Straży Pożarnej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ranks",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po zakończeniu szkolenia podstawowego otrzymał stopień <w>strażaka</w>.",
                            "target_lang_translations": {
                                "ru": "После завершения начальной подготовки он получил звание <w>пожарного</w>."
                            }
                        },
                        {
                            "text": "Jego pierwszym awansem było przejście ze stopnia <w>strażaka</w> na starszego strażaka.",
                            "target_lang_translations": {
                                "ru": "Его первым повышением был переход со звания <w>пожарного</w> на старшего пожарного."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stopień strażaka",
                        "mianowanie na strażaka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Низшее служебное звание в структуре Государственной пожарной службы."
                    },

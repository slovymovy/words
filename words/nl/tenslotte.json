    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "42b3d826-98b1-3a5b-b285-7a9c60fd5aee",
                    "sense_definition": "Om aan te geven dat iets als laatste punt komt in een reeks, opsomming of redenering.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil <w>tenslotte</w> nog even mijn collega's bedanken voor hun harde werk.",
                            "target_lang_translations": {
                                "en": "<w>Finally</w>, I would like to thank my colleagues for their hard work."
                            }
                        },
                        {
                            "text": "We hebben de hele dag gewerkt en zijn <w>tenslotte</w> naar huis gegaan.",
                            "target_lang_translations": {
                                "en": "We worked all day and <w>finally</w> went home."
                            }
                        }
                    ],
                    "synonyms": [
                        "tot slot",
                        "uiteindelijk",
                        "aan het einde"
                    ],
                    "antonyms": [
                        "ten eerste",
                        "aanvankelijk",
                        "in het begin"
                    ],
                    "common_phrases": [
                        "en tenslotte",
                        "maar tenslotte"
                    ],
                    "target_lang_definitions": {
                        "en": "To indicate that something comes as the last point in a series, list, or reasoning."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "finally",
                                "target_lang_sense_clarification": "used to introduce the last point or item"
                            },
                            {
                                "target_lang_word": "lastly",
                                "target_lang_sense_clarification": "used specifically when listing points"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0674e3b-9c2d-3f4f-a894-5f4724acbd65",
                    "sense_definition": "Gebruikt om een reden, verklaring of rechtvaardiging te benadrukken; per slot van rekening.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reasoning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet goed je best doen op school, het gaat <w>tenslotte</w> om je toekomst.",
                            "target_lang_translations": {
                                "en": "You have to do your best at school; it is about your future, <w>after all</w>."
                            }
                        },
                        {
                            "text": "Het is <w>tenslotte</w> bijna vrijdag, dus we kunnen wel wat eerder stoppen.",
                            "target_lang_translations": {
                                "en": "It is almost Friday, <w>after all</w>, so we can stop a bit earlier."
                            }
                        }
                    ],
                    "synonyms": [
                        "immers",
                        "per slot van rekening",
                        "welbeschouwd"
                    ],
                    "common_phrases": [
                        "is tenslotte",
                        "je bent tenslotte"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to emphasize a reason, explanation, or justification; considering the fact that."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "after all",
                                "target_lang_sense_clarification": "used to remind someone of a fact or reason"
                            }
                        ]
                    }
                }
            ]
        }
    ]

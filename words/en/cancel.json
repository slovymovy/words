            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c2eef74d-2105-31d9-a0e9-548f07280098",
                    "sense_definition": "To decide that an organized event or arrangement will not take place; to annul an agreement.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>cancelled</w> his order on their website."
                        },
                        {
                            "text": "We had to <w>cancel</w> the picnic because of the heavy rain."
                        }
                    ],
                    "synonyms": [
                        "call off",
                        "annul",
                        "revoke",
                        "void",
                        "withdraw"
                    ],
                    "antonyms": [
                        "book",
                        "schedule",
                        "confirm",
                        "arrange"
                    ],
                    "common_phrases": [

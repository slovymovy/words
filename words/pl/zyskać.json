    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "39bf9a94-d5fb-3ab2-ae2f-d5a786a42e77",
                    "sense_definition": "Wejść w posiadanie czegoś pożądanego lub potrzebnego (np. czasu, wiedzy, przewagi); otrzymać coś jako rezultat działania.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzięki szybkiej reakcji lekarzy pacjent <zyskał> szansę na pełne wyzdrowienie.",
                            "target_lang_translations": {
                                "ru": "Благодаря быстрой реакции врачей пациент <получил> шанс на полное выздоровление."
                            }
                        },
                        {
                            "text": "Musimy się pospieszyć, aby <zyskać> trochę czasu przed przyjazdem gości.",
                            "target_lang_translations": {
                                "ru": "Мы должны поторопиться, чтобы <выиграть> немного времени до приезда гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdobyć",
                        "osiągnąć",
                        "otrzymać"
                    ],
                    "antonyms": [
                        "stracić",
                        "utracić"
                    ],
                    "common_phrases": [
                        "zyskać czas",
                        "zyskać przewagę",
                        "zyskać dostęp",
                        "zyskać pewność",
                        "zyskać sławę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить что-либо желаемое или необходимое (например, время, знания, преимущество); обрести в результате действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получить",
                                "target_lang_sense_clarification": "общий смысл: получить шанс, доступ, результат"
                            },
                            {
                                "target_lang_word": "приобрести",
                                "target_lang_sense_clarification": "знания, опыт, уверенность"
                            },
                            {
                                "target_lang_word": "выиграть",
                                "target_lang_sense_clarification": "время, преимущество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "916700ed-9d3b-3cc2-a859-8dd9d7886d04",
                    "sense_definition": "Sprawić, że ktoś zacznie nas lubić, szanować lub popierać; zdobyć czyjeś pozytywne nastawienie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_relations",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy pracownik szybko <zyskał> zaufanie swojego szefa dzięki rzetelnej pracy.",
                            "target_lang_translations": {
                                "ru": "Новый сотрудник быстро <завоевал> доверие своего начальника благодаря добросовестной работе."
                            }
                        },
                        {
                            "text": "Polityk obiecał zmiany, aby <zyskać> poparcie wyborców w nadchodzących wyborach.",
                            "target_lang_translations": {
                                "ru": "Политик обещал перемены, чтобы <заручиться> поддержкой избирателей на предстоящих выборах."
                            }
                        }
                    ],
                    "synonyms": [
                        "zjednać",
                        "pozyskać",
                        "zdobyć"
                    ],
                    "antonyms": [
                        "zrazić"
                    ],
                    "common_phrases": [
                        "zyskać zaufanie",
                        "zyskać uznanie",
                        "zyskać szacunek",
                        "zyskać przyjaciela",
                        "zyskać sympatię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добиться чьего-либо расположения, уважения или поддержки; склонить кого-либо на свою сторону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завоевать",
                                "target_lang_sense_clarification": "доверие, симпатию, уважение"
                            },
                            {
                                "target_lang_word": "заручиться",
                                "target_lang_sense_clarification": "поддержкой, согласием"
                            },
                            {
                                "target_lang_word": "заслужить",
                                "target_lang_sense_clarification": "уважение, признание, репутацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d507e37-cb13-30d2-a688-4a32e30ac90c",
                    "sense_definition": "Osiągnąć korzyść finansową lub materialną; zarobić na czymś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma <zyskała> miliony złotych na sprzedaży nowoczesnego oprogramowania.",
                            "target_lang_translations": {
                                "ru": "Компания <заработала> миллионы злотых на продаже современного программного обеспечения."
                            }
                        },
                        {
                            "text": "Kupując ten samochód teraz, możesz wiele <zyskać>, ponieważ ceny wkrótce wzrosną.",
                            "target_lang_translations": {
                                "ru": "Покупая этот автомобиль сейчас, ты можешь много <выгадать>, так как цены скоро вырастут."
                            }
                        }
                    ],
                    "synonyms": [
                        "zarobić",
                        "wzbogacić się"
                    ],
                    "antonyms": [
                        "stracić"
                    ],
                    "common_phrases": [
                        "zyskać na sprzedaży",
                        "zyskać fortunę",
                        "wiele zyskać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить финансовую или материальную выгоду; заработать на чём-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заработать",
                                "target_lang_sense_clarification": "деньги, капитал"
                            },
                            {
                                "target_lang_word": "выгадать",
                                "target_lang_sense_clarification": "сэкономить или получить преимущество при сделке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30ebbfbd-d649-3a4f-b3d0-ba80411327d4",
                    "sense_definition": "Zaprezentować się korzystniej lub stać się lepszym w określonym aspekcie (np. wyglądzie, wartości).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "improvement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary budynek bardzo <zyskał> na wyglądzie po odnowieniu elewacji.",
                            "target_lang_translations": {
                                "ru": "Старое здание очень <выиграло> во внешнем виде после обновления фасада."
                            }
                        },
                        {
                            "text": "Ta prosta potrawa <zyskała> na smaku dzięki dodaniu świeżych ziół.",
                            "target_lang_translations": {
                                "ru": "Это простое блюдо <выиграло> во вкусе благодаря добавлению свежей зелени."
                            }
                        }
                    ],
                    "synonyms": [
                        "polepszyć się"
                    ],
                    "antonyms": [
                        "stracić"
                    ],
                    "common_phrases": [
                        "zyskać na wartości",
                        "zyskać w oczach",
                        "zyskać na znaczeniu",
                        "zyskać przy bliższym poznaniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать лучше в каком-либо отношении (например, внешне); произвести более выгодное впечатление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выиграть",
                                "target_lang_sense_clarification": "выглядеть лучше, выделяться на фоне чего-то"
                            },
                            {
                                "target_lang_word": "стать лучше",
                                "target_lang_sense_clarification": "улучшить свои качества"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "cdc697f0-2bfd-3f27-a8d9-d3f4e138b19f",
                    "sense_definition": "jeden z departamentów Gwatemali, położony w południowej części kraju",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "region",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Departament <w>Gwatemala</w> charakteryzuje się bardzo dużą gęstością zaludnienia w porównaniu do reszty kraju.",
                            "target_lang_translations": {
                                "ru": "Департамент <w>Гватемала</w> характеризуется очень высокой плотностью населения по сравнению с остальной частью страны."
                            }
                        },
                        {
                            "text": "Władze administracyjne, którymi zarządza departament <w>Gwatemala</w>, ogłosiły nowy budżet na infrastrukturę.",
                            "target_lang_translations": {
                                "ru": "Административные власти, управляющие департаментом <w>Гватемала</w>, объявили о новом бюджете на инфраструктуру."
                            }
                        }

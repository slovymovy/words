                            },
                            {
                                "target_lang_word": "ходил",
                                "target_lang_sense_clarification": "морской термин для обозначения плавания судна"
                            },
                            {
                                "target_lang_word": "справился",
                                "target_lang_sense_clarification": "переносное значение: легко преодолел что-либо"
                            }
                        ]

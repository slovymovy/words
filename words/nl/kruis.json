                {
                    "sense_id": "60040847-ef94-3dc4-8233-f4a244cde69b",
                    "sense_definition": "Een belangrijk symbool in het christendom dat verwijst naar de kruisiging van Jezus, vaak uitgevoerd als voorwerp.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de oude kerk hangt een groot houten <w>kruis</w> aan de muur.",
                            "target_lang_translations": {
                                "en": "In the old church, a large wooden <w>cross</w> hangs on the wall.",
                                "ru": "В старой церкви на стене висит большой деревянный <w>крест</w>."
                            }
                        },
                        {
                            "text": "Zij draagt een gouden <w>kruis</w> aan een ketting om haar nek.",
                            "target_lang_translations": {
                                "en": "She wears a gold <w>cross</w> on a chain around her neck.",
                                "ru": "Она носит золотой <w>крестик</w> на цепочке на шее."
                            }

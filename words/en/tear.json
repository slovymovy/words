                        {
                            "text": "The team has been on a <w>tear</w>, winning ten games in a row.",
                            "target_lang_translations": {
                                "ru": "Команда была <w>в ударе</w>, выиграв десять игр подряд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go on a tear"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "mainly US"
                        }

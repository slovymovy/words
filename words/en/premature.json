    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5405ee80-6f37-397b-95ff-00183fa867e5",
                    "sense_definition": "Occurring or happening before the natural, proper, or usual time, especially regarding growth or physical events.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "timing_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor was concerned about the possibility of a <w>premature</w> birth.",
                            "target_lang_translations": {
                                "ru": "Врач был обеспокоен возможностью <w>преждевременных</w> родов."
                            }
                        },
                        {
                            "text": "His <w>premature</w> death at a young age shocked the entire community.",
                            "target_lang_translations": {
                                "ru": "Его <w>преждевременная</w> смерть в молодом возрасте потрясла всё сообщество."
                            }
                        }
                    ],
                    "synonyms": [
                        "early",
                        "untimely"
                    ],
                    "antonyms": [
                        "overdue"
                    ],
                    "common_phrases": [
                        "premature birth",
                        "premature baby",
                        "premature death",
                        "premature aging"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий или случающийся раньше естественного, надлежащего или обычного времени, особенно в отношении роста или физических событий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преждевременный",
                                "target_lang_sense_clarification": "происходящий раньше срока"
                            },
                            {
                                "target_lang_word": "ранний",
                                "target_lang_sense_clarification": "случившийся раньше обычного (о смерти, начале чего-либо)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ba47216-7856-3e5a-a877-f68d6fc390f5",
                    "sense_definition": "Done too soon or before enough preparation or consideration has been made.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "timing_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It would be <w>premature</w> to make a final decision without seeing all the data.",
                            "target_lang_translations": {
                                "ru": "Было бы <w>преждевременно</w> принимать окончательное решение, не видя всех данных."
                            }
                        },
                        {
                            "text": "The celebration was <w>premature</w> because the official results had not yet been announced.",
                            "target_lang_translations": {
                                "ru": "Празднование было <w>преждевременным</w>, так как официальные результаты ещё не были объявлены."
                            }
                        }
                    ],
                    "synonyms": [
                        "hasty",
                        "precipitate"
                    ],
                    "common_phrases": [
                        "premature decision",
                        "premature conclusion",
                        "premature announcement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный слишком рано или до того, как была проведена достаточная подготовка или рассмотрение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преждевременный",
                                "target_lang_sense_clarification": "слишком ранний"
                            },
                            {
                                "target_lang_word": "поспешный",
                                "target_lang_sense_clarification": "сделанный в спешке, необдуманный"
                            }
                        ]
                    }
                }
            ]
        },

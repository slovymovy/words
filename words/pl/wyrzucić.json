    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b700f1d2-d2c6-3be8-8128-e91d0ca51781",
                    "sense_definition": "Pozbyć się czegoś niepotrzebnego, zwykle umieszczając to w koszu na śmieci.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "discarding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <wyrzucić> te stare gazety, bo zajmują za dużo miejsca.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <выбросить> эти старые газеты, потому что они занимают слишком много места."
                            }
                        },
                        {
                            "text": "Czy możesz <wyrzucić> śmieci, kiedy będziesz wychodzić z domu?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <выбросить> мусор, когда будешь выходить из дома?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zwolnić"
                    ],
                    "antonyms": [
                        "zachować",
                        "zostawić"
                    ],
                    "common_phrases": [
                        "wyrzucić śmieci",
                        "wyrzucić do kosza",
                        "wyrzucić pieniądze w błoto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Избавиться от чего-то ненужного, обычно выбросив это в мусор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбросить",
                                "target_lang_sense_clarification": "избавиться от мусора или ненужных вещей"
                            },
                            {
                                "target_lang_word": "выкинуть",
                                "target_lang_sense_clarification": "разговорный синоним слова выбросить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a396a35a-ebe5-3aee-b377-eb9b490a76fb",
                    "sense_definition": "Zmusić kogoś do opuszczenia miejsca, organizacji, szkoły lub zwolnić z pracy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "dismissal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szef postanowił <wyrzucić> go z pracy za ciągłe spóźnienia.",
                            "target_lang_translations": {
                                "ru": "Начальник решил <выгнать> его с работы за постоянные опоздания."
                            }
                        },
                        {
                            "text": "Jeśli nie poprawisz ocen, mogą cię <wyrzucić> ze studiów.",
                            "target_lang_translations": {
                                "ru": "Если ты не исправишь оценки, тебя могут <исключить> из университета."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwolnić"
                    ],
                    "antonyms": [
                        "zatrudnić",
                        "przyjąć"
                    ],
                    "common_phrases": [
                        "wyrzucić z pracy",
                        "wyrzucić z domu",
                        "wyrzucić za drzwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то покинуть место, организацию, учебное заведение или уволить с работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выгнать",
                                "target_lang_sense_clarification": "заставить уйти; выставить вон"
                            },
                            {
                                "target_lang_word": "уволить",
                                "target_lang_sense_clarification": "освободить от должности, с работы"
                            },
                            {
                                "target_lang_word": "исключить",
                                "target_lang_sense_clarification": "из учебного заведения или организации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae5a47af-e415-3ada-b8eb-c47b7f8703e3",
                    "sense_definition": "Gwałtownym ruchem sprawić, że coś znajdzie się na zewnątrz lub w górze; rzucić czymś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Siła wybuchu <wyrzuciła> odłamki szkła na środek ulicy.",
                            "target_lang_translations": {
                                "ru": "Сила взрыва <выбросила> осколки стекла на середину улицы."
                            }
                        },
                        {
                            "text": "Morze <wyrzuciło> na brzeg kawałki drewna i bursztyny.",
                            "target_lang_translations": {
                                "ru": "Море <выбросило> на берег куски дерева и янтарь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyrzucić ręce w górę",
                        "wyrzucić piłkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резким движением переместить что-то наружу или вверх; вынести на берег (о море)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбросить",
                                "target_lang_sense_clarification": "резко кинуть или вынести волнами"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
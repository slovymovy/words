                    "frequency": "Middle",
                    "semantic_group_id": "geography",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We are taking a ferry to <w>Europe</w> for our holiday.",
                            "target_lang_translations": {
                                "ru": "Мы плывем на пароме в <w>Европу</w> в отпуск."
                            }
                        },
                        {
                            "text": "Driving in <w>Europe</w> feels different because they drive on the right side of the road.",
                            "target_lang_translations": {
                                "ru": "Вождение в <w>Европе</w> ощущается иначе, потому что там ездят по правой стороне дороги."
                            }
                        }
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f7309603-1240-3179-8afc-27ab7f1c9a33",
                    "sense_definition": "Подвергшийся абордажу; захваченный в ходе морского боя путём сцепления с судном противника.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_capture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Капитан с горечью смотрел на свой <w>абордированный</w> корабль, на палубе которого хозяйничали враги.",
                            "target_lang_translations": {
                                "en": "The captain looked bitterly at his <w>boarded</w> ship, on whose deck the enemies were now in charge."
                            }
                        },
                        {
                            "text": "<w>Абордированный</w> фрегат был взят на буксир и доставлен в порт победителей.",
                            "target_lang_translations": {
                                "en": "The <w>boarded</w> frigate was taken in tow and delivered to the victors' port."
                            }
                        }
                    ],
                    "synonyms": [
                        "захваченный"
                    ],
                    "common_phrases": [
                        "абордированное судно",
                        "абордированный корабль"
                    ],
                    "target_lang_definitions": {
                        "en": "Subjected to boarding; captured during a naval battle by grappling with an enemy vessel."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boarded",
                                "target_lang_sense_clarification": "captured by boarding (nautical)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "95183bf6-1c8b-3c3b-8842-d0e5f8f18b42",
                    "sense_definition": "Страдательное причастие прошедшего времени от глагола <w>абордировать</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_capture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Торговое судно было <w>абордировано</w> пиратами в нейтральных водах.",
                            "target_lang_translations": {
                                "en": "The merchant vessel was <w>boarded</w> by pirates in neutral waters."
                            }
                        },
                        {
                            "text": "Несмотря на отчаянное сопротивление экипажа, эсминец был <w>абордирован</w>.",
                            "target_lang_translations": {
                                "en": "Despite the desperate resistance of the crew, the destroyer was <w>boarded</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Past passive participle of the verb 'to board'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boarded",
                                "target_lang_sense_clarification": "entered by force (nautical)"
                            }
                        ]
                    }
                }
            ]
        }

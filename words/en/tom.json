    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d6075f11-485f-3639-a739-20988187e4ea",
                    "sense_definition": "A diminutive of the male given name Thomas, also used as a formal male given name.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My brother <w>Tom</w> is coming to visit us this weekend.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>Том</w> приедет навестить нас в эти выходные."
                            }
                        },
                        {
                            "text": "Please tell <w>Tom</w> that I will call him later.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, передай <w>Тому</w>, что я позвоню ему позже."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Peeping Tom",
                        "Tom Dick and Harry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма мужского имени Томас, также используемая как самостоятельное мужское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Том",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        },
        {

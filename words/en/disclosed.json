            "senses": [
                {
                    "sense_id": "2e0e7e59-213b-31ca-97b6-80e4f73d3ca9",
                    "sense_definition": "Simple past and past participle of <w>disclose</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The investigation <disclosed> several errors in the accounts.",
                            "target_lang_translations": {
                                "ru": "Расследование <выявило> несколько ошибок в счетах."
                            }
                        },
                        {
                            "text": "She has not <disclosed> her reasons for leaving the company.",
                            "target_lang_translations": {
                                "ru": "Она не <раскрыла> свои причины ухода из компании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "disclosed the truth",
                        "disclosed details"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола disclose (раскрывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрыл",
                                "target_lang_sense_clarification": "сделал известным (прошедшее время)"
                            },
                            {
                                "target_lang_word": "выявил",
                                "target_lang_sense_clarification": "обнаружил (прошедшее время)"
                            },
                            {
                                "target_lang_word": "обнародовал",
                                "target_lang_sense_clarification": "опубликовал (прошедшее время)"
                            }
                        ]
                    }
                }
            ]
        },
        {

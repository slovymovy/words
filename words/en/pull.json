                            {
                                "target_lang_word": "язычо́к",
                                "target_lang_sense_clarification": "деталь молнии"
                            },
                            {
                                "target_lang_word": "тя́га",
                                "target_lang_sense_clarification": "техническая деталь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3404f1f-4895-3e2e-86a8-c4dad7b5247a",
                    "sense_definition": "A force that attracts or draws someone or something in a particular direction.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "attraction_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The moon exerts a gravitational <w>pull</w> on the earth's oceans.",
                            "target_lang_translations": {
                                "ru": "Луна оказывает гравитационное <w>притяжение</w> на океаны Земли."
                            }
                        },
                        {
                            "text": "She felt the strong <w>pull</w> of her hometown after years abroad.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала сильную <w>тягу</w> к родному городу после лет за границей."
                            }
                        }
                    ],
                    "synonyms": [
                        "attraction",
                        "draw"
                    ],
                    "antonyms": [
                        "repulsion"
                    ],
                    "common_phrases": [
                        "gravitational pull",
                        "magnetic pull",
                        "strong pull"
                    ],
                    "target_lang_definitions": {

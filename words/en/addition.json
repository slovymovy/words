{
    "word_family": [
        "add",
        "additional",
        "additionally",
        "additive"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "84e5be6a-5704-3d99-aa04-f03b5a1dddd4",
                    "sense_definition": "The arithmetic operation of calculating the sum of two or more numbers.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "math_calc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The students are learning basic <w>addition</w> and subtraction.",
                            "target_lang_translations": {
                                "ru": "Ученики изучают простое <w>сложение</w> и вычитание."
                            }
                        },
                        {
                            "text": "You made a small mistake in your <w>addition</w>.",
                            "target_lang_translations": {
                                "ru": "Ты сделал небольшую ошибку в <w>сложении</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "subtraction"
                    ],
                    "common_phrases": [
                        "simple addition",
                        "addition and subtraction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Арифметическая операция вычисления суммы двух или более чисел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сложение",
                                "target_lang_sense_clarification": "математическая операция"
                            },
                            {
                                "target_lang_word": "суммирование",
                                "target_lang_sense_clarification": "процесс вычисления суммы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3033c37b-1e13-3ded-ae43-35ca9e73af62",
                    "sense_definition": "The act of adding something to something else.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "act_adding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>addition</w> of fresh herbs improved the flavor of the soup.",
                            "target_lang_translations": {
                                "ru": "<w>Добавление</w> свежей зелени улучшило вкус супа."
                            }
                        },
                        {
                            "text": "In <w>addition</w> to his regular job, he volunteers at the shelter.",
                            "target_lang_translations": {
                                "ru": "В <w>дополнение</w> к своей основной работе он работает волонтером в приюте."
                            }
                        }
                    ],
                    "synonyms": [
                        "adding",
                        "inclusion"
                    ],
                    "antonyms": [
                        "removal",
                        "reduction",
                        "exclusion"
                    ],
                    "common_phrases": [
                        "in addition to",
                        "with the addition of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по добавлению чего-либо к чему-либо другому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добавление",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "дополнение",
                                "target_lang_sense_clarification": "то, что дополняет; часто во фразе «в дополнение»"
                            },
                            {
                                "target_lang_word": "прибавление",
                                "target_lang_sense_clarification": "увеличение количества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4fe7a3f8-25a4-38f8-af79-9adaf6352a81",
                    "sense_definition": "A person or thing that is added to a group, collection, or place.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "thing_added",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new library is a wonderful <w>addition</w> to our town.",
                            "target_lang_translations": {
                                "ru": "Новая библиотека — чудесное <w>дополнение</w> к нашему городу."
                            }
                        },
                        {
                            "text": "They welcomed the latest <w>addition</w> to their family last week.",
                            "target_lang_translations": {
                                "ru": "На прошлой неделе они приветствовали <w>пополнение</w> в своем семействе."
                            }
                        }
                    ],
                    "synonyms": [
                        "extra",
                        "supplement"
                    ],
                    "common_phrases": [
                        "welcome addition",
                        "latest addition",
                        "new addition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или вещь, добавленные к группе, коллекции или месту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пополнение",
                                "target_lang_sense_clarification": "о людях, семье или коллекции"
                            },
                            {
                                "target_lang_word": "дополнение",
                                "target_lang_sense_clarification": "новая часть чего-то целого"
                            },
                            {
                                "target_lang_word": "прибавление",
                                "target_lang_sense_clarification": "разг. о рождении ребенка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0c33f45-eb81-3a76-8cb1-56f4a6cb71a7",
                    "sense_definition": "An organic reaction where two or more molecules combine to form a larger one.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor explained the mechanism of the <w>addition</w> reaction.",
                            "target_lang_translations": {
                                "ru": "Профессор объяснил механизм реакции <w>присоединения</w>."
                            }
                        },
                        {
                            "text": "We observed the <w>addition</w> of bromine to the compound during the experiment.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдали <w>присоединение</w> брома к соединению во время эксперимента."
                            }
                        }
                    ],
                    "common_phrases": [
                        "addition reaction"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical (chemistry)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Органическая реакция, при которой две или более молекул соединяются, образуя более крупную."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присоединение",
                                "target_lang_sense_clarification": "химическая реакция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfe38a26-5970-3071-af62-6fdee6cc70eb",
                    "sense_definition": "A title annexed to a person's name to identify him or her more precisely.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In ancient documents, a specific <w>addition</w> was often required after a name.",
                            "target_lang_translations": {
                                "ru": "В древних документах после имени часто требовалось особое <w>прибавление</w>."
                            }
                        },
                        {
                            "text": "He bore the <w>addition</w> of \"The Brave\" after his heroic deeds.",
                            "target_lang_translations": {
                                "ru": "После своих героических подвигов он носил <w>прозвание</w> «Храбрый»."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул или эпитет, добавляемый к имени человека для его более точной идентификации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозвание",
                                "target_lang_sense_clarification": "устар. эпитет к имени"
                            },
                            {
                                "target_lang_word": "прибавление",
                                "target_lang_sense_clarification": "устар. дополнение к имени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cebe844-4a45-359b-a81e-7794331edd8a",
                    "sense_definition": "A dot at the right side of a note as an indication that its sound is to be lengthened one half.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The composer used an <w>addition</w> to extend the duration of the final note.",
                            "target_lang_translations": {
                                "ru": "Композитор использовал <w>точку</w>, чтобы продлить длительность последней ноты."
                            }
                        },
                        {
                            "text": "Musicians must pay close attention to every <w>addition</w> on the score.",
                            "target_lang_translations": {
                                "ru": "Музыканты должны обращать пристальное внимание на каждое <w>добавление</w> в партитуре."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical (music)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Точка справа от ноты, указывающая на удлинение её звучания наполовину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точка",
                                "target_lang_sense_clarification": "музыкальный знак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44c98d6f-3ff8-34ca-b290-773e492f39ca",
                    "sense_definition": "Something added to a coat of arms as a mark of honour.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "heraldry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king granted an <w>addition</w> to the family crest for their loyalty.",
                            "target_lang_translations": {
                                "ru": "Король пожаловал <w>дополнение</w> к фамильному гербу за их верность."
                            }
                        },
                        {
                            "text": "This specific <w>addition</w> signifies a great military victory.",
                            "target_lang_translations": {
                                "ru": "Это конкретное <w>дополнение</w> означает великую военную победу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical (heraldry)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент, добавляемый к гербу в знак почести."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дополнение",
                                "target_lang_sense_clarification": "геральдический элемент"
                            },
                            {
                                "target_lang_word": "прибавление",
                                "target_lang_sense_clarification": "увеличение герба"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "faf43952-95ff-3506-be1e-9a43e8a63c2e",
                    "sense_definition": "Прятать кого-либо или что-либо, делать невидимым, помещая в укрытие или закрывая чем-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Густой туман начал <скрывать> очертания города.",
                            "target_lang_translations": {
                                "en": "The thick fog began to <hide> the outlines of the city."
                            }
                        },
                        {
                            "text": "Маска помогала ему <скрывать> лицо от прохожих.",
                            "target_lang_translations": {
                                "en": "The mask helped him <hide> his face from passersby."
                            }
                        }
                    ],
                    "synonyms": [
                        "прятать"
                    ],
                    "antonyms": [
                        "обнаруживать",
                        "выставлять",
                        "раскрывать"
                    ],
                    "common_phrases": [
                        "скрывать лицо",
                        "скрывать следы",
                        "скрывать от глаз"
                    ],
                    "target_lang_definitions": {
                        "en": "To hide someone or something, to make invisible by placing in a shelter or covering with something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hide",
                                "target_lang_sense_clarification": "general term for putting something out of sight"
                            },
                            {
                                "target_lang_word": "conceal",
                                "target_lang_sense_clarification": "formal, often implies covering or obscuring from view"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "994d98dc-68a0-3c36-bf4b-df2dad31c001",
                    "sense_definition": "Держать в тайне, не разглашать информацию, мысли или чувства.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Невозможно вечно <скрывать> правду от общественности.",
                            "target_lang_translations": {
                                "en": "It is impossible to <hide> the truth from the public forever."
                            }
                        },
                        {
                            "text": "Она старалась <скрывать> своё волнение перед экзаменом.",
                            "target_lang_translations": {
                                "en": "She tried to <hide> her nervousness before the exam."
                            }
                        }
                    ],
                    "synonyms": [
                        "утаивать",
                        "умалчивать",
                        "таить"
                    ],
                    "antonyms": [
                        "разглашать",
                        "раскрывать"
                    ],
                    "common_phrases": [
                        "скрывать правду",
                        "скрывать чувства",
                        "скрывать факты"
                    ],
                    "target_lang_definitions": {
                        "en": "To keep secret, not to disclose information, thoughts, or feelings."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hide",
                                "target_lang_sense_clarification": "to keep feelings or information secret"
                            },
                            {
                                "target_lang_word": "conceal",
                                "target_lang_sense_clarification": "formal, to prevent something from being known"
                            },
                            {
                                "target_lang_word": "keep secret",
                                "target_lang_sense_clarification": "to maintain secrecy about something"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "roosteren",
        "broodrooster"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b3faeed1-7f5c-31be-bcdf-7c56156741d8",
                    "sense_definition": "Een schema waarop staat aangegeven wanneer bepaalde activiteiten, zoals lessen, werk of diensten, plaatsvinden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "schedule",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn <w>rooster</w> is deze week erg vol met afspraken.",
                            "target_lang_translations": {
                                "en": "My <w>schedule</w> is very full of appointments this week.",
                                "ru": "Моё <w>расписание</w> на этой неделе очень плотное из-за встреч."
                            }
                        },
                        {
                            "text": "Kijk even op het <w>rooster</w> hoe laat de trein vertrekt.",
                            "target_lang_translations": {
                                "en": "Check the <w>timetable</w> to see what time the train leaves.",
                                "ru": "Посмотри в <w>расписании</w>, во сколько отправляется поезд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "volgens het rooster",
                        "op het rooster kijken",
                        "het rooster wijzigen"
                    ],
                    "target_lang_definitions": {
                        "en": "A plan or list showing when specific events, tasks, or lessons are scheduled to take place.",
                        "ru": "План или список, показывающий, когда должны состояться определенные мероприятия, задачи или уроки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "schedule",
                                "target_lang_sense_clarification": "general plan of events or tasks"
                            },
                            {
                                "target_lang_word": "timetable",
                                "target_lang_sense_clarification": "specifically for transport or school lessons"
                            },
                            {
                                "target_lang_word": "roster",
                                "target_lang_sense_clarification": "list of duties or work shifts"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "расписание",
                                "target_lang_sense_clarification": "уроков, транспорта"
                            },
                            {
                                "target_lang_word": "график",
                                "target_lang_sense_clarification": "работы, дежурств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86e84fe1-d66e-364c-beac-b688077a722d",
                    "sense_definition": "Een voorwerp bestaande uit evenwijdige of kruisende staven of draden, vaak gebruikt om een opening af te dekken of om voedsel op te bakken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "grid_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het regenwater stroomt via het <w>rooster</w> het riool in.",
                            "target_lang_translations": {
                                "en": "The rainwater flows through the <w>grate</w> into the sewer.",
                                "ru": "Дождевая вода стекает через <w>решётку</w> в канализацию."
                            }
                        },
                        {
                            "text": "Leg de worstjes voorzichtig op het hete <w>rooster</w>.",
                            "target_lang_translations": {
                                "en": "Place the sausages carefully on the hot <w>grill</w>.",
                                "ru": "Осторожно положи сосиски на горячую <w>решётку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "traliehek",
                        "afrastering"
                    ],
                    "common_phrases": [
                        "achter de roosters",
                        "het rooster schoonmaken"
                    ],
                    "target_lang_definitions": {
                        "en": "An object made of parallel or crossed bars, used to cover an opening or to support food while cooking.",
                        "ru": "Предмет из параллельных или перекрещивающихся прутьев, используемый для закрытия отверстия или для приготовления пищи."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grate",
                                "target_lang_sense_clarification": "cover for a drain or ventilation opening"
                            },
                            {
                                "target_lang_word": "grill",
                                "target_lang_sense_clarification": "metal surface for cooking food"
                            },
                            {
                                "target_lang_word": "grid",
                                "target_lang_sense_clarification": "network of bars or lines"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "решётка",
                                "target_lang_sense_clarification": "для вентиляции или жарки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec5941b0-f369-3237-812e-9dc27793ef92",
                    "sense_definition": "Een elektrisch apparaat om brood mee te roosteren.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stop twee sneetjes brood in het <w>rooster</w> voor het ontbijt.",
                            "target_lang_translations": {
                                "en": "Put two slices of bread in the <w>toaster</w> for breakfast.",
                                "ru": "Положи два ломтика хлеба в <w>тостер</w> на завтрак."
                            }
                        },
                        {
                            "text": "Ons oude <w>rooster</w> werkt niet meer goed, het brood verbrandt steeds.",
                            "target_lang_translations": {
                                "en": "Our old <w>toaster</w> doesn't work well anymore; the bread keeps burning.",
                                "ru": "Наш старый <w>тостер</w> больше не работает как надо, хлеб постоянно подгорает."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brood in het rooster"
                    ],
                    "target_lang_definitions": {
                        "en": "An electrical appliance used for toasting bread.",
                        "ru": "Электрический прибор для поджаривания хлеба."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "toaster",
                                "target_lang_sense_clarification": "device for bread"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тостер",
                                "target_lang_sense_clarification": "бытовой прибор"
                            }
                        ]
                    }
                }
            ]
        },

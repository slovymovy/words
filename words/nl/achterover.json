    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "3c49992e-04df-39f8-a744-cf34aac1360e",
                    "sense_definition": "In achterwaartse richting; met de rug naar achteren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij leunde ontspannen <w>achterover</w> in zijn luie stoel.",
                            "target_lang_translations": {
                                "en": "He leaned <w>back</w> relaxedly in his easy chair.",
                                "ru": "Он расслабленно откинулся <w>назад</w> в своем кресле."
                            }
                        },
                        {
                            "text": "Pas op dat je niet <w>achterover</w> valt als je op die kruk gaat staan.",
                            "target_lang_translations": {
                                "en": "Be careful not to fall <w>backwards</w> when you stand on that stool.",
                                "ru": "Смотри не упади <w>назад</w>, когда встанешь на эту табуретку."
                            }
                        }
                    ],
                    "antonyms": [
                        "voorover"
                    ],
                    "common_phrases": [
                        "achterover leunen",
                        "achterover vallen",
                        "achterover kammen"
                    ],
                    "target_lang_definitions": {
                        "en": "In a backward direction; with one's back towards the rear.",
                        "ru": "В обратном направлении; спиной назад."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "backwards",
                                "target_lang_sense_clarification": "direction"
                            },
                            {
                                "target_lang_word": "back",
                                "target_lang_sense_clarification": "movement or position"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "назад",
                                "target_lang_sense_clarification": "направление движения или положение"
                            },
                            {
                                "target_lang_word": "навзничь",
                                "target_lang_sense_clarification": "о падении на спину"
                            },
                            {
                                "target_lang_word": "откинувшись",
                                "target_lang_sense_clarification": "о положении тела (сидя)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cb78df0-e53d-3b2c-ac24-569806bd90d8",
                    "sense_definition": "Snel en in één teug opdrinken (meestal in combinatie met werkwoorden als slaan of gooien).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "drinking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de toespraak sloeg hij zijn glas champagne in één keer <w>achterover</w>.",
                            "target_lang_translations": {
                                "en": "After the speech, he knocked <w>back</w> his glass of champagne in one go.",
                                "ru": "После речи он разом <w>опрокинул</w> бокал шампанского."
                            }
                        },
                        {
                            "text": "Ze gooide het vieze drankje snel <w>achterover</w> om er vanaf te zijn.",
                            "target_lang_translations": {
                                "en": "She quickly tossed <w>back</w> the nasty drink to get it over with.",
                                "ru": "Она быстро <w>опрокинула</w> в себя этот противный напиток, чтобы покончить с этим."
                            }
                        }
                    ],
                    "common_phrases": [
                        "achterover slaan",
                        "achterover gooien"
                    ],
                    "target_lang_definitions": {
                        "en": "To drink quickly and in one draft (usually combined with verbs like 'slaan' or 'gooien').",
                        "ru": "Быстро выпить одним глотком (обычно в сочетании с глаголами, означающими опрокидывание)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "back",
                                "target_lang_sense_clarification": "in phrases like 'knock back' or 'toss back'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "залпом",
                                "target_lang_sense_clarification": "выпить одним глотком"
                            },
                            {
                                "target_lang_word": "опрокинуть",
                                "target_lang_sense_clarification": "в значении 'выпить быстро' (разг.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

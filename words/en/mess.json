    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "17e8d5f0-3f45-31e0-8c97-6a1dad5ee071",
                    "sense_definition": "A state of being dirty, untidy, or confused; a chaotic or difficult situation.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "disorder_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please clean up the <w>mess</w> in your bedroom before you go out with your friends.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, убери <w>беспорядок</w> в своей спальне, прежде чем идти гулять с друзьями."
                            }
                        },
                        {
                            "text": "The company accounts were in a complete <w>mess</w> when the new manager took over.",
                            "target_lang_translations": {
                                "ru": "Счета компании были в полном <w>беспорядке</w>, когда новый менеджер вступил в должность."
                            }
                        }
                    ],
                    "synonyms": [
                        "chaos",
                        "disorder",
                        "disarray",
                        "muddle",
                        "shambles",
                        "confusion"
                    ],
                    "common_phrases": [
                        "make a mess",
                        "in a mess",
                        "clean up the mess",
                        "what a mess"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние загрязненности, неопрятности или замешательства; хаотичная или сложная ситуация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беспорядок",
                                "target_lang_sense_clarification": "отсутствие порядка, хаос"
                            },
                            {
                                "target_lang_word": "бардак",
                                "target_lang_sense_clarification": "разговорное; сильный беспорядок, неразбериха"
                            },
                            {
                                "target_lang_word": "неразбериха",
                                "target_lang_sense_clarification": "запутанная ситуация, отсутствие ясности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ccb0f9ab-1789-32a0-acf6-a34d34114612",
                    "sense_definition": "A person who is in a state of emotional confusion, distress, or disarray.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disorder_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was a nervous <w>mess</w> while waiting for the results of her medical exam.",
                            "target_lang_translations": {
                                "ru": "Она была нервной <w>развалиной</w> в ожидании результатов медицинского обследования."
                            }
                        },
                        {
                            "text": "He has been a total <w>mess</w> ever since he lost his job last month.",
                            "target_lang_translations": {
                                "ru": "Он стал полной <w>развалиной</w> с тех пор, как потерял работу в прошлом месяце."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hot mess",
                        "emotional mess"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used to describe someone falling apart emotionally"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, находящийся в состоянии эмоционального замешательства, стресса или душевного разлада."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развалина",

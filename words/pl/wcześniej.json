{
    "word_family": [
        "wczesny",
        "wcześnie"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c59b064e-b045-39da-9bb5-21ea6f207d22",
                    "sense_definition": "Stopień wyższy od <w>wcześnie</w>; o porze poprzedzającej oczekiwany czas lub w krótszym czasie niż ktoś inny.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy wstać jutro <w>wcześniej</w>, żeby zdążyć na pociąg.",
                            "target_lang_translations": {
                                "ru": "Мы должны встать завтра <w>раньше</w>, чтобы успеть на поезд."
                            }
                        },
                        {
                            "text": "W tym roku zima przyszła znacznie <w>wcześniej</w> niż zwykle.",
                            "target_lang_translations": {
                                "ru": "В этом году зима пришла значительно <w>раньше</w>, чем обычно."
                            }
                        }
                    ],
                    "antonyms": [
                        "później"
                    ],
                    "common_phrases": [
                        "dużo wcześniej",
                        "nieco wcześniej",
                        "trochę wcześniej"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сравнительная степень наречия <w>рано</w>; в более раннее время, чем ожидалось, или быстрее, чем кто-то другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раньше",
                                "target_lang_sense_clarification": "сравнительная степень"
                            },
                            {
                                "target_lang_word": "пораньше",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c55dc8a0-fab8-3742-a38a-99ff192da442",
                    "sense_definition": "W przeszłości, w czasie poprzedzającym obecną chwilę; w dawnych czasach.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "past_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nigdy <w>wcześniej</w> nie widziałem tak pięknego zachodu słońca.",
                            "target_lang_translations": {
                                "ru": "Я никогда <w>раньше</w> не видел такого красивого заката."
                            }
                        },
                        {
                            "text": "<w>Wcześniej</w> mieszkałem w małym miasteczku na południu kraju.",
                            "target_lang_translations": {
                                "ru": "<w>Раньше</w> я жил в маленьком городке на юге страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "dawniej",
                        "kiedyś",
                        "przedtem",
                        "poprzednio",
                        "uprzednio"
                    ],
                    "antonyms": [
                        "później",
                        "potem"
                    ],
                    "common_phrases": [
                        "nigdy wcześniej",
                        "jak wcześniej"
                    ],
                    "target_lang_definitions": {
                        "ru": "В прошлом, во время, предшествующее настоящему моменту; в прежние времена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раньше",
                                "target_lang_sense_clarification": "в прошлом"
                            },
                            {
                                "target_lang_word": "прежде",
                                "target_lang_sense_clarification": "в прошлом"
                            },
                            {
                                "target_lang_word": "ранее",
                                "target_lang_sense_clarification": "книжное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95df46c5-203a-3932-8f23-d4c7ad3d2a2c",
                    "sense_definition": "Przed upływem pewnego określonego czasu lub przed jakimś wydarzeniem, stanowiąc punkt odniesienia w narracji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relative_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spóźniłem się na przystanek, bo autobus odjechał minutę <w>wcześniej</w>.",
                            "target_lang_translations": {
                                "ru": "Я опоздал на остановку, потому что автобус уехал минутой <w>раньше</w>."
                            }
                        },
                        {
                            "text": "Spotkaliśmy się dwa dni <w>wcześniej</w> na przyjęciu u znajomych.",
                            "target_lang_translations": {
                                "ru": "Мы встретились двумя днями <w>раньше</w> на вечеринке у знакомых."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedtem",
                        "wstecz (po określeniu czasu)"
                    ],
                    "antonyms": [
                        "później"
                    ],
                    "common_phrases": [
                        "chwilę wcześniej",
                        "dzień wcześniej",
                        "rok wcześniej"
                    ],
                    "target_lang_definitions": {
                        "ru": "До определенного времени или события, используемое как точка отсчета в повествовании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раньше",
                                "target_lang_sense_clarification": "до этого момента"
                            },
                            {
                                "target_lang_word": "ранее",
                                "target_lang_sense_clarification": "до этого момента"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "fdd2c920-eab9-3c14-bb5a-82b9db31e3dc",
                    "sense_definition": "В очень высокой степени; чрезвычайно, очень.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот фильм был <w>невероятно</w> интересным и захватывающим.",
                            "target_lang_translations": {
                                "en": "This movie was <w>incredibly</w> interesting and exciting."
                            }
                        },
                        {
                            "text": "Мы <w>невероятно</w> устали после долгой прогулки по лесу.",
                            "target_lang_translations": {
                                "en": "We were <w>incredibly</w> tired after a long walk in the forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "чрезвычайно",
                        "очень",
                        "крайне",
                        "безумно"
                    ],
                    "antonyms": [
                        "мало",
                        "слегка"
                    ],
                    "common_phrases": [
                        "невероятно красивый",
                        "невероятно быстро",
                        "невероятно трудно",
                        "невероятно повезло"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Extremely; to a very high degree."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "incredibly",
                                "target_lang_sense_clarification": "used to emphasize the degree of something"
                            },
                            {
                                "target_lang_word": "unbelievably",
                                "target_lang_sense_clarification": "to a degree that is hard to believe"
                            },
                            {
                                "target_lang_word": "extremely",
                                "target_lang_sense_clarification": "to a very high degree"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3c9389a-3bbe-3346-8c35-bda4bbee5358",
                    "sense_definition": "Оценка ситуации как невозможной, неправдоподобной или вызывающей сильное удивление.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "plausibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Невероятно</w>, но мы действительно выиграли этот сложный матч.",
                            "target_lang_translations": {
                                "en": "It's <w>incredible</w>, but we really won this difficult match."
                            }
                        },
                        {
                            "text": "Просто <w>невероятно</w>, как быстро растут чужие дети.",
                            "target_lang_translations": {
                                "en": "It is simply <w>incredible</w> how fast other people's children grow."
                            }
                        }
                    ],
                    "synonyms": [
                        "невозможно",
                        "немыслимо",
                        "удивительно"
                    ],
                    "antonyms": [
                        "возможно",
                        "вероятно",
                        "ожидаемо"
                    ],
                    "common_phrases": [
                        "это невероятно",
                        "просто невероятно",
                        "звучит невероятно"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to express that a situation is impossible, implausible, or very surprising."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "incredible",
                                "target_lang_sense_clarification": "expressing great surprise or disbelief"
                            },
                            {
                                "target_lang_word": "unbelievable",
                                "target_lang_sense_clarification": "difficult to believe; extraordinary"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c3ac88d-fd56-3938-ab24-787dfdf25745",
                    "sense_definition": "Так, что в это трудно или невозможно поверить; неправдоподобно.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "plausibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вся эта история звучит <w>невероятно</w>, но она правдива.",
                            "target_lang_translations": {
                                "en": "This whole story sounds <w>improbable</w>, but it is true."
                            }
                        },
                        {
                            "text": "Сюжет книги закручен <w>невероятно</w> и держит в напряжении до конца.",
                            "target_lang_translations": {
                                "en": "The book's plot is twisted <w>incredibly</w> and keeps you in suspense until the end."
                            }
                        }
                    ],
                    "synonyms": [
                        "неправдоподобно",
                        "фантастично"
                    ],
                    "antonyms": [
                        "правдоподобно",
                        "обыденно"
                    ],
                    "target_lang_definitions": {
                        "en": "In a manner that is difficult or impossible to believe; implausibly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "implausibly",
                                "target_lang_sense_clarification": "in a way that does not seem likely or probable"
                            },
                            {
                                "target_lang_word": "unbelievably",
                                "target_lang_sense_clarification": "in a way that is hard to believe"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "951d0608-f547-3b59-aba3-d1b6db2e7597",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>невероятный</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "plausibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это предположение <w>невероятно</w> по своей сути и требует доказательств.",
                            "target_lang_translations": {
                                "en": "This assumption is <w>improbable</w> in its essence and requires proof."
                            }
                        },
                        {
                            "text": "То, что ты говоришь, совершенно <w>невероятно</w>.",
                            "target_lang_translations": {
                                "en": "What you are saying is absolutely <w>incredible</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short form neuter singular of the adjective 'incredible' or 'improbable'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "improbable",
                                "target_lang_sense_clarification": "unlikely to be true or to occur"
                            },
                            {
                                "target_lang_word": "incredible",
                                "target_lang_sense_clarification": "impossible to believe"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
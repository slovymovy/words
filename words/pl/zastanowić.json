    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d900a84-a7ae-3fe3-99c6-f80536a69263",
                    "sense_definition": "Skupić myśli na czymś, aby to przeanalizować, zrozumieć lub podjąć decyzję; rozważyć coś w umyśle (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "thinking_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę się chwilę <zastanowić>, zanim dam ci ostateczną odpowiedź.",
                            "target_lang_translations": {
                                "ru": "Мне нужно немного <w>подумать</w>, прежде чем я дам тебе окончательный ответ."
                            }
                        },
                        {
                            "text": "Warto się <zastanowić> nad konsekwencjami, zanim podejmie się ryzyko.",
                            "target_lang_translations": {
                                "ru": "Стоит <w>задуматься</w> о последствиях, прежде чем идти на риск."
                            }
                        }
                    ],
                    "synonyms": [
                        "przemyśleć",
                        "rozważyć",
                        "pomyśleć"
                    ],
                    "antonyms": [
                        "zignorować",
                        "zlekceważyć"
                    ],
                    "common_phrases": [
                        "zastanowić się nad czymś",
                        "muszę się zastanowić",
                        "głęboko się zastanowić",
                        "zastanowić się dwa razy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сосредоточиться на мыслях о чем-либо, чтобы проанализировать, понять или принять решение; обдумать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задуматься",
                                "target_lang_sense_clarification": "погрузиться в размышления"
                            },
                            {
                                "target_lang_word": "подумать",
                                "target_lang_sense_clarification": "поразмыслить некоторое время"
                            },
                            {
                                "target_lang_word": "обдумать",
                                "target_lang_sense_clarification": "всесторонне рассмотреть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8299bedd-9883-300e-8403-3accaee0cf1e",
                    "sense_definition": "Sprawić, że ktoś zaczyna o czymś myśleć, analizować fakty lub mieć wątpliwości; zaintrygować lub zdziwić kogoś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "causing_thought",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego dziwne zachowanie mocno mnie <zastanowiło> i wzbudziło podejrzenia.",
                            "target_lang_translations": {
                                "ru": "Его странное поведение сильно меня <w>насторожило</w> и вызвало подозрения."
                            }
                        },
                        {
                            "text": "To nagłe milczenie powinno nas wszystkich <zastanowić>.",
                            "target_lang_translations": {
                                "ru": "Это внезапное молчание должно нас всех <w>заставить задуматься</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaintrygować",
                        "zaciekawić",
                        "zdziwić"
                    ],
                    "common_phrases": [
                        "to mnie zastanowiło",
                        "zastanowić kogoś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то начать думать о чем-то, анализировать факты или вызвать сомнения; заинтриговать или удивить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заставить задуматься",
                                "target_lang_sense_clarification": "побудить к размышлению"
                            },
                            {
                                "target_lang_word": "озадачить",
                                "target_lang_sense_clarification": "вызвать недоумение"
                            },
                            {
                                "target_lang_word": "насторожить",
                                "target_lang_sense_clarification": "вызвать подозрение или беспокойство"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
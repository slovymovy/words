    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c2d66abe-3ab8-34bb-9d57-9d30557857ce",
                    "sense_definition": "Een weersverschijnsel waarbij regen gepaard gaat met donder en bliksem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er is zwaar <w>onweer</w> op komst, dus we blijven liever binnen.",
                            "target_lang_translations": {
                                "ru": "Надвигается сильная <w>гроза</w>, поэтому мы предпочитаем остаться дома."
                            }
                        },
                        {
                            "text": "Het <w>onweer</w> trok langzaam over de stad en maakte veel lawaai.",
                            "target_lang_translations": {
                                "ru": "<w>Гроза</w> медленно проходила над городом, создавая много шума."
                            }
                        }
                    ],
                    "synonyms": [
                        "donderbui"
                    ],
                    "common_phrases": [
                        "zwaar onweer",
                        "onweer op komst",
                        "losbarstend onweer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Погодное явление, при котором дождь сопровождается громом и молнией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гроза",
                                "target_lang_sense_clarification": "погодное явление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2fc28f57-0eaa-3080-b683-5b0f54fa284f",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>onweren</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>onweer</w> zelden tegen mijn kinderen, maar vandaag was het nodig.",
                            "target_lang_translations": {
                                "ru": "Я редко <w>гремлю</w> на своих детей, но сегодня это было необходимо."
                            }
                        },
                        {
                            "text": "Soms <w>onweer</w> ik van binnen, terwijl ik aan de buitenkant rustig lijk.",
                            "target_lang_translations": {
                                "ru": "Иногда я <w>бушую</w> внутри, хотя внешне кажусь спокойным."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола onweren (греметь, бушевать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гремлю",
                                "target_lang_sense_clarification": "метафорически: громко ругаться"
                            },
                            {
                                "target_lang_word": "бушую",
                                "target_lang_sense_clarification": "метафорически: испытывать или проявлять сильный гнев"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4dfacba4-8d41-38e2-a18b-b0c8623d0021",
                    "sense_definition": "Gebiedende wijs van <w>onweren</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Onweer</w> niet zo tegen mij, ik heb niets verkeerd gedaan.",
                            "target_lang_translations": {
                                "ru": "Не <w>греми</w> так на меня, я ничего плохого не сделал."
                            }
                        },
                        {
                            "text": "<w>Onweer</w> maar raak, ik ben niet bang voor je woede.",
                            "target_lang_translations": {
                                "ru": "<w>Бушуй</w> сколько влезет, я не боюсь твоего гнева."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение от глагола onweren."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "греми",
                                "target_lang_sense_clarification": "метафорически: громко ругаться"
                            },
                            {
                                "target_lang_word": "бушуй",
                                "target_lang_sense_clarification": "метафорически: проявлять гнев"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
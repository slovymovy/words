    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "49aba43e-d2d5-39c7-914f-23e07c850387",
                    "sense_definition": "Podnosić z ziemi lub zrywać wiele rzeczy (np. owoce, grzyby, przedmioty), aby zgromadzić je w jednym miejscu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_gathering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem całą rodziną chodzimy do lasu i <w>zbieramy</w> jagody.",
                            "target_lang_translations": {
                                "ru": "Летом мы всей семьей ходим в лес и <w>собираем</w> ягоды."
                            }
                        },
                        {
                            "text": "Muszę <w>zebrać</w> z podłogi wszystkie rozsypane klocki.",
                            "target_lang_translations": {
                                "ru": "Я должен <w>собрать</w> с пола все рассыпанные кубики."
                            }
                        }
                    ],
                    "synonyms": [
                        "podnosić",
                        "gromadzić"
                    ],
                    "antonyms": [
                        "rozsypywać",
                        "rozrzucać"
                    ],
                    "common_phrases": [
                        "zbierać grzyby",
                        "zbierać owoce",
                        "zbierać śmieci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поднимать с земли или срывать (ягоды, грибы), складывая в одно место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "ягоды, грибы, предметы"
                            },
                            {
                                "target_lang_word": "подбирать",
                                "target_lang_sense_clarification": "с земли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e4d8194-445e-31c6-a918-1478190d9a6b",
                    "sense_definition": "Kolekcjonować określone przedmioty, tworzyć ich zbiór jako hobby.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "hobby",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek od wielu lat <w>zbiera</w> rzadkie monety.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка уже много лет <w>собирает</w> редкие монеты."
                            }
                        },
                        {
                            "text": "Kiedy byłem mały, <w>zbierałem</w> kolorowe znaczki pocztowe.",
                            "target_lang_translations": {
                                "ru": "Когда я был маленьким, я <w>собирал</w> цветные почтовые марки."
                            }
                        }
                    ],
                    "synonyms": [
                        "kolekcjonować"
                    ],
                    "common_phrases": [
                        "zbierać znaczki",
                        "zbierać monety",
                        "zbierać autografy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Коллекционировать предметы в качестве хобби."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "коллекцию"
                            },
                            {
                                "target_lang_word": "коллекционировать",
                                "target_lang_sense_clarification": "хобби"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b868a63-62cb-30c9-beb0-8c9b2cc96ccd",
                    "sense_definition": "Przygotowywać się do wyjścia lub do rozpoczęcia jakiejś czynności (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy już <w>zbierać się</w> do domu, bo robi się późno.",
                            "target_lang_translations": {
                                "ru": "Нам уже пора <w>собираться</w> домой, потому что становится поздно."
                            }
                        },
                        {
                            "text": "Długo <w>zbierałem się</w>, żeby do niej zadzwonić i przeprosić.",
                            "target_lang_translations": {
                                "ru": "Я долго <w>собирался</w> позвонить ей и извиниться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zbierać się do wyjścia",
                        "zbierać się do pracy",
                        "zbierać się w sobie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовиться к выходу или к началу какого-либо действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "готовиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "466cc01d-37f1-3d4b-b63b-bcb95c30ee1a",
                    "sense_definition": "Gromadzić coś stopniowo w większej ilości, np. pieniądze, informacje lub siły.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "accumulating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Od trzech miesięcy <w>zbieram</w> pieniądze na nowy rower.",
                            "target_lang_translations": {
                                "ru": "Я уже три месяца <w>собираю</w> деньги на новый велосипед."
                            }
                        },
                        {
                            "text": "Dziennikarz jeździł po kraju i <w>zbierał</w> materiały do artykułu.",
                            "target_lang_translations": {
                                "ru": "Журналист ездил по стране и <w>собирал</w> материалы для статьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "gromadzić"
                    ],
                    "antonyms": [
                        "rozdawać",
                        "oddawać"
                    ],
                    "common_phrases": [
                        "zbierać pieniądze",
                        "zbierać siły",
                        "zbierać informacje",
                        "zbierać myśli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Накапливать что-либо постепенно (деньги, информацию)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "деньги, информацию"
                            },
                            {
                                "target_lang_word": "копить",
                                "target_lang_sense_clarification": "деньги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91100218-b0ae-3f33-b319-cf34e1ed233f",
                    "sense_definition": "Gromadzić się w jednym miejscu (o ludziach) lub tworzyć skupisko (o zjawiskach, używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_gathering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciemne chmury <w>zbierają się</w> nad miastem, chyba będzie burza.",
                            "target_lang_translations": {
                                "ru": "Темные тучи <w>собираются</w> над городом, кажется, будет гроза."
                            }
                        },
                        {
                            "text": "Przed wejściem do teatru zaczęli <w>zbierać się</w> widzowie.",
                            "target_lang_translations": {
                                "ru": "Перед входом в театр начали <w>собираться</w> зрители."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zbierać się na deszcz",
                        "tłum się zbiera"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скапливаться в одном месте (о людях или тучах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "вместе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72e58e34-9b38-3fa2-a6e8-e97090ed8c88",
                    "sense_definition": "Zwoływać grupę ludzi w jedno miejsce, organizować spotkanie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "people_gathering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel <w>zebrał</w> wszystkich uczniów na boisku szkolnym.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>собрал</w> всех учеников на школьном дворе."
                            }
                        },
                        {
                            "text": "Próbuję <w>zebrać</w> ekipę na wspólny wyjazd w góry.",
                            "target_lang_translations": {
                                "ru": "Я пытаюсь <w>собрать</w> компанию для совместной поездки в горы."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwoływać",
                        "gromadzić",
                        "przywoływać"
                    ],
                    "common_phrases": [
                        "zbierać zespół",
                        "zbierać ekipę",
                        "zbierać ludzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Созывать людей в одно место, организовывать встречу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "группу, людей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "db5ab003-da5e-35dd-ab25-f7e390e2859a",
                    "sense_definition": "A pointed piece of wood or metal driven into the ground as a marker or support.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gardener drove a wooden <w>stake</w> into the soil to support the young tree.",
                            "target_lang_translations": {
                                "ru": "Садовник вбил деревянный <w>кол</w> в землю, чтобы подпереть молодое дерево."
                            }
                        },
                        {
                            "text": "Surveyors placed a <w>stake</w> at each corner of the field to mark the property lines.",
                            "target_lang_translations": {
                                "ru": "Геодезисты установили <w>колышек</w> в каждом углу поля, чтобы отметить границы участка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wooden stake",
                        "drive a stake"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заострённый кусок дерева или металла, вбиваемый в землю в качестве метки или опоры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кол",
                                "target_lang_sense_clarification": "заостренная палка"
                            },
                            {
                                "target_lang_word": "колышек",
                                "target_lang_sense_clarification": "маленький кол"
                            },
                            {
                                "target_lang_word": "столб",
                                "target_lang_sense_clarification": "опора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11de9b6e-b8b4-3145-a1a3-b84f98467fa6",
                    "sense_definition": "A share or financial interest in a business or situation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "business_interest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a controlling <w>stake</w> in the software company last year.",
                            "target_lang_translations": {
                                "ru": "В прошлом году она купила контрольный <w>пакет</w> акций компании-разработчика программного обеспечения."
                            }
                        },
                        {
                            "text": "All employees feel they have a personal <w>stake</w> in the success of the project.",
                            "target_lang_translations": {
                                "ru": "Все сотрудники чувствуют свою личную <w>заинтересованность</w> в успехе проекта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "majority stake",
                        "equity stake",
                        "minority stake"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доля или финансовый интерес в бизнесе или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доля",
                                "target_lang_sense_clarification": "часть в бизнесе"
                            },
                            {
                                "target_lang_word": "пай",
                                "target_lang_sense_clarification": "взнос/часть"
                            },
                            {
                                "target_lang_word": "пакет акций",
                                "target_lang_sense_clarification": "ценные бумаги"
                            },
                            {
                                "target_lang_word": "заинтересованность",
                                "target_lang_sense_clarification": "личный интерес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef71dbb7-9b43-3ba7-af1c-e146726e3ac7",
                    "sense_definition": "Something valuable, such as money, that is risked on the result of a game or competition.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "gambling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>stakes</w> were very high during the final round of the poker tournament.",
                            "target_lang_translations": {
                                "ru": "<w>Ставки</w> были очень высоки во время финального раунда покерного турнира."
                            }
                        },
                        {
                            "text": "They are playing for high <w>stakes</w>, risking their entire savings on one game.",
                            "target_lang_translations": {
                                "ru": "Они играют по высоким <w>ставкам</w>, рискуя всеми своими сбережениями в одной игре."
                            }
                        }
                    ],
                    "synonyms": [
                        "wager",
                        "bet"
                    ],
                    "common_phrases": [
                        "high stakes",
                        "raise the stakes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-либо ценное, например деньги, чем рискуют в игре или соревновании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ставка",
                                "target_lang_sense_clarification": "сумма риска"
                            },
                            {
                                "target_lang_word": "кон",
                                "target_lang_sense_clarification": "место для ставок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b682585-6ed1-3b9b-bdf7-a5f515b4bb7a",
                    "sense_definition": "A wooden post to which a condemned person was tied to be burned alive as a form of execution.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "execution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In medieval history, heretics were sometimes burned at the <w>stake</w>.",
                            "target_lang_translations": {
                                "ru": "В средневековой истории еретиков иногда сжигали на <w>костре</w>."
                            }
                        },
                        {
                            "text": "Joan of Arc is a famous historical figure who died at the <w>stake</w>.",
                            "target_lang_translations": {
                                "ru": "Жанна д'Арк — известная историческая личность, погибшая на <w>костре</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "at the stake",
                        "burn at the stake"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревянный столб, к которому привязывали приговоренного для сожжения заживо."

                {
                    "sense_id": "969ee267-5a35-3e6d-b8f4-febdf71447fe",
                    "sense_definition": "To press the accelerator of a vehicle briefly to rev the engine.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>goosed</w> the engine to keep it from stalling.",
                            "target_lang_translations": {
                                "ru": "Он <w>газанул</w> двигателем, чтобы тот не заглох."

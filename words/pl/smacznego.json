    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "7e9c50c9-1ae1-3acf-b54e-4bbb72f898c4",
                    "sense_definition": "Zwrot grzecznościowy używany przed posiłkiem, wyrażający życzenie, aby jedzenie smakowało.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy kelner podał nam obiad, uśmiechnął się i powiedział <w>smacznego</w>.",
                            "target_lang_translations": {
                                "ru": "Когда официант подал нам обед, он улыбнулся и сказал: «<w>Приятного аппетита</w>»."
                            }
                        },
                        {
                            "text": "Panie i panowie! Kapitan statku oraz szef kuchni życzą <w>smacznego</w>!",
                            "target_lang_translations": {
                                "ru": "Дамы и господа! Капитан корабля и шеф-повар желают вам <w>приятного аппетита</w>!"
                            }
                        }
                    ],
                    "antonyms": [
                        "a udław się!"
                    ],
                    "common_phrases": [
                        "życzyć smacznego",
                        "nawzajem smacznego"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливое выражение, используемое перед едой, чтобы пожелать приятного аппетита."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приятного аппетита",
                                "target_lang_sense_clarification": "пожелание перед едой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0e918c9f-b1b0-3372-a3a4-63507d41ab9d",
                    "sense_definition": "Forma fleksyjna przymiotnika <w>smaczny</w> w dopełniaczu liczby pojedynczej (rodzaj męski i nijaki) lub bierniku (rodzaj męskoosobowy).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szukam w lodówce czegoś <w>smacznego</w> na szybką kolację.",
                            "target_lang_translations": {
                                "ru": "Я ищу в холодильнике чего-нибудь <w>вкусного</w> на быстрый ужин."
                            }
                        },
                        {
                            "text": "Znam jednego bardzo <w>smacznego</w> pączka, którego sprzedają w tej piekarni.",
                            "target_lang_translations": {
                                "ru": "Я знаю один очень <w>вкусный</w> пончик, который продают в этой пекарне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "coś smacznego",
                        "wszystkiego smacznego"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Склоняемая форма прилагательного «smaczny» (вкусный) в родительном или винительном падеже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкусного",
                                "target_lang_sense_clarification": "форма прилагательного «вкусный»"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "frequency": "Low",
                    "semantic_group_id": "disorder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Внезапный порыв ветра успел <смешать> все бумаги на столе.",
                            "target_lang_translations": {
                                "en": "A sudden gust of wind managed to <mix up> all the papers on the table."
                            }

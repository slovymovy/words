    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4d577bd7-c28f-33d0-a276-bd9b63696b99",
                    "sense_definition": "Lasting for a long time, often longer than necessary or expected.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She died last night after a <w>lengthy</w> illness.",
                            "target_lang_translations": {
                                "ru": "Она умерла прошлой ночью после <w>продолжительной</w> болезни."
                            }
                        },
                        {
                            "text": "The committee engaged in a <w>lengthy</w> discussion regarding the new policy.",
                            "target_lang_translations": {
                                "ru": "Комитет вступил в <w>длительную</w> дискуссию по поводу новой политики."
                            }
                        }
                    ],
                    "synonyms": [
                        "extended",
                        "prolonged",
                        "protracted",
                        "long"
                    ],
                    "antonyms": [
                        "brief",
                        "short"
                    ],
                    "common_phrases": [
                        "lengthy process",
                        "lengthy discussion",
                        "lengthy delay",
                        "lengthy investigation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжающийся долгое время, часто дольше, чем необходимо или ожидалось."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "длительный",
                                "target_lang_sense_clarification": "продолжающийся долго"
                            },
                            {
                                "target_lang_word": "затяжной",
                                "target_lang_sense_clarification": "длящийся слишком долго, утомительный"
                            },
                            {
                                "target_lang_word": "продолжительный",
                                "target_lang_sense_clarification": "занимающий много времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36dd13d1-c3fe-3768-8ce5-d0f49f5a31e6",
                    "sense_definition": "Describing a person who speaks or writes using too many words.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verbosity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>lengthy</w> preacher made the service last much longer than usual.",
                            "target_lang_translations": {
                                "ru": "<w>Многословный</w> проповедник затянул службу намного дольше обычного."
                            }
                        },
                        {
                            "text": "He is a <w>lengthy</w> storyteller who tends to include every minor detail.",
                            "target_lang_translations": {
                                "ru": "Он <w>многословный</w> рассказчик, который склонен включать каждую мелкую деталь."
                            }
                        }
                    ],
                    "synonyms": [
                        "long-winded",
                        "verbose",
                        "wordy"
                    ],
                    "antonyms": [
                        "concise",
                        "terse"
                    ],
                    "target_lang_definitions": {

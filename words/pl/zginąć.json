    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a8982f52-d094-3df1-803c-28357b55ac76",
                    "sense_definition": "Stracić życie, zazwyczaj w sposób nagły, tragiczny, w wyniku wypadku lub wojny",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj w poważnym wypadku samochodowym <w>zginęły</w> dwie osoby.",
                            "target_lang_translations": {
                                "ru": "Вчера в серьезной автомобильной аварии <w>погибли</w> два человека."
                            }
                        },
                        {
                            "text": "Mój pradziadek <w>zginął</w> na wojnie, walcząc w obronie ojczyzny.",
                            "target_lang_translations": {
                                "ru": "Мой прадед <w>погиб</w> на войне, сражаясь за родину."
                            }
                        }
                    ],
                    "synonyms": [
                        "umrzeć",
                        "ponieść śmierć",
                        "polec"
                    ],
                    "antonyms": [
                        "przeżyć",
                        "ocaleć"
                    ],
                    "common_phrases": [
                        "zginąć na miejscu",
                        "zginąć tragicznie",
                        "zginąć w wypadku",
                        "zginąć na wojnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишиться жизни, обычно внезапно, трагически, в результате несчастного случая или войны"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погибнуть",
                                "target_lang_sense_clarification": "умереть насильственной смертью или в результате катастрофы"
                            },
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "быть убитым на войне (высокий стиль)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9882333f-e3ca-34f8-ae3a-276d3325c380",
                    "sense_definition": "Zostać zgubionym, zniknąć bez śladu lub zawieruszyć się (o przedmiotach lub osobach)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moje klucze do mieszkania gdzieś <w>zginęły</w> i nie mogę wejść do środka.",
                            "target_lang_translations": {
                                "ru": "Мои ключи от квартиры где-то <w>пропали</w>, и я не могу попасть внутрь."
                            }
                        },
                        {
                            "text": "Pies uciekł z podwórka i <w>zginął</w> bez śladu w pobliskim lesie.",
                            "target_lang_translations": {
                                "ru": "Пёс убежал со двора и бесследно <w>сгинул</w> в соседнем лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgubić się",
                        "zaginąć",
                        "zniknąć",
                        "przepaść",
                        "zawieruszyć się"
                    ],
                    "antonyms": [
                        "znaleźć się",
                        "odnaleźć się"
                    ],
                    "common_phrases": [
                        "zginąć bez śladu",
                        "zginąć w tłumie",
                        "zginąć bez wieści"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потеряться, исчезнуть без следа или запропаститься (о предметах или людях)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропасть",
                                "target_lang_sense_clarification": "исчезнуть, быть утерянным"
                            },
                            {
                                "target_lang_word": "потеряться",
                                "target_lang_sense_clarification": "быть утраченным (о вещах)"
                            },
                            {
                                "target_lang_word": "сгинуть",
                                "target_lang_sense_clarification": "исчезнуть бесследно (разговорное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0aa80496-2930-3161-a721-dcb8568e88e8",
                    "sense_definition": "Przestać istnieć, ulec całkowitemu zniszczeniu lub zapomnieniu (o ideach, państwach, wartościach)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_end",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele starożytnych cywilizacji bezpowrotnie <w>zginęło</w> z powierzchni ziemi.",
                            "target_lang_translations": {
                                "ru": "Многие древние цивилизации безвозвратно <w>исчезли</w> с лица земли."
                            }
                        },
                        {
                            "text": "Ostatnia nadzieja na pokojowe rozwiązanie konfliktu <w>zginęła</w> wraz z wybuchem walk.",
                            "target_lang_translations": {
                                "ru": "Последняя надежда на мирное разрешение конфликта <w>погибла</w> с началом боев."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeminąć",
                        "upaść",
                        "skończyć się"
                    ],
                    "antonyms": [
                        "przetrwać",
                        "powstać",
                        "ocaleć"
                    ],
                    "common_phrases": [
                        "zginąć marnie",
                        "nadzieja zginęła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать существовать, подвергнуться полному уничтожению или забвению (об идеях, государствах, ценностях)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исчезнуть",
                                "target_lang_sense_clarification": "перестать быть, скрыться из виду"
                            },
                            {
                                "target_lang_word": "погибнуть",
                                "target_lang_sense_clarification": "разрушиться, прекратить существование"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
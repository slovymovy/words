    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b5697e29-ac17-3875-be5a-749082ab9381",
                    "sense_definition": "Typical or normal; being the most common type or standard example.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "typical_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>average</w> person spends about eight hours a day sleeping.",

                    ],
                    "synonyms": [
                        "chest"
                    ],
                    "common_phrases": [
                        "beat one's breast"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary or formal compared to 'chest'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя передняя часть человеческого тела между шеей и животом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грудь",
                                "target_lang_sense_clarification": "часть тела"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "47c3f2b7-01c5-3f51-b269-558733a11d50",
                    "sense_definition": "Voedsel bereiden door het in een pan of oven te verhitten, meestal met boter of olie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn moeder gaat morgen een heerlijke appeltaart <w>bakken</w>.",
                            "target_lang_translations": {
                                "en": "My mother is going to <w>bake</w> a delicious apple pie tomorrow.",
                                "ru": "Завтра моя мама будет <w>печь</w> вкусный яблочный пирог."
                            }
                        },
                        {
                            "text": "Je moet het vlees goed <w>bakken</w> totdat het bruin is.",
                            "target_lang_translations": {
                                "en": "You have to <w>fry</w> the meat well until it is brown.",
                                "ru": "Мясо нужно хорошо <w>обжарить</w>, пока оно не подрумянится."
                            }
                        }
                    ],
                    "synonyms": [
                        "braden"
                    ],
                    "antonyms": [
                        "koken"
                    ],
                    "common_phrases": [
                        "brood bakken",
                        "taart bakken",
                        "vlees bakken",
                        "bruin bakken",
                        "eieren bakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To prepare food by heating it in a pan or oven, usually with butter or oil.",
                        "ru": "Готовить пищу, нагревая её в сковороде или духовке, обычно с использованием масла или жира."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bake",
                                "target_lang_sense_clarification": "in an oven (bread, cakes, pie)"
                            },
                            {
                                "target_lang_word": "fry",
                                "target_lang_sense_clarification": "in a pan with fat (meat, eggs)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "печь",
                                "target_lang_sense_clarification": "в духовке (пироги, хлеб)"
                            },
                            {
                                "target_lang_word": "жарить",
                                "target_lang_sense_clarification": "на сковороде (мясо, яйца)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd6d4b84-c52a-3c3c-9ad4-84deecfc606e",
                    "sense_definition": "Voorwerpen van klei of aarde hard maken door ze sterk te verhitten.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "crafting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kunstenaar <w>bakte</w> prachtige vazen in zijn eigen oven.",
                            "target_lang_translations": {
                                "en": "The artist <w>fired</w> beautiful vases in his own kiln.",
                                "ru": "Художник <w>обжигал</w> прекрасные вазы в своей собственной печи."
                            }
                        },
                        {
                            "text": "Vroeger werden stenen in de zon gedroogd in plaats van <w>gebakken</w>.",
                            "target_lang_translations": {
                                "en": "In the past, bricks were dried in the sun instead of <w>baked</w>.",
                                "ru": "Раньше кирпичи сушили на солнце, а не <w>обжигали</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "potten bakken",
                        "stenen bakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To harden objects made of clay or earth by heating them strongly.",
                        "ru": "Придавать твердость предметам из глины или земли путем сильного нагревания."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fire",
                                "target_lang_sense_clarification": "pottery or ceramics in a kiln"
                            },
                            {
                                "target_lang_word": "bake",
                                "target_lang_sense_clarification": "bricks or clay"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обжигать",
                                "target_lang_sense_clarification": "керамику или кирпичи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e7d27bf-95b2-33b4-aa3f-cbace7c710b9",
                    "sense_definition": "Niet slagen voor een examen of toets.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "education_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft niet genoeg gestudeerd en is voor het examen <w>gebakt</w>.",
                            "target_lang_translations": {
                                "en": "He didn't study enough and <w>failed</w> the exam.",
                                "ru": "Он недостаточно учился и <w>провалился</w> на экзамене."
                            }
                        },
                        {
                            "text": "Als je nu niet begint met leren, zul je zeker <w>bakken</w>.",
                            "target_lang_translations": {
                                "en": "If you don't start studying now, you will definitely <w>fail</w>.",
                                "ru": "Если ты сейчас не начнешь учиться, ты точно <w>завалишь</w> экзамен."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakken",
                        "buizen"
                    ],
                    "antonyms": [
                        "slagen"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "studententaal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To fail an exam or test.",
                        "ru": "Не сдать экзамен или тест (разговорное)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fail",
                                "target_lang_sense_clarification": "not pass an exam"
                            },
                            {
                                "target_lang_word": "flunk",
                                "target_lang_sense_clarification": "informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "провалиться",
                                "target_lang_sense_clarification": "потерпеть неудачу на экзамене"
                            },
                            {
                                "target_lang_word": "завалить",
                                "target_lang_sense_clarification": "не сдать (сленг)"
                            }
                        ]
                    }
                }
            ]
        },

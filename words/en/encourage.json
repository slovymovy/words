{
    "word_family": [
        "courage",
        "encouragement",
        "courageous"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "799eaefe-03b7-3e13-a0fb-9ed6e6e71949",
                    "sense_definition": "To give support, confidence, or hope to someone.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My parents always <w>encourage</w> me to follow my dreams.",
                            "target_lang_translations": {
                                "ru": "Мои родители всегда <w>поддерживают</w> моё стремление следовать за мечтой."
                            }
                        },
                        {
                            "text": "The teacher <w>encouraged</w> the shy student to answer the question.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>ободрил</w> застенчивого ученика, чтобы тот ответил на вопрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "motivate",
                        "hearten",
                        "embolden",
                        "inspire"
                    ],
                    "antonyms": [
                        "discourage",
                        "dishearten"
                    ],
                    "common_phrases": [
                        "encourage someone",
                        "encourage students",
                        "encourage children"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать кому-либо поддержку, уверенность или надежду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ободрять",
                                "target_lang_sense_clarification": "внушать бодрость и уверенность"
                            },
                            {
                                "target_lang_word": "поддерживать",
                                "target_lang_sense_clarification": "оказывать моральную помощь"
                            },
                            {
                                "target_lang_word": "воодушевлять",
                                "target_lang_sense_clarification": "вдохновлять на свершения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbf1a855-0811-352d-9f5a-3a7369f707e6",
                    "sense_definition": "To strongly recommend or advise an action.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>encourage</w> the use of public transport to reduce pollution.",
                            "target_lang_translations": {
                                "ru": "Мы <w>поощряем</w> использование общественного транспорта для снижения загрязнения среды."
                            }
                        },
                        {
                            "text": "Doctors <w>encourage</w> patients to eat a balanced diet.",
                            "target_lang_translations": {
                                "ru": "Врачи <w>настоятельно рекомендуют</w> пациентам придерживаться сбалансированной диеты."
                            }
                        }
                    ],
                    "synonyms": [
                        "urge",
                        "recommend",
                        "advocate"
                    ],
                    "antonyms": [
                        "discourage"
                    ],
                    "common_phrases": [
                        "encourage the use of",
                        "encourage people to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настоятельно рекомендовать или советовать какое-либо действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рекомендовать",
                                "target_lang_sense_clarification": "советовать поступить определенным образом"
                            },
                            {
                                "target_lang_word": "призывать",
                                "target_lang_sense_clarification": "настойчиво просить или побуждать к действию"
                            },
                            {
                                "target_lang_word": "поощрять",
                                "target_lang_sense_clarification": "выражать одобрение действию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d79cfa1-69e5-310d-9806-e0fe08aed447",
                    "sense_definition": "To help something develop, grow, or become more successful.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government wants to <w>encourage</w> investment in small businesses.",
                            "target_lang_translations": {
                                "ru": "Правительство хочет <w>стимулировать</w> инвестиции в малый бизнес."
                            }
                        },
                        {
                            "text": "Warm weather helps to <w>encourage</w> the growth of plants.",
                            "target_lang_translations": {
                                "ru": "Тёплая погода <w>способствует</w> росту растений."
                            }
                        }
                    ],
                    "synonyms": [
                        "foster",
                        "promote",
                        "nurture",
                        "stimulate"
                    ],
                    "antonyms": [
                        "discourage"
                    ],
                    "common_phrases": [
                        "encourage growth",
                        "encourage development",
                        "encourage innovation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помогать чему-либо развиваться, расти или становиться более успешным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стимулировать",
                                "target_lang_sense_clarification": "давать толчок к развитию"
                            },
                            {
                                "target_lang_word": "способствовать",
                                "target_lang_sense_clarification": "содействовать процессу или росту"
                            },
                            {
                                "target_lang_word": "развивать",
                                "target_lang_sense_clarification": "улучшать или усиливать что-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
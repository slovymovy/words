    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e565db4-f7e4-35ca-ac97-d911112e6d86",
                    "sense_definition": "Miejsce zamieszkania lub przebywania ludzi, często obejmujące dom wraz z otaczającym go terenem gospodarczym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dwelling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi dziadkowie mieli piękne, stare <w>siedlisko</w> na skraju lasu.",
                            "target_lang_translations": {
                                "ru": "У моих дедушки и бабушки была красивая старая <w>усадьба</w> на опушке леса."
                            }
                        },
                        {
                            "text": "Młode małżeństwo kupiło opuszczone <w>siedlisko</w> i planuje je wyremontować.",
                            "target_lang_translations": {
                                "ru": "Молодая пара купила заброшенную <w>усадьбу</w> и планирует её отремонтировать."
                            }
                        }
                    ],
                    "synonyms": [
                        "dom",
                        "siedziba",
                        "sadyba"
                    ],
                    "common_phrases": [
                        "rodzinne siedlisko",
                        "opuszczone siedlisko",
                        "wiejskie siedlisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место жительства, часто включающее дом с прилегающей хозяйственной территорией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усадьба",
                                "target_lang_sense_clarification": "дом с земельным участком"
                            },
                            {
                                "target_lang_word": "жилище",
                                "target_lang_sense_clarification": "место проживания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b11d55d-597c-3b56-9084-533fb8ad428b",
                    "sense_definition": "Naturalne środowisko przyrodnicze, w którym żyją określone gatunki roślin lub zwierząt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wylesianie niszczy naturalne <w>siedlisko</w> wielu rzadkich gatunków ptaków.",
                            "target_lang_translations": {
                                "ru": "Вырубка лесов уничтожает естественную <w>среду обитания</w> многих редких видов птиц."
                            }
                        },
                        {
                            "text": "To bagna są idealnym <w>siedliskiem</w> dla płazów i owadów.",
                            "target_lang_translations": {
                                "ru": "Именно болота являются идеальной <w>средой обитания</w> для земноводных и насекомых."
                            }
                        }
                    ],
                    "synonyms": [
                        "habitat",
                        "ostoja",
                        "matecznik"
                    ],
                    "common_phrases": [
                        "naturalne siedlisko",
                        "ochrona siedlisk",
                        "siedlisko przyrodnicze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественная природная среда, в которой живут определенные виды растений или животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "среда обитания",
                                "target_lang_sense_clarification": "экология"
                            },
                            {
                                "target_lang_word": "местообитание",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f46e66d-b390-3d4b-ab76-1b669b7f3ed9",
                    "sense_definition": "Miejsce, w którym rozwijają się zarazki, choroby lub negatywne zjawiska społeczne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "source_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Brudna i wilgotna piwnica stała się <w>siedliskiem</w> pleśni i bakterii.",
                            "target_lang_translations": {
                                "ru": "Грязный и влажный подвал стал <w>рассадником</w> плесени и бактерий."
                            }
                        },
                        {
                            "text": "Ta dzielnica była przez lata uważana za <w>siedlisko</w> przestępczości.",
                            "target_lang_translations": {
                                "ru": "Этот район годами считался <w>рассадником</w> преступности."
                            }
                        }
                    ],
                    "synonyms": [
                        "ognisko"
                    ],
                    "common_phrases": [
                        "siedlisko bakterii",
                        "siedlisko zła",
                        "siedlisko korupcji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где развиваются микробы, болезни или негативные социальные явления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассадник",
                                "target_lang_sense_clarification": "источник чего-либо негативного"
                            },
                            {
                                "target_lang_word": "очаг",
                                "target_lang_sense_clarification": "место зарождения или распространения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "synonyms": [
                        "очищать"
                    ],
                    "common_phrases": [
                        "чистить картошку",
                        "чистить лук",
                        "чистить рыбу",
                        "чистить фрукты"
                    ],
                    "target_lang_definitions": {
                        "en": "To remove the skin, peel, or top layer from vegetables, fruits, or fish."
                    },
                    "translations": {

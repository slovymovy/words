                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нечёткий",
                                "target_lang_sense_clarification": "лишенный резкости, фокуса"
                            },
                            {
                                "target_lang_word": "размытый",
                                "target_lang_sense_clarification": "с неясными контурами"
                            },
                            {
                                "target_lang_word": "неясный",
                                "target_lang_sense_clarification": "плохо различимый глазом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6d70d19-f657-3799-bbae-e40213eef474",
                    "sense_definition": "Not thinking clearly or showing a lack of focus; absent-minded.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She looked a bit <w>vague</w> during the meeting and didn't seem to be listening.",
                            "target_lang_translations": {
                                "ru": "Во время встречи она выглядела немного <w>рассеянной</w> и, казалось, не слушала."
                            }
                        },
                        {
                            "text": "He gave me a <w>vague</w> smile but didn't say anything.",
                            "target_lang_translations": {
                                "ru": "Он <w>рассеянно</w> улыбнулся мне, но ничего не сказал."
                            }
                        }

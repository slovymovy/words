                            {
                                "target_lang_word": "публиковать",
                                "target_lang_sense_clarification": "делать общеизвестным через печать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d526668-3419-3401-b91f-8091de0abff5",
                    "sense_definition": "Zich voordoen als iemand of iets wat men niet is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oplichter <w>gaf</w> zich <w>uit</w> voor een medewerker van de bank.",
                            "target_lang_translations": {
                                "en": "The scammer <w>posed</w> as a bank employee.",
                                "ru": "Мошенник <w>выдавал</w> себя за сотрудника банка."
                            }
                        },
                        {
                            "text": "Hij probeerde zich <w>uit</w> te <w>geven</w> voor een beroemde acteur om binnen te komen.",
                            "target_lang_translations": {
                                "en": "He tried to <w>pass</w> himself <w>off</w> as a famous actor to get in.",
                                "ru": "Он пытался <w>выдать</w> себя за известного актера, чтобы войти."
                            }
                        }
                    ],
                    "synonyms": [
                        "zich voordoen als"
                    ],
                    "common_phrases": [
                        "zich uitgeven voor"
                    ],
                    "target_lang_definitions": {
                        "en": "To pretend to be someone or something one is not.",
                        "ru": "Выдавать себя за кого-то или что-то, чем человек не является."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pose as",
                                "target_lang_sense_clarification": "impersonate"
                            },
                            {
                                "target_lang_word": "pass oneself off as",
                                "target_lang_sense_clarification": "pretend to be"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выдавать себя",
                                "target_lang_sense_clarification": "притворяться кем-то другим"
                            }
                        ]
                    }
                }
            ]

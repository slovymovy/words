        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "93e6d103-179f-3a8a-b594-bd63740f69cf",
                    "sense_definition": "Zorgen dat iets in goede staat blijft door er regelmatig aandacht aan te besteden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "maintenance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>onderhoud</w> mijn auto goed zodat hij lang meegaat.",
                            "target_lang_translations": {
                                "en": "I <w>maintain</w> my car well so that it lasts a long time.",
                                "ru": "Я хорошо <w>слежу</w> за своей машиной, чтобы она прослужила долго."
                            }
                        },
                        {
                            "text": "De oude gebouwen in het centrum worden slecht <w>onderhouden</w>.",
                            "target_lang_translations": {
                                "en": "The old buildings in the center are poorly <w>maintained</w>.",
                                "ru": "Старые здания в центре плохо <w>содержатся</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verzorgen",
                        "bijhouden"
                    ],
                    "antonyms": [
                        "verwaarlozen"
                    ],
                    "common_phrases": [
                        "goed onderhouden",
                        "slecht onderhouden",
                        "tuin onderhouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To keep something in good condition by checking or repairing it regularly.",
                        "ru": "Следить за тем, чтобы что-то оставалось в хорошем состоянии, уделяя этому регулярное внимание."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "maintain",
                                "target_lang_sense_clarification": "keep in good condition"
                            },
                            {
                                "target_lang_word": "service",
                                "target_lang_sense_clarification": "perform maintenance on a machine/vehicle"
                            },
                            {
                                "target_lang_word": "keep up",
                                "target_lang_sense_clarification": "prevent from deteriorating"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "содержать",
                                "target_lang_sense_clarification": "в порядке или исправности"
                            },
                            {
                                "target_lang_word": "обслуживать",
                                "target_lang_sense_clarification": "технически"
                            },
                            {
                                "target_lang_word": "ухаживать",
                                "target_lang_sense_clarification": "заботиться о состоянии"
                            }
                        ]

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбрызгивать",
                                "target_lang_sense_clarification": "жидкость"
                            },
                            {
                                "target_lang_word": "расплёскивать",
                                "target_lang_sense_clarification": "проливать, брызгать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac145dd0-e0ee-39f4-95df-8b39f8aa3783",
                    "sense_definition": "O cieczy: uderzać o coś i gwałtownie rozdzielać się na wiele drobnych kropel",

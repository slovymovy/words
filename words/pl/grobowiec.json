            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0b7ebbc6-264c-3085-abf8-d4ad2a40f180",
                    "sense_definition": "Okazały grób, często w formie wolnostojącej budowli, kaplicy lub wydzielonego pomieszczenia wewnątrz innej budowli, w którym spoczywają zmarli.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "tomb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zamożna rodzina wybudowała sobie piękny <w>grobowiec</w> na miejskim cmentarzu.",
                            "target_lang_translations": {
                                "ru": "Богатая семья построила себе красивый <w>склеп</w> на городском кладбище."
                            }
                        },
                        {
                            "text": "Archeolodzy odkryli starożytny <w>grobowiec</w> pełen skarbów i artefaktów.",
                            "target_lang_translations": {
                                "ru": "Археологи обнаружили древнюю <w>гробницу</w>, полную сокровищ и артефактов."
                            }
                        }
                    ],
                    "synonyms": [
                        "grobowisko"
                    ],
                    "common_phrases": [
                        "rodzinny grobowiec",
                        "kamienny grobowiec",
                        "złożyć w grobowcu",
                        "otworzyć grobowiec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Монументальное захоронение, часто в виде отдельного здания, часовни или выделенного помещения, где покоятся умершие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "склеп",
                                "target_lang_sense_clarification": "постройка с внутренним помещением для гробов"
                            },
                            {
                                "target_lang_word": "гробница",
                                "target_lang_sense_clarification": "архитектурное сооружение, вмещающее тело умершего"
                            },
                            {
                                "target_lang_word": "мавзолей",
                                "target_lang_sense_clarification": "большое монументальное погребальное сооружение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9935de00-ef4e-3db1-8cf9-8f26b61c74b5",
                    "sense_definition": "Żartobliwe lub potoczne określenie fortepianu, nawiązujące do jego dużych rozmiarów i kształtu przypominającego skrzynię.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "piano_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W małym salonie stał wielki, czarny <w>grobowiec</w>, na którym nikt już nie grał.",
                            "target_lang_translations": {
                                "ru": "В маленькой гостиной стоял огромный черный <w>гроб</w>, на котором никто уже не играл."
                            }
                        },
                        {
                            "text": "Musimy wynająć ekipę, żeby wnieść ten ciężki <w>grobowiec</w> na drugie piętro.",
                            "target_lang_translations": {
                                "ru": "Нам нужно нанять бригаду, чтобы затащить этот тяжелый <w>гроб</w> на третий этаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "grobowisko"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "humorous"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шутливое или разговорное название рояля, намекающее на его большие размеры и форму, напоминающую ящик."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гроб",
                                "target_lang_sense_clarification": "сленговое название рояля или большого футляра"
                            },
                            {
                                "target_lang_word": "бандура",
                                "target_lang_sense_clarification": "о громоздком предмете или музыкальном инструменте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a959a0b-8f04-3bfe-bbc4-540d7ac3fabc",
                    "sense_definition": "W gwarze przestępczej: piwnica, często służąca jako kryjówka lub miejsce przetrzymywania.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "criminal_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodzieje schowali skradzione towary w wilgotnym <w>grobowcu</w> pod starym budynkiem.",
                            "target_lang_translations": {
                                "ru": "Воры спрятали краденые товары в сыром <w>подвале</w> под старым зданием."
                            }
                        },
                        {
                            "text": "Nikt nie chciał trafić do tego ciemnego <w>grobowca</w> bez okien.",
                            "target_lang_translations": {
                                "ru": "Никто не хотел попасть в этот темный <w>подвал</w> без окон."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "criminal/prison slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "На воровском жаргоне: подвал, часто служащий тайником или местом содержания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвал",
                                "target_lang_sense_clarification": "помещение ниже уровня земли"
                            },
                            {
                                "target_lang_word": "каменный мешок",
                                "target_lang_sense_clarification": "тесное, темное помещение"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "translation",
        "translator",
        "translatable",
        "translational",
        "mistranslate",
        "retranslate"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4415e4ef-9791-3f72-bb6e-434fca9d4fd2",
                    "sense_definition": "To change spoken words or written text from one language into another.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you <w>translate</w> this sentence into French for me?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>перевести</w> это предложение на французский для меня?"
                            }
                        },
                        {
                            "text": "She works hard to <w>translate</w> official documents for the government.",
                            "target_lang_translations": {
                                "ru": "Она усердно работает, чтобы <w>переводить</w> официальные документы для правительства."
                            }
                        }
                    ],
                    "synonyms": [
                        "interpret",
                        "render",
                        "convert"
                    ],
                    "common_phrases": [
                        "translate into English",
                        "translate from Spanish",
                        "translate word for word",
                        "translate literally"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переводить устную речь или письменный текст с одного языка на другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переводить",
                                "target_lang_sense_clarification": "текст или речь"
                            },
                            {
                                "target_lang_word": "перевести",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce73247a-72fe-3cfd-9553-4421426b8218",
                    "sense_definition": "To express the meaning of something in a different, often simpler, way to make it easier to understand.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor tried to <w>translate</w> the complex medical terms into simple language.",
                            "target_lang_translations": {
                                "ru": "Врач попытался <w>перевести</w> сложные медицинские термины на простой язык."
                            }
                        },
                        {
                            "text": "It is difficult to <w>translate</w> technical jargon for a general audience.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>перевести</w> технический жаргон для широкой аудитории."
                            }
                        }
                    ],
                    "synonyms": [
                        "paraphrase",
                        "rephrase",
                        "explain",
                        "decode"
                    ],
                    "common_phrases": [
                        "translate into plain English",
                        "translate jargon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать смысл чего-либо иным, часто более простым способом, для облегчения понимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переводить",
                                "target_lang_sense_clarification": "объяснять простыми словами"
                            },
                            {
                                "target_lang_word": "истолковывать",
                                "target_lang_sense_clarification": "интерпретировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9f3cd60-bd5e-3ff8-a968-d9c7fe195d41",
                    "sense_definition": "To change something into a different form or medium, or to result in a particular outcome.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "conversion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>translate</w> our ideas into a concrete plan of action.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>воплотить</w> наши идеи в конкретный план действий."
                            }
                        },
                        {
                            "text": "His enthusiasm did not <w>translate</w> into actual sales figures.",
                            "target_lang_translations": {
                                "ru": "Его энтузиазм не <w>трансформировался</w> в реальные показатели продаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "transform",
                        "convert",
                        "turn",
                        "adapt"
                    ],
                    "common_phrases": [
                        "translate into action",
                        "translate well",
                        "translate poorly",
                        "translate into success"
                    ],
                    "target_lang_definitions": {
                        "ru": "Преобразовывать что-либо в иную форму или среду, или приводить к определенному результату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преобразовывать",
                                "target_lang_sense_clarification": "изменять форму"
                            },
                            {
                                "target_lang_word": "воплощать",
                                "target_lang_sense_clarification": "идеи в жизнь"
                            },
                            {
                                "target_lang_word": "трансформировать",
                                "target_lang_sense_clarification": "превращать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3325bb0-cf68-3623-a82c-0c7c61f3dae6",
                    "sense_definition": "To move a geometric figure or object from one place to another without rotating it or changing its shape.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "spatial_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you <w>translate</w> the triangle three units to the right, its orientation remains the same.",
                            "target_lang_translations": {
                                "ru": "Если вы <w>сдвинете</w> треугольник на три единицы вправо, его ориентация останется прежней."
                            }
                        },
                        {
                            "text": "The software allows users to rotate and <w>translate</w> 3D models on the screen.",
                            "target_lang_translations": {
                                "ru": "Программа позволяет пользователям вращать и <w>перемещать</w> 3D-модели на экране."
                            }
                        }
                    ],
                    "synonyms": [
                        "shift",
                        "slide",
                        "move"
                    ],
                    "antonyms": [
                        "rotate"
                    ],
                    "common_phrases": [
                        "translate the axis",
                        "translate an object"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать геометрическую фигуру или объект в пространстве без вращения или деформации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдвигать",
                                "target_lang_sense_clarification": "геометрия"
                            },
                            {
                                "target_lang_word": "перемещать",
                                "target_lang_sense_clarification": "двигать объект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "858d2fca-fb73-3fc7-bbff-bc22f3a28ad7",
                    "sense_definition": "To generate a specific protein based on the genetic code carried by an mRNA molecule.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biological_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ribosomes read the genetic sequence and <w>translate</w> it into amino acids.",
                            "target_lang_translations": {
                                "ru": "Рибосомы считывают генетическую последовательность и <w>транслируют</w> её в аминокислоты."
                            }
                        },
                        {
                            "text": "The virus uses the host cell to <w>translate</w> its own genetic material.",
                            "target_lang_translations": {
                                "ru": "Вирус использует клетку-хозяина, чтобы <w>транслировать</w> свой собственный генетический материал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "translate mRNA"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Синтезировать белок на основе генетического кода мРНК."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "транслировать",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0c482ea7-8091-395a-a3ce-49f88665c28a",
                    "sense_definition": "A mathematical object or set of points formed by moving another set of points by a fixed distance and direction.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "math_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The resulting figure is a <w>translate</w> of the original square.",
                            "target_lang_translations": {
                                "ru": "Полученная фигура представляет собой <w>сдвиг</w> исходного квадрата."
                            }
                        },
                        {
                            "text": "In this vector space, every subspace is a <w>translate</w> of the origin.",
                            "target_lang_translations": {
                                "ru": "В этом векторном пространстве каждое подпространство — это <w>сдвиг</w> начала координат."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Математический объект, полученный путем параллельного переноса другого объекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдвиг",
                                "target_lang_sense_clarification": "математика"
                            },
                            {
                                "target_lang_word": "образ при переносе",
                                "target_lang_sense_clarification": "результат смещения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a41e2d2e-e530-36da-94d3-976905f8ccfe",
                    "sense_definition": "Высокая обувь, закрывающая стопу и голень (иногда до колена или выше).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В дождливую погоду лучше всего надеть резиновые <w>сапоги</w>.",
                            "target_lang_translations": {
                                "en": "In rainy weather, it is best to wear rubber <w>boots</w>."
                            }
                        },
                        {
                            "text": "Мне нужно купить тёплые зимние <w>сапоги</w> для поездки в горы.",
                            "target_lang_translations": {
                                "en": "I need to buy warm winter <w>boots</w> for a trip to the mountains."
                            }
                        }
                    ],
                    "common_phrases": [
                        "резиновые сапоги",
                        "зимние сапоги",
                        "два сапога пара",
                        "чистить сапоги"
                    ],
                    "target_lang_definitions": {
                        "en": "High footwear covering the foot and the lower leg, sometimes reaching the knee or higher."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boot",
                                "target_lang_sense_clarification": "footwear"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1b1d710-0b1a-3ac1-8020-e499d1ceb44a",
                    "sense_definition": "Кадровый офицер, профессиональный военный (в отличие от призванного из запаса).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старый <w>сапог</w> требовал идеальной дисциплины от всех подчинённых.",
                            "target_lang_translations": {
                                "en": "The old <w>martinet</w> demanded perfect discipline from all subordinates."
                            }
                        },
                        {
                            "text": "В штабе к нему относились с недоверием, потому что он был не <w>сапог</w>, а гражданский специалист.",
                            "target_lang_translations": {
                                "en": "At headquarters, he was treated with distrust because he was not a <w>career soldier</w>, but a civilian specialist."
                            }
                        }
                    ],
                    "antonyms": [
                        "пиджак"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "военный"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A career officer or professional soldier, often implying strict adherence to discipline or lack of flexibility (slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "martinet",
                                "target_lang_sense_clarification": "strict disciplinarian"
                            },
                            {
                                "target_lang_word": "career soldier",
                                "target_lang_sense_clarification": "professional military"
                            },
                            {
                                "target_lang_word": "boot",
                                "target_lang_sense_clarification": "soldier (slang)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1e637bd-52f5-39cb-be17-ba5162caf2fb",
                    "sense_definition": "Паспорт или удостоверение личности.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Без <w>сапога</w> тебя через проходную не пропустят.",
                            "target_lang_translations": {
                                "en": "Without an <w>ID</w>, they won't let you through the checkpoint."
                            }
                        },
                        {
                            "text": "Он забыл свой <w>сапог</w> дома и не смог оформить документы.",
                            "target_lang_translations": {
                                "en": "He forgot his <w>passport</w> at home and couldn't process the documents."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "специальный"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A passport or identity document (slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "passport",
                                "target_lang_sense_clarification": "document"
                            },
                            {

    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "570bb90b-6449-3775-846b-c9a69fbe618c",
                    "sense_definition": "Een klank die men maakt tijdens het nadenken of bij aarzeling.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "uiting_van_gedachte",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hm</w>, ik weet nog niet zeker of ik mee ga.",
                            "target_lang_translations": {
                                "en": "<w>Hmm</w>, I'm not sure yet if I'm going.",
                                "ru": "<w>Хм</w>, я еще не уверен, пойду ли я."
                            }
                        },
                        {
                            "text": "<w>Hm</w>, laat me daar even rustig over nadenken.",
                            "target_lang_translations": {
                                "en": "<w>Hmm</w>, let me think about that for a moment.",
                                "ru": "<w>Гм</w>, дай мне немного подумать об этом."
                            }
                        }
                    ],
                    "synonyms": [
                        "tja",
                        "nou"
                    ],
                    "common_phrases": [
                        "hm, even denken",
                        "hm, tja"
                    ],
                    "target_lang_definitions": {
                        "en": "A sound made while thinking or hesitating.",
                        "ru": "Звук, который произносят во время размышлений или при нерешительности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hmm",
                                "target_lang_sense_clarification": "sound of thinking"
                            },
                            {
                                "target_lang_word": "erm",
                                "target_lang_sense_clarification": "sound of hesitation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хм",
                                "target_lang_sense_clarification": "раздумье"
                            },
                            {
                                "target_lang_word": "гм",
                                "target_lang_sense_clarification": "колебание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bf5f4b7-0611-3a35-9898-ff1dc2be6453",
                    "sense_definition": "Een gemompeld geluid dat ontevredenheid, twijfel of bezwaar uitdrukt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "uiting_van_gedachte",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hm</w>, dat lijkt me eigenlijk heel onwaarschijnlijk.",
                            "target_lang_translations": {
                                "en": "<w>Hmm</w>, that actually seems very unlikely to me.",
                                "ru": "<w>Хм</w>, это кажется мне весьма маловероятным."
                            }
                        },
                        {
                            "text": "Hij keek naar de rekening en mompelde nors: \"<w>Hm</w>.\"",
                            "target_lang_translations": {
                                "en": "He looked at the bill and mumbled gruffly: \"<w>Hmph</w>.\"",
                                "ru": "Он посмотрел на счет и угрюмо пробормотал: «<w>Хм</w>»."
                            }
                        }
                    ],
                    "synonyms": [
                        "mwah"
                    ],
                    "common_phrases": [
                        "hm, nee",
                        "hm, vreemd"
                    ],
                    "target_lang_definitions": {
                        "en": "A mumbled sound expressing dissatisfaction, doubt, or objection.",
                        "ru": "Невнятный звук, выражающий недовольство, сомнение или возражение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hmph",
                                "target_lang_sense_clarification": "expressing dissatisfaction or objection"
                            },
                            {
                                "target_lang_word": "hmm",
                                "target_lang_sense_clarification": "expressing doubt"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хм",
                                "target_lang_sense_clarification": "сомнение"
                            },
                            {
                                "target_lang_word": "гм",
                                "target_lang_sense_clarification": "недовольство"
                            }
                        ]
                    }
                }
            ]
        },
        {

            "senses": [
                {
                    "sense_id": "9bb734de-0729-310d-9409-781e2900624f",
                    "sense_definition": "Een aanwijzing of suggestie die iemand helpt om iets te begrijpen of op te lossen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We gaven hem een <w>hint</w> bij het maken van de moeilijke puzzel.",
                            "target_lang_translations": {
                                "en": "We gave him a <w>hint</w> while doing the difficult puzzle.",
                                "ru": "Мы дали ему <w>подсказку</w>, когда он собирал сложный пазл."
                            }
                        },
                        {
                            "text": "Als je het antwoord echt niet weet, zal ik je een kleine <w>hint</w> geven.",
                            "target_lang_translations": {
                                "en": "If you really don't know the answer, I will give you a small <w>hint</w>.",
                                "ru": "Если ты действительно не знаешь ответа, я дам тебе маленькую <w>подсказку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanwijzing"
                    ],
                    "common_phrases": [
                        "een hint geven",
                        "een hint begrijpen",
                        "de hint snappen"
                    ],
                    "target_lang_definitions": {
                        "en": "A clue or suggestion that helps someone understand or solve something.",
                        "ru": "Подсказка или совет, который помогает кому-то что-то понять или решить."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hint",
                                "target_lang_sense_clarification": "suggestion or clue"
                            },

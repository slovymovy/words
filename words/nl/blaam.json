                {
                    "sense_id": "1ee249d8-85b6-308b-a4ee-68619dca78e7",
                    "sense_definition": "Een aantasting van iemands goede naam of reputatie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit schandaal wierp een zware <w>blaam</w> op zijn carrière.",
                            "target_lang_translations": {
                                "ru": "Этот скандал бросил темную <w>тень</w> на его карьеру.",
                                "en": "This scandal cast a heavy <w>stain</w> on his career."
                            }
                        },
                        {
                            "text": "Hij wilde zijn ambtstermijn afsluiten zonder enige <w>blaam</w>.",
                            "target_lang_translations": {
                                "ru": "Он хотел завершить свой срок полномочий без единого <w>пятна</w>.",
                                "en": "He wanted to conclude his term of office without any <w>blemish</w>."
                            }
                        }
                    ],
                    "synonyms": [

            "senses": [
                {
                    "sense_id": "f552cc96-01d4-3682-a6fd-6c890b975e4e",
                    "sense_definition": "Часть одежды, покрывающая руку (полностью или частично).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этой рубашки слишком длинные <w>рукава</w>.",
                            "target_lang_translations": {
                                "en": "This shirt has <w>sleeves</w> that are too long."
                            }
                        },
                        {
                            "text": "Мальчик вытер грязное лицо <w>рукавом</w>.",
                            "target_lang_translations": {
                                "en": "The boy wiped his dirty face with his <w>sleeve</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "длинный рукав",
                        "короткий рукав",
                        "засучить рукава"
                    ],
                    "target_lang_definitions": {
                        "en": "The part of a garment that covers the arm (fully or partially)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sleeve",
                                "target_lang_sense_clarification": "part of a garment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbff1459-1c26-333b-b5b5-46f91fa7ca23",
                    "sense_definition": "Гибкая труба для подачи жидкостей, газов или сыпучих веществ.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tube_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожарные быстро размотали <w>рукав</w> и начали тушить огонь.",
                            "target_lang_translations": {
                                "en": "The firefighters quickly unrolled the <w>hose</w> and started extinguishing the fire."
                            }
                        },
                        {
                            "text": "Вода под большим напором поступает в <w>рукав</w>.",
                            "target_lang_translations": {
                                "en": "Water enters the <w>hose</w> under high pressure."
                            }
                        }
                    ],
                    "synonyms": [
                        "шланг"
                    ],
                    "common_phrases": [
                        "пожарный рукав"
                    ],
                    "target_lang_definitions": {
                        "en": "A flexible tube for conveying liquids, gases, or loose substances."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hose",
                                "target_lang_sense_clarification": "flexible tube"
                            },
                            {
                                "target_lang_word": "fire hose",
                                "target_lang_sense_clarification": "specific type for firefighting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c7e620a-aecc-3d4d-aa64-078b075d69a8",
                    "sense_definition": "Устройство в виде коридора для перехода пассажиров из здания аэропорта прямо в самолёт.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "airport_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Посадка на наш рейс осуществляется через <w>рукав</w>.",
                            "target_lang_translations": {
                                "en": "Boarding for our flight is taking place via the <w>jet bridge</w>."
                            }
                        },
                        {
                            "text": "К самолёту подали <w>рукав</w>, и пассажиры начали выходить.",
                            "target_lang_translations": {
                                "en": "A <w>jet bridge</w> was connected to the plane, and passengers started to exit."
                            }
                        }
                    ],
                    "synonyms": [
                        "труба"
                    ],
                    "common_phrases": [
                        "выход через рукав"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in travel context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A corridor-like device for passengers to move directly from the airport terminal into the aircraft."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "jet bridge",
                                "target_lang_sense_clarification": "airport boarding equipment"
                            },
                            {
                                "target_lang_word": "jetway",
                                "target_lang_sense_clarification": "airport boarding equipment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5d21a53-690e-3747-9d95-0edf06ee6b2b",
                    "sense_definition": "Ответвление от главного русла реки.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "geography_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом месте река разделяется на два широких <w>рукава</w>.",
                            "target_lang_translations": {
                                "en": "At this place, the river splits into two wide <w>branches</w>."
                            }
                        },
                        {
                            "text": "Лодка медленно вошла в узкий <w>рукав</w> реки.",
                            "target_lang_translations": {
                                "en": "The boat slowly entered a narrow <w>arm</w> of the river."
                            }
                        }
                    ],
                    "synonyms": [
                        "полоса"
                    ],
                    "common_phrases": [
                        "рукав реки"
                    ],
                    "target_lang_definitions": {
                        "en": "A branch flowing away from the main channel of a river."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "branch",
                                "target_lang_sense_clarification": "of a river"
                            },
                            {
                                "target_lang_word": "arm",
                                "target_lang_sense_clarification": "of a river or body of water"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d76d8b34-aab0-3d11-ab6d-a80394019df7",
                    "sense_definition": "Полоса на местности, выделяющаяся чем-либо (например, лесом или снегом).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geography_land",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы шли по снежному <w>рукаву</w> между лесом и дорогой.",
                            "target_lang_translations": {
                                "en": "The tourists walked along a snowy <w>arm</w> between the forest and the road."
                            }
                        },
                        {
                            "text": "Зелёный <w>рукав</w> леса тянулся до самого горизонта.",
                            "target_lang_translations": {
                                "en": "A green <w>arm</w> of the forest stretched to the very horizon."
                            }
                        }
                    ],
                    "synonyms": [
                        "полоса"
                    ],
                    "target_lang_definitions": {

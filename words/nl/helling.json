    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "495b15db-1e6e-32d7-a4a6-4fd02630f70c",
                    "sense_definition": "Een schuin oplopend of aflopend vlak in het landschap.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "terrain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De fietsers hadden veel moeite om de steile <w>helling</w> op te komen.",
                            "target_lang_translations": {
                                "ru": "Велосипедистам стоило больших усилий подняться по крутому <w>склону</w>."
                            }
                        },
                        {
                            "text": "In de winter glijden de kinderen met hun slee van de <w>helling</w> af.",
                            "target_lang_translations": {
                                "ru": "Зимой дети катаются на санках со <w>склона</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "glooiing",
                        "steilte"
                    ],
                    "antonyms": [
                        "vlakte"
                    ],
                    "common_phrases": [
                        "steile helling",
                        "flauwe helling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наклонная поверхность местности, поднимающаяся или опускающаяся; склон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "склон",
                                "target_lang_sense_clarification": "поверхность горы или холма"
                            },
                            {
                                "target_lang_word": "подъём",
                                "target_lang_sense_clarification": "участок дороги вверх"
                            },
                            {
                                "target_lang_word": "спуск",
                                "target_lang_sense_clarification": "участок дороги вниз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f5a519d-6e37-3015-9b67-227f640ce2e1",
                    "sense_definition": "De mate waarin een lijn of vlak afwijkt van de horizon; de hoek die gemaakt wordt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De architect berekende de exacte <w>helling</w> van het dak voor de zonnepanelen.",
                            "target_lang_translations": {
                                "ru": "Архитектор рассчитал точный <w>уклон</w> крыши для установки солнечных панелей."
                            }
                        },
                        {
                            "text": "Door de sterke <w>helling</w> van de vloer rolde de bal direct naar de hoek.",
                            "target_lang_translations": {
                                "ru": "Из-за сильного <w>наклона</w> пола мяч покатился прямо в угол."
                            }
                        }
                    ],
                    "synonyms": [
                        "inclinatie",
                        "hoek"
                    ],
                    "common_phrases": [
                        "helling van de grafiek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Степень отклонения линии или поверхности от горизонтали; угол наклона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наклон",
                                "target_lang_sense_clarification": "положение предмета"
                            },
                            {
                                "target_lang_word": "уклон",
                                "target_lang_sense_clarification": "покатость поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a13d759-c3bc-3c2c-9dd8-71fe9e3323a4",
                    "sense_definition": "Een schuin aflopend deel van een werf waar schepen gebouwd of gerepareerd worden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het oude vissersschip moest voor groot onderhoud de <w>helling</w> op.",
                            "target_lang_translations": {
                                "ru": "Старое рыболовное судно пришлось поднять на <w>стапель</w> для капитального ремонта."
                            }
                        },
                        {
                            "text": "Tijdens de feestelijke ceremonie gleed de nieuwe boot soepel van de <w>helling</w>.",
                            "target_lang_translations": {
                                "ru": "Во время торжественной церемонии новая лодка плавно сошла со <w>стапеля</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "scheepshelling"
                    ],
                    "common_phrases": [
                        "op de helling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наклонная площадка на верфи для спуска, подъёма или ремонта судов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стапель",
                                "target_lang_sense_clarification": "платформа для судов"
                            },
                            {
                                "target_lang_word": "эллинг",
                                "target_lang_sense_clarification": "сооружение на верфи"
                            },
                            {
                                "target_lang_word": "слип",
                                "target_lang_sense_clarification": "наклонная плоскость"
                            }
                        ]
                    }
                }
            ]
        },

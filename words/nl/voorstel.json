                {
                    "sense_id": "16c95a63-8223-3b6e-a041-a74028528067",
                    "sense_definition": "Een plan, idee of suggestie dat aan anderen wordt voorgelegd ter overweging of goedkeuring.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij deed mij een interessant <w>voorstel</w> voor een nieuwe baan.",
                            "target_lang_translations": {
                                "en": "He made me an interesting <w>proposal</w> for a new job.",
                                "ru": "Он сделал мне интересное <w>предложение</w> о новой работе."
                            }
                        },
                        {
                            "text": "De directeur heeft het <w>voorstel</w> van de afdeling goedgekeurd.",
                            "target_lang_translations": {
                                "en": "The director approved the department's <w>proposal</w>.",
                                "ru": "Директор одобрил <w>предложение</w> отдела."
                            }
                        }
                    ],
                    "synonyms": [
                        "suggestie",
                        "aanbod"
                    ],
                    "common_phrases": [
                        "een voorstel doen",
                        "een voorstel aannemen",
                        "een voorstel verwerpen",
                        "een voorstel indienen"
                    ],
                    "target_lang_definitions": {
                        "en": "A plan, idea, or suggestion submitted to others for consideration or approval.",
                        "ru": "План, идея или предложение, выносимые на обсуждение или утверждение."
                    },
                    "translations": {
                        "en": [
                            {

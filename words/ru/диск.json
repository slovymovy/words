    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "76b5a6b1-d289-3659-bbf5-c674a2213416",
                    "sense_definition": "Устройство для хранения информации в компьютере или область памяти на таком устройстве.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На моём компьютере переполнен жёсткий <w>диск</w>.",
                            "target_lang_translations": {
                                "en": "The hard <w>disk</w> on my computer is full."
                            }
                        },
                        {
                            "text": "Пожалуйста, сохраните этот файл на локальный <w>диск</w>.",
                            "target_lang_translations": {
                                "en": "Please save this file to the local <w>disk</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "жёсткий диск",
                        "локальный диск",
                        "системный диск",
                        "форматировать диск"

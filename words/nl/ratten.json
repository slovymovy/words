                    "sense_definition": "Iets wegnemen zonder toestemming; stelen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_steal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij probeerde mijn fiets te <ratten> toen ik even niet oplette.",
                            "target_lang_translations": {
                                "en": "He tried to <swipe> my bike when I wasn't paying attention.",
                                "ru": "Он пытался <спереть> мой велосипед, пока я не смотрел."
                            }
                        },
                        {
                            "text": "Ze hebben al het geld uit de kassa <gerat>.",
                            "target_lang_translations": {
                                "en": "They <nicked> all the money from the cash register.",
                                "ru": "Они <стащили> все деньги из кассы."
                            }
                        }
                    ],
                    "synonyms": [
                        "stelen",
                        "jatten",
                        "gappen"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Bargoens"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To take something without permission; to steal.",
                        "ru": "Взять что-то без разрешения; украсть."
                    },
                    "translations": {

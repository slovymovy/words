    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5bc133f3-0684-3525-ad66-a8b2a500eb83",
                    "sense_definition": "Present participle and gerund of <w>plant</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is currently <w>planting</w> roses in the front garden.",
                            "target_lang_translations": {
                                "ru": "Она сейчас <w>сажает</w> розы в палисаднике."
                            }
                        },
                        {
                            "text": "We spent the whole afternoon <w>planting</w> vegetables.",
                            "target_lang_translations": {
                                "ru": "Мы провели весь день, <w>высаживая</w> овощи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "planting trees",
                        "planting seeds"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола plant."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сажать",
                                "target_lang_sense_clarification": "помещать растение в землю"
                            },
                            {
                                "target_lang_word": "высаживать",
                                "target_lang_sense_clarification": "переносить растение в грунт"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6a1e2ae8-99e9-3919-9ab5-21efa3e49a82",
                    "sense_definition": "The act or process of putting seeds or plants into the ground to grow.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmers finished the <w>planting</w> before the heavy rains started.",
                            "target_lang_translations": {
                                "ru": "Фермеры закончили <w>посадку</w> до начала сильных дождей."
                            }
                        },
                        {
                            "text": "Spring is usually the best time for the <w>planting</w> of new flowers.",
                            "target_lang_translations": {
                                "ru": "Весна обычно лучшее время для <w>посадки</w> новых цветов."
                            }
                        }
                    ],
                    "synonyms": [
                        "sowing",
                        "cultivation"
                    ],
                    "antonyms": [
                        "harvesting",
                        "uprooting"
                    ],
                    "common_phrases": [
                        "tree planting",
                        "spring planting",
                        "rice planting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс помещения семян или растений в землю для выращивания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посадка",
                                "target_lang_sense_clarification": "процесс размещения растений в почве"
                            },
                            {
                                "target_lang_word": "высадка",
                                "target_lang_sense_clarification": "высаживание рассады или саженцев"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37a3ecaf-3f17-3ea4-8f21-b4acc560f99b",
                    "sense_definition": "A plant, clipping, or group of plants that has been recently put into the soil.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Take one of the new <w>plantings</w> over to the neighbor as a gift.",
                            "target_lang_translations": {
                                "ru": "Отнеси один из новых <w>саженцев</w> соседу в подарок."
                            }
                        },
                        {
                            "text": "The gardener checked the <w>plantings</w> daily to ensure they were taking root.",
                            "target_lang_translations": {
                                "ru": "Садовник ежедневно проверял <w>посадки</w>, чтобы убедиться, что они приживаются."
                            }
                        }
                    ],
                    "synonyms": [
                        "transplant",
                        "seedling"
                    ],
                    "common_phrases": [
                        "new plantings",
                        "fresh plantings"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растение, черенок или группа растений, недавно помещенных в почву."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "саженец",
                                "target_lang_sense_clarification": "молодое растение"
                            },
                            {
                                "target_lang_word": "посадки",
                                "target_lang_sense_clarification": "высаженные растения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40156e3c-c3ac-3a04-a506-0f0a98c8504f",
                    "sense_definition": "An area of land established for growing crops or trees; a plantation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They walked through the old rubber <w>planting</w> near the river.",
                            "target_lang_translations": {
                                "ru": "Они шли через старую каучуковую <w>плантацию</w> у реки."
                            }
                        },
                        {
                            "text": "The family managed a small coffee <w>planting</w> in the hills.",
                            "target_lang_translations": {
                                "ru": "Семья управляла небольшой кофейной <w>плантацией</w> в холмах."
                            }
                        }
                    ],
                    "synonyms": [
                        "plantation",
                        "grove",
                        "orchard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок земли, отведенный для выращивания сельскохозяйственных культур или деревьев; плантация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плантация",
                                "target_lang_sense_clarification": "крупное хозяйство по выращиванию культур"
                            },
                            {
                                "target_lang_word": "насаждение",
                                "target_lang_sense_clarification": "участок с высаженными деревьями"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
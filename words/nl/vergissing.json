    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ddedcf11-e6cf-36f4-852e-cabd0be0fe80",
                    "sense_definition": "Iets wat je doet, zegt of denkt dat niet juist is; een fout.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mistake_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb uw brief <w>bij vergissing</w> geopend omdat ik niet goed naar de naam keek.",
                            "target_lang_translations": {
                                "en": "I opened your letter <w>by mistake</w> because I didn't look closely at the name.",
                                "ru": "Я открыл ваше письмо <w>по ошибке</w>, так как невнимательно посмотрел на имя."
                            }
                        },
                        {
                            "text": "Het is een veelgemaakte <w>vergissing</w> dat pinguïns op de Noordpool leven.",
                            "target_lang_translations": {
                                "en": "It is a common <w>mistake</w> to think that penguins live at the North Pole.",
                                "ru": "Это распространённое <w>заблуждение</w>, что пингвины живут на Северном полюсе."
                            }
                        }
                    ],
                    "synonyms": [
                        "fout",
                        "abuis",
                        "misverstand"
                    ],
                    "common_phrases": [
                        "bij vergissing",
                        "een vergissing maken",
                        "een vergissing begaan",
                        "op een vergissing berusten"
                    ],
                    "target_lang_definitions": {
                        "en": "Something you do, say, or think that is not correct; a mistake.",
                        "ru": "Что-то, что вы делаете, говорите или думаете неправильно; ошибка."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mistake",
                                "target_lang_sense_clarification": "incorrect action or thought"
                            },
                            {
                                "target_lang_word": "error",
                                "target_lang_sense_clarification": "formal; incorrectness"
                            },
                            {
                                "target_lang_word": "oversight",
                                "target_lang_sense_clarification": "failure to notice something"
                            }
                        ],
                        "ru": [

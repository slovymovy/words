                    "frequency": "Middle",
                    "semantic_group_id": "physiological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pies sąsiada zawsze <w>leje</w> pod tym samym drzewem w parku.",
                            "target_lang_translations": {
                                "ru": "Соседская собака всегда <w>писает</w> под одним и тем же деревом в парке."
                            }

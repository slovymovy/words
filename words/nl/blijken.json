    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "58eae8cd-1844-3053-baea-2f4399435ab0",
                    "sense_definition": "Duidelijk worden of aan het licht komen door feiten, onderzoek of gebeurtenissen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "become_evident",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uit het onderzoek <w>blijkt</w> dat veel mensen te weinig bewegen.",
                            "target_lang_translations": {
                                "ru": "Из исследования <w>следует</w>, что многие люди слишком мало двигаются.",
                                "en": "The research <w>shows</w> that many people do not exercise enough."
                            }
                        },
                        {
                            "text": "Het <w>bleek</w> uiteindelijk een grote vergissing te zijn.",
                            "target_lang_translations": {
                                "ru": "В итоге это <w>оказалось</w> большой ошибкой.",
                                "en": "It eventually <w>turned out</w> to be a big mistake."
                            }
                        }
                    ],
                    "synonyms": [
                        "duidelijk worden",
                        "aan het licht komen",
                        "uitwijzen"
                    ],
                    "antonyms": [
                        "verborgen blijven"
                    ],
                    "common_phrases": [
                        "uit onderzoek blijkt",
                        "achteraf blijkt",
                        "het blijkt dat",
                        "zoals blijkt uit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться очевидным или известным на основе фактов, исследований или событий.",
                        "en": "To become evident or come to light through facts, research, or events."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следовать",
                                "target_lang_sense_clarification": "вытекать из фактов или исследований"
                            },
                            {
                                "target_lang_word": "выясняться",
                                "target_lang_sense_clarification": "становиться известным"
                            },
                            {
                                "target_lang_word": "явствовать",
                                "target_lang_sense_clarification": "быть очевидным (официально)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "turn out",
                                "target_lang_sense_clarification": "to be discovered or become known"
                            },
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "to become visible based on evidence"
                            },
                            {
                                "target_lang_word": "become apparent",
                                "target_lang_sense_clarification": "to become clear or obvious"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11adee83-f00c-3df4-ad25-4e82b8694735",
                    "sense_definition": "Achteraf een bepaalde eigenschap of hoedanigheid blijken te bezitten (vaak in combinatie met 'te zijn').",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "turn_out_to_be",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De nieuwe collega <w>blijkt</w> erg aardig te zijn.",
                            "target_lang_translations": {
                                "ru": "Новый коллега <w>оказывается</w> очень приятным.",
                                "en": "The new colleague <w>turns out</w> to be very nice."
                            }
                        },
                        {
                            "text": "Zijn oplossing <w>bleek</w> veel eenvoudiger dan we dachten.",
                            "target_lang_translations": {
                                "ru": "Его решение <w>оказалось</w> намного проще, чем мы думали.",
                                "en": "His solution <w>proved</w> much simpler than we thought."
                            }
                        }
                    ],
                    "common_phrases": [
                        "blijken te zijn",
                        "waar blijken",
                        "nodig blijken",
                        "juist blijken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказываться обладателем определенного свойства или качества (часто неожиданно).",
                        "en": "To prove to possess a certain quality or characteristic afterwards (often with 'to be')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оказываться",
                                "target_lang_sense_clarification": "обнаруживать качество или состояние"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "turn out",
                                "target_lang_sense_clarification": "to be found to be something"
                            },
                            {
                                "target_lang_word": "prove",
                                "target_lang_sense_clarification": "to demonstrate a certain quality"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "327d615a-4651-3efc-b316-48ad2cb6d371",
                    "sense_definition": "Zichtbaar maken of tot uiting laten komen (vrijwel altijd in combinatie met 'laten').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reveal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij liet duidelijk <w>blijken</w> dat hij het er niet mee eens was.",
                            "target_lang_translations": {
                                "ru": "Он ясно дал <w>понять</w>, что не согласен с этим.",
                                "en": "He clearly <w>showed</w> that he did not agree."
                            }
                        },
                        {
                            "text": "Zij heeft haar waardering voor het team laten <w>blijken</w>.",
                            "target_lang_translations": {
                                "ru": "Она <w>выразила</w> свою признательность команде.",
                                "en": "She <w>expressed</w> her appreciation for the team."
                            }
                        }
                    ],
                    "synonyms": [
                        "tonen",
                        "laten merken"
                    ],
                    "antonyms": [
                        "verbergen"
                    ],
                    "common_phrases": [
                        "laten blijken",
                        "duidelijk laten blijken",
                        "niets laten blijken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать видимым или выражать (обычно в конструкции 'laten blijken').",
                        "en": "To make visible or express (usually in the phrase 'laten blijken')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показывать",
                                "target_lang_sense_clarification": "демонстрировать"
                            },
                            {
                                "target_lang_word": "давать понять",
                                "target_lang_sense_clarification": "намекать или сообщать"
                            },
                            {
                                "target_lang_word": "проявлять",
                                "target_lang_sense_clarification": "выражать чувства или отношение"
                            }
                        ],
                        "en": [
                            {

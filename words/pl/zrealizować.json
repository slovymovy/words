    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "99ce6c31-e0b6-3a25-bc88-b4480064d831",
                    "sense_definition": "Doprowadzić do skutku zaplanowane działanie, projekt lub marzenie; sprawić, że coś stało się rzeczywistością.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_execution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Udało nam się w końcu <zrealizować> nasz plan podróży dookoła świata.",
                            "target_lang_translations": {
                                "ru": "Нам наконец удалось <w>осуществить</w> наш план кругосветного путешествия."
                            }
                        },
                        {
                            "text": "Firma budowlana obiecała <zrealizować> ten projekt w ciągu dwóch lat.",
                            "target_lang_translations": {
                                "ru": "Строительная компания обещала <w>реализовать</w> этот проект в течение двух лет."
                            }
                        },
                        {
                            "text": "Musisz ciężko pracować, aby <zrealizować> swoje największe marzenia.",
                            "target_lang_translations": {
                                "ru": "Ты должен упорно работать, чтобы <w>осуществить</w> свои самые заветные мечты."
                            }
                        }
                    ],
                    "synonyms": [
                        "urzeczywistnić"
                    ],
                    "antonyms": [
                        "zaniechać",
                        "porzucić"
                    ],
                    "common_phrases": [
                        "zrealizować plan",
                        "zrealizować projekt",
                        "zrealizować marzenie",
                        "zrealizować cel",
                        "zrealizować czek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осуществить запланированное действие, проект или мечту; сделать так, чтобы что-то стало реальностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осуществить",
                                "target_lang_sense_clarification": "план, мечту"
                            },
                            {
                                "target_lang_word": "реализовать",
                                "target_lang_sense_clarification": "проект, идею"
                            },
                            {
                                "target_lang_word": "воплотить",
                                "target_lang_sense_clarification": "мечту в жизнь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7ac2bab-edb0-3499-a80e-7c8999701670",
                    "sense_definition": "Osiągnąć osobistą satysfakcję i wykorzystać swój potencjał w jakiejś dziedzinie (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "personal_fulfillment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ona chce <zrealizować> się jako artystka i otworzyć własną galerię.",
                            "target_lang_translations": {
                                "ru": "Она хочет <w>реализовать</w> себя как художница и открыть собственную галерею."
                            }
                        },
                        {
                            "text": "W nowej pracy wreszcie mogę <zrealizować> się zawodowo.",
                            "target_lang_translations": {
                                "ru": "На новой работе я наконец могу <w>реализовать</w> себя профессионально."
                            }
                        }
                    ],
                    "synonyms": [
                        "spełnić się"
                    ],
                    "common_phrases": [
                        "zrealizować się zawodowo",
                        "zrealizować się w życiu",
                        "zrealizować się jako rodzic"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Достичь личного удовлетворения и раскрыть свой потенциал в какой-либо области (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реализовать себя",
                                "target_lang_sense_clarification": "раскрыть свой потенциал"
                            },
                            {
                                "target_lang_word": "состояться",
                                "target_lang_sense_clarification": "как личность или профессионал"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "169ad37c-49e1-3344-8338-140061ccf3b9",
                    "sense_definition": "Something to regret; a cause for disappointment or sadness.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "regret_pity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is a <w>shame</w> that you cannot come to the party with us."
                        },
                        {
                            "text": "What a <w>shame</w> to waste so much good food."
                        }
                    ],
                    "synonyms": [
                        "pity",
                        "misfortune"
                    ],
                    "common_phrases": [
                        "what a shame",
                        "it is a shame",
                        "crying shame",
                        "real shame"
                    ]
                },
                {
                    "sense_id": "2b42d786-952f-33bf-89bb-941895900c67",
                    "sense_definition": "An uncomfortable or painful feeling of guilt, embarrassment, or disgrace caused by the consciousness of one's own wrong behavior or foolishness.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "feeling_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt deep <w>shame</w> for lying to his parents about his grades."
                        },
                        {
                            "text": "She hung her head in <w>shame</w> after realizing her mistake."
                        }
                    ],
                    "synonyms": [
                        "embarrassment",
                        "guilt",
                        "remorse",
                        "humiliation",
                        "mortification"
                    ],
                    "antonyms": [
                        "honor",
                        "exaltation",
                        "dignify"
                    ],
                    "common_phrases": [
                        "feel shame",
                        "hang head in shame",
                        "burn with shame",
                        "overcome with shame"
                    ]
                },
                {
                    "sense_id": "e9965c45-b7d9-3a5d-8677-5667eca2ec4c",
                    "sense_definition": "The capacity to be ashamed, acting as a restraint against improper behavior; a sense of decency.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "feeling_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Have you no <w>shame</w> to speak to your mother like that?"
                        },
                        {
                            "text": "He has absolutely no sense of <w>shame</w> regarding his rude behavior."
                        }
                    ],
                    "synonyms": [
                        "decency",
                        "modesty"
                    ],
                    "common_phrases": [
                        "have no shame",
                        "sense of shame",
                        "lost all shame"
                    ]
                },
                {
                    "sense_id": "f81d8e37-b04f-3fe5-8f9a-792d04555446",
                    "sense_definition": "A state of dishonor, disgrace, or loss of reputation incurred by wrong or foolish actions.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "disgrace_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His criminal actions brought <w>shame</w> upon his entire family."
                        },
                        {
                            "text": "There is no <w>shame</w> in admitting that you need help."
                        }
                    ],
                    "synonyms": [
                        "disgrace",
                        "dishonor",
                        "ignominy",
                        "infamy"
                    ],
                    "antonyms": [
                        "honor",
                        "exaltation"
                    ],
                    "common_phrases": [
                        "bring shame on",
                        "live in shame",
                        "cover in shame"
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "514885c3-4588-3fd2-9362-b72416255b50",
                    "sense_definition": "To cause someone to feel ashamed, humiliated, or embarrassed.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cause_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher <w>shamed</w> the student in front of the whole class for forgetting his homework."
                        },
                        {
                            "text": "He was <w>shamed</w> by the public revelation of his secrets."
                        }
                    ],
                    "synonyms": [
                        "humiliate",
                        "embarrass",
                        "mortify",
                        "abash"
                    ],
                    "antonyms": [
                        "honor",
                        "dignify"
                    ]
                },
                {
                    "sense_id": "01a8e6da-8f2b-33bd-b830-2b85cc7d483f",
                    "sense_definition": "To criticize or denounce someone publicly with the intent of causing them to feel ashamed, often for specific behaviors or characteristics.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "criticize",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "People should stop <w>shaming</w> others on social media for their appearance."
                        },
                        {
                            "text": "The campaign aims to stop <w>shaming</w> mothers for breastfeeding in public."
                        }
                    ],
                    "synonyms": [
                        "criticize",
                        "denounce"
                    ],
                    "common_phrases": [
                        "body shame",
                        "publicly shame",
                        "online shaming"
                    ]
                },
                {
                    "sense_id": "fe9af164-5a59-34a5-bff0-77e95bf1f3c4",
                    "sense_definition": "To force or compel someone to do something by making them feel ashamed.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "compel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The politician was <w>shamed</w> into resigning after the scandal broke."
                        },
                        {
                            "text": "They <w>shamed</w> him into donating money to the charity."
                        }
                    ],
                    "common_phrases": [
                        "shame into"
                    ]
                }
            ]
        },
        {

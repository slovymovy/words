    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "acaba452-cd02-36cb-9998-7836e5a45443",
                    "sense_definition": "Сменить место жительства или пребывания, переместившись в другое помещение, город или страну.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "change_residence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <переехать> в новую квартиру ближе к центру города.",
                            "target_lang_translations": {
                                "en": "We decided to <move> to a new apartment closer to the city center."
                            }
                        },
                        {
                            "text": "Мой друг планирует <переехать> в другую страну ради учёбы.",
                            "target_lang_translations": {
                                "en": "My friend plans to <move> to another country for his studies."
                            }
                        }
                    ],
                    "synonyms": [
                        "переселиться",
                        "перебраться"
                    ],
                    "antonyms": [
                        "остаться"
                    ],
                    "common_phrases": [
                        "переехать в новый дом",
                        "переехать в другой город",
                        "переехать к родителям",
                        "переехать на квартиру"
                    ],
                    "target_lang_definitions": {
                        "en": "To change one's place of residence or stay by moving to another accommodation, city, or country."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "change residence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "035b4f30-53fe-355b-b889-99330d819601",
                    "sense_definition": "Пересечь какое-либо пространство, линию или границу, передвигаясь на транспортном средстве.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы успешно <переехали> границу и продолжили своё путешествие.",
                            "target_lang_translations": {
                                "en": "The tourists successfully <crossed> the border and continued their journey."
                            }
                        },
                        {
                            "text": "Нам нужно аккуратно <переехать> через железнодорожные пути.",
                            "target_lang_translations": {
                                "en": "We need to carefully <cross> the railway tracks."
                            }
                        }
                    ],
                    "synonyms": [
                        "пересечь"
                    ],
                    "common_phrases": [
                        "переехать границу",
                        "переехать через мост",
                        "переехать на другую сторону",
                        "переехать реку"
                    ],
                    "target_lang_definitions": {
                        "en": "To cross a space, line, or border while traveling in a vehicle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cross",
                                "target_lang_sense_clarification": "go across in a vehicle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5891067c-2022-345f-826d-573bff3b8cc1",
                    "sense_definition": "Наехать колёсами на кого-либо или что-либо, раздавить тяжестью транспортного средства.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "movement_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель не заметил коробку на дороге и случайно <переехал> её.",
                            "target_lang_translations": {
                                "en": "The driver didn't notice the box on the road and accidentally <ran over> it."
                            }
                        },
                        {
                            "text": "Грузовик едва не <переехал> упавший дорожный знак.",
                            "target_lang_translations": {
                                "en": "The truck almost <ran over> the fallen road sign."
                            }
                        }
                    ],
                    "synonyms": [
                        "задавить"
                    ],
                    "common_phrases": [
                        "случайно переехать",
                        "переехать колесом"
                    ],
                    "target_lang_definitions": {
                        "en": "To drive over someone or something with wheels, crushing them with the weight of the vehicle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run over",
                                "target_lang_sense_clarification": "drive over with a vehicle"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
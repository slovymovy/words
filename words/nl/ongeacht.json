{
    "word_family": [
        "achten",
        "achting",
        "geacht",
        "achteloos"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "386e3df3-234b-330d-a9b7-31dbfac62b8e",
                    "sense_definition": "Zonder rekening te houden met iets of iemand; onafhankelijk van de omstandigheden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Iedereen is welkom op het feest, <w>ongeacht</w> leeftijd of afkomst.",
                            "target_lang_translations": {
                                "en": "Everyone is welcome at the party, <w>regardless of</w> age or background.",
                                "ru": "На вечеринку приглашаются все, <w>независимо от</w> возраста или происхождения."
                            }
                        },
                        {
                            "text": "We gaan morgen wandelen, <w>ongeacht</w> het weer.",
                            "target_lang_translations": {
                                "en": "We are going for a walk tomorrow, <w>regardless of</w> the weather.",
                                "ru": "Мы пойдем гулять завтра, <w>несмотря на</w> погоду."
                            }
                        }
                    ],
                    "synonyms": [
                        "los van",
                        "afgezien van"
                    ],
                    "common_phrases": [
                        "ongeacht de gevolgen",
                        "ongeacht de kosten",
                        "ongeacht wie",
                        "ongeacht het resultaat"
                    ],
                    "target_lang_definitions": {
                        "en": "Without taking something or someone into account; independent of the circumstances.",
                        "ru": "Не принимая во внимание что-либо или кого-либо; независимо от обстоятельств."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "regardless of",
                                "target_lang_sense_clarification": "standard translation, used to indicate that something is not influenced by something else"
                            },
                            {
                                "target_lang_word": "irrespective of",
                                "target_lang_sense_clarification": "slightly more formal synonym"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "независимо от",
                                "target_lang_sense_clarification": "без учета обстоятельств"
                            },
                            {
                                "target_lang_word": "несмотря на",
                                "target_lang_sense_clarification": "вопреки чему-либо"
                            },
                            {
                                "target_lang_word": "невзирая на",
                                "target_lang_sense_clarification": "игнорируя что-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "592e5ec4-9460-3bee-b023-c360bb579469",
                    "sense_definition": "Niet gerespecteerd of gewaardeerd; waar men geen achting voor heeft.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>ongeachte</w> gast werd door niemand begroet bij binnenkomst.",
                            "target_lang_translations": {
                                "en": "The <w>unesteemed</w> guest was greeted by no one upon entry.",
                                "ru": "<w>Неуважаемого</w> гостя никто не поприветствовал при входе."
                            }
                        },
                        {
                            "text": "Hij voelde zich een <w>ongeacht</w> lid van de vereniging.",
                            "target_lang_translations": {
                                "en": "He felt like an <w>unvalued</w> member of the association.",
                                "ru": "Он чувствовал себя <w>неуважаемым</w> членом ассоциации."
                            }
                        }
                    ],
                    "antonyms": [
                        "geacht"
                    ],
                    "target_lang_definitions": {
                        "en": "Not respected or valued; held in no esteem.",
                        "ru": "Не пользующийся уважением или почтением; тот, кого не ценят."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unesteemed",
                                "target_lang_sense_clarification": "literal antonym of 'esteemed', formal"
                            },
                            {
                                "target_lang_word": "unvalued",
                                "target_lang_sense_clarification": "feeling not appreciated or respected"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "неуважаемый",
                                "target_lang_sense_clarification": "о человеке или статусе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ef49767-9ecc-3fdc-838b-a4003d231ba4",
                    "sense_definition": "Waar geen aandacht aan is geschonken; wat niet in overweging is genomen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn waarschuwing bleef helaas volledig <w>ongeacht</w>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, his warning remained completely <w>unheeded</w>.",
                                "ru": "К сожалению, его предупреждение осталось полностью <w>без внимания</w>."
                            }
                        },
                        {
                            "text": "Dat detail laten we voorlopig even <w>ongeacht</w>.",
                            "target_lang_translations": {
                                "en": "We will leave that detail <w>disregarded</w> for the time being.",
                                "ru": "Эту деталь мы пока оставим <w>без внимания</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To which no attention has been paid; not taken into consideration.",
                        "ru": "Оставленный без внимания; не принятый в расчет."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unheeded",
                                "target_lang_sense_clarification": "often used with warnings or advice"
                            },
                            {
                                "target_lang_word": "disregarded",
                                "target_lang_sense_clarification": "ignored or treated as unimportant"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "оставленный без внимания",
                                "target_lang_sense_clarification": "проигнорированный"
                            },
                            {
                                "target_lang_word": "неучтённый",
                                "target_lang_sense_clarification": "не принятый во внимание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защищать от наводнений",
                                "target_lang_sense_clarification": "принимать меры безопасности"
                            },
                            {
                                "target_lang_word": "обезопасить от затопления",
                                "target_lang_sense_clarification": "предотвращать затопление"

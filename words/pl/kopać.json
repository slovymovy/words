            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ba238d42-99ff-3ea9-a80c-e982e07aee98",
                    "sense_definition": "Uderzać kogoś lub coś stopą lub nogą.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Piłkarz mocno <w>kopał</w> piłkę w stronę bramki przeciwnika.",
                            "target_lang_translations": {
                                "ru": "Футболист сильно <w>пинал</w> мяч в сторону ворот противника."
                            }
                        },
                        {
                            "text": "Nie wolno <w>kopać</w> psa, ponieważ to sprawia mu ból.",
                            "target_lang_translations": {
                                "ru": "Нельзя <w>пинать</w> собаку, потому что это причиняет ей боль."
                            }
                        }
                    ],
                    "synonyms": [
                        "butować"
                    ],
                    "common_phrases": [
                        "kopać piłkę",
                        "kopać w drzwi",
                        "kopać leżącego"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ударять кого-то или что-то ногой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пинать",
                                "target_lang_sense_clarification": "наносить удары ногой"
                            },
                            {
                                "target_lang_word": "ударять ногой",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04a121e2-c209-37eb-a55c-de0748efdaee",
                    "sense_definition": "Robić wgłębienie w ziemi lub usuwać ziemię za pomocą narzędzi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "excavation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek lubi <w>kopać</w> w ogrodzie i sadzić nowe rośliny.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка любит <w>копать</w> в саду и сажать новые растения."
                            }
                        },
                        {
                            "text": "Robotnicy musieli <w>kopać</w> głęboki rów pod nowe rury.",
                            "target_lang_translations": {
                                "ru": "Рабочие должны были <w>копать</w> глубокую траншею для новых труб."
                            }
                        }
                    ],
                    "synonyms": [
                        "butować"
                    ],
                    "antonyms": [
                        "zakopywać"
                    ],
                    "common_phrases": [
                        "kopać dół",
                        "kopać rów",
                        "kopać studnię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать углубление в земле или перемещать землю с помощью инструментов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копать",
                                "target_lang_sense_clarification": "работать лопатой или инструментом"
                            },
                            {
                                "target_lang_word": "рыть",
                                "target_lang_sense_clarification": "создавать яму или углубление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4dd41b8-1b58-3442-91d2-6ba2ea75cf49",
                    "sense_definition": "Wydobywać z ziemi płody rolne lub surowce mineralne.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "excavation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jesienią cała wieś wychodziła na pole, aby <w>kopać</w> ziemniaki.",
                            "target_lang_translations": {
                                "ru": "Осенью вся деревня выходила в поле, чтобы <w>копать</w> картошку."
                            }
                        },
                        {
                            "text": "Górnicy zjeżdżają głęboko pod ziemię, żeby <w>kopać</w> węgiel.",
                            "target_lang_translations": {
                                "ru": "Шахтёры спускаются глубоко под землю, чтобы <w>добывать</w> уголь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kopać ziemniaki",
                        "kopać węgiel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Извлекать из земли урожай или полезные ископаемые."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копать",
                                "target_lang_sense_clarification": "собирать корнеплоды (картофель)"
                            },
                            {
                                "target_lang_word": "добывать",
                                "target_lang_sense_clarification": "извлекать ископаемые (уголь)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cce86084-e7ee-3c87-8796-0fb7d9f85b56",
                    "sense_definition": "Uderzać się nawzajem nogami, zwykle podczas bójki lub zabawy.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopcy pokłócili się na boisku i zaczęli się <w>kopać</w>.",
                            "target_lang_translations": {
                                "ru": "Мальчики поссорились на поле и начали <w>пинаться</w>."
                            }
                        },
                        {
                            "text": "To niegrzeczne <w>kopać</w> się z kolegami w przedszkolu.",
                            "target_lang_translations": {
                                "ru": "Невежливо <w>пинаться</w> с друзьями в детском саду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kopać się po kostkach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бить друг друга ногами, обычно в драке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пинаться",
                                "target_lang_sense_clarification": "наносить удары ногами друг другу"
                            },
                            {
                                "target_lang_word": "лягаться",
                                "target_lang_sense_clarification": "разговорное, часто о беспорядочных ударах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "966c2a40-ab5f-3056-b7da-8418724572e5",
                    "sense_definition": "O prądzie elektrycznym: powodować ból lub wstrząs przy dotknięciu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "electricity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważaj na ten stary przewód, bo prąd <w>kopie</w>.",
                            "target_lang_translations": {
                                "ru": "Осторожно с этим старым проводом, потому что ток <w>бьёт</w>."
                            }
                        },
                        {
                            "text": "Dotknąłem obudowy pralki i poczułem, że lekko <w>kopie</w>.",
                            "target_lang_translations": {
                                "ru": "Я дотронулся до корпуса стиральной машины и почувствовал, что она слегка <w>бьёт</w> током."
                            }
                        }
                    ],
                    "synonyms": [
                        "butować"
                    ],
                    "common_phrases": [
                        "prąd kopie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Об электрическом токе: причинять боль или шок при прикосновении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "о токе"
                            },
                            {
                                "target_lang_word": "ударять",
                                "target_lang_sense_clarification": "о внезапном воздействии тока"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a45f2d5b-b588-30b3-b09e-d586ab3705ec",
                    "sense_definition": "O broni palnej: gwałtownie cofać się po oddaniu strzału, uderzając strzelającego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta stara strzelba myśliwska bardzo mocno <w>kopie</w> w ramię.",
                            "target_lang_translations": {
                                "ru": "Это старое охотничье ружье очень сильно <w>бьёт</w> в плечо."
                            }
                        },
                        {
                            "text": "Instruktor ostrzegł mnie, że ten karabin <w>kopie</w> przy każdym strzale.",
                            "target_lang_translations": {
                                "ru": "Инструктор предупредил меня, что этот карабин <w>отдает</w> при каждом выстреле."
                            }
                        }
                    ],
                    "common_phrases": [
                        "broń kopie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Об оружии: резко отскакивать назад при выстреле (отдача)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "ударять в плечо при отдаче"
                            },
                            {
                                "target_lang_word": "отдавать",
                                "target_lang_sense_clarification": "иметь отдачу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3cbfab6-488d-3afc-a82d-8c98c7f8f866",
                    "sense_definition": "Poruszać się z wielkim trudem przez sypką lub grząską przeszkodę.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "struggle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samochód utknął w zaspie i musieliśmy się <w>kopać</w> przez śnieg.",
                            "target_lang_translations": {
                                "ru": "Машина застряла в сугробе, и нам пришлось <w>пробираться</w> через снег."
                            }
                        },
                        {
                            "text": "Turysta powoli <w>kopał</w> się przez głęboki piasek na wydmie.",
                            "target_lang_translations": {
                                "ru": "Турист медленно <w>пробирался</w> через глубокий песок на дюне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kopać się w śniegu",
                        "kopać się z koniem"
                    ],
                    "target_lang_definitions": {
                        "ru": "С большим трудом передвигаться через сыпучее или вязкое препятствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пробираться",
                                "target_lang_sense_clarification": "преодолевать трудный путь"
                            },
                            {
                                "target_lang_word": "барахтаться",
                                "target_lang_sense_clarification": "двигаться с трудом, увязая"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "bossy",
        "bossiness",
        "bossism"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8e0d0df2-e2e1-383f-8e10-9c0fa9ed94e1",
                    "sense_definition": "A person who oversees and directs the work of others, or is in charge of a business or organization.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "authority_figure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>boss</w> gave me permission to leave work early today.",
                            "target_lang_translations": {
                                "ru": "Мой <w>начальник</w> разрешил мне уйти с работы пораньше сегодня."
                            }
                        },
                        {
                            "text": "She is the <w>boss</w> of a large international company.",
                            "target_lang_translations": {
                                "ru": "Она — <w>руководитель</w> крупной международной компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "manager",
                        "supervisor",
                        "employer",
                        "chief",
                        "head",
                        "leader"
                    ],
                    "antonyms": [
                        "worker",
                        "subordinate",
                        "laborer"
                    ],
                    "common_phrases": [
                        "the big boss",
                        "boss of the company",
                        "ask the boss",
                        "like a boss"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который контролирует и направляет работу других, или возглавляет бизнес или организацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нача́льник",
                                "target_lang_sense_clarification": "самый распространенный термин"
                            },
                            {
                                "target_lang_word": "босс",
                                "target_lang_sense_clarification": "разговорное, заимствование"
                            },
                            {
                                "target_lang_word": "руководи́тель",
                                "target_lang_sense_clarification": "формальное"
                            },
                            {
                                "target_lang_word": "шеф",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14b0bee2-00b9-3fc7-8bb6-2d105799c66a",
                    "sense_definition": "A term of address to a man, especially a customer, but also to a friend or acquaintance.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "address_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thanks for the help, <w>boss</w>.",
                            "target_lang_translations": {
                                "ru": "Спасибо за помощь, <w>шеф</w>."
                            }
                        },
                        {
                            "text": "What can I get for you today, <w>boss</w>?",
                            "target_lang_translations": {
                                "ru": "Что вам предложить сегодня, <w>шеф</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "mate",
                        "chief",
                        "guv"
                    ],
                    "common_phrases": [
                        "cheers boss",
                        "okay boss",
                        "sorry boss"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used informally"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращение к мужчине, особенно к клиенту, а также к другу или знакомому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шеф",
                                "target_lang_sense_clarification": "популярное обращение к таксистам, официантам"
                            },
                            {
                                "target_lang_word": "босс",
                                "target_lang_sense_clarification": "дружеское обращение"
                            },
                            {
                                "target_lang_word": "команди́р",
                                "target_lang_sense_clarification": "распространенное уличное обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0077808e-37c7-3faa-9fd7-5ce8f8aa1095",
                    "sense_definition": "An enemy in a video game, often at the end of a level, that is particularly challenging and must be defeated to progress.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gaming_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It took me three hours to finally defeat the final <w>boss</w>.",
                            "target_lang_translations": {
                                "ru": "Мне потребовалось три часа, чтобы наконец победить финального <w>босса</w>."
                            }
                        },
                        {
                            "text": "The <w>boss</w> battle requires quick reflexes and pattern recognition.",
                            "target_lang_translations": {
                                "ru": "Битва с <w>боссом</w> требует быстрой реакции и распознавания паттернов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "final boss",
                        "boss battle",
                        "beat the boss",
                        "level boss"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gaming context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Враг в видеоигре, часто в конце уровня, которого особенно трудно победить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "босс",
                                "target_lang_sense_clarification": "игровой сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9105e96f-6903-346e-8128-a3a5e5a6aeef",
                    "sense_definition": "A slang term referring to one's wife.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "authority_figure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have to check with the <w>boss</w> before I can agree to the trip.",
                            "target_lang_translations": {
                                "ru": "Мне нужно согласовать это с <w>начальством</w> (женой), прежде чем я соглашусь на поездку."
                            }
                        },
                        {
                            "text": "If the <w>boss</w> says we are redecorating the kitchen, then we are redecorating.",
                            "target_lang_translations": {
                                "ru": "Если <w>начальство</w> говорит, что мы переделываем кухню, значит, мы переделываем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the boss"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "humorous"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сленговое обозначение жены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нача́льство",
                                "target_lang_sense_clarification": "шутливое, собирательное"
                            },
                            {
                                "target_lang_word": "жена́",
                                "target_lang_sense_clarification": "прямое значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29750012-cad4-32d6-9ab0-605ffc77e405",
                    "sense_definition": "A swelling, knob, or round projection on an object, such as on a shield, a vault in architecture, or a machine part.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_projection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient shield featured a heavy metal <w>boss</w> in the center for protection.",
                            "target_lang_translations": {
                                "ru": "Древний щит имел тяжелый металлический <w>умбон</w> в центре для защиты."
                            }
                        },
                        {
                            "text": "The mechanic tightened the screw into the threaded <w>boss</w> on the engine block.",
                            "target_lang_translations": {
                                "ru": "Механик затянул винт в резьбовую <w>бобышку</w> на блоке двигателя."
                            }
                        }
                    ],
                    "synonyms": [
                        "knob",
                        "protuberance",
                        "protrusion",
                        "stud"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical usage in architecture, geology, or mechanics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпуклость, шишка или круглый выступ на объекте, например, на щите, своде или детали машины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вы́ступ",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "умбо́н",
                                "target_lang_sense_clarification": "на щите"
                            },
                            {
                                "target_lang_word": "бобы́шка",
                                "target_lang_sense_clarification": "в механике"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "14395db3-e501-36d4-ae45-1b76a1b3000c",
                    "sense_definition": "To exercise authoritative control over someone; to tell someone what to do, often repeatedly or aggressively.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "control_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stop trying to <w>boss</w> me around and let me do my job.",
                            "target_lang_translations": {
                                "ru": "Перестань мною <w>командовать</w> и дай мне делать мою работу."
                            }
                        },
                        {
                            "text": "She <w>bossed</w> the team until the project was finished ahead of schedule.",
                            "target_lang_translations": {
                                "ru": "Она <w>командовала</w> командой, пока проект не был завершен с опережением графика."
                            }
                        }
                    ],
                    "synonyms": [
                        "order around",
                        "lord over",
                        "dominate"
                    ],
                    "common_phrases": [
                        "boss around",
                        "boss the show"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осуществлять властный контроль над кем-либо; указывать кому-либо, что делать, часто неоднократно или агрессивно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кома́ндовать",
                                "target_lang_sense_clarification": "распоряжаться кем-либо"
                            },
                            {
                                "target_lang_word": "распоряжа́ться",
                                "target_lang_sense_clarification": "давать указания"
                            },
                            {
                                "target_lang_word": "ука́зывать",
                                "target_lang_sense_clarification": "говорить, что делать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac2a01a6-33f6-34f6-9e57-766952d880a0",
                    "sense_definition": "To decorate with raised ornaments or projections; to emboss.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_projection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The silversmith <w>bossed</w> the metal plate with an intricate floral design.",
                            "target_lang_translations": {
                                "ru": "Серебряных дел мастер <w>украсил чеканкой</w> металлическую пластину сложным цветочным узором."
                            }
                        },
                        {
                            "text": "The leather cover was <w>bossed</w> with gold leaf patterns.",
                            "target_lang_translations": {
                                "ru": "Кожаная обложка была <w>тиснена</w> узорами из сусального золота."
                            }
                        }
                    ],
                    "synonyms": [
                        "emboss"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical or artistic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшать выпуклым орнаментом или выступами; чеканить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чека́нить",
                                "target_lang_sense_clarification": "по металлу"
                            },
                            {
                                "target_lang_word": "делать тисне́ние",
                                "target_lang_sense_clarification": "по коже или бумаге"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d0432545-fc24-3850-ae86-3aca0eec4b30",
                    "sense_definition": "Of excellent quality; first-rate.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quality_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That new car you bought is really <w>boss</w>.",
                            "target_lang_translations": {
                                "ru": "Та новая машина, что ты купил, реально <w>крутая</w>."
                            }
                        },
                        {
                            "text": "We had a <w>boss</w> time at the concert last night.",
                            "target_lang_translations": {
                                "ru": "Мы <w>классно</w> провели время на концерте вчера вечером."
                            }
                        }
                    ],
                    "synonyms": [
                        "excellent",
                        "great",
                        "cool"
                    ],
                    "common_phrases": [
                        "really boss"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often considered dated"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличного качества; первоклассный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круто́й",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "кла́ссный",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "отли́чный",
                                "target_lang_sense_clarification": "стандартное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c5614aa0-e67f-3c1c-9322-8bd05883b13c",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Boss</w> has lived in this neighborhood for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Босс</w> живет в этом районе уже двадцать лет."
                            }
                        },
                        {
                            "text": "I saw the name <w>Boss</w> listed in the directory.",
                            "target_lang_translations": {
                                "ru": "Я видел фамилию <w>Босс</w> в справочнике."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Босс",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
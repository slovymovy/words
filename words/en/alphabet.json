{
    "word_family": [
        "alphabetical",
        "alphabetically",
        "alphabetize",
        "alphabetization",
        "alphanumeric"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bd6956cd-b2ad-3a49-95ca-fe7651b0bb1f",
                    "sense_definition": "The fixed set of letters or symbols used to write a language, usually arranged in a standard order.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "writing_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Children usually learn to recite the <w>alphabet</w> in kindergarten.",
                            "target_lang_translations": {
                                "ru": "Обычно дети учатся рассказывать <w>алфавит</w> в детском саду."
                            }
                        },
                        {
                            "text": "The English <w>alphabet</w> starts with A and ends with Z.",
                            "target_lang_translations": {
                                "ru": "Английский <w>алфавит</w> начинается с A и заканчивается на Z."
                            }
                        }
                    ],
                    "synonyms": [
                        "ABC"
                    ],
                    "common_phrases": [
                        "letters of the alphabet",
                        "English alphabet",
                        "phonetic alphabet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фиксированный набор букв или символов, используемых для письма на языке, обычно расположенных в стандартном порядке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "алфавит",
                                "target_lang_sense_clarification": "набор букв"
                            },
                            {
                                "target_lang_word": "азбука",
                                "target_lang_sense_clarification": "набор букв"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cec6737-8478-3623-a0a1-71dc07a78217",
                    "sense_definition": "A specific type of writing system where characters represent individual sounds, including both vowels and consonants.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "writing_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Greek <w>alphabet</w> was the first to include dedicated symbols for vowels.",
                            "target_lang_translations": {
                                "ru": "Греческий <w>алфавит</w> был первым, включившим специальные символы для гласных."
                            }
                        },
                        {
                            "text": "Linguists distinguish a true <w>alphabet</w> from an abjad or a syllabary.",
                            "target_lang_translations": {
                                "ru": "Лингвисты отличают истинный <w>алфавит</w> от абджада или слогового письма."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Latin alphabet",
                        "Cyrillic alphabet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особый тип системы письма, где символы обозначают отдельные звуки, включая как гласные, так и согласные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "алфавит",
                                "target_lang_sense_clarification": "тип системы письма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5301cdca-778e-332d-8957-1a393d245047",
                    "sense_definition": "An individual letter or character within a writing system.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "writing_units",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please write your name in block <w>alphabets</w> on the form.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, напишите ваше имя печатными <w>буквами</w> в бланке."
                            }
                        },
                        {
                            "text": "He claimed there are twenty-six <w>alphabets</w> in the English language.",
                            "target_lang_translations": {
                                "ru": "Он утверждал, что в английском языке двадцать шесть <w>букв</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Indian English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная буква или символ в системе письма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "буква",
                                "target_lang_sense_clarification": "значение в индийском диалекте английского"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e85bb20-95b0-3ca9-adc6-353eba380646",
                    "sense_definition": "A finite set of distinguishable symbols used in formal languages, logic, or computer science.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "formal_logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In binary code, the <w>alphabet</w> consists only of the digits zero and one.",
                            "target_lang_translations": {
                                "ru": "В двоичном коде <w>алфавит</w> состоит только из цифр ноль и один."
                            }
                        },
                        {
                            "text": "We defined a formal language over an <w>alphabet</w> containing three distinct symbols.",
                            "target_lang_translations": {
                                "ru": "Мы определили формальный язык над <w>алфавитом</w>, содержащим три различных символа."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конечный набор различимых символов, используемых в формальных языках, логике или информатике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "алфавит",
                                "target_lang_sense_clarification": "термин в информатике и логике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65123df3-ca48-39f8-a4c7-5fe1d044b01b",
                    "sense_definition": "The fundamental elements or simplest rudiments of a subject.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fundamentals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Understanding the Constitution is the <w>alphabet</w> of our legal system.",
                            "target_lang_translations": {
                                "ru": "Понимание Конституции — это <w>азбука</w> нашей правовой системы."
                            }
                        },
                        {
                            "text": "He had to learn the <w>alphabet</w> of survival before moving to the wilderness.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось выучить <w>азы</w> выживания перед тем, как перебраться в дикую местность."
                            }
                        }
                    ],
                    "synonyms": [
                        "ABC"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фундаментальные элементы или простейшие основы предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "азы",
                                "target_lang_sense_clarification": "основы"
                            },
                            {
                                "target_lang_word": "азбука",
                                "target_lang_sense_clarification": "основы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbb90dbe-9fe5-3754-8ef7-a6cdb82fa72d",
                    "sense_definition": "An agent working for a government agency known by an acronym, such as the FBI or CIA.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local police were annoyed when the <w>alphabets</w> took over the investigation.",
                            "target_lang_translations": {
                                "ru": "Местная полиция была раздражена, когда <w>агенты спецслужб</w> взяли расследование на себя."
                            }
                        },
                        {
                            "text": "He suspected that an <w>alphabet</w> was monitoring his phone calls.",
                            "target_lang_translations": {
                                "ru": "Он подозревал, что <w>агент спецслужб</w> прослушивает его телефонные звонки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Агент, работающий на правительственное учреждение, известное под аббревиатурой (например, ФБР или ЦРУ)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "агент спецслужб",
                                "target_lang_sense_clarification": "контекстуальный перевод сленга"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c13fa00e-3abc-3f53-8257-795609c6b419",
                    "sense_definition": "To arrange items in the order of the letters of the alphabet.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "ordering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The librarian <w>alphabeted</w> the new collection of books.",
                            "target_lang_translations": {
                                "ru": "Библиотекарь <w>расположил по алфавиту</w> новую коллекцию книг."
                            }
                        },
                        {
                            "text": "You should <w>alphabet</w> the list of names before printing it.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>отсортировать по алфавиту</w> список имен перед печатью."
                            }
                        }
                    ],
                    "synonyms": [
                        "alphabetize"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Располагать предметы в порядке букв алфавита."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "располагать по алфавиту",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "сортировать по алфавиту",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    ],
                    "antonyms": [
                        "pełnia"
                    ],
                    "common_phrases": [
                        "czuć pustkę",
                        "wypełnić pustkę",
                        "pustka w sercu",
                        "pustka w głowie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Эмоциональное состояние, характеризующееся чувством одиночества, бессмысленности или грусти после утраты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пустота",

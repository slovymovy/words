    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d2942b81-fe0d-3a9b-9a4d-a3ab1fecbf6d",
                    "sense_definition": "Naturalny, duży ciek wodny płynący w wyżłobionym przez siebie korycie, zazwyczaj uchodzący do morza lub jeziora.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wisła to najdłuższa <w>rzeka</w> w Polsce.",
                            "target_lang_translations": {
                                "ru": "Висла — самая длинная <w>река</w> в Польше."
                            }
                        },
                        {
                            "text": "Wczoraj poszliśmy na długi spacer nad <w>rzeką</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера мы ходили на долгую прогулку вдоль <w>реки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nad rzeką",
                        "źródło rzeki",
                        "ujście rzeki",
                        "koryto rzeki",
                        "głęboka rzeka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественный, большой водный поток, текущий в выработанном им русле, обычно впадающий в море или озеро."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "река",
                                "target_lang_sense_clarification": "водный поток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94ded0ab-a124-3168-b353-752413dc99e3",
                    "sense_definition": "Duża liczba przemieszczających się ludzi lub obiektów, przypominająca płynącą wodę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphor_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez centrum miasta przepływała <w>rzeka</w> samochodów.",
                            "target_lang_translations": {
                                "ru": "Через центр города текла <w>река</w> автомобилей."
                            }
                        },
                        {
                            "text": "Na stadion zmierzała <w>rzeka</w> kibiców ubranych w barwy klubowe.",
                            "target_lang_translations": {
                                "ru": "На стадион направлялась <w>река</w> болельщиков, одетых в клубные цвета."
                            }
                        }
                    ],
                    "synonyms": [
                        "potok",
                        "strumień",
                        "fala",
                        "tłum"
                    ],
                    "common_phrases": [
                        "rzeka ludzi",
                        "rzeka samochodów",
                        "rzeka słów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество движущихся людей или объектов, напоминающее течение воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "река",
                                "target_lang_sense_clarification": "метафора: о множестве"
                            },
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "движущаяся масса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9b2aaac-8d9e-3b65-a28a-f2dc451edf1e",
                    "sense_definition": "Duża ilość cieczy płynąca strumieniem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po jej policzkach popłynęła <w>rzeka</w> gorzkich łez.",
                            "target_lang_translations": {
                                "ru": "По её щекам потекла <w>река</w> горьких слёз."
                            }
                        },
                        {
                            "text": "Podczas gwałtownej burzy ulicą płynęła prawdziwa <w>rzeka</w> wody.",
                            "target_lang_translations": {
                                "ru": "Во время сильной грозы по улице текла настоящая <w>река</w> воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "potok",
                        "strumień"
                    ],
                    "common_phrases": [
                        "rzeka łez",
                        "rzeka krwi",
                        "rzeka lawy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество жидкости, текущее потоком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "река",
                                "target_lang_sense_clarification": "обилие жидкости"
                            },
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "струя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "37902b78-8f77-3056-882e-2558e80e422a",
                    "sense_definition": "Gmina w Czechach, w kraju morawsko-śląskim.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Miejscowość <w>Rzeka</w> leży niedaleko granicy z Polską.",
                            "target_lang_translations": {
                                "ru": "Населённый пункт <w>Ржека</w> находится недалеко от границы с Польшей."
                            }
                        },
                        {
                            "text": "Planujemy wycieczkę w góry, przejeżdżając przez gminę <w>Rzeka</w>.",
                            "target_lang_translations": {
                                "ru": "Мы планируем поездку в горы, проезжая через общину <w>Ржека</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Община в Чехии, в Моравско-Силезском крае."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ржека",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]

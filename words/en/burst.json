{
    "word_family": [
        "outburst",
        "cloudburst",
        "sunburst",
        "starburst",
        "burster"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "db8bb1bf-fb36-32e3-ba46-068d34b1bcf9",
                    "sense_definition": "To break open or apart suddenly and violently, usually because of internal pressure.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "break_pressure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The balloon will <w>burst</w> if you blow it up too much.",
                            "target_lang_translations": {
                                "ru": "Воздушный шар <w>лопнет</w>, если надуть его слишком сильно."
                            }
                        },
                        {
                            "text": "The water pipes might <w>burst</w> during the freezing winter weather.",
                            "target_lang_translations": {
                                "ru": "Водопроводные трубы могут <w>лопнуть</w> в морозную зимнюю погоду."
                            }
                        }
                    ],
                    "synonyms": [
                        "spurt",
                        "spurt"
                    ],
                    "common_phrases": [
                        "burst open",
                        "burst apart"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно разрываться или лопаться, обычно из-за внутреннего давления"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лопаться",
                                "target_lang_sense_clarification": "о шарах, шинах, терпении"
                            },
                            {
                                "target_lang_word": "лопнуть",
                                "target_lang_sense_clarification": "совершенный вид"
                            },
                            {
                                "target_lang_word": "разрываться",
                                "target_lang_sense_clarification": "о трубах, бомбах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e89e9495-30fe-35c4-b8a7-f0dd5650be97",
                    "sense_definition": "To suddenly start doing something or expressing a strong emotion.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "sudden_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>burst</w> into tears after hearing the sad news.",
                            "target_lang_translations": {
                                "ru": "Она <w>разразилась</w> слезами, услышав печальную новость."
                            }
                        },
                        {
                            "text": "The entire audience <w>burst</w> out laughing at his joke.",
                            "target_lang_translations": {
                                "ru": "Вся аудитория <w>разразилась</w> смехом над его шуткой."
                            }
                        }
                    ],
                    "synonyms": [
                        "spurt",
                        "spurt"
                    ],
                    "common_phrases": [
                        "burst into tears",
                        "burst out laughing",
                        "burst into song",
                        "burst into flames"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно начинать делать что-либо или бурно выражать сильную эмоцию"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разразиться",
                                "target_lang_sense_clarification": "смехом, слезами, бранью"
                            },
                            {
                                "target_lang_word": "расплакаться",
                                "target_lang_sense_clarification": "внезапно заплакать"
                            },
                            {
                                "target_lang_word": "рассмеяться",
                                "target_lang_sense_clarification": "внезапно засмеяться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54f22895-4d9c-3ca2-b849-d11163f217a6",
                    "sense_definition": "To enter or leave a place hurriedly, suddenly, or forcefully.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sudden_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police <w>burst</w> through the front door without warning.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>ворвалась</w> через парадную дверь без предупреждения."
                            }
                        },
                        {
                            "text": "He <w>burst</w> into the room to deliver the urgent message.",
                            "target_lang_translations": {
                                "ru": "Он <w>ворвался</w> в комнату, чтобы передать срочное сообщение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "burst in",
                        "burst out",
                        "burst through"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стремительно и с силой входить или выходить откуда-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ворваться",
                                "target_lang_sense_clarification": "в помещение"
                            },
                            {
                                "target_lang_word": "вырваться",
                                "target_lang_sense_clarification": "наружу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5807d835-f226-3bcf-bf92-a16cf99e3bea",
                    "sense_definition": "To cause something to break open or apart suddenly.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "break_pressure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful not to <w>burst</w> the balloon with that sharp pin.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, чтобы не <w>лопнуть</w> шарик этой острой булавкой."
                            }
                        },
                        {
                            "text": "The heavy floodwaters finally <w>burst</w> the dam.",
                            "target_lang_translations": {
                                "ru": "Сильные паводковые воды наконец <w>прорвали</w> плотину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "burst a bubble",
                        "burst the banks"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить что-либо внезапно разорваться или лопнуть"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прорвать",
                                "target_lang_sense_clarification": "плотину, оборону"
                            },
                            {
                                "target_lang_word": "разорвать",
                                "target_lang_sense_clarification": "на части"
                            },
                            {
                                "target_lang_word": "лопнуть",
                                "target_lang_sense_clarification": "разг. заставить лопнуть (шарик)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1b8e495c-30cb-3225-aeda-fedbcabce527",
                    "sense_definition": "A sudden, short, and intense period of activity, emotion, or effort.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "sudden_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He worked in short <w>bursts</w> of energy followed by long rests.",
                            "target_lang_translations": {
                                "ru": "Он работал короткими <w>рывками</w>, сменявшимися долгим отдыхом."
                            }
                        },
                        {
                            "text": "A sudden <w>burst</w> of laughter came from the meeting room.",
                            "target_lang_translations": {
                                "ru": "Внезапный <w>взрыв</w> смеха донесся из переговорной."
                            }
                        }
                    ],
                    "synonyms": [
                        "spurt"
                    ],
                    "common_phrases": [
                        "burst of energy",
                        "burst of speed",
                        "burst of anger",
                        "short bursts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапный, короткий период интенсивной активности, эмоций или усилий"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вспышка",
                                "target_lang_sense_clarification": "гнева, эмоций"
                            },
                            {
                                "target_lang_word": "прилив",
                                "target_lang_sense_clarification": "энергии, сил"
                            },
                            {
                                "target_lang_word": "взрыв",
                                "target_lang_sense_clarification": "смеха, аплодисментов"
                            },
                            {
                                "target_lang_word": "рывок",
                                "target_lang_sense_clarification": "резкое усилие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab31fab7-69c9-35a8-adc5-16b87e809af0",
                    "sense_definition": "An instance of breaking open or exploding violently.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "break_pressure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The loud <w>burst</w> of the tire startled everyone on the street.",
                            "target_lang_translations": {
                                "ru": "Громкий <w>разрыв</w> шины напугал всех на улице."
                            }
                        },
                        {
                            "text": "We heard the <w>burst</w> of fireworks celebrating the new year.",
                            "target_lang_translations": {
                                "ru": "Мы слышали <w>залпы</w> фейерверков, празднующих новый год."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sudden burst",
                        "loud burst"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случай разрыва или насильственного взрыва"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрыв",
                                "target_lang_sense_clarification": "шины, снаряда"
                            },
                            {
                                "target_lang_word": "взрыв",
                                "target_lang_sense_clarification": "бомбы, фейерверка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d8b47f4-e990-3b6f-b321-178489dee2b6",
                    "sense_definition": "A rapid series of shots fired from an automatic weapon.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sudden_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldier fired a short <w>burst</w> at the target range.",
                            "target_lang_translations": {
                                "ru": "Солдат выпустил короткую <w>очередь</w> на стрельбище."
                            }
                        },
                        {
                            "text": "We heard a <w>burst</w> of machine-gun fire in the distance.",
                            "target_lang_translations": {
                                "ru": "Мы услышали пулеметную <w>очередь</w> вдалеке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "short burst",
                        "three-round burst"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстрая серия выстрелов из автоматического оружия"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очередь",
                                "target_lang_sense_clarification": "выстрелов"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
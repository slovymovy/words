                    }
                },
                {
                    "sense_id": "40d7d95f-187d-353a-b5a4-1d57246a44bc",
                    "sense_definition": "Osoba stojąca na czele partii politycznej lub organizacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_in_charge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy <w>lider</w> partii ogłosił zmiany w programie wyborczym.",

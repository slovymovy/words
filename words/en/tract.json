                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брошюра",
                                "target_lang_sense_clarification": "небольшая книга"
                            },
                            {
                                "target_lang_word": "трактат",
                                "target_lang_sense_clarification": "научное или философское сочинение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

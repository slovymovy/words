    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ca03596f-c1b4-30a8-84f0-1ccd70b16518",
                    "sense_definition": "Duży obiekt sportowy lub widowiskowy, często zadaszony, służący do rozgrywania zawodów i organizacji koncertów",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa <w>arena</w> w naszym mieście może pomieścić dwadzieścia tysięcy widzów.",
                            "target_lang_translations": {
                                "ru": "Новая <w>арена</w> в нашем городе может вместить двадцать тысяч зрителей."
                            }
                        },
                        {
                            "text": "Finałowy mecz siatkówki odbędzie się na głównej <w>arenie</w> sportowej.",
                            "target_lang_translations": {
                                "ru": "Финальный волейбольный матч состоится на главной спортивной <w>арене</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stadion"
                    ],
                    "common_phrases": [
                        "arena sportowa",
                        "nowoczesna arena",
                        "trybuny areny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное спортивное или зрелищное сооружение, часто крытое, используемое для проведения соревнований и концертов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "арена",
                                "target_lang_sense_clarification": "спортивный комплекс"
                            },
                            {
                                "target_lang_word": "стадион",
                                "target_lang_sense_clarification": "открытое спортивное сооружение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "973a2dbc-01da-3093-b5fb-658bbef0e47c",
                    "sense_definition": "Koliste miejsce otoczone widownią, na którym odbywają się różnego typu pokazy, zwłaszcza cyrkowe",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy treser lwów wszedł na <w>arenę</w>, na widowni zapadła cisza.",
                            "target_lang_translations": {
                                "ru": "Когда дрессировщик львов вышел на <w>арену</w>, в зрительном зале воцарилась тишина."
                            }
                        },
                        {
                            "text": "Wszystkie światła skierowano na środek <w>areny</w>, gdzie stał akrobata.",
                            "target_lang_translations": {
                                "ru": "Все огни направили на центр <w>арены</w>, где стоял акробат."
                            }
                        }
                    ],
                    "common_phrases": [
                        "arena cyrkowa",
                        "wejść na arenę",
                        "środek areny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Круглая площадка, окруженная зрительскими местами, на которой проходят представления, особенно цирковые"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "арена",
                                "target_lang_sense_clarification": "цирковая"
                            },
                            {
                                "target_lang_word": "манеж",
                                "target_lang_sense_clarification": "площадка в цирке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "096e73da-4a65-3f84-9a39-e52656060f26",
                    "sense_definition": "Sfera lub obszar, w którym toczą się ważne wydarzenia polityczne, społeczne lub zawodowe",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten kraj odgrywa coraz ważniejszą rolę na <w>arenie</w> międzynarodowej.",
                            "target_lang_translations": {
                                "ru": "Эта страна играет всё более важную роль на международной <w>арене</w>."
                            }
                        },
                        {
                            "text": "Konflikt między partiami przeniósł się na <w>arenę</w> polityczną.",
                            "target_lang_translations": {
                                "ru": "Конфликт между партиями перенесся на политическую <w>арену</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "arena międzynarodowa",
                        "arena polityczna",
                        "na arenie dziejów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сфера или область, в которой происходят важные политические, общественные или профессиональные события"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "арена",
                                "target_lang_sense_clarification": "сфера деятельности"
                            },
                            {
                                "target_lang_word": "поприще",
                                "target_lang_sense_clarification": "область деятельности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71808e66-03d4-3860-a511-d1e858047e3b",
                    "sense_definition": "W starożytnym amfiteatrze: wysypany piaskiem plac, na którym walczyli gladiatorzy",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ranny gladiator upadł na piasek <w>areny</w> i czekał na werdykt cesarza.",
                            "target_lang_translations": {
                                "ru": "Раненый гладиатор упал на песок <w>арены</w> и ждал вердикта императора."
                            }
                        },
                        {
                            "text": "Tłum wiwatował, gdy zwycięzca dumnie kroczył wokół <w>areny</w>.",
                            "target_lang_translations": {
                                "ru": "Толпа ликовала, когда победитель гордо шел вокруг <w>арены</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "krew na arenie",
                        "piasek areny"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "dotyczy starożytności"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В древнем амфитеатре: посыпанная песком площадка, на которой сражались гладиаторы"
                    },

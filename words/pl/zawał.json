                },
                {
                    "sense_id": "c45a4116-1af0-33f5-8fac-e3054bc176fa",
                    "sense_definition": "Zawalenie się stropu sztolni, jaskini lub wyrobiska górniczego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mining_collapse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starej części kopalni nastąpił nieoczekiwany <w>zawał</w>.",

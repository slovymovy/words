    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9916b804-eb1a-3de6-a7ed-9203149b84db",
                    "sense_definition": "In a manner relating to genes or genetics.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many consumers are concerned about eating <w>genetically</w> modified foods.",
                            "target_lang_translations": {
                                "ru": "Многие потребители обеспокоены употреблением в пищу <w>генетически</w> модифицированных продуктов."
                            }
                        },
                        {
                            "text": "The two species are <w>genetically</w> distinct despite looking very similar.",
                            "target_lang_translations": {
                                "ru": "Эти два вида <w>генетически</w> различны, несмотря на сильное внешнее сходство."
                            }
                        }
                    ],
                    "synonyms": [
                        "biologically",
                        "hereditarily"
                    ],
                    "antonyms": [
                        "environmentally"
                    ],
                    "common_phrases": [
                        "genetically modified",
                        "genetically engineered",
                        "genetically determined",
                        "genetically distinct"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относительно генов, наследственности или науки генетики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "генетически",
                                "target_lang_sense_clarification": "биология/наследственность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a211dfe-c20d-3bf8-a2a0-b063b672caeb",
                    "sense_definition": "By being members of the same linguistic family.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "English and German are <w>genetically</w> related languages.",
                            "target_lang_translations": {
                                "ru": "Английский и немецкий — <w>генетически</w> родственные языки."
                            }
                        },
                        {
                            "text": "Linguists analyze whether the dialects are <w>genetically</w> linked to a common ancestor.",
                            "target_lang_translations": {
                                "ru": "Лингвисты анализируют, связаны ли эти диалекты <w>генетически</w> с общим предком."
                            }
                        }
                    ],
                    "synonyms": [
                        "genealogically"
                    ],
                    "common_phrases": [
                        "genetically related languages"
                    ],
                    "target_lang_definitions": {
                        "ru": "На основе принадлежности к одной языковой семье или общего происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "генетически",
                                "target_lang_sense_clarification": "лингвистика/родство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "599d41a9-6e70-3801-82f3-629b2e63322e",
                    "sense_definition": "In terms of origin or development.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The philosopher explained the concept <w>genetically</w> by tracing its historical evolution.",
                            "target_lang_translations": {
                                "ru": "Философ объяснил эту концепцию <w>генетически</w>, проследив её историческую эволюцию."
                            }
                        },
                        {
                            "text": "We must analyze the problem <w>genetically</w> to understand how it began.",
                            "target_lang_translations": {
                                "ru": "Мы должны проанализировать проблему <w>генетически</w>, чтобы понять, как она возникла."
                            }
                        }
                    ],
                    "synonyms": [
                        "developmentally",
                        "historically"
                    ],
                    "common_phrases": [
                        "explained genetically"
                    ],
                    "target_lang_definitions": {
                        "ru": "С точки зрения происхождения, возникновения или процесса развития (генезиса)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "генетически",
                                "target_lang_sense_clarification": "происхождение/развитие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

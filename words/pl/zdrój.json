{
    "word_family": [
        "zdrojowy",
        "zdrojowisko"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5b3e3f6b-66d8-3361-bb1d-1675850392b0",
                    "sense_definition": "Naturalne miejsce, z którego woda podziemna wypływa na powierzchnię ziemi",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "water_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wędrowcy zatrzymali się przy leśnym <w>zdroju</w>, aby ugasić pragnienie.",
                            "target_lang_translations": {
                                "ru": "Путники остановились у лесного <w>родника</w>, чтобы утолить жажду."
                            }
                        },
                        {
                            "text": "Woda czerpana prosto ze <w>zdroju</w> smakowała wyśmienicie.",
                            "target_lang_translations": {
                                "ru": "Вода, набранная прямо из <w>источника</w>, была восхитительной на вкус."
                            }
                        }
                    ],
                    "synonyms": [
                        "źródło",
                        "krynica"
                    ],
                    "common_phrases": [
                        "czysty zdrój",
                        "woda ze zdroju",
                        "leśny zdrój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественный выход подземных вод на поверхность земли"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "родник",
                                "target_lang_sense_clarification": "естественный источник воды"
                            },
                            {
                                "target_lang_word": "источник",
                                "target_lang_sense_clarification": "место выхода воды"
                            },
                            {
                                "target_lang_word": "ключ",
                                "target_lang_sense_clarification": "бьющий из земли источник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "582dcee1-0b16-3b58-86a2-da3446000200",
                    "sense_definition": "Miejscowość posiadająca naturalne wody lecznicze i urządzenia do lecznictwa kąpielowego",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "spa_resort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi dziadkowie każdego lata wyjeżdżają do <w>zdroju</w> na kurację.",
                            "target_lang_translations": {
                                "ru": "Мои бабушка и дедушка каждое лето ездят на <w>курорт</w> на лечение."
                            }
                        },
                        {
                            "text": "W tym znanym <w>zdroju</w> leczy się choroby serca i układu krążenia.",
                            "target_lang_translations": {
                                "ru": "На этом известном <w>курорте</w> лечат болезни сердца и системы кровообращения."
                            }
                        }
                    ],
                    "synonyms": [
                        "krynica"
                    ],
                    "common_phrases": [
                        "jechać do zdroju",
                        "pijalnia wód w zdroju",
                        "park zdrojowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Курортная местность с минеральными водами и лечебницами"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курорт",
                                "target_lang_sense_clarification": "место отдыха и лечения"
                            },
                            {
                                "target_lang_word": "воды",
                                "target_lang_sense_clarification": "минеральные источники как место лечения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a656cef-6810-3db9-b5af-e3622996d957",
                    "sense_definition": "Obfity, gwałtowny strumień cieczy lub (w przenośni) czegoś innego",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "stream_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z rany popłynął <w>zdrój</w> czerwonej krwi.",
                            "target_lang_translations": {
                                "ru": "Из раны хлынул <w>поток</w> красной крови."
                            }
                        },
                        {
                            "text": "Po policzkach dziewczyny spłynął <w>zdrój</w> gorzkich łez.",
                            "target_lang_translations": {
                                "ru": "По щекам девушки потек <w>поток</w> горьких слез."
                            }
                        }
                    ],
                    "synonyms": [
                        "strumień",
                        "struga"
                    ],
                    "common_phrases": [
                        "zdrój łez",
                        "zdrój światła",
                        "zdrój krwi"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обильный, сильный поток жидкости или (перен.) чего-либо другого"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "стремительное течение"
                            },
                            {
                                "target_lang_word": "струя",
                                "target_lang_sense_clarification": "сильный напор жидкости"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "f5cd5345-5869-370a-981b-b5cd84751096",
                    "sense_definition": "Nazwa kilku wsi położonych na terenie Polski",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place_name_village",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mała wieś <w>Zdrój</w> leży w województwie warmińsko-mazurskim.",
                            "target_lang_translations": {
                                "ru": "Маленькая деревня <w>Здруй</w> находится в Варминьско-Мазурском воеводстве."
                            }
                        },
                        {
                            "text": "Mieszkańcy miejscowości <w>Zdrój</w> dbają o lokalne tradycje.",
                            "target_lang_translations": {
                                "ru": "Жители местности <w>Здруй</w> заботятся о местных традициях."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название нескольких деревень на территории Польши"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Здруй",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]

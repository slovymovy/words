                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "овощно́й",
                                "target_lang_sense_clarification": "относящийся к овощам"
                            },
                            {
                                "target_lang_word": "расти́тельный",
                                "target_lang_sense_clarification": "относящийся к растениям"

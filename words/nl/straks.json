{
    "word_family": [
        "strak",
        "strakjes"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ba07229e-e290-33b2-b9d5-de74fa837a38",
                    "sense_definition": "Op een moment in de nabije toekomst; later vandaag of zo dadelijk.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "future_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We zullen <w>straks</w> de vragen doorlopen.",
                            "target_lang_translations": {
                                "ru": "Мы <w>позже</w> разберем вопросы."
                            }
                        },
                        {
                            "text": "Ik kom <w>straks</w> even bij je langs om te praten.",
                            "target_lang_translations": {
                                "ru": "Я <w>позже</w> загляну к тебе поговорить."
                            }
                        },
                        {
                            "text": "Tot <w>straks</w>, ik zie je over een uurtje!",
                            "target_lang_translations": {
                                "ru": "<w>До скорого</w>, увидимся через час!"
                            }
                        }
                    ],
                    "synonyms": [
                        "later",
                        "strakjes",
                        "binnenkort"
                    ],
                    "common_phrases": [
                        "tot straks"
                    ],
                    "target_lang_definitions": {
                        "ru": "В момент в ближайшем будущем; позже сегодня или с минуты на минуту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позже",
                                "target_lang_sense_clarification": "позднее сегодня"
                            },
                            {
                                "target_lang_word": "скоро",
                                "target_lang_sense_clarification": "в ближайшее время"
                            },
                            {
                                "target_lang_word": "сейчас",
                                "target_lang_sense_clarification": "с минуты на минуту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6e7d4d9-3f08-39ff-bad2-334aad5fe61c",
                    "sense_definition": "Misschien of mogelijk; wordt vaak gebruikt om te waarschuwen voor een ongewenste gebeurtenis die kan plaatsvinden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "possibility_warning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zet die vaas weg, want <w>straks</w> valt hij nog kapot.",
                            "target_lang_translations": {
                                "ru": "Убери вазу, а то <w>вдруг</w> она разобьется."
                            }
                        },
                        {
                            "text": "<w>Straks</w> denken de buren nog dat we ruzie hebben.",
                            "target_lang_translations": {
                                "ru": "<w>Вдруг</w> соседи подумают, что мы ссоримся."
                            }
                        }
                    ],
                    "common_phrases": [
                        "straks nog"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возможно или может быть; часто используется для предупреждения о нежелательном событии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вдруг",
                                "target_lang_sense_clarification": "выражение опасения"
                            },
                            {
                                "target_lang_word": "а то",
                                "target_lang_sense_clarification": "иначе может случиться"
                            },
                            {
                                "target_lang_word": "чего доброго",
                                "target_lang_sense_clarification": "не дай бог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07ba89ee-8bb3-339d-8d84-b4a64ffe657c",
                    "sense_definition": "Een korte tijd geleden; daarnet.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "past_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Straks</w> deed de televisie het nog gewoon.",
                            "target_lang_translations": {
                                "ru": "Телевизор <w>только что</w> работал нормально."
                            }
                        },
                        {
                            "text": "Ik heb hem <w>straks</w> nog gezien in de gang.",
                            "target_lang_translations": {
                                "ru": "Я <w>недавно</w> видел его в коридоре."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Некоторое время назад; только что."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "только что",
                                "target_lang_sense_clarification": "совсем недавно"
                            },
                            {
                                "target_lang_word": "недавно",
                                "target_lang_sense_clarification": "некоторое время назад"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f5911817-2a7a-3100-9d78-f2332e02ac07",
                    "sense_definition": "De partitieve vorm van <w>strak</w> die wordt gebruikt na woorden als 'iets', 'niets', 'veel' of 'weinig'.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb je iets <w>straks</w> om aan te trekken voor het sporten?",
                            "target_lang_translations": {
                                "ru": "У тебя есть что-нибудь <w>обтягивающее</w> для спорта?"
                            }
                        },
                        {
                            "text": "Er zit niets <w>straks</w> aan deze wijde broek.",
                            "target_lang_translations": {
                                "ru": "В этих широких брюках нет ничего <w>тугого</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iets straks"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "partitief"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Партитивная форма прилагательного 'strak' (тугой, обтягивающий), используемая после неопределенных местоимений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обтягивающее",
                                "target_lang_sense_clarification": "об одежде"
                            },
                            {
                                "target_lang_word": "тугое",
                                "target_lang_sense_clarification": "о натяжении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
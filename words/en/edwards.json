                {
                    "sense_id": "c7dd3cc6-c26e-3669-81e3-7984f648abc2",
                    "sense_definition": "An English and Welsh surname derived from the given name Edward.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person_related",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Edwards</w> has been teaching history at our school for ten years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Эдвардс</w> преподает историю в нашей школе уже десять лет."
                            }
                        },
                        {
                            "text": "I received a letter from the <w>Edwards</w> family regarding the reunion.",
                            "target_lang_translations": {
                                "ru": "Я получил письмо от семьи <w>Эдвардс</w> по поводу встречи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Edwards family",
                        "Edwards syndrome"
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "derived from the name Edward"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Английская и валлийская фамилия, образованная от имени Эдвард."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Эдвардс",
                                "target_lang_sense_clarification": "фамилия"
                            }

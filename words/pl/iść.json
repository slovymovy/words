    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3abf4085-40d9-39cf-a2a7-d2d1dcc103e9",
                    "sense_definition": "Poruszać się, zmieniając miejsce za pomocą nóg.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy wolisz <iść> pieszo, czy jechać autobusem?",
                            "target_lang_translations": {
                                "ru": "Ты предпочитаешь <идти> пешком или ехать на автобусе?"
                            }
                        },
                        {
                            "text": "Musimy szybko <iść>, żeby zdążyć na pociąg.",
                            "target_lang_translations": {
                                "ru": "Нам нужно быстро <идти>, чтобы успеть на поезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "chodzić"
                    ],
                    "antonyms": [
                        "stać"
                    ],
                    "common_phrases": [
                        "iść pieszo",
                        "iść na spacer",
                        "iść prosto",
                        "iść do domu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться, перемещаться в пространстве, переступая ногами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "передвигаться шагом"
                            },
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "совершать движение пешком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adfd973e-15ca-397c-88dd-ee8a15f2f596",
                    "sense_definition": "Udawać się w określone miejsce lub w określonym celu, na przykład do pracy lub szkoły.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_destination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jutro rano muszę <iść> do lekarza na badania.",
                            "target_lang_translations": {
                                "ru": "Завтра утром я должен <идти> к врачу на обследование."
                            }
                        },
                        {
                            "text": "Czy chcesz <iść> z nami wieczorem do kina?",
                            "target_lang_translations": {
                                "ru": "Ты хочешь <пойти> с нами вечером в кино?"
                            }
                        }
                    ],
                    "synonyms": [
                        "chodzić"
                    ],
                    "common_phrases": [
                        "iść do pracy",
                        "iść do szkoły",
                        "iść na zakupy",
                        "iść spać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направляться в определенное место или с определенной целью (например, на работу или в школу)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "направляться куда-либо"
                            },
                            {
                                "target_lang_word": "отправляться",
                                "target_lang_sense_clarification": "начинать движение к цели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a3af21a-26f5-3d40-b9f6-c6f115f43019",
                    "sense_definition": "Przebiegać, rozwijać się lub postępować w określony sposób (o sprawach, planach, pracy).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "progress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystko <idzie> zgodnie z naszym wcześniejszym planem.",
                            "target_lang_translations": {
                                "ru": "Всё <идет> по нашему первоначальному плану."
                            }
                        },
                        {
                            "text": "Jak ci <idzie> nauka języka polskiego?",
                            "target_lang_translations": {
                                "ru": "Как у тебя <идет> изучение польского языка?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "iść zgodnie z planem",
                        "robota idzie",
                        "iść jak po maśle",
                        "wszystko idzie dobrze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Развиваться, протекать или происходить определенным образом (о делах, планах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "протекать, развиваться"
                            },
                            {
                                "target_lang_word": "продвигаться",
                                "target_lang_sense_clarification": "о прогрессе в делах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fa752ea-dfec-307b-99b8-630bb6c9d667",
                    "sense_definition": "Prowadzić, ciągnąć się lub być położonym w określonym kierunku (o drodze, rurze, kablu).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta wąska ścieżka <idzie> przez gęsty las aż do rzeki.",
                            "target_lang_translations": {
                                "ru": "Эта узкая тропинка <идет> через густой лес прямо к реке."
                            }
                        },
                        {
                            "text": "Główna rura z gazem <idzie> wzdłuż ulicy pod ziemią.",
                            "target_lang_translations": {
                                "ru": "Главная газовая труба <идет> вдоль улицы под землей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "droga idzie",
                        "szlak idzie",
                        "ulica idzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пролегать, простираться или вести в определенном направлении (о дороге, трубе, кабеле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "пролегать (о дороге)"
                            },
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "направлять (о дороге)"
                            },
                            {
                                "target_lang_word": "проходить",
                                "target_lang_sense_clarification": "пересекать пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3efcd68-5902-3d3b-acd8-353d7bb3c753",
                    "sense_definition": "Mijać lub upływać (o czasie).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_passing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czas <idzie> bardzo szybko, gdy się dobrze bawimy.",
                            "target_lang_translations": {
                                "ru": "Время <идет> очень быстро, когда мы хорошо проводим время."
                            }
                        },
                        {
                            "text": "Już <idzie> wiosna i dni stają się coraz cieplejsze.",
                            "target_lang_translations": {
                                "ru": "Уже <идет> весна, и дни становятся всё теплее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czas idzie",
                        "idzie zima",
                        "idzie noc",
                        "lata idą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить, миновать (о времени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "о течении времени"
                            },
                            {
                                "target_lang_word": "бежать",
                                "target_lang_sense_clarification": "быстро проходить (о времени)"
                            },
                            {
                                "target_lang_word": "проходить",
                                "target_lang_sense_clarification": "миновать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "308b8bc2-e243-3562-b0f0-ae41fc65225d",
                    "sense_definition": "Działać lub funkcjonować (o mechanizmach, zegarkach).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "functioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój stary zegarek <idzie> bardzo dokładnie mimo wieku.",
                            "target_lang_translations": {
                                "ru": "Мои старые часы <идут> очень точно, несмотря на возраст."
                            }
                        },
                        {
                            "text": "Silnik w samochodzie <idzie> cicho i równo po naprawie.",
                            "target_lang_translations": {
                                "ru": "Двигатель в машине <работает> тихо и ровно после ремонта."
                            }
                        }
                    ],
                    "synonyms": [
                        "chodzić"
                    ],
                    "antonyms": [
                        "stać"
                    ],
                    "common_phrases": [
                        "zegarek idzie",
                        "silnik idzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Функционировать, быть в рабочем состоянии (о механизмах, особенно часах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "о часах"
                            },
                            {
                                "target_lang_word": "работать",
                                "target_lang_sense_clarification": "функционировать (о механизмах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4da043ce-e0b2-3d3c-875c-8f4cc625311e",
                    "sense_definition": "Wydobywać się lub ulatniać z czegoś (np. dym, para, woda).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z komina sąsiada <idzie> gęsty, czarny dym.",
                            "target_lang_translations": {
                                "ru": "Из трубы соседа <идет> густой черный дым."
                            }
                        },
                        {
                            "text": "Kiedy kroję cebulę, łzy same <idą> mi z oczu.",
                            "target_lang_translations": {
                                "ru": "Когда я режу лук, слезы сами <текут> из глаз."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydobywać się",
                        "wydostawać się"
                    ],
                    "common_phrases": [
                        "krew idzie",
                        "dym idzie",
                        "para idzie",
                        "woda idzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделяться, исходить откуда-либо (о дыме, паре, жидкости)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "выделяться (о дыме, паре)"
                            },
                            {
                                "target_lang_word": "течь",
                                "target_lang_sense_clarification": "литься (о крови, слезах)"
                            },
                            {
                                "target_lang_word": "валить",
                                "target_lang_sense_clarification": "густо выходить (о дыме)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3158cf31-14e2-3fbc-9ce7-2e02e10a25c3",
                    "sense_definition": "Być przeznaczonym na jakiś cel lub zużywanym na coś (np. o pieniądzach).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "allocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prawie cała moja wypłata <idzie> na bieżące rachunki.",
                            "target_lang_translations": {
                                "ru": "Почти вся моя зарплата <уходит> на текущие счета."
                            }
                        },
                        {
                            "text": "Dużo pieniędzy z budżetu <idzie> na remont dróg.",
                            "target_lang_translations": {
                                "ru": "Много денег из бюджета <идет> на ремонт дорог."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pieniądze idą na",
                        "wypłata idzie na",
                        "wszystko idzie na"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расходоваться или предназначаться для чего-либо (о деньгах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "тратиться на что-то"
                            },
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "расходоваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ae1b430-b297-3d40-aaad-daf41b671f13",
                    "sense_definition": "Być możliwym do zrobienia (używane w formie bezosobowej).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "possibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z nim nie <idzie> się w ogóle normalnie dogadać.",
                            "target_lang_translations": {
                                "ru": "С ним вообще <невозможно> нормально договориться."
                            }
                        },
                        {
                            "text": "W tym hałasie po prostu nie <idzie> wytrzymać.",
                            "target_lang_translations": {
                                "ru": "В этом шуме просто <невозможно> выдержать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nie idzie wytrzymać",
                        "idzie zwariować",
                        "nie idzie zrozumieć"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть возможным или удаваться (обычно в безличных отрицательных конструкциях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получаться",
                                "target_lang_sense_clarification": "удаваться"
                            },
                            {
                                "target_lang_word": "возможно",
                                "target_lang_sense_clarification": "наречие возможности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
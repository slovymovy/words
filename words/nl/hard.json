                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>hard</w> het staal in de oven.",
                            "target_lang_translations": {
                                "ru": "Я <w>закаляю</w> сталь в печи."
                            }

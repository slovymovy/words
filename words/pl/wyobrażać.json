{
    "word_family": [
        "wyobraźnia",
        "wyobrażenie",
        "wyobrażalny",
        "niewyobrażalny",
        "wyobrażeniowy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "df3dff36-7af4-3406-a18e-75909bfb0b0d",
                    "sense_definition": "Tworzyć w umyśle obraz kogoś, czegoś lub jakiejś sytuacji; fantazjować (wymaga zaimka zwrotnego 'sobie').",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mental_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci lubią <wyobrażać> sobie, że są odważnymi rycerzami lub księżniczkami.",
                            "target_lang_translations": {
                                "ru": "Дети любят <представлять> себе, что они отважные рыцари или принцессы."
                            }
                        },
                        {
                            "text": "Często <wyobrażam> sobie, jak będzie wyglądał mój nowy dom po remoncie.",
                            "target_lang_translations": {
                                "ru": "Я часто <представляю> себе, как будет выглядеть мой новый дом после ремонта."
                            }
                        }
                    ],
                    "synonyms": [
                        "imaginować",
                        "widzieć"
                    ],
                    "common_phrases": [
                        "wyobrażać-sobie przyszłość",
                        "wyobrażać-sobie coś",
                        "wyobrażać-sobie życie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать в уме образ кого-либо, чего-либо или какой-либо ситуации; фантазировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представлять себе",
                                "target_lang_sense_clarification": "создавать мысленный образ"
                            },
                            {
                                "target_lang_word": "воображать",
                                "target_lang_sense_clarification": "фантазировать, мыслить образами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02189b2b-407e-3cb0-bf7c-9afd04e9262a",
                    "sense_definition": "Rozumieć coś, pojmować lub zdawać sobie sprawę z czegoś (często używane w przeczeniach, aby wyrazić niedowierzanie).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <wyobrażam> sobie, jak można być tak nieodpowiedzialnym człowiekiem.",
                            "target_lang_translations": {
                                "ru": "Я не <представляю> себе, как можно быть таким безответственным человеком."
                            }
                        },
                        {
                            "text": "Nawet nie <wyobrażasz> sobie, ile pracy włożyłem w ten projekt.",
                            "target_lang_translations": {
                                "ru": "Ты даже не <представляешь> себе, сколько труда я вложил в этот проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozumieć",
                        "pojmować"
                    ],
                    "common_phrases": [
                        "nie-wyobrażać sobie",
                        "trudno-wyobrażać sobie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Понимать, осознавать что-либо (часто используется в отрицаниях для выражения недоверия или невозможности что-то постичь)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представлять себе",
                                "target_lang_sense_clarification": "понимать, осознавать масштабы или суть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7929013-c7e4-38a2-947b-c772b3200123",
                    "sense_definition": "Przedstawiać kogoś lub coś w formie wizualnej, na przykład w dziele sztuki, lub być symbolem czegoś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten zabytkowy obraz <wyobraża> scenę bitwy z czasów średniowiecza.",
                            "target_lang_translations": {
                                "ru": "Эта старинная картина <изображает> сцену битвы времен Средневековья."
                            }
                        },
                        {
                            "text": "Rzeźba w parku <wyobraża> mitologiczną postać chroniącą miasto.",
                            "target_lang_translations": {
                                "ru": "Скульптура в парке <изображает> мифологического персонажа, защищающего город."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedstawiać",
                        "obrazować"
                    ],
                    "common_phrases": [
                        "wyobrażać postać",
                        "wyobrażać scenę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изображать кого-либо или что-либо в визуальной форме (например, в искусстве) или символизировать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изображать",
                                "target_lang_sense_clarification": "показывать в художественном произведении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
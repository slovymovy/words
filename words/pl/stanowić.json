{
    "word_family": [
        "postanowić",
        "ustanowić",
        "zastanowić",
        "stanowisko"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "081d0d5a-bb26-3fba-8886-f97c727d4055",
                    "sense_definition": "Tworzyć coś, składać się na jakąś całość lub być jej elementem (często w odniesieniu do liczebności lub struktury).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_composition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kobiety <stanowią> większość pracowników w naszej firmie.",
                            "target_lang_translations": {
                                "ru": "Женщины <составляют> большинство сотрудников в нашей фирме."
                            }
                        },
                        {
                            "text": "Lasy <stanowią> naturalne bogactwo tego regionu.",
                            "target_lang_translations": {
                                "ru": "Леса <составляют> природное богатство этого региона."
                            }
                        },
                        {
                            "text": "Opłaty za prąd <stanowią> dużą część naszego domowego budżetu.",
                            "target_lang_translations": {
                                "ru": "Счета за электричество <составляют> большую часть нашего семейного бюджета."
                            }
                        }
                    ],
                    "synonyms": [
                        "tworzyć",
                        "składać się"
                    ],
                    "common_phrases": [
                        "stanowić większość",
                        "stanowić część",
                        "stanowić całość",
                        "stanowić połowę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образовывать, составлять собой целое или быть его частью (часто в контексте количества или структуры)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составлять",
                                "target_lang_sense_clarification": "образовывать количество или часть целого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcd637cc-5c50-3e5e-8166-e9fe73331bd6",
                    "sense_definition": "Być czymś, reprezentować sobą określony stan, cechę lub wartość (często w połączeniu z rzeczownikami abstrakcyjnymi takimi jak problem, zagrożenie, wyzwanie).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Brak pieniędzy <stanowi> poważny problem dla wielu rodzin.",
                            "target_lang_translations": {
                                "ru": "Нехватка денег <является> серьёзной проблемой для многих семей."
                            }
                        },
                        {
                            "text": "To odkrycie <stanowi> przełom w nowoczesnej medycynie.",
                            "target_lang_translations": {
                                "ru": "Это открытие <является> прорывом в современной медицине."
                            }
                        },
                        {
                            "text": "Silny wiatr może <stanowić> zagrożenie dla turystów w górach.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер может <представлять> угрозу для туристов в горах."
                            }
                        }
                    ],
                    "synonyms": [
                        "być"
                    ],
                    "common_phrases": [
                        "stanowić problem",
                        "stanowić zagrożenie",
                        "stanowić wyzwanie",
                        "stanowić podstawę",
                        "stanowić dowód"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть чем-то, представлять собой определённое состояние или качество (часто с абстрактными понятиями, такими как проблема, угроза)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представлять (собой)",
                                "target_lang_sense_clarification": "выражать собой (угрозу, проблему, интерес)"
                            },
                            {
                                "target_lang_word": "являться",
                                "target_lang_sense_clarification": "быть, выступать в качестве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b06b21e-3b54-3728-be15-6e44af60d6cf",
                    "sense_definition": "Decydować o czymś, być czynnikiem, który przesądza o wyniku, wartości lub charakterze czegoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_determination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "O sile tej drużyny <stanowią> jej doskonali napastnicy.",
                            "target_lang_translations": {
                                "ru": "Силу этой команды <определяют> её превосходные нападающие."
                            }
                        },
                        {
                            "text": "Jakość użytych materiałów <stanowi> o trwałości produktu.",
                            "target_lang_translations": {
                                "ru": "Качество использованных материалов <определяет> долговечность продукта."
                            }
                        },
                        {
                            "text": "To wydarzenie będzie <stanowić> o naszej wspólnej przyszłości.",
                            "target_lang_translations": {
                                "ru": "Это событие будет <определять> наше общее будущее."
                            }
                        }
                    ],
                    "synonyms": [
                        "decydować",
                        "składać się"
                    ],
                    "common_phrases": [
                        "stanowić o czymś",
                        "stanowić o sile",
                        "stanowić o wartości",
                        "stanowić o sukcesie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определять, быть решающим фактором, влияющим на результат, ценность или характер чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "определять",
                                "target_lang_sense_clarification": "быть решающим фактором"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72e07bbb-9e15-39e6-a9fa-c015fb1c435e",
                    "sense_definition": "Ustanawiać prawo, wydawać oficjalną decyzję lub rozporządzenie (używane głównie w kontekście prawnym).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Konstytucja jasno <stanowi>, że każdy obywatel ma prawo do wolności słowa.",
                            "target_lang_translations": {
                                "ru": "Конституция ясно <гласит>, что каждый гражданин имеет право на свободу слова."
                            }
                        },
                        {
                            "text": "Regulamin szkoły <stanowi> o obowiązku noszenia mundurków.",
                            "target_lang_translations": {
                                "ru": "Школьный устав <устанавливает> обязанность носить форму."
                            }
                        }
                    ],
                    "synonyms": [
                        "ustanawiać",
                        "decydować"
                    ],
                    "common_phrases": [
                        "jak stanowi prawo",
                        "ustawa stanowi",
                        "przepis stanowi",
                        "o czym stanowi"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "official"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устанавливать правило, содержать официальное решение или предписание (в юридическом контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гласить",
                                "target_lang_sense_clarification": "содержать текст закона или правила"
                            },
                            {
                                "target_lang_word": "устанавливать",
                                "target_lang_sense_clarification": "вводить правило или норму"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_id": "c55f8a39-ff32-38c4-ac2a-24cb075bfbf6",
                    "sense_definition": "Nabyć lub zgromadzić dla siebie potrzebne rzeczy, zwykle na zapas (używane z zaimkiem zwrotnym \"się\").",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wyjazdem w góry musimy <w>zaopatrzyć</w> się w odpowiedni sprzęt i ciepłą odzież.",
                            "target_lang_translations": {
                                "ru": "Перед поездкой в горы мы должны <w>запастись</w> соответствующим снаряжением и теплой одеждой."
                            }
                        },
                        {
                            "text": "Warto wcześniej <w>zaopatrzyć</w> się w bilety, aby uniknąć stania w długich kolejkach.",
                            "target_lang_translations": {
                                "ru": "Стоит заранее <w>приобрести</w> билеты, чтобы избежать стояния в длинных очередях."
                            }
                        }
                    ],
                    "synonyms": [
                        "kupić",
                        "nabyć",
                        "zgromadzić"
                    ],
                    "antonyms": [
                        "pozbyć się"
                    ],
                    "common_phrases": [
                        "zaopatrzyć się w prowiant",
                        "zaopatrzyć się w leki",
                        "zaopatrzyć się na drogę",
                        "zaopatrzyć się w gotówkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приобрести или запасти для себя необходимые вещи, обычно впрок (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запастись",
                                "target_lang_sense_clarification": "сделать запас чего-либо"
                            },
                            {
                                "target_lang_word": "приобрести",
                                "target_lang_sense_clarification": "купить, получить в обладание"
                            },
                            {
                                "target_lang_word": "обзавестись",
                                "target_lang_sense_clarification": "получить, раздобыть"

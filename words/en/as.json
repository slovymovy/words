{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "958b6649-e06a-32af-9d48-422cefe212cb",
                    "sense_definition": "In the role, function, or capacity of.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He works <w>as</w> a teacher at the local school.",
                            "target_lang_translations": {
                                "ru": "Он работает <w>в качестве</w> учителя в местной школе."
                            }
                        },
                        {
                            "text": "She participated in the event <w>as</w> a special guest.",
                            "target_lang_translations": {
                                "ru": "Она участвовала в мероприятии <w>как</w> специальный гость."
                            }
                        }
                    ],
                    "common_phrases": [
                        "work as",
                        "act as",
                        "known as",
                        "serve as"
                    ],
                    "target_lang_definitions": {
                        "ru": "В роли, функции или качестве кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в качестве",
                                "target_lang_sense_clarification": "указывает на функцию или роль"
                            },
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "в роли кого-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26d12e32-a2e6-3de0-81af-b8804a8a3242",
                    "sense_definition": "Used to introduce a basis of comparison, stating what someone or something is similar to.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You are not as tall <w>as</w> my sister.",
                            "target_lang_translations": {
                                "ru": "Ты не такой высокий, <w>как</w> моя сестра."
                            }
                        },
                        {
                            "text": "The box was heavy <w>as</w> lead.",
                            "target_lang_translations": {
                                "ru": "Коробка была тяжелой, <w>как</w> свинец."
                            }
                        }
                    ],
                    "synonyms": [
                        "like"
                    ],
                    "common_phrases": [
                        "the same as",
                        "such as"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения основы сравнения, указывая, на кого или на что похож объект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "при сравнении"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "aca645ad-4591-3cf2-acc2-921d16e3d2eb",
                    "sense_definition": "To such an extent or degree; used to start a comparison.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is not <w>as</w> hard as you think.",
                            "target_lang_translations": {
                                "ru": "Это не <w>так</w> сложно, как ты думаешь."
                            }
                        },
                        {
                            "text": "Please come <w>as</w> quickly as you can.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, приходи <w>как</w> можно скорее."
                            }
                        }
                    ],
                    "synonyms": [
                        "so"
                    ],
                    "common_phrases": [
                        "as soon as",
                        "as well as",
                        "as much as",
                        "as far as"
                    ],
                    "target_lang_definitions": {
                        "ru": "До такой степени или меры; используется для начала сравнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "указание на степень"
                            },
                            {
                                "target_lang_word": "настолько",
                                "target_lang_sense_clarification": "в такой степени"
                            },
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "в конструкции 'как... так и'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "006d96b8-a38a-3d94-8b8c-bb6a5d43b467",
                    "sense_definition": "For example; for instance.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "example",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Healthy foods, such <w>as</w> fruits and vegetables, are essential for your diet.",
                            "target_lang_translations": {
                                "ru": "Здоровая еда, такая <w>как</w> фрукты и овощи, необходима для вашей диеты."
                            }
                        },
                        {
                            "text": "Primary colors, <w>as</w> red and blue, can be mixed to make other colors.",
                            "target_lang_translations": {
                                "ru": "Основные цвета, <w>как</w> красный и синий, можно смешивать для получения других цветов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "such as"
                    ],
                    "target_lang_definitions": {
                        "ru": "Например; к примеру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "при перечислении примеров"
                            },
                            {
                                "target_lang_word": "например",
                                "target_lang_sense_clarification": "вводное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3efeb24a-995a-35cc-a0b7-cb24c2192b92",
                    "sense_definition": "Very much; extremely. Used to intensify an adjective.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The weather today is hot <w>as</w>.",
                            "target_lang_translations": {
                                "ru": "Погода сегодня <w>ну просто</w> жара."
                            }
                        },
                        {
                            "text": "That new car is sweet <w>as</w>.",
                            "target_lang_translations": {
                                "ru": "Эта новая машина <w>просто</w> класс."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sweet as"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "regional",
                            "comment": "Australia and New Zealand"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень; чрезвычайно. Используется для усиления прилагательного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ну просто",
                                "target_lang_sense_clarification": "разговорное усиление"
                            },
                            {
                                "target_lang_word": "очень",
                                "target_lang_sense_clarification": "наречие меры"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "07fb6dff-91b2-33f0-ac98-e5eb00d54209",
                    "sense_definition": "At the same time that; while something else is happening.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I saw him <w>as</w> I was leaving the store.",
                            "target_lang_translations": {
                                "ru": "Я увидел его, <w>когда</w> выходил из магазина."
                            }
                        },
                        {
                            "text": "She sang a song <w>as</w> she worked in the garden.",
                            "target_lang_translations": {
                                "ru": "Она пела песню, <w>пока</w> работала в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "while",
                        "when"
                    ],
                    "common_phrases": [
                        "just as"
                    ],
                    "target_lang_definitions": {
                        "ru": "В то же время, когда происходит что-то другое; пока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "когда",
                                "target_lang_sense_clarification": "союз времени"
                            },
                            {
                                "target_lang_word": "в то время как",
                                "target_lang_sense_clarification": "одновременность действия"
                            },
                            {
                                "target_lang_word": "пока",
                                "target_lang_sense_clarification": "в течение времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36404cfc-3cdf-349c-87ae-d5cd8b5a968d",
                    "sense_definition": "In the way or manner that.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do exactly <w>as</w> I say.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, делай точно <w>как</w> я говорю."
                            }
                        },
                        {
                            "text": "Leave everything <w>as</w> it is.",
                            "target_lang_translations": {
                                "ru": "Оставь всё <w>как</w> есть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "as follows",
                        "as usual",
                        "as agreed",
                        "as you know"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тем образом или способом, которым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "союз образа действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac71b3c7-c67b-367b-8b23-f4a94a1053ea",
                    "sense_definition": "Because; considering that.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reason",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>As</w> it was raining, we decided to stay inside.",
                            "target_lang_translations": {
                                "ru": "<w>Так как</w> шёл дождь, мы решили остаться дома."
                            }
                        },
                        {
                            "text": "You should go first, <w>as</w> you are the oldest.",
                            "target_lang_translations": {
                                "ru": "Ты должен идти первым, <w>поскольку</w> ты самый старший."
                            }
                        }
                    ],
                    "synonyms": [
                        "because",
                        "since",
                        "given that"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потому что; учитывая, что."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "так как",
                                "target_lang_sense_clarification": "причинный союз"
                            },
                            {
                                "target_lang_word": "поскольку",
                                "target_lang_sense_clarification": "причина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40dd1870-064c-3130-82d6-08ef7d69d24e",
                    "sense_definition": "Varying through time in the same proportion that.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "proportion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>As</w> we get older, we get wiser.",
                            "target_lang_translations": {
                                "ru": "<w>По мере того как</w> мы становимся старше, мы становимся мудрее."
                            }
                        },
                        {
                            "text": "The air becomes thinner <w>as</w> you climb higher up the mountain.",
                            "target_lang_translations": {
                                "ru": "Воздух становится разреженнее, <w>по мере того как</w> поднимаешься выше в гору."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменяясь во времени в той же пропорции, что и."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по мере того как",
                                "target_lang_sense_clarification": "постепенное изменение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "875d8c40-e26a-3f35-bf3f-7eaefb4b4eef",
                    "sense_definition": "Although; even though.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "concession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hard <w>as</w> it was, he managed to finish the race.",
                            "target_lang_translations": {
                                "ru": "<w>Как ни</w> трудно это было, ему удалось закончить гонку."
                            }
                        },
                        {
                            "text": "Much <w>as</w> I like him, I cannot agree with his plan.",
                            "target_lang_translations": {
                                "ru": "<w>Как бы</w> он мне ни нравился, я не могу согласиться с его планом."
                            }
                        }
                    ],
                    "synonyms": [
                        "although",
                        "though",
                        "albeit"
                    ],
                    "common_phrases": [
                        "try as I might"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хотя; несмотря на то, что."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как ни",
                                "target_lang_sense_clarification": "уступительный оборот"
                            },
                            {
                                "target_lang_word": "хотя",
                                "target_lang_sense_clarification": "союз уступки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a32bbac2-8d08-351f-8bab-ec1d4bd75b02",
                    "sense_definition": "Used to introduce a result or consequence.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was so quiet <w>as</w> to be practically invisible.",
                            "target_lang_translations": {
                                "ru": "Он был настолько тихим, <w>что</w> был практически незаметен."
                            }
                        },
                        {
                            "text": "Please be so good <w>as</w> to close the door behind you.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, будьте так добры, <w>чтобы</w> закрыть за собой дверь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "so as to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения результата или следствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чтобы",
                                "target_lang_sense_clarification": "целевой союз"
                            },
                            {
                                "target_lang_word": "так... что",
                                "target_lang_sense_clarification": "следствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31f55a9f-5984-397b-b991-6a0f4825497d",
                    "sense_definition": "As if; as though.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "hypothetical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He stood silent, <w>as</w> stunned by the news.",
                            "target_lang_translations": {
                                "ru": "Он стоял молча, <w>словно</w> оглушенный новостью."
                            }
                        },
                        {
                            "text": "It seemed <w>as</w> it were a dream.",
                            "target_lang_translations": {
                                "ru": "Казалось, <w>как будто</w> это был сон."
                            }
                        }
                    ],
                    "synonyms": [
                        "as if",
                        "as though"
                    ],
                    "common_phrases": [
                        "as it were"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "when used without 'if' or 'though'"
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Как будто; словно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "в значении 'как будто'"
                            },
                            {
                                "target_lang_word": "словно",
                                "target_lang_sense_clarification": "сравнительный союз"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6b0d104b-a1fc-3db3-81fb-cf5c43d73c61",
                    "sense_definition": "An ancient Roman bronze or copper coin.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "currency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum displayed a rare Roman <w>as</w> from the first century.",
                            "target_lang_translations": {
                                "ru": "В музее был выставлен редкий римский <w>асс</w> первого века."
                            }
                        },
                        {
                            "text": "He found an old <w>as</w> while digging in the ruins.",
                            "target_lang_translations": {
                                "ru": "Он нашел старый <w>асс</w>, копаясь в руинах."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Древнеримская бронзовая или медная монета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "асс",
                                "target_lang_sense_clarification": "монета"
                            },
                            {
                                "target_lang_word": "ас",
                                "target_lang_sense_clarification": "вариант написания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f467b589-1f3f-3cd6-bd1b-5e23705e69b4",
                    "sense_definition": "A large hoofed mammal, Equus ferus caballus, commonly used for riding and pulling heavy loads.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer used a strong <w>horse</w> to pull the heavy cart.",
                            "target_lang_translations": {
                                "ru": "Фермер использовал сильную <w>лошадь</w>, чтобы тянуть тяжелую телегу."
                            }
                        },
                        {
                            "text": "She learned to ride a <w>horse</w> when she was just a child.",
                            "target_lang_translations": {
                                "ru": "Она научилась ездить верхом на <w>лошади</w>, когда была еще ребенком."
                            }
                        }
                    ],
                    "synonyms": [
                        "steed",
                        "equine",
                        "nag"
                    ],
                    "common_phrases": [
                        "ride a horse",
                        "dark horse",
                        "hold your horses",
                        "eat a horse",
                        "on a horse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное непарнокопытное млекопитающее, обычно используемое для верховой езды и перевозки грузов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лошадь",
                                "target_lang_sense_clarification": "общее название животного"
                            },
                            {
                                "target_lang_word": "конь",
                                "target_lang_sense_clarification": "самец лошади; часто используется в поэтическом или военном контексте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb39ef6b-d2ca-3ad8-a51c-91563c0d666a",
                    "sense_definition": "The chess piece shaped like a horse's head, formally known as a knight.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "game_piece",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can move your <w>horse</w> in an L-shape to jump over other pieces.",
                            "target_lang_translations": {
                                "ru": "Вы можете ходить <w>конем</w> буквой «Г», чтобы перепрыгивать через другие фигуры."
                            }
                        },
                        {
                            "text": "He captured my bishop with his <w>horse</w> early in the game.",
                            "target_lang_translations": {
                                "ru": "Он взял моего слона своим <w>конем</w> в начале партии."
                            }
                        }
                    ],
                    "synonyms": [
                        "knight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шахматная фигура в форме головы лошади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конь",
                                "target_lang_sense_clarification": "шахматная фигура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5bbb138-33b8-359e-b1c5-09e8054ab793",
                    "sense_definition": "A piece of gymnastic equipment with a padded body on legs, used for vaulting or performing exercises.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "equipment_gym",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gymnast performed a difficult routine on the pommel <w>horse</w>.",
                            "target_lang_translations": {
                                "ru": "Гимнаст выполнил сложную программу на <w>коне</w>."
                            }
                        },
                        {
                            "text": "She scored very highly for her vault over the <w>horse</w>.",
                            "target_lang_translations": {
                                "ru": "Она получила очень высокую оценку за опорный прыжок через <w>коня</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pommel horse",
                        "vaulting horse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гимнастический снаряд с мягким корпусом на ножках, используемый для опорных прыжков и упражнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конь",
                                "target_lang_sense_clarification": "гимнастический снаряд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62f0c0cb-882a-3f0b-9398-3c49ac07dd70",
                    "sense_definition": "Soldiers who fight on horseback, considered as a group.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general sent a thousand <w>horse</w> to attack the enemy flank.",
                            "target_lang_translations": {
                                "ru": "Генерал послал тысячу <w>всадников</w>, чтобы атаковать фланг врага."
                            }
                        },
                        {
                            "text": "We should place two units of <w>horse</w> on this side of the field.",
                            "target_lang_translations": {
                                "ru": "Мы должны разместить два отряда <w>конницы</w> на этой стороне поля."
                            }
                        }
                    ],
                    "synonyms": [
                        "cavalry"
                    ],
                    "antonyms": [
                        "foot",
                        "infantry"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "collective"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Солдаты, сражающиеся верхом; кавалерия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конница",
                                "target_lang_sense_clarification": "род войск"
                            },
                            {
                                "target_lang_word": "кавалерия",
                                "target_lang_sense_clarification": "военный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4806cb02-cbfc-3eec-9741-c9a368cc6271",
                    "sense_definition": "A frame with legs used to support something, such as wood being sawed or clothes being dried.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "equipment_frame",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He placed the wooden plank on the <w>horse</w> to cut it safely.",
                            "target_lang_translations": {
                                "ru": "Он положил деревянную доску на <w>козлы</w>, чтобы безопасно ее распилить."
                            }
                        },
                        {
                            "text": "Please hang the wet shirts on the clothes <w>horse</w> near the heater.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, повесь мокрые рубашки на <w>сушилку</w> возле обогревателя."
                            }
                        }
                    ],
                    "synonyms": [
                        "sawhorse",
                        "clothes horse"
                    ],
                    "common_phrases": [
                        "clothes horse",
                        "sawhorse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рама на ножках, используемая для опоры чего-либо, например, при распиливании дров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "козлы",
                                "target_lang_sense_clarification": "подставка для пилки дров или строительных работ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ba12ae5-55cc-3bd1-9e6b-b34a50d3fc29",
                    "sense_definition": "Heroin.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "drug_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police arrested the dealer for selling <w>horse</w> on the street.",
                            "target_lang_translations": {
                                "ru": "Полиция арестовала дилера за продажу <w>Геры</w> на улице."
                            }
                        },
                        {
                            "text": "He struggled with an addiction to <w>horse</w> for many years.",
                            "target_lang_translations": {
                                "ru": "Он много лет боролся с зависимостью от <w>герыча</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "heroin",
                        "smack",
                        "junk",
                        "scag"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Героин (слэнг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Гера",
                                "target_lang_sense_clarification": "слэнг наркоманов"
                            },
                            {
                                "target_lang_word": "герыч",
                                "target_lang_sense_clarification": "распространенный сленговый вариант"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "83524695-de06-3bf7-a75c-35118b95f98e",
                    "sense_definition": "To behave in a silly, noisy, or rough manner; often used with 'around'.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "behavior_play",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stop <w>horsing</w> around and get back to your homework immediately.",
                            "target_lang_translations": {
                                "ru": "Перестань <w>дурачиться</w> и немедленно возвращайся к домашней работе."
                            }
                        },
                        {
                            "text": "The children were <w>horsing</w> in the backyard all afternoon.",
                            "target_lang_translations": {
                                "ru": "Дети <w>бесились</w> на заднем дворе весь день."
                            }
                        }
                    ],
                    "synonyms": [
                        "horse around",
                        "fool around"
                    ],
                    "common_phrases": [
                        "horse around"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя глупо, шумно или грубо; часто используется с 'around'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дурачиться",
                                "target_lang_sense_clarification": "вести себя несерьезно"
                            },
                            {
                                "target_lang_word": "валять дурака",
                                "target_lang_sense_clarification": "фразеологизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f877d00-5ec5-3e8f-8778-608833e2b1de",
                    "sense_definition": "To pull, haul, or move something heavy with great effort.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_move",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sailors were <w>horsing</w> the boat out of the mud.",
                            "target_lang_translations": {
                                "ru": "Моряки <w>вытаскивали</w> лодку из грязи."
                            }
                        },
                        {
                            "text": "We had to <w>horse</w> the heavy piano up three flights of stairs.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <w>переть</w> тяжелое пианино на третий этаж."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тащить или перемещать что-то тяжелое с большим усилием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тащить",
                                "target_lang_sense_clarification": "перемещать с усилием"
                            },
                            {
                                "target_lang_word": "переть",
                                "target_lang_sense_clarification": "разговорное, грубое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5584b289-d5a9-35c1-986c-9ff9ae1bab88",
                    "sense_definition": "To provide or supply with a horse or horses.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_supply",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The regiment was well <w>horsed</w> for the upcoming campaign.",
                            "target_lang_translations": {
                                "ru": "Полк был хорошо <w>укомплектован лошадьми</w> для предстоящей кампании."
                            }
                        },
                        {
                            "text": "They needed to <w>horse</w> the carriage before the journey could begin.",
                            "target_lang_translations": {
                                "ru": "Им нужно было <w>запрячь</w> экипаж, прежде чем путешествие могло начаться."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабжать лошадьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снабжать лошадьми",
                                "target_lang_sense_clarification": "обеспечивать транспортом"
                            },
                            {
                                "target_lang_word": "запрягать",
                                "target_lang_sense_clarification": "готовить экипаж к поездке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7e4350b3-7f86-32b8-b42f-47e0cfcd3308",
                    "sense_definition": "The seventh animal in the 12-year cycle of the Chinese zodiac.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "zodiac",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "People born in the Year of the <w>Horse</w> are said to be energetic and independent.",
                            "target_lang_translations": {
                                "ru": "Говорят, что люди, рожденные в год <w>Лошади</w>, энергичны и независимы."
                            }
                        },
                        {
                            "text": "The <w>Horse</w> appears after the Snake in the Chinese zodiac cycle.",
                            "target_lang_translations": {
                                "ru": "<w>Лошадь</w> идет после Змеи в цикле китайского зодиака."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Year of the Horse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Седьмое животное в 12-летнем цикле китайского зодиака."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Лошадь",
                                "target_lang_sense_clarification": "знак зодиака"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8c840fdc-b548-37bd-892d-80ba08dc030e",
                    "sense_definition": "Pojazd na dwóch kołach napędzany siłą mięśni nóg przy pomocy pedałów.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bardzo lubię jeździć na <w>rowerze</w>.",
                            "target_lang_translations": {
                                "ru": "Я очень люблю кататься на <w>велосипеде</w>."
                            }
                        },
                        {
                            "text": "<w>Rower</w> ten ma stalową ramę, którą można złożyć na pół.",
                            "target_lang_translations": {
                                "ru": "У этого <w>велосипеда</w> стальная рама, которую можно сложить пополам."
                            }
                        }
                    ],
                    "synonyms": [
                        "dwa kółka",
                        "bicykl"
                    ],
                    "common_phrases": [
                        "jeździć na rowerze",
                        "wycieczka rowerowa",
                        "kask na rower",
                        "ścieżka rowerowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Транспортное средство на двух колесах, приводимое в движение силой мышц ног с помощью педалей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "велосипед",
                                "target_lang_sense_clarification": "транспортное средство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd93afa9-973c-399b-830b-d40c0604e20e",
                    "sense_definition": "Urządzenie do ćwiczeń w miejscu, które wygląda jak rower i służy do treningu fizycznego.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą często trenuję formę na <w>rowerze</w> stacjonarnym w domu.",
                            "target_lang_translations": {
                                "ru": "Зимой я часто поддерживаю форму на <w>велотренажёре</w> дома."
                            }
                        },
                        {
                            "text": "Na siłowni ten <w>rower</w> jest zawsze zajęty przez innych ćwiczących.",
                            "target_lang_translations": {
                                "ru": "В спортзале этот <w>велотренажёр</w> всегда занят другими посетителями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rower stacjonarny",
                        "rower treningowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство для тренировок в помещении, имитирующее езду на велосипеде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "велотренажёр",
                                "target_lang_sense_clarification": "спортивный тренажёр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba835ac1-b8ff-3017-8d77-c2725b4341c6",
                    "sense_definition": "Ćwiczenie gimnastyczne polegające na ruchach nóg przypominających pedałowanie, wykonywane w pozycji leżącej.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Instruktor kazał nam położyć się na plecach i robić <w>rower</w>.",
                            "target_lang_translations": {
                                "ru": "Инструктор велел нам лечь на спину и делать <w>велосипед</w>."
                            }
                        },
                        {
                            "text": "To ćwiczenie przypomina jazdę na <w>rowerze</w>, ale wykonuje się je w powietrzu.",
                            "target_lang_translations": {
                                "ru": "Это упражнение напоминает езду на <w>велосипеде</w>, но выполняется в воздухе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "robić rower",
                        "ćwiczenie rower"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гимнастическое упражнение, выполняемое лежа на спине и имитирующее вращение педалей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "велосипед",
                                "target_lang_sense_clarification": "упражнение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
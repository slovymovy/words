            "pos": "noun",
            "senses": [
                {
                    "sense_id": "433ef97d-be1e-327b-8ecf-c83f76b02248",
                    "sense_definition": "Osoba potrafiąca pięknie i przekonująco przemawiać; doskonały mówca.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "speaker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako wybitny <w>retor</w>, potrafił porwać tłumy swoim płomiennym wystąpieniem.",
                            "target_lang_translations": {
                                "ru": "Как выдающийся <w>оратор</w>, он умел увлечь толпу своим пламенным выступлением."
                            }
                        },
                        {
                            "text": "Nawet najzdolniejszy <w>retor</w> musi przygotować się do ważnej debaty.",
                            "target_lang_translations": {
                                "ru": "Даже самый способный <w>оратор</w> должен готовиться к важным дебатам."
                            }
                        }
                    ],
                    "synonyms": [
                        "krasomówca",
                        "orator"
                    ],
                    "common_phrases": [
                        "wybitny retor",
                        "zdolny retor",
                        "antyczny retor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, умеющий красиво и убедительно говорить; превосходный оратор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оратор",
                                "target_lang_sense_clarification": "красноречивый человек"
                            },
                            {
                                "target_lang_word": "ритор",
                                "target_lang_sense_clarification": "книжное, искусный оратор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c604b66-61f6-3436-93c3-b9c180ba76fb",
                    "sense_definition": "Nauczyciel wymowy i zasad krasomówstwa w dawnych szkołach.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "teacher",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starożytnym Rzymie <w>retor</w> był odpowiedzialny za wykształcenie przyszłych polityków.",
                            "target_lang_translations": {
                                "ru": "В Древнем Риме <w>ритор</w> отвечал за образование будущих политиков."
                            }
                        },
                        {
                            "text": "Młodzi uczniowie uważnie słuchali wykładów, które prowadził ich <w>retor</w>.",
                            "target_lang_translations": {
                                "ru": "Молодые ученики внимательно слушали лекции, которые вел их <w>ритор</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szkoła retora"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "odnosi się do funkcji historycznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Учитель красноречия и правил ораторского искусства в древних школах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ритор",
                                "target_lang_sense_clarification": "учитель риторики в древности"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                },
                {
                    "sense_id": "3ad285b5-32ed-3279-8b15-42fa9003f3d8",
                    "sense_definition": "To park a train or rail vehicle.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_rail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train was <w>stabled</w> at the depot overnight for maintenance.",
                            "target_lang_translations": {
                                "ru": "Поезд был <w>поставлен в депо</w> на ночь для технического обслуживания."
                            }
                        },
                        {
                            "text": "Drivers must ensure the locomotive is properly <w>stabled</w> before leaving.",
                            "target_lang_translations": {
                                "ru": "Машинисты должны убедиться, что локомотив правильно <w>поставлен в отстой</w> перед уходом."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "rail transport"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Парковать поезд или железнодорожный состав."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ставить в депо",
                                "target_lang_sense_clarification": "о поезде"
                            },
                            {
                                "target_lang_word": "ставить в отстой",
                                "target_lang_sense_clarification": "железнодорожный термин"
                            }
                        ]

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e23afb66-2339-3b73-bfad-f3c2025e22e6",
                    "sense_definition": "Een kunstmatige installatie die water omhoog spuit, vaak ter decoratie op pleinen of in parken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "water_feature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen spelen in de zomer graag bij de <w>fontein</w>.",
                            "target_lang_translations": {
                                "ru": "Летом дети любят играть у <w>фонтана</w>.",
                                "en": "In the summer, the children like to play by the <w>fountain</w>."
                            }
                        },
                        {
                            "text": "Midden op het marktplein staat een prachtige oude <w>fontein</w>.",
                            "target_lang_translations": {
                                "ru": "Посреди рыночной площади стоит красивый старинный <w>фонтан</w>.",
                                "en": "In the middle of the market square stands a beautiful old <w>fountain</w>."
                            }
                        },
                        {
                            "text": "Het geluid van de spuitende <w>fontein</w> werkt erg rustgevend.",
                            "target_lang_translations": {
                                "ru": "Звук бьющего <w>фонтана</w> очень успокаивает.",
                                "en": "The sound of the spraying <w>fountain</w> is very soothing."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een spuitende fontein",
                        "in de fontein",
                        "de fontein aanzetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственное сооружение, выбрасывающее струю воды вверх, часто служащее украшением на площадях или в парках.",
                        "en": "An artificial structure that sprays water upwards, often for decoration in squares or parks."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фонтан",
                                "target_lang_sense_clarification": "декоративное водное сооружение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fountain",
                                "target_lang_sense_clarification": "ornamental water feature"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc133eec-61ef-3418-b0ff-69195a86d7b9",
                    "sense_definition": "Een kleine wasbak met een kraan, meestal te vinden in een toiletruimte.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sanitary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het toiletbezoek waste hij zijn handen in de <w>fontein</w>.",
                            "target_lang_translations": {
                                "ru": "После посещения туалета он вымыл руки в <w>раковине</w>.",
                                "en": "After visiting the toilet, he washed his hands in the <w>washbasin</w>."
                            }
                        },
                        {
                            "text": "Er is in deze kleine ruimte nauwelijks plaats voor een <w>fontein</w>.",
                            "target_lang_translations": {
                                "ru": "В этом маленьком помещении едва хватает места для <w>рукомойника</w>.",
                                "en": "There is hardly any room for a <w>hand basin</w> in this small space."
                            }
                        }
                    ],
                    "synonyms": [
                        "wasbekken"
                    ],
                    "common_phrases": [
                        "een fontein plaatsen",
                        "koud water"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая раковина с краном, обычно устанавливаемая в туалетной комнате для мытья рук.",
                        "en": "A small sink with a tap, usually found in a toilet room or restroom."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раковина",
                                "target_lang_sense_clarification": "для мытья рук"
                            },
                            {
                                "target_lang_word": "рукомойник",
                                "target_lang_sense_clarification": "небольшой умывальник"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "washbasin",
                                "target_lang_sense_clarification": "for washing hands"
                            },
                            {
                                "target_lang_word": "hand basin",
                                "target_lang_sense_clarification": "small sink"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "343e2c1d-8abe-3a1c-84f8-fbf3d47fdf61",

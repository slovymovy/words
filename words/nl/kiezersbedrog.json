            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2849866e-96f9-344f-b3a1-580b2b80f892",
                    "sense_definition": "Het niet nakomen van beloften die tijdens de verkiezingscampagne zijn gedaan door een politicus of partij.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "political_deceit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oppositie beschuldigde de regering van <w>kiezersbedrog</w> toen de beloofde belastingverlaging niet doorging.",
                            "target_lang_translations": {
                                "en": "The opposition accused the government of <w>voter betrayal</w> when the promised tax cut did not go through."
                            }
                        },
                        {
                            "text": "Veel burgers voelden zich slachtoffer van <w>kiezersbedrog</w> na het zien van het nieuwe regeerakkoord.",
                            "target_lang_translations": {
                                "en": "Many citizens felt like victims of <w>voter betrayal</w> after seeing the new coalition agreement."
                            }
                        }
                    ],
                    "synonyms": [
                        "woordbreuk"
                    ],
                    "common_phrases": [
                        "van kiezersbedrog spreken",
                        "beschuldiging van kiezersbedrog"
                    ],
                    "target_lang_definitions": {
                        "en": "The failure to fulfill promises made during an election campaign by a politician or party."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "voter betrayal",
                                "target_lang_sense_clarification": "breaking campaign promises"
                            },
                            {
                                "target_lang_word": "breach of promise",
                                "target_lang_sense_clarification": "political context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c790e98-0751-319c-bdac-5591fb8557a9",
                    "sense_definition": "Het plegen van fraude of manipulatie tijdens het stemproces of bij het tellen van de stemmen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "election_fraud",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De internationale waarnemers vonden geen bewijs voor <w>kiezersbedrog</w> in de stembureaus.",
                            "target_lang_translations": {
                                "en": "The international observers found no evidence of <w>voter fraud</w> at the polling stations."
                            }
                        },
                        {
                            "text": "De verliezende partij claimde dat er sprake was van <w>kiezersbedrog</w> bij het tellen van de stemmen.",
                            "target_lang_translations": {
                                "en": "The losing party claimed that there was <w>election fraud</w> during the vote count."
                            }
                        }
                    ],
                    "synonyms": [

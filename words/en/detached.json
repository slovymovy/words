    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d8b5e33c-de5e-386d-ab45-11f5c4dace08",
                    "sense_definition": "Of a house: not joined to another house on either side.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They bought a large <w>detached</w> house with a big garden.",
                            "target_lang_translations": {
                                "ru": "Они купили большой <w>отдельно стоящий</w> дом с большим садом."
                            }
                        },
                        {
                            "text": "Living in a <w>detached</w> property offers much more privacy than an apartment.",
                            "target_lang_translations": {
                                "ru": "Проживание в <w>отдельном</w> доме обеспечивает гораздо больше уединения, чем в квартире."
                            }
                        }
                    ],
                    "synonyms": [
                        "separate"
                    ],
                    "antonyms": [
                        "attached"
                    ],
                    "common_phrases": [
                        "detached house",
                        "semi-detached"
                    ],
                    "target_lang_definitions": {
                        "ru": "О доме: не имеющий общих стен с другими домами; стоящий отдельно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдельно стоящий",
                                "target_lang_sense_clarification": "о здании"
                            },
                            {
                                "target_lang_word": "обособленный",
                                "target_lang_sense_clarification": "изолированный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f608598f-6a17-3922-b509-d9f4d2da995f",
                    "sense_definition": "Not physically attached; separated from something to which it could connect.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The garage is <w>detached</w> from the main building.",
                            "target_lang_translations": {
                                "ru": "Гараж <w>отделен</w> от главного здания."
                            }
                        },
                        {
                            "text": "He had to undergo surgery to repair a <w>detached</w> retina.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось перенести операцию по восстановлению <w>отслоившейся</w> сетчатки."
                            }
                        }
                    ],
                    "synonyms": [
                        "separate",
                        "unattached",
                        "loose",
                        "unconnected"
                    ],
                    "antonyms": [
                        "attached",
                        "connected"
                    ],
                    "common_phrases": [
                        "detached garage",
                        "detached retina"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физически не прикрепленный; отделенный от чего-то, с чем мог бы быть соединен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдельный",
                                "target_lang_sense_clarification": "не соединенный"
                            },
                            {
                                "target_lang_word": "отслоившийся",
                                "target_lang_sense_clarification": "медицинский термин (о сетчатке)"

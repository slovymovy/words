    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0a1c4dcf-5ed6-3986-b6f9-170067fc4d61",
                    "sense_definition": "Relating to the sciatic nerve or the pain caused by its compression (sciatica).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "sciatic_medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>sciatic</w> nerve is the largest and longest nerve in the human body.",
                            "target_lang_translations": {
                                "ru": "<w>Седалищный</w> нерв — самый крупный и длинный нерв в человеческом теле."
                            }
                        },
                        {
                            "text": "She visited the doctor because of severe <w>sciatic</w> pain in her lower back.",
                            "target_lang_translations": {
                                "ru": "Она обратилась к врачу из-за сильной <w>седалищной</w> боли в пояснице."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sciatic nerve",
                        "sciatic pain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к седалищному нерву или боли, вызванной его защемлением (ишиасу)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "седалищный",
                                "target_lang_sense_clarification": "относящийся к нерву"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bc2de7b-89a7-33cf-8b08-592fa0325173",
                    "sense_definition": "Relating to the ischium, which is the lower and back part of the hip bone.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sciatic_anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>sciatic</w> notch allows major nerves and vessels to pass through the pelvis.",
                            "target_lang_translations": {
                                "ru": "<w>Седалищная</w> вырезка позволяет крупным нервам и сосудам проходить через таз."
                            }
                        },
                        {
                            "text": "Anatomy students must learn to identify the <w>sciatic</w> spine on the bone model.",
                            "target_lang_translations": {
                                "ru": "Студенты-анатомы должны научиться находить <w>седалищную</w> ость на модели кости."
                            }
                        }
                    ],
                    "synonyms": [
                        "ischial"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к седалищной кости (нижней задней части тазовой кости)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "седалищный",
                                "target_lang_sense_clarification": "относящийся к кости"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c4a294a1-f850-32ef-a610-9a7a8dbd84ff",
                    "sense_definition": "The sciatic nerve.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sciatic_medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Damage to the <w>sciatic</w> can cause numbness and weakness in the leg.",
                            "target_lang_translations": {
                                "ru": "Повреждение <w>седалищного нерва</w> может вызвать онемение и слабость в ноге."
                            }
                        },
                        {
                            "text": "The surgeon carefully exposed the <w>sciatic</w> to avoid injuring it during the operation.",
                            "target_lang_translations": {
                                "ru": "Хирург осторожно обнажил <w>седалищный нерв</w>, чтобы не повредить его во время операции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "ellipsis"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Седалищный нерв."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "седалищный нерв",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11b5a56e-0466-3f81-98e7-acadd81cc24b",
                    "sense_definition": "A person suffering from sciatica.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The clinic offers a specialized exercise program for <w>sciatics</w>.",
                            "target_lang_translations": {
                                "ru": "Клиника предлагает специализированную программу упражнений для <w>больных ишиасом</w>."
                            }
                        },
                        {
                            "text": "Many <w>sciatics</w> find that stretching helps alleviate their symptoms.",
                            "target_lang_translations": {
                                "ru": "Многие <w>страдающие ишиасом</w> находят, что растяжка помогает облегчить симптомы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, страдающий ишиасом (воспалением седалищного нерва)."
                    },
                    "translations": {

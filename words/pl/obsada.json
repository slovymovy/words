                {
                    "sense_id": "194155cc-5330-3d2c-b511-e13c78e4d117",
                    "sense_definition": "Zespół aktorów biorących udział w filmie, sztuce teatralnej lub innym widowisku.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "actors_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten film ma gwiazdorską <w>obsadę</w>, co przyciąga do kin tłumy widzów.",
                            "target_lang_translations": {
                                "ru": "У этого фильма звёздный <w>актёрский состав</w>, что привлекает в кинотеатры толпы зрителей."
                            }
                        },
                        {
                            "text": "Reżyser skompletował już pełną <w>obsadę</w> do swojego najnowszego spektaklu.",
                            "target_lang_translations": {
                                "ru": "Режиссёр уже набрал полный <w>состав исполнителей</w> для своего нового спектакля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gwiazdorska obsada",
                        "obsada aktorska",
                        "zmiana obsady",
                        "w doborowej obsadzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа актёров, участвующих в фильме, пьесе или другом представлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "актёрский состав",
                                "target_lang_sense_clarification": "группа актёров в постановке или фильме"
                            },
                            {
                                "target_lang_word": "состав исполнителей",
                                "target_lang_sense_clarification": "список участников представления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d583d063-57f2-3761-8eda-724a7542a99c",
                    "sense_definition": "Ogół osób zatrudnionych na określonych stanowiskach lub czynność przydzielania tych stanowisk.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "staffing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szpital boryka się z problemami w <w>obsadzie</w> dyżurów lekarskich w okresie świątecznym.",
                            "target_lang_translations": {
                                "ru": "Больница сталкивается с проблемами в <w>укомплектовании</w> врачебных дежурств в праздничный период."
                            }
                        },
                        {
                            "text": "Nowa <w>obsada</w> personalna urzędu została ogłoszona w oficjalnym komunikacie.",
                            "target_lang_translations": {
                                "ru": "Новый <w>кадровый состав</w> ведомства был объявлен в официальном сообщении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "obsada personalna",
                        "obsada stanowisk",
                        "pełna obsada",
                        "braki w obsadzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность сотрудников, занимающих определённые должности, или процесс их назначения."
                    },
                    "translations": {
                        "ru": [
                            {

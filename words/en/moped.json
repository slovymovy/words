{
    "word_family": [
        "mope",
        "moper",
        "mopish",
        "mopedist"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ff7f7763-69ce-34c0-aab5-25009da116ed",
                    "sense_definition": "A lightweight, two-wheeled vehicle equipped with a small motor and pedals, designed to go no faster than some specified speed limit.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He rides his <w>moped</w> to work to save money on gas.",
                            "target_lang_translations": {
                                "ru": "Он ездит на работу на <w>мопеде</w>, чтобы сэкономить на бензине."
                            }
                        },
                        {
                            "text": "We decided to rent a <w>moped</w> to explore the small island.",
                            "target_lang_translations": {
                                "ru": "Мы решили арендовать <w>мопед</w>, чтобы осмотреть маленький остров."
                            }
                        }
                    ],
                    "synonyms": [
                        "scooter",
                        "motorbike"
                    ],
                    "common_phrases": [
                        "ride a moped",
                        "rent a moped",
                        "electric moped"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкое двухколесное транспортное средство с небольшим мотором и педалями, скорость которого обычно ограничена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мопед",
                                "target_lang_sense_clarification": "транспортное средство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fc14a88c-4194-3809-bdef-add0eb1549c6",
                    "sense_definition": "The simple past and past participle of <w>mope</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sadness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>moped</w> around the house for days after hearing the bad news.",
                            "target_lang_translations": {
                                "ru": "Она целыми днями <w>хандрила</w> дома, узнав плохие новости."
                            }
                        },
                        {
                            "text": "He <w>moped</w> in his room instead of joining the celebration.",
                            "target_lang_translations": {
                                "ru": "Он <w>унывал</w> в своей комнате вместо того, чтобы присоединиться к празднику."
                            }
                        }
                    ],
                    "synonyms": [
                        "sulked",
                        "brooded"
                    ],
                    "antonyms": [
                        "rejoiced"
                    ],
                    "common_phrases": [
                        "moped around",
                        "moped about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола \"mope\" (хандрить, унывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хандрить",
                                "target_lang_sense_clarification": "быть в унынии"
                            },
                            {
                                "target_lang_word": "унывать",
                                "target_lang_sense_clarification": "падать духом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9582d037-1380-3060-a867-c0a6b3fb7ee5",
                    "sense_definition": "Feeling sad, low in spirits, or lacking enthusiasm.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sadness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He sat in the corner looking <w>moped</w> and miserable.",
                            "target_lang_translations": {
                                "ru": "Он сидел в углу, выглядя <w>подавленным</w> и несчастным."
                            }
                        },
                        {
                            "text": "The <w>moped</w> expression on her face made me worry about her.",
                            "target_lang_translations": {
                                "ru": "<w>Унылое</w> выражение её лица заставило меня беспокоиться о ней."
                            }
                        }
                    ],
                    "synonyms": [
                        "dejected",
                        "melancholic",
                        "glum",
                        "downcast"
                    ],
                    "antonyms": [
                        "cheerful",
                        "happy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий грусть, упадок духа или отсутствие энтузиазма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подавленный",
                                "target_lang_sense_clarification": "угнетенный"
                            },
                            {
                                "target_lang_word": "унылый",
                                "target_lang_sense_clarification": "грустный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
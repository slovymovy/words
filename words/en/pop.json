                },
                {
                    "sense_id": "c890854d-abc0-3056-a1e3-00420842ee4b",
                    "sense_definition": "To stand out brightly or distinctly against a background.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "visual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bright blue sofa really makes the living room <w>pop</w>.",
                            "target_lang_translations": {
                                "ru": "Ярко-синий диван действительно <w>оживляет</w> гостиную."
                            }
                        },
                        {
                            "text": "Use a dark border to make the white text <w>pop</w>.",
                            "target_lang_translations": {
                                "ru": "Используйте темную рамку, чтобы белый текст <w>выделялся</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ярко выделяться на фоне чего-либо."

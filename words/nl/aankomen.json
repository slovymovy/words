                            {
                                "target_lang_word": "прикасаться",
                                "target_lang_sense_clarification": "вступать в физический контакт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4f425ef-b7a5-3860-aa9b-6bfd079475ba",
                    "sense_definition": "Dichterbij komen in tijd of ruimte; iets zien of horen naderen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zag de auto helemaal niet <w>aankomen</w>.",
                            "target_lang_translations": {
                                "en": "I didn't see the car <w>coming</w> at all.",
                                "ru": "Я совсем не видел, как <w>приближалась</w> машина."
                            }
                        },
                        {
                            "text": "Ze voelde de griep al dagen <w>aankomen</w>.",
                            "target_lang_translations": {
                                "en": "She felt the flu <w>coming on</w> for days.",
                                "ru": "Она чувствовала, что грипп <w>надвигается</w>, уже несколько дней."
                            }
                        }
                    ],
                    "synonyms": [
                        "naderen"
                    ],
                    "common_phrases": [
                        "zien aankomen",
                        "horen aankomen",
                        "voelen aankomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To come closer in time or space; to see or hear something approaching.",
                        "ru": "Приближаться во времени или пространстве; видеть или слышать приближение чего-либо."
                    },
                    "translations": {
                        "en": [

                },
                {
                    "sense_id": "96dc6540-2831-3587-8d9d-0147ff31da79",
                    "sense_definition": "Enkelvoud tegenwoordige tijd van <w>sloten</w>",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer <w>sloot</w> de greppels om de afwatering te verbeteren.",
                            "target_lang_translations": {
                                "ru": "Фермер <w>рыл</w> канавы, чтобы улучшить водоотвод."
                            }
                        },
                        {
                            "text": "Hij <w>sloot</w> elke herfst de watergangen rondom zijn akkers.",
                            "target_lang_translations": {
                                "ru": "Каждую осень он <w>чистил</w> водотоки вокруг своих полей."
                            }

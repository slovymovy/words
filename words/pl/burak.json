    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6fd27ee6-fd2d-345e-8568-b78bff875479",
                    "sense_definition": "Warzywo o jadalnym korzeniu, zazwyczaj ciemnoczerwonym, używane do przygotowywania zup, sałatek i przetworów.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "vegetable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na dzisiejszy obiad ugotowałam pyszną zupę z <w>buraków</w>.",
                            "target_lang_translations": {
                                "ru": "На сегодняшний обед я приготовила вкусный суп из <w>свёклы</w>."
                            }
                        },
                        {
                            "text": "Czerwony <w>burak</w> jest bardzo zdrowym warzywem pełnym witamin.",
                            "target_lang_translations": {
                                "ru": "Красная <w>свёкла</w> — очень полезный овощ, полный витаминов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czerwony jak burak",
                        "barszcz z buraków",
                        "sałatka z buraków",
                        "cukier z buraków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Овощ со съедобным корнем, обычно темно-красного цвета, используемый для приготовления супов, салатов и заготовок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свёкла",
                                "target_lang_sense_clarification": "овощ"
                            },
                            {
                                "target_lang_word": "бурак",
                                "target_lang_sense_clarification": "региональное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc65ae2b-0ee3-30be-ba15-7fe6c8529cf9",
                    "sense_definition": "Potoczne, obraźliwe określenie osoby niekulturalnej, zachowującej się prostacko, arogancko lub niestosownie do sytuacji.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź takim <w>burakiem</w> i przeproś ją za swoje zachowanie.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>хамом</w> и извинись перед ней за своё поведение."
                            }
                        },
                        {
                            "text": "Ten kierowca to zwykły <w>burak</w>, zajechał mi drogę i jeszcze krzyczał.",
                            "target_lang_translations": {
                                "ru": "Этот водитель — обычный <w>жлоб</w>, подрезал меня и ещё кричал."
                            }
                        }
                    ],
                    "synonyms": [
                        "cham",
                        "prostak",
                        "buc",
                        "prymityw"
                    ],
                    "common_phrases": [
                        "zachować się jak burak",
                        "wyjść na buraka",
                        "straszny burak"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo o wydźwięku pejoratywnym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, оскорбительное название бескультурного человека, ведущего себя грубо, высокомерно или неуместно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хам",
                                "target_lang_sense_clarification": "грубиян"
                            },
                            {
                                "target_lang_word": "жлоб",
                                "target_lang_sense_clarification": "некультурный человек"
                            },
                            {
                                "target_lang_word": "деревенщина",
                                "target_lang_sense_clarification": "простак, невежа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2dd2929-ea2a-3b10-b59d-48308b7a31cc",
                    "sense_definition": "Tanie wino owocowe bardzo niskiej jakości.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "alcohol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod sklepem stała grupa mężczyzn pijących tanie <w>buraki</w>.",
                            "target_lang_translations": {
                                "ru": "У магазина стояла группа мужчин, пьющих дешёвую <w>бормотуху</w>."
                            }
                        },
                        {
                            "text": "To wino smakuje okropnie, jak jakiś <w>burak</w>.",
                            "target_lang_translations": {
                                "ru": "Это вино ужасное на вкус, как какой-то <w>шмурдяк</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "określenie środowiskowe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дешёвое плодово-ягодное вино очень низкого качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бормотуха",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "шмурдяк",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c64db12f-2a20-3a5c-be17-c9f35c24b11e",
                    "sense_definition": "Nazwisko używane w Polsce.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Burak</w> jest naszym nowym sąsiadem z parteru.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Бурак</w> — наш новый сосед с первого этажа."
                            }
                        },
                        {
                            "text": "Nigdy wcześniej nie słyszałem o rodzinie państwa <w>Buraków</w>.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не слышал о семье <w>Бураков</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "nazwisko męskie i żeńskie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, встречающаяся в Польше."
                    },
                    "translations": {

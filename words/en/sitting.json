            "senses": [
                {
                    "sense_id": "fba7232b-22b8-3232-88c5-7dcc39b1f517",
                    "sense_definition": "The present participle and gerund of the verb <w>sit</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>sitting</w> on the chair near the window.",
                            "target_lang_translations": {
                                "ru": "Она <w>сидит</w> на стуле у окна."
                            }
                        },
                        {
                            "text": "We saw the cat <w>sitting</w> on the fence.",
                            "target_lang_translations": {
                                "ru": "Мы видели кошку, <w>сидящую</w> на заборе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола sit."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидящий",
                                "target_lang_sense_clarification": "причастие"
                            },
                            {
                                "target_lang_word": "сидение",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2909888b-d8c8-3255-abff-42d7924588ce",
                    "sense_definition": "Currently holding a specific official office or legal position.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>sitting</w> president has announced plans to run for a second term.",
                            "target_lang_translations": {
                                "ru": "<w>Действующий</w> президент объявил о планах баллотироваться на второй срок."
                            }
                        },
                        {
                            "text": "It is often difficult to defeat a <w>sitting</w> member of parliament in an election.",
                            "target_lang_translations": {
                                "ru": "Часто бывает трудно победить <w>действующего</w> члена парламента на выборах."
                            }
                        }
                    ],
                    "synonyms": [
                        "incumbent",
                        "current"
                    ],
                    "antonyms": [
                        "former",
                        "past"
                    ],
                    "common_phrases": [
                        "sitting president",
                        "sitting tenant",
                        "sitting MP"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занимающий официальную должность в данный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "действующий",
                                "target_lang_sense_clarification": "о должностном лице"
                            },
                            {
                                "target_lang_word": "нынешний",
                                "target_lang_sense_clarification": "текущий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e54abd5-aa22-3cb9-8463-12acc9c57e65",
                    "sense_definition": "Performed or executed while in a seated position.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor recommended <w>sitting</w> exercises to avoid strain on the back.",
                            "target_lang_translations": {
                                "ru": "Врач порекомендовал упражнения <w>сидя</w>, чтобы избежать нагрузки на спину."
                            }
                        },
                        {
                            "text": "He participates in <w>sitting</w> volleyball competitions.",
                            "target_lang_translations": {
                                "ru": "Он участвует в соревнованиях по волейболу <w>сидя</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "seated"
                    ],
                    "antonyms": [
                        "standing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполняемый в положении сидя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидячий",
                                "target_lang_sense_clarification": "об образе жизни, работе"
                            },
                            {
                                "target_lang_word": "сидя",
                                "target_lang_sense_clarification": "в положении сидя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5b37c0e6-d790-3cf8-89b5-98cadbe8434c",
                    "sense_definition": "A continuous period of time spent doing an activity without stopping.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "period_seated",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The novel was so gripping that I read it all in one <w>sitting</w>.",
                            "target_lang_translations": {
                                "ru": "Роман был таким захватывающим, что я прочитал его за один <w>присест</w>."
                            }
                        },
                        {
                            "text": "He managed to finish the entire project in a single <w>sitting</w>.",
                            "target_lang_translations": {
                                "ru": "Ему удалось закончить весь проект за один <w>присест</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "session",
                        "stretch"
                    ],
                    "common_phrases": [
                        "in one sitting",
                        "at a sitting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непрерывный период времени, потраченный на какое-либо занятие без перерыва."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присест",
                                "target_lang_sense_clarification": "разг., обычно в выражении \"в один присест\""
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee10669a-df2f-3bbd-a139-f6a191fd6490",
                    "sense_definition": "A specific period of time during which a person is seated, often for a meal or to have a portrait painted.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "period_seated",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The restaurant is very busy and has two <w>sittings</w> for dinner each evening.",
                            "target_lang_translations": {
                                "ru": "Ресторан очень загружен, и каждый вечер там бывает две <w>смены</w> на ужин."
                            }
                        },
                        {
                            "text": "The artist required five <w>sittings</w> to complete the realistic portrait.",
                            "target_lang_translations": {
                                "ru": "Художнику потребовалось пять <w>сеансов</w>, чтобы закончить реалистичный портрет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "first sitting",
                        "portrait sitting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени, отведенный для приема пищи или позирования художнику."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сеанс",
                                "target_lang_sense_clarification": "позирования"
                            },
                            {
                                "target_lang_word": "смена",
                                "target_lang_sense_clarification": "в ресторане"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eafcc06a-1f0c-3bbf-9ba7-8ac0fb24e39f",
                    "sense_definition": "A formal meeting or session of a legislature, court, or other official body.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "official_session",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Parliament held a late-night <w>sitting</w> to debate the urgent bill.",
                            "target_lang_translations": {
                                "ru": "Парламент провел ночное <w>заседание</w> для обсуждения срочного законопроекта."
                            }
                        },
                        {
                            "text": "The court is currently in a <w>sitting</w> and cannot be disturbed.",
                            "target_lang_translations": {
                                "ru": "Суд в данный момент находится на <w>заседании</w>, и его нельзя беспокоить."
                            }
                        }
                    ],
                    "synonyms": [
                        "session",
                        "meeting"
                    ],
                    "antonyms": [
                        "recess"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное собрание или сессия законодательного органа, суда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заседание",
                                "target_lang_sense_clarification": "суда, парламента"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afc019e2-0f9c-3570-9908-5b21e63990d3",
                    "sense_definition": "A meeting or session with a spiritual medium or fortuneteller.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She paid a fee for a private <w>sitting</w> with the famous medium.",
                            "target_lang_translations": {
                                "ru": "Она заплатила за частный <w>сеанс</w> с известным медиумом."
                            }
                        },
                        {
                            "text": "During the <w>sitting</w>, the psychic claimed to hear voices from the past.",
                            "target_lang_translations": {
                                "ru": "Во время <w>сеанса</w> экстрасенс утверждал, что слышит голоса из прошлого."
                            }
                        }
                    ],
                    "synonyms": [
                        "seance",
                        "reading"
                    ],
                    "target_lang_definitions": {
                        "ru": "Встреча или сеанс со спиритом или гадалкой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сеанс",
                                "target_lang_sense_clarification": "спиритический"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
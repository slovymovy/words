    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8f88a8d4-8b71-31ee-bc0b-7250263b0557",
                    "sense_definition": "The state of having complete legal control of something; possession; proprietorship.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The document proves his legal <w>ownership</w> of the property.",
                            "target_lang_translations": {
                                "ru": "Этот документ подтверждает его законное <w>право собственности</w> на это имущество."
                            }
                        },
                        {
                            "text": "They decided to transfer <w>ownership</w> of the family business to their children.",
                            "target_lang_translations": {
                                "ru": "Они решили передать <w>право собственности</w> на семейный бизнес своим детям."
                            }
                        }
                    ],
                    "synonyms": [
                        "possession",
                        "proprietorship"
                    ],
                    "antonyms": [
                        "nonownership",
                        "nonpossession"
                    ],
                    "common_phrases": [
                        "transfer ownership",
                        "private ownership",
                        "joint ownership",
                        "claim ownership"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние полного юридического контроля над чем-либо; владение; право собственности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "со́бственность",
                                "target_lang_sense_clarification": "принадлежность имущества"
                            },
                            {
                                "target_lang_word": "пра́во со́бственности",
                                "target_lang_sense_clarification": "юридическое право"
                            },
                            {
                                "target_lang_word": "владе́ние",
                                "target_lang_sense_clarification": "факт обладания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7814528-4bfc-3036-aec5-a874cf1ffdba",
                    "sense_definition": "Responsibility for or control over something, often regarding a task, project, or problem.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The successful candidate will take <w>ownership</w> of all internal design projects.",
                            "target_lang_translations": {
                                "ru": "Успешный кандидат возьмет на себя <w>ответственность</w> за все внутренние дизайн-проекты."
                            }
                        },
                        {
                            "text": "Employees are encouraged to take <w>ownership</w> of their mistakes and learn from them.",
                            "target_lang_translations": {
                                "ru": "Сотрудников призывают брать на себя <w>ответственность</w> за свои ошибки и учиться на них."
                            }
                        }
                    ],
                    "common_phrases": [
                        "take ownership",
                        "sense of ownership"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ответственность за что-либо или контроль над чем-либо, часто в отношении задачи, проекта или проблемы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отве́тственность",
                                "target_lang_sense_clarification": "личная вовлеченность и обязательства"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
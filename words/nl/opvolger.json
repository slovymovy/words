{
    "word_family": [
        "opvolgen",
        "opvolging",
        "opvolgster"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "16eb23d7-660b-393d-88f6-240ed4984931",
                    "sense_definition": "Iemand die een functie, ambt of taak overneemt van een ander.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "successor_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur stelde gisteren zijn <w>opvolger</w> voor aan het personeel.",
                            "target_lang_translations": {
                                "en": "The director introduced his <w>successor</w> to the staff yesterday.",
                                "ru": "Вчера директор представил сотрудникам своего <w>преемника</w>."
                            }
                        },
                        {
                            "text": "Zij is de beoogde <w>opvolger</w> van de huidige voorzitter.",
                            "target_lang_translations": {
                                "en": "She is the intended <w>successor</w> to the current chairman.",
                                "ru": "Она — предполагаемая <w>преемница</w> нынешнего председателя."
                            }
                        }
                    ],
                    "antonyms": [
                        "voorganger"
                    ],
                    "common_phrases": [
                        "de beoogde opvolger",
                        "een waardige opvolger",
                        "zijn opvolger aanwijzen"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who takes over a position, office, or task from someone else.",
                        "ru": "Тот, кто перенимает должность, пост или задачу у другого."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "successor",
                                "target_lang_sense_clarification": "person taking over a role"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "преемник",
                                "target_lang_sense_clarification": "человек, занимающий чье-то место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "680da5f3-4528-3cc8-999a-467e77ad76c8",
                    "sense_definition": "Iets dat na een eerdere versie komt en deze vervangt of voortzet.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "successor_thing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze nieuwe telefoon is de <w>opvolger</w> van het populaire model uit 2020.",
                            "target_lang_translations": {
                                "en": "This new phone is the <w>successor</w> to the popular model from 2020.",
                                "ru": "Этот новый телефон — <w>преемник</w> популярной модели 2020 года."
                            }
                        },
                        {
                            "text": "Het boek is een waardige <w>opvolger</w> van zijn succesvolle debuutroman.",
                            "target_lang_translations": {
                                "en": "The book is a worthy <w>successor</w> to his successful debut novel.",
                                "ru": "Эта книга — достойный <w>преемник</w> его успешного дебютного романа."
                            }
                        }
                    ],
                    "antonyms": [
                        "voorganger"
                    ],
                    "common_phrases": [
                        "de logische opvolger",
                        "de spirituele opvolger"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that comes after an earlier version and replaces or continues it.",
                        "ru": "То, что следует за предыдущей версией и заменяет или продолжает её."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "successor",
                                "target_lang_sense_clarification": "product or work following another"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "преемник",
                                "target_lang_sense_clarification": "продукт или работа, следующие за другими"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
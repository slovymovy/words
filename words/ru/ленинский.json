    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "955e2dab-f71a-3150-b00d-a58985a7ffca",
                    "sense_definition": "Относящийся к Владимиру Ленину, принадлежащий или свойственный ему.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В музее мы увидели личный <w>ленинский</w> автомобиль.",
                            "target_lang_translations": {
                                "en": "In the museum, we saw <w>Lenin's</w> personal car."
                            }
                        },
                        {
                            "text": "Мнение вождя часто считалось правильным просто потому, что оно — <w>ленинское</w>.",
                            "target_lang_translations": {
                                "en": "The leader's opinion was often considered correct simply because it was <w>Lenin's</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ленинская премия",
                        "ленинский субботник",
                        "ленинская комната"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to Vladimir Lenin, belonging to or characteristic of him."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Lenin's",
                                "target_lang_sense_clarification": "possessive or relational"
                            },
                            {
                                "target_lang_word": "Lenin",
                                "target_lang_sense_clarification": "attributive use in phrases"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b9a0a36-7e54-3fc1-99a1-3d7a7b4c19ea",
                    "sense_definition": "Основанный на идеях Владимира Ленина, соответствующий принципам ленинизма.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "ideological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Партия торжественно обещала продолжать <w>ленинский</w> курс.",
                            "target_lang_translations": {
                                "en": "The party solemnly promised to continue the <w>Leninist</w> course."
                            }
                        },
                        {
                            "text": "Этот подход к экономике был охарактеризован как истинно <w>ленинский</w>.",
                            "target_lang_translations": {
                                "en": "This approach to the economy was characterized as truly <w>Leninist</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ленинистский"
                    ],
                    "antonyms": [
                        "антиленинский"
                    ],
                    "common_phrases": [
                        "ленинский завет",
                        "ленинский принцип",
                        "ленинский стиль"
                    ],
                    "target_lang_definitions": {
                        "en": "Based on the ideas of Vladimir Lenin, corresponding to the principles of Leninism."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Leninist",
                                "target_lang_sense_clarification": "ideological"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "9de21da5-0aa7-3f57-8974-e6a10005f278",
                    "sense_definition": "Название различных населённых пунктов (посёлков, районов) в России и странах бывшего СССР.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Моя бабушка живёт в небольшом посёлке <w>Ленинский</w>.",
                            "target_lang_translations": {
                                "en": "My grandmother lives in the small settlement of <w>Leninsky</w>."
                            }
                        },
                        {
                            "text": "Автобус отправляется в <w>Ленинский</w> через час.",
                            "target_lang_translations": {
                                "en": "The bus leaves for <w>Leninsky</w> in an hour."
                            }
                        }
                    ],
                    "common_phrases": [
                        "посёлок Ленинский",
                        "Ленинский район",
                        "Ленинский проспект"
                    ],
                    "target_lang_definitions": {
                        "en": "Name of various settlements (villages, districts) in Russia and countries of the former USSR."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Leninsky",
                                "target_lang_sense_clarification": "place name"

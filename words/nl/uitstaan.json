{
    "word_family": [
        "uitstand",
        "uitstaand"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8fc0c2a2-d5ad-3341-80b0-281270d9d5f0",
                    "sense_definition": "Niet in werking zijn of uitgeschakeld zijn (van apparaten of machines)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_off",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn telefoon <w>staat</w> 's nachts altijd <w>uit</w>.",
                            "target_lang_translations": {
                                "en": "My phone is always <w>off</w> at night.",
                                "ru": "Мой телефон по ночам всегда <w>выключен</w>."
                            }
                        },
                        {
                            "text": "Zorg ervoor dat de televisie <w>uitstaat</w> voordat je naar bed gaat.",
                            "target_lang_translations": {
                                "en": "Make sure the television is <w>turned off</w> before you go to bed.",
                                "ru": "Убедись, что телевизор <w>выключен</w>, перед тем как лечь спать."
                            }
                        }
                    ],
                    "antonyms": [
                        "aanstaan"
                    ],
                    "common_phrases": [
                        "de tv staat uit",
                        "het licht staat uit"
                    ],
                    "target_lang_definitions": {
                        "en": "To be switched off or not in operation (of devices or machines)",
                        "ru": "Быть выключенным или не функционировать (об устройствах или машинах)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be off",
                                "target_lang_sense_clarification": "state of a device"
                            },
                            {
                                "target_lang_word": "be turned off",
                                "target_lang_sense_clarification": "state of being switched off"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть выключенным",
                                "target_lang_sense_clarification": "состояние прибора"
                            },
                            {
                                "target_lang_word": "не работать",
                                "target_lang_sense_clarification": "быть отключенным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46e8e490-8f9b-3faa-a2a7-105d5c1754af",
                    "sense_definition": "Iemand of iets verdragen of dulden (vrijwel altijd gebruikt in combinatie met 'niet kunnen')",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tolerate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan zijn arrogante houding echt niet <w>uitstaan</w>.",
                            "target_lang_translations": {
                                "en": "I really cannot <w>stand</w> his arrogant attitude.",
                                "ru": "Я действительно не могу <w>терпеть</w> его высокомерное поведение."
                            }
                        },
                        {
                            "text": "Zij heeft in haar leven veel pijn en verdriet moeten <w>uitstaan</w>.",
                            "target_lang_translations": {
                                "en": "She has had to <w>endure</w> a lot of pain and sorrow in her life.",
                                "ru": "Ей пришлось <w>перенести</w> много боли и горя в своей жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdragen",
                        "uithouden",
                        "velen",
                        "harden",
                        "ondergaan"
                    ],
                    "common_phrases": [
                        "niet kunnen uitstaan",
                        "pijn uitstaan",
                        "angst uitstaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To tolerate or endure someone or something (often used with 'cannot')",
                        "ru": "Выносить или терпеть кого-то или что-то (почти всегда используется с «не мочь»)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand",
                                "target_lang_sense_clarification": "tolerate (usually negative)"
                            },
                            {
                                "target_lang_word": "endure",
                                "target_lang_sense_clarification": "suffer through pain or hardship"
                            },
                            {
                                "target_lang_word": "bear",
                                "target_lang_sense_clarification": "tolerate or withstand"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выносить",
                                "target_lang_sense_clarification": "терпеть (обычно с отрицанием)"
                            },
                            {
                                "target_lang_word": "терпеть",
                                "target_lang_sense_clarification": "мириться с чем-то"
                            },
                            {
                                "target_lang_word": "переносить",
                                "target_lang_sense_clarification": "испытывать страдания или трудности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4de3ffd-425a-3366-a216-6ee3c24c0753",
                    "sense_definition": "Nog betaald of afgehandeld moeten worden (van geldbedragen of vorderingen)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er <w>staat</w> nog een groot bedrag <w>uit</w> bij die klant.",
                            "target_lang_translations": {
                                "en": "A large amount is still <w>outstanding</w> with that customer.",
                                "ru": "За этим клиентом всё ещё <w>числится</w> большая сумма."
                            }
                        },
                        {
                            "text": "De factuur die nog <w>uitstond</w>, is gisteren eindelijk betaald.",
                            "target_lang_translations": {
                                "en": "The invoice that was still <w>outstanding</w> was finally paid yesterday.",
                                "ru": "Счет, который всё ещё <w>оставался неоплаченным</w>, вчера наконец был оплачен."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een rekening staat uit",
                        "uitstaande schulden"
                    ],
                    "target_lang_definitions": {
                        "en": "To remain unpaid or unsettled (of debts, invoices, or claims)",
                        "ru": "Ожидать оплаты или урегулирования (о денежных суммах или требованиях)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be outstanding",
                                "target_lang_sense_clarification": "financial/unpaid"
                            },
                            {
                                "target_lang_word": "be due",
                                "target_lang_sense_clarification": "owed"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть неоплаченным",
                                "target_lang_sense_clarification": "о счетах"
                            },
                            {
                                "target_lang_word": "числиться",
                                "target_lang_sense_clarification": "о долге (быть записанным за кем-то)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
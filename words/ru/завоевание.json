    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3bab7fa7-422c-3514-8458-dc99cbc620ed",
                    "sense_definition": "Действие по значению глагола <w>завоёвывать</w>; покорение чужой территории силой или обретение чего-либо в результате борьбы и усилий.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Император планировал быстрое <завоевание> соседних земель.",
                            "target_lang_translations": {
                                "en": "The emperor planned a quick <conquest> of the neighboring lands."
                            }
                        },
                        {
                            "text": "Для нашей компании <завоевание> рынка является приоритетной задачей.",
                            "target_lang_translations": {
                                "en": "For our company, the <conquest> of the market is a priority task."
                            }
                        }
                    ],
                    "synonyms": [
                        "захват",
                        "покорение"
                    ],
                    "antonyms": [
                        "потеря",
                        "утрата"
                    ],
                    "common_phrases": [
                        "завоевание мира",
                        "завоевание доверия",
                        "завоевание рынка",
                        "история завоеваний"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of taking control of a country, city, etc., through the use of force, or the process of gaining something through effort and struggle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conquest",
                                "target_lang_sense_clarification": "act of taking control of territory or a market"
                            },
                            {
                                "target_lang_word": "winning",
                                "target_lang_sense_clarification": "act of gaining something, e.g., trust or recognition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7ed5eef-e99b-37b5-b077-037bd7c34276",
                    "sense_definition": "То, что было завоёвано; значительное достижение или успех в какой-либо области.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Свобода слова — это важное <завоевание> демократического общества.",
                            "target_lang_translations": {
                                "en": "Freedom of speech is an important <achievement> of a democratic society."
                            }
                        },
                        {
                            "text": "Мы обязаны сохранить социальные <завоевания> прошлых поколений.",
                            "target_lang_translations": {
                                "en": "We must preserve the social <gains> of past generations."
                            }
                        }
                    ],
                    "synonyms": [
                        "достижение"
                    ],
                    "antonyms": [
                        "потеря",
                        "утрата"
                    ],
                    "common_phrases": [
                        "демократические завоевания",
                        "великие завоевания",
                        "научные завоевания"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that has been achieved or gained, especially a right or benefit obtained through struggle or hard work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "achievement",
                                "target_lang_sense_clarification": "significant success or accomplishment"
                            },
                            {
                                "target_lang_word": "gain",
                                "target_lang_sense_clarification": "benefit or right obtained, often used in plural"
                            }
                        ]
                    }
                }
            ]
        }
    ]

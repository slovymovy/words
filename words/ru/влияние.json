    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "83ece87d-dfdf-3638-9418-86d8c38722e0",
                    "sense_definition": "Действие, которое кто-то или что-то оказывает на кого-то или что-то; воздействие, изменяющее что-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Плохая погода оказывает сильное <w>влияние</w> на моё настроение.",
                            "target_lang_translations": {
                                "en": "Bad weather has a strong <w>influence</w> on my mood."
                            }
                        },
                        {
                            "text": "Мы попали под <w>влияние</w> моды и купили эти вещи.",
                            "target_lang_translations": {
                                "en": "We fell under the <w>influence</w> of fashion and bought these things."
                            }
                        }
                    ],
                    "synonyms": [
                        "воздействие",
                        "эффект"
                    ],
                    "common_phrases": [
                        "оказать влияние",
                        "под влиянием",
                        "дурное влияние",
                        "влияние на здоровье"
                    ],
                    "target_lang_definitions": {
                        "en": "The effect that a person or thing has on another's decisions, opinions, or state."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "influence",
                                "target_lang_sense_clarification": "general effect"
                            },
                            {
                                "target_lang_word": "impact",
                                "target_lang_sense_clarification": "strong effect"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50fc1bf1-7bb3-3b01-a93f-55eac9fae286",
                    "sense_definition": "Авторитет в обществе, власть, возможность воздействовать на ход событий или поведение людей.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этого политика есть большое <w>влияние</w> в правительстве.",
                            "target_lang_translations": {
                                "en": "This politician has great <w>influence</w> in the government."
                            }
                        },
                        {
                            "text": "Она использовала своё <w>влияние</w>, чтобы помочь другу получить работу.",
                            "target_lang_translations": {
                                "en": "She used her <w>influence</w> to help a friend get a job."
                            }
                        }
                    ],
                    "synonyms": [
                        "авторитет",
                        "вес",
                        "влиятельность",
                        "связи",
                        "сила"
                    ],
                    "antonyms": [
                        "беспомощность"
                    ],
                    "common_phrases": [
                        "пользоваться влиянием",
                        "сфера влияния",
                        "политическое влияние",
                        "иметь влияние"
                    ],
                    "target_lang_definitions": {
                        "en": "The power to have an important effect on someone or something; social or political standing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "influence",

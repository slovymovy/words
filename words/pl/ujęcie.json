                {
                    "sense_id": "0ce01097-751d-3e4f-9e32-a2fd343cec8c",
                    "sense_definition": "Fragment filmu nagrany bez przerywania pracy kamery lub sposób skadrowania obrazu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "film_photo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Reżyser zdecydował, że to <ujęcie> musi zostać powtórzone z powodu złego oświetlenia.",
                            "target_lang_translations": {
                                "ru": "Режиссер решил, что этот <дубль> нужно переснять из-за плохого освещения."
                            }
                        },
                        {
                            "text": "Ostatnie <ujęcie> w tym filmie jest niezwykle wzruszające i zapada w pamięć.",
                            "target_lang_translations": {
                                "ru": "Последний <кадр> в этом фильме невероятно трогательный и запоминающийся."
                            }
                        }
                    ],
                    "synonyms": [
                        "kadr",
                        "scena"
                    ],
                    "common_phrases": [
                        "dobre ujęcie",
                        "ujęcie filmowe",
                        "próbne ujęcie",
                        "ujęcie z bliska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фрагмент фильма, снятый без остановки камеры, или способ кадрирования изображения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кадр",
                                "target_lang_sense_clarification": "фрагмент фильма или композиция снимка"
                            },
                            {
                                "target_lang_word": "дубль",
                                "target_lang_sense_clarification": "попытка съемки сцены"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48a6438f-c7a4-3a0d-80c0-5782b8939303",
                    "sense_definition": "Sposób przedstawienia, interpretacji lub zrozumienia danego tematu lub problemu.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "perspective",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autor książki prezentuje ciekawe <ujęcie> historii drugiej wojny światowej.",
                            "target_lang_translations": {
                                "ru": "Автор книги представляет интересную <трактовку> истории Второй мировой войны."
                            }
                        },
                        {
                            "text": "W ujęciu psychologicznym zachowanie tego człowieka można wytłumaczyć traumą z dzieciństwa.",
                            "target_lang_translations": {
                                "ru": "В психологическом <аспекте> поведение этого человека можно объяснить детской травмой."
                            }
                        }
                    ],
                    "synonyms": [
                        "interpretacja",
                        "perspektywa",
                        "podejście",
                        "wizja"
                    ],
                    "common_phrases": [

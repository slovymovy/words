{
    "word_family": [
        "compassionate",
        "compassionately",
        "self-compassion"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc62e807-4e17-3c3b-aea2-9e6434b0e3e7",
                    "sense_definition": "A strong feeling of sympathy and sadness for the suffering or bad luck of others and a desire to help them.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She showed great <w>compassion</w> for the victims of the earthquake.",
                            "target_lang_translations": {
                                "ru": "Она проявила огромное <w>сострадание</w> к жертвам землетрясения."
                            }
                        },
                        {
                            "text": "Doctors must treat their patients with care and <w>compassion</w>.",
                            "target_lang_translations": {
                                "ru": "Врачи должны относиться к своим пациентам с заботой и <w>состраданием</w>."
                            }
                        },
                        {
                            "text": "He felt a deep <w>compassion</w> for the homeless man on the street.",
                            "target_lang_translations": {
                                "ru": "Он почувствовал глубокое <w>сострадание</w> к бездомному на улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "sympathy",
                        "empathy",
                        "pity",
                        "kindness",
                        "mercy",
                        "commiseration"
                    ],
                    "antonyms": [
                        "ruthlessness",
                        "pitilessness"
                    ],
                    "common_phrases": [
                        "show compassion",
                        "have compassion",
                        "lack compassion",
                        "deep compassion",
                        "act with compassion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное чувство сочувствия и печали, вызванное страданиями или неудачей других людей, и желание помочь им."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сострадание",
                                "target_lang_sense_clarification": "глубокое осознание чужих страданий и желание помочь"
                            },
                            {
                                "target_lang_word": "сочувствие",
                                "target_lang_sense_clarification": "отзывчивое отношение к чужому горю или переживаниям"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fd17c3cd-9c14-3d63-a9c8-ca97a245f1df",
                    "sense_definition": "To feel pity or sympathy for someone.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king <w>compassioned</w> the beggar and gave him a gold coin.",
                            "target_lang_translations": {
                                "ru": "Король <w>сжалился</w> над нищим и дал ему золотую монету."
                            }
                        },
                        {
                            "text": "She <w>compassioned</w> him in his time of great sorrow.",
                            "target_lang_translations": {
                                "ru": "Она <w>сочувствовала</w> ему в пору его великой скорби."
                            }
                        }
                    ],
                    "synonyms": [
                        "pity",
                        "commiserate",
                        "take pity"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rare in modern usage; usually replaced by 'to have compassion on' or 'to pity'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать жалость или сочувствие к кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сострадать",
                                "target_lang_sense_clarification": "испытывать страдание вместе с кем-либо"
                            },
                            {
                                "target_lang_word": "сжалиться",
                                "target_lang_sense_clarification": "почувствовать жалость к кому-либо (часто с оттенком милосердия)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "тоже",
        "также",
        "даже",
        "неужели"
    ],
    "entries": [
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "16bcd259-b39e-3a4d-9f00-2cf3cb99d7c3",
                    "sense_definition": "Служит для обозначения тождества, полного сходства; употребляется с местоимениями и наречиями (тот, такой, так, там, туда).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я купил такую <w>же</w> книгу, как у тебя.",
                            "target_lang_translations": {
                                "en": "I bought <w>the same</w> book as you have."
                            }
                        },
                        {
                            "text": "Мы встретились на том <w>же</w> месте, что и вчера.",
                            "target_lang_translations": {
                                "en": "We met at <w>the same</w> place as yesterday."
                            }
                        }
                    ],
                    "common_phrases": [
                        "тот же самый",
                        "в то же время",
                        "так же как",
                        "одно и то же"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to indicate identity or complete similarity; typically used with pronouns and adverbs."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the same",
                                "target_lang_sense_clarification": "indicating identity with something previously mentioned"
                            },
                            {
                                "target_lang_word": "very",
                                "target_lang_sense_clarification": "used for emphasis in phrases like 'the very same'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c526e74-d786-3c55-8d10-5c575db735d8",
                    "sense_definition": "Усиливает высказывание, подчёркивая очевидность факта, часто с оттенком напоминания или возражения (в значении «ведь»).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты <w>же</w> обещал позвонить мне вечером!",
                            "target_lang_translations": {
                                "en": "<w>But</w> you promised to call me in the evening!"
                            }
                        },
                        {
                            "text": "Это <w>же</w> очевидно, почему вы не понимаете?",
                            "target_lang_translations": {
                                "en": "<w>Why</w>, it's obvious, why don't you understand?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "я же говорил",
                        "он же знает",
                        "это же правда",
                        "конечно же"
                    ],
                    "target_lang_definitions": {
                        "en": "Strengthens a statement by emphasizing the obviousness of a fact, often with a tone of reminder or objection."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "after all",
                                "target_lang_sense_clarification": "emphasizing a known fact or reason"
                            },
                            {
                                "target_lang_word": "but",
                                "target_lang_sense_clarification": "expressing objection or reminder (e.g., 'But you said...')"
                            },
                            {
                                "target_lang_word": "why",
                                "target_lang_sense_clarification": "expressing surprise or obviousness (e.g., 'Why, it's simple!')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac5aa8bb-2afe-34fd-9186-bb27bdaecb12",
                    "sense_definition": "Усиливает слова, обозначающие время или последовательность действий, подчёркивая быстроту или немедленность.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "immediacy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Прекратите этот шум сейчас <w>же</w>!",
                            "target_lang_translations": {
                                "en": "Stop this noise <w>right</w> now!"
                            }
                        },
                        {
                            "text": "После ужина она сразу <w>же</w> легла спать.",
                            "target_lang_translations": {
                                "en": "After dinner, she went to bed <w>immediately</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сейчас же",
                        "сразу же",
                        "тут же",
                        "тогда же"
                    ],
                    "target_lang_definitions": {
                        "en": "Emphasizes words denoting time or sequence, stressing speed or immediacy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "right",
                                "target_lang_sense_clarification": "emphasizing exactness of time (e.g., right now)"
                            },
                            {
                                "target_lang_word": "immediately",
                                "target_lang_sense_clarification": "emphasizing that an action happens without delay"
                            },
                            {
                                "target_lang_word": "very",
                                "target_lang_sense_clarification": "emphasizing a specific moment (e.g., that very moment)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cbd38fa-80d5-3e13-b2ad-57f19abd683a",
                    "sense_definition": "Употребляется в вопросительных предложениях для выражения недоумения, удивления, нетерпения или для усиления вопроса.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "question_emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Где <w>же</w> мои ключи, я не могу их найти?",
                            "target_lang_translations": {
                                "en": "Where <w>on earth</w> are my keys? I can't find them."
                            }
                        },
                        {
                            "text": "Что <w>же</w> нам теперь делать в этой ситуации?",
                            "target_lang_translations": {
                                "en": "What <w>then</w> are we to do in this situation?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "ну что же",
                        "где же",
                        "как же так",
                        "кто же это"
                    ],
                    "target_lang_definitions": {
                        "en": "Used in questions to express bewilderment, surprise, impatience, or to strengthen the inquiry."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on earth",
                                "target_lang_sense_clarification": "emphasizing bewilderment (e.g., where on earth)"
                            },
                            {
                                "target_lang_word": "then",
                                "target_lang_sense_clarification": "emphasizing impatience or consequence (e.g., what then)"
                            },
                            {
                                "target_lang_word": "exactly",
                                "target_lang_sense_clarification": "asking for precise information with emphasis"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5555959-ae07-3ccb-865a-ae21eca78d52",
                    "sense_definition": "Используется в побудительных предложениях для выражения настойчивой просьбы, приказа или нетерпения.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "imperative_emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отвечайте <w>же</w>, не молчите!",
                            "target_lang_translations": {
                                "en": "<w>Do</w> answer, don't be silent!"
                            }
                        },
                        {
                            "text": "Пойдём <w>же</w> скорее, мы опаздываем.",
                            "target_lang_translations": {
                                "en": "<w>Come on</w>, let's go faster, we're late."
                            }
                        }
                    ],
                    "common_phrases": [
                        "говорите же",
                        "идемте же"
                    ],
                    "target_lang_definitions": {
                        "en": "Used in imperative sentences to express an insistent request, command, or impatience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "do",
                                "target_lang_sense_clarification": "adding insistence to a command (e.g., Do answer!)"
                            },
                            {
                                "target_lang_word": "come on",
                                "target_lang_sense_clarification": "encouraging action or expressing impatience"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "d720564f-5db8-3589-bbe4-78d586652523",
                    "sense_definition": "Обозначает противопоставление, соответствуя по значению союзам «а», «но» (стоит после первого слова во второй части предложения).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "contrast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Брат любит футбол, я <w>же</w> предпочитаю баскетбол.",
                            "target_lang_translations": {
                                "en": "My brother likes football, <w>while</w> I prefer basketball."
                            }
                        },
                        {
                            "text": "Петя ушёл в отпуск, мне <w>же</w> пришлось работать одному.",
                            "target_lang_translations": {
                                "en": "Petya went on vacation, <w>whereas</w> I had to work alone."
                            }
                        }
                    ],
                    "synonyms": [
                        "а",
                        "между тем"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates contrast, corresponding in meaning to conjunctions like 'whereas' or 'but'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whereas",
                                "target_lang_sense_clarification": "introducing a clause that contrasts with the previous one"
                            },
                            {
                                "target_lang_word": "while",
                                "target_lang_sense_clarification": "indicating a contrast between two subjects or actions"
                            },
                            {
                                "target_lang_word": "but",
                                "target_lang_sense_clarification": "simple contrast"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a99e279e-46c4-3e23-af99-1f53a2db3493",
                    "sense_definition": "Название восьмой буквы русского алфавита «Ж».",
                    "learner_level": "A1",
                    "frequency": "Low",
                    "semantic_group_id": "letter_name",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Слово «журнал» начинается на букву <w>же</w>.",
                            "target_lang_translations": {
                                "en": "The word \"zhurnal\" starts with the letter <w>zhe</w>."
                            }
                        },
                        {
                            "text": "Ученик аккуратно вывел прописную <w>же</w> в тетради.",
                            "target_lang_translations": {
                                "en": "The student neatly wrote a capital <w>zhe</w> in the notebook."
                            }
                        }
                    ],
                    "synonyms": [
                        "живете"
                    ],
                    "common_phrases": [
                        "буква же",
                        "прописная же"
                    ],
                    "target_lang_definitions": {
                        "en": "The name of the eighth letter of the Russian alphabet 'Ж'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "zhe",
                                "target_lang_sense_clarification": "name of the Cyrillic letter"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
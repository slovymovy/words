            "senses": [
                {
                    "sense_id": "16c2582a-3d00-330f-abc0-47e9e9bde248",
                    "sense_definition": "An English and Scottish surname originating as an occupation for a miller.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "name_surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Miller</w> has been our neighbor for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Миллер</w> является нашим соседом уже более двадцати лет."
                            }
                        },
                        {
                            "text": "The <w>Miller</w> family donated a large sum to the local library.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Миллер</w> пожертвовала крупную сумму местной библиотеке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "English and Scottish origin"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Английская и шотландская фамилия, происходящая от названия профессии мельника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Миллер",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0ea3a58-5faa-3bff-8f38-80e1a2c09d9b",
                    "sense_definition": "A male given name transferred from the surname.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "name_given",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "They decided to name their first son <w>Miller</w>.",
                            "target_lang_translations": {
                                "ru": "Они решили назвать своего первенца <w>Миллер</w>."
                            }
                        },
                        {
                            "text": "<w>Miller</w> is waiting for you in the lobby.",
                            "target_lang_translations": {
                                "ru": "<w>Миллер</w> ждет вас в вестибюле."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя, образованное от фамилии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Миллер",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16db5ddb-0334-361f-b1e3-1d7c2959df44",
                    "sense_definition": "A place name referring to various towns, communities, and suburbs in the United States, Canada, and Australia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "name_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We stopped in the small town of <w>Miller</w> to refuel the car.",
                            "target_lang_translations": {
                                "ru": "Мы остановились в маленьком городке <w>Миллер</w>, чтобы заправить машину."
                            }
                        },
                        {
                            "text": "The population of <w>Miller</w> has remained steady for the last decade.",
                            "target_lang_translations": {
                                "ru": "Население <w>Миллера</w> остается стабильным в течение последнего десятилетия."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название различных городов, общин и пригородов в США, Канаде и Австралии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Миллер",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]

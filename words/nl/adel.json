    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4c5f4e23-fc2e-3653-a163-26465b1397dd",
                    "sense_definition": "Een bevoorrechte sociale klasse van personen die vaak erfelijke titels dragen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "class",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>adel</w> had vroeger veel macht in Europa.",
                            "target_lang_translations": {
                                "en": "The <w>nobility</w> used to have a lot of power in Europe.",
                                "ru": "<w>Дворянство</w> раньше имело большую власть в Европе."
                            }
                        },
                        {
                            "text": "De graaf van Almelo is van <w>adel</w>.",
                            "target_lang_translations": {
                                "en": "The Count of Almelo is of the <w>nobility</w>.",
                                "ru": "Граф Алмело — из <w>дворян</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "noblesse"
                    ],
                    "common_phrases": [
                        "van adel",
                        "van adel zijn",
                        "hoge adel",
                        "lage adel"
                    ],
                    "target_lang_definitions": {
                        "en": "A privileged social class of people who often bear hereditary titles.",
                        "ru": "Привилегированный социальный класс, представители которого часто носят наследственные титулы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nobility",
                                "target_lang_sense_clarification": "social class"
                            },
                            {
                                "target_lang_word": "aristocracy",
                                "target_lang_sense_clarification": "privileged class"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "дворянство",
                                "target_lang_sense_clarification": "сословие"
                            },
                            {
                                "target_lang_word": "знать",
                                "target_lang_sense_clarification": "аристократия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e34572f1-7f21-3f72-b5b4-591adf0f231b",
                    "sense_definition": "De eigenschap van edel of groots van karakter te zijn.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij had een <w>adel</w> van geest die doorscheen in zijn werken.",
                            "target_lang_translations": {
                                "en": "He had a <w>nobility</w> of spirit that shone through in his works.",
                                "ru": "Он обладал <w>благородством</w> духа, которое просвечивало в его работах."
                            }
                        },
                        {
                            "text": "Haar natuurlijke <w>adel</w> dwong bij iedereen respect af.",
                            "target_lang_translations": {
                                "en": "Her natural <w>nobility</w> commanded respect from everyone.",
                                "ru": "Её природное <w>благородство</w> внушало всем уважение."
                            }
                        }
                    ],
                    "synonyms": [
                        "edelheid",
                        "noblesse"
                    ],
                    "common_phrases": [
                        "adel van geest"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality of being noble or grand in character.",
                        "ru": "Качество благородства или величия характера."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nobility",
                                "target_lang_sense_clarification": "quality of character"
                            },
                            {
                                "target_lang_word": "nobleness",
                                "target_lang_sense_clarification": "quality"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "благородство",
                                "target_lang_sense_clarification": "качество личности"
                            }
                        ]
                    }
                }
            ]
        },
        {

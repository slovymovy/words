{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "99eebc56-959e-3429-94f1-00a21ed5e450",
                    "sense_definition": "Wykrzyknik wyrażający powątpiewanie, niedowierzanie lub lekceważący stosunek do czyjejś wypowiedzi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "doubt_dismissal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ee</w>, chyba przesadzasz z tymi opowieściami.",
                            "target_lang_translations": {
                                "ru": "<w>Да ну</w>, ты, наверное, преувеличиваешь с этими рассказами."
                            }
                        },
                        {
                            "text": "<w>Ee</w> tam, wcale się tym nie przejmuję.",
                            "target_lang_translations": {
                                "ru": "<w>Ай</w>, я совсем об этом не беспокоюсь."
                            }
                        }
                    ],
                    "synonyms": [
                        "akurat",
                        "gdzie tam"
                    ],
                    "common_phrases": [
                        "ee tam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Междометие, выражающее сомнение, недоверие или пренебрежительное отношение к чьим-либо словам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да ну",
                                "target_lang_sense_clarification": "выражение сомнения или недоверия"
                            },
                            {
                                "target_lang_word": "ай",
                                "target_lang_sense_clarification": "пренебрежительное отмахивание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d25364a-32d5-38db-998e-006bbeecb9e5",
                    "sense_definition": "Słowo używane w języku dziecięcym do zasygnalizowania potrzeby fizjologicznej (siku lub kupy).",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "childish_needs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe dziecko zaczęło płakać i wołać <w>ee</w>.",
                            "target_lang_translations": {
                                "ru": "Маленький ребенок начал плакать и проситься <w>а-а</w>."
                            }
                        },
                        {
                            "text": "Musimy się zatrzymać, bo synek chce <w>ee</w>.",
                            "target_lang_translations": {
                                "ru": "Нам нужно остановиться, потому что сынок хочет <w>а-а</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "język dziecięcy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое в детской речи для обозначения физиологической потребности (сходить в туалет)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а-а",
                                "target_lang_sense_clarification": "детское обозначение туалета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19e00d4b-f47d-3cf4-9e76-b4162b9a6149",
                    "sense_definition": "Wykrzyknik wyrażający zniechęcenie, brak entuzjazmu lub rezygnację z działania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "discouragement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ee</w>, nie chce mi się dzisiaj nigdzie iść.",
                            "target_lang_translations": {
                                "ru": "<w>Э</w>, мне не хочется сегодня никуда идти."
                            }
                        },
                        {
                            "text": "<w>Ee</w>, to chyba nie jest dobry pomysł.",
                            "target_lang_translations": {
                                "ru": "<w>Э</w>, это, пожалуй, не лучшая идея."
                            }
                        }
                    ],
                    "synonyms": [
                        "eh"
                    ],
                    "target_lang_definitions": {
                        "ru": "Междометие, выражающее уныние, отсутствие энтузиазма или нежелание действовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "э",
                                "target_lang_sense_clarification": "выражение неохоты"
                            },
                            {
                                "target_lang_word": "не",
                                "target_lang_sense_clarification": "разговорный отказ или сомнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5244c28f-aab5-356f-9f98-444721f8e9d9",
                    "sense_definition": "Wykrzyknik wyrażający pozytywne zaskoczenie lub uznanie dla czegoś.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "surprise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ee</w>, całkiem nieźle ci to wyszło!",
                            "target_lang_translations": {
                                "ru": "<w>О</w>, у тебя совсем неплохо получилось!"
                            }
                        },
                        {
                            "text": "<w>Ee</w>, jaki piękny widok z tego okna.",
                            "target_lang_translations": {
                                "ru": "<w>Ого</w>, какой красивый вид из этого окна."
                            }
                        }
                    ],
                    "synonyms": [
                        "o",
                        "wow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Междометие, выражающее приятное удивление или одобрение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о",
                                "target_lang_sense_clarification": "удивление"
                            },
                            {
                                "target_lang_word": "ого",
                                "target_lang_sense_clarification": "сильное удивление"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
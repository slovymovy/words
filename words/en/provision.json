    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "879d482f-bf18-38b5-a4b7-bca0ea59fdbb",
                    "sense_definition": "The act of supplying something to someone or making preparations for something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_supply",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government is responsible for the <w>provision</w> of public education."
                        },
                        {
                            "text": "She made <w>provision</w> for her children in her will."
                        }
                    ],
                    "common_phrases": [
                        "provision of services",
                        "make provision for"
                    ]
                },
                {
                    "sense_id": "83eb8a0a-9d7d-3b02-9bc9-fc0866558a0c",
                    "sense_definition": "A specific statement or condition in a law, contract, or agreement.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "group_legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The contract contains a <w>provision</w> for early termination."
                        },
                        {
                            "text": "You must follow the <w>provisions</w> of the new safety law."
                        }
                    ],
                    "synonyms": [
                        "stipulation",
                        "condition"
                    ],
                    "common_phrases": [
                        "under the provisions",
                        "legal provision"
                    ]
                },
                {
                    "sense_id": "41556717-d1ad-3c2f-ad93-7851e1804584",
                    "sense_definition": "Supplies of food, drink, and other necessary items, especially for a journey.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_supply",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hikers carried enough <w>provisions</w> to last for three days."
                        },
                        {
                            "text": "We need to stop at the next town to buy fresh <w>provisions</w>."
                        }
                    ],
                    "common_phrases": [
                        "fresh provisions",
                        "emergency provisions"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ]
                },
                {
                    "sense_id": "bb3200f2-795b-3645-8450-cc6525890e67",
                    "sense_definition": "An amount of money saved or set aside for a specific future cost or debt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company made a <w>provision</w> for bad debts in this year's accounts."
                        },
                        {
                            "text": "There is no <w>provision</w> in the budget for these extra expenses."
                        }
                    ],
                    "common_phrases": [
                        "make a provision",
                        "tax provision"
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a29df8aa-e15f-3427-b0dc-ae33848c5153",
                    "sense_definition": "To set up a computer system, account, or network so that it is ready for use.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The IT department will <w>provision</w> your new email account shortly."
                        },
                        {
                            "text": "The software automatically <w>provisions</w> servers based on demand."
                        }
                    ],
                    "antonyms": [
                        "deprovision"
                    ],
                    "common_phrases": [
                        "provision a server",
                        "auto-provisioning"
                    ]
                },
                {
                    "sense_id": "3772f11c-835f-3e21-8726-606aa9806fcb",
                    "sense_definition": "To supply a person, ship, or group with food and other necessary equipment.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_supply",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>provisioned</w> the ship for a long voyage across the ocean."
                        },
                        {
                            "text": "The army was well <w>provisioned</w> before the winter campaign began."
                        }
                    ],
                    "synonyms": [
                        "supply"
                    ],
                    "common_phrases": [
                        "fully provisioned"
                    ]
                }
            ]
        }
    ]
}
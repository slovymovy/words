    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "748f48ac-c5af-3587-9ded-bad2d917ed7d",
                    "sense_definition": "Przebywać w pozycji wyprostowanej, opierając się na nogach (o ludziach i zwierzętach)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "posture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dlaczego ci ludzie tu <w>stoją</w>?",
                            "target_lang_translations": {
                                "ru": "Почему эти люди здесь <w>стоят</w>?"
                            }
                        },
                        {
                            "text": "<w>Stała</w> przed nim przez pięć minut i patrzyła mu w oczy.",
                            "target_lang_translations": {
                                "ru": "Она <w>стояла</w> перед ним пять минут и смотрела ему в глаза."
                            }
                        }
                    ],
                    "antonyms": [
                        "siedzieć",
                        "siedzieć",
                        "biec",
                        "iść"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в вертикальном положении, опираясь на ноги (о людях и животных)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоять",
                                "target_lang_sense_clarification": "находиться на ногах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7696b621-51d1-3cfe-8bfa-fa63a8cf8b0f",
                    "sense_definition": "Znajdować się w określonym miejscu (o przedmiotach ustawionych pionowo, meblach, budynkach)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te książki wcześniej <w>stały</w> na półce.",
                            "target_lang_translations": {
                                "ru": "Эти книги раньше <w>стояли</w> на полке."
                            }
                        },
                        {
                            "text": "Na przystanku <w>stoi</w> autobus.",
                            "target_lang_translations": {
                                "ru": "На остановке <w>стоит</w> автобус."
                            }
                        },
                        {
                            "text": "Regał <w>stoi</w> pod ścianą w dużym pokoju.",
                            "target_lang_translations": {
                                "ru": "Стеллаж <w>стоит</w> у стены в большой комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "(1.7) znajdować się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в определенном месте (о предметах, мебели, зданиях)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоять",
                                "target_lang_sense_clarification": "находиться (вертикально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc42d935-6791-3c44-8d04-4b0fdcca4949",
                    "sense_definition": "Nie przemieszczać się, pozostawać w bezruchu (np. w oczekiwaniu na coś)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motionless",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Stała</w> w korku przez pięć minut.",
                            "target_lang_translations": {
                                "ru": "Она <w>стояла</w> в пробке пять минут."
                            }
                        },
                        {
                            "text": "Musieliśmy długo <w>stać</w> w kolejce do sklepu.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось долго <w>стоять</w> в очереди в магазин."
                            }
                        }
                    ],
                    "antonyms": [
                        "iść",
                        "jechać",
                        "biec"
                    ],
                    "common_phrases": [
                        "stać w kolejce",
                        "stać w korku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не двигаться, оставаться на месте (например, в ожидании, в очереди или пробке)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоять",
                                "target_lang_sense_clarification": "не двигаться, ждать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b642c4d7-7cbe-33e3-8b65-32ddee99b01e",
                    "sense_definition": "Mieć wystarczające środki finansowe na zakup czegoś lub opłacenie czegoś (używane w konstrukcjach bezosobowych)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "affordability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wreszcie <w>stać</w> mnie na ten płaszcz.",
                            "target_lang_translations": {
                                "ru": "Наконец-то я могу <w>позволить себе</w> это пальто."
                            }
                        },
                        {
                            "text": "Nawet za pięć lat nie będzie go <w>stać</w> na ten samochód.",
                            "target_lang_translations": {
                                "ru": "Даже через пять лет он не сможет <w>позволить себе</w> эту машину."
                            }
                        },
                        {
                            "text": "Czy Pańską rodzinę <w>stać</w> na adwokata?",
                            "target_lang_translations": {
                                "ru": "Может ли ваша семья <w>позволить себе</w> адвоката?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "stać mnie na",
                        "nie stać mnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь достаточно денег для покупки или оплаты чего-либо (безличная конструкция)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позволить себе",
                                "target_lang_sense_clarification": "иметь финансовую возможность"
                            },
                            {
                                "target_lang_word": "быть по карману",
                                "target_lang_sense_clarification": "разговорное выражение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af51c06b-973e-3d94-931d-e2d56fc0b04a",
                    "sense_definition": "Wydarzyć się, mieć miejsce (używane z zaimkiem zwrotnym 'się')",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "happen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Co tu się <w>stało</w>?",
                            "target_lang_translations": {
                                "ru": "Что здесь <w>случилось</w>?"
                            }
                        },
                        {
                            "text": "<w>Stała</w> się rzecz niepojęta, Tomek pogodził się z ojcem!",
                            "target_lang_translations": {
                                "ru": "<w>Случилась</w> непостижимая вещь, Томек помирился с отцом!"
                            }
                        },
                        {
                            "text": "Czuję, że <w>stanie</w> się coś złego.",
                            "target_lang_translations": {
                                "ru": "Чувствую, что <w>случится</w> что-то плохое."
                            }
                        }
                    ],
                    "common_phrases": [
                        "co się stało",
                        "nic się nie stało"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произойти, случиться (используется с возвратной частицей 'się')"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "случиться",
                                "target_lang_sense_clarification": "произойти"
                            },
                            {
                                "target_lang_word": "произойти",
                                "target_lang_sense_clarification": "иметь место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "948ef216-ee40-3fb1-ab73-08aad10e71a8",
                    "sense_definition": "Zmienić się w kogoś lub coś, nabrać nowych cech (używane z zaimkiem zwrotnym 'się')",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "become",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po kilku dniach <w>stali</w> się najlepszymi przyjaciółmi.",
                            "target_lang_translations": {
                                "ru": "Через несколько дней они <w>стали</w> лучшими друзьями."
                            }
                        },
                        {
                            "text": "Jego twarz <w>stała</w> się czerwona ze złości.",
                            "target_lang_translations": {
                                "ru": "Его лицо <w>стало</w> красным от злости."
                            }
                        },
                        {
                            "text": "Myślisz, że się zmieni, gdy <w>stanie</w> się sławna?",
                            "target_lang_translations": {
                                "ru": "Думаешь, она изменится, когда <w>станет</w> знаменитой?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Превратиться, приобрести новые свойства или состояние (используется с возвратной частицей 'się')"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стать",
                                "target_lang_sense_clarification": "сделаться кем-то/чем-то"
                            },
                            {
                                "target_lang_word": "сделаться",
                                "target_lang_sense_clarification": "превратиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cc4abd8-57ad-37a1-bbc3-617c7a1bebc0",
                    "sense_definition": "Przestać działać, zatrzymać się (o mechanizmach, zegarkach)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "malfunction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zegarek <w>stał</w>, a ja nie mogłem go naprawić.",
                            "target_lang_translations": {
                                "ru": "Часы <w>стояли</w>, и я не мог их починить."
                            }
                        },
                        {
                            "text": "Zegarek mi <w>stoi</w>, muszę wymienić baterię.",
                            "target_lang_translations": {
                                "ru": "У меня <w>стоят</w> часы, нужно поменять батарейку."
                            }
                        }
                    ],
                    "antonyms": [
                        "działać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать работать, остановиться (о механизмах, часах)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоять",
                                "target_lang_sense_clarification": "не ходить (о часах)"
                            },
                            {
                                "target_lang_word": "остановиться",
                                "target_lang_sense_clarification": "прекратить работу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2768ec8-3774-3c47-9d7d-920ab6eaff5f",
                    "sense_definition": "Obowiązywać, być aktualnym (np. o umowie lub zakładzie)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "validity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza umowa nadal <w>stoi</w>.",
                            "target_lang_translations": {
                                "ru": "Наш договор всё ещё <w>в силе</w>."
                            }
                        },
                        {
                            "text": "Zakład <w>stoi</w>, przegrany stawia kolację.",
                            "target_lang_translations": {
                                "ru": "Спор <w>в силе</w>, проигравший ставит ужин."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть действительным, оставаться в силе (о договоре или пари)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть в силе",
                                "target_lang_sense_clarification": "действовать (о договоре)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "d71aa985-ef31-38d6-917e-407dfe17fdfc",
                    "sense_definition": "Rozkaz natychmiastowego zatrzymania się",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Stać</w>! Ani kroku dalej!",
                            "target_lang_translations": {
                                "ru": "<w>Стоять</w>! Ни шагу дальше!"
                            }
                        },
                        {
                            "text": "Policjant krzyknął: <w>stać</w>, policja!",
                            "target_lang_translations": {
                                "ru": "Полицейский крикнул: <w>стоять</w>, полиция!"
                            }
                        }
                    ],
                    "synonyms": [
                        "stój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приказ немедленно остановиться"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоять",
                                "target_lang_sense_clarification": "команда"
                            },
                            {
                                "target_lang_word": "стой",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
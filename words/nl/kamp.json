    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4186c949-ee28-3972-a250-896132d0710f",
                    "sense_definition": "Een plaats met tenten of eenvoudige huisjes waar mensen tijdelijk verblijven, vaak voor vakantie of als activiteit van een vereniging.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verblijfplaats",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen gaan volgende week op <w>kamp</w> met de scouting.",
                            "target_lang_translations": {
                                "ru": "Дети на следующей неделе едут в <w>лагерь</w> со скаутами."
                            }
                        },
                        {
                            "text": "We zochten een vlak stuk grond om ons <w>kamp</w> op te zetten.",
                            "target_lang_translations": {
                                "ru": "Мы искали ровный участок земли, чтобы разбить наш <w>лагерь</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op kamp gaan",
                        "kamp opslaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место с палатками или простыми домиками для временного проживания, часто для отдыха или мероприятий организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лагерь",
                                "target_lang_sense_clarification": "место отдыха или сборов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe580b1b-0818-3b5f-86e0-8b0a4599616f",
                    "sense_definition": "Een bewaakt of afgesloten terrein waar grote groepen mensen, zoals militairen, vluchtelingen of gevangenen, (tijdelijk) gehuisvest zijn.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verblijfplaats",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten keerden na hun missie terug naar het <w>kamp</w>.",
                            "target_lang_translations": {
                                "ru": "Солдаты вернулись в <w>лагерь</w> после выполнения задания."
                            }
                        },
                        {
                            "text": "In het <w>kamp</w> werden duizenden mensen opgevangen.",
                            "target_lang_translations": {
                                "ru": "В <w>лагере</w> были размещены тысячи людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "kampement"
                    ],
                    "common_phrases": [
                        "militair kamp"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охраняемая или закрытая территория, где размещаются большие группы людей, например военные, беженцы или заключенные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лагерь",
                                "target_lang_sense_clarification": "военный или для заключенных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "901176f6-9e60-35b9-8474-18d3609c529f",
                    "sense_definition": "Een groep mensen die dezelfde mening of doelen deelt en vaak tegenover een andere groep staat.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "groep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politieke partij is verdeeld geraakt in twee <w>kampen</w>.",
                            "target_lang_translations": {
                                "ru": "Политическая партия раскололась на два <w>лагеря</w>."
                            }
                        },
                        {
                            "text": "Ik weet niet tot welk <w>kamp</w> hij behoort in deze discussie.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, к какому <w>лагерю</w> он принадлежит в этой дискуссии."
                            }
                        }
                    ],
                    "synonyms": [
                        "partij"
                    ],
                    "common_phrases": [
                        "in twee kampen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, разделяющих одно мнение или цели и часто противостоящая другой группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лагерь",
                                "target_lang_sense_clarification": "группа единомышленников"
                            },
                            {
                                "target_lang_word": "стан",
                                "target_lang_sense_clarification": "сторона в споре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1039edcf-dbeb-3274-ac77-925889274f74",
                    "sense_definition": "Een strijd of gevecht tussen twee personen of partijen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "strijd",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een lange <w>kamp</w> werd eindelijk de winnaar bekendgemaakt.",
                            "target_lang_translations": {
                                "ru": "После долгого <w>поединка</w> наконец был объявлен победитель."
                            }
                        },
                        {
                            "text": "De ridders bereidden zich voor op de <w>kamp</w>.",
                            "target_lang_translations": {
                                "ru": "Рыцари готовились к <w>бою</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "voornamelijk literair of historisch gebruik"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Битва или сражение между двумя людьми или сторонами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поединок",
                                "target_lang_sense_clarification": "сражение"
                            },
                            {
                                "target_lang_word": "бой",
                                "target_lang_sense_clarification": "битва"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a30f8464-7d12-3d2b-823c-b4e47cd9d44d",
                    "sense_definition": "Een stuk land of afgegrensd veld, vaak gebruikt voor landbouw.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "terrein",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer liet zijn vee grazen op de <w>kamp</w> achter de boerderij.",
                            "target_lang_translations": {
                                "ru": "Фермер пас свой скот на <w>поле</w> за фермой."
                            }
                        },
                        {
                            "text": "Dit stukje <w>kamp</w> is eigendom van de gemeente.",
                            "target_lang_translations": {
                                "ru": "Этот <w>участок земли</w> является собственностью муниципалитета."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "komt voor in dialecten en specifieke streeknamen"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок земли или огороженное поле, часто используемое для сельского хозяйства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поле",
                                "target_lang_sense_clarification": "участок земли"
                            },
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "огороженная земля"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dbc7aaaf-e4b9-3a8e-82e4-088cb447ce73",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>kampen</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>kamp</w> al weken met een zware verkoudheid.",
                            "target_lang_translations": {
                                "ru": "Я уже несколько недель <w>борюсь</w> с сильной простудой."
                            }
                        },
                        {
                            "text": "Sinds het ontslag <w>kamp</w> ik met financiële problemen.",
                            "target_lang_translations": {
                                "ru": "С момента увольнения я <w>сталкиваюсь</w> с финансовыми проблемами."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "vervoeging"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени от глагола 'kampen' (бороться, сталкиваться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "борюсь",
                                "target_lang_sense_clarification": "с трудностями или болезнью"
                            },
                            {
                                "target_lang_word": "сталкиваюсь",
                                "target_lang_sense_clarification": "имею дело с проблемами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eae8b734-4dd4-36a9-9ebb-a283a260a809",
                    "sense_definition": "Gebiedende wijs van <w>kampen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Kamp</w> niet alleen met je zorgen, maar zoek steun.",
                            "target_lang_translations": {
                                "ru": "Не <w>борись</w> со своими тревогами в одиночку, ищи поддержки."
                            }
                        },
                        {
                            "text": "<w>Kamp</w> moedig verder, ook als het tegenzit.",
                            "target_lang_translations": {
                                "ru": "Мужественно <w>сражайся</w> дальше, даже если всё идет не так."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "vervoeging"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола 'kampen'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "борись",
                                "target_lang_sense_clarification": "преодолевай"
                            },
                            {
                                "target_lang_word": "сражайся",
                                "target_lang_sense_clarification": "не сдавайся"
                            }
                        ]
                    }
                }
            ]
        },
        {

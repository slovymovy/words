        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "78d11506-1bb4-3636-91ec-a00ee1c2c2d9",
                    "sense_definition": "A sound made by liquid striking something or moving about; a splashing sound.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sound_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gentle <plashing> of the fountain created a soothing atmosphere in the courtyard.",
                            "target_lang_translations": {
                                "ru": "Нежный <плеск> фонтана создавал успокаивающую атмосферу во внутреннем дворике."
                            }
                        },
                        {
                            "text": "We could hear the <plashing> of oars as the boat approached the dock.",
                            "target_lang_translations": {
                                "ru": "Мы слышали <плеск> весел, когда лодка приближалась к причалу."
                            }
                        }
                    ],
                    "synonyms": [
                        "splashing",
                        "splash"
                    ],
                    "antonyms": [

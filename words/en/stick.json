                    "sense_definition": "To become fixed or jammed so that movement is impossible.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "jam",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The drawer tends to <w>stick</w> when the weather is humid.",
                            "target_lang_translations": {
                                "ru": "Ящик имеет обыкновение <w>заедать</w> во влажную погоду."
                            }
                        },
                        {
                            "text": "My zipper is <w>stuck</w> and I cannot get my jacket off.",
                            "target_lang_translations": {
                                "ru": "Моя молния <w>застряла</w>, и я не могу снять куртку."
                            }
                        }
                    ],
                    "synonyms": [
                        "jam",
                        "seize",
                        "stall"
                    ],
                    "common_phrases": [
                        "get stuck"
                    ],
                    "target_lang_definitions": {
                        "ru": "Застревать или заклинивать так, что движение становится невозможным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заедать",
                                "target_lang_sense_clarification": "о механизмах"
                            },
                            {
                                "target_lang_word": "застревать",
                                "target_lang_sense_clarification": "о невозможности движения"
                            }
                        ]

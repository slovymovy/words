    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "aacaca3e-cc3e-3a66-b76a-568e37a3515f",
                    "sense_definition": "Женская форма указательного местоимения <w>тот</w> (именительный падеж, единственное число); указывает на удалённый объект или лицо женского рода.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кто <та> женщина, которая стоит у окна?",
                            "target_lang_translations": {
                                "en": "Who is <that> woman standing by the window?"
                            }
                        },
                        {
                            "text": "Мне нравится не эта сумка, а вон <та>.",
                            "target_lang_translations": {
                                "en": "I don't like this bag, but <that one> over there."
                            }
                        }
                    ],
                    "antonyms": [
                        "эта"
                    ],
                    "common_phrases": [
                        "та самая",
                        "вон та",
                        "не та",
                        "и та и другая"
                    ],
                    "target_lang_definitions": {
                        "en": "Feminine form of the demonstrative pronoun \"that\" (nominative, singular); indicates a distant object or person of feminine gender."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "demonstrative pronoun referring to a feminine noun"
                            },
                            {
                                "target_lang_word": "that one",
                                "target_lang_sense_clarification": "referring to a specific feminine object"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "5c3d16e9-ecdc-3c38-ada2-1138a4a04d75",
                    "sense_definition": "Разговорный вариант частицы <w>да</w> или междометия, используемый для усиления эмоций, выражения отмашки или сомнения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Та> ладно тебе, не переживай так сильно!",
                            "target_lang_translations": {
                                "en": "<Oh> come on, don't worry so much!"
                            }
                        },
                        {
                            "text": "<Та> ну его, пойдём лучше домой.",
                            "target_lang_translations": {
                                "en": "<Oh> forget it, let's just go home."
                            }
                        }
                    ],
                    "synonyms": [
                        "телефон"
                    ],
                    "common_phrases": [
                        "та ладно",
                        "та ну",
                        "та брось"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто встречается в южнорусских говорах и просторечии"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial variant of the particle \"da\" or an interjection used to emphasize emotions, dismiss something, or express doubt."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oh",
                                "target_lang_sense_clarification": "interjection used for emphasis or dismissal"
                            },
                            {
                                "target_lang_word": "ah",
                                "target_lang_sense_clarification": "expressing emotion or dismissal"
                            },
                            {
                                "target_lang_word": "come on",
                                "target_lang_sense_clarification": "expression of disbelief or dismissal"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bfc1b82b-4831-3da9-903a-d81e90733489",
                    "sense_definition": "Сокращение от термина <w>телефонный аппарат</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В каждом кабинете должен быть установлен исправный <ТА>.",
                            "target_lang_translations": {
                                "en": "A working <telephone set> must be installed in every office."
                            }
                        },
                        {
                            "text": "Инженер проверил <ТА> и подключил его к линии.",
                            "target_lang_translations": {
                                "en": "The engineer checked the <telephone set> and connected it to the line."
                            }
                        }
                    ],
                    "synonyms": [
                        "телефон"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "аббревиатура"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for the term \"telephone set\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "telephone set",
                                "target_lang_sense_clarification": "technical abbreviation"
                            },
                            {
                                "target_lang_word": "phone",
                                "target_lang_sense_clarification": "device"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
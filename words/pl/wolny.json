                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медленный",
                                "target_lang_sense_clarification": "о скорости движения или процесса"
                            },
                            {
                                "target_lang_word": "неторопливый",
                                "target_lang_sense_clarification": "лишенный спешки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19ee0a57-2b04-3574-bc3b-bf1c19f87ef7",
                    "sense_definition": "Taki, z którego nikt w danej chwili nie korzysta; dostępny do użycia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy to miejsce obok pana jest <w>wolne</w>, czy ktoś tu siedzi?",
                            "target_lang_translations": {
                                "ru": "Это место рядом с вами <w>свободно</w>, или здесь кто-то сидит?"
                            }
                        },
                        {
                            "text": "Szukaliśmy długo parkingu, ale żaden plac nie był <w>wolny</w>.",
                            "target_lang_translations": {
                                "ru": "Мы долго искали парковку, но ни одно место не было <w>свободным</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "niezajęty",
                        "wakujący"
                    ],
                    "antonyms": [
                        "zajęty"
                    ],
                    "common_phrases": [
                        "wolne miejsce",
                        "wolny pokój",
                        "wolny stolik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, которым никто в данный момент не пользуется; доступный для использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свободный",
                                "target_lang_sense_clarification": "незанятый, пустой"

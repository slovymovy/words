                },
                {
                    "sense_id": "1c683bbe-9f20-333f-bb14-8b43811c60e4",
                    "sense_definition": "Snel en vlak langs een oppervlak bewegen zonder het te raken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zwaluw <scheerde> laag over het water op zoek naar insecten.",
                            "target_lang_translations": {

                    "sense_id": "0069adc6-b3c3-398a-b246-d92e57b9d332",
                    "sense_definition": "Een gebouw waar monniken of nonnen samenwonen en hun leven aan het geloof wijden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious_building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De monniken wonen al jaren in stilte in het oude <w>klooster</w>.",
                            "target_lang_translations": {
                                "ru": "Монахи уже много лет живут в тишине в старом <w>монастыре</w>."
                            }
                        },
                        {
                            "text": "Zij besloot om in het <w>klooster</w> te gaan en haar leven aan God te geven.",
                            "target_lang_translations": {
                                "ru": "Она решила уйти в <w>монастырь</w> и посвятить свою жизнь Богу."
                            }
                        }
                    ],
                    "synonyms": [
                        "abdij",
                        "convent"
                    ],
                    "common_phrases": [
                        "in het klooster gaan",
                        "in een klooster leven"
                    ],
                    "target_lang_definitions": {

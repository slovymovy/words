    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0c00a304-df71-38bc-9b19-3ee20df2429f",
                    "sense_definition": "Verwijst naar iets dat eerder genoemd is, in combinatie met het voorzetsel 'voor'.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pronominal_ref",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je echt iets wilt bereiken in het leven, moet je <w>ervoor</w> gaan.",
                            "target_lang_translations": {
                                "en": "If you really want to achieve something in life, you have to go <w>for it</w>.",
                                "ru": "Если ты действительно хочешь чего-то достичь в жизни, ты должен взяться <w>за это</w>."
                            }
                        },
                        {
                            "text": "De leraar zal <w>ervoor</w> zorgen dat alle studenten de juiste informatie ontvangen.",
                            "target_lang_translations": {
                                "en": "The teacher will see <w>to it</w> that all students receive the correct information.",
                                "ru": "Учитель позаботится <w>о том</w>, чтобы все студенты получили правильную информацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "daarvoor"
                    ],
                    "common_phrases": [
                        "ervoor zorgen",
                        "ervoor kiezen",
                        "ervoor gaan",
                        "ervoor staan"
                    ],
                    "target_lang_definitions": {
                        "en": "Refers to something previously mentioned, used with the preposition 'for'.",
                        "ru": "Указывает на что-то, упомянутое ранее, в сочетании с предлогом 'voor' (для, за, о)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for it",
                                "target_lang_sense_clarification": "general reference to a preceding object or idea"
                            },
                            {
                                "target_lang_word": "to it",
                                "target_lang_sense_clarification": "used in specific fixed expressions like 'see to it'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "об этом",
                                "target_lang_sense_clarification": "в сочетании с глаголами, требующими предлога 'о' (например, позаботиться)"
                            },
                            {
                                "target_lang_word": "за это",
                                "target_lang_sense_clarification": "в значении 'взяться за дело' или 'нести ответственность'"
                            },
                            {
                                "target_lang_word": "к этому",
                                "target_lang_sense_clarification": "в контексте стремления или движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "474d7110-9cba-33cc-a76f-00f5f0d28fd3",
                    "sense_definition": "Ten behoeve van dat doel of als tegenprestatie voor iets.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pronominal_ref",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft jarenlang hard getraind en is uiteindelijk <w>ervoor</w> beloond.",
                            "target_lang_translations": {
                                "en": "He trained hard for years and was eventually rewarded <w>for it</w>.",
                                "ru": "Он упорно тренировался годами и в конце концов был вознагражден <w>за это</w>."
                            }
                        },
                        {
                            "text": "Het is een zware taak, maar je moet het <w>ervoor</w> overhebben als je succes wilt.",
                            "target_lang_translations": {
                                "en": "It is a tough task, but you have to be willing to sacrifice <w>for it</w> if you want success.",
                                "ru": "Это тяжелая задача, но ты должен быть готов пойти <w>на это</w>, если хочешь успеха."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ervoor overhebben",
                        "ervoor werken"
                    ],
                    "target_lang_definitions": {
                        "en": "For that purpose or in return for something.",
                        "ru": "Ради определенной цели или в качестве ответной меры за что-то."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for it",
                                "target_lang_sense_clarification": "in exchange or return for something"
                            },
                            {
                                "target_lang_word": "for that",
                                "target_lang_sense_clarification": "for that specific purpose or cause"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "за это",
                                "target_lang_sense_clarification": "в обмен или в награду"
                            },
                            {
                                "target_lang_word": "ради этого",
                                "target_lang_sense_clarification": "для достижения цели"
                            },
                            {
                                "target_lang_word": "на это",
                                "target_lang_sense_clarification": "тратить ресурсы или усилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "146fe672-c1a4-3b26-be77-f03e227941a7",
                    "sense_definition": "Voorafgaand aan een bepaald moment of een specifieke gebeurtenis.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het concert begon om acht uur, maar wij waren er al ruim <w>ervoor</w>.",
                            "target_lang_translations": {
                                "en": "The concert started at eight o'clock, but we were there well <w>beforehand</w>.",
                                "ru": "Концерт начинался в восемь часов, но мы были там уже задолго <w>до этого</w>."
                            }
                        },
                        {
                            "text": "Ze vertrok naar het buitenland, maar vlak <w>ervoor</w> kwam ze nog even langs om afscheid te nemen.",
                            "target_lang_translations": {
                                "en": "She left for abroad, but just <w>before that</w> she stopped by to say goodbye.",
                                "ru": "Она уезжала за границу, но прямо <w>перед этим</w> зашла попрощаться."
                            }
                        }
                    ],
                    "synonyms": [
                        "eerder",
                        "tevoren",
                        "daarvoor"
                    ],
                    "antonyms": [
                        "erna",
                        "daarna",
                        "later"
                    ],
                    "common_phrases": [
                        "vlak ervoor",
                        "kort ervoor",
                        "net ervoor"
                    ],
                    "target_lang_definitions": {
                        "en": "Before a specific moment or event.",
                        "ru": "Предшествуя определенному моменту или событию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beforehand",
                                "target_lang_sense_clarification": "in advance of an event"
                            },
                            {
                                "target_lang_word": "before that",
                                "target_lang_sense_clarification": "prior to that specific time"
                            },
                            {
                                "target_lang_word": "before it",
                                "target_lang_sense_clarification": "preceding the mentioned event"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "до этого",
                                "target_lang_sense_clarification": "раньше по времени"
                            },
                            {
                                "target_lang_word": "перед этим",
                                "target_lang_sense_clarification": "непосредственно перед событием"
                            }
                        ]
                    }
                }
            ]
        }
    ]

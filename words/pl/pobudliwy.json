    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "771858af-ed4f-3922-95f8-770da349e375",
                    "sense_definition": "Taki, który łatwo i silnie reaguje na bodźce zewnętrzne lub emocje; charakteryzujący się dużą wrażliwością układu nerwowego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reactivity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego układ nerwowy jest bardzo <w>pobudliwy</w>, dlatego hałas szybko go męczy.",
                            "target_lang_translations": {
                                "ru": "Его нервная система очень <w>возбудима</w>, поэтому шум быстро его утомляет."
                            }
                        },
                        {
                            "text": "Czasem nadmiernie krzykliwi i <w>pobudliwi</w>, nigdy nie tracili wewnętrznej równowagi i pogody ducha.",
                            "target_lang_translations": {
                                "ru": "Иногда чрезмерно крикливые и <w>возбудимые</w>, они никогда не теряли внутреннего равновесия и бодрости духа."
                            }
                        }
                    ],
                    "synonyms": [
                        "wrażliwy",
                        "impulsywny"
                    ],
                    "antonyms": [
                        "obojętny",
                        "niewrażliwy",
                        "flegmatyczny"
                    ],
                    "common_phrases": [
                        "nadmiernie pobudliwy",
                        "pobudliwy układ nerwowy",
                        "pobudliwy charakter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легко и сильно реагирующий на внешние раздражители или эмоции; характеризующийся высокой чувствительностью нервной системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возбудимый",
                                "target_lang_sense_clarification": "о нервной системе или реакции на раздражители"
                            },
                            {
                                "target_lang_word": "впечатлительный",
                                "target_lang_sense_clarification": "эмоционально восприимчивый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8321491b-081b-3f22-909c-c04ef11994c8",
                    "sense_definition": "Skłonny do gwałtownych reakcji, nerwowy, łatwo wpadający w gniew lub irytację.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reactivity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ostatnio stał się bardzo <w>pobudliwy</w> i kłóci się o byle drobiazg.",
                            "target_lang_translations": {
                                "ru": "В последнее время он стал очень <w>раздражительным</w> и ссорится из-за любого пустяка."
                            }
                        },
                        {
                            "text": "Brak snu sprawia, że jestem bardziej <w>pobudliwy</w> niż zwykle i wszystko mnie denerwuje.",
                            "target_lang_translations": {
                                "ru": "Недосыпание делает меня более <w>раздражительным</w>, чем обычно, и всё меня нервирует."
                            }
                        }
                    ],
                    "synonyms": [
                        "przewrażliwiony",
                        "przeczulony"
                    ],
                    "antonyms": [
                        "spokojny",
                        "opanowany",
                        "zrównoważony"
                    ],
                    "common_phrases": [
                        "stać się pobudliwym",
                        "pobudliwy nastrój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склонный к бурным реакциям, нервный, легко впадающий в гнев или раздражение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздражительный",
                                "target_lang_sense_clarification": "легко приходящий в состояние недовольства"
                            },
                            {
                                "target_lang_word": "вспыльчивый",
                                "target_lang_sense_clarification": "склонный к резким вспышкам гнева"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
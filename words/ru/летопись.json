                        "семейная летопись",
                        "живая летопись"
                    ],
                    "target_lang_definitions": {
                        "en": "A regular record of events; the history of the development of something"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chronicle",
                                "target_lang_sense_clarification": "record of events"
                            },
                            {
                                "target_lang_word": "record",
                                "target_lang_sense_clarification": "account of history"
                            }
                        ]
                    }

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страховать",
                                "target_lang_sense_clarification": "в альпинизме"
                            },
                            {
                                "target_lang_word": "закреплять",
                                "target_lang_sense_clarification": "фиксировать"
                            }
                        ]
                    }
                }
            ]
        }
    ]

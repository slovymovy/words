        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "19654a91-47eb-341d-b9a5-f893bf0bf797",
                    "sense_definition": "A mother.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "mother_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>mum</w> cooks the best dinner on Sundays.",
                            "target_lang_translations": {
                                "ru": "Моя <w>мама</w> готовит лучший ужин по воскресеньям."
                            }
                        },
                        {
                            "text": "I am going to the shops with my <w>mum</w> and dad.",
                            "target_lang_translations": {
                                "ru": "Я иду по магазинам с <w>мамой</w> и папой."
                            }
                        }
                    ],
                    "synonyms": [
                        "Mother",
                        "Mom",
                        "Mommy",
                        "Mummy",
                        "Mum"
                    ],
                    "antonyms": [
                        "Dad",
                        "Dad"
                    ],
                    "common_phrases": [
                        "my mum",
                        "mum and dad",
                        "single mum"
                    ],
                    "traits": [

            "senses": [
                {
                    "sense_id": "423b8d39-0522-3b80-982a-75a4b5124dca",
                    "sense_definition": "Present participle and gerund of <w>help</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>helping</w> my brother with his homework.",
                            "target_lang_translations": {
                                "ru": "Я <w>помогаю</w> брату с домашним заданием."
                            }
                        },
                        {
                            "text": "Thank you for <w>helping</w> me carry these bags.",
                            "target_lang_translations": {
                                "ru": "Спасибо, что <w>помогли</w> мне донести эти сумки."

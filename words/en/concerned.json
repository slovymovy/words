                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The documentary <w>concerned</w> the daily lives of lions in the wild.",
                            "target_lang_translations": {

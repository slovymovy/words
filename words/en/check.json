                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Put a <w>check</w> next to the items you want to order.",
                            "target_lang_translations": {
                                "ru": "Поставьте <w>галочку</w> рядом с пунктами, которые вы хотите заказать."
                            }

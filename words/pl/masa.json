    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "339caefd-20a6-3d25-b147-e66de4a520f1",
                    "sense_definition": "Gęsta, bezkształtna substancja, z której można formować przedmioty lub która wypełnia jakąś przestrzeń",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "substance_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci w przedszkolu lepiły figurki z <masy> solnej.",
                            "target_lang_translations": {
                                "ru": "Дети в детском саду лепили фигурки из соленого <теста>."
                            }
                        },
                        {
                            "text": "Ta szara <masa> szybko twardnieje po wyjęciu z opakowania.",
                            "target_lang_translations": {
                                "ru": "Эта серая <масса> быстро твердеет после извлечения из упаковки."
                            }
                        }
                    ],
                    "synonyms": [
                        "mieszanina"
                    ],
                    "common_phrases": [
                        "masa plastyczna",
                        "masa solna",
                        "masa papierowa",
                        "masa bitumiczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Густое, бесформенное вещество, из которого можно лепить предметы или которое заполняет пространство"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "масса",
                                "target_lang_sense_clarification": "вещество"
                            },
                            {
                                "target_lang_word": "смесь",
                                "target_lang_sense_clarification": "состав"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c78b5d28-024c-3a92-ae5d-c6f0a751335d",
                    "sense_definition": "Słodki, gęsty krem używany do przekładania lub dekorowania ciast i deserów",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "substance_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do tego tortu najlepiej pasuje <masa> orzechowa.",
                            "target_lang_translations": {
                                "ru": "К этому торту лучше всего подходит ореховая <масса>."
                            }
                        },
                        {
                            "text": "Musisz ucierać <masę>, aż cukier całkowicie się rozpuści.",
                            "target_lang_translations": {
                                "ru": "Ты должен растирать <массу>, пока сахар полностью не растворится."
                            }
                        }
                    ],
                    "common_phrases": [
                        "masa tortowa",
                        "masa makowa",
                        "masa krówkowa",
                        "masa czekoladowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкий густой крем, используемый для прослойки или украшения тортов и десертов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "масса",
                                "target_lang_sense_clarification": "кондитерская"
                            },
                            {
                                "target_lang_word": "крем",
                                "target_lang_sense_clarification": "начинка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a36e86f-b98b-3528-8431-1fc100c21d14",
                    "sense_definition": "Podstawowa wielkość fizyczna określająca ilość materii w obiekcie oraz jego bezwładność",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physics_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W stanie nieważkości <masa> ciała pozostaje taka sama, choć ciężar znika.",
                            "target_lang_translations": {
                                "ru": "В состоянии невесомости <масса> тела остается той же, хотя вес исчезает."
                            }
                        },
                        {
                            "text": "Nauczyciel fizyki wyjaśnił nam różnicę między <masą> a ciężarem.",
                            "target_lang_translations": {
                                "ru": "Учитель физики объяснил нам разницу между <массой> и весом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "masa ciała",
                        "masa atomowa",
                        "masa całkowita",
                        "masa słońca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая величина, определяющая количество материи в теле и его инерцию"

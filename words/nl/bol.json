                {
                    "sense_id": "5d945618-fc1c-37dc-aefb-9fd6b0fee17d",
                    "sense_definition": "Een rond, zacht broodje.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik eet graag een bruine <w>bol</w> met kaas als lunch.",
                            "target_lang_translations": {
                                "en": "I like to eat a brown <w>roll</w> with cheese for lunch.",
                                "ru": "Я люблю есть на обед черную <w>булочку</w> с сыром."
                            }
                        },
                        {
                            "text": "Bij de bakker kocht hij een zak met witte <w>bollen</w>.",
                            "target_lang_translations": {
                                "en": "At the bakery he bought a bag of white <w>buns</w>.",
                                "ru": "В пекарне он купил пакет белых <w>булочек</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een bruine bol",
                        "een witte bol",
                        "belegde bol"
                    ],
                    "target_lang_definitions": {
                        "en": "A round, soft bread roll.",
                        "ru": "Круглая мягкая булочка."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "roll",
                                "target_lang_sense_clarification": "bread"
                            },
                            {
                                "target_lang_word": "bun",
                                "target_lang_sense_clarification": "bread"
                            }

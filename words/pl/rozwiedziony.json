            "senses": [
                {
                    "sense_id": "71dbe9ea-3a18-384c-8cce-968f9d9fc4ab",
                    "sense_definition": "Mężczyzna, który nie jest już w związku małżeńskim z powodu rozwodu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "marital_status_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>rozwiedziony</w> regularnie spotyka się ze swoimi dziećmi.",
                            "target_lang_translations": {
                                "ru": "Этот <w>разведённый</w> регулярно встречается со своими детьми."
                            }

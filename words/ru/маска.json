                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Моего нового знакомого зовут <Маска>.",
                            "target_lang_translations": {
                                "en": "My new acquaintance is named <Maska>."
                            }
                        },
                        {
                            "text": "В списке приглашенных была девушка по имени <Маска>.",
                            "target_lang_translations": {
                                "en": "There was a girl named <Maska> on the guest list."
                            }
                        }
                    ],
                    "target_lang_definitions": {

{
    "word_family": [
        "journeyer",
        "journeyman"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dbe1f93d-9ead-3cb3-89e2-1534a3ae59a5",
                    "sense_definition": "A set amount of travelling, seen as a single unit; a discrete trip or voyage.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "travel_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>journey</w> to London takes two hours by train.",
                            "target_lang_translations": {
                                "ru": "<w>Поездка</w> в Лондон занимает два часа на поезде."
                            }
                        },
                        {
                            "text": "We packed our bags for the long <w>journey</w> home.",
                            "target_lang_translations": {
                                "ru": "Мы собрали сумки в долгий <w>путь</w> домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "trip"
                    ],
                    "common_phrases": [
                        "safe journey",
                        "long journey",
                        "train journey",
                        "return journey"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поездка или перемещение из одного места в другое, часто на значительное расстояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путешествие",
                                "target_lang_sense_clarification": "длительное или значимое перемещение"
                            },
                            {
                                "target_lang_word": "поездка",
                                "target_lang_sense_clarification": "перемещение на транспорте, обычно с конкретной целью"
                            },
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "процесс перемещения, дорога"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60239739-784e-388f-a9c8-1433c6169382",
                    "sense_definition": "Any process or progression likened to a journey, especially one that involves difficulties or personal development.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "travel_metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her <w>journey</w> to recovery was difficult but rewarding.",
                            "target_lang_translations": {
                                "ru": "Её <w>путь</w> к выздоровлению был трудным, но принес плоды."
                            }
                        },
                        {
                            "text": "Learning a new language is a long <w>journey</w>.",
                            "target_lang_translations": {
                                "ru": "Изучение нового языка — это долгое <w>путешествие</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spiritual journey",
                        "life journey",
                        "emotional journey"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс развития или изменений, часто сопряженный с трудностями или личностным ростом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "жизненный этап или процесс развития"
                            },
                            {
                                "target_lang_word": "путешествие",
                                "target_lang_sense_clarification": "метафорическое значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fa9b6db8-e3ac-330e-b919-82b34f42763e",
                    "sense_definition": "To travel; to make a trip or voyage.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "travel_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>journeyed</w> through the forest for three days.",
                            "target_lang_translations": {
                                "ru": "Они <w>путешествовали</w> по лесу три дня."
                            }
                        },
                        {
                            "text": "Many people <w>journey</w> to the coast for their holidays.",
                            "target_lang_translations": {
                                "ru": "Многие люди <w>едут</w> на побережье, чтобы провести отпуск."
                            }
                        }
                    ],
                    "synonyms": [
                        "wayfare"
                    ],
                    "common_phrases": [
                        "journey through",
                        "journey back"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершать путешествие, ехать или идти куда-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путешествовать",
                                "target_lang_sense_clarification": "совершать поездку"
                            },
                            {
                                "target_lang_word": "странствовать",
                                "target_lang_sense_clarification": "книжное, обычно о долгих скитаниях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a635e4f0-f7de-3605-8784-3c80e2921c34",
                    "sense_definition": "A female given name from English.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My friend <w>Journey</w> is coming to visit us tomorrow.",
                            "target_lang_translations": {
                                "ru": "Моя подруга <w>Джорни</w> приедет навестить нас завтра."
                            }
                        },
                        {
                            "text": "They decided to name their daughter <w>Journey</w>.",
                            "target_lang_translations": {
                                "ru": "Они решили назвать дочь <w>Джорни</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя английского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Джорни",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
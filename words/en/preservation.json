            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f075e87d-0221-38ee-8be1-b926727b00d0",
                    "sense_definition": "The act of keeping something in existence or protecting it from harm, loss, or decay.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>preservation</w> of historic buildings is vital for maintaining our cultural heritage.",
                            "target_lang_translations": {
                                "ru": "<w>Сохранение</w> исторических зданий жизненно важно для поддержания нашего культурного наследия."
                            }
                        },
                        {
                            "text": "Scientists are working on the <w>preservation</w> of endangered species in the rainforest.",
                            "target_lang_translations": {
                                "ru": "Ученые работают над <w>сохранением</w> исчезающих видов в тропических лесах."
                            }
                        }
                    ],
                    "synonyms": [
                        "conservation",
                        "protection",
                        "safeguarding",
                        "maintenance"
                    ],
                    "antonyms": [
                        "destruction",
                        "neglect",
                        "ruin"
                    ],
                    "common_phrases": [
                        "wildlife preservation",
                        "food preservation",
                        "historic preservation",
                        "self-preservation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по сохранению чего-либо, защите от порчи, разрушения или исчезновения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сохранение",
                                "target_lang_sense_clarification": "общий смысл"
                            },

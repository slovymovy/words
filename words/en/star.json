    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c45d660a-7307-32be-8546-390186ea3876",
                    "sense_definition": "Any small, natural and bright dot in the sky, visible at night.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Look at that bright <w>star</w> in the northern sky.",
                            "target_lang_translations": {
                                "ru": "Посмотри на ту яркую <w>звезду</w> на северном небе."
                            }
                        },
                        {
                            "text": "We lay on the grass and watched the shooting <w>star</w> pass by.",
                            "target_lang_translations": {
                                "ru": "Мы лежали на траве и наблюдали за падающей <w>звездой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shooting star",
                        "bright star",
                        "morning star",
                        "north star"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небесное тело, видимое ночью как светящаяся точка."
                    },
                    "translations": {

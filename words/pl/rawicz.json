                {
                    "sense_id": "7248a378-aa0e-3000-83b1-629b2e3b0f7e",
                    "sense_definition": "Miasto powiatowe w zachodniej Polsce, położone w województwie wielkopolskim.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "places",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Rozwój <w>Rawicza</w> przerwała jednak wojna Napoleona z Rosją.",
                            "target_lang_translations": {
                                "ru": "Развитие <w>Равича</w> прервала, однако, война Наполеона с Россией."
                            }
                        },
                        {
                            "text": "W <w>Rawiczu</w> znajduje się zabytkowy rynek oraz znany zakład karny.",
                            "target_lang_translations": {
                                "ru": "В <w>Равиче</w> находится старинная рыночная площадь, а также известная тюрьма."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gmina Rawicz",
                        "powiat rawicki",
                        "zakład karny w Rawiczu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Районный город в западной Польше, расположенный в Великопольском воеводстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Равич",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "470f38e4-755e-3107-ac25-50716378ab83",
                    "sense_definition": "Polski herb szlachecki przedstawiający pannę w koronie siedzącą na czarnym niedźwiedziu.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "Na herb <w>Rawicz</w> miała z dużym prawdopodobieństwem wpływ zachodnioeuropejska heraldyka.",
                            "target_lang_translations": {
                                "ru": "На герб <w>Равич</w>, по всей вероятности, оказала влияние западноевропейская геральдика."
                            }
                        },
                        {
                            "text": "Wielu polskich szlachciców pieczętowało się herbem <w>Rawicz</w>.",
                            "target_lang_translations": {
                                "ru": "Многие польские дворяне использовали герб <w>Равич</w> на своих печатях."
                            }
                        }
                    ],
                    "common_phrases": [

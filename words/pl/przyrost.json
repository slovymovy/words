    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7b7b446d-a651-32f7-af34-7f1795d8add9",
                    "sense_definition": "Wzrost ilości, liczby lub wielkości czegoś; to, co przybyło.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prawidłowy <w>przyrost</w> wagi niemowlaka zależy od częstości karmienia.",
                            "target_lang_translations": {
                                "ru": "Правильная <w>прибавка</w> в весе младенца зависит от частоты кормления."
                            }
                        },
                        {
                            "text": "W ostatnich latach obserwujemy znaczny <w>przyrost</w> liczby samochodów na drogach.",
                            "target_lang_translations": {
                                "ru": "В последние годы мы наблюдаем значительный <w>прирост</w> количества автомобилей на дорогах."
                            }
                        }
                    ],
                    "synonyms": [
                        "wzrost"
                    ],
                    "common_phrases": [
                        "przyrost naturalny",
                        "przyrost wagi",
                        "przyrost ludności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличение количества, числа или размера чего-либо; то, что прибавилось."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прирост",
                                "target_lang_sense_clarification": "увеличение количества"
                            },
                            {
                                "target_lang_word": "рост",
                                "target_lang_sense_clarification": "процесс увеличения"
                            },
                            {
                                "target_lang_word": "прибавка",
                                "target_lang_sense_clarification": "веса, зарплаты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "129024cc-f8d1-3873-b737-ddb5134e32fa",
                    "sense_definition": "Różnica pomiędzy wartością końcową a początkową jakiejś wielkości.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lekcji fizyki liczyliśmy <w>przyrost</w> prędkości spadającego ciała.",
                            "target_lang_translations": {
                                "ru": "На уроке физики мы считали <w>приращение</w> скорости падающего тела."
                            }
                        },
                        {
                            "text": "Inwestorzy analizują roczny <w>przyrost</w> wartości akcji tej spółki.",
                            "target_lang_translations": {
                                "ru": "Инвесторы анализируют годовой <w>прирост</w> стоимости акций этой компании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przyrost funkcji",
                        "przyrost prędkości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разница между конечным и начальным значением какой-либо величины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приращение",
                                "target_lang_sense_clarification": "математический или физический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41b980dc-4822-3a45-97d1-e7fd78434158",
                    "sense_definition": "Część spadku otrzymana przez spadkobiercę w wyniku zrzeczenia się jej przez innych uprawnionych.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kancelaria notarialna potwierdziła <w>przyrost</w> udziałów w spadku dla mojej siostry.",
                            "target_lang_translations": {
                                "ru": "Нотариальная контора подтвердила <w>приращение</w> долей в наследстве для моей сестры."
                            }
                        },
                        {
                            "text": "Dzięki instytucji zwanej <w>przyrostem</w> otrzymał on większą część majątku po zmarłym.",
                            "target_lang_translations": {
                                "ru": "Благодаря институту, называемому <w>приращением</w>, он получил большую часть имущества умершего."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть наследства, полученная наследником в результате отказа от нее других правомочных лиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приращение",
                                "target_lang_sense_clarification": "юридический термин, наследство"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "45217484-1301-3bc1-a9a2-008fa9e16b71",
                    "sense_definition": "A sweet, viscous, gold-colored fluid produced from plant nectar by bees, often consumed by humans.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I like to put a spoonful of <w>honey</w> in my tea instead of sugar.",
                            "target_lang_translations": {
                                "ru": "Я люблю класть ложку <w>мёда</w> в чай вместо сахара."
                            }
                        },
                        {
                            "text": "The bear climbed the tree to steal <w>honey</w> from the hive.",
                            "target_lang_translations": {
                                "ru": "Медведь залез на дерево, чтобы украсть <w>мёд</w> из улья."
                            }
                        }
                    ],
                    "common_phrases": [
                        "honey bee",
                        "jar of honey",
                        "milk and honey"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкая, вязкая жидкость золотистого цвета, вырабатываемая пчелами из нектара растений, часто употребляемая в пищу людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мёд",
                                "target_lang_sense_clarification": "продукт питания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6248cd2-837f-3c98-b761-9a5fc7e94449",
                    "sense_definition": "A term of affection used for a loved one, such as a partner or child.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "affection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hi <w>honey</w>, could you help me with the groceries?",
                            "target_lang_translations": {
                                "ru": "Привет, <w>милый</w>, не мог бы ты помочь мне с продуктами?"
                            }
                        },
                        {
                            "text": "Don't worry, <w>honey</w>, everything is going to be alright.",
                            "target_lang_translations": {
                                "ru": "Не волнуйся, <w>милая</w>, всё будет хорошо."
                            }
                        }
                    ],
                    "synonyms": [
                        "darling",
                        "sweetie",
                        "sweetheart"
                    ],
                    "common_phrases": [
                        "hi honey",
                        "thanks honey"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое обращение к любимому человеку, например, к партнеру или ребенку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "милый / милая",
                                "target_lang_sense_clarification": "обращение к мужчине или женщине"
                            },
                            {
                                "target_lang_word": "дорогой / дорогая",
                                "target_lang_sense_clarification": "стандартное ласковое обращение"
                            },
                            {
                                "target_lang_word": "солнышко",
                                "target_lang_sense_clarification": "очень распространенное ласковое обращение"
                            },
                            {
                                "target_lang_word": "душенька",
                                "target_lang_sense_clarification": "устаревшее или ироничное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc935290-519f-32cd-b027-5c6777f0ead2",
                    "sense_definition": "A pale yellow to brownish-yellow color, resembling the substance produced by bees.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The walls of the kitchen were painted in a warm <w>honey</w>.",
                            "target_lang_translations": {
                                "ru": "Стены кухни были покрашены в теплый <w>медовый цвет</w>."
                            }
                        },
                        {
                            "text": "She chose a rich <w>honey</w> for the curtains to brighten the room.",
                            "target_lang_translations": {
                                "ru": "Она выбрала насыщенный <w>цвет мёда</w> для штор, чтобы осветлить комнату."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бледно-желтый или коричневато-желтый цвет, напоминающий вещество, производимое пчелами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медовый цвет",
                                "target_lang_sense_clarification": "оттенок"
                            },
                            {
                                "target_lang_word": "цвет мёда",
                                "target_lang_sense_clarification": "описание цвета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d76a03a-05db-346c-91f9-0721c71e3f44",
                    "sense_definition": "An attractive woman.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He went to the party hoping to meet some <w>honeys</w>.",
                            "target_lang_translations": {
                                "ru": "Он пошел на вечеринку в надежде встретить каких-нибудь <w>красоток</w>."
                            }
                        },
                        {
                            "text": "That car is a real <w>honey</w>.",
                            "target_lang_translations": {
                                "ru": "Эта машина — настоящая <w>конфетка</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Привлекательная женщина или что-то превосходное (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красотка",
                                "target_lang_sense_clarification": "о женщине"
                            },
                            {
                                "target_lang_word": "милашка",
                                "target_lang_sense_clarification": "о девушке"
                            },
                            {
                                "target_lang_word": "конфетка",
                                "target_lang_sense_clarification": "о предмете или девушке (перен.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a2e5eea-fe56-34e1-b75d-91dc67d4918d",
                    "sense_definition": "Something sweet, desirable, or pleasant.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He enjoyed the <w>honey</w> of success after years of hard work.",
                            "target_lang_translations": {
                                "ru": "Он наслаждался <w>сладостью</w> успеха после лет упорного труда."
                            }
                        },
                        {
                            "text": "The poet wrote about the <w>honey</w> of her words.",
                            "target_lang_translations": {
                                "ru": "Поэт писал о <w>мёде</w> её слов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often poetic or literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то сладкое, желанное или приятное (метафорически)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сладость",
                                "target_lang_sense_clarification": "приятное ощущение"
                            },
                            {
                                "target_lang_word": "мёд",
                                "target_lang_sense_clarification": "метафора удовольствия"
                            }
                        ]
                    }
                }
            ]
        },
        {

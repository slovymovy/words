                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pospiesz się trochę, nie bądź takim <w>ślimakiem</w>!",
                            "target_lang_translations": {
                                "ru": "Поторопись немного, не будь такой <w>копушей</w>!"

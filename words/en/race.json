    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f022bc96-eb75-3c02-8657-f9f23734f203",
                    "sense_definition": "A competition between people, animals, or vehicles to see which is the fastest to reach a finish line.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "speed_contest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The horse <w>race</w> will start at three o'clock in the afternoon.",
                            "target_lang_translations": {
                                "ru": "Лошадиные <w>скачки</w> начнутся в три часа дня."
                            }
                        },
                        {
                            "text": "He trained very hard to win the <w>race</w> against his rivals.",
                            "target_lang_translations": {
                                "ru": "Он очень усердно тренировался, чтобы выиграть <w>гонку</w> у своих соперников."
                            }
                        }
                    ],
                    "synonyms": [
                        "competition",
                        "contest",
                        "dash"
                    ],
                    "common_phrases": [
                        "win the race",
                        "horse race",
                        "run a race",
                        "rat race"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соревнование между людьми, животными или транспортными средствами, чтобы определить, кто быстрее достигнет финиша."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гонка",
                                "target_lang_sense_clarification": "общее название состязания в скорости"
                            },
                            {
                                "target_lang_word": "забег",
                                "target_lang_sense_clarification": "соревнование в беге"
                            },
                            {
                                "target_lang_word": "скачки",
                                "target_lang_sense_clarification": "конные соревнования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e93fd49b-7b47-319e-bf58-82ab0f13879a",
                    "sense_definition": "A large group of people distinguished from others on the basis of common physical characteristics or ancestry.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ancestry_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "People of every <w>race</w> and religion live together in this city.",
                            "target_lang_translations": {
                                "ru": "Люди всех <w>рас</w> и религий живут вместе в этом городе."
                            }
                        },
                        {
                            "text": "The form asks for information about your age, gender, and <w>race</w>.",
                            "target_lang_translations": {
                                "ru": "В анкете запрашивается информация о вашем возрасте, полe и <w>расе</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ethnicity",
                        "ethnic group",
                        "people"
                    ],
                    "common_phrases": [
                        "human race",
                        "mixed race",
                        "race relations"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая группа людей, отличающаяся от других общими физическими признаками или происхождением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раса",
                                "target_lang_sense_clarification": "антропологическая группа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fe92918-0216-3707-b68c-cf435200e2c0",
                    "sense_definition": "A distinct group of living beings, often used in fiction to describe different species like elves, dwarves, or aliens.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "ancestry_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The story features a <w>race</w> of giants living in the mountains.",
                            "target_lang_translations": {
                                "ru": "В истории рассказывается о <w>расе</w> гигантов, живущих в горах."
                            }
                        },
                        {
                            "text": "Humans are not the only intelligent <w>race</w> in this science fiction universe.",
                            "target_lang_translations": {
                                "ru": "Люди — не единственная разумная <w>раса</w> в этой научно-фантастической вселенной."
                            }
                        }
                    ],
                    "synonyms": [
                        "species",
                        "breed"
                    ],
                    "common_phrases": [
                        "alien race",
                        "ancient race"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in fantasy or sci-fi contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная группа живых существ, часто используемая в фантастике для описания различных видов, таких как эльфы, гномы или инопланетяне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раса",
                                "target_lang_sense_clarification": "вид существ в фантастике"
                            },
                            {
                                "target_lang_word": "народ",
                                "target_lang_sense_clarification": "в значении племени или вида"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39cfeb75-e6e8-3dc8-9a33-0181ec18d6fc",
                    "sense_definition": "A strong current of water, or a channel built to carry water to a mill.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "water_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The water flows swiftly through the mill <w>race</w> to turn the wheel.",
                            "target_lang_translations": {
                                "ru": "Вода стремительно течет через мельничный <w>канал</w>, вращая колесо."
                            }
                        },
                        {
                            "text": "Swimmers were warned to stay away from the dangerous tidal <w>race</w>.",
                            "target_lang_translations": {
                                "ru": "Пловцов предупредили держаться подальше от опасного приливного <w>течения</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "channel",
                        "current"
                    ],
                    "common_phrases": [
                        "mill race",
                        "tidal race"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often refers to historical watermills"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный поток воды или канал, построенный для подачи воды на мельницу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канал",
                                "target_lang_sense_clarification": "искусственное русло"
                            },
                            {
                                "target_lang_word": "быстрое течение",
                                "target_lang_sense_clarification": "стремительный поток воды"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5b76045e-72e1-3b29-a2d8-a077a4ae3f38",
                    "sense_definition": "To compete against someone or something to see who is faster.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "speed_contest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will <w>race</w> you to the big tree at the end of the park.",
                            "target_lang_translations": {
                                "ru": "Я <w>побегу наперегонки</w> с тобой до большого дерева в конце парка."
                            }
                        },
                        {
                            "text": "The two drivers will <w>race</w> each other for the championship trophy.",
                            "target_lang_translations": {
                                "ru": "Два гонщика будут <w>соревноваться</w> друг с другом за чемпионский кубок."
                            }
                        }
                    ],
                    "synonyms": [
                        "compete"
                    ],
                    "common_phrases": [
                        "race against"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соревноваться с кем-то или чем-то, чтобы выяснить, кто быстрее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соревноваться",
                                "target_lang_sense_clarification": "в скорости"
                            },
                            {
                                "target_lang_word": "гоняться",
                                "target_lang_sense_clarification": "разговорное, бежать наперегонки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4d46288-969d-3dd3-8532-83ebc786a292",
                    "sense_definition": "To move or go very quickly; to rush.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "speed_contest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We had to <w>race</w> to the airport to catch our flight on time.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <w>мчаться</w> в аэропорт, чтобы успеть на рейс вовремя."
                            }
                        },
                        {
                            "text": "Her heart began to <w>race</w> when she heard the sudden noise.",
                            "target_lang_translations": {
                                "ru": "Её сердце начало бешено <w>колотиться</w>, когда она услышала внезапный шум."
                            }
                        }
                    ],
                    "synonyms": [
                        "rush",
                        "dash",
                        "hurry",
                        "speed",
                        "zoom"
                    ],
                    "antonyms": [
                        "crawl"
                    ],
                    "common_phrases": [
                        "race against time",
                        "heart is racing",
                        "race home"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться или идти очень быстро; спешить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мчаться",
                                "target_lang_sense_clarification": "очень быстро ехать или бежать"
                            },
                            {
                                "target_lang_word": "нестись",
                                "target_lang_sense_clarification": "стремительно двигаться"
                            },
                            {
                                "target_lang_word": "спешить",
                                "target_lang_sense_clarification": "торопиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8aec605-be1c-35b6-9699-de6df8312140",
                    "sense_definition": "Of an engine, to run very fast, often uncontrollably or when not in gear.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "speed_contest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car engine started to <w>race</w> loudly when I stepped on the gas pedal.",
                            "target_lang_translations": {
                                "ru": "Двигатель машины начал громко <w>реветь</w>, когда я нажал на педаль газа."
                            }
                        },
                        {
                            "text": "He could hear the motor <w>race</w> in the garage as he tried to fix it.",
                            "target_lang_translations": {
                                "ru": "Он слышал, как мотор <w>работает на высоких оборотах</w> в гараже, пока пытался его починить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "engine racing"
                    ],
                    "target_lang_definitions": {
                        "ru": "О двигателе: работать очень быстро, часто неконтролируемо или на холостом ходу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реветь",
                                "target_lang_sense_clarification": "издавать громкий звук на высоких оборотах"
                            },
                            {
                                "target_lang_word": "работать на высоких оборотах",
                                "target_lang_sense_clarification": "техническое описание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "9de836f9-4c69-3f7d-a038-0bf09571fae2",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Race</w> signed the documents at the meeting yesterday.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "38d0058b-4157-3b81-8fbf-ffdebabe395f",
                    "sense_definition": "Стремительно текущая масса воды или воздуха.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Горный <w>поток</w> шумел где-то внизу в ущелье.",
                            "target_lang_translations": {
                                "en": "A mountain <w>stream</w> murmured somewhere down in the gorge."
                            }
                        },
                        {
                            "text": "Сильный воздушный <w>поток</w> сорвал шляпу с моей головы.",
                            "target_lang_translations": {
                                "en": "A strong air <w>current</w> blew the hat off my head."
                            }
                        }
                    ],
                    "synonyms": [
                        "течение",
                        "ток"
                    ],
                    "common_phrases": [
                        "бурный поток",
                        "воздушный поток",
                        "против потока",
                        "поток воды"
                    ],
                    "target_lang_definitions": {
                        "en": "A rapidly flowing mass of water or air."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stream",
                                "target_lang_sense_clarification": "a body of moving water"
                            },
                            {
                                "target_lang_word": "current",
                                "target_lang_sense_clarification": "force of moving water or air"
                            },
                            {
                                "target_lang_word": "flow",
                                "target_lang_sense_clarification": "movement of fluid or gas"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2087887-75be-3ff0-a4bb-150203927347",
                    "sense_definition": "Непрерывное движение большого количества людей, машин или других объектов.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В час пик на дорогах образуется огромный <w>поток</w> машин.",
                            "target_lang_translations": {
                                "en": "During rush hour, a huge <w>flow</w> of cars forms on the roads."
                            }
                        },
                        {
                            "text": "Бесконечный <w>поток</w> туристов мешал нам пройти к музею.",
                            "target_lang_translations": {
                                "en": "An endless <w>stream</w> of tourists prevented us from getting to the museum."
                            }
                        }
                    ],
                    "common_phrases": [
                        "поток людей",
                        "поток машин",
                        "транспортный поток",
                        "денежный поток"
                    ],
                    "target_lang_definitions": {
                        "en": "Continuous movement of a large number of people, vehicles, or other objects."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flow",
                                "target_lang_sense_clarification": "steady movement of traffic or people"
                            },
                            {
                                "target_lang_word": "stream",
                                "target_lang_sense_clarification": "continuous succession of moving objects"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd4509c8-8326-3174-9e75-78bf7c0c074a",
                    "sense_definition": "Непрерывная последовательность событий, слов, чувств или информации.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не мог остановить этот бесконечный <w>поток</w> жалоб.",
                            "target_lang_translations": {
                                "en": "I couldn't stop this endless <w>stream</w> of complaints."
                            }
                        },
                        {
                            "text": "На нас обрушился <w>поток</w> новостей из интернета.",
                            "target_lang_translations": {
                                "en": "A <w>flood</w> of news from the internet crashed down on us."
                            }
                        }
                    ],
                    "synonyms": [
                        "череда",
                        "ряд",
                        "последовательность"
                    ],
                    "common_phrases": [
                        "поток слов",
                        "поток сознания",
                        "поток информации",
                        "поток слёз"
                    ],
                    "target_lang_definitions": {
                        "en": "A continuous sequence of events, words, feelings, or information."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stream",
                                "target_lang_sense_clarification": "uninterrupted succession (e.g., of consciousness)"
                            },
                            {
                                "target_lang_word": "flow",
                                "target_lang_sense_clarification": "continuous output (e.g., of information)"
                            },
                            {
                                "target_lang_word": "torrent",
                                "target_lang_sense_clarification": "overwhelming amount (e.g., of words/abuse)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05af9b46-d948-3f77-a034-fd8a2c0c96e2",
                    "sense_definition": "Объединение нескольких учебных групп одной специальности для проведения общих занятий.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На нашем <w>потоке</w> учится более ста студентов.",
                            "target_lang_translations": {
                                "en": "There are over a hundred students in our <w>cohort</w>."
                            }
                        },
                        {
                            "text": "Профессор читал лекцию сразу для всего <w>потока</w>.",
                            "target_lang_translations": {
                                "en": "The professor gave a lecture to the entire <w>year</w> at once."
                            }
                        }
                    ],
                    "common_phrases": [
                        "староста потока",
                        "весь поток",
                        "студенческий поток"
                    ],
                    "target_lang_definitions": {
                        "en": "A large group of students from the same year attending shared lectures."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cohort",
                                "target_lang_sense_clarification": "group of students starting together"
                            },
                            {
                                "target_lang_word": "year",
                                "target_lang_sense_clarification": "all students in the same year of study"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77343faa-4018-3713-969c-b107616ad1f0",
                    "sense_definition": "Последовательность данных или команд, обрабатываемых компьютерной программой.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Программа не смогла обработать входящий <w>поток</w> данных.",
                            "target_lang_translations": {
                                "en": "The program could not process the incoming data <w>stream</w>."
                            }
                        },
                        {
                            "text": "Ошибка произошла в основном <w>потоке</w> приложения.",
                            "target_lang_translations": {
                                "en": "The error occurred in the main <w>thread</w> of the application."
                            }
                        }
                    ],
                    "common_phrases": [
                        "поток данных",
                        "многопоточный режим",
                        "входной поток"
                    ],
                    "target_lang_definitions": {
                        "en": "A sequence of data or commands processed by a computer program."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stream",
                                "target_lang_sense_clarification": "sequence of data elements"
                            },
                            {
                                "target_lang_word": "thread",
                                "target_lang_sense_clarification": "sequence of programmed instructions"
                            },
                            {
                                "target_lang_word": "flow",
                                "target_lang_sense_clarification": "transfer of data"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "70c03efe-2e08-347a-91e9-239a13cb4968",
                    "sense_definition": "The quality of being based on personal feelings, tastes, or opinions rather than external facts.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is always an element of <w>subjectivity</w> when judging art competitions.",
                            "target_lang_translations": {
                                "ru": "При судействе художественных конкурсов всегда присутствует элемент <w>субъективности</w>."
                            }
                        },
                        {
                            "text": "The <w>subjectivity</w> of his report made it difficult to determine what actually happened.",
                            "target_lang_translations": {
                                "ru": "<w>Субъективность</w> его отчета затрудняла определение того, что произошло на самом деле."
                            }
                        }
                    ],
                    "synonyms": [
                        "subjectiveness"
                    ],
                    "antonyms": [
                        "objectivity"
                    ],
                    "common_phrases": [
                        "degree of subjectivity",
                        "inherent subjectivity",
                        "element of subjectivity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство опираться на личные чувства, вкусы или мнения, а не на объективные факты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "субъективность",
                                "target_lang_sense_clarification": "свойство или качество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f4e7615-6198-3418-8726-8fd9898298f0",
                    "sense_definition": "A specific personal perspective, thought, or feeling, often regarding identity or social consciousness.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author explores the conflicting <w>subjectivities</w> of the characters in the novel.",
                            "target_lang_translations": {
                                "ru": "Автор исследует противоречивые <w>субъективности</w> персонажей романа."
                            }
                        },
                        {
                            "text": "Sociologists study how collective <w>subjectivities</w> are formed within different communities.",
                            "target_lang_translations": {
                                "ru": "Социологи изучают, как коллективные <w>субъективности</w> формируются в различных сообществах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "collective subjectivities",
                        "political subjectivities"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфическая личная точка зрения, мышление или чувство, часто в контексте самосознания или социальной идентичности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "субъективность",
                                "target_lang_sense_clarification": "философское или психологическое понятие"
                            },
                            {
                                "target_lang_word": "субъектность",
                                "target_lang_sense_clarification": "способность быть субъектом, носителем действия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

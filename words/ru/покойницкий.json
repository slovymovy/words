    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4a748b9d-625d-305e-b0ae-e55cea6bda58",
                    "sense_definition": "Относящийся к умершему человеку, принадлежащий ему или связанный с ним.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "relational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В заброшенном доме стоял тяжёлый и неприятный <покойницкий> запах.",
                            "target_lang_translations": {
                                "en": "There was a heavy and unpleasant <cadaverous> smell in the abandoned house."
                            }
                        },
                        {
                            "text": "Родственники начали разбирать <покойницкие> вещи сразу после похорон.",
                            "target_lang_translations": {
                                "en": "The relatives started sorting through the <deceased's> things immediately after the funeral."
                            }
                        }
                    ],
                    "synonyms": [
                        "покойничий"
                    ],
                    "common_phrases": [
                        "покойницкий запах",
                        "покойницкий стол"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to a deceased person, belonging to them, or connected with them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deceased's",
                                "target_lang_sense_clarification": "belonging to the person who has died"
                            },
                            {
                                "target_lang_word": "dead person's",
                                "target_lang_sense_clarification": "belonging to or relating to a dead person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04ee7431-ae3e-32cc-9212-7dfd47371193",
                    "sense_definition": "Напоминающий вид мёртвого человека; мертвенно-бледный, холодный или неподвижный.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "qualitative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На его лице внезапно появилась пугающая <покойницкая> бледность.",
                            "target_lang_translations": {
                                "en": "A frightening <deathly> pallor suddenly appeared on his face."
                            }
                        },
                        {
                            "text": "Руки пациента были холодными и имели совершенно <покойницкий> вид.",
                            "target_lang_translations": {
                                "en": "The patient's hands were cold and had a completely <corpse-like> appearance."
                            }
                        }
                    ],
                    "synonyms": [
                        "мертвенный",
                        "безжизненный"
                    ],
                    "antonyms": [
                        "живой",
                        "цветущий"
                    ],
                    "common_phrases": [
                        "покойницкая бледность",
                        "покойницкий вид",
                        "покойницкая холодность"
                    ],
                    "target_lang_definitions": {
                        "en": "Resembling the appearance of a dead person; deathly pale, cold, or motionless."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cadaverous",
                                "target_lang_sense_clarification": "resembling a corpse in being very pale, thin, or bony"
                            },
                            {
                                "target_lang_word": "deathly",
                                "target_lang_sense_clarification": "suggestive of death (e.g., pallor or silence)"
                            },
                            {
                                "target_lang_word": "corpse-like",
                                "target_lang_sense_clarification": "having the appearance or characteristics of a dead body"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
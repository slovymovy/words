            "senses": [
                {
                    "sense_id": "3700523e-5283-3cca-93b6-e8acf6eaf7ce",
                    "sense_definition": "Przejście zawodnika z jednego klubu sportowego do innego na podstawie umowy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sports_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klub ogłosił głośny <w>transfer</w> nowego napastnika z ligi hiszpańskiej.",
                            "target_lang_translations": {
                                "ru": "Клуб объявил о громком <w>трансфере</w> нового нападающего из испанской лиги."
                            }
                        },
                        {
                            "text": "Letnie okno transferowe to czas, kiedy każdy <w>transfer</w> budzi wielkie emocje.",
                            "target_lang_translations": {
                                "ru": "Летнее трансферное окно — это время, когда каждый <w>трансфер</w> вызывает большие эмоции."
                            }
                        }
                    ],
                    "synonyms": [
                        "przesiadka"
                    ],
                    "common_phrases": [
                        "okno transferowe",
                        "kwota transferu",
                        "lista transferowa",
                        "transfer definitywny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переход игрока из одного спортивного клуба в другой на основании контракта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трансфер",
                                "target_lang_sense_clarification": "в спорте"
                            },
                            {
                                "target_lang_word": "переход",
                                "target_lang_sense_clarification": "смена клуба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73ac3112-d33e-3863-b96c-94cdd0f00f02",
                    "sense_definition": "Zorganizowany przewóz osób, najczęściej turystów, z lotniska do hotelu lub w inne miejsce docelowe.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biuro podróży zapewnia darmowy <w>transfer</w> z lotniska do hotelu.",
                            "target_lang_translations": {
                                "ru": "Турагентство предоставляет бесплатный <w>трансфер</w> из аэропорта в отель."
                            }
                        },
                        {
                            "text": "Musimy zamówić prywatny <w>transfer</w>, ponieważ przylatujemy w nocy.",
                            "target_lang_translations": {
                                "ru": "Нам нужно заказать частный <w>трансфер</w>, так как мы прилетаем ночью."
                            }
                        }
                    ],
                    "synonyms": [
                        "przewóz",
                        "transport"
                    ],
                    "common_phrases": [
                        "transfer z lotniska",
                        "transfer do hotelu",
                        "prywatny transfer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованная перевозка пассажиров, чаще всего туристов, из аэропорта в отель или другое место назначения."

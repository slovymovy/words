                        {
                            "text": "Please <w>bear</w> in mind that the deadline is tomorrow.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>имейте</w> в виду, что крайний срок завтра."
                            }
                        }
                    ],
                    "synonyms": [
                        "harbour",
                        "hold"
                    ],
                    "common_phrases": [
                        "bear a grudge",
                        "bear in mind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удерживать чувство или мысль в уме, особенно негативную."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таить",
                                "target_lang_sense_clarification": "скрывать в душе чувство (злобу, обиду)"

{
    "word_family": [
        "transportation",
        "transporter",
        "transportable",
        "transportability"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d8427371-8d00-36c0-839b-85f0228a17a3",
                    "sense_definition": "A system of vehicles, such as buses, trains, or planes, used for moving people or goods from one place to another.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We rely on public <w>transport</w> to get to work every day.",
                            "target_lang_translations": {
                                "ru": "Мы полагаемся на общественный <w>транспорт</w>, чтобы добираться на работу каждый день."
                            }
                        },
                        {
                            "text": "The city needs to improve its local <w>transport</w> network.",
                            "target_lang_translations": {
                                "ru": "Городу необходимо улучшить сеть местного <w>транспорта</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "public transport"
                    ],
                    "common_phrases": [
                        "public transport",
                        "local transport",
                        "air transport",
                        "road transport"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система транспортных средств, таких как автобусы, поезда или самолеты, используемая для перевозки людей или грузов из одного места в другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "транспорт",
                                "target_lang_sense_clarification": "система перевозок"
                            },
                            {
                                "target_lang_word": "общественный транспорт",
                                "target_lang_sense_clarification": "инфраструктура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30f2363e-be7d-3838-9398-2eb3be604a4f",
                    "sense_definition": "The action or process of moving people or goods from one place to another.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>transport</w> of heavy machinery requires special trucks.",
                            "target_lang_translations": {
                                "ru": "<w>Транспортировка</w> тяжелой техники требует специальных грузовиков."
                            }
                        },
                        {
                            "text": "They arranged for the safe <w>transport</w> of the paintings.",
                            "target_lang_translations": {
                                "ru": "Они организовали безопасную <w>перевозку</w> картин."
                            }
                        }
                    ],
                    "synonyms": [
                        "conveyance",
                        "moving",
                        "relocation"
                    ],
                    "common_phrases": [
                        "means of transport",
                        "transport of goods"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс перемещения людей или грузов из одного места в другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевозка",
                                "target_lang_sense_clarification": "действие по перемещению"
                            },
                            {
                                "target_lang_word": "транспортировка",
                                "target_lang_sense_clarification": "доставка грузов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61817a05-2c7b-384f-969c-4d4e4412688a",
                    "sense_definition": "A vehicle, especially a ship or aircraft, used to carry soldiers or supplies.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The military <w>transport</w> landed at the base with new supplies.",
                            "target_lang_translations": {
                                "ru": "Военный <w>транспорт</w> приземлился на базе с новыми припасами."
                            }
                        },
                        {
                            "text": "Soldiers boarded the <w>transport</w> heading for the island.",
                            "target_lang_translations": {
                                "ru": "Солдаты погрузились на <w>транспорт</w>, направляющийся на остров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "troop transport"
                    ],
                    "target_lang_definitions": {
                        "ru": "Транспортное средство, особенно корабль или самолет, используемое для перевозки войск или припасов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "транспорт",
                                "target_lang_sense_clarification": "военное судно или самолет"
                            },
                            {
                                "target_lang_word": "транспортное судно",
                                "target_lang_sense_clarification": "корабль обеспечения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84077261-c22f-36dd-bbbd-165a185b1f2b",
                    "sense_definition": "A state of overwhelming emotion, such as extreme joy or delight.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She watched the opera in a <w>transport</w> of delight.",
                            "target_lang_translations": {
                                "ru": "Она смотрела оперу в <w>порыве</w> восторга."
                            }
                        },
                        {
                            "text": "He was in a <w>transport</w> of rage when he heard the news.",
                            "target_lang_translations": {
                                "ru": "Он был в <w>порыве</w> ярости, когда услышал новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "rapture"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние сильного эмоционального возбуждения, например, восторга или ярости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порыв",
                                "target_lang_sense_clarification": "сильное проявление чувства"
                            },
                            {
                                "target_lang_word": "восторг",
                                "target_lang_sense_clarification": "состояние радости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0169f986-0ee1-3d9d-96ea-db4caba43ca3",
                    "sense_definition": "A convict who was sent to a penal colony in the past.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>transport</w> was sent to Australia to serve his sentence.",
                            "target_lang_translations": {
                                "ru": "<w>Каторжник</w> был отправлен в Австралию отбывать наказание."
                            }
                        },
                        {
                            "text": "Life was very hard for a <w>transport</w> in the colonies.",
                            "target_lang_translations": {
                                "ru": "Жизнь в колониях была очень тяжелой для <w>каторжника</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "deportee"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Осужденный, которого в прошлом отправляли в исправительную колонию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каторжник",
                                "target_lang_sense_clarification": "ссыльный преступник"
                            },
                            {
                                "target_lang_word": "ссыльный",
                                "target_lang_sense_clarification": "человек в ссылке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "616417f6-4a05-36f8-8232-46a7f5a997a5",
                    "sense_definition": "To take or carry people or goods from one place to another by means of a vehicle, aircraft, or ship.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Large ships <w>transport</w> oil across the ocean.",
                            "target_lang_translations": {
                                "ru": "Большие корабли <w>перевозят</w> нефть через океан."
                            }
                        },
                        {
                            "text": "The bus <w>transports</w> students to school every morning.",
                            "target_lang_translations": {
                                "ru": "Автобус <w>перевозит</w> учеников в школу каждое утро."
                            }
                        }
                    ],
                    "synonyms": [
                        "convey",
                        "move",
                        "ship",
                        "ferry"
                    ],
                    "common_phrases": [
                        "transport goods",
                        "transport passengers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать или перевозить людей или грузы из одного места в другое с помощью транспортного средства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевозить",
                                "target_lang_sense_clarification": "перемещать транспортом"
                            },
                            {
                                "target_lang_word": "транспортировать",
                                "target_lang_sense_clarification": "доставлять грузы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13aca1fc-42d5-3faa-ab1e-3ac276860aa3",
                    "sense_definition": "To cause someone to feel a strong emotion.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The beautiful music <w>transported</w> the audience to another world.",
                            "target_lang_translations": {
                                "ru": "Прекрасная музыка <w>перенесла</w> слушателей в другой мир."
                            }
                        },
                        {
                            "text": "She was <w>transported</w> with joy when she saw her family again.",
                            "target_lang_translations": {
                                "ru": "Она была <w>охвачена</w> радостью, когда снова увидела свою семью."
                            }
                        }
                    ],
                    "synonyms": [
                        "carry away",
                        "enrapture"
                    ],
                    "common_phrases": [
                        "transported with joy",
                        "transported with delight"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать у кого-либо сильные эмоции; переносить в воображаемый мир."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "захватывать",
                                "target_lang_sense_clarification": "о сильных чувствах"
                            },
                            {
                                "target_lang_word": "переносить",
                                "target_lang_sense_clarification": "метафорически перемещать"
                            },
                            {
                                "target_lang_word": "охватить",
                                "target_lang_sense_clarification": "овладеть (о чувстве)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d58bb22-30fb-30c8-a34e-a30506a98a0d",
                    "sense_definition": "To send a criminal to a penal colony in a distant land as punishment.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "historical_legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge decided to <w>transport</w> the thief to the colonies.",
                            "target_lang_translations": {
                                "ru": "Судья решил <w>сослать</w> вора в колонии."
                            }
                        },
                        {
                            "text": "Many convicts were <w>transported</w> for minor crimes in the nineteenth century.",
                            "target_lang_translations": {
                                "ru": "Многих осужденных <w>ссылали</w> за мелкие преступления в девятнадцатом веке."
                            }
                        }
                    ],
                    "synonyms": [
                        "deport",
                        "banish",
                        "exile"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправлять преступника в исправительную колонию в отдаленные земли в качестве наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ссылать",
                                "target_lang_sense_clarification": "отправлять в ссылку"
                            },
                            {
                                "target_lang_word": "сослать",
                                "target_lang_sense_clarification": "наказание (сов. вид)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
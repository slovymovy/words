    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "69da7579-a994-3b4f-8baa-b2e5adc71059",
                    "sense_definition": "Иметь сведения о ком-либо или чём-либо, обладать информацией или фактами.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cognitive_knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я точно <w>знаю</w>, где находится ближайшая станция метро.",
                            "target_lang_translations": {
                                "en": "I <w>know</w> exactly where the nearest metro station is."
                            }
                        },
                        {
                            "text": "Никто в классе не <w>знает</w> правильного ответа на этот вопрос.",
                            "target_lang_translations": {
                                "en": "No one in the class <w>knows</w> the correct answer to this question."
                            }
                        }
                    ],
                    "common_phrases": [
                        "знать правду",
                        "кто знает",
                        "насколько я знаю",
                        "дать знать"
                    ],
                    "target_lang_definitions": {
                        "en": "To possess information or facts about someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "know",
                                "target_lang_sense_clarification": "to possess information"
                            },
                            {
                                "target_lang_word": "be aware of",
                                "target_lang_sense_clarification": "to have knowledge of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "530d7702-4d0e-3ed4-a950-7fc66a8bdecb",
                    "sense_definition": "Быть знакомым с кем-либо, состоять в знакомстве, лично встречаться ранее.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "acquaintance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я давно <w>знаю</w> твоего брата, мы учились в одной школе.",
                            "target_lang_translations": {
                                "en": "I have <w>known</w> your brother for a long time; we went to the same school."
                            }
                        },
                        {
                            "text": "Вы <w>знаете</w> этого человека, который только что вошёл?",
                            "target_lang_translations": {
                                "en": "Do you <w>know</w> this person who just walked in?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "знать в лицо",
                        "знать лично"
                    ],
                    "target_lang_definitions": {
                        "en": "To be acquainted or familiar with someone personally."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "know",
                                "target_lang_sense_clarification": "to be acquainted with"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "516db9a1-29e7-3a88-8750-0f5ecf74314f",
                    "sense_definition": "Владеть какими-либо навыками, разбираться в науке, искусстве или языке.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cognitive_knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой друг отлично <w>знает</w> математику и помогает мне с задачами.",
                            "target_lang_translations": {
                                "en": "My friend <w>knows</w> mathematics perfectly and helps me with problems."
                            }
                        },
                        {
                            "text": "Чтобы работать в этой компании, нужно <w>знать</w> английский язык.",
                            "target_lang_translations": {
                                "en": "To work in this company, one needs to <w>know</w> English."
                            }
                        }
                    ],
                    "synonyms": [
                        "узнавать"
                    ],
                    "common_phrases": [
                        "знать своё дело",
                        "знать язык",
                        "знать толк"
                    ],
                    "target_lang_definitions": {
                        "en": "To possess skills or understanding in a specific subject or language."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "know",
                                "target_lang_sense_clarification": "to have knowledge of a subject"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02c4d5f7-9876-377b-a562-8d604c7af55e",
                    "sense_definition": "Вводное слово (обычно во 2-м лице: «знаешь», «знаете»), используемое для привлечения внимания собеседника или заполнения паузы.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "discourse_marker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я, <w>знаешь</w>, совсем не хочу идти сегодня в кино.",
                            "target_lang_translations": {
                                "en": "I, <w>you know</w>, don't want to go to the cinema today at all."
                            }
                        },
                        {
                            "text": "Это, <w>знаете</w>, очень сложная и запутанная история.",
                            "target_lang_translations": {
                                "en": "This is, <w>you know</w>, a very complicated and confusing story."
                            }
                        }
                    ],
                    "common_phrases": [
                        "знаешь ли",
                        "как знать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used as a filler word"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A filler phrase used to draw attention or fill a pause (equivalent to \"you know\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "you know",
                                "target_lang_sense_clarification": "discourse marker"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdda22f9-f6f1-399c-9186-7e13dd982ac6",
                    "sense_definition": "Понимать, осознавать что-либо, отдавать себе отчёт в чём-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cognitive_knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она прекрасно <w>знала</w>, что поступает неправильно, но не могла остановиться.",
                            "target_lang_translations": {
                                "en": "She <w>knew</w> perfectly well that she was acting wrongly, but couldn't stop."
                            }
                        },
                        {
                            "text": "Мы должны <w>знать</w> меру в своих желаниях и требованиях.",
                            "target_lang_translations": {
                                "en": "We must <w>know</w> the limit in our desires and demands."
                            }
                        }
                    ],
                    "synonyms": [
                        "понимать",
                        "осознавать"
                    ],
                    "common_phrases": [
                        "знать меру",
                        "знать своё место",
                        "знать цену"
                    ],
                    "target_lang_definitions": {
                        "en": "To understand, realize, or be fully conscious of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "know",
                                "target_lang_sense_clarification": "to be conscious of"
                            },
                            {
                                "target_lang_word": "realize",
                                "target_lang_sense_clarification": "to understand clearly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ce282d2-d6e6-3335-8f6f-62dac2bf2136",
                    "sense_definition": "Испытывать, переживать что-либо на собственном опыте (обычно о чувствах или состояниях).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "experience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот народ <w>знал</w> много горя и страданий в прошлом веке.",
                            "target_lang_translations": {
                                "en": "This people <w>knew</w> much grief and suffering in the last century."
                            }
                        },
                        {
                            "text": "Он работал с утра до ночи и не <w>знал</w> ни минуты покоя.",
                            "target_lang_translations": {
                                "en": "He worked from morning till night and did not <w>know</w> a minute of peace."
                            }
                        }
                    ],
                    "synonyms": [
                        "испытывать",
                        "переживать",
                        "терпеть"
                    ],
                    "common_phrases": [
                        "не знать покоя",
                        "не знать усталости",
                        "знать нужду"
                    ],
                    "target_lang_definitions": {
                        "en": "To experience or undergo something personally (usually feelings or states)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "know",
                                "target_lang_sense_clarification": "to experience"
                            },
                            {
                                "target_lang_word": "experience",
                                "target_lang_sense_clarification": "to undergo"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "539496d4-c529-3e47-af96-87d82463bde4",
                    "sense_definition": "Аристократия, высший привилегированный слой общества.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_class",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вся городская <w>знать</w> собралась на торжественный бал во дворце.",
                            "target_lang_translations": {
                                "en": "All the city <w>nobility</w> gathered for the grand ball at the palace."
                            }
                        },
                        {
                            "text": "В девятнадцатом веке местная <w>знать</w> часто говорила на французском языке.",
                            "target_lang_translations": {
                                "en": "In the nineteenth century, the local <w>nobility</w> often spoke French."
                            }
                        }
                    ],
                    "synonyms": [
                        "аристократия",
                        "эли́та",
                        "вы́сшее о́бщество"
                    ],
                    "antonyms": [
                        "плебс"
                    ],
                    "common_phrases": [
                        "местная знать",
                        "родовитая знать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "collective noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The aristocracy or highest privileged social class."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nobility",
                                "target_lang_sense_clarification": "social class"
                            },
                            {
                                "target_lang_word": "aristocracy",
                                "target_lang_sense_clarification": "privileged class"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "94ba4f0c-1613-30f5-94f9-f4dbf00fe18a",
                    "sense_definition": "Вводное слово, означающее «по-видимому», «должно быть», «вероятно».",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adverbial_inference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Знать</w>, ты не хочешь со мной разговаривать, раз всё время молчишь.",
                            "target_lang_translations": {
                                "en": "<w>Apparently</w>, you don't want to talk to me, since you are silent all the time."
                            }
                        },
                        {
                            "text": "На улице стало совсем темно, <w>знать</w>, скоро пойдёт сильный дождь.",
                            "target_lang_translations": {
                                "en": "It has become quite dark outside; <w>it seems</w> heavy rain will start soon."
                            }
                        }
                    ],
                    "synonyms": [
                        "вида́ть",
                        "ви́дно",
                        "зна́чит",
                        "очевидно",
                        "по-видимому"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in poetic or archaic style"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A colloquial word indicating probability or inference; apparently."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "apparently",
                                "target_lang_sense_clarification": "seemingly"
                            },
                            {
                                "target_lang_word": "must be",
                                "target_lang_sense_clarification": "inference"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aaefc24b-84f2-399a-8bdd-9321533e2e07",
                    "sense_definition": "Одна из четырёх сторон света, противоположная северу; направление, где солнце находится в полдень.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Окна нашей новой квартиры выходят на <w>юг</w>, поэтому в комнатах всегда много света.",
                            "target_lang_translations": {
                                "en": "The windows of our new apartment face <w>south</w>, so there is always a lot of light in the rooms."
                            }
                        },
                        {
                            "text": "Осенью перелётные птицы собираются в стаи и улетают на <w>юг</w>.",
                            "target_lang_translations": {
                                "en": "In autumn, migratory birds gather in flocks and fly <w>south</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "север"
                    ],
                    "common_phrases": [
                        "на юг",
                        "к югу",
                        "ветер с юга"
                    ],
                    "target_lang_definitions": {
                        "en": "One of the four cardinal directions, opposite to the north; the direction where the sun is at noon."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "south",
                                "target_lang_sense_clarification": "cardinal direction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b3fed4d-a29f-383d-9f6c-bbf55779d833",
                    "sense_definition": "Местность с тёплым климатом; южная часть страны или территории (часто используется как место отдыха).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "region",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждое лето мы всей семьёй ездим отдыхать на <w>юг</w> к морю.",
                            "target_lang_translations": {
                                "en": "Every summer, our whole family goes to the <w>south</w> to the sea for a vacation."
                            }
                        },
                        {
                            "text": "На <w>юге</w> страны выращивают вкусные фрукты и виноград.",
                            "target_lang_translations": {
                                "en": "Delicious fruits and grapes are grown in the <w>south</w> of the country."
                            }
                        }
                    ],
                    "antonyms": [
                        "север"
                    ],
                    "common_phrases": [
                        "жить на юге",
                        "поехать на юг",
                        "отдых на юге"
                    ],
                    "target_lang_definitions": {
                        "en": "An area with a warm climate; the southern part of a country or territory (often used as a holiday destination)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the South",
                                "target_lang_sense_clarification": "region"
                            },
                            {
                                "target_lang_word": "south",
                                "target_lang_sense_clarification": "general direction or area"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cd95874-6a66-349d-9184-1b63b54a0462",
                    "sense_definition": "Конфедеративные Штаты Америки во время Гражданской войны в США (1861–1865).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гражданская война между Севером и <w>Югом</w> стала поворотным моментом в истории США.",
                            "target_lang_translations": {
                                "en": "The Civil War between the North and the <w>South</w> became a turning point in US history."
                            }
                        },
                        {
                            "text": "Армия <w>Юга</w> сражалась за сохранение традиционного уклада жизни.",
                            "target_lang_translations": {
                                "en": "The Army of the <w>South</w> fought to preserve the traditional way of life."
                            }
                        }
                    ],
                    "synonyms": [
                        "Конфедерация"
                    ],
                    "antonyms": [
                        "Север"
                    ],
                    "target_lang_definitions": {
                        "en": "The Confederate States of America during the US Civil War (1861–1865)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the South",
                                "target_lang_sense_clarification": "historical/political entity"
                            },
                            {
                                "target_lang_word": "the Confederacy",
                                "target_lang_sense_clarification": "synonym"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae8bfcda-f707-3fe4-b3a2-ed950ceea907",
                    "sense_definition": "Тёплый ветер, дующий с южной стороны.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "wind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ласковый <w>юг</w> принёс с собой аромат цветущих садов и моря.",
                            "target_lang_translations": {
                                "en": "The gentle <w>south wind</w> brought with it the scent of blooming gardens and the sea."
                            }
                        },
                        {
                            "text": "Рыбаки ждали, когда подует <w>юг</w>, чтобы выйти в открытое море.",
                            "target_lang_translations": {
                                "en": "The fishermen were waiting for the <w>south wind</w> to blow to go out into the open sea."
                            }
                        }
                    ],
                    "synonyms": [
                        "ю́жный ве́тер"
                    ],
                    "target_lang_definitions": {
                        "en": "A warm wind blowing from the south."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "south wind",
                                "target_lang_sense_clarification": "wind direction"
                            },
                            {
                                "target_lang_word": "southerly",
                                "target_lang_sense_clarification": "nautical/meteorological"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "41caf84a-6f64-3eb5-82ae-05a50f33f9f7",
                    "sense_definition": "Высокоорганизованное млекопитающее из отряда приматов, наиболее близкое по строению тела к человеку.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В зоопарке маленькая <обезьяна> ловко прыгала по веткам высокого дерева.",
                            "target_lang_translations": {
                                "en": "In the zoo, a little <monkey> was jumping deftly along the branches of a tall tree."
                            }
                        },
                        {
                            "text": "Учёные считают, что человекообразная <обезьяна> обладает высоким интеллектом.",
                            "target_lang_translations": {
                                "en": "Scientists believe that an anthropoid <ape> possesses high intelligence."
                            }
                        }
                    ],
                    "common_phrases": [
                        "человекообразная обезьяна",
                        "хитрая обезьяна",
                        "год Обезьяны"
                    ],
                    "target_lang_definitions": {
                        "en": "A highly organized mammal of the primate order, closest in body structure to humans."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monkey",
                                "target_lang_sense_clarification": "general term for simians"
                            },
                            {
                                "target_lang_word": "ape",
                                "target_lang_sense_clarification": "large, tailless primate (e.g., chimpanzee, gorilla)"
                            },
                            {
                                "target_lang_word": "simian",
                                "target_lang_sense_clarification": "scientific or formal term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c27d0503-48f1-3856-b8d4-f799f58eaf12",
                    "sense_definition": "Человек, который слепо подражает другим, копирует чьи-то манеры или неестественно кривляется.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "human_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Прекрати повторять за мной каждое слово, ты ведёшь себя как <обезьяна>!",
                            "target_lang_translations": {
                                "en": "Stop repeating every word after me, you are acting like a <monkey>!"
                            }
                        },
                        {
                            "text": "Этот актёр просто <обезьяна>, у него нет своего стиля, он лишь копирует чужие жесты.",
                            "target_lang_translations": {
                                "en": "This actor is just a <copycat>; he has no style of his own, he only copies others' gestures."
                            }
                        }
                    ],
                    "synonyms": [
                        "кривляка",
                        "имитатор"
                    ],
                    "common_phrases": [
                        "вести себя как обезьяна"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "disapproving"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person who blindly imitates others, copies someone's manners, or grimaces unnaturally."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "copycat",
                                "target_lang_sense_clarification": "person who imitates others"
                            },
                            {
                                "target_lang_word": "mimic",
                                "target_lang_sense_clarification": "person skilled at imitating"
                            },
                            {
                                "target_lang_word": "ape",
                                "target_lang_sense_clarification": "clumsy or foolish imitator"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18b0d25a-bc72-3f87-9814-23e91f158176",
                    "sense_definition": "О человеке с очень некрасивой, отталкивающей внешностью.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "human_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Злая мачеха часто называла бедную девушку <обезьяной>, хотя та была мила.",
                            "target_lang_translations": {
                                "en": "The evil stepmother often called the poor girl a <monkey>, although she was sweet."
                            }
                        },
                        {
                            "text": "Он считал себя красавцем, но за спиной соседи звали его <обезьяной>.",
                            "target_lang_translations": {
                                "en": "He considered himself handsome, but behind his back, the neighbors called him an <ape>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "abusive"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Referring to a person with a very ugly, repulsive appearance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ape",
                                "target_lang_sense_clarification": "implying brutish or primitive ugliness"
                            },
                            {
                                "target_lang_word": "monkey",
                                "target_lang_sense_clarification": "implying unattractive or funny-looking features"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07207137-b19c-317d-8e4c-cdc6d19e7b15",
                    "sense_definition": "Название символа «@», используемого в адресах электронной почты (реже, чем «собака»).",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В некоторых странах этот символ называют <обезьяной>, а у нас — собакой.",
                            "target_lang_translations": {
                                "en": "In some countries, this symbol is called a <monkey>, but here — a dog."
                            }
                        },
                        {
                            "text": "Продиктуйте ваш email: логин, затем <обезьяна>, затем название почтового сервера.",
                            "target_lang_translations": {
                                "en": "Dictate your email: login, then <at>, then the name of the mail server."
                            }
                        }
                    ],
                    "synonyms": [
                        "собака",
                        "собачка"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "computer slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Name for the symbol '@' used in email addresses (less common than 'dog' in Russian)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at sign",
                                "target_lang_sense_clarification": "the symbol @"
                            },
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "short form used when reading addresses"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9629277-e912-3a8f-8b41-a62eff1e14a8",
                    "sense_definition": "Крайне оскорбительное именование темнокожего человека.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "human_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Использование слова <обезьяна> в адрес человека другой расы абсолютно недопустимо.",
                            "target_lang_translations": {
                                "en": "Using the word <monkey> to address a person of another race is absolutely unacceptable."
                            }
                        },
                        {
                            "text": "За то, что фанат назвал игрока <обезьяной>, его выгнали со стадиона.",
                            "target_lang_translations": {
                                "en": "For calling the player a <monkey>, the fan was kicked out of the stadium."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "offensive, ethnic slur"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An extremely offensive term for a dark-skinned person."
                    },

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "226b44a8-f99b-3033-8eb5-12e331897191",
                    "sense_definition": "The populated residential area surrounding a city, often characterized by detached houses and a quieter lifestyle.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "residential_areas",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many families prefer to live in the <w>suburbs</w> because there is more space for children to play.",
                            "target_lang_translations": {
                                "ru": "Многие семьи предпочитают жить в <w>пригороде</w>, так как там больше места для детских игр."
                            }
                        },
                        {
                            "text": "After years of city life, they decided to move to the <w>suburbs</w> for a peaceful retirement.",
                            "target_lang_translations": {
                                "ru": "После многих лет жизни в городе они решили переехать в <w>пригород</w> ради спокойной старости."
                            }
                        }
                    ],
                    "synonyms": [
                        "outskirts",
                        "suburbia",
                        "residential area"
                    ],
                    "antonyms": [
                        "city center",
                        "downtown",
                        "inner city"
                    ],
                    "common_phrases": [
                        "live in the suburbs",
                        "move to the suburbs",
                        "quiet suburbs",
                        "leafy suburbs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Населенный жилой район вокруг города, часто застроенный частными домами и отличающийся более спокойным ритмом жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пригород",
                                "target_lang_sense_clarification": "общая территория за пределами центра города"
                            },
                            {
                                "target_lang_word": "окраина",
                                "target_lang_sense_clarification": "удаленная от центра часть города"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efcd33be-ffaf-3018-9870-ae029fea21a9",
                    "sense_definition": "Plural of <w>suburb</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "residential_areas",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train line connects the central station with the northern <w>suburbs</w>.",
                            "target_lang_translations": {
                                "ru": "Железнодорожная линия соединяет центральный вокзал с северными <w>пригородами</w>."
                            }
                        },
                        {
                            "text": "Several new <w>suburbs</w> are being developed on the edge of the town to accommodate the growing population.",
                            "target_lang_translations": {
                                "ru": "На краю города строятся несколько новых <w>пригородов</w> для размещения растущего населения."
                            }
                        }
                    ],
                    "common_phrases": [

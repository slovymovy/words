    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8f13c910-ea37-381c-b2fe-409963e8b607",
                    "sense_definition": "Огороженный участок земли при доме, здании или группе домов.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дети любят играть в футбол во <w>дворе</w>.",
                            "target_lang_translations": {
                                "en": "Children love playing football in the <w>yard</w>."
                            }
                        },
                        {
                            "text": "Окна нашей квартиры выходят в тихий и зелёный <w>двор</w>.",
                            "target_lang_translations": {
                                "en": "The windows of our apartment face a quiet and green <w>courtyard</w>."
                            }
                        },
                        {
                            "text": "Машина заехала во <w>двор</w> и остановилась у подъезда.",
                            "target_lang_translations": {
                                "en": "The car drove into the <w>courtyard</w> and stopped by the entrance."
                            }
                        }
                    ],
                    "antonyms": [
                        "улица"
                    ],
                    "common_phrases": [
                        "играть во дворе",
                        "выйти во двор",
                        "проходной двор",
                        "на дворе"
                    ],
                    "target_lang_definitions": {
                        "en": "An enclosed area of land attached to a house, building, or group of houses."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "yard",
                                "target_lang_sense_clarification": "area adjoining a house"
                            },
                            {
                                "target_lang_word": "courtyard",
                                "target_lang_sense_clarification": "enclosed open space within a building complex"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d63a3f38-104a-31b4-bb3e-6d9e7b340749",
                    "sense_definition": "Монарх, его семья и приближённые к ним лица, составляющие окружение правителя.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодой дворянин мечтал служить при царском <w>дворе</w>.",
                            "target_lang_translations": {
                                "en": "The young nobleman dreamed of serving at the tsar's <w>court</w>."
                            }
                        },
                        {
                            "text": "Весь королевский <w>двор</w> отправился на летний отдых в загородную резиденцию.",
                            "target_lang_translations": {
                                "en": "The entire royal <w>court</w> went on summer vacation to the country residence."
                            }
                        }
                    ],
                    "synonyms": [
                        "свита"
                    ],
                    "common_phrases": [
                        "при дворе",
                        "королевский двор",
                        "царский двор"
                    ],
                    "target_lang_definitions": {
                        "en": "The monarch, their family, and the people attending them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "court",
                                "target_lang_sense_clarification": "royal entourage or residence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73dcfa2f-84bb-372a-ba91-e223c46e4232",
                    "sense_definition": "Отдельное крестьянское хозяйство, включающее дом и все хозяйственные постройки (часто используется как единица счёта в деревне).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этой маленькой деревне осталось всего пять жилых <w>дворов</w>.",
                            "target_lang_translations": {
                                "en": "Only five inhabited <w>households</w> remained in this small village."
                            }
                        },
                        {
                            "text": "Каждый <w>двор</w> должен был платить налог помещику.",
                            "target_lang_translations": {
                                "en": "Every <w>household</w> had to pay a tax to the landlord."
                            }
                        }
                    ],
                    "synonyms": [
                        "хозяйство",
                        "усадьба",
                        "подворье"
                    ],
                    "common_phrases": [
                        "постоялый двор",
                        "крестьянский двор"
                    ],
                    "target_lang_definitions": {
                        "en": "A separate peasant farming unit including the house and outbuildings."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "household",
                                "target_lang_sense_clarification": "unit of counting in a village"
                            },
                            {
                                "target_lang_word": "farmstead",
                                "target_lang_sense_clarification": "physical farm complex"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7948ddc1-edff-3b29-8964-067ce01b904a",
                    "sense_definition": "Специализированная территория или учреждение (обычно в составе устойчивых названий).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы посетили старый Гостиный <w>двор</w> в центре Петербурга.",
                            "target_lang_translations": {
                                "en": "Tourists visited the old Gostiny <w>Dvor</w> in the center of St. Petersburg."
                            }
                        },
                        {
                            "text": "На ферме построили новый современный скотный <w>двор</w>.",
                            "target_lang_translations": {
                                "en": "A new modern barn<w>yard</w> was built on the farm."
                            }
                        }
                    ],
                    "common_phrases": [
                        "монетный двор",
                        "скотный двор",
                        "гостиный двор"
                    ],
                    "target_lang_definitions": {
                        "en": "A specialized area or establishment (used in set phrases)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "yard",
                                "target_lang_sense_clarification": "in agricultural contexts (e.g., barnyard)"
                            },
                            {
                                "target_lang_word": "court",
                                "target_lang_sense_clarification": "in historical or commercial names"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "5f54e398-bec2-3963-8206-c24c6884608b",
                    "sense_definition": "Przemieścić pokarm, płyn lub ślinę z jamy ustnej do gardła i dalej do żołądka.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "biological_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chory musiał szybko <przełknąć> gorzkie lekarstwo popijając je wodą.",
                            "target_lang_translations": {
                                "ru": "Больному пришлось быстро <проглотить> горькое лекарство, запивая его водой."
                            }
                        },
                        {
                            "text": "Byłem tak zdenerwowany przed egzaminem, że z trudem mogłem <przełknąć> ślinę.",
                            "target_lang_translations": {
                                "ru": "Я так нервничал перед экзаменом, что с трудом мог <сглотнуть> слюну."
                            }
                        }
                    ],
                    "synonyms": [
                        "połknąć"
                    ],
                    "antonyms": [
                        "wypluć"
                    ],
                    "common_phrases": [

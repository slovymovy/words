{
    "word_family": [
        "bevestigen",
        "bevestigend"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f5751a76-d2e6-339f-80e9-2d7f0f2ce0fc",
                    "sense_definition": "Een verklaring of bewijs dat iets waar, juist of ontvangen is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_confirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb nog geen <w>bevestiging</w> van mijn bestelling ontvangen.",
                            "target_lang_translations": {
                                "en": "I haven't received a <w>confirmation</w> of my order yet.",
                                "ru": "Я еще не получил <w>подтверждения</w> моего заказа."
                            }
                        },
                        {
                            "text": "Ter <w>bevestiging</w> stuur ik u hierbij de getekende overeenkomst.",
                            "target_lang_translations": {
                                "en": "I hereby send you the signed agreement as <w>confirmation</w>.",
                                "ru": "В качестве <w>подтверждения</w> отправляю вам подписанное соглашение."
                            }
                        }
                    ],
                    "synonyms": [
                        "confirmatie",
                        "verificatie"
                    ],
                    "common_phrases": [
                        "schriftelijke bevestiging",
                        "bevestiging van ontvangst",
                        "ter bevestiging"
                    ],
                    "target_lang_definitions": {
                        "en": "A statement or proof that something is true, correct, or received.",
                        "ru": "Заявление или доказательство того, что что-то является истинным, правильным или было получено."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "confirmation",
                                "target_lang_sense_clarification": "proof of truth or receipt"
                            },
                            {
                                "target_lang_word": "verification",
                                "target_lang_sense_clarification": "process of establishing truth"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подтверждение",
                                "target_lang_sense_clarification": "доказательство истинности или получения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21be584c-75dc-3d51-bdf1-5592b60741ff",
                    "sense_definition": "Erkenning of goedkeuring van iemands gedrag, gevoelens of bestaan door anderen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_confirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zoekt voortdurend naar <w>bevestiging</w> van zijn ouders.",
                            "target_lang_translations": {
                                "en": "He is constantly seeking <w>validation</w> from his parents.",
                                "ru": "Он постоянно ищет <w>одобрения</w> от своих родителей."
                            }
                        },
                        {
                            "text": "Het compliment gaf haar de <w>bevestiging</w> die ze nodig had om door te gaan.",
                            "target_lang_translations": {
                                "en": "The compliment gave her the <w>affirmation</w> she needed to continue.",
                                "ru": "Комплимент дал ей то <w>подтверждение</w>, которое ей было нужно, чтобы продолжать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zoeken naar bevestiging",
                        "behoefte aan bevestiging"
                    ],
                    "target_lang_definitions": {
                        "en": "Recognition or approval of someone's behavior, feelings, or existence by others.",
                        "ru": "Признание или одобрение чьего-либо поведения, чувств или существования другими людьми."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "validation",
                                "target_lang_sense_clarification": "psychological recognition or acceptance"
                            },
                            {
                                "target_lang_word": "affirmation",
                                "target_lang_sense_clarification": "emotional support or encouragement"
                            },
                            {
                                "target_lang_word": "approval",
                                "target_lang_sense_clarification": "positive agreement or sanction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "одобрение",
                                "target_lang_sense_clarification": "положительная оценка действий"
                            },
                            {
                                "target_lang_word": "признание",
                                "target_lang_sense_clarification": "подтверждение значимости"
                            },
                            {
                                "target_lang_word": "подтверждение",
                                "target_lang_sense_clarification": "психологическая поддержка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7522b691-644f-331e-a192-a163a72fe334",
                    "sense_definition": "De handeling van het vastmaken of de toestand van vastzitten aan iets anders.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_attachment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>bevestiging</w> van de zware plank aan de muur kostte veel moeite.",
                            "target_lang_translations": {
                                "en": "The <w>mounting</w> of the heavy shelf to the wall took a lot of effort.",
                                "ru": "<w>Крепление</w> тяжелой полки к стене потребовало больших усилий."
                            }
                        },
                        {
                            "text": "Zorg voor een stevige <w>bevestiging</w> zodat het niet losraakt tijdens het vervoer.",
                            "target_lang_translations": {
                                "en": "Ensure a secure <w>attachment</w> so it doesn't come loose during transport.",
                                "ru": "Обеспечьте надежное <w>крепление</w>, чтобы ничего не отсоединилось при транспортировке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stevige bevestiging",
                        "bevestiging aan de muur"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of attaching or the state of being attached to something else.",
                        "ru": "Действие по прикреплению или состояние прикрепленности к чему-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attachment",
                                "target_lang_sense_clarification": "act of connecting things"
                            },
                            {
                                "target_lang_word": "mounting",
                                "target_lang_sense_clarification": "act of fixing to a support or surface"
                            },
                            {
                                "target_lang_word": "fastening",
                                "target_lang_sense_clarification": "act of securing firmly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "крепление",
                                "target_lang_sense_clarification": "процесс или способ соединения"
                            },
                            {
                                "target_lang_word": "фиксация",
                                "target_lang_sense_clarification": "закрепление на месте"
                            },
                            {
                                "target_lang_word": "прикрепление",
                                "target_lang_sense_clarification": "действие по соединению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0aebf4e8-f256-30d8-ad18-db6757ec5a19",
                    "sense_definition": "Het materiaal, onderdeel of mechanisme waarmee dingen aan elkaar vastzitten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_attachment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het hek was gevallen nadat de <w>bevestiging</w> was gebroken.",
                            "target_lang_translations": {
                                "en": "The fence fell after the <w>fastening</w> broke.",
                                "ru": "Забор упал после того, как сломалось <w>крепление</w>."
                            }
                        },
                        {
                            "text": "Controleer of de <w>bevestiging</w> niet verroest is door de regen.",
                            "target_lang_translations": {
                                "en": "Check that the <w>mount</w> is not rusted by the rain.",
                                "ru": "Проверьте, не заржавел ли <w>крепеж</w> из-за дождя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kapotte bevestiging",
                        "losse bevestiging"
                    ],
                    "target_lang_definitions": {
                        "en": "The material, part, or mechanism by which things are attached to each other.",
                        "ru": "Материал, деталь или механизм, с помощью которого предметы скреплены друг с другом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mount",
                                "target_lang_sense_clarification": "support mechanism or setting"
                            },
                            {
                                "target_lang_word": "fastening",
                                "target_lang_sense_clarification": "device used to hold things together"
                            },
                            {
                                "target_lang_word": "fixture",
                                "target_lang_sense_clarification": "piece of equipment fixed in position"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "крепление",
                                "target_lang_sense_clarification": "деталь или механизм"
                            },
                            {
                                "target_lang_word": "крепеж",
                                "target_lang_sense_clarification": "детали для соединения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                            {
                                "target_lang_word": "потентат",
                                "target_lang_sense_clarification": "книжное, историческое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fff42aba-3c9d-3c66-a073-82e4bd236bde",
                    "sense_definition": "A support used for walking; a crutch.",

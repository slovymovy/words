    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9e919abe-4042-3015-b162-1db216a91260",
                    "sense_definition": "Сообщить информацию, сделать официальное заявление или представить отчёт начальству или аудитории.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дежурный офицер обязан каждое утро <доложить> командиру обстановку в части.",
                            "target_lang_translations": {
                                "en": "The duty officer is required to <report> the situation in the unit to the commander every morning."
                            }
                        },
                        {
                            "text": "Я прошу вас <доложить> о результатах проделанной работы на следующем собрании.",
                            "target_lang_translations": {
                                "en": "I ask you to <report> on the results of the work done at the next meeting."
                            }
                        }
                    ],
                    "synonyms": [
                        "сообщить",
                        "сделать доклад",
                        "отрапортовать"
                    ],
                    "common_phrases": [
                        "доложить обстановку",
                        "доложить начальству",
                        "разрешите доложить",
                        "доложить о результатах"
                    ],
                    "target_lang_definitions": {
                        "en": "To report information, make an official statement, or present a report to superiors or an audience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "report",
                                "target_lang_sense_clarification": "to give an official account or statement"
                            },
                            {
                                "target_lang_word": "announce",
                                "target_lang_sense_clarification": "to make a formal declaration (e.g., someone's arrival)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67c5e99a-6541-3275-b14e-a89e1cdbac16",
                    "sense_definition": "Положить что-либо дополнительно к уже имеющемуся; добавить.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама решила <доложить> сыну ещё немного картошки в тарелку.",
                            "target_lang_translations": {
                                "en": "Mom decided to <add> some more potatoes to her son's plate."
                            }
                        },
                        {
                            "text": "Нам пришлось <доложить> свои деньги, чтобы купить этот дорогой подарок.",
                            "target_lang_translations": {
                                "en": "We had to <add> our own money to buy this expensive gift."
                            }
                        }
                    ],
                    "common_phrases": [
                        "доложить денег",
                        "доложить еды",
                        "забыл доложить"
                    ],
                    "target_lang_definitions": {
                        "en": "To put something additionally to what is already there; to add."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "add",
                                "target_lang_sense_clarification": "to supplement or put in an additional amount"
                            },
                            {
                                "target_lang_word": "put more",
                                "target_lang_sense_clarification": "physical action of placing additional items"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
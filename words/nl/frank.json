        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1bd6894c-b40e-3d30-8dc6-24464784da41",
                    "sense_definition": "Een munteenheid die onder andere in Zwitserland wordt gebruikt en vroeger in België en Frankrijk een wettig betaalmiddel was.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "currency_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In Zwitserland betaal je met de <w>frank</w>.",
                            "target_lang_translations": {
                                "en": "In Switzerland, you pay with the <w>franc</w>.",
                                "ru": "В Швейцарии платят <w>франками</w>."
                            }
                        },
                        {
                            "text": "De Belgische <w>frank</w> is in 2002 vervangen door de euro.",
                            "target_lang_translations": {
                                "en": "The Belgian <w>franc</w> was replaced by the euro in 2002.",
                                "ru": "Бельгийский <w>франк</w> был заменен на евро в 2002 году."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Zwitserse frank",
                        "Belgische frank"
                    ],
                    "target_lang_definitions": {
                        "en": "A currency unit used in Switzerland, among others, and formerly legal tender in Belgium and France.",
                        "ru": "Денежная единица, используемая, в частности, в Швейцарии, а ранее являвшаяся законным платежным средством в Бельгии и Франции."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "franc",
                                "target_lang_sense_clarification": "currency"

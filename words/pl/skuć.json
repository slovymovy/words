{
    "word_family": [
        "skuwać",
        "odkuć",
        "podkuć",
        "wykuć",
        "rozkuć",
        "zakuć",
        "kucie",
        "skuwka"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7d0c9f91-d05c-3aae-8556-a4c510f88d35",
                    "sense_definition": "Usunąć warstwę materiału (np. tynku, kafelków, betonu) ze ściany lub podłogi, zazwyczaj za pomocą uderzeń młota lub specjalistycznego narzędzia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "construction_renovation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <skuć> stare płytki w łazience, zanim położymy nową glazurę.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <сбить> старую плитку в ванной, прежде чем класть новую."
                            }
                        },
                        {
                            "text": "Robotnicy zaczęli <skuć> tynk ze ścian już o siódmej rano.",
                            "target_lang_translations": {
                                "ru": "Рабочие начали <сбивать> штукатурку со стен уже в семь утра."
                            }
                        }
                    ],
                    "synonyms": [
                        "odbić",
                        "rozbić",
                        "usunąć"
                    ],
                    "antonyms": [
                        "położyć",
                        "nałożyć"
                    ],
                    "common_phrases": [
                        "skuć tynk",
                        "skuć kafelki",
                        "skuć podłogę",
                        "skuć beton"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалить слой материала (например, штукатурку, плитку, бетон) со стены или пола, обычно с помощью ударов молотка или специального инструмента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбить",
                                "target_lang_sense_clarification": "штукатурку, плитку"
                            },
                            {
                                "target_lang_word": "сколоть",
                                "target_lang_sense_clarification": "отбить куски материала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "260416cb-0088-35b3-b337-7fe2df2c09c2",
                    "sense_definition": "Założyć komuś kajdanki na ręce, aby ograniczyć swobodę ruchów; często używane w kontekście działań policji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "restraint",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policjant musiał <skuć> agresywnego napastnika, aby zapewnić bezpieczeństwo.",
                            "target_lang_translations": {
                                "ru": "Полицейскому пришлось <заковать> агрессивного нападавшего в наручники, чтобы обеспечить безопасность."
                            }
                        },
                        {
                            "text": "Złodziej został <skuty> i natychmiast przewieziony na komisariat.",
                            "target_lang_translations": {
                                "ru": "Вор был <закован> в наручники и немедленно доставлен в участок."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakuć",
                        "obezwładnić",
                        "spiąć"
                    ],
                    "antonyms": [
                        "rozkuć",
                        "uwolnić"
                    ],
                    "common_phrases": [
                        "skuć kajdankami",
                        "skuć ręce",
                        "skuć aresztowanego"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надеть на кого-либо наручники, чтобы ограничить свободу движений; часто используется в полицейском контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заковать",
                                "target_lang_sense_clarification": "в наручники"
                            },
                            {
                                "target_lang_word": "надеть наручники",
                                "target_lang_sense_clarification": "фраза"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97682f08-b441-3c05-ab76-ef08287fda43",
                    "sense_definition": "Upić się alkoholem lub odurzyć narkotykami do stanu utraty świadomości lub kontroli nad sobą (używane w formie zwrotnej „skuć się”).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "intoxication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek tak się wczoraj <skuł> na imprezie, że nic nie pamięta.",
                            "target_lang_translations": {
                                "ru": "Марек вчера так <напился> на вечеринке, что ничего не помнит."
                            }
                        },
                        {
                            "text": "Nie warto się <skuć> w środku tygodnia, jeśli rano trzeba iść do pracy.",
                            "target_lang_translations": {
                                "ru": "Не стоит <напиваться> посреди недели, если утром нужно идти на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "upić się",
                        "zaprawić się",
                        "nawalić się"
                    ],
                    "antonyms": [
                        "wytrzeźwieć"
                    ],
                    "common_phrases": [
                        "skuć się jak świnia",
                        "skuć się do nieprzytomności",
                        "nieźle się skuć"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common among youth and in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно напиться алкоголем или одурманить себя наркотиками до потери контроля (используется в возвратной форме)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напиться",
                                "target_lang_sense_clarification": "алкоголем"
                            },
                            {
                                "target_lang_word": "нажраться",
                                "target_lang_sense_clarification": "грубое, сленг"
                            },
                            {
                                "target_lang_word": "убраться",
                                "target_lang_sense_clarification": "молодежный сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0825d00f-7c8b-31fe-b3a7-b824c50301d8",
                    "sense_definition": "O mrozie: spowodować zamarznięcie wody w rzece lub jeziorze, pokrywając ją warstwą lodu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "weather_phenomena",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tegoroczna zima szybko <skuła> lodem wszystkie okoliczne stawy.",
                            "target_lang_translations": {
                                "ru": "Нынешняя зима быстро <сковала> льдом все окрестные пруды."
                            }
                        },
                        {
                            "text": "Czekamy, aż mróz <skuje> taflę jeziora, żeby pojeździć na łyżwach.",
                            "target_lang_translations": {
                                "ru": "Мы ждем, когда мороз <скует> гладь озера, чтобы покататься на коньках."
                            }
                        }
                    ],
                    "synonyms": [
                        "zamrozić",
                        "ściąć"
                    ],
                    "antonyms": [
                        "roztopić"
                    ],
                    "common_phrases": [
                        "mróz skuł rzekę",
                        "lód skuł jezioro",
                        "skuć lodem"
                    ],
                    "target_lang_definitions": {
                        "ru": "О морозе: заставить воду в реке или озере замерзнуть, покрыв её слоем льда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сковать",
                                "target_lang_sense_clarification": "льдом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbe26225-c73f-3dad-b914-130a932d0d4a",
                    "sense_definition": "Połączyć trwale dwa metalowe elementy, uderzając w nie młotem (np. przy użyciu nitów) lub zgrzewając je.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metalwork",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kowal potrafił mistrzowsko <skuć> dwa kawałki rozgrzanego żelaza w jedną całość.",
                            "target_lang_translations": {
                                "ru": "Кузнец умел мастерски <сковать> два куска раскаленного железа в единое целое."
                            }
                        },
                        {
                            "text": "Musisz <skuć> zerwane ogniwo łańcucha, żeby rower znowu działał.",
                            "target_lang_translations": {
                                "ru": "Ты должен <склепать> разорванное звено цепи, чтобы велосипед снова работал."
                            }
                        }
                    ],
                    "synonyms": [
                        "znitować",
                        "spoić",
                        "połączyć"
                    ],
                    "antonyms": [
                        "rozkuć"
                    ],
                    "common_phrases": [
                        "skuć łańcuch",
                        "skuć ogniwa",
                        "skuć blachę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прочно соединить два металлических элемента ударами молота (ковкой) или с помощью заклепок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сковать",
                                "target_lang_sense_clarification": "соединить ковкой"
                            },
                            {
                                "target_lang_word": "склепать",
                                "target_lang_sense_clarification": "соединить заклепками"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "684a9a75-4dae-34b7-a5a8-b946d11cf51e",
                    "sense_definition": "Pobić kogoś bardzo mocno, uderzyć w twarz (często w zwrocie „skuć mordę”).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Groził mi, że jeśli nie oddam pieniędzy, to może mi <skuć> twarz.",
                            "target_lang_translations": {
                                "ru": "Он угрожал мне, что если я не верну деньги, он может <разбить> мне лицо."
                            }
                        },
                        {
                            "text": "Chłopaki z osiedla chcieli go <skuć> za to, co powiedział o ich koledze.",
                            "target_lang_translations": {
                                "ru": "Парни с района хотели его <избить> за то, что он сказал об их друге."
                            }
                        }
                    ],
                    "synonyms": [
                        "skatować",
                        "sprać",
                        "poturbować"
                    ],
                    "common_phrases": [
                        "skuć mordę",
                        "skuć pysk"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно избить кого-то, ударить по лицу (часто используется в грубых выражениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набить",
                                "target_lang_sense_clarification": "морду, лицо"
                            },
                            {
                                "target_lang_word": "разбить",
                                "target_lang_sense_clarification": "лицо"
                            },
                            {
                                "target_lang_word": "избить",
                                "target_lang_sense_clarification": "нанести побои"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4dcec3f4-690d-383f-ad85-bc3cce6ef3e6",
                    "sense_definition": "Wcielić się w postać w sztuce teatralnej lub filmie; w znaczeniu przenośnym: mieć istotny wpływ na przebieg jakiegoś wydarzenia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "performance_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten znany aktor świetnie <odegrał> rolę króla w nowym filmie historycznym.",
                            "target_lang_translations": {
                                "ru": "Этот известный актер великолепно <w>сыграл</w> роль короля в новом историческом фильме."
                            }
                        },
                        {
                            "text": "Twoja pomoc może <odegrać> kluczową rolę w sukcesie naszego projektu.",
                            "target_lang_translations": {
                                "ru": "Твоя помощь может <w>сыграть</w> ключевую роль в успехе нашего проекта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odegrać ważną rolę",
                        "odegrać kluczową rolę",
                        "odegrać scenę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воплотить персонажа в пьесе или фильме; в переносном смысле: оказать существенное влияние на ход события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сыграть",
                                "target_lang_sense_clarification": "роль в театре, кино или событии"
                            },
                            {
                                "target_lang_word": "исполнить",
                                "target_lang_sense_clarification": "роль (более формально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c4b63d5-7956-38e4-a13f-70ea311e9d78",
                    "sense_definition": "Wydarzyć się lub mieć miejsce w określonych okolicznościach (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "event_occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała ta nieprzyjemna scena <odegrała> się na oczach zdumionych przechodniów.",
                            "target_lang_translations": {
                                "ru": "Вся эта неприятная сцена <w>разыгралась</w> на глазах у изумленных прохожих."
                            }
                        },
                        {
                            "text": "Wczoraj w centrum miasta <odegrały> się dramatyczne wydarzenia podczas protestu.",
                            "target_lang_translations": {
                                "ru": "Вчера в центре города <w>разыгрались</w> драматические события во время протеста."
                            }
                        }
                    ],
                    "synonyms": [
                        "odbyć się",
                        "wydarzyć się",
                        "zajść"
                    ],
                    "common_phrases": [
                        "odegrać się na oczach",
                        "sceny się odegrały",
                        "dramat się odegrał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произойти или иметь место при определенных обстоятельствах (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разыграться",
                                "target_lang_sense_clarification": "о событиях, сценах, драмах"

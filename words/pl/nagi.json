                        "nagi tynk",
                        "naga ściana"
                    ],
                    "target_lang_definitions": {
                        "ru": "О предметах или интерьерах: суровый, лишенный украшений и декора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голый",
                                "target_lang_sense_clarification": "о стенах, без отделки"
                            }
                        ]
                    }
                }
            ]
        },

            "senses": [
                {
                    "sense_id": "0b930607-4c12-353f-96b3-f6eba6796079",
                    "sense_definition": "Een kind doen ontstaan, meestal gezegd van de biologische vader.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "biologisch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft twee kinderen bij zijn vrouw <w>verwekt</w>.",
                            "target_lang_translations": {
                                "ru": "Он <w>зачал</w> двоих детей со своей женой.",
                                "en": "He <w>fathered</w> two children with his wife."
                            }
                        },
                        {
                            "text": "Het lukte het paar lange tijd niet om een kind te <w>verwekken</w>.",
                            "target_lang_translations": {
                                "ru": "Паре долгое время не удавалось <w>зачать</w> ребенка.",
                                "en": "The couple did not succeed in <w>conceiving</w> a child for a long time."
                            }
                        }
                    ],
                    "synonyms": [
                        "voortbrengen"
                    ],
                    "common_phrases": [
                        "een kind verwekken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать жизнь ребенку; обычно используется по отношению к биологическому отцу.",
                        "en": "To bring a child into existence, usually said of the biological father."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зачать",
                                "target_lang_sense_clarification": "ребенка"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "beget",
                                "target_lang_sense_clarification": "formal or literary"
                            },
                            {
                                "target_lang_word": "father",
                                "target_lang_sense_clarification": "to be the biological father of"
                            },
                            {
                                "target_lang_word": "sire",
                                "target_lang_sense_clarification": "biological context, often used for animals"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3575a2c-7084-3494-9d67-2d7c3cb3908f",
                    "sense_definition": "Iets veroorzaken of laten ontstaan, vaak met betrekking tot gevoelens, gedachten of onrust.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "oorzakelijk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn vreemde gedrag zal zeker argwaan <w>verwekken</w> bij de politie.",
                            "target_lang_translations": {
                                "ru": "Его странное поведение наверняка <w>вызовет</w> подозрения у полиции.",
                                "en": "His strange behavior will certainly <w>arouse</w> suspicion with the police."
                            }
                        },
                        {
                            "text": "De plannen van de regering kunnen veel onrust <w>verwekken</w> onder de bevolking.",
                            "target_lang_translations": {
                                "ru": "Планы правительства могут <w>вызвать</w> сильные волнения среди населения.",
                                "en": "The government's plans could <w>cause</w> much unrest among the population."
                            }
                        }
                    ],
                    "synonyms": [
                        "veroorzaken",
                        "teweegbrengen",
                        "oproepen"
                    ],
                    "common_phrases": [
                        "argwaan verwekken",
                        "verbazing verwekken",
                        "onrust verwekken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызвать или создать что-либо; часто используется в контексте чувств, мыслей или беспокойства.",
                        "en": "To cause something to happen or exist, often regarding feelings, thoughts, or unrest."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызвать",
                                "target_lang_sense_clarification": "стать причиной чувства или реакции"
                            },
                            {
                                "target_lang_word": "породить",
                                "target_lang_sense_clarification": "дать начало чему-то абстрактному"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "arouse",

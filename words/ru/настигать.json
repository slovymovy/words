    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "85d5bba9-c37b-3e3a-b2d3-aa6d95fb4267",
                    "sense_definition": "Преследуя кого-либо или что-либо, приближаться вплотную, равняться с ним.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хищник быстро бежал и уже начинал <настигать> свою добычу.",
                            "target_lang_translations": {
                                "en": "The predator ran fast and was already starting to <catch up with> its prey."
                            }
                        },
                        {
                            "text": "Мы видели, как наша лодка стала <настигать> соперников в гонке.",
                            "target_lang_translations": {
                                "en": "We saw how our boat began to <gain on> the rivals in the race."
                            }
                        }
                    ],
                    "synonyms": [
                        "догонять"
                    ],
                    "antonyms": [
                        "отставать"
                    ],
                    "common_phrases": [
                        "быстро настигать",
                        "настигать беглеца",
                        "почти настигать"
                    ],
                    "target_lang_definitions": {
                        "en": "To catch up with or reach someone or something while pursuing them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "catch up with",
                                "target_lang_sense_clarification": "to reach someone moving in the same direction"
                            },
                            {
                                "target_lang_word": "overtake",
                                "target_lang_sense_clarification": "to catch up with and often pass"
                            },
                            {
                                "target_lang_word": "gain on",
                                "target_lang_sense_clarification": "to get closer to someone being pursued"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11c3354b-9399-3a4b-b36e-e9ee0ec77c31",
                    "sense_definition": "Внезапно заставать, случаться или охватывать силой своего воздействия (о явлениях, событиях, чувствах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильная гроза начала <настигать> путников в открытом поле.",
                            "target_lang_translations": {
                                "en": "A strong thunderstorm began to <overtake> the travelers in the open field."
                            }
                        },
                        {
                            "text": "Усталость имеет свойство <настигать> человека в самый неподходящий момент.",
                            "target_lang_translations": {
                                "en": "Fatigue has a tendency to <overcome> a person at the most inopportune moment."
                            }
                        }
                    ],
                    "synonyms": [
                        "застигать"
                    ],
                    "common_phrases": [
                        "настигать врасплох",
                        "беда настигает",
                        "смерть настигает"
                    ],
                    "target_lang_definitions": {
                        "en": "To suddenly affect, happen to, or overcome someone (regarding events, feelings, or phenomena)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overtake",
                                "target_lang_sense_clarification": "to affect suddenly or overwhelmingly"
                            },
                            {
                                "target_lang_word": "befall",
                                "target_lang_sense_clarification": "to happen to someone, usually of something bad"
                            },
                            {
                                "target_lang_word": "strike",
                                "target_lang_sense_clarification": "to happen suddenly and with force"
                            }
                        ]
                    }
                }
            ]
        }

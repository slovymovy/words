    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "00923c40-b454-3ef2-9e3f-2127c4b6518d",
                    "sense_definition": "Bovenop iets rustend of steunend, waarbij er fysiek contact is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "locatie_fysiek",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het boek ligt al de hele dag <w>op</w> de grote tafel.",
                            "target_lang_translations": {
                                "en": "The book has been lying <w>on</w> the big table all day.",
                                "ru": "Книга лежит <w>на</w> большом столе весь день."
                            }
                        },
                        {
                            "text": "Er zit een klein vogeltje <w>op</w> het dak van de schuur.",
                            "target_lang_translations": {
                                "en": "There is a little bird sitting <w>on</w> the roof of the shed.",
                                "ru": "Маленькая птичка сидит <w>на</w> крыше сарая."
                            }
                        }
                    ],
                    "antonyms": [
                        "onder"
                    ],
                    "common_phrases": [
                        "op tafel",
                        "op de grond",
                        "op straat"
                    ],
                    "target_lang_definitions": {
                        "en": "Resting or supported on top of something, with physical contact.",
                        "ru": "Находящийся сверху чего-либо, опирающийся на что-либо при физическом контакте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on",
                                "target_lang_sense_clarification": "Positioned at the upper surface of, touching from above"
                            },
                            {
                                "target_lang_word": "upon",
                                "target_lang_sense_clarification": "Formal or literary variation of 'on'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "указание на поверхность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7939c821-5526-3d7b-a4b3-a38a89a8d954",
                    "sense_definition": "Aanwezig op een locatie, bij een instelling of tijdens een activiteit.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "locatie_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn jongste broertje zit nog steeds <w>op</w> school.",
                            "target_lang_translations": {
                                "en": "My youngest brother is still <w>in</w> school.",
                                "ru": "Мой младший брат все еще учится <w>в</w> школе."
                            }
                        },
                        {
                            "text": "We zijn vorige week met de hele familie <w>op</w> vakantie geweest.",
                            "target_lang_translations": {
                                "en": "We went <w>on</w> vacation with the whole family last week.",
                                "ru": "На прошлой неделе мы всей семьей были <w>в</w> отпуске."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op school",
                        "op kantoor",
                        "op vakantie",
                        "op bezoek"
                    ],
                    "target_lang_definitions": {
                        "en": "Present at a location, institution, or during an activity.",
                        "ru": "Присутствующий в определенном месте, учреждении или участвующий в деятельности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "Used for general locations or institutions (e.g., at school, at the office)"
                            },
                            {
                                "target_lang_word": "on",
                                "target_lang_sense_clarification": "Used for states or activities (e.g., on holiday, on a visit)"
                            },
                            {
                                "target_lang_word": "in",
                                "target_lang_sense_clarification": "Used for enrollment or being inside (e.g., in school)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "в учреждении, в состоянии (отпуск)"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "на мероприятии, на работе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15b873b9-de13-375e-b799-f24f22135b6a",
                    "sense_definition": "Gebruikt om een specifiek tijdstip of een bepaalde dag aan te duiden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "tijd",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben jarig <w>op</w> de eerste dag van mei.",
                            "target_lang_translations": {
                                "en": "My birthday is <w>on</w> the first day of May.",
                                "ru": "У меня день рождения <w>в</w> первый день мая."
                            }
                        },
                        {
                            "text": "We hebben een afspraak gemaakt <w>op</w> maandag.",
                            "target_lang_translations": {
                                "en": "We made an appointment <w>on</w> Monday.",
                                "ru": "Мы договорились о встрече <w>в</w> понедельник."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op tijd",
                        "op maandag",
                        "op dit moment"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to indicate a specific time or day.",
                        "ru": "Используется для указания конкретного времени или дня."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on",
                                "target_lang_sense_clarification": "Used with days and dates"
                            },
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "Used in specific phrases like 'at this moment'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "с днями недели и датами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "accad4bf-7563-3e41-912f-f2bab676f2ab",
                    "sense_definition": "Met gebruik van een bepaald middel, brandstof of mechanisme.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "middel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De meeste vrachtwagens rijden nog steeds <w>op</w> diesel.",
                            "target_lang_translations": {
                                "en": "Most trucks still run <w>on</w> diesel.",
                                "ru": "Большинство грузовиков все еще ездят <w>на</w> дизеле."
                            }
                        },
                        {
                            "text": "Hij gaat elke ochtend <w>op</w> de fiets naar zijn werk.",
                            "target_lang_translations": {
                                "en": "He goes to work <w>on</w> his bike every morning.",
                                "ru": "Каждое утро он ездит на работу <w>на</w> велосипеде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op gas",
                        "op de fiets",
                        "op elektriciteit"
                    ],
                    "target_lang_definitions": {
                        "en": "Using a specific means, fuel, or mechanism.",
                        "ru": "С использованием определенного средства, топлива или механизма."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on",
                                "target_lang_sense_clarification": "Functioning by means of fuel or a device (e.g., on diesel, on gas)"
                            },
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "Used for modes of transport (e.g., by bike)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "транспорт, топливо, энергия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e7299b6-7aef-3820-a209-9880ef026d35",
                    "sense_definition": "Om een verhouding tussen twee getallen of hoeveelheden aan te geven.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verhouding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ongeveer een <w>op</w> de tien mensen is linkshandig.",
                            "target_lang_translations": {
                                "en": "About one <w>in</w> ten people is left-handed.",
                                "ru": "Примерно один <w>из</w> десяти человек — левша."
                            }
                        },
                        {
                            "text": "De leraar wil de leerlingen een <w>op</w> een begeleiden.",
                            "target_lang_translations": {
                                "en": "The teacher wants to guide the students one <w>on</w> one.",
                                "ru": "Учитель хочет заниматься с учениками один <w>на</w> один."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een op een",
                        "een op de tien"
                    ],
                    "target_lang_definitions": {
                        "en": "To indicate a ratio between two numbers or quantities.",
                        "ru": "Для указания соотношения между двумя числами или количествами."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in",
                                "target_lang_sense_clarification": "Used for statistical ratios (e.g., one in ten)"
                            },
                            {
                                "target_lang_word": "on",
                                "target_lang_sense_clarification": "Used for direct comparison or confrontation (e.g., one on one)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "из",
                                "target_lang_sense_clarification": "пропорция (один из десяти)"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "соотношение (один на один)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fbcd0709-9a32-39df-a9c6-d38aa6df954f",
                    "sense_definition": "Volledig verbruikt of niet meer aanwezig.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "toestand_leeg",
                    "name_type": "no",
                    "examples": [

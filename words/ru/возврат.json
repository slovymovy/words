    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9a98833e-4914-3b9f-abd5-a0ebf645e844",
                    "sense_definition": "Действие по глаголу <w>возвращать</w>; передача денег, товара или имущества обратно владельцу.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Магазин гарантирует полный <w>возврат</w> денег в течение двух недель.",
                            "target_lang_translations": {
                                "en": "The store guarantees a full <w>refund</w> within two weeks."
                            }
                        },
                        {
                            "text": "Мы оформили <w>возврат</w> бракованного товара на складе поставщика.",
                            "target_lang_translations": {
                                "en": "We processed the <w>return</w> of the defective goods at the supplier's warehouse."
                            }
                        }
                    ],
                    "synonyms": [
                        "возвращение",
                        "отдача"
                    ],
                    "common_phrases": [
                        "возврат товара",
                        "возврат денег",
                        "оформить возврат",
                        "возврат долга"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of giving back money, goods, or property to the owner."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "refund",
                                "target_lang_sense_clarification": "money"
                            },
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "goods or items"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbee713a-06bb-36c3-8e4c-a43e494cb7b2",
                    "sense_definition": "Действие по глаголу <w>возвращаться</w>; обратный переход к прежнему состоянию, месту или теме.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Постоянный <w>возврат</w> к прошлым ошибкам мешает двигаться вперёд.",
                            "target_lang_translations": {
                                "en": "Constant <w>return</w> to past mistakes hinders moving forward."
                            }
                        },
                        {
                            "text": "Быстрый <w>возврат</w> капитала в страну укрепил национальную экономику.",
                            "target_lang_translations": {
                                "en": "The rapid <w>return</w> of capital to the country strengthened the national economy."
                            }
                        }
                    ],
                    "synonyms": [
                        "возвращение"
                    ],
                    "common_phrases": [
                        "возврат к прошлому",
                        "точка невозврата",
                        "возврат домой"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of going back to a previous state, place, or topic."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "general action"
                            },
                            {
                                "target_lang_word": "reversion",
                                "target_lang_sense_clarification": "to a previous state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3d85e78-f53d-3216-891f-71626f2dcab7",
                    "sense_definition": "Повторное появление или наступление чего-либо (обычно о явлениях природы или болезнях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи предупредили пациента про возможный <w>возврат</w> симптомов гриппа.",
                            "target_lang_translations": {
                                "en": "Doctors warned the patient about a possible <w>recurrence</w> of flu symptoms."
                            }
                        },
                        {
                            "text": "Синоптики обещают скорый <w>возврат</w> зимних холодов в нашем регионе.",
                            "target_lang_translations": {
                                "en": "Forecasters promise a soon <w>return</w> of winter cold in our region."
                            }
                        }
                    ],
                    "synonyms": [
                        "возвращение"
                    ],
                    "common_phrases": [
                        "возврат болезни",
                        "возврат холодов"
                    ],
                    "target_lang_definitions": {
                        "en": "The reappearance or recurrence of something, typically weather or illness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "recurrence",
                                "target_lang_sense_clarification": "of a disease or problem"
                            },
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "of a phenomenon"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c161fc89-6dd1-350e-a12d-3a36cefc7627",
                    "sense_definition": "Прибыль или полезный результат от финансовых вложений.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компания рассчитывает на быстрый <w>возврат</w> инвестиций в новый проект.",
                            "target_lang_translations": {
                                "en": "The company expects a quick <w>return</w> on investment in the new project."
                            }
                        },
                        {
                            "text": "Этот актив обеспечивает стабильный <w>возврат</w> вложенных средств акционерам.",
                            "target_lang_translations": {
                                "en": "This asset ensures a stable <w>return</w> on invested funds for shareholders."
                            }
                        }
                    ],
                    "synonyms": [
                        "отдача"
                    ],
                    "common_phrases": [
                        "возврат инвестиций",
                        "возврат на капитал"
                    ],
                    "target_lang_definitions": {
                        "en": "The profit or benefit gained from a financial investment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "financial yield"
                            }
                        ]
                    }
                }
            ]
        }
    ]

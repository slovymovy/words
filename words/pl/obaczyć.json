    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1a603993-8082-3422-aa2c-070c1cc7286e",
                    "sense_definition": "Dostrzec kogoś lub coś za pomocą wzroku; skierować wzrok na obiekt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy tylko <obaczył> nadchodzącego wroga, natychmiast sięgnął po broń.",
                            "target_lang_translations": {
                                "ru": "Как только он <увидел> приближающегося врага, он немедленно схватился за оружие."
                            }
                        },
                        {
                            "text": "Musisz koniecznie <obaczyć> ten stary zamek, gdy będziesz w okolicy.",
                            "target_lang_translations": {
                                "ru": "Ты обязательно должен <посмотреть> этот старый замок, когда будешь в округе."
                            }
                        }
                    ],
                    "synonyms": [
                        "zobaczyć",
                        "obejrzeć"
                    ],
                    "common_phrases": [
                        "idź obaczyć",
                        "daj obaczyć"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo rzadko używane we współczesnej polszczyźnie, częste w literaturze dawnej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заметить кого-то или что-то с помощью зрения; направить взгляд на объект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увидеть",
                                "target_lang_sense_clarification": "воспринять зрением"
                            },
                            {
                                "target_lang_word": "посмотреть",
                                "target_lang_sense_clarification": "направить взгляд"
                            },
                            {
                                "target_lang_word": "узреть",
                                "target_lang_sense_clarification": "устаревшее или книжное: увидеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "641b97c9-92b7-35f4-aa14-c5cff4be097c",
                    "sense_definition": "Zrozumieć coś, pojąć sens sytuacji lub przekonać się o czymś w przyszłości.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeszcze <obaczymy>, kto miał rację w tym sporze.",
                            "target_lang_translations": {
                                "ru": "Еще <посмотрим>, кто был прав в этом споре."
                            }
                        },
                        {
                            "text": "Przyjdź jutro na spotkanie, a sam <obaczysz>, jak to działa.",
                            "target_lang_translations": {
                                "ru": "Приходи завтра на встречу, и сам <увидишь>, как это работает."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrozumieć",
                        "pojąć"
                    ],
                    "common_phrases": [
                        "to się obaczy",
                        "zaraz obaczymy"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "regional",
                            "comment": "spotykane w niektórych gwarach"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Понять что-то, осознать смысл ситуации или убедиться в чем-то в будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посмотреть",
                                "target_lang_sense_clarification": "проверить или узнать в будущем (напр. «посмотрим, что будет»)"
                            },
                            {
                                "target_lang_word": "увидеть",
                                "target_lang_sense_clarification": "убедиться лично"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fe6c2a2-e896-3ff1-a75e-7692cf1472cc",
                    "sense_definition": "Spotkać się z kimś wzajemnie; zobaczyć się z drugą osobą.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam wielką nadzieję, że wkrótce znowu się <obaczymy>.",
                            "target_lang_translations": {
                                "ru": "Я очень надеюсь, что скоро мы снова <увидимся>."
                            }
                        },
                        {
                            "text": "Obiecali sobie, że <obaczą> się w rodzinnym domu przed świętami.",
                            "target_lang_translations": {
                                "ru": "Они пообещали друг другу, что <увидятся> в родном доме перед праздниками."
                            }
                        }
                    ],
                    "synonyms": [
                        "zobaczyć się"
                    ],
                    "common_phrases": [
                        "do obaczenia"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Встретиться с кем-то; увидеть друг друга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увидеться",
                                "target_lang_sense_clarification": "встретиться лично"
                            },
                            {
                                "target_lang_word": "свидеться",
                                "target_lang_sense_clarification": "устаревшее или разговорное: встретиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbe9c868-955c-32ab-b2a1-2814f85d7e7e",
                    "sense_definition": "Odzyskać równowagę psychiczną, rozsądek lub świadomość; opamiętać się.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dopiero po dłuższej chwili <obaczył> się i przeprosił za swój wybuch gniewu.",
                            "target_lang_translations": {
                                "ru": "Только спустя долгое время он <опомнился> и извинился за свою вспышку гнева."
                            }
                        },
                        {
                            "text": "Musisz się <obaczyć>, zanim podejmiesz tak ryzykowną decyzję.",
                            "target_lang_translations": {
                                "ru": "Ты должен <опомниться>, прежде чем принимать такое рискованное решение."
                            }
                        }
                    ],
                    "synonyms": [
                        "opamiętać się",
                        "oprzytomnieć",
                        "ocknąć się"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вернуть душевное равновесие, рассудок или сознание; прийти в себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опомниться",
                                "target_lang_sense_clarification": "вернуть способность здраво рассуждать"
                            },
                            {
                                "target_lang_word": "прийти в себя",
                                "target_lang_sense_clarification": "вернуть самообладание"
                            }
                        ]
                    }
                }
            ]
        }

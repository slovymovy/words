            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e51e27b1-be54-3219-aad6-1b3dd3ad4c71",
                    "sense_definition": "Utwór muzyczny przeznaczony do śpiewania, często o charakterze uroczystym, religijnym lub ludowym, różniący się od piosenki poważniejszą tematyką.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "music_composition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy zgromadzeni w kościele głośno śpiewali tę <w>pieśń</w>.",
                            "target_lang_translations": {
                                "ru": "Все собравшиеся в церкви громко пели эту <w>песню</w>."
                            }
                        },
                        {
                            "text": "Stare <w>pieśni</w> ludowe są ważnym elementem naszej kultury narodowej.",
                            "target_lang_translations": {
                                "ru": "Старые народные <w>песни</w> являются важным элементом нашей национальной культуры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pieśń ludowa",
                        "pieśń patriotyczna",
                        "pieśń religijna",
                        "pieśń wojskowa",
                        "śpiewać pieśni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальное произведение для пения, часто торжественного, религиозного или народного характера, отличающееся от простой песенки более серьезной тематикой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "песня",
                                "target_lang_sense_clarification": "музыкальное произведение"
                            },
                            {
                                "target_lang_word": "песнь",
                                "target_lang_sense_clarification": "торжественная или высокая поэзия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebf7d190-435c-3751-b039-d4fa7116b6e0",
                    "sense_definition": "Gatunek poezji lirycznej o regularnej budowie stroficznej i rytmicznej, zazwyczaj poruszający tematykę poważną i podniosłą.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "literary_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jan Kochanowski pisał piękne <w>pieśni</w>, które do dziś są czytane w szkołach.",
                            "target_lang_translations": {
                                "ru": "Ян Кохановский писал прекрасные <w>песни</w>, которые до сих пор читают в школах."
                            }
                        },
                        {
                            "text": "Na lekcji języka polskiego analizowaliśmy budowę i rymy tej <w>pieśni</w>.",
                            "target_lang_translations": {
                                "ru": "На уроке польского языка мы анализировали строение и рифмы этой <w>песни</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zbiór pieśni",
                        "tworzyć pieśni"
                    ],

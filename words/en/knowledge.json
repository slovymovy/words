    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "81cf8837-b729-30c3-ae57-4a86de5eebd3",
                    "sense_definition": "The fact of knowing about something; general understanding or familiarity with a subject, place, or situation.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_understanding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>knowledge</w> of local history is very impressive.",
                            "target_lang_translations": {
                                "ru": "Его <w>познания</w> в местной истории весьма впечатляют."
                            }
                        },
                        {
                            "text": "A secretary should have a good <w>knowledge</w> of computer software.",
                            "target_lang_translations": {
                                "ru": "Секретарь должен обладать хорошим <w>знанием</w> компьютерного программного обеспечения."
                            }
                        }
                    ],
                    "synonyms": [
                        "learning",
                        "awareness"
                    ],
                    "antonyms": [
                        "ignorance"
                    ],
                    "common_phrases": [
                        "general knowledge",
                        "common knowledge",
                        "acquire knowledge",
                        "thirst for knowledge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Факт обладания информацией о чем-либо; общее понимание или знакомство с предметом, местом или ситуацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знание",
                                "target_lang_sense_clarification": "общее владение информацией"
                            },
                            {
                                "target_lang_word": "познания",
                                "target_lang_sense_clarification": "глубокие или обширные знания в области"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e56cddd-e2a5-35a7-a47d-578d67e05052",
                    "sense_definition": "Awareness of a particular fact or situation; the state of having been informed about something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "awareness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She left the party without my <w>knowledge</w>.",
                            "target_lang_translations": {
                                "ru": "Она ушла с вечеринки без моего <w>ведома</w>."
                            }
                        },
                        {
                            "text": "They denied all <w>knowledge</w> of the missing documents.",
                            "target_lang_translations": {
                                "ru": "Они отрицали какую-либо <w>осведомлённость</w> о пропавших документах."
                            }
                        }
                    ],
                    "synonyms": [
                        "awareness",
                        "cognizance"
                    ],
                    "antonyms": [
                        "ignorance"
                    ],
                    "common_phrases": [
                        "to my knowledge",
                        "without my knowledge",
                        "public knowledge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осведомленность о конкретном факте или ситуации; состояние информированности о чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осведомлённость",
                                "target_lang_sense_clarification": "информированность о факте"
                            },
                            {
                                "target_lang_word": "ведома",
                                "target_lang_sense_clarification": "в идиомах: без моего ведома"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ce225ab-78e6-39dd-954d-8d4288845d4a",
                    "sense_definition": "The total of what is known; all information and products of learning accumulated by humanity.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "collective_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The library contained the accumulated <w>knowledge</w> of the ancient world.",
                            "target_lang_translations": {
                                "ru": "Библиотека содержала накопленные <w>знания</w> древнего мира."
                            }
                        },
                        {
                            "text": "Scientific <w>knowledge</w> has expanded rapidly in the last century.",
                            "target_lang_translations": {
                                "ru": "Научное <w>знание</w> стремительно расширилось в прошлом столетии."
                            }
                        }
                    ],
                    "synonyms": [
                        "learning"
                    ],
                    "common_phrases": [
                        "body of knowledge",
                        "scientific knowledge",
                        "human knowledge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность всего известного; вся информация и результаты обучения, накопленные человечеством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знания",
                                "target_lang_sense_clarification": "накопленный опыт (мн. ч.)"
                            },
                            {
                                "target_lang_word": "знание",
                                "target_lang_sense_clarification": "как абстрактное понятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "310b349b-42b3-3fbc-a037-155142b44fee",
                    "sense_definition": "Sexual intimacy or intercourse.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sexual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The archaic legal term carnal <w>knowledge</w> refers to sexual intercourse.",
                            "target_lang_translations": {
                                "ru": "Устаревший юридический термин 'плотское <w>познание</w>' относится к половому сношению."
                            }
                        },
                        {
                            "text": "He was charged with unlawful carnal <w>knowledge</w> under the old statutes.",
                            "target_lang_translations": {
                                "ru": "Его обвинили в незаконном плотском <w>познании</w> согласно старым законам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "carnal knowledge"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "primarily used in the phrase 'carnal knowledge'"
                        },
                        {
                            "trait_type": "form",
                            "comment": "legal or biblical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Половая близость или сношение (обычно в юридическом или устаревшем контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "познание",
                                "target_lang_sense_clarification": "библейское/устаревшее (плотское познание)"
                            },
                            {
                                "target_lang_word": "сношение",
                                "target_lang_sense_clarification": "юридическое (половое сношение)"
                            }
                        ]
                    }
                }
            ]
        },
        {

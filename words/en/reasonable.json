                            {
                                "target_lang_word": "удовлетворительный",
                                "target_lang_sense_clarification": "соответствующий требованиям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2c851d2-2561-3c89-baf7-e72364df8532",
                    "sense_definition": "Not excessive or extreme; kept within appropriate limits.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please try to keep the noise to a <w>reasonable</w> level after midnight.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, постарайтесь поддерживать шум на <w>приемлемом</w> уровне после полуночи."
                            }
                        },
                        {
                            "text": "We need to finish this project within a <w>reasonable</w> amount of time.",
                            "target_lang_translations": {
                                "ru": "Нам нужно закончить этот проект в <w>разумные</w> сроки."
                            }
                        }
                    ],
                    "synonyms": [
                        "moderate",
                        "proper",
                        "temperate"

{
    "word_family": [
        "balanced",
        "unbalanced",
        "imbalance",
        "counterbalance",
        "overbalance",
        "rebalance",
        "balancer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6ac3968f-bd78-3ef9-aa66-38bef823ba83",
                    "sense_definition": "The amount of money that you have in your bank account, or the amount that you still owe.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "financial_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to check my bank <w>balance</w> before I go shopping.",
                            "target_lang_translations": {
                                "ru": "Мне нужно проверить <w>баланс</w> моего банковского счета, прежде чем идти за покупками."
                            }
                        },
                        {
                            "text": "Please pay the outstanding <w>balance</w> on your credit card immediately.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, немедленно оплатите непогашенный <w>остаток</w> по вашей кредитной карте."
                            }
                        }
                    ],
                    "synonyms": [
                        "account",
                        "disinterest"
                    ],
                    "common_phrases": [
                        "bank balance",
                        "account balance",
                        "outstanding balance",
                        "check the balance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сумма денег, имеющаяся на банковском счете, или сумма, которую необходимо выплатить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баланс",
                                "target_lang_sense_clarification": "состояние счета"
                            },
                            {
                                "target_lang_word": "остаток",
                                "target_lang_sense_clarification": "оставшаяся сумма денег"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f2d7435-7f79-35fc-8802-3c06e37c67f5",
                    "sense_definition": "A state where weight or forces are evenly distributed so that someone or something remains upright and steady.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_stability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baby is learning to walk and often loses his <w>balance</w>.",
                            "target_lang_translations": {
                                "ru": "Ребенок учится ходить и часто теряет <w>равновесие</w>."
                            }
                        },
                        {
                            "text": "It takes good <w>balance</w> to ride a bicycle slowly without falling.",
                            "target_lang_translations": {
                                "ru": "Чтобы медленно ехать на велосипеде и не падать, нужно хорошее <w>равновесие</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "equilibrium",
                        "poise",
                        "equipoise"
                    ],
                    "antonyms": [
                        "imbalance",
                        "unbalance",
                        "nonequilibrium",
                        "fall over",
                        "lose one's balance"
                    ],
                    "common_phrases": [
                        "lose one's balance",
                        "keep one's balance",
                        "sense of balance",
                        "off balance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, при котором вес или силы распределены равномерно, что позволяет сохранять устойчивость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "равновесие",
                                "target_lang_sense_clarification": "физическая устойчивость"
                            },
                            {
                                "target_lang_word": "баланс",
                                "target_lang_sense_clarification": "устойчивое положение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9b464b0-bda3-30d2-9f49-5aac5efccfd5",
                    "sense_definition": "A device used for weighing things, usually consisting of a bar with a pan at each end.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chemist measured the powder carefully on a sensitive <w>balance</w>.",
                            "target_lang_translations": {
                                "ru": "Химик тщательно отмерил порошок на чувствительных <w>весах</w>."
                            }
                        },
                        {
                            "text": "She placed the gold on the <w>balance</w> to determine its exact weight.",
                            "target_lang_translations": {
                                "ru": "Она положила золото на <w>весы</w>, чтобы определить его точный вес."
                            }
                        }
                    ],
                    "synonyms": [
                        "scales",
                        "pair of scales",
                        "set of scales",
                        "weighing machine"
                    ],
                    "common_phrases": [
                        "spring balance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибор для взвешивания предметов, обычно состоящий из коромысла с чашами на концах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "весы",
                                "target_lang_sense_clarification": "прибор для взвешивания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af0f5171-fa13-3cef-abd0-603094665fa0",
                    "sense_definition": "A situation where different things exist in equal or correct proportions; the quality of being fair and considering all views.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_harmony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is important to find a healthy <w>balance</w> between work and family life.",
                            "target_lang_translations": {
                                "ru": "Важно найти здоровый <w>баланс</w> между работой и семейной жизнью."
                            }
                        },
                        {
                            "text": "The news report lacked <w>balance</w> because it only showed one side of the story.",
                            "target_lang_translations": {
                                "ru": "Новостному репортажу не хватало <w>объективности</w>, так как в нем была показана только одна сторона истории."
                            }
                        }
                    ],
                    "synonyms": [
                        "fairness",
                        "impartiality",
                        "neutrality",
                        "even-handedness"
                    ],
                    "antonyms": [
                        "bias",
                        "prejudice",
                        "partiality",
                        "unfairness"
                    ],
                    "common_phrases": [
                        "strike a balance",
                        "balance of power",
                        "work-life balance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, когда различные элементы находятся в правильных пропорциях; способность учитывать все точки зрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баланс",
                                "target_lang_sense_clarification": "гармоничное соотношение"
                            },
                            {
                                "target_lang_word": "равновесие",
                                "target_lang_sense_clarification": "состояние покоя или гармонии"
                            },
                            {
                                "target_lang_word": "объективность",
                                "target_lang_sense_clarification": "непредвзятость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2f2f6fe-c5a7-3eaa-9e3c-c003537a785f",
                    "sense_definition": "The part of something that remains after another part has been used, paid, or removed.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "remainder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We spent the <w>balance</w> of the evening watching a movie.",
                            "target_lang_translations": {
                                "ru": "Мы провели <w>остаток</w> вечера за просмотром фильма."
                            }
                        },
                        {
                            "text": "The <w>balance</w> of the order will be delivered next week.",
                            "target_lang_translations": {
                                "ru": "<w>Остаток</w> заказа будет доставлен на следующей неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "disinterest"
                    ],
                    "common_phrases": [
                        "balance of the day",
                        "balance of the year"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть чего-либо, оставшаяся после того, как другая часть была использована, оплачена или удалена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаток",
                                "target_lang_sense_clarification": "оставшаяся часть"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2202865a-4275-31d6-a4d5-c99041b1588b",
                    "sense_definition": "To put or hold something in a steady position so that it does not fall.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_stability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He managed to <w>balance</w> the tray of drinks on one hand.",
                            "target_lang_translations": {
                                "ru": "Ему удалось <w>удержать</w> поднос с напитками на одной руке."
                            }
                        },
                        {
                            "text": "The cat can <w>balance</w> on the top of the narrow fence.",
                            "target_lang_translations": {
                                "ru": "Кошка может <w>балансировать</w> на верхушке узкого забора."
                            }
                        }
                    ],
                    "synonyms": [
                        "poise"
                    ],
                    "antonyms": [
                        "fall over",
                        "lose one's balance"
                    ],
                    "common_phrases": [
                        "balance on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удерживать что-либо в устойчивом положении, чтобы оно не упало."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балансировать",
                                "target_lang_sense_clarification": "сохранять равновесие"
                            },
                            {
                                "target_lang_word": "удерживать в равновесии",
                                "target_lang_sense_clarification": "не давать упасть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aadf7c4b-6efd-3ddd-8193-48da4832ed23",
                    "sense_definition": "To give equal importance to different things or parts; to offset or compare opposing forces.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_harmony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>balance</w> the benefits of the plan against the costs.",
                            "target_lang_translations": {
                                "ru": "Вам нужно <w>взвесить</w> преимущества плана и затраты."
                            }
                        },
                        {
                            "text": "She tries hard to <w>balance</w> her career and her personal life.",
                            "target_lang_translations": {
                                "ru": "Она очень старается <w>совмещать</w> карьеру и личную жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "counterbalance",
                        "equilibrate"
                    ],
                    "antonyms": [
                        "disequilibrate",
                        "unbalance"
                    ],
                    "common_phrases": [
                        "balance the risks",
                        "balance pros and cons"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать равное значение разным вещам; сопоставлять или уравновешивать противодействующие силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уравновешивать",
                                "target_lang_sense_clarification": "приводить в соответствие"
                            },
                            {
                                "target_lang_word": "взвешивать",
                                "target_lang_sense_clarification": "оценивать, сравнивать"
                            },
                            {
                                "target_lang_word": "совмещать",
                                "target_lang_sense_clarification": "находить баланс между делами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a86de597-3965-3762-a215-6aca051d61c0",
                    "sense_definition": "To calculate the total credits and debits of an account to ensure they match.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "financial_account",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accountant stayed late at the office to <w>balance</w> the books.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер задержался в офисе допоздна, чтобы <w>свести баланс</w>."
                            }
                        },
                        {
                            "text": "It is difficult for the government to <w>balance</w> the budget every year.",
                            "target_lang_translations": {
                                "ru": "Правительству трудно <w>сбалансировать</w> бюджет каждый год."
                            }
                        }
                    ],
                    "common_phrases": [
                        "balance the books",
                        "balance the budget"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подсчитывать доходы и расходы, чтобы убедиться в их соответствии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сводить баланс",
                                "target_lang_sense_clarification": "бухгалтерский учет"
                            },
                            {
                                "target_lang_word": "сбалансировать",
                                "target_lang_sense_clarification": "привести в соответствие (бюджет)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "31d82f0b-3f48-353e-b1fd-e3418130e966",
                    "sense_definition": "Związany z ekranem urządzenia elektronicznego, wyświetlany na nim lub służący do jego obsługi.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tech_display",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ustawieniach tabletu możesz włączyć dużą klawiaturę <w>ekranową</w>.",
                            "target_lang_translations": {
                                "ru": "В настройках планшета можно включить большую <w>экранную</w> клавиатуру."
                            }
                        },
                        {
                            "text": "Menu <w>ekranowe</w> pozwala na szybką zmianę jasności i kontrastu monitora.",
                            "target_lang_translations": {
                                "ru": "<w>Экранное</w> меню позволяет быстро изменить яркость и контрастность монитора."
                            }
                        }
                    ],
                    "common_phrases": [
                        "klawiatura ekranowa",
                        "czytnik ekranowy",
                        "menu ekranowe",
                        "wygaszacz ekranowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с экраном электронного устройства, отображаемый на нем или служащий для управления им."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экранный",
                                "target_lang_sense_clarification": "относящийся к дисплею, монитору"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ff87922-3cdf-3e19-8468-80d22931444b",
                    "sense_definition": "Związany z filmem, kinem lub telewizją; odnoszący się do tego, co jest przedstawiane widzom.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "film_media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>ekranowy</w> wizerunek znacznie różni się od tego, jaki jest prywatnie.",
                            "target_lang_translations": {
                                "ru": "Его <w>экранный</w> образ значительно отличается от того, какой он в частной жизни."
                            }
                        },
                        {
                            "text": "Aktor narzekał na zbyt krótki czas <w>ekranowy</w> w nowej produkcji filmowej.",
                            "target_lang_translations": {
                                "ru": "Актер жаловался на слишком короткое <w>экранное</w> время в новой кинопостановке."
                            }
                        }
                    ],
                    "synonyms": [
                        "filmowy",
                        "kinowy",
                        "telewizyjny"
                    ],
                    "antonyms": [
                        "pozaekranowy"
                    ],
                    "common_phrases": [
                        "debiut ekranowy",
                        "czas ekranowy",
                        "partner ekranowy",
                        "adaptacja ekranowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с кино, фильмами или телевидением; относящийся к тому, что показывается зрителям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экранный",
                                "target_lang_sense_clarification": "относящийся к кино или телевидению"
                            },
                            {
                                "target_lang_word": "кинематографический",
                                "target_lang_sense_clarification": "относящийся к искусству кино"
                            }
                        ]
                    }
                }
            ]
        }

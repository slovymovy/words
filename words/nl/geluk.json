            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d412f646-acff-309e-ad7c-da4465bfbe28",
                    "sense_definition": "Een gunstige loop van omstandigheden die in iemands voordeel werkt; toeval dat goed uitpakt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "luck",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met een beetje <w>geluk</w> halen we de laatste trein naar huis nog.",
                            "target_lang_translations": {
                                "en": "With a bit of <w>luck</w>, we'll still catch the last train home."
                            }
                        },
                        {
                            "text": "Hij had het <w>geluk</w> dat hij net op tijd binnen was voor de regenbui.",
                            "target_lang_translations": {
                                "en": "He had the <w>luck</w> that he was inside just in time for the rain."
                            }
                        }
                    ],
                    "synonyms": [
                        "mazzel",
                        "meevaller"
                    ],
                    "antonyms": [
                        "pech",
                        "ongeluk"
                    ],
                    "common_phrases": [
                        "veel geluk",
                        "geluk hebben",
                        "met een beetje geluk"
                    ],
                    "target_lang_definitions": {
                        "en": "A favorable course of circumstances working to someone's advantage; chance that turns out well."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "luck",
                                "target_lang_sense_clarification": "chance occurrence"
                            },
                            {
                                "target_lang_word": "good fortune",
                                "target_lang_sense_clarification": "favorable outcome"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d95d3edd-63b1-3128-a417-0df5592f1ba6",
                    "sense_definition": "Een prettige gemoedstoestand waarin men zeer tevreden en blij is met het leven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "happiness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Geld is handig, maar het garandeert geen <w>geluk</w> of gezondheid.",
                            "target_lang_translations": {
                                "en": "Money is useful, but it guarantees neither <w>happiness</w> nor health."
                            }
                        },
                        {
                            "text": "Ze straalde van <w>geluk</w> toen ze haar pasgeboren kleinkind vasthield.",
                            "target_lang_translations": {
                                "en": "She beamed with <w>happiness</w> when she held her newborn grandchild."
                            }
                        }
                    ],
                    "antonyms": [
                        "ongeluk"
                    ],
                    "common_phrases": [
                        "op zoek naar geluk",
                        "huiselijk geluk"
                    ],
                    "target_lang_definitions": {
                        "en": "A pleasant state of mind in which one is very satisfied and happy with life."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "happiness",
                                "target_lang_sense_clarification": "state of well-being"
                            }
                        ]
                    }
                }
            ]
        },
        {

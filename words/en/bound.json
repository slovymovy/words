                    "common_phrases": [
                        "out of bounds",
                        "within bounds",
                        "know no bounds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предел или граница, определяющая область или допустимое поведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грани́ца",

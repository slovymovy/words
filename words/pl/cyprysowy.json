    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e9983357-a3b8-3cfe-8b2b-9aea4ee385aa",
                    "sense_definition": "Wykonany z drewna cyprysu lub uzyskany z innej części tego drzewa.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W salonie stała zabytkowa <w>cyprysowa</w> skrzynia, która pięknie pachniała lasem.",
                            "target_lang_translations": {
                                "ru": "В гостиной стоял старинный <w>кипарисовый</w> сундук, который прекрасно пах лесом."
                            }
                        },
                        {
                            "text": "Do masażu relaksacyjnego najlepiej użyć naturalny olejek <w>cyprysowy</w>.",
                            "target_lang_translations": {
                                "ru": "Для расслабляющего массажа лучше всего использовать натуральное <w>кипарисовое</w> масло."
                            }
                        }
                    ],
                    "common_phrases": [
                        "olejek cyprysowy",
                        "drewno cyprysowe",
                        "skrzynia cyprysowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный из древесины кипариса или полученный из этого дерева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кипарисовый",
                                "target_lang_sense_clarification": "сделанный из кипариса"
                            },
                            {
                                "target_lang_word": "кипарисный",
                                "target_lang_sense_clarification": "изготовленный из дерева"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69ef475e-1039-34ca-b7b4-00a02ce0a8fd",
                    "sense_definition": "Związany z cyprysem, będący jego częścią lub składający się z drzew cyprysowych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "relational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wakacji we Włoszech zachwyciła nas długa aleja <w>cyprysowa</w>.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска в Италии нас восхитила длинная <w>кипарисовая</w> аллея."
                            }
                        },
                        {
                            "text": "Mała szyszka <w>cyprysowa</w> spadła prosto pod moje nogi.",
                            "target_lang_translations": {
                                "ru": "Маленькая <w>кипарисовая</w> шишка упала прямо мне под ноги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gaj cyprysowy",
                        "aleja cyprysowa",
                        "szyszka cyprysowa",
                        "gałązka cyprysowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к кипарису, являющийся его частью или состоящий из кипарисов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кипарисовый",
                                "target_lang_sense_clarification": "относящийся к дереву"
                            }
                        ]
                    }
                }
            ]
        }
    ]

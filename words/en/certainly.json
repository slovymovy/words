            "senses": [
                {
                    "sense_id": "f9f4d4b6-dfbd-3cbb-a253-4da5f82e3b2c",
                    "sense_definition": "Used to give an emphatic affirmative answer or to express agreement.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "\"Can you help me with these bags?\" \"<w>Certainly</w>, I would be happy to.\"",
                            "target_lang_translations": {
                                "ru": "«Вы можете помочь мне с этими сумками?» «— <w>Конечно</w>, с удовольствием»."
                            }
                        },
                        {
                            "text": "\"May I sit here?\" \"<w>Certainly</w>, the seat is free.\"",
                            "target_lang_translations": {
                                "ru": "«Можно мне здесь сесть?» «— <w>Конечно</w>, место свободно»."
                            }
                        }
                    ],
                    "synonyms": [
                        "absolutely",
                        "of course",
                        "sure",
                        "sure thing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения решительного согласия или подтверждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конечно",
                                "target_lang_sense_clarification": "выражение согласия"
                            },
                            {
                                "target_lang_word": "безусловно",
                                "target_lang_sense_clarification": "усиленное подтверждение"
                            },
                            {
                                "target_lang_word": "разумеется",
                                "target_lang_sense_clarification": "само собой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5928ab1b-4002-3748-9d16-8d547881cec6",
                    "sense_definition": "Without any doubt; definitely or surely.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accident was <w>certainly</w> caused by a mechanical failure rather than human error.",
                            "target_lang_translations": {
                                "ru": "Авария, <w>несомненно</w>, была вызвана механической поломкой, а не человеческой ошибкой."
                            }
                        },
                        {
                            "text": "It will <w>certainly</w> rain later today, so you should take an umbrella.",
                            "target_lang_translations": {
                                "ru": "Сегодня <w>наверняка</w> пойдет дождь, так что тебе стоит взять зонт."
                            }
                        },
                        {
                            "text": "I will <w>certainly</w> not allow such behavior in my classroom.",
                            "target_lang_translations": {
                                "ru": "Я, <w>безусловно</w>, не допущу такого поведения в моем классе."
                            }
                        }
                    ],
                    "synonyms": [
                        "definitely",
                        "undoubtedly",
                        "surely",
                        "truly",
                        "indeed",
                        "really",
                        "unquestionably"
                    ],
                    "antonyms": [
                        "arguably",
                        "doubtfully",
                        "questionably",
                        "uncertainly"
                    ],
                    "common_phrases": [
                        "almost certainly",
                        "certainly not",
                        "most certainly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без сомнения; определенно или наверняка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несомненно",
                                "target_lang_sense_clarification": "без сомнения"
                            },
                            {

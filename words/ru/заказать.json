            "pos": "verb",
            "senses": [
                {
                    "sense_id": "913ef85c-b026-3a6d-be67-4f3b91830222",
                    "sense_definition": "Поручить кому-либо изготовить, выполнить, привезти или доставить что-либо за плату.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "order_service",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <заказать> пиццу на ужин, так как не хотели готовить.",
                            "target_lang_translations": {
                                "en": "We decided to <order> a pizza for dinner since we didn't want to cook."
                            }
                        },
                        {
                            "text": "Вам нужно заранее <заказать> столик в этом ресторане, чтобы попасть туда вечером.",
                            "target_lang_translations": {
                                "en": "You need to <reserve> a table at this restaurant in advance to get in there in the evening."
                            }
                        }
                    ],
                    "antonyms": [
                        "отменить"
                    ],
                    "common_phrases": [
                        "заказать столик",
                        "заказать такси",
                        "заказать еду",
                        "заказать билеты",
                        "заказать пропуск"
                    ],
                    "target_lang_definitions": {
                        "en": "To request goods or services to be made, supplied, or delivered, usually for payment; to make a reservation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "order",
                                "target_lang_sense_clarification": "food, goods, services, taxis"
                            },
                            {
                                "target_lang_word": "reserve",
                                "target_lang_sense_clarification": "tables, tickets, rooms"
                            },
                            {
                                "target_lang_word": "book",
                                "target_lang_sense_clarification": "tables, tickets, appointments"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c3dfb70-267a-3dce-814e-30860ccfcb09",
                    "sense_definition": "Сделать невозможным или недоступным; запретить (обычно используется в устойчивых выражениях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "forbid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему туда дорога <заказана> после того громкого скандала.",
                            "target_lang_translations": {
                                "en": "The way there is <barred> to him after that loud scandal."
                            }
                        },
                        {
                            "text": "Сердцу не <закажешь>, кого ему любить и о ком страдать.",
                            "target_lang_translations": {
                                "en": "You cannot <command> the heart whom to love and whom to suffer for."
                            }

                {
                    "sense_id": "959541cd-4f15-3af4-86a7-b78ee27e2bd3",
                    "sense_definition": "A common white substance, consisting mainly of sodium chloride, used to season and preserve food.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_preservation",
                    "name_type": "no",

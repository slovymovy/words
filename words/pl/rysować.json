    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "87c30966-2557-32a2-9ae0-c71fa07d9106",
                    "sense_definition": "Tworzyć obraz, wzór lub szkic na powierzchni (np. papierze) za pomocą narzędzia takiego jak ołówek, kredka, węgiel lub pędzel.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "artistic_creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci w przedszkolu chętnie <rysują> kolorowe domki i drzewa.",
                            "target_lang_translations": {
                                "ru": "Дети в детском саду охотно <рисуют> цветные домики и деревья."
                            }
                        },
                        {
                            "text": "Muszę <narysować> dokładny plan pokoju, żeby kupić odpowiednie meble.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <нарисовать> точный план комнаты, чтобы купить подходящую мебель."
                            }
                        }
                    ],
                    "synonyms": [
                        "szkicować",
                        "kreślić"
                    ],
                    "antonyms": [
                        "gumować",
                        "wymazywać"
                    ],
                    "common_phrases": [
                        "rysować ołówkiem",
                        "rysować kredkami",
                        "rysować z natury",
                        "ładnie rysować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать изображение, узор или набросок на поверхности (например, бумаге) с помощью инструмента, такого как карандаш, мелок, уголь или кисть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рисовать",
                                "target_lang_sense_clarification": "создавать изображение"
                            },
                            {
                                "target_lang_word": "чертить",
                                "target_lang_sense_clarification": "создавать чертеж или схему"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b8c48af-8131-3335-903b-98e0d36bd06d",
                    "sense_definition": "Stawać się widocznym, wyłaniać się z tła lub ciemności; o sytuacjach: stawać się jasnym, wyraźnym lub prawdopodobnym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na horyzoncie powoli zaczęły <rysować> się kontury wysokich gór.",
                            "target_lang_translations": {
                                "ru": "На горизонте медленно начали <вырисовываться> контуры высоких гор."
                            }
                        },
                        {
                            "text": "Przed naszą firmą <rysuje> się szansa na duży sukces finansowy.",
                            "target_lang_translations": {
                                "ru": "Перед нашей фирмой <вырисовывается> шанс на большой финансовый успех."
                            }
                        }
                    ],
                    "synonyms": [
                        "uwidaczniać się",
                        "zarysowywać się",
                        "widnieć"
                    ],
                    "antonyms": [
                        "znikać",
                        "rozpływać się",
                        "zacierać się"
                    ],
                    "common_phrases": [
                        "rysować się w tle",
                        "rysować się wyraźnie",
                        "rysować się na horyzoncie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться видимым, выделяться на фоне или в темноте; о ситуациях: становиться ясным, чётким или вероятным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вырисовываться",
                                "target_lang_sense_clarification": "становиться видным, проявляться"
                            },
                            {
                                "target_lang_word": "обозначаться",
                                "target_lang_sense_clarification": "становиться заметным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cd01b79-cb1a-32a0-b588-e9418bf28c29",
                    "sense_definition": "Robić rysy, zadrapania lub nacięcia na twardej powierzchni ostrym przedmiotem.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Piasek wniesiony na butach może trwale <rysować> drewnianą podłogę.",
                            "target_lang_translations": {
                                "ru": "Песок, принесенный на обуви, может сильно <царапать> деревянный пол."
                            }
                        },
                        {
                            "text": "Nie powinieneś <rysować> ławki cyrklem, bo zniszczysz lakier.",
                            "target_lang_translations": {
                                "ru": "Ты не должен <царапать> парту циркулем, иначе испортишь лак."
                            }
                        }
                    ],
                    "synonyms": [
                        "drapać",
                        "zarysowywać się"
                    ],
                    "antonyms": [
                        "wygładzać",
                        "polerować"
                    ],
                    "common_phrases": [
                        "rysować szkło",
                        "rysować powierzchnię",
                        "rysować lakier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать царапины, повреждения или надрезы на твердой поверхности острым предметом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "царапать",
                                "target_lang_sense_clarification": "оставлять следы острым предметом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6bc9695-7523-3633-8b66-de188bf9a350",
                    "sense_definition": "Ulegać zarysowaniu, pokrywać się rysami (zwykle o gładkich przedmiotach).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_damage_passive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ekran twojego telefonu bardzo łatwo <się rysuje> bez folii ochronnej.",
                            "target_lang_translations": {
                                "ru": "Экран твоего телефона очень легко <царапается> без защитной пленки."
                            }
                        },
                        {
                            "text": "Ten materiał jest wytrzymały i nie <rysuje> się nawet przy intensywnym użytkowaniu.",
                            "target_lang_translations": {
                                "ru": "Этот материал прочный и не <царапается> даже при интенсивном использовании."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmatowić się",
                        "podrapać się"
                    ],
                    "antonyms": [
                        "wygładzać się",
                        "polerować się"
                    ],
                    "common_phrases": [
                        "łatwo się rysować",
                        "szybko się rysować"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрываться царапинами, повреждаться (обычно о гладких предметах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "царапаться",
                                "target_lang_sense_clarification": "получать повреждения поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75faba48-5272-3b3d-808e-b457d1e3773e",
                    "sense_definition": "Opisywać coś w sposób obrazowy i plastyczny, przedstawiać wizję czegoś słowami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_description",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autor książki wspaniale <rysuje> psychologiczne portrety swoich bohaterów.",
                            "target_lang_translations": {
                                "ru": "Автор книги великолепно <рисует> психологические портреты своих героев."
                            }
                        },
                        {
                            "text": "Polityk <rysował> przed wyborcami wizję bezpiecznej i bogatej przyszłości.",
                            "target_lang_translations": {
                                "ru": "Политик <рисовал> перед избирателями картину безопасного и богатого будущего."
                            }
                        }
                    ],
                    "synonyms": [
                        "kreślić"
                    ],
                    "antonyms": [
                        "mącić",
                        "mataczyć"
                    ],
                    "common_phrases": [
                        "rysować słowami",
                        "rysować wizję",
                        "rysować obraz sytuacji"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary or rhetorical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывать что-либо образно и живописно, представлять видение чего-либо словами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рисовать",
                                "target_lang_sense_clarification": "описывать словами"
                            },
                            {
                                "target_lang_word": "изображать",
                                "target_lang_sense_clarification": "представлять в описании"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "108ebfdf-eb45-3f8c-aee1-cf6dd862ce5f",
                    "sense_definition": "Szczególne podkreślenie wagi czegoś w wypowiedzi, tekście lub działaniu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W swoim przemówieniu polityk położył <w>akcent</w> na sprawy gospodarcze.",

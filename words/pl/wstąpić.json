    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bbe4f372-27f0-312b-9782-49497e911933",
                    "sense_definition": "Odwiedzić kogoś lub jakieś miejsce na krótki czas, zazwyczaj przy okazji załatwiania innych spraw lub będąc w pobliżu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "visit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <w>wstąpić</w> do sklepu po drodze do domu, żeby kupić mleko.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>зайти</w> в магазин по дороге домой, чтобы купить молоко."
                            }
                        },
                        {
                            "text": "Czy możecie <w>wstąpić</w> do nas na kawę w przyszły weekend?",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>зайти</w> к нам на кофе в следующие выходные?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wpaść",
                        "zajrzeć",
                        "odwiedzić",
                        "zajść"
                    ],
                    "common_phrases": [
                        "wstąpić po drodze",
                        "wstąpić na chwilę",
                        "wstąpić do sklepu",
                        "wstąpić na kawę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зайти к кому-то или куда-то ненадолго, обычно по пути или заодно с другими делами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зайти",
                                "target_lang_sense_clarification": "посетить ненадолго"
                            },
                            {
                                "target_lang_word": "заглянуть",
                                "target_lang_sense_clarification": "быстро навестить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cb5cc00-debf-3a08-8f74-8a8f85e711a8",
                    "sense_definition": "Stać się oficjalnym członkiem jakiejś organizacji, stowarzyszenia, grupy religijnej lub formacji wojskowej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "membership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek postanowił <w>wstąpić</w> do wojska, gdy miał osiemnaście lat.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка решил <w>вступить</w> в армию, когда ему было восемнадцать лет."
                            }
                        },
                        {
                            "text": "Wielu młodych ludzi chce <w>wstąpić</w> do tej organizacji charytatywnej.",
                            "target_lang_translations": {
                                "ru": "Многие молодые люди хотят <w>вступить</w> в эту благотворительную организацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "przystąpić",
                        "zapisać się",
                        "dołączyć się",
                        "przyłączyć się"
                    ],
                    "antonyms": [
                        "wystąpić",
                        "odejść"
                    ],
                    "common_phrases": [
                        "wstąpić do wojska",
                        "wstąpić do zakonu",
                        "wstąpić do partii",
                        "wstąpić do policji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать официальным членом организации, группы, религиозного сообщества или воинского формирования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вступить",
                                "target_lang_sense_clarification": "стать участником"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "749ede99-eeb1-3175-a591-0b39a70e072a",
                    "sense_definition": "Zacząć nagle kierować czyimś zachowaniem, powodując nietypowe, gwałtowne lub dziwne reakcje (używane w kontekście emocji, sił wyższych lub w idiomach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wiem, co w niego <w>wstąpiło</w>, nigdy wcześniej tak na mnie nie krzyczał.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, что на него <w>нашло</w>, он никогда раньше так на меня не кричал."
                            }
                        },
                        {
                            "text": "Nagle w zawodników <w>wstąpiła</w> nowa energia i zaczęli grać znacznie lepiej.",
                            "target_lang_translations": {
                                "ru": "Внезапно в игроков <w>вселилась</w> новая энергия, и они начали играть значительно лучше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "co w ciebie wstąpiło",
                        "diabeł w niego wstąpił",
                        "wstąpiła nowa nadzieja",
                        "licho wstąpiło"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in phrases asking about strange behavior"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно овладеть чьим-то поведением, вызывая странные реакции (об эмоциях или сверхъестественных силах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "найти",
                                "target_lang_sense_clarification": "в идиомах: что на него нашло"
                            },
                            {
                                "target_lang_word": "вселиться",
                                "target_lang_sense_clarification": "о духах или чувствах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98a80a7a-01b0-3e6b-b1d0-280e7b35c419",
                    "sense_definition": "Wejść na miejsce położone wyżej, zająć ważne, oficjalne stanowisko lub pozycję (często w kontekście historycznym lub uroczystym).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ascend",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po śmierci ojca młody książę <w>wstąpił</w> na tron.",
                            "target_lang_translations": {
                                "ru": "После смерти отца молодой принц <w>взошёл</w> на трон."
                            }
                        },
                        {
                            "text": "Zwycięzca zawodów dumnie <w>wstąpił</w> na najwyższy stopień podium.",
                            "target_lang_translations": {
                                "ru": "Победитель соревнований гордо <w>взошёл</w> на высшую ступень пьедестала."
                            }
                        }
                    ],
                    "synonyms": [
                        "wejść",
                        "wkroczyć",
                        "wznieść się"
                    ],
                    "common_phrases": [
                        "wstąpić na tron",
                        "wstąpić na podium",
                        "wstąpić na stopień"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often literary or formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подняться на возвышенное место или занять высокую официальную должность (трон)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взойти",
                                "target_lang_sense_clarification": "подняться наверх"
                            },
                            {
                                "target_lang_word": "вступить",
                                "target_lang_sense_clarification": "занять место"
                            }
                        ]
                    }
                }
            ]
        }

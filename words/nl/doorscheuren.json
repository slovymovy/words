                    "sense_id": "468fddfd-499f-3e9e-a2d0-213c9a078b94",
                    "sense_definition": "Iets volledig doormidden of in stukken scheuren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "tearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jason had alle liefdesbrieven uit woede <w>doorgescheurd</w>.",
                            "target_lang_translations": {
                                "ru": "Джейсон в ярости <w>разорвал</w> все любовные письма.",
                                "en": "Jason had <w>torn</w> all the love letters <w>in half</w> out of anger."
                            }
                        },
                        {
                            "text": "Je moet het papier precies op de stippellijn <w>doorscheuren</w>.",
                            "target_lang_translations": {
                                "ru": "Нужно <w>разорвать</w> бумагу точно по пунктирной линии.",
                                "en": "You must <w>tear</w> the paper <w>apart</w> exactly on the dotted line."
                            }
                        }
                    ],
                    "synonyms": [
                        "verscheuren",
                        "kapotscheuren"
                    ],
                    "antonyms": [
                        "plakken",
                        "lijmen"
                    ],
                    "common_phrases": [
                        "een brief doorscheuren",
                        "papier doorscheuren",
                        "midden doorscheuren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разорвать что-либо полностью пополам или на куски.",
                        "en": "To tear something completely in half or into pieces."
                    },
                    "translations": {

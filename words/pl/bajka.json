    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b0bf908e-3338-3327-a5e9-eaf3b81fcf3a",
                    "sense_definition": "Utwór o treści fantastycznej, opowiadany lub czytany dzieciom; baśń.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "stories",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdego wieczoru mama czyta synkowi <bajkę> na dobranoc.",
                            "target_lang_translations": {
                                "ru": "Каждый вечер мама читает сыну <сказку> на ночь."
                            }
                        },
                        {
                            "text": "Kopciuszek to piękna <bajka>, którą znają dzieci na całym świecie.",
                            "target_lang_translations": {
                                "ru": "Золушка — это красивая <сказка>, которую знают дети во всём мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "baśń"
                    ],
                    "common_phrases": [
                        "czytać bajki",
                        "bajka na dobranoc",
                        "jak z bajki",
                        "świat bajek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произведение с фантастическим содержанием, рассказываемое или читаемое детям; сказка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сказка",
                                "target_lang_sense_clarification": "волшебная история"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80e9b461-4142-39e5-b3b4-c63ae51c774b",
                    "sense_definition": "Film lub serial animowany przeznaczony dla dzieci.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "stories",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sobotnie poranki zawsze oglądamy w telewizji <bajki>.",
                            "target_lang_translations": {
                                "ru": "По субботам утром мы всегда смотрим <мультики> по телевизору."
                            }
                        },
                        {
                            "text": "Moja młodsza siostra uwielbia <bajkę> o psim patrolu.",
                            "target_lang_translations": {
                                "ru": "Моя младшая сестра обожает <мультик> про Щенячий патруль."
                            }
                        }
                    ],
                    "synonyms": [
                        "kreskówka"
                    ],
                    "common_phrases": [
                        "oglądać bajki",
                        "ulubiona bajka",
                        "postać z bajki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Анимационный фильм или сериал, предназначенный для детей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мультфильм",
                                "target_lang_sense_clarification": "анимационное произведение"
                            },
                            {
                                "target_lang_word": "мультик",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba81e62d-69fb-344f-92ff-4f450ce2c733",
                    "sense_definition": "Zmyślona historia, informacja nieprawdziwa lub bzdura.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wierzę w ani jedno twoje słowo, to są zwykłe <bajki>.",
                            "target_lang_translations": {
                                "ru": "Я не верю ни единому твоему слову, это обычные <сказки>."
                            }
                        },
                        {
                            "text": "Przestań opowiadać <bajki> i powiedz wreszcie, co się stało.",
                            "target_lang_translations": {
                                "ru": "Перестань рассказывать <сказки> и скажи наконец, что случилось."
                            }
                        }
                    ],
                    "synonyms": [
                        "bujda",
                        "bzdura",
                        "bajda",
                        "dyrdymały"
                    ],
                    "common_phrases": [
                        "opowiadać bajki",
                        "wierzyć w bajki",
                        "włożyć między bajki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выдуманная история, неправдивая информация или чепуха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сказка",
                                "target_lang_sense_clarification": "выдумка, ложь (переносное значение)"
                            },
                            {
                                "target_lang_word": "выдумка",
                                "target_lang_sense_clarification": "неправда"
                            },
                            {
                                "target_lang_word": "небылица",
                                "target_lang_sense_clarification": "неправдоподобная история"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08b75bd7-18b1-3ab8-8a4d-915e04041a1e",
                    "sense_definition": "Krótki utwór literacki, zwykle wierszowany, zawierający morał i często posługujący się alegorią zwierzęcą.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "stories",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lekcji języka polskiego omawialiśmy pouczające <bajki> Ignacego Krasickiego.",
                            "target_lang_translations": {
                                "ru": "На уроке польского языка мы обсуждали поучительные <басни> Игнация Красицкого."
                            }
                        },
                        {
                            "text": "Każda <bajka> kończy się morałem, który uczy nas właściwego postępowania.",
                            "target_lang_translations": {
                                "ru": "Каждая <басня> заканчивается моралью, которая учит нас правильному поведению."
                            }
                        }
                    ],
                    "common_phrases": [
                        "morał z bajki",
                        "pisać bajki",
                        "bajki Ezopa"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary term"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткое литературное произведение, обычно в стихах, содержащее мораль и часто использующее аллегорию с животными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "басня",
                                "target_lang_sense_clarification": "литературный жанр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "844846e8-693f-3c0f-8077-9a087303fa19",
                    "sense_definition": "Sfera zainteresowań, specyficzna sytuacja lub sprawa (używane w wyrażeniach idiomatycznych).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie lubię chodzić do opery, to zupełnie nie moja <bajka>.",
                            "target_lang_translations": {
                                "ru": "Я не люблю ходить в оперу, это совершенно не моя <стихия>."
                            }
                        },
                        {
                            "text": "Naprawa samochodów to dla mnie inna <bajka>, kompletnie się na tym nie znam.",
                            "target_lang_translations": {
                                "ru": "Ремонт автомобилей — это для меня другая <история>, я в этом совершенно не разбираюсь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "to nie moja bajka",
                        "inna bajka",
                        "z innej bajki"
                    ],

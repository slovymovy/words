{
    "word_family": [
        "perception",
        "perceptive",
        "perceptible",
        "imperceptible",
        "perceiver"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fb64ccdd-5e3e-3ab9-9b6e-75c622edc1c3",
                    "sense_definition": "To become aware of something through the senses, especially sight or hearing.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "awareness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cats can <w>perceive</w> sounds that are too high for humans to hear.",
                            "target_lang_translations": {
                                "ru": "Кошки могут <w>воспринимать</w> звуки, которые слишком высоки для человеческого слуха."
                            }
                        },
                        {
                            "text": "She could <w>perceive</w> a subtle change in the atmosphere of the room.",
                            "target_lang_translations": {
                                "ru": "Она смогла <w>уловить</w> едва заметное изменение в атмосфере комнаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "sense",
                        "discern",
                        "make out"
                    ],
                    "common_phrases": [
                        "perceive a difference",
                        "perceive a threat",
                        "perceive reality"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осознавать или замечать что-либо с помощью органов чувств (зрения, слуха)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспринимать",
                                "target_lang_sense_clarification": "органами чувств"
                            },
                            {
                                "target_lang_word": "ощущать",
                                "target_lang_sense_clarification": "чувствовать"
                            },
                            {
                                "target_lang_word": "улавливать",
                                "target_lang_sense_clarification": "различать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5a18255-02a1-3c55-b8f6-1adf09c5c967",
                    "sense_definition": "To interpret or regard someone or something in a particular way.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "interpretation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new tax law was <w>perceived</w> as unfair by the general public.",
                            "target_lang_translations": {
                                "ru": "Новый налоговый закон был <w>воспринят</w> общественностью как несправедливый."
                            }
                        },
                        {
                            "text": "He is generally <w>perceived</w> to be the best candidate for the job.",
                            "target_lang_translations": {
                                "ru": "Его, как правило, <w>воспринимают</w> как лучшего кандидата на эту должность."
                            }
                        }
                    ],
                    "synonyms": [
                        "ken",
                        "discern"
                    ],
                    "common_phrases": [
                        "perceive as",
                        "perceive to be",
                        "widely perceived"
                    ],
                    "target_lang_definitions": {
                        "ru": "Понимать, оценивать или относиться к кому-либо или чему-либо определённым образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспринимать",
                                "target_lang_sense_clarification": "понимать, расценивать"
                            },
                            {
                                "target_lang_word": "расценивать",
                                "target_lang_sense_clarification": "считать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
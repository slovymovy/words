{
    "word_family": [
        "los",
        "laten",
        "loslating"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fdff3873-4fd9-32d0-b901-9bae8e450e76",
                    "sense_definition": "Iets of iemand niet langer met de handen vasthouden; de greep verslappen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je doet pijn aan mijn arm, <w>laat</w> me alsjeblieft <w>los</w>.",
                            "target_lang_translations": {
                                "en": "You are hurting my arm, please <w>let go</w> of me.",
                                "ru": "Ты делаешь мне больно, пожалуйста, <w>отпусти</w> меня."
                            }
                        },
                        {
                            "text": "Hij <w>liet</w> het touw <w>los</w> en de boot dreef langzaam weg.",
                            "target_lang_translations": {
                                "en": "He <w>let go</w> of the rope and the boat slowly drifted away.",
                                "ru": "Он <w>отпустил</w> веревку, и лодка медленно поплыла прочь."
                            }
                        }
                    ],
                    "synonyms": [
                        "laten gaan"
                    ],
                    "antonyms": [
                        "vasthouden",
                        "grijpen",
                        "vastpakken"
                    ],
                    "common_phrases": [
                        "laat me los",
                        "het stuur loslaten",
                        "elkaar loslaten"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop holding something or someone with one's hands; to relax one's grip.",
                        "ru": "Перестать держать что-то или кого-то руками; ослабить хватку."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let go",
                                "target_lang_sense_clarification": "physical grip"
                            },
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "formal or mechanical"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отпускать",
                                "target_lang_sense_clarification": "физически"
                            },
                            {
                                "target_lang_word": "выпускать",
                                "target_lang_sense_clarification": "из рук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc31558e-b0e6-3020-9676-09916f8c72cf",
                    "sense_definition": "Stoppen met piekeren over iets of emotioneel afstand nemen van iemand; de controle opgeven.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotional_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is beter om het verleden <w>los</w> te <w>laten</w> en vooruit te kijken.",
                            "target_lang_translations": {
                                "en": "It is better to <w>let go</w> of the past and look ahead.",
                                "ru": "Лучше <w>отпустить</w> прошлое и смотреть вперед."
                            }
                        },
                        {
                            "text": "Ze vond het moeilijk om haar kinderen <w>los</w> te <w>laten</w> toen ze gingen studeren.",
                            "target_lang_translations": {
                                "en": "She found it difficult to <w>let go</w> of her children when they went off to study.",
                                "ru": "Ей было трудно <w>отпустить</w> детей, когда они уехали учиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "laten varen",
                        "vergeten",
                        "accepteren"
                    ],
                    "antonyms": [
                        "piekeren",
                        "vasthouden aan",
                        "opkroppen"
                    ],
                    "common_phrases": [
                        "het verleden loslaten",
                        "angsten loslaten",
                        "de controle loslaten"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop worrying about something or to emotionally detach from someone; to give up control.",
                        "ru": "Перестать беспокоиться о чем-то или эмоционально отстраниться; перестать контролировать ситуацию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let go",
                                "target_lang_sense_clarification": "emotional attachment"
                            },
                            {
                                "target_lang_word": "move on",
                                "target_lang_sense_clarification": "from the past"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отпускать",
                                "target_lang_sense_clarification": "эмоционально"
                            },
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "в прошлом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ae846fe-5966-3a29-a0b0-c2b11a21bb3b",
                    "sense_definition": "Iemand of iets de vrijheid geven om weg te gaan; niet langer gevangen houden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jager heeft het hert weer in het bos <w>losgelaten</w>.",
                            "target_lang_translations": {
                                "en": "The hunter <w>released</w> the deer back into the forest.",
                                "ru": "Охотник снова <w>выпустил</w> оленя в лес."
                            }
                        },
                        {
                            "text": "Na het onderzoek besloot de politie de verdachte <w>los</w> te <w>laten</w>.",
                            "target_lang_translations": {
                                "en": "After the investigation, the police decided to <w>release</w> the suspect.",
                                "ru": "После расследования полиция решила <w>отпустить</w> подозреваемого."
                            }
                        }
                    ],
                    "synonyms": [
                        "vrijlaten",
                        "bevrijden"
                    ],
                    "antonyms": [
                        "vangen",
                        "opsluiten",
                        "gevangennemen"
                    ],
                    "common_phrases": [
                        "in het wild loslaten",
                        "een dier loslaten"
                    ],
                    "target_lang_definitions": {
                        "en": "To give someone or something freedom to leave; to no longer keep captive.",
                        "ru": "Дать кому-то или чему-то свободу уйти; перестать удерживать в неволе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "prisoners or animals"
                            },
                            {
                                "target_lang_word": "set free",
                                "target_lang_sense_clarification": "liberate"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выпускать",
                                "target_lang_sense_clarification": "животных на волю"
                            },
                            {
                                "target_lang_word": "освобождать",
                                "target_lang_sense_clarification": "из заключения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d666cbcf-472d-33c1-ad27-fbce09abc483",
                    "sense_definition": "Iets of iemand niet meer in bedwang houden en vrij spel geven (vaak met kracht of emotie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotional_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>liet</w> al zijn frustraties <w>los</w> op zijn onschuldige collega.",
                            "target_lang_translations": {
                                "en": "He <w>unleashed</w> all his frustrations on his innocent colleague.",
                                "ru": "Он <w>выплеснул</w> все свое разочарование на невиновного коллегу."
                            }
                        },
                        {
                            "text": "De regisseur wilde zijn acteurs op het publiek <w>loslaten</w> zonder script.",
                            "target_lang_translations": {
                                "en": "The director wanted to <w>unleash</w> his actors on the audience without a script.",
                                "ru": "Режиссер хотел <w>выпустить</w> своих актеров на публику без сценария."
                            }
                        }
                    ],
                    "synonyms": [
                        "botvieren",
                        "afsturen"
                    ],
                    "antonyms": [
                        "inhouden",
                        "beheersen"
                    ],
                    "common_phrases": [
                        "woede loslaten op",
                        "iemand op iets loslaten"
                    ],
                    "target_lang_definitions": {
                        "en": "To no longer restrain something or someone, often allowing them to act freely or forcefully.",
                        "ru": "Перестать сдерживать что-то или кого-то, давая полную свободу действий (часто о силе или эмоциях)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unleash",
                                "target_lang_sense_clarification": "emotions or forces"
                            },
                            {
                                "target_lang_word": "let loose",
                                "target_lang_sense_clarification": "allow free rein"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выплескивать",
                                "target_lang_sense_clarification": "эмоции"
                            },
                            {
                                "target_lang_word": "давать волю",
                                "target_lang_sense_clarification": "чувствам или действиям"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
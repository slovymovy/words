{
    "word_family": [
        "rating",
        "ratable",
        "overrate",
        "underrate",
        "prorate",
        "berate"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "78494f7d-eccb-3a8b-92af-c3d8b874198f",
                    "sense_definition": "A fixed price paid or charged for something, especially goods or services.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cost_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hotel offers a special discounted <w>rate</w> for guests staying over the weekend.",
                            "target_lang_translations": {
                                "ru": "Отель предлагает специальный <w>тариф</w> со скидкой для гостей, останавливающихся на выходные."
                            }
                        },
                        {
                            "text": "You will be paid an hourly <w>rate</w> for your work on this project.",
                            "target_lang_translations": {
                                "ru": "Вам будут платить по почасовой <w>ставке</w> за работу над этим проектом."
                            }
                        }
                    ],
                    "synonyms": [
                        "rank"
                    ],
                    "common_phrases": [
                        "hourly rate",
                        "interest rate",
                        "exchange rate",
                        "flat rate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фиксированная цена, выплачиваемая или взимаемая за что-либо, особенно за товары или услуги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тариф",
                                "target_lang_sense_clarification": "установленная плата за услуги (связь, транспорт, отель)"
                            },
                            {
                                "target_lang_word": "ставка",
                                "target_lang_sense_clarification": "размер оплаты труда или платежа"
                            },
                            {
                                "target_lang_word": "цена",
                                "target_lang_sense_clarification": "общая стоимость"
                            },
                            {
                                "target_lang_word": "расценка",
                                "target_lang_sense_clarification": "установленная стоимость единицы работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bac6c7fb-a23c-3e06-984a-f26047c9ed52",
                    "sense_definition": "The speed at which something happens or changes over a specific period of time.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "speed_frequency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car was moving at a dangerous <w>rate</w> down the icy highway.",
                            "target_lang_translations": {
                                "ru": "Машина двигалась с опасной <w>скоростью</w> по обледенелому шоссе."
                            }
                        },
                        {
                            "text": "The <w>rate</w> of production at the factory has increased significantly this year.",
                            "target_lang_translations": {
                                "ru": "<w>Темп</w> производства на заводе значительно вырос в этом году."
                            }
                        }
                    ],
                    "synonyms": [
                        "rank"
                    ],
                    "common_phrases": [
                        "at a rate",
                        "heart rate",
                        "birth rate",
                        "rate of change"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скорость, с которой что-либо происходит или изменяется за определенный период времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скорость",
                                "target_lang_sense_clarification": "быстрота движения или процесса"
                            },
                            {
                                "target_lang_word": "темп",
                                "target_lang_sense_clarification": "интенсивность развития или осуществления чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7266bbe9-a6e4-388e-954e-e8d901dc031e",
                    "sense_definition": "A quantity or amount measured in relation to another quantity or amount.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "proportion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The unemployment <w>rate</w> fell by two percent last month.",
                            "target_lang_translations": {
                                "ru": "<w>Уровень</w> безработицы снизился на два процента в прошлом месяце."
                            }
                        },
                        {
                            "text": "We need to calculate the <w>rate</w> of return on this investment before proceeding.",
                            "target_lang_translations": {
                                "ru": "Нам нужно рассчитать <w>коэффициент</w> окупаемости этих инвестиций, прежде чем продолжать."
                            }
                        }
                    ],
                    "synonyms": [
                        "rank"
                    ],
                    "common_phrases": [
                        "unemployment rate",
                        "success rate",
                        "tax rate",
                        "mortality rate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Величина или количество, измеренное по отношению к другой величине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "степень величины (напр. безработицы, преступности)"
                            },
                            {
                                "target_lang_word": "ставка",
                                "target_lang_sense_clarification": "процентная доля (напр. налоговая)"
                            },
                            {
                                "target_lang_word": "коэффициент",
                                "target_lang_sense_clarification": "числовой множитель или показатель"
                            },
                            {
                                "target_lang_word": "процент",
                                "target_lang_sense_clarification": "доля от целого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46afda7c-6021-368a-b2f6-83429a74d0f3",
                    "sense_definition": "A local tax paid by householders or business owners based on the value of their property.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "taxation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shop closed down because the business <w>rates</w> were too high for the owner to afford.",
                            "target_lang_translations": {
                                "ru": "Магазин закрылся, потому что налоги на <w>недвижимость</w> были слишком высоки для владельца."
                            }
                        },
                        {
                            "text": "I hardly have enough money left every month to pay the <w>rates</w>.",
                            "target_lang_translations": {
                                "ru": "У меня едва остается достаточно денег каждый месяц, чтобы оплачивать местные <w>налоги</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rank"
                    ],
                    "common_phrases": [
                        "business rates",
                        "pay the rates"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK, Australia, New Zealand"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Местный налог, уплачиваемый домовладельцами или владельцами бизнеса в зависимости от стоимости их имущества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "муниципальный налог",
                                "target_lang_sense_clarification": "местный сбор с недвижимости"
                            },
                            {
                                "target_lang_word": "коммунальный платеж",
                                "target_lang_sense_clarification": "плата за обслуживание жилья"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f190002-aaf5-347e-b556-5b2ce2a4bb70",
                    "sense_definition": "To assess the value, quality, or standard of someone or something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "evaluate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The critics <w>rated</w> the movie as the best of the year.",
                            "target_lang_translations": {
                                "ru": "Критики <w>оценили</w> этот фильм как лучший в году."
                            }
                        },
                        {
                            "text": "How would you <w>rate</w> your experience with our customer service team?",
                            "target_lang_translations": {
                                "ru": "Как бы вы <w>оценили</w> свой опыт общения с нашей службой поддержки?"
                            }
                        }
                    ],
                    "synonyms": [
                        "rank"
                    ],
                    "common_phrases": [
                        "highly rated",
                        "rate highly",
                        "rate as"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценивать стоимость, качество или уровень кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оценивать",
                                "target_lang_sense_clarification": "давать оценку качеству"
                            },
                            {
                                "target_lang_word": "расценивать",
                                "target_lang_sense_clarification": "воспринимать определенным образом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83000c45-e357-39bb-a4eb-3507d2e3f159",
                    "sense_definition": "To have a particular status or position in a specific class or group.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This restaurant <w>rates</w> among the finest in the entire city.",
                            "target_lang_translations": {
                                "ru": "Этот ресторан <w>считается</w> одним из лучших во всем городе."
                            }
                        },
                        {
                            "text": "He <w>rates</w> as one of the most influential scientists of the century.",
                            "target_lang_translations": {
                                "ru": "Он <w>котируется</w> как один из самых влиятельных ученых столетия."
                            }
                        }
                    ],
                    "synonyms": [
                        "rank"
                    ],
                    "common_phrases": [
                        "rate as",
                        "rate among"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занимать определенное положение или статус в конкретном классе или группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "считаться",
                                "target_lang_sense_clarification": "иметь репутацию"
                            },
                            {
                                "target_lang_word": "котироваться",
                                "target_lang_sense_clarification": "цениться, иметь вес в обществе"
                            },
                            {
                                "target_lang_word": "занимать место",
                                "target_lang_sense_clarification": "находиться в ряду других"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70489864-b2a0-3037-96bb-ac38c53881ec",
                    "sense_definition": "To think highly of someone or something; to like.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I really don't <w>rate</w> that new band playing on the radio.",
                            "target_lang_translations": {
                                "ru": "Я не особо <w>ценю</w> эту новую группу, играющую по радио."
                            }
                        },
                        {
                            "text": "The customers simply did not <w>rate</w> the new burgers on the menu.",
                            "target_lang_translations": {
                                "ru": "Клиенты просто не <w>оценили</w> новые бургеры в меню."
                            }
                        }
                    ],
                    "synonyms": [
                        "rank"
                    ],
                    "common_phrases": [
                        "don't rate",
                        "rate him"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "mostly UK"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть высокого мнения о ком-то или чем-то; ценить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ценить",
                                "target_lang_sense_clarification": "признавать достоинства"
                            },
                            {
                                "target_lang_word": "высоко ставить",
                                "target_lang_sense_clarification": "уважать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5493c779-ee2d-3335-a6dc-f9a0d873d340",
                    "sense_definition": "To be worthy of something; to deserve.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "merit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This minor issue does not <w>rate</w> a mention in the final report.",
                            "target_lang_translations": {
                                "ru": "Эта мелкая проблема не <w>заслуживает</w> упоминания в итоговом отчете."
                            }
                        },
                        {
                            "text": "Her performance definitely <w>rates</w> a round of applause from the audience.",
                            "target_lang_translations": {
                                "ru": "Её выступление определенно <w>заслуживает</w> аплодисментов зрителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "rank"
                    ],
                    "common_phrases": [
                        "rate a mention",
                        "hardly rate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть достойным чего-либо; заслуживать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заслуживать",
                                "target_lang_sense_clarification": "быть достойным награды или внимания"
                            },
                            {
                                "target_lang_word": "стоить",
                                "target_lang_sense_clarification": "иметь ценность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "631ef67c-e0a6-3b48-86aa-dde5b759b50c",
                    "sense_definition": "To scold or criticize someone angrily.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "scold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The angry father began to <w>rate</w> his son for breaking the window.",
                            "target_lang_translations": {
                                "ru": "Рассерженный отец начал <w>бранить</w> сына за разбитое окно."
                            }
                        },
                        {
                            "text": "She was <w>rated</w> by her master for losing the keys to the library.",
                            "target_lang_translations": {
                                "ru": "Хозяин <w>отчитал</w> её за потерю ключей от библиотеки."
                            }
                        }
                    ],
                    "synonyms": [
                        "rank"
                    ],
                    "common_phrases": [
                        "rate soundly"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сердито ругать или критиковать кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бранить",
                                "target_lang_sense_clarification": "ругать"
                            },
                            {
                                "target_lang_word": "отчитывать",
                                "target_lang_sense_clarification": "делать строгий выговор"
                            },
                            {
                                "target_lang_word": "журить",
                                "target_lang_sense_clarification": "слегка ругать (устар.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
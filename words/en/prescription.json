                {
                    "sense_id": "b9301737-12bc-3027-9f7a-a34fd22d9772",
                    "sense_definition": "A plan or suggestion intended to handle a situation or achieve a specific result.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "plan",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hard work and consistent effort are a reliable <w>prescription</w> for success."
                        },
                        {
                            "text": "Ignoring the warning signs in the economy is a sure <w>prescription</w> for disaster."
                        }
                    ],
                    "common_phrases": [
                        "prescription for success",
                        "prescription for disaster"
                    ]
                },
                {
                    "sense_id": "54865b36-f28b-3d35-93d4-2e748fa2d073",
                    "sense_definition": "The establishment of rules defining how language should be used rather than describing how it is actually used.",

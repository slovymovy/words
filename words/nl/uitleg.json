    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "deb8337f-c775-3d3e-b43c-a54cb6f37a33",
                    "sense_definition": "Een verhaal of beschrijving bedoeld om iets begrijpelijk te maken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "explanation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar gaf een duidelijke <w>uitleg</w> over de moeilijke opdracht.",
                            "target_lang_translations": {
                                "ru": "Учитель дал четкое <w>объяснение</w> сложного задания."
                            }
                        },
                        {
                            "text": "Zonder jouw <w>uitleg</w> had ik de regels nooit begrepen.",
                            "target_lang_translations": {
                                "ru": "Без твоего <w>объяснения</w> я бы никогда не понял правила."
                            }
                        }
                    ],
                    "synonyms": [
                        "verklaring"
                    ],
                    "common_phrases": [
                        "uitleg geven",
                        "om uitleg vragen",
                        "een duidelijke uitleg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассказ или описание, предназначенные для того, чтобы сделать что-то понятным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объяснение",
                                "target_lang_sense_clarification": "процесс или результат разъяснения чего-либо"
                            },
                            {
                                "target_lang_word": "разъяснение",
                                "target_lang_sense_clarification": "уточнение, подробное объяснение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5be89f4-497f-3423-b2fb-3fd7521e7e65",
                    "sense_definition": "De vergroting van een gebied, met name de uitbreiding van een stad buiten de vroegere muren.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De historische kaarten tonen de <w>uitleg</w> van de stad in de zeventiende eeuw.",
                            "target_lang_translations": {
                                "ru": "Исторические карты показывают <w>расширение</w> города в семнадцатом веке."
                            }
                        },
                        {
                            "text": "Door de <w>uitleg</w> van het grondgebied ontstonden nieuwe wijken.",
                            "target_lang_translations": {
                                "ru": "Благодаря <w>расширению</w> территории возникли новые районы."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitbreiding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличение территории, в частности расширение города за пределы прежних стен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расширение",
                                "target_lang_sense_clarification": "увеличение площади или границ"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07a73959-9d6a-3912-b0b9-40af029ff2a5",
                    "sense_definition": "Vervoeging van <w>uitleggen</w>: eerste persoon enkelvoud in een bijzin.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is belangrijk dat ik het probleem rustig <w>uitleg</w>.",
                            "target_lang_translations": {
                                "ru": "Важно, чтобы я спокойно <w>объяснил</w> проблему."
                            }
                        },
                        {
                            "text": "Als ik de spelregels <w>uitleg</w>, moet iedereen goed luisteren.",
                            "target_lang_translations": {
                                "ru": "Когда я <w>объясняю</w> правила игры, все должны внимательно слушать."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {

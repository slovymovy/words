    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4df4236c-8342-34bd-bd7b-ee17f8550f2c",
                    "sense_definition": "Pospolity, latający owad o dwóch skrzydłach, często uciążliwy dla ludzi i zwierząt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "insect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W kuchni lata wielka <mucha>, weź packę i ją przegon.",
                            "target_lang_translations": {
                                "ru": "На кухне летает огромная <муха>, возьми мухобойку и прогони её."
                            }
                        },
                        {
                            "text": "Ten człowiek jest tak łagodny, że nie skrzywdziłby nawet <muchy>.",
                            "target_lang_translations": {
                                "ru": "Этот человек настолько кроткий, что и <мухи> не обидит."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nie skrzywdzić muchy",
                        "mieć muchy w nosie",
                        "padać jak muchy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обычное летающее двукрылое насекомое, часто досаждающее людям и животным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "муха",
                                "target_lang_sense_clarification": "насекомое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74809840-455d-3664-9be6-f1e62152a032",
                    "sense_definition": "Męska ozdoba szyi w kształcie kokardy, noszona do koszuli zamiast krawata, często do stroju wieczorowego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do smokingu zawsze należy zakładać czarną <muchę>.",
                            "target_lang_translations": {
                                "ru": "Со смокингом всегда следует надевать черную <бабочку>."
                            }
                        },
                        {
                            "text": "Mój dziadek woli nosić <muchę> zamiast tradycyjnego krawata.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка предпочитает носить <бабочку> вместо традиционного галстука."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nosić muchę",
                        "wiązać muchę",
                        "elegancka mucha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское шейное украшение в форме банта, которое носят с рубашкой вместо галстука, часто с вечерним костюмом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бабочка",
                                "target_lang_sense_clarification": "галстук"
                            },
                            {
                                "target_lang_word": "галстук-бабочка",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cee56b4c-faf6-37c3-8750-2369fc614f67",
                    "sense_definition": "Coś bardzo łatwego do zrobienia; zadanie niewymagające wysiłku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "idiom_easy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naprawa tego kranu to dla hydraulika <mucha>, zajmie mu to chwilę.",
                            "target_lang_translations": {
                                "ru": "Починка этого крана для сантехника — <пустяк>, это займет у него минуту."
                            }
                        },
                        {
                            "text": "Egzamin z matematyki okazał się <muchą>, wszyscy zdali go bez problemu.",
                            "target_lang_translations": {
                                "ru": "Экзамен по математике оказался <ерундой>, все сдали его без проблем."
                            }
                        }
                    ],
                    "synonyms": [
                        "pestka",
                        "pikuś",
                        "łatwizna"
                    ],
                    "common_phrases": [
                        "to jest mucha"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то очень легкое для выполнения; задача, не требующая усилий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пустяк",
                                "target_lang_sense_clarification": "легкое дело"
                            },
                            {
                                "target_lang_word": "ерунда",
                                "target_lang_sense_clarification": "нечто незначительное"
                            },
                            {
                                "target_lang_word": "пара пустяков",
                                "target_lang_sense_clarification": "фразеологизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90c4e4b6-1f25-37ce-81b2-349687c21784",
                    "sense_definition": "Sztuczna przynęta wędkarska imitująca owada, używana do łowienia ryb metodą muchową.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fishing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wędkarz zarzucił wędkę, używając kolorowej <muchy> jako przynęty na pstrągi.",
                            "target_lang_translations": {
                                "ru": "Рыбак забросил удочку, используя разноцветную <мушку> в качестве приманки на форель."
                            }
                        },
                        {
                            "text": "Łowienie na <muchę> wymaga dużej wprawy i cierpliwości.",
                            "target_lang_translations": {
                                "ru": "Ловля на <мушку> требует большой сноровки и терпения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "łowić na muchę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "jargon wędkarski"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственная рыболовная приманка, имитирующая насекомое, используемая для ловли рыбы нахлыстом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мушка",
                                "target_lang_sense_clarification": "рыболовная снасть"
                            },
                            {
                                "target_lang_word": "муха",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "toedoen",
        "toegaan",
        "toekomen",
        "toelaten",
        "toenemen",
        "toetje",
        "naartoe",
        "daartoe"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ff971cd0-b318-348f-a113-01ee76823a85",
                    "sense_definition": "Duidt een richting of bestemming aan, vaak in combinatie met 'naar'; onderdeel van voornaamwoordelijke bijwoorden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "direction_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We gaan morgen naar oma <toe>.",
                            "target_lang_translations": {
                                "en": "We are going <to> grandma's tomorrow.",
                                "ru": "Мы завтра поедем <к> бабушке."
                            }
                        },
                        {
                            "text": "Het doet er niet <toe> wat je daarvan vindt.",
                            "target_lang_translations": {
                                "en": "It doesn't <matter> what you think about that.",
                                "ru": "Не имеет <значения>, что ты об этом думаешь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "er niet toe doen",
                        "naar ... toe",
                        "eraan toe zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates a direction or destination, often combined with 'naar' (to); also part of the phrase 'er toe doen' (to matter).",
                        "ru": "Указывает на направление или место назначения, часто в сочетании с 'naar'; часть местоименных наречий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "towards",
                                "target_lang_sense_clarification": "indicating direction"
                            },
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "indicating destination"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "предлог направления (с 'naar')"
                            },
                            {
                                "target_lang_word": "в направлении",
                                "target_lang_sense_clarification": "указание пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "183032b1-0257-3d16-8fa1-2b402ea9f0a3",
                    "sense_definition": "Geeft een uiterste grens of intensiteit aan in de constructie 'tot ... toe'.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij herhaalde het verhaal tot vervelens <toe>.",
                            "target_lang_translations": {
                                "en": "He repeated the story <to the point of> boredom.",
                                "ru": "Он повторял историю <до> тошноты."
                            }
                        },
                        {
                            "text": "Ze vochten door tot bloedens <toe>.",
                            "target_lang_translations": {
                                "en": "They fought on <until> they bled.",
                                "ru": "Они дрались <до> крови."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tot bloedens toe",
                        "tot vervelens toe"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates an extreme limit or intensity in the construction 'tot ... toe'.",
                        "ru": "Указывает на крайний предел или интенсивность в конструкции 'tot ... toe'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to the point of",
                                "target_lang_sense_clarification": "indicating a limit or degree"
                            },
                            {
                                "target_lang_word": "until",
                                "target_lang_sense_clarification": "indicating duration or result"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "указание предела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03ac22aa-d1b5-3edb-8b62-3baa8323e1c5",
                    "sense_definition": "Als extraatje of toevoeging, vaak na het hoofdgerecht.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "extra",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We kregen een lekker ijsje <toe>.",
                            "target_lang_translations": {
                                "en": "We got a nice ice cream <for dessert>.",
                                "ru": "Мы получили вкусное мороженое <на десерт>."
                            }
                        },
                        {
                            "text": "Wat wil je <toe> na het eten?",
                            "target_lang_translations": {
                                "en": "What do you want <for dessert> after dinner?",
                                "ru": "Что ты хочешь <на десерт> после еды?"
                            }
                        }
                    ],
                    "synonyms": [
                        "erbij",
                        "extra"
                    ],
                    "target_lang_definitions": {
                        "en": "As an extra treat or addition, often after the main course.",
                        "ru": "В качестве дополнения или десерта, часто после основного блюда."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for dessert",
                                "target_lang_sense_clarification": "after a meal"
                            },
                            {
                                "target_lang_word": "to follow",
                                "target_lang_sense_clarification": "as a subsequent course"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "на десерт",
                                "target_lang_sense_clarification": "сладкое блюдо после еды"
                            },
                            {
                                "target_lang_word": "в придачу",
                                "target_lang_sense_clarification": "дополнительно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d3ad54b0-fa23-360b-8f2e-f62875a3a80c",
                    "sense_definition": "Niet open; gesloten.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "closed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De winkel is al <toe>.",
                            "target_lang_translations": {
                                "en": "The shop is already <closed>.",
                                "ru": "Магазин уже <закрыт>."
                            }
                        },
                        {
                            "text": "Doe je jas <toe>, want het is koud buiten.",
                            "target_lang_translations": {
                                "en": "Do your coat <up>, because it is cold outside.",
                                "ru": "<Застегни> куртку, так как на улице холодно."
                            }
                        }
                    ],
                    "synonyms": [
                        "dicht",
                        "gesloten"
                    ],
                    "antonyms": [
                        "open"
                    ],
                    "common_phrases": [
                        "deur toe",
                        "ogen toe"
                    ],
                    "target_lang_definitions": {
                        "en": "Not open; closed.",
                        "ru": "Не открытый; закрытый."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "closed",
                                "target_lang_sense_clarification": "not open"
                            },
                            {
                                "target_lang_word": "shut",
                                "target_lang_sense_clarification": "fastened or closed"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "закрытый",
                                "target_lang_sense_clarification": "о двери или магазине"
                            },
                            {
                                "target_lang_word": "застёгнутый",
                                "target_lang_sense_clarification": "об одежде"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "7124f333-21c7-30fa-bae0-5e3b35a6bf98",
                    "sense_definition": "Uitroep om iemand aan te moedigen, te smeken of gerust te stellen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "encouragement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ach <toe>, help me even met deze zware tas.",
                            "target_lang_translations": {
                                "en": "Oh <please>, help me with this heavy bag for a moment.",
                                "ru": "Ах, <пожалуйста>, помоги мне с этой тяжелой сумкой."
                            }
                        },
                        {
                            "text": "<Toe> maar, neem er nog eentje.",
                            "target_lang_translations": {
                                "en": "<Go ahead>, take another one.",
                                "ru": "<Давай>, возьми еще одну."
                            }
                        }
                    ],
                    "synonyms": [
                        "kom op",
                        "vooruit",
                        "alsjeblieft"
                    ],
                    "common_phrases": [
                        "toe maar",
                        "toe nou",
                        "ach toe"
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamation to encourage, beg, or reassure someone.",
                        "ru": "Возглас для поощрения, мольбы или успокоения кого-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "please",
                                "target_lang_sense_clarification": "pleading or begging"
                            },
                            {
                                "target_lang_word": "come on",
                                "target_lang_sense_clarification": "encouragement"
                            },
                            {
                                "target_lang_word": "go ahead",
                                "target_lang_sense_clarification": "giving permission"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "побуждение или усиление"
                            },
                            {
                                "target_lang_word": "пожалуйста",
                                "target_lang_sense_clarification": "просьба"
                            },
                            {
                                "target_lang_word": "давай",
                                "target_lang_sense_clarification": "поощрение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "dab2f6b2-78b6-3104-b3fe-f5bc23e7e014",
                    "sense_definition": "Verouderde of dialectische vorm van 'toen'.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "time_conjunction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Toe> hij binnenkwam, werd het stil.",
                            "target_lang_translations": {
                                "en": "<When> he entered, it became quiet.",
                                "ru": "<Когда> он вошел, стало тихо."
                            }
                        },
                        {
                            "text": "Dat was in de tijd <toe> we nog jong waren.",
                            "target_lang_translations": {
                                "en": "That was in the time <when> we were still young.",
                                "ru": "Это было в то время, <когда> мы были еще молоды."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "or archaic"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Archaic or dialectal form of 'toen' (when).",
                        "ru": "Устаревшая или диалектная форма слова 'toen' (когда)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "when",
                                "target_lang_sense_clarification": "referring to a past time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "когда",
                                "target_lang_sense_clarification": "союз времени"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
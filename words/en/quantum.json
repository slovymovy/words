    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6cdf4aa6-31e8-3c60-af8b-ef536ec05779",
                    "sense_definition": "Relating to the branch of physics that deals with the behavior of matter and energy at the atomic and subatomic levels.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physics_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Modern technology relies heavily on the principles of <w>quantum</w> mechanics.",
                            "target_lang_translations": {
                                "ru": "Современные технологии во многом опираются на принципы <w>квантовой</w> механики."
                            }
                        },
                        {
                            "text": "Scientists are working to build a powerful <w>quantum</w> computer.",
                            "target_lang_translations": {
                                "ru": "Ученые работают над созданием мощного <w>квантового</w> компьютера."
                            }
                        }
                    ],
                    "antonyms": [
                        "classical"
                    ],
                    "common_phrases": [
                        "quantum mechanics",
                        "quantum physics",
                        "quantum theory",
                        "quantum computer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к разделу физики, изучающему поведение материи и энергии на атомном и субатомном уровнях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квантовый",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb97ea7b-0329-3fe4-9074-f15218d8c14a",
                    "sense_definition": "Describing a sudden, significant, and often large change or advance.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "significant_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The invention of the internet represented a <w>quantum</w> leap in global communication.",
                            "target_lang_translations": {
                                "ru": "Изобретение интернета представляло собой <w>квантовый</w> скачок в глобальной коммуникации."
                            }
                        },
                        {
                            "text": "We need a <w>quantum</w> shift in our approach to solving this environmental crisis.",
                            "target_lang_translations": {
                                "ru": "Нам необходим <w>качественный</w> сдвиг в подходе к решению этого экологического кризиса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "quantum leap",
                        "quantum jump"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывающий внезапное, значительное и часто большое изменение или продвижение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квантовый",
                                "target_lang_sense_clarification": "в выражении 'квантовый скачок'"
                            },
                            {
                                "target_lang_word": "качественный",
                                "target_lang_sense_clarification": "о резком изменении или скачке"
                            },
                            {
                                "target_lang_word": "кардинальный",
                                "target_lang_sense_clarification": "существенный, глубокий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e3fbf4ab-7139-3b51-acb8-23a3f2a63954",
                    "sense_definition": "The smallest possible discrete unit of a physical property, such as energy or matter.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physics_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Max Planck proposed that energy is emitted in discrete packets called <w>quanta</w>.",
                            "target_lang_translations": {
                                "ru": "Макс Планк предположил, что энергия испускается дискретными порциями, называемыми <w>квантами</w>."
                            }
                        },
                        {
                            "text": "A photon is essentially a single <w>quantum</w> of light.",
                            "target_lang_translations": {
                                "ru": "Фотон — это, по сути, единичный <w>квант</w> света."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наименьшая возможная дискретная единица физической величины, такой как энергия или материя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квант",
                                "target_lang_sense_clarification": "неделимая порция энергии"
                            }

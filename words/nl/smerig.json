{
    "word_family": [
        "smerigheid",
        "smeerlap",
        "smeerboel",
        "smeerpoets",
        "besmeren"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e553d737-4725-3036-9a7a-459d7344e43f",
                    "sense_definition": "Erg vies, vuil of niet schoon.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_dirt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doe eerst die <w>smerige</w> broek eens in de was!",
                            "target_lang_translations": {
                                "ru": "Сначала отправь эти <w>грязные</w> штаны в стирку!"
                            }
                        },
                        {
                            "text": "De vloer in de keuken is <w>smerig</w> en moet gedweild worden.",
                            "target_lang_translations": {
                                "ru": "Пол на кухне <w>грязный</w>, и его нужно помыть."
                            }
                        }
                    ],
                    "synonyms": [
                        "vies",
                        "vuil",
                        "goor"
                    ],
                    "antonyms": [
                        "schoon",
                        "fris",
                        "rein",
                        "proper"
                    ],
                    "common_phrases": [
                        "smerige handen",
                        "smerige boel",
                        "smerig werk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень грязный, запачканный или вызывающий отвращение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грязный",
                                "target_lang_sense_clarification": "покрытый грязью, нечистый"
                            },
                            {
                                "target_lang_word": "мерзкий",
                                "target_lang_sense_clarification": "вызывающий отвращение, очень грязный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f2a1c5a-c20a-30ba-a79c-ada8c84efea7",
                    "sense_definition": "Met een heel vieze smaak; niet lekker.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "taste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De melk was bedorven en smaakte echt <w>smerig</w>.",
                            "target_lang_translations": {
                                "ru": "Молоко испортилось и было <w>противным</w> на вкус."
                            }
                        },
                        {
                            "text": "Ik vind spruitjes <w>smerig</w>, maar mijn broer vindt ze heerlijk.",
                            "target_lang_translations": {
                                "ru": "Я нахожу брюссельскую капусту <w>гадкой</w>, но мой брат её обожает."
                            }
                        }
                    ],
                    "synonyms": [
                        "vies",
                        "onsmakelijk"
                    ],
                    "antonyms": [
                        "lekker",
                        "smakelijk",
                        "heerlijk"
                    ],
                    "common_phrases": [
                        "smaakt smerig",
                        "smerig drankje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий очень неприятный вкус; отвратительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противный",
                                "target_lang_sense_clarification": "неприятный на вкус"
                            },
                            {
                                "target_lang_word": "гадкий",
                                "target_lang_sense_clarification": "очень невкусный, мерзкий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15a3699b-3fd8-35f8-9af7-4c6099150ace",
                    "sense_definition": "Moreel verwerpelijk, gemeen of onfatsoenlijk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_mean",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat een <w>smerige</w> streek is dat!",
                            "target_lang_translations": {
                                "ru": "Что за <w>подлая</w> выходка!"
                            }
                        },
                        {
                            "text": "Hij vertelde een <w>smerige</w> mop waar niemand om kon lachen.",
                            "target_lang_translations": {
                                "ru": "Он рассказал <w>пошлый</w> анекдот, над которым никто не мог посмеяться."
                            }
                        }
                    ],
                    "synonyms": [
                        "gemeen",
                        "vals",
                        "achterbaks",
                        "schunnig"
                    ],
                    "antonyms": [
                        "fatsoenlijk",
                        "eerlijk",
                        "netjes"
                    ],
                    "common_phrases": [
                        "smerige streek",
                        "smerig zaakje",
                        "smerige mop",
                        "smerig spelletje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Морально низкий, подлый или непристойный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подлый",
                                "target_lang_sense_clarification": "о поступке или характере"
                            },
                            {
                                "target_lang_word": "грязный",
                                "target_lang_sense_clarification": "в переносном смысле: нечестный, аморальный"
                            },
                            {
                                "target_lang_word": "пошлый",
                                "target_lang_sense_clarification": "непристойный, вульгарный (о шутках)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2780c2c5-9601-3d04-a8f8-17ef0af0dad9",
                    "sense_definition": "Een grammofoonplaat met aan beide kanten ongeveer twintig minuten muziek.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>lp</w> is bijna helemaal in onbruik geraakt.",
                            "target_lang_translations": {
                                "ru": "<w>Долгоиграющая пластинка</w> почти полностью вышла из употребления."
                            }
                        },
                        {
                            "text": "Mijn vader heeft een grote verzameling met oude <w>lp's</w> op zolder staan.",
                            "target_lang_translations": {
                                "ru": "У моего отца на чердаке стоит большая коллекция старых <w>пластинок</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "langspeelplaat"
                    ],
                    "common_phrases": [
                        "een lp draaien",
                        "oude lp's",
                        "lp opzetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грампластинка, на каждой стороне которой записано около двадцати минут музыки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пластинка",
                                "target_lang_sense_clarification": "грампластинка"
                            },
                            {
                                "target_lang_word": "долгоиграющая пластинка",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2a9f5a0-87f1-3585-a078-837a15fe047e",
                    "sense_definition": "Een medische procedure waarbij vocht uit het ruggenmerg wordt afgenomen; afkorting van <w>lumbale punctie</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor het diagnosticeren van hersenvliesontsteking was vroeger een <w>lp</w> nodig.",
                            "target_lang_translations": {
                                "ru": "Раньше для диагностики менингита требовалась <w>люмбальная пункция</w>."
                            }
                        },
                        {
                            "text": "De neuroloog legde uit waarom een <w>lp</w> noodzakelijk was voor het onderzoek.",
                            "target_lang_translations": {
                                "ru": "Невролог объяснил, почему для обследования необходима <w>люмбальная пункция</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een lp doen",
                        "lp uitvoeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Медицинская процедура, при которой производится забор жидкости из спинномозгового канала; сокращение от \"люмбальная пункция\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "люмбальная пункция",
                                "target_lang_sense_clarification": "медицинская процедура"
                            }
                        ]
                    }
                }
            ]
        }
    ]

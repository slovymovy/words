            "pos": "verb",
            "senses": [
                {
                    "sense_id": "654d593d-80d1-3a86-a24f-aebe2af102ca",
                    "sense_definition": "Oddawać towar, własność lub usługi innej osobie w zamian za pieniądze.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "commerce_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad <w>sprzedaje</w> swój stary samochód za niską cenę.",
                            "target_lang_translations": {
                                "ru": "Мой сосед <w>продаёт</w> свою старую машину по низкой цене."
                            }
                        },
                        {
                            "text": "W tym sklepie można <w>sprzedawać</w> używane książki i podręczniki.",
                            "target_lang_translations": {
                                "ru": "В этом магазине можно <w>продавать</w> подержанные книги и учебники."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbywać",
                        "opychać",
                        "opylać"
                    ],
                    "antonyms": [
                        "kupować",
                        "nabywać"
                    ],
                    "common_phrases": [
                        "sprzedawać na raty",
                        "sprzedawać z zyskiem",
                        "sprzedawać detalicznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдавать товар, собственность или услуги другому лицу в обмен на деньги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продавать",
                                "target_lang_sense_clarification": "обменивать на деньги"
                            },
                            {
                                "target_lang_word": "сбывать",
                                "target_lang_sense_clarification": "продавать (разговорное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8fd0781-fc89-3fd7-b078-c3f4845598ab",
                    "sense_definition": "Cieszyć się popytem i znajdować nabywców (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "commerce_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta nowa powieść kryminalna świetnie <w>się sprzedaje</w> w całym kraju.",
                            "target_lang_translations": {
                                "ru": "Этот новый детективный роман отлично <w>продаётся</w> по всей стране."
                            }
                        },
                        {
                            "text": "Bilety na koncert znanego zespołu <w>sprzedawały się</w> jak świeże bułeczki.",
                            "target_lang_translations": {
                                "ru": "Билеты на концерт известной группы <w>продавались</w> как горячие пирожки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dobrze się sprzedawać",
                        "sprzedawać się jak świeże bułeczki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пользоваться спросом и находить покупателей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продаваться",
                                "target_lang_sense_clarification": "пользоваться спросом"
                            },
                            {
                                "target_lang_word": "расходиться",
                                "target_lang_sense_clarification": "быстро раскупаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1d600d3-42a1-359f-aa32-981643c5dcf5",
                    "sense_definition": "Rezygnować z własnych przekonań, honoru lub wartości w zamian za korzyści majątkowe lub awans.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "betrayal_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu krytyków uważa, że ten artysta <w>sprzedał się</w> komercji.",
                            "target_lang_translations": {
                                "ru": "Многие критики считают, что этот артист <w>продался</w> коммерции."
                            }
                        },
                        {
                            "text": "Nie zamierzam <w>sprzedawać</w> swoich ideałów dla szybkiej kariery.",
                            "target_lang_translations": {
                                "ru": "Я не собираюсь <w>продавать</w> свои идеалы ради быстрой карьеры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sprzedać duszę",
                        "sprzedać swoje ideały"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказываться от собственных убеждений, чести или ценностей ради материальной выгоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продаваться",
                                "target_lang_sense_clarification": "поступаться принципами"
                            },
                            {
                                "target_lang_word": "продавать",
                                "target_lang_sense_clarification": "торговать совестью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ff862ff-54e9-304f-8b17-21a45d3d2b6c",
                    "sense_definition": "Zdradzić kogoś, donieść na kogoś policji lub władzom (język potoczny).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "betrayal_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestępca <w>sprzedał</w> swoich wspólników, aby dostać mniejszy wyrok.",
                            "target_lang_translations": {
                                "ru": "Преступник <w>сдал</w> своих сообщников, чтобы получить меньший срок."
                            }
                        },
                        {
                            "text": "Bał się, że ktoś go <w>sprzeda</w> i trafi do więzienia.",
                            "target_lang_translations": {
                                "ru": "Он боялся, что кто-то его <w>сдаст</w> и он попадёт в тюрьму."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sprzedać kumpla",
                        "sprzedać informację"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w kontekście zdrady lub donosicielstwa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предать кого-то, донести на кого-то в полицию или властям (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "доносить властям"
                            },
                            {
                                "target_lang_word": "продавать",
                                "target_lang_sense_clarification": "предавать, выдавать"
                            }
                        ]
                    }
                }
            ]
        }

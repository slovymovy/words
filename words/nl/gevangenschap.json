    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d36c94cd-7063-3943-a02c-c31d5c188ef9",
                    "sense_definition": "Het opgesloten zitten in een gevangenis, meestal als straf voor een misdaad",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_of_confinement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij werd veroordeeld tot vijf jaar <w>gevangenschap</w> voor zijn misdaden.",
                            "target_lang_translations": {
                                "en": "He was sentenced to five years <w>imprisonment</w> for his crimes."
                            }
                        },
                        {
                            "text": "De rechter oordeelde dat een lange <w>gevangenschap</w> noodzakelijk was voor de veiligheid van de samenleving.",
                            "target_lang_translations": {
                                "en": "The judge ruled that a long <w>incarceration</w> was necessary for the safety of society."
                            }
                        }
                    ],
                    "synonyms": [
                        "detentie",
                        "hechtenis"
                    ],
                    "common_phrases": [
                        "levenslange gevangenschap",
                        "tot gevangenschap veroordelen"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of being locked up in a prison, usually as punishment for a crime"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "imprisonment",
                                "target_lang_sense_clarification": "legal punishment"
                            },
                            {
                                "target_lang_word": "incarceration",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28166b02-2209-3590-ab6d-df709f8ca278",
                    "sense_definition": "De toestand waarin een mens of dier gevangen wordt gehouden en niet vrij is",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_of_confinement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De laatste quagga in <w>gevangenschap</w> stierf op 12 augustus 1883 in Artis.",
                            "target_lang_translations": {
                                "en": "The last quagga in <w>captivity</w> died on August 12, 1883 in Artis."
                            }
                        },
                        {
                            "text": "Veel wilde dieren vertonen ander gedrag wanneer ze in <w>gevangenschap</w> leven.",
                            "target_lang_translations": {
                                "en": "Many wild animals exhibit different behavior when living in <w>captivity</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in gevangenschap leven",
                        "in gevangenschap geboren",
                        "in gevangenschap houden"
                    ],
                    "target_lang_definitions": {
                        "en": "The condition in which a human or animal is held captive and is not free"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "captivity",
                                "target_lang_sense_clarification": "animals or hostages"
                            }
                        ]
                    }
                }
            ]
        }
    ]

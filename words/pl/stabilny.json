{
    "word_family": [
        "stabilność",
        "stabilizacja",
        "stabilizator",
        "stabilizować",
        "ustabilizować",
        "stabilnie"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a1901b47-253a-35f0-8dc8-254e94931d17",
                    "sense_definition": "Niezmieniający się przez dłuższy czas, zachowujący stały poziom lub stan (np. o sytuacji, gospodarce, uczuciach).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_stability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sytuacja gospodarcza w kraju jest obecnie <w>stabilna</w> i przewidywalna.",
                            "target_lang_translations": {
                                "ru": "Экономическая ситуация в стране сейчас <w>стабильная</w> и предсказуемая."
                            }
                        },
                        {
                            "text": "Mimo wielu problemów, ich związek pozostaje <w>stabilny</w> i pełen zaufania.",
                            "target_lang_translations": {
                                "ru": "Несмотря на множество проблем, их отношения остаются <w>стабильными</w> и полными доверия."
                            }
                        }
                    ],
                    "synonyms": [
                        "niezmienny",
                        "stały",
                        "trwały",
                        "ciągły"
                    ],
                    "antonyms": [
                        "niestabilny",
                        "zmienny",
                        "chwiejny"
                    ],
                    "common_phrases": [
                        "stabilna sytuacja",
                        "stabilny wzrost",
                        "stabilna praca",
                        "stabilny rynek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не меняющийся в течение длительного времени, сохраняющий постоянный уровень или состояние (например, о ситуации, экономике, чувствах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стабильный",
                                "target_lang_sense_clarification": "о ситуации, экономике, отношениях"
                            },
                            {
                                "target_lang_word": "устойчивый",
                                "target_lang_sense_clarification": "постоянный, не подверженный колебаниям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49619786-be66-3a38-b054-90cd5a680754",
                    "sense_definition": "O przedmiotach lub konstrukcjach: mocno osadzony, nieprzewracający się, utrzymujący stałą pozycję.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_stability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To krzesło nie jest zbyt <w>stabilne</w>, lepiej na nim nie stawaj.",
                            "target_lang_translations": {
                                "ru": "Этот стул не слишком <w>устойчивый</w>, лучше не вставай на него."
                            }
                        },
                        {
                            "text": "Musimy znaleźć <w>stabilny</w> grunt, aby bezpiecznie rozbić namiot.",
                            "target_lang_translations": {
                                "ru": "Нам нужно найти <w>устойчивый</w> грунт, чтобы безопасно поставить палатку."
                            }
                        }
                    ],
                    "synonyms": [
                        "solidny",
                        "nieruchomy",
                        "niewzruszony"
                    ],
                    "antonyms": [
                        "chwiejny",
                        "niestabilny"
                    ],
                    "common_phrases": [
                        "stabilna konstrukcja",
                        "stabilny grunt",
                        "stabilne podłoże"
                    ],
                    "target_lang_definitions": {
                        "ru": "О предметах или конструкциях: прочно стоящий, не падающий, сохраняющий неподвижное положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устойчивый",
                                "target_lang_sense_clarification": "о мебели, опоре, равновесии"
                            },
                            {
                                "target_lang_word": "прочный",
                                "target_lang_sense_clarification": "надежный, крепкий (о конструкции)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2909533b-0b8f-3f02-bf5c-15a389cc5e96",
                    "sense_definition": "Zdolny do powrotu do równowagi po zakłóceniu lub działający bezawaryjnie (np. o systemach, chemii, emocjach).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "system_stability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy system operacyjny jest bardzo <w>stabilny</w> i rzadko się zawiesza.",
                            "target_lang_translations": {
                                "ru": "Новая операционная система очень <w>стабильная</w> и редко зависает."
                            }
                        },
                        {
                            "text": "Pacjent jest emocjonalnie <w>stabilny</w> i dobrze reaguje na terapię.",
                            "target_lang_translations": {
                                "ru": "Пациент эмоционально <w>стабилен</w> и хорошо реагирует на терапию."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrównoważony"
                    ],
                    "antonyms": [
                        "labilny",
                        "niestabilny",
                        "niezrównoważony"
                    ],
                    "common_phrases": [
                        "stabilny system",
                        "stabilne łącze",
                        "układ stabilny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный возвращаться к равновесию или работающий без сбоев (например, о системах, химии, эмоциях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стабильный",
                                "target_lang_sense_clarification": "о работе системы, состоянии здоровья"
                            },
                            {
                                "target_lang_word": "устойчивый",
                                "target_lang_sense_clarification": "о психике, связи, иммунитете"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
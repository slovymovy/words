    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e36c74fa-592e-3b47-8b0f-f16cbc98bf20",
                    "sense_definition": "Het beweegbare onderdeel van een schip of vliegtuig waarmee de koers wordt bepaald.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "steering_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kapitein stond urenlang aan het <w>roer</w> om het schip door de storm te loodsen.",
                            "target_lang_translations": {
                                "ru": "Капитан часами стоял у <w>руля</w>, чтобы провести корабль сквозь шторм.",
                                "en": "The captain stood at the <w>helm</w> for hours to pilot the ship through the storm."
                            }
                        },
                        {
                            "text": "Zonder goed functionerend <w>roer</w> is de boot onbestuurbaar op de wilde rivier.",
                            "target_lang_translations": {
                                "ru": "Без хорошо работающего <w>руля</w> лодка неуправляема на бурной реке.",
                                "en": "Without a well-functioning <w>rudder</w>, the boat is uncontrollable on the wild river."
                            }
                        }
                    ],
                    "synonyms": [
                        "vuurroer"
                    ],
                    "common_phrases": [
                        "aan het roer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвижная часть судна или самолета, с помощью которой определяется курс.",
                        "en": "The movable part of a ship or aircraft used to determine the course."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "руль",
                                "target_lang_sense_clarification": "механизм управления"
                            },
                            {
                                "target_lang_word": "штурвал",
                                "target_lang_sense_clarification": "колесо управления"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "rudder",
                                "target_lang_sense_clarification": "steering mechanism"
                            },
                            {
                                "target_lang_word": "helm",
                                "target_lang_sense_clarification": "steering gear"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "362cf380-0beb-3a56-bd2c-b67c0843de30",
                    "sense_definition": "De leiding of controle over een situatie, organisatie of beleid.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "steering_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het vertrek van de directeur kwam er iemand anders aan het <w>roer</w> van het bedrijf.",
                            "target_lang_translations": {
                                "ru": "После ухода директора у <w>руля</w> компании встал кто-то другой.",
                                "en": "After the director's departure, someone else came to the <w>helm</w> of the company."
                            }
                        },
                        {
                            "text": "We moeten het <w>roer</w> omgooien als we de concurrentie voor willen blijven.",
                            "target_lang_translations": {
                                "ru": "Мы должны резко повернуть <w>руль</w>, если хотим опередить конкурентов.",
                                "en": "We must change <w>course</w> if we want to stay ahead of the competition."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het roer omgooien",
                        "aan het roer staan",
                        "het roer overnemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Руководство или контроль над ситуацией, организацией или политикой.",
                        "en": "Leadership or control over a situation, organization, or policy."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "руль",
                                "target_lang_sense_clarification": "перен. контроль, власть"
                            },
                            {
                                "target_lang_word": "штурвал",
                                "target_lang_sense_clarification": "перен. управление"
                            },
                            {
                                "target_lang_word": "руководство",
                                "target_lang_sense_clarification": "лидерство"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "helm",
                                "target_lang_sense_clarification": "figurative: leadership"
                            },
                            {
                                "target_lang_word": "control",
                                "target_lang_sense_clarification": "authority"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bab33a18-e98f-361e-ba16-c6ee551c09ba",
                    "sense_definition": "Een oud type vuurwapen, vergelijkbaar met een musket.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het legermuseum kun je een zestiende-eeuws <w>roer</w> van dichtbij bekijken.",
                            "target_lang_translations": {
                                "ru": "В военном музее можно вблизи рассмотреть <w>пищаль</w> шестнадцатого века.",
                                "en": "In the army museum, you can view a sixteenth-century <w>arquebus</w> up close."
                            }
                        },
                        {
                            "text": "De soldaat laadde zijn <w>roer</w> met buskruit voordat de strijd begon.",
                            "target_lang_translations": {
                                "ru": "Солдат зарядил свою <w>пищаль</w> порохом перед началом битвы.",
                                "en": "The soldier loaded his <w>musket</w> with gunpowder before the battle began."
                            }
                        }
                    ],
                    "synonyms": [
                        "vuurroer"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historisch wapen"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинный вид огнестрельного оружия, похожий на мушкет.",
                        "en": "An old type of firearm, similar to a musket."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пищаль",
                                "target_lang_sense_clarification": "старинное оружие"
                            },
                            {
                                "target_lang_word": "аркебуза",
                                "target_lang_sense_clarification": "тип мушкета"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "arquebus",
                                "target_lang_sense_clarification": "historical firearm"
                            },
                            {
                                "target_lang_word": "musket",
                                "target_lang_sense_clarification": "type of gun"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d1036d63-d05e-3559-b35a-89164b9f7250",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>roeren</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>roer</w> de soep goed door zodat de groenten zich mengen.",
                            "target_lang_translations": {
                                "ru": "Я хорошо <w>мешаю</w> суп, чтобы овощи перемешались.",
                                "en": "I <w>stir</w> the soup well so that the vegetables mix."
                            }
                        },
                        {
                            "text": "Elke ochtend <w>roer</w> ik een beetje suiker in mijn koffie.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я <w>размешиваю</w> немного сахара в своем кофе.",
                                "en": "Every morning I <w>stir</w> a little sugar into my coffee."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени от глагола roeren (мешать, шевелить).",
                        "en": "First person singular present tense of the verb 'roeren' (to stir, to move)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мешаю",
                                "target_lang_sense_clarification": "перемешиваю"
                            },
                            {
                                "target_lang_word": "размешиваю",
                                "target_lang_sense_clarification": "растворяю"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "stir",
                                "target_lang_sense_clarification": "mix"
                            },
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "change position"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7cc7e42-49c5-350f-8663-c837d567ac55",
                    "sense_definition": "Gebiedende wijs van <w>roeren</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Roer</w> de verf stevig door voordat je begint met schilderen.",
                            "target_lang_translations": {
                                "ru": "Тщательно <w>перемешай</w> краску, прежде чем начать красить.",
                                "en": "<w>Stir</w> the paint vigorously before you start painting."
                            }
                        },
                        {
                            "text": "<w>Roer</w> je niet, anders ziet de bewaker ons misschien!",
                            "target_lang_translations": {
                                "ru": "Не <w>шевелись</w>, иначе охранник может нас заметить!",
                                "en": "Don't <w>move</w>, otherwise the guard might see us!"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола roeren.",
                        "en": "Imperative mood of the verb 'roeren'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мешай",
                                "target_lang_sense_clarification": "перемешивай"
                            },
                            {
                                "target_lang_word": "шевелись",
                                "target_lang_sense_clarification": "двигайся (возвр.)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "stir",
                                "target_lang_sense_clarification": "mix"
                            },
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "reflexive: move oneself"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "fee9608b-c098-39a9-a2d4-5a44efd365fb",
                    "sense_definition": "Een rivier die door België, Duitsland en Nederland stroomt en uitmondt in de Maas.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Tijdens onze vakantie hebben we heerlijk gewandeld langs de oevers van de <w>Roer</w>.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска мы чудесно погуляли по берегам <w>Рура</w>.",
                                "en": "During our vacation, we had a lovely walk along the banks of the <w>Rur</w>."
                            }
                        },
                        {
                            "text": "De stad Roermond dankt haar naam aan de monding van de <w>Roer</w>.",
                            "target_lang_translations": {
                                "ru": "Город Рурмонд обязан своим названием устью <w>Рура</w>.",
                                "en": "The city of Roermond owes its name to the mouth of the <w>Rur</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Река, протекающая через Бельгию, Германию и Нидерланды и впадающая в Маас.",
                        "en": "A river flowing through Belgium, Germany, and the Netherlands, emptying into the Meuse."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "782bb188-49b3-3013-9f2d-241bb84e21e4",
                    "sense_definition": "Never changing or capable of being changed; always staying the same.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "unchanging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her <w>invariable</w> kindness made her popular with everyone in the office.",
                            "target_lang_translations": {
                                "ru": "Её <w>неизменная</w> доброта сделала её популярной среди всех в офисе."
                            }
                        },
                        {
                            "text": "It is an <w>invariable</w> rule that employees must wash their hands before returning to work.",
                            "target_lang_translations": {
                                "ru": "Это <w>неизменное</w> правило: сотрудники должны мыть руки перед возвращением к работе."
                            }
                        }
                    ],
                    "antonyms": [
                        "variable",
                        "changing",
                        "inconsistent"
                    ],
                    "common_phrases": [
                        "invariable rule",
                        "invariable routine",
                        "invariable habit",
                        "invariable consequence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Никогда не меняющийся или не поддающийся изменению; всегда остающийся прежним."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неизменный",
                                "target_lang_sense_clarification": "о качестве, правиле или привычке"
                            },
                            {
                                "target_lang_word": "постоянный",
                                "target_lang_sense_clarification": "синоним, указывающий на стабильность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cec25b2c-af38-3b5e-894d-b8b4372752e0",
                    "sense_definition": "Having only one form; not changing for grammatical case, gender, or number.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "linguistic_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In English, prepositions are <w>invariable</w> words that never change their spelling.",
                            "target_lang_translations": {
                                "ru": "В английском языке предлоги — это <w>неизменяемые</w> слова, написание которых никогда не меняется."
                            }
                        },
                        {
                            "text": "Students find <w>invariable</w> adjectives easier to learn because they do not require agreement.",
                            "target_lang_translations": {
                                "ru": "Студенты находят <w>неизменяемые</w> прилагательные более легкими для изучения, так как они не требуют согласования."
                            }
                        }
                    ],
                    "synonyms": [
                        "uninflectable"
                    ],
                    "antonyms": [
                        "inflected",
                        "variable"
                    ],
                    "common_phrases": [
                        "invariable noun",
                        "invariable adjective",
                        "invariable part-of-speech"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий только одну форму; не изменяющийся по падежам, родам или числам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неизменяемый",
                                "target_lang_sense_clarification": "грамматический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2417fe00-8d5b-3209-b338-97d32a620c99",
                    "sense_definition": "A quantity, situation, or factor that remains constant and does not change.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "constant_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The speed of light is considered an <w>invariable</w> in modern physics.",
                            "target_lang_translations": {
                                "ru": "Скорость света считается <w>константой</w> в современной физике."
                            }
                        },
                        {
                            "text": "Amidst the chaos of the market, his integrity remained the only <w>invariable</w>.",
                            "target_lang_translations": {
                                "ru": "Посреди хаоса рынка его честность оставалась единственной <w>константой</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "variable"
                    ],
                    "target_lang_definitions": {
                        "ru": "Величина, ситуация или фактор, который остается постоянным и не меняется."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "константа",
                                "target_lang_sense_clarification": "в научном или переносном смысле"
                            },
                            {
                                "target_lang_word": "постоянная величина",
                                "target_lang_sense_clarification": "математический или физический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
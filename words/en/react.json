                    "common_phrases": [
                        "sad reacts only"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Эмодзи или иконка, используемая для выражения реакции на пост в социальных сетях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реакция",
                                "target_lang_sense_clarification": "существительное"
                            }

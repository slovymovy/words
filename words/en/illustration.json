            "senses": [
                {
                    "sense_id": "6463827e-9253-38c6-8715-8760e8da3221",
                    "sense_definition": "A picture, drawing, or diagram in a book, magazine, or other publication, used to explain the text or make it look attractive.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>illustration</w> showing the water cycle made the concept much easier for the children to understand."
                        },
                        {
                            "text": "This book contains a beautiful full-color <w>illustration</w> on almost every page."
                        }
                    ],
                    "synonyms": [
                        "picture",
                        "drawing",
                        "image",
                        "sketch",
                        "figure",
                        "diagram"
                    ],
                    "common_phrases": [

            "senses": [
                {
                    "sense_id": "74f455ac-a562-3688-9296-d9dd08320657",
                    "sense_definition": "Een keuze die gemaakt wordt na het overwegen van verschillende mogelijkheden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur nam gisteren het <w>besluit</w> om het kantoor te sluiten.",
                            "target_lang_translations": {
                                "en": "The director made the <w>decision</w> yesterday to close the office.",
                                "ru": "Вчера директор принял <w>решение</w> закрыть офис."
                            }
                        },
                        {
                            "text": "Na lang nadenken kwamen we tot een <w>besluit</w>.",
                            "target_lang_translations": {
                                "en": "After thinking for a long time, we came to a <w>decision</w>.",
                                "ru": "После долгих раздумий мы пришли к <w>решению</w>."
                            }
                        }

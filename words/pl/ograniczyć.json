    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ba10d38e-cb52-3f69-baa2-03a088900655",
                    "sense_definition": "Sprawić, że coś staje się mniejsze, ma mniejszy zasięg, swobodę lub intensywność.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "restriction_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowe przepisy mają na celu <w>ograniczyć</w> emisję szkodliwych spalin do atmosfery.",
                            "target_lang_translations": {
                                "ru": "Новые правила нацелены на то, чтобы <w>ограничить</w> выброс вредных газов в атмосферу."
                            }
                        },
                        {
                            "text": "Rodzice postanowili <w>ograniczyć</w> dzieciom czas spędzany przed komputerem.",
                            "target_lang_translations": {
                                "ru": "Родители решили <w>ограничить</w> время, проводимое детьми за компьютером."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmniejszyć",
                        "zredukować",
                        "ukrócić",
                        "zawęzić"
                    ],
                    "antonyms": [
                        "zwiększyć",
                        "rozszerzyć",
                        "powiększyć",
                        "nasilić"
                    ],
                    "common_phrases": [
                        "ograniczyć dostęp",
                        "ograniczyć ryzyko",
                        "ograniczyć swobodę",
                        "ograniczyć zaufanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать что-то меньше, уменьшить диапазон, свободу или интенсивность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ограничить",
                                "target_lang_sense_clarification": "поставить пределы, стеснить"
                            },
                            {
                                "target_lang_word": "лимитировать",
                                "target_lang_sense_clarification": "установить лимит"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2cbf7996-bbe8-37f9-bf41-2cdee115d13d",
                    "sense_definition": "Zmniejszyć ilość wydawanych pieniędzy, zużywanych zasobów lub spożywanych produktów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "restriction_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <w>ograniczyć</w> nasze wydatki w tym miesiącu, aby zaoszczędzić na wakacje.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>сократить</w> наши расходы в этом месяце, чтобы сэкономить на отпуск."
                            }
                        },
                        {
                            "text": "Lekarz zalecił pacjentowi, aby stanowczo <w>ograniczył</w> spożycie cukru i soli.",
                            "target_lang_translations": {
                                "ru": "Врач посоветовал пациенту решительно <w>ограничить</w> потребление сахара и соли."
                            }
                        }
                    ],
                    "synonyms": [
                        "obciąć",
                        "zminimalizować"
                    ],
                    "antonyms": [
                        "zwiększyć",
                        "podwyższyć"
                    ],
                    "common_phrases": [
                        "ograniczyć wydatki",
                        "ograniczyć koszty",
                        "ograniczyć jedzenie",
                        "ograniczyć budżet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшить количество расходуемых денег, используемых ресурсов или потребляемых продуктов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сократить",
                                "target_lang_sense_clarification": "уменьшить количество или расходы"
                            },
                            {
                                "target_lang_word": "урезать",
                                "target_lang_sense_clarification": "значительно уменьшить (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1323c514-dc83-3e54-a342-285835db9371",
                    "sense_definition": "Zrobić tylko to, co konieczne, lub zadowolić się mniejszą ilością czegoś (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "restriction_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas zebrania dyrektor <w>ograniczył</w> się jedynie do przedstawienia wyników finansowych.",
                            "target_lang_translations": {
                                "ru": "Во время собрания директор <w>ограничился</w> лишь представлением финансовых результатов."
                            }
                        },
                        {
                            "text": "Nie miałem dużo czasu, więc <w>ograniczyłem</w> się do krótkiego spaceru po parku.",
                            "target_lang_translations": {
                                "ru": "У меня было мало времени, поэтому я <w>ограничился</w> короткой прогулкой по парку."
                            }
                        }
                    ],
                    "synonyms": [
                        "poprzestać"
                    ],
                    "common_phrases": [
                        "ograniczyć się do minimum",
                        "ograniczyć się do stwierdzenia",
                        "ograniczyć się do roli"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "reflexive with 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать только необходимое или довольствоваться малым (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ограничиться",
                                "target_lang_sense_clarification": "довольствоваться чем-то одним"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
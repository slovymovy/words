{
    "word_family": [
        "przychodzić",
        "przychód",
        "przyszłość",
        "przybysz"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "13a8ee84-9100-3c20-9e44-6cf654be81ad",
                    "sense_definition": "Dotrzeć do określonego miejsca, znajdując się w nim (zwykle poruszając się pieszo).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój najlepszy przyjaciel <przyszedł> wczoraj do mnie z wizytą."
                        },
                        {
                            "text": "Musisz <przyjść> do biura punktualnie o ósmej rano."
                        }
                    ],
                    "synonyms": [
                        "pojawić się",
                        "reg. pozn. przytośtać się"
                    ],
                    "common_phrases": [
                        "przyjść do domu",
                        "przyjść na spotkanie",
                        "przyjść z wizytą"
                    ]
                },
                {
                    "sense_id": "dd309dd7-f683-375e-8561-178c0967a693",
                    "sense_definition": "Nastąpić lub zacząć się (o czasie, porze roku lub wydarzeniu).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W końcu <przyszła> długo oczekiwana wiosna i zrobiło się ciepło."
                        },
                        {
                            "text": "Nagle <przyszła> potężna burza i musieliśmy uciekać do domu."
                        }
                    ],
                    "common_phrases": [
                        "czas przyszedł",
                        "przyszła zima",
                        "przyszła kolej"
                    ]
                },
                {
                    "sense_id": "a5ccb690-a127-3f9a-8a79-2a6a3953ed8c",
                    "sense_definition": "Pojawić się z powrotem w miejscu, w którym się wcześniej było.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy <przyszedł> z powrotem do pokoju, nikogo już tam nie było."
                        },
                        {
                            "text": "Pies uciekł rano, ale <przyszedł> pod drzwi wieczorem."
                        }
                    ],
                    "synonyms": [
                        "wrócić"
                    ],
                    "common_phrases": [
                        "przyjść z powrotem"
                    ]
                },
                {
                    "sense_id": "18960572-122c-34a7-b1f7-e38fdf3664fc",
                    "sense_definition": "Pojawić się w świadomości, pamięci lub odczuciach (o myśli, pomyśle, ochocie).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagle <przyszło> mi do głowy świetne rozwiązanie tego trudnego problemu."
                        },
                        {
                            "text": "Nigdy nie <przyszło> jej na myśl, że może zostać zwolniona."
                        }
                    ],
                    "synonyms": [
                        "nasunąć się"
                    ],
                    "common_phrases": [
                        "przyjść do głowy",
                        "przyjść na myśl",
                        "przyjść ochota"
                    ]
                },
                {
                    "sense_id": "0c6cdef2-6d97-3acc-9a37-4db99807afe7",
                    "sense_definition": "Zostać dostarczonym do odbiorcy (np. o liście, paczce, przelewie).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "delivery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj <przyszła> do nas paczka, na którą czekaliśmy od tygodnia."
                        },
                        {
                            "text": "Pieniądze z wypłaty powinny <przyjść> na twoje konto jutro rano."
                        }
                    ],
                    "common_phrases": [
                        "przyjść pocztą",
                        "przelew przyszedł"
                    ]
                },
                {
                    "sense_id": "edc133bd-6dd6-323e-ac8e-9ea49b8e59b9",
                    "sense_definition": "Stać się częścią jakiejś grupy lub wziąć udział w jakimś wydarzeniu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu nowych członków <przyszło> do naszego klubu w tym miesiącu."
                        },
                        {
                            "text": "Możesz <przyjść> do nas i pomóc w przygotowaniach do imprezy."
                        }
                    ],
                    "synonyms": [
                        "dołączyć się",
                        "przyłączyć się"
                    ],
                    "common_phrases": [
                        "przyjść z pomocą"
                    ]
                },
                {
                    "sense_id": "53edc76c-8da4-31be-b050-d2cf86cdfb73",
                    "sense_definition": "Odzyskać sprawność, zdrowie lub równowagę psychiczną (zwykle w zwrotach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent szybko <przyszedł> do siebie po skomplikowanej operacji serca."
                        },
                        {
                            "text": "Muszę chwilę odpocząć, żeby <przyjść> do równowagi po tym stresie."
                        }
                    ],
                    "common_phrases": [
                        "przyjść do siebie",
                        "przyjść do zdrowia",
                        "przyjść do formy"
                    ]
                },
                {
                    "sense_id": "65523d09-2e5a-3bea-8f61-c0cdf2f5ea69",
                    "sense_definition": "Urodzić się (używane w stałym wyrażeniu \"przyjść na świat\").",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "life",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ich pierwsze dziecko <przyszło> na świat wczesnym rankiem w niedzielę."
                        },
                        {
                            "text": "Mały kotek <przyszedł> na świat w bezpiecznym schronieniu pod schodami."
                        }
                    ],
                    "synonyms": [
                        "urodzić się"
                    ],
                    "common_phrases": [
                        "przyjść na świat"
                    ]
                },
                {
                    "sense_id": "7711b07f-037b-3143-afe1-aa6a79610b87",
                    "sense_definition": "Być zmuszonym przez los lub okoliczności do zrobienia czegoś (forma bezosobowa).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "impersonal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety <przyszło> mi zapłacić wysoki mandat za złe parkowanie."
                        },
                        {
                            "text": "<Przyszło> nam żyć w bardzo ciekawych i dynamicznych czasach."
                        }
                    ],
                    "common_phrases": [
                        "przyszło mi",
                        "przyszło nam żyć"
                    ]
                }
            ]
        }
    ]
}
{
    "word_family": [
        "u",
        "uwe"
    ],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "079369a0-61a1-37a5-838a-0413390a8782",
                    "sense_definition": "Van u; geeft aan dat iets toebehoort aan de persoon die men op beleefde wijze aanspreekt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possessive_formal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik u vragen wat <w>uw</w> naam is?",
                            "target_lang_translations": {
                                "en": "May I ask you what <w>your</w> name is?",
                                "ru": "Могу я спросить, как <w>Ваше</w> имя?"
                            }
                        },
                        {
                            "text": "Wij danken u hartelijk voor <w>uw</w> begrip in deze situatie.",
                            "target_lang_translations": {
                                "en": "We thank you sincerely for <w>your</w> understanding in this situation.",
                                "ru": "Мы сердечно благодарим вас за <w>Ваше</w> понимание в этой ситуации."
                            }
                        }
                    ],
                    "antonyms": [
                        "mijn"
                    ],
                    "common_phrases": [
                        "tot uw dienst",
                        "naar uw wens",
                        "voor uw rekening",
                        "met uw welnemen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "beleefdheidsvorm"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Of you; indicates that something belongs to the person being addressed in a polite manner.",
                        "ru": "Ваш; указывает на то, что что-то принадлежит лицу, к которому обращаются в вежливой форме."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "your",
                                "target_lang_sense_clarification": "formal/polite"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ваш",
                                "target_lang_sense_clarification": "вежливая форма"
                            },
                            {
                                "target_lang_word": "свой",
                                "target_lang_sense_clarification": "возвратное притяжательное местоимение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba61866f-8bec-3d75-8c18-8b74357362b7",
                    "sense_definition": "Van u; geeft aan dat iets toebehoort aan de aangesprokene (gebruikt in combinatie met 'gij').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "possessive_regional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijt ge zeker dat dit <w>uw</w> jas is?",
                            "target_lang_translations": {
                                "en": "Are you sure that this is <w>your</w> coat?",
                                "ru": "Ты уверен, что это <w>твоя</w> куртка?"
                            }
                        },
                        {
                            "text": "Ge moet goed op <w>uw</w> gezondheid letten.",
                            "target_lang_translations": {
                                "en": "You must take good care of <w>your</w> health.",
                                "ru": "Тебе нужно хорошо следить за <w>своим</w> здоровьем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op uw gemak",
                        "trek uw plan"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Vlaanderen"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "in Nederland"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Of you; indicates that something belongs to the person addressed (used in combination with 'gij').",
                        "ru": "Ваш (или твой); указывает на принадлежность собеседнику (используется в сочетании с 'gij' — региональной или устаревшей формой обращения)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "your",
                                "target_lang_sense_clarification": "regional (Flanders) or archaic"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "твой",
                                "target_lang_sense_clarification": "региональное/разговорное употребление (Фландрия)"
                            },
                            {
                                "target_lang_word": "ваш",
                                "target_lang_sense_clarification": "устаревшее или диалектное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
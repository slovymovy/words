                },
                {
                    "sense_id": "87bfdc1b-4cb6-3dfc-a418-a903a766539f",
                    "sense_definition": "An item of clothing worn under or over another.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is best to wear <w>layers</w> when hiking in changing weather.",

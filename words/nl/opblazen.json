{
    "word_family": [
        "opblaasbaar",
        "opblaasboot",
        "opblaaspop"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cc755a24-3026-3714-9d47-dc9cf3d29cd6",
                    "sense_definition": "Iets vullen met lucht of gas zodat het groter en boller wordt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vullen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor het feestje moet ik nog tien ballonnen <w>opblazen</w>.",
                            "target_lang_translations": {
                                "ru": "Перед вечеринкой мне нужно <w>надуть</w> еще десять шариков.",
                                "en": "For the party I still have to <w>blow up</w> ten balloons."
                            }
                        },
                        {
                            "text": "Papa gaat het luchtbed <w>opblazen</w> zodat we kunnen zwemmen.",
                            "target_lang_translations": {
                                "ru": "Папа собирается <w>надуть</w> надувной матрас, чтобы мы могли поплавать.",
                                "en": "Dad is going to <w>inflate</w> the air mattress so we can swim."
                            }
                        }
                    ],
                    "synonyms": [
                        "oppompen"
                    ],
                    "common_phrases": [
                        "een ballon opblazen",
                        "een luchtbed opblazen",
                        "banden opblazen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наполнять что-либо воздухом или газом, чтобы оно увеличилось в объеме.",
                        "en": "To fill something with air or gas so that it becomes larger and rounder."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надувать",
                                "target_lang_sense_clarification": "наполнять воздухом (шар, шину)"
                            },
                            {
                                "target_lang_word": "накачивать",
                                "target_lang_sense_clarification": "наполнять насосом"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "blow up",
                                "target_lang_sense_clarification": "fill with air"
                            },
                            {
                                "target_lang_word": "inflate",
                                "target_lang_sense_clarification": "formal or technical"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e62a94e8-b734-36ed-8e1c-70dd1c8c28cc",
                    "sense_definition": "Iets vernietigen of kapotmaken door het te laten ontploffen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "vernietigen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten wilden de brug <w>opblazen</w> om de vijand te stoppen.",
                            "target_lang_translations": {
                                "ru": "Солдаты хотели <w>взорвать</w> мост, чтобы остановить врага.",
                                "en": "The soldiers wanted to <w>blow up</w> the bridge to stop the enemy."
                            }
                        },
                        {
                            "text": "In de actiefilm werd het hele gebouw <w>opgeblazen</w>.",
                            "target_lang_translations": {
                                "ru": "В боевике <w>взорвали</w> целое здание.",
                                "en": "In the action movie, the whole building was <w>blown up</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een brug opblazen",
                        "de boel opblazen",
                        "met dynamiet opblazen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уничтожать или разрушать что-либо с помощью взрыва.",
                        "en": "To destroy or break something by causing it to explode."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрывать",
                                "target_lang_sense_clarification": "разрушать взрывом"
                            },
                            {
                                "target_lang_word": "подрывать",
                                "target_lang_sense_clarification": "взрывать снизу или частично"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "blow up",
                                "target_lang_sense_clarification": "destroy by explosion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3468d35-ecc4-3f0e-b704-2b85be7b5d9f",
                    "sense_definition": "Een verhaal of gebeurtenis veel groter, belangrijker of erger voorstellen dan het in werkelijkheid is.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "overdrijven",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kranten hebben dit kleine incident enorm <w>opgeblazen</w>.",
                            "target_lang_translations": {
                                "ru": "Газеты сильно <w>раздули</w> этот мелкий инцидент.",
                                "en": "The newspapers have enormously <w>blown</w> this small incident <w>out of proportion</w>."
                            }
                        },
                        {
                            "text": "Je moet het probleem niet zo <w>opblazen</w>, het valt best mee.",
                            "target_lang_translations": {
                                "ru": "Не стоит так <w>раздувать</w> проблему, всё не так уж плохо.",
                                "en": "You shouldn't <w>exaggerate</w> the problem so much, it's not that bad."
                            }
                        }
                    ],
                    "synonyms": [
                        "overdrijven",
                        "chargeren"
                    ],
                    "antonyms": [
                        "bagatelliseren"
                    ],
                    "common_phrases": [
                        "een verhaal opblazen",
                        "de zaak opblazen",
                        "iets enorm opblazen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "figuurlijk"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Представлять историю или событие более значительным или серьезным, чем на самом деле; преувеличивать.",
                        "en": "To present a story or event as much larger, more important, or worse than it actually is."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздувать",
                                "target_lang_sense_clarification": "преувеличивать значение (перен.)"
                            },
                            {
                                "target_lang_word": "преувеличивать",
                                "target_lang_sense_clarification": "делать более значимым"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "exaggerate",
                                "target_lang_sense_clarification": "overstate facts"
                            },
                            {
                                "target_lang_word": "blow up",
                                "target_lang_sense_clarification": "make seem bigger (informal)"
                            },
                            {
                                "target_lang_word": "blow out of proportion",
                                "target_lang_sense_clarification": "idiomatic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d25f9b8b-9a91-3447-a8d1-263ac4e1e252",
                    "sense_definition": "Lucht of wind krachtig in een bepaalde richting of omhoog blazen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "blazen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De harde wind <w>blies</w> het zand <w>op</w> in onze gezichten.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер <w>поднял</w> песок нам в лицо.",
                                "en": "The strong wind <w>blew</w> the sand <w>up</w> into our faces."
                            }
                        },
                        {
                            "text": "Hij <w>blies</w> de rook van zijn sigaar recht omhoog <w>op</w>.",
                            "target_lang_translations": {
                                "ru": "Он <w>выпустил</w> дым от сигары прямо вверх.",
                                "en": "He <w>blew</w> the smoke from his cigar straight <w>up</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stof opblazen",
                        "rook opblazen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно дуть воздухом или ветром в определенном направлении или вверх.",
                        "en": "To blow air or wind forcefully in a certain direction or upwards."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поднимать (ветром)",
                                "target_lang_sense_clarification": "о пыли или песке"
                            },
                            {
                                "target_lang_word": "выпускать (вверх)",
                                "target_lang_sense_clarification": "о дыме"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "blow up",
                                "target_lang_sense_clarification": "blow upwards"
                            },
                            {
                                "target_lang_word": "kick up",
                                "target_lang_sense_clarification": "dust or sand"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
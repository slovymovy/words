                    "semantic_group_id": "toponiem",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Hij woont in de buurtschap <w>Kamp</w> in Limburg.",
                            "target_lang_translations": {
                                "ru": "Он живет в местечке <w>Камп</w> в Лимбурге."
                            }
                        },
                        {
                            "text": "De wijk <w>Kamp</w> ligt vlak bij het centrum.",
                            "target_lang_translations": {
                                "ru": "Район <w>Камп</w> находится недалеко от центра."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хутор или район в различных населенных пунктах Нидерландов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Камп",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b89498b6-de65-3431-baab-81e794ead4f6",
                    "sense_definition": "Een achternaam.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "naam",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Meneer <w>Kamp</w> is vandaag niet op kantoor.",
                            "target_lang_translations": {
                                "ru": "Господина <w>Кампа</w> сегодня нет в офисе."
                            }
                        },
                        {
                            "text": "Ik heb een afspraak met mevrouw <w>Kamp</w>.",
                            "target_lang_translations": {
                                "ru": "У меня встреча с госпожой <w>Камп</w>."
                            }
                        }
                    ],

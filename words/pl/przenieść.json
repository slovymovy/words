                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перенести",
                                "target_lang_sense_clarification": "данные, файлы, болезнь"
                            },
                            {
                                "target_lang_word": "передать",
                                "target_lang_sense_clarification": "права, инфекцию"
                            }

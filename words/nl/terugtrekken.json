{
    "word_family": [
        "trekken",
        "terugtocht",
        "terugtrekking"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0d015b4c-7c8e-3755-829e-faa106140e75",
                    "sense_definition": "Een lichaamsdeel of voorwerp weer naar je toe halen of van iets verwijderen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>trok</w> zijn hand direct <w>terug</w> toen hij de hete kachel voelde.",
                            "target_lang_translations": {
                                "en": "He immediately <w>pulled back</w> his hand when he felt the hot stove.",
                                "ru": "Он сразу же <w>отдёрнул</w> руку, когда почувствовал горячую печь."
                            }
                        },
                        {
                            "text": "De kat <w>trok</w> haar scherpe nagels <w>terug</w> na het spelen.",
                            "target_lang_translations": {
                                "en": "The cat <w>retracted</w> her sharp claws after playing.",
                                "ru": "Кошка <w>втянула</w> свои острые когти после игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "intrekken",
                        "weghalen"
                    ],
                    "antonyms": [
                        "uitsteken",
                        "uitstrekken"
                    ],
                    "common_phrases": [
                        "je hand terugtrekken"
                    ],
                    "target_lang_definitions": {
                        "en": "To pull a body part or object back towards oneself or away from something.",
                        "ru": "Притянуть часть тела или предмет обратно к себе или убрать от чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pull back",
                                "target_lang_sense_clarification": "general physical action"
                            },
                            {
                                "target_lang_word": "withdraw",
                                "target_lang_sense_clarification": "formal physical action"
                            },
                            {
                                "target_lang_word": "retract",
                                "target_lang_sense_clarification": "specifically for claws, nails, or mechanisms"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отдёргивать",
                                "target_lang_sense_clarification": "резко убрать (обычно руку)"
                            },
                            {
                                "target_lang_word": "втягивать",
                                "target_lang_sense_clarification": "спрятать внутрь (например, когти)"
                            },
                            {
                                "target_lang_word": "убирать",
                                "target_lang_sense_clarification": "переместить назад или прочь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a158a5ac-051b-3166-bdfc-2480218599f9",
                    "sense_definition": "Een gebied verlaten en naar een veiligere of eerdere positie gaan, vaak gebruikt in een militaire context.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_retreat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het leger moest zich na de zware strijd <w>terugtrekken</w> uit de stad.",
                            "target_lang_translations": {
                                "en": "The army had to <w>withdraw</w> from the city after the heavy battle.",
                                "ru": "Армии пришлось <w>отступить</w> из города после тяжелой битвы."
                            }
                        },
                        {
                            "text": "De soldaten <w>trokken</w> zich <w>terug</w> naar de veilige zone achter de rivier.",
                            "target_lang_translations": {
                                "en": "The soldiers <w>retreated</w> to the safe zone behind the river.",
                                "ru": "Солдаты <w>отошли</w> в безопасную зону за рекой."
                            }
                        }
                    ],
                    "synonyms": [
                        "wijken",
                        "de aftocht blazen"
                    ],
                    "antonyms": [
                        "oprukken",
                        "aanvallen"
                    ],
                    "common_phrases": [
                        "troepen terugtrekken",
                        "zich terugtrekken uit"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave an area and move to a safer or previous position, often used in a military context.",
                        "ru": "Покинуть территорию и отойти на более безопасную или прежнюю позицию (обычно о войсках)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "retreat",
                                "target_lang_sense_clarification": "military movement away from enemy"
                            },
                            {
                                "target_lang_word": "withdraw",
                                "target_lang_sense_clarification": "leaving a position or area"
                            },
                            {
                                "target_lang_word": "pull back",
                                "target_lang_sense_clarification": "moving troops backwards"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отступать",
                                "target_lang_sense_clarification": "покидать позиции под натиском"
                            },
                            {
                                "target_lang_word": "отходить",
                                "target_lang_sense_clarification": "перемещаться назад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8db98217-bc4e-369b-98b8-6222e1f71ac8",
                    "sense_definition": "Jezelf verwijderen uit een groep of openbare ruimte om alleen te zijn of rust te zoeken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_withdrawal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het drukke diner <w>trok</w> hij zich <w>terug</w> in zijn werkkamer.",
                            "target_lang_translations": {
                                "en": "After the busy dinner, he <w>withdrew</w> to his study.",
                                "ru": "После шумного ужина он <w>уединился</w> в своем кабинете."
                            }
                        },
                        {
                            "text": "Zij <w>trekt</w> zich graag <w>terug</w> in de natuur om na te denken.",
                            "target_lang_translations": {
                                "en": "She likes to <w>retreat</w> into nature to think.",
                                "ru": "Она любит <w>уединяться</w> на природе, чтобы подумать."
                            }
                        }
                    ],
                    "synonyms": [
                        "zich afzonderen"
                    ],
                    "antonyms": [
                        "zich mengen",
                        "deelnemen"
                    ],
                    "common_phrases": [
                        "zich stilletjes terugtrekken"
                    ],
                    "target_lang_definitions": {
                        "en": "To remove oneself from a group or public space to be alone or seek quiet.",
                        "ru": "Уйти от людей или из общественного места, чтобы побыть в одиночестве."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "withdraw",
                                "target_lang_sense_clarification": "removing oneself socially"
                            },
                            {
                                "target_lang_word": "retreat",
                                "target_lang_sense_clarification": "going to a quiet or safe place"
                            },
                            {
                                "target_lang_word": "retire",
                                "target_lang_sense_clarification": "going to a private room or bed"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уединяться",
                                "target_lang_sense_clarification": "обособляться от других"
                            },
                            {
                                "target_lang_word": "удаляться",
                                "target_lang_sense_clarification": "уходить в другое помещение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df7e9e02-56b5-3618-8d8c-a279bf5da9a3",
                    "sense_definition": "Een beslissing, belofte, verklaring of kandidatuur ongedaan maken of annuleren.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_revocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De minister besloot zijn omstreden wetsvoorstel <w>terug</w> te <w>trekken</w>.",
                            "target_lang_translations": {
                                "en": "The minister decided to <w>withdraw</w> his controversial bill.",
                                "ru": "Министр решил <w>отозвать</w> свой спорный законопроект."
                            }
                        },
                        {
                            "text": "Hij heeft zijn bod op het huis gisteren onverwacht <w>teruggetrokken</w>.",
                            "target_lang_translations": {
                                "en": "He unexpectedly <w>withdrew</w> his bid on the house yesterday.",
                                "ru": "Вчера он неожиданно <w>отозвал</w> свое предложение о покупке дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "herroepen",
                        "intrekken",
                        "annuleren"
                    ],
                    "antonyms": [
                        "indienen",
                        "handhaven"
                    ],
                    "common_phrases": [
                        "een kandidatuur terugtrekken",
                        "een voorstel terugtrekken",
                        "een klacht terugtrekken"
                    ],
                    "target_lang_definitions": {
                        "en": "To undo or cancel a decision, promise, statement, or candidacy.",
                        "ru": "Отменить решение, обещание, заявление или снять кандидатуру."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "withdraw",
                                "target_lang_sense_clarification": "for proposals, bids, or candidacies"
                            },
                            {
                                "target_lang_word": "retract",
                                "target_lang_sense_clarification": "for statements or confessions"
                            },
                            {
                                "target_lang_word": "revoke",
                                "target_lang_sense_clarification": "formal cancellation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отзывать",
                                "target_lang_sense_clarification": "аннулировать официальное предложение или документ"
                            },
                            {
                                "target_lang_word": "снимать",
                                "target_lang_sense_clarification": "отменять кандидатуру или участие"
                            },
                            {
                                "target_lang_word": "забирать",
                                "target_lang_sense_clarification": "брать назад слова или обещание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
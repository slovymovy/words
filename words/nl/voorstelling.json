    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a49735b0-71ee-3d16-905c-20b455d46526",
                    "sense_definition": "Een uitvoering van toneel, dans, muziek of film voor een publiek.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben gisteren een prachtige <w>voorstelling</w> in de schouwburg gezien.",
                            "target_lang_translations": {
                                "en": "We saw a beautiful <w>performance</w> in the theatre yesterday.",
                                "ru": "Вчера мы посмотрели в театре прекрасное <w>представление</w>."
                            }
                        },
                        {
                            "text": "De <w>voorstelling</w> begint om acht uur, dus we moeten opschieten.",
                            "target_lang_translations": {
                                "en": "The <w>show</w> starts at eight o'clock, so we have to hurry.",
                                "ru": "<w>Представление</w> начинается в восемь часов, поэтому нам нужно поторопиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "optreden",
                        "uitvoering",
                        "show"
                    ],
                    "common_phrases": [
                        "naar de voorstelling",
                        "een voorstelling geven",
                        "de laatste voorstelling"
                    ],
                    "target_lang_definitions": {
                        "en": "A performance of theatre, dance, music, or film for an audience.",
                        "ru": "Исполнение театральной пьесы, танца, музыки или показ фильма для публики."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "performance",
                                "target_lang_sense_clarification": "theatrical or musical execution"
                            },
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "entertainment event"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "представление",
                                "target_lang_sense_clarification": "театральное или развлекательное мероприятие"
                            },
                            {
                                "target_lang_word": "спектакль",
                                "target_lang_sense_clarification": "театральная постановка"
                            },
                            {
                                "target_lang_word": "шоу",
                                "target_lang_sense_clarification": "развлекательная программа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "039329ed-7dc0-30ef-b445-1cb3ecf6e8ce",
                    "sense_definition": "Een beeld of idee dat iemand in zijn gedachten vormt over iets.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan me geen <w>voorstelling</w> maken van hoe groot het heelal eigenlijk is.",
                            "target_lang_translations": {
                                "en": "I cannot form an <w>idea</w> of how big the universe actually is.",
                                "ru": "Я не могу составить <w>представления</w> о том, насколько на самом деле велика Вселенная."
                            }
                        },
                        {
                            "text": "Zijn <w>voorstelling</w> van de toekomst was heel anders dan de werkelijkheid.",
                            "target_lang_translations": {
                                "en": "His <w>vision</w> of the future was very different from reality.",
                                "ru": "Его <w>представление</w> о будущем сильно отличалось от реальности."
                            }
                        }
                    ],
                    "synonyms": [
                        "idee",
                        "beeld",
                        "besef",
                        "concept"
                    ],
                    "common_phrases": [
                        "zich een voorstelling maken",
                        "geen flauwe voorstelling",
                        "een duidelijke voorstelling"
                    ],
                    "target_lang_definitions": {
                        "en": "An image or idea that someone forms in their mind about something.",
                        "ru": "Образ или идея, которые человек формирует в своем сознании относительно чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "idea",
                                "target_lang_sense_clarification": "mental concept"
                            },
                            {
                                "target_lang_word": "image",
                                "target_lang_sense_clarification": "mental picture"
                            },
                            {
                                "target_lang_word": "concept",
                                "target_lang_sense_clarification": "abstract notion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "представление",
                                "target_lang_sense_clarification": "мыленный образ или понимание"
                            },
                            {
                                "target_lang_word": "понятие",
                                "target_lang_sense_clarification": "абстрактная идея"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24a37a37-9357-37c8-ac7b-5d6fc44eda8f",
                    "sense_definition": "De manier waarop iets of iemand wordt uitgebeeld of beschreven, bijvoorbeeld in een kunstwerk of verslag.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het schilderij is een abstracte <w>voorstelling</w> van het drukke stadsleven.",
                            "target_lang_translations": {
                                "en": "The painting is an abstract <w>representation</w> of busy city life.",
                                "ru": "Эта картина представляет собой абстрактное <w>изображение</w> суетливой городской жизни."
                            }
                        },
                        {
                            "text": "De advocaat gaf een verkeerde <w>voorstelling</w> van zaken om zijn cliënt te beschermen.",
                            "target_lang_translations": {
                                "en": "The lawyer gave a false <w>representation</w> of the facts to protect his client.",
                                "ru": "Адвокат дал ложное <w>представление</w> фактов, чтобы защитить своего клиента."
                            }
                        }
                    ],
                    "synonyms": [
                        "weergave",
                        "uitbeelding",
                        "afbeelding"
                    ],
                    "common_phrases": [
                        "voorstelling van zaken",
                        "een getrouwe voorstelling",
                        "artistieke voorstelling"
                    ],
                    "target_lang_definitions": {
                        "en": "The way something or someone is depicted or described, for example in an artwork or report.",
                        "ru": "То, как что-то или кто-то изображается или описывается, например, в произведении искусства или отчете."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "representation",
                                "target_lang_sense_clarification": "depiction or description"
                            },
                            {

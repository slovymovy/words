    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ce96555c-8497-3ff3-978c-af17630014c6",
                    "sense_definition": "Present participle and gerund of <w>leak</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to call a plumber because the kitchen sink is <w>leaking</w>.",
                            "target_lang_translations": {
                                "ru": "Нам нужно вызвать сантехника, потому что кухонная раковина <w>протекает</w>."
                            }
                        },
                        {
                            "text": "He put a bucket under the <w>leaking</w> roof to catch the water.",
                            "target_lang_translations": {
                                "ru": "Он поставил ведро под <w>протекающую</w> крышу, чтобы собрать воду."
                            }
                        }
                    ],
                    "synonyms": [
                        "dripping",
                        "seeping"
                    ],
                    "common_phrases": [
                        "leaking pipe",
                        "leaking roof",
                        "leaking faucet"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle and gerund"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола leak."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протекающий",
                                "target_lang_sense_clarification": "о крыше, трубе или емкости"
                            },
                            {
                                "target_lang_word": "текущий",
                                "target_lang_sense_clarification": "о кране или жидкости"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f70d5ad0-1d7a-350f-869f-d165ddc110f1",
                    "sense_definition": "The act or process by which fluid or gas escapes through an unintended opening.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The constant <w>leaking</w> of the valve caused the pressure to drop significantly.",
                            "target_lang_translations": {
                                "ru": "Постоянное <w>протекание</w> клапана привело к значительному падению давления."
                            }
                        },
                        {
                            "text": "We must stop the <w>leaking</w> of confidential data to the press.",
                            "target_lang_translations": {
                                "ru": "Мы должны остановить <w>утечку</w> конфиденциальных данных в прессу."
                            }
                        }
                    ],
                    "synonyms": [
                        "leakage"
                    ],
                    "common_phrases": [
                        "constant leaking",
                        "prevent leaking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс, при котором жидкость или газ выходят через непреднамеренное отверстие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утечка",
                                "target_lang_sense_clarification": "газа, жидкости или информации"
                            },
                            {
                                "target_lang_word": "протекание",
                                "target_lang_sense_clarification": "процесс пропускания жидкости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69770ab4-6cba-328a-821b-8cc2eb2ca5d2",
                    "sense_definition": "The substance or fluid that has escaped through a hole or crack.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He carefully wiped up the oily <w>leakings</w> from the garage floor.",
                            "target_lang_translations": {
                                "ru": "Он осторожно вытер маслянистые <w>подтёки</w> с пола гаража."
                            }
                        },
                        {
                            "text": "The <w>leakings</w> from the roof damaged the wooden furniture in the attic.",
                            "target_lang_translations": {
                                "ru": "<w>Протечки</w> с крыши повредили деревянную мебель на чердаке."
                            }
                        }
                    ],
                    "synonyms": [
                        "leakage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество или жидкость, вытекшие через отверстие или трещину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подтёки",
                                "target_lang_sense_clarification": "следы вытекшей жидкости"
                            },
                            {
                                "target_lang_word": "протечка",
                                "target_lang_sense_clarification": "место или результат протекания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
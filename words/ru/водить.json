            "senses": [
                {
                    "sense_id": "b9e1abd8-7ea7-3db0-a9d4-6918886ebab2",
                    "sense_definition": "Сопровождать кого-либо или что-либо, указывая путь или помогая идти (обычно регулярно, туда и обратно или в разных направлениях).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_guide",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама каждый день <водит> сына в детский сад."
                        },
                        {
                            "text": "Экскурсовод <водил> туристов по музею целый час."
                        }
                    ],
                    "synonyms": [
                        "вести"
                    ],
                    "common_phrases": [
                        "водить детей в школу",
                        "водить экскурсии",
                        "водить за руку",
                        "водить за нос"
                    ]
                },
                {
                    "sense_id": "3460fd9e-1ae5-328c-9a69-e2dac2c47c61",
                    "sense_definition": "Управлять транспортным средством (машиной, автобусом и т. д.).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_guide",
                    "name_type": "no",
                    "examples": [

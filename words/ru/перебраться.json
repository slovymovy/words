            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b7a82c2c-5903-3c84-8ebe-dc1b5246291f",
                    "sense_definition": "Переехать на новое место жительства или пребывания.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "move_residence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Семья решила <перебраться> в деревню, чтобы жить ближе к природе."
                        },
                        {
                            "text": "Нам пришлось <перебраться> на новую квартиру из-за ремонта."
                        }
                    ],
                    "synonyms": [
                        "переехать",
                        "переселиться"
                    ],
                    "antonyms": [
                        "остаться"
                    ],
                    "common_phrases": [
                        "перебраться в город",
                        "перебраться на квартиру",
                        "перебраться в дом"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in spoken language instead of 'переехать'"
                        }
                    ]
                },
                {
                    "sense_id": "fb27943c-95f7-344a-9f19-c2e659bd6662",
                    "sense_definition": "Переместиться на другую сторону чего-либо, преодолевая препятствие (реку, забор, гору и т.п.).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cross_obstacle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристам удалось <перебраться> через бурную реку только к вечеру."
                        },
                        {
                            "text": "Кот ловко сумел <перебраться> через высокий забор в соседний сад."
                        }
                    ],
                    "synonyms": [
                        "переправиться",
                        "перейти",
                        "перелезть"
                    ],
                    "common_phrases": [
                        "перебраться через реку",
                        "перебраться на берег",
                        "перебраться через забор"
                    ]
                },
                {
                    "sense_id": "434940b3-93ba-3f37-8a52-7ff4af2cf004",
                    "sense_definition": "Переместиться с одного места на другое (обычно недалеко, в пределах одного пространства).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "move_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гости решили <перебраться> из душной комнаты на свежий воздух в сад."
                        },
                        {
                            "text": "Бабушка попросила помочь ей <перебраться> в удобное кресло."
                        }
                    ],
                    "synonyms": [
                        "переместиться",
                        "пересесть"
                    ],
                    "common_phrases": [
                        "перебраться поближе",
                        "перебраться в кресло"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "36b01589-af71-3621-9eff-db08427a7ac2",
                    "sense_definition": "Сменить шерсть или оперение (о животных и птицах); полинять.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "molt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К зиме заяц должен полностью <перебраться> и стать белым."
                        },
                        {
                            "text": "Молодые птицы успели <перебраться> до начала осеннего перелёта."
                        }
                    ],
                    "synonyms": [
                        "полинять"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "hunting jargon"
                        }
                    ]
                }
            ]

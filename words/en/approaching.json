{
    "word_family": [
        "approach",
        "approachable",
        "unapproachable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "757c84d3-c21b-318d-9dd1-26e0e5371627",
                    "sense_definition": "Present participle and gerund of <w>approach</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The army was <w>approaching</w> from the north.",
                            "target_lang_translations": {
                                "ru": "Армия <w>приближалась</w> с севера."
                            }
                        },
                        {
                            "text": "We are <w>approaching</w> the end of the project.",
                            "target_lang_translations": {
                                "ru": "Мы <w>приближаемся</w> к завершению проекта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fast approaching",
                        "rapidly approaching"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола approach (приближаться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приближающийся",
                                "target_lang_sense_clarification": "причастие; находящийся в процессе приближения"
                            },
                            {
                                "target_lang_word": "подходящий",
                                "target_lang_sense_clarification": "причастие; идущий или наступающий (о времени)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a3fcd89d-47df-38e7-b053-f59a4cc326e5",
                    "sense_definition": "Moving or coming closer in distance, time, or quality.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>approaching</w> storm forced the hikers to turn back.",
                            "target_lang_translations": {
                                "ru": "<w>Надвигающийся</w> шторм заставил туристов повернуть назад."
                            }
                        },
                        {
                            "text": "She felt anxious about the <w>approaching</w> deadline.",
                            "target_lang_translations": {
                                "ru": "Она беспокоилась о <w>приближающемся</w> крайнем сроке."
                            }
                        }
                    ],
                    "synonyms": [
                        "oncoming",
                        "nearing",
                        "impending"
                    ],
                    "antonyms": [
                        "receding",
                        "departing"
                    ],
                    "common_phrases": [
                        "approaching storm",
                        "approaching deadline"
                    ],
                    "target_lang_definitions": {
                        "ru": "Движущийся или подходящий ближе в пространстве, времени или по качеству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надвигающийся",
                                "target_lang_sense_clarification": "о чем-то угрожающем или неизбежном (например, шторм)"
                            },
                            {
                                "target_lang_word": "приближающийся",
                                "target_lang_sense_clarification": "общий термин для движения навстречу"
                            },
                            {
                                "target_lang_word": "наступающий",
                                "target_lang_sense_clarification": "о времени, сезонах или событиях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "46c15a1b-c133-3913-958a-648b44e7cee9",
                    "sense_definition": "Nearly; used to indicate a quantity is getting close to a specific amount.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quantity_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The temperature outside is <w>approaching</w> forty degrees.",
                            "target_lang_translations": {
                                "ru": "Температура на улице <w>приближается</w> к сорока градусам."
                            }
                        },
                        {
                            "text": "The cost of the new stadium is <w>approaching</w> one billion dollars.",
                            "target_lang_translations": {
                                "ru": "Стоимость нового стадиона <w>приближается</w> к одному миллиарду долларов."
                            }
                        }
                    ],
                    "synonyms": [
                        "nearly",
                        "almost",
                        "approximately"
                    ],
                    "common_phrases": [
                        "approaching completion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Почти; указывает на то, что количество или величина становится близкой к определенному значению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приближающийся",
                                "target_lang_sense_clarification": "к определенному числу или уровню"
                            },
                            {
                                "target_lang_word": "достигающий",
                                "target_lang_sense_clarification": "почти доходящий до какого-либо уровня"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d645229f-b727-3222-9b5c-e6f0ba1bd08e",
                    "sense_definition": "The act of coming closer.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "movement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shy cat made several tentative <w>approachings</w> toward the food bowl.",
                            "target_lang_translations": {
                                "ru": "Пугливая кошка сделала несколько неуверенных <w>приближений</w> к миске с едой."
                            }
                        },
                        {
                            "text": "He observed the <w>approachings</w> and retreatings of the waves.",
                            "target_lang_translations": {
                                "ru": "Он наблюдал за <w>приближениями</w> и отступлениями волн."
                            }
                        }
                    ],
                    "synonyms": [
                        "approach"
                    ],
                    "antonyms": [
                        "departure",
                        "retreat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по приближению к чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приближение",
                                "target_lang_sense_clarification": "процесс сокращения расстояния"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
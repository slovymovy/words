            "senses": [
                {
                    "sense_id": "eb73d6e3-9396-3b9c-affd-a04ac8392366",
                    "sense_definition": "Podgrzać coś, zwiększyć temperaturę czegoś (zazwyczaj jedzenia lub pomieszczenia), aby stało się ciepłe.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "heat_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wróciłem późno do domu, więc musiałem sobie <przygrzać> obiad w mikrofalówce.",
                            "target_lang_translations": {
                                "ru": "Я поздно вернулся домой, поэтому мне пришлось <w>разогреть</w> себе обед в микроволновке."
                            }
                        },
                        {
                            "text": "W zimowe wieczory warto mocniej <przygrzać> w piecu, żeby w domu było przytulnie.",
                            "target_lang_translations": {
                                "ru": "Зимними вечерами стоит посильнее <w>натопить</w> печь, чтобы в доме было уютно."
                            }
                        }
                    ],
                    "synonyms": [
                        "podgrzać",
                        "ogrzać"
                    ],
                    "antonyms": [
                        "ostudzić",
                        "schłodzić"
                    ],
                    "common_phrases": [
                        "przygrzać zupę",
                        "przygrzać obiad",
                        "przygrzać mleko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подогреть что-то, повысить температуру чего-либо (обычно еды или помещения), чтобы оно стало теплым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подогреть",
                                "target_lang_sense_clarification": "сделать теплым (о еде)"
                            },
                            {
                                "target_lang_word": "разогреть",
                                "target_lang_sense_clarification": "сделать горячим (о еде)"
                            },
                            {
                                "target_lang_word": "натопить",
                                "target_lang_sense_clarification": "обогреть помещение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d12eeb40-0429-39f3-9d37-52946ac69ad0",
                    "sense_definition": "O słońcu lub innym źródle ciepła: zacząć świecić lub oddziaływać bardzo intensywnie, dając odczuwalne gorąco.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "heat_emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Około południa słońce tak <przygrzało>, że musieliśmy zejść z plaży.",
                            "target_lang_translations": {
                                "ru": "Около полудня солнце так <w>припекло</w>, что нам пришлось уйти с пляжа."
                            }
                        },
                        {
                            "text": "Uważaj, żeby słońce za bardzo ci nie <przygrzało>, bo dostaniesz udaru.",
                            "target_lang_translations": {
                                "ru": "Смотри, чтобы солнце тебе не слишком <w>напекло</w>, а то получишь удар."
                            }
                        }
                    ],
                    "synonyms": [
                        "przypiec",
                        "dogrzać"
                    ],
                    "common_phrases": [
                        "słońce przygrzało",
                        "mocno przygrzać",
                        "nieźle przygrzać"
                    ],
                    "target_lang_definitions": {
                        "ru": "О солнце или другом источнике тепла: начать светить или воздействовать очень интенсивно, создавая ощутимый жар."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "припечь",
                                "target_lang_sense_clarification": "о солнце: начать сильно греть"
                            },
                            {
                                "target_lang_word": "пригреть",
                                "target_lang_sense_clarification": "начать греть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c37b99b2-5d07-38a1-9003-f07034c8644f",
                    "sense_definition": "Uderzyć kogoś lub coś gwałtownie i z dużą siłą.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "hit_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napastnik próbował mu <przygrzać>, ale on zdołał zrobić szybki unik.",
                            "target_lang_translations": {
                                "ru": "Нападавший пытался ему <w>врезать</w>, но он сумел быстро увернуться."
                            }
                        },
                        {
                            "text": "Mężczyzna ze złości <przygrzał> pięścią w stół, aż podskoczyły talerze.",
                            "target_lang_translations": {
                                "ru": "Мужчина со злости <w>ударил</w> кулаком по столу, так что подпрыгнули тарелки."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywalić",
                        "walnąć",
                        "rąbnąć",
                        "uderzyć",
                        "grzmotnąć"
                    ],
                    "common_phrases": [
                        "przygrzać w ucho",
                        "przygrzać z całej siły",
                        "przygrzać piłką"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech to describe hitting"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ударить кого-то или что-то резко и с большой силой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "врезать",
                                "target_lang_sense_clarification": "разговорное: сильно ударить"
                            },
                            {
                                "target_lang_word": "ударить",
                                "target_lang_sense_clarification": "нанести удар"
                            },
                            {
                                "target_lang_word": "заехать",
                                "target_lang_sense_clarification": "разговорное: ударить (обычно рукой)"
                            }
                        ]
                    }
                }
            ]

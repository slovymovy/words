{
    "word_family": [
        "vastzetten",
        "vastigheid"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "593be71c-533c-3555-92d5-f23eb947a769",
                    "sense_definition": "Niet kunnen bewegen of loskomen doordat iets klem zit of stevig bevestigd is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude roestige bout <zat> compleet <vast> in het metaal.",
                            "target_lang_translations": {
                                "en": "The old rusty bolt <was> completely <stuck> in the metal."
                            }
                        },
                        {
                            "text": "Wij <zitten vast> in het verkeer en zullen iets later arriveren.",
                            "target_lang_translations": {
                                "en": "We <are stuck> in traffic and will arrive a little later."
                            }
                        }
                    ],
                    "antonyms": [
                        "loszitten"
                    ],
                    "common_phrases": [
                        "muurvast zitten",
                        "vastzitten in het verkeer"
                    ],
                    "target_lang_definitions": {
                        "en": "To be unable to move or come loose because something is jammed or firmly attached."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be stuck",
                                "target_lang_sense_clarification": "unable to move"
                            },
                            {
                                "target_lang_word": "be jammed",
                                "target_lang_sense_clarification": "mechanically stuck"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a0aae82-0c6a-3e6f-a0b7-723b6fe302ef",
                    "sense_definition": "Opgesloten zijn in een gevangenis of cel als straf of in afwachting van een proces.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte <zit> momenteel <vast> op het politiebureau voor verhoor.",
                            "target_lang_translations": {
                                "en": "The suspect <is> currently <detained> at the police station for interrogation."
                            }
                        },
                        {
                            "text": "Hij heeft jarenlang onschuldig <vastgezeten> voordat de waarheid boven tafel kwam.",
                            "target_lang_translations": {
                                "en": "He <was imprisoned> innocently for years before the truth came out."
                            }
                        }
                    ],
                    "common_phrases": [
                        "onschuldig vastzitten",
                        "vastzitten voor een misdrijf"
                    ],
                    "target_lang_definitions": {
                        "en": "To be locked up in a prison or cell as punishment or while awaiting trial."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be detained",
                                "target_lang_sense_clarification": "held in custody"
                            },
                            {
                                "target_lang_word": "be in jail",
                                "target_lang_sense_clarification": "imprisoned"
                            },
                            {
                                "target_lang_word": "be locked up",
                                "target_lang_sense_clarification": "confined"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
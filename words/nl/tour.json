    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fed00626-2700-3c6f-9305-7ec8248b2d3d",
                    "sense_definition": "Een rondreis of tocht langs verschillende plaatsen, vaak met een specifiek doel zoals optredens of vakantie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "trip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bekende rockband gaat volgende maand op <w>tour</w> door Europa.",
                            "target_lang_translations": {
                                "ru": "Известная рок-группа в следующем месяце отправляется в <w>турне</w> по Европе."
                            }
                        },
                        {
                            "text": "We maken een uitgebreide <w>tour</w> langs alle historische monumenten in de stad.",
                            "target_lang_translations": {
                                "ru": "Мы совершаем обширную <w>экскурсию</w> по всем историческим памятникам города."
                            }
                        }
                    ],
                    "synonyms": [
                        "rondreis"
                    ],
                    "common_phrases": [
                        "op tour gaan",
                        "een tour maken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путешествие или поездка по нескольким местам, часто с определенной целью, например, гастроли или отпуск."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тур",
                                "target_lang_sense_clarification": "поездка"
                            },
                            {
                                "target_lang_word": "турне",
                                "target_lang_sense_clarification": "гастроли"
                            },
                            {
                                "target_lang_word": "экскурсия",
                                "target_lang_sense_clarification": "осмотр достопримечательностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d12db6d7-7069-391f-b571-c1af20c60e3c",
                    "sense_definition": "De jaarlijkse wielerwedstrijd door Frankrijk (Tour de France).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cycling",
                    "name_type": "event_name",
                    "examples": [
                        {
                            "text": "Miljoenen mensen kijken in de zomer naar de <w>tour</w> op televisie.",
                            "target_lang_translations": {
                                "ru": "Миллионы людей смотрят <w>Тур</w> по телевизору летом."
                            }
                        },
                        {
                            "text": "De wielrenner hoopt dit jaar de <w>tour</w> te winnen.",
                            "target_lang_translations": {
                                "ru": "Велогонщик надеется выиграть <w>Тур</w> в этом году."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de Tour winnen",
                        "in de Tour"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ежегодная велогонка по Франции (Тур де Франс)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Тур",
                                "target_lang_sense_clarification": "велогонка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "81676507-971d-3a94-8f43-15dc61658edd",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>touren</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>tour</w> graag met de auto door de bergen tijdens mijn vakantie.",
                            "target_lang_translations": {
                                "ru": "Я люблю <w>путешествовать</w> на машине по горам во время отпуска."
                            }
                        },
                        {
                            "text": "Als muzikant <w>tour</w> ik het hele jaar door langs kleine theaters.",
                            "target_lang_translations": {
                                "ru": "Как музыкант, я весь год <w>гастролирую</w> по небольшим театрам."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола \"touren\" (путешествовать, гастролировать): 1-е лицо единственного числа настоящего времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путешествую",
                                "target_lang_sense_clarification": "езжу"
                            },
                            {
                                "target_lang_word": "гастролирую",

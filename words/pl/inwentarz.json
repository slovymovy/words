            "senses": [
                {
                    "sense_id": "02b24055-b2f8-3859-800b-9e541efd8c66",
                    "sense_definition": "Szczegółowy spis wszystkich przedmiotów, towarów lub składników majątku znajdujących się w danym miejscu",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierownik magazynu musi podpisać sporządzony wczoraj <inwentarz>.",
                            "target_lang_translations": {
                                "ru": "Заведующий складом должен подписать составленную вчера <w>опись</w>."
                            }
                        },
                        {
                            "text": "Każdy mebel w biurze ma naklejony numer <inwentarza>.",
                            "target_lang_translations": {
                                "ru": "На каждой мебели в офисе наклеен номер <w>инвентаря</w>."
                            }

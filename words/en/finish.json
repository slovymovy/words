                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завершённость",
                                "target_lang_sense_clarification": "целостность"
                            },
                            {
                                "target_lang_word": "отточенность",
                                "target_lang_sense_clarification": "совершенство исполнения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

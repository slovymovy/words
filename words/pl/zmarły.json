            "senses": [
                {
                    "sense_id": "c7fbf74f-e2de-36bb-9b08-09e0b463edb7",
                    "sense_definition": "o człowieku, który nie żyje",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "dead_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wspominamy naszego <w>zmarłego</w> nauczyciela z wielkim szacunkiem i wdzięcznością.",
                            "target_lang_translations": {
                                "ru": "Мы вспоминаем нашего <w>покойного</w> учителя с большим уважением и благодарностью."
                            }
                        },
                        {
                            "text": "Jej <w>zmarły</w> mąż był znanym i cenionym w mieście lekarzem.",
                            "target_lang_translations": {
                                "ru": "Её <w>покойный</w> муж был известным и ценимым в городе врачом."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieżyjący",
                        "umarły",
                        "nieżywy"
                    ],
                    "antonyms": [
                        "żywy",
                        "żyjący"
                    ],
                    "common_phrases": [
                        "zmarły mąż",
                        "zmarła żona",
                        "zmarli rodzice"
                    ],
                    "target_lang_definitions": {
                        "ru": "о человеке, который ушёл из жизни"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умерший",
                                "target_lang_sense_clarification": "скончавшийся, переставший жить"
                            },
                            {
                                "target_lang_word": "покойный",
                                "target_lang_sense_clarification": "об умершем человеке (часто с оттенком уважения или о родственниках)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9f570e0-e275-3efe-84c7-f33e191fa53f",
                    "sense_definition": "o zwierzęciu, które nie żyje (używane zazwyczaj w kontekście emocjonalnym)",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci płakały po swoim <w>zmarłym</w> psie przez kilka dni.",
                            "target_lang_translations": {
                                "ru": "Дети плакали по своей <w>умершей</w> собаке несколько дней."
                            }
                        },
                        {
                            "text": "W ogrodzie pochowano <w>zmarłego</w> kota, który był ulubieńcem całej rodziny.",
                            "target_lang_translations": {
                                "ru": "В саду похоронили <w>умершего</w> кота, который был любимцем всей семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieżywy"
                    ],
                    "antonyms": [
                        "żywy"
                    ],
                    "target_lang_definitions": {
                        "ru": "о животном, которое умерло (обычно в эмоциональном контексте)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умерший",
                                "target_lang_sense_clarification": "о животном, воспринимаемом как член семьи"
                            },
                            {
                                "target_lang_word": "мёртвый",
                                "target_lang_sense_clarification": "неживой (биологическое состояние)"
                            }
                        ]
                    }
                }
            ]
        },

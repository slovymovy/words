    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ef530c96-58f9-3d98-b490-90e1e9ece01c",
                    "sense_definition": "Een vrouw die (vroeger) beroepsmatig voor een pasgeborene en de kraamvrouw zorgde.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>baker</w> hielp de jonge moeder met de verzorging van de pasgeborene.",
                            "target_lang_translations": {
                                "ru": "<w>Няня</w> помогала молодой матери ухаживать за новорождённым."
                            }
                        },
                        {
                            "text": "Vroeger was de <w>baker</w> een vertrouwde verschijning in het kraambed.",
                            "target_lang_translations": {
                                "ru": "Раньше <w>няня</w> была привычным явлением у постели роженицы."
                            }
                        }
                    ],
                    "synonyms": [
                        "kraamverzorgster"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historisch beroep"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, которая (раньше) профессионально ухаживала за новорождённым и роженицей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "няня",
                                "target_lang_sense_clarification": "для новорождённого (устар.)"
                            },
                            {
                                "target_lang_word": "сиделка",
                                "target_lang_sense_clarification": "при роженице"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a2678732-5b27-3ad4-9d4d-8f00fd079e2b",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>bakeren</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>baker</w> mijn dochtertje in voordat ze gaat slapen.",
                            "target_lang_translations": {
                                "ru": "Я <w>пеленаю</w> свою дочку перед тем, как она ложится спать."
                            }
                        },
                        {
                            "text": "Elke avond <w>baker</w> ik de baby opnieuw in doeken.",
                            "target_lang_translations": {
                                "ru": "Каждый вечер я снова <w>пеленаю</w> ребёнка в пелёнки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма 1-го лица единственного числа настоящего времени глагола \"bakeren\" (пеленать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пеленаю",
                                "target_lang_sense_clarification": "форма глагола пеленать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f73bb52-95ba-32be-b118-d7b200258579",
                    "sense_definition": "Gebiedende wijs van <w>bakeren</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Baker</w> de baby niet te strak in.",
                            "target_lang_translations": {
                                "ru": "Не <w>пеленай</w> ребёнка слишком туго."
                            }
                        },
                        {
                            "text": "<w>Baker</w> hem voorzichtig in warme doeken.",
                            "target_lang_translations": {
                                "ru": "Осторожно <w>запеленай</w> его в тёплые пелёнки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола \"bakeren\" (пеленать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пеленай",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "626f5b38-cde6-3685-9758-5f919bf6fff2",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>bakeren</w> (bij inversie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Baker</w> je de baby nog steeds elke nacht?",
                            "target_lang_translations": {
                                "ru": "Ты всё ещё <w>пеленаешь</w> ребёнка каждую ночь?"
                            }
                        },
                        {
                            "text": "Waarom <w>baker</w> jij het kind niet in?",
                            "target_lang_translations": {
                                "ru": "Почему ты не <w>пеленаешь</w> ребёнка?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма 2-го лица единственного числа настоящего времени глагола \"bakeren\" (пеленать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пеленаешь",
                                "target_lang_sense_clarification": "форма глагола пеленать"
                            }
                        ]
                    }
                }
            ]
        }

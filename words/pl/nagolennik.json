{
    "word_family": [
        "goleń",
        "goleniowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9395eb02-13d1-39cd-80b8-876240cfd8d7",
                    "sense_definition": "Ochraniacz noszony na przedniej części podudzia, używany w sporcie, zwłaszcza w piłce nożnej, do ochrony przed urazami.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy zawodnik musi mieć założone <w>nagolenniki</w> podczas oficjalnego meczu.",
                            "target_lang_translations": {
                                "ru": "Каждый игрок должен надеть <w>щитки</w> во время официального матча."
                            }
                        },
                        {
                            "text": "Sędzia nie dopuścił go do gry, ponieważ zapomniał zabrać <w>nagolenniki</w> z szatni.",
                            "target_lang_translations": {
                                "ru": "Судья не допустил его к игре, так как он забыл взять <w>щитки</w> из раздевалки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piłkarskie nagolenniki",
                        "założyć nagolenniki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защитное приспособление, надеваемое на переднюю часть голени, используемое в спорте, особенно в футболе, для защиты от травм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щиток",
                                "target_lang_sense_clarification": "спортивный"
                            },
                            {
                                "target_lang_word": "наголенник",
                                "target_lang_sense_clarification": "защитная экипировка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebff006a-4e24-3b50-969c-24d1b63982f4",
                    "sense_definition": "Część zbroi osłaniająca goleń, używana dawniej przez wojowników i rycerzy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz założył metalowy <w>nagolennik</w>, aby chronić nogi przed ciosami miecza.",
                            "target_lang_translations": {
                                "ru": "Рыцарь надел металлический <w>наголенник</w>, чтобы защитить ноги от ударов меча."
                            }
                        },
                        {
                            "text": "Wykopaliska archeologiczne ujawniły starożytne <w>nagolenniki</w> wykonane z brązu.",
                            "target_lang_translations": {
                                "ru": "Археологические раскопки обнаружили древние <w>поножи</w>, сделанные из бронзы."
                            }
                        }
                    ],
                    "synonyms": [
                        "nagolenica"
                    ],
                    "common_phrases": [
                        "stalowy nagolennik",
                        "skórzane nagolenniki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть доспеха, закрывающая голень, использовавшаяся в прошлом воинами и рыцарями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поножи",
                                "target_lang_sense_clarification": "часть доспехов"
                            },
                            {
                                "target_lang_word": "наголенник",
                                "target_lang_sense_clarification": "элемент брони"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dd3480d4-cd93-3c84-9330-fa3db10d1832",
                    "sense_definition": "A particular length or portion of time.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The offer is available for a limited <w>period</w> only.",
                            "target_lang_translations": {
                                "ru": "Предложение действительно только в течение ограниченного <w>периода</w>."
                            }
                        },
                        {
                            "text": "We are expecting a <w>period</w> of cold weather this week.",
                            "target_lang_translations": {
                                "ru": "На этой неделе мы ожидаем <w>период</w> холодной погоды."
                            }
                        }
                    ],
                    "synonyms": [
                        "time",
                        "interval",
                        "duration",
                        "span",
                        "space"
                    ],
                    "common_phrases": [
                        "a short period",
                        "a long period",
                        "over a period",
                        "period of time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отрезок или промежуток времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пери́од",
                                "target_lang_sense_clarification": "время"
                            },
                            {
                                "target_lang_word": "промежу́ток вре́мени",
                                "target_lang_sense_clarification": "интервал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6ed35ae-fe0e-325b-91d6-4e186a4dadc9",
                    "sense_definition": "The punctuation mark “.” used at the end of a sentence or to mark an abbreviation.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "punctuation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't forget to put a <w>period</w> at the end of every sentence.",
                            "target_lang_translations": {
                                "ru": "Не забудь поставить <w>точку</w> в конце каждого предложения."
                            }
                        },
                        {
                            "text": "In the email address, there is a <w>period</w> between his first and last name.",
                            "target_lang_translations": {
                                "ru": "В адресе электронной почты между его именем и фамилией стоит <w>точка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "full stop",
                        "point",
                        "dot"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "North America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Знак препинания «.», используемый в конце предложения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "то́чка",
                                "target_lang_sense_clarification": "знак препинания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "087b538f-718b-3cf5-8c05-40161fbe0284",
                    "sense_definition": "A length of time in history characterized by specific events, conditions, or culture.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The painting dates back to the Victorian <w>period</w>.",
                            "target_lang_translations": {
                                "ru": "Картина относится к викторианскому <w>периоду</w>."
                            }
                        },
                        {
                            "text": "Food rationing continued in the post-war <w>period</w>.",
                            "target_lang_translations": {
                                "ru": "Нормирование продовольствия продолжалось в послевоенный <w>период</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "end of discussion",
                        "that's that",
                        "full stop"
                    ],
                    "common_phrases": [
                        "period of history",
                        "medieval period",
                        "colonial period"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отрезок времени в истории, характеризующийся определенными событиями или условиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пери́од",
                                "target_lang_sense_clarification": "исторический"
                            },
                            {
                                "target_lang_word": "эпо́ха",
                                "target_lang_sense_clarification": "эра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3b38119-cd59-3af9-8963-20bd416d5ba2",
                    "sense_definition": "The monthly flow of blood from the uterus in women.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is feeling tired because she is on her <w>period</w>.",
                            "target_lang_translations": {
                                "ru": "Она чувствует усталость, потому что у нее <w>месячные</w>."
                            }
                        },
                        {
                            "text": "When she missed her <w>period</w>, she suspected she might be pregnant.",
                            "target_lang_translations": {
                                "ru": "Когда у нее была задержка <w>месячных</w>, она заподозрила, что может быть беременна."
                            }
                        }
                    ],
                    "synonyms": [
                        "menstruation"
                    ],
                    "common_phrases": [
                        "on her period",
                        "miss a period"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ежемесячное кровотечение из матки у женщин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ме́сячные",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "менструа́ция",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3d6f112-806e-3631-bf4c-7af323f95eac",
                    "sense_definition": "One of the fixed divisions of time in a school day allocated to a specific subject.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "school",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have a math test during the second <w>period</w>.",
                            "target_lang_translations": {
                                "ru": "У меня контрольная по математике на втором <w>уроке</w>."
                            }
                        },
                        {
                            "text": "The students have a free <w>period</w> after lunch.",
                            "target_lang_translations": {
                                "ru": "У студентов есть свободный <w>урок</w> после обеда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "first period",
                        "free period",
                        "lunch period"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть учебного дня в школе, отведенная под определенный предмет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уро́к",
                                "target_lang_sense_clarification": "занятие"
                            }
                        ]

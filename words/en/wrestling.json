    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "576efb26-97da-313d-be2d-9b958d4786f7",
                    "sense_definition": "A sport where two opponents attempt to subdue each other in bare-handed grappling using techniques of leverage, holding, and pressure points.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sport_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He joined the school <w>wrestling</w> team to improve his strength.",
                            "target_lang_translations": {
                                "ru": "Он записался в школьную команду по <w>борьбе</w>, чтобы стать сильнее."
                            }
                        },
                        {
                            "text": "Olympic <w>wrestling</w> has very strict rules regarding holds and throws.",
                            "target_lang_translations": {
                                "ru": "В олимпийской <w>борьбе</w> действуют очень строгие правила касательно захватов и бросков."
                            }
                        }
                    ],
                    "synonyms": [
                        "grappling"
                    ],
                    "common_phrases": [
                        "freestyle wrestling",
                        "Greco-Roman wrestling",
                        "arm wrestling",
                        "wrestling match"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид спорта, в котором два соперника пытаются подчинить друг друга в рукопашной схватке, используя приемы рычага, удержания и воздействия на болевые точки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "борьба",
                                "target_lang_sense_clarification": "вид спорта"
                            },
                            {
                                "target_lang_word": "спортивная борьба",
                                "target_lang_sense_clarification": "дисциплина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "606ac140-cb0e-3ea2-80a4-c44a091cd070",
                    "sense_definition": "Professional wrestling; a performance art that emulates the sport of wrestling.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sport_sense",
                    "name_type": "no",
                    "examples": [
                        {

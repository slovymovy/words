                    "common_phrases": [
                        "bunch of flowers",
                        "bouquet of flowers",
                        "fresh flowers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стебель цветущего растения с бутоном или соцветием, используемый для украшения или в качестве подарка."
                    },

                },
                {
                    "sense_id": "460b9dbc-9c82-3f1d-a238-348f27c9f74a",
                    "sense_definition": "To tighten the eyebrows to show concentration, worry, or anger.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>knitted</w> his brows as he tried to solve the difficult puzzle.",
                            "target_lang_translations": {
                                "ru": "Он <w>нахмурил</w> брови, пытаясь решить сложную головоломку."
                            }
                        },
                        {
                            "text": "She <w>knitted</w> her forehead in deep thought while reading the letter.",
                            "target_lang_translations": {
                                "ru": "Она <w>наморщила</w> лоб в глубокой задумчивости, читая письмо."
                            }
                        }
                    ],
                    "synonyms": [
                        "furrow",
                        "frown",
                        "wrinkle"
                    ],
                    "antonyms": [
                        "smooth",
                        "relax"
                    ],
                    "common_phrases": [
                        "knit one's brows",
                        "knit one's forehead"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сдвигать брови, выражая сосредоточенность, беспокойство или гнев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хмурить",
                                "target_lang_sense_clarification": "брови"
                            },
                            {
                                "target_lang_word": "сдвигать",
                                "target_lang_sense_clarification": "брови"
                            },
                            {
                                "target_lang_word": "морщить",
                                "target_lang_sense_clarification": "лоб"
                            }
                        ]
                    }
                }
            ]
        },
        {

                        "nondisclosed"
                    ],
                    "common_phrases": [
                        "disclosed information",
                        "disclosed amount",
                        "fully disclosed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный известным публично или признанный; не держащийся в секрете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрытый",
                                "target_lang_sense_clarification": "ставший известным"
                            },
                            {
                                "target_lang_word": "обнародованный",
                                "target_lang_sense_clarification": "опубликованный официально"
                            }
                        ]
                    }
                },
                {

                {
                    "sense_id": "dbd826c2-6ec4-3256-994c-21cc11773aa6",
                    "sense_definition": "A commercial establishment, resort, or health club offering health and beauty treatments such as steam baths, massages, and facials.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "wellness_facilities",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She spent the entire afternoon at the <w>spa</w> enjoying a relaxing massage.",
                            "target_lang_translations": {
                                "ru": "Она провела весь день в <w>спа</w>, наслаждаясь расслабляющим массажем."
                            }
                        },
                        {
                            "text": "We booked a weekend at a luxury <w>spa</w> to recover from the stress of work.",
                            "target_lang_translations": {
                                "ru": "Мы забронировали выходные в роскошном <w>спа-отеле</w>, чтобы восстановиться после рабочего стресса."
                            }
                        }
                    ],
                    "synonyms": [
                        "health club",
                        "resort"
                    ],
                    "common_phrases": [
                        "day spa",
                        "spa treatment",
                        "luxury spa",
                        "spa hotel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Коммерческое учреждение, курорт или оздоровительный клуб, предлагающий процедуры для здоровья и красоты, такие как паровые бани, массаж и уход за лицом."

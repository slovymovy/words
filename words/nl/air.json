                {
                    "sense_id": "638f6162-2f6f-3198-bcaf-cfec4403d782",
                    "sense_definition": "Een arrogante of gemaakte houding waarmee iemand probeert indruk te maken of zich belangrijker voordoet dan hij is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij liep met een verwaande <w>air</w> de vergaderzaal binnen.",
                            "target_lang_translations": {
                                "ru": "Он вошел в зал заседаний с надменным <w>видом</w>."
                            }
                        },
                        {
                            "text": "Zij heeft altijd zo'n <w>air</w> alsof ze alles beter weet dan de rest.",
                            "target_lang_translations": {
                                "ru": "У нее всегда такой <w>вид</w>, словно она знает все лучше других."
                            }
                        }
                    ],
                    "synonyms": [
                        "kapsones",
                        "verwaandheid",
                        "arrogantie",
                        "hooghartigheid"
                    ],
                    "antonyms": [
                        "bescheidenheid",
                        "nederigheid",
                        "eenvoud"
                    ],
                    "common_phrases": [
                        "met een air",
                        "zich een air aanmeten",
                        "een air hebben"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4bbfef4e-444d-3856-916e-6aea096e8011",
                    "sense_definition": "Давать часть того, что имеешь, другим людям для совместного использования.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "share_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Брат всегда <делится> своими игрушками с младшей сестрой.",
                            "target_lang_translations": {
                                "en": "The brother always <shares> his toys with his younger sister."
                            }
                        },
                        {
                            "text": "Мы привыкли <делиться> едой во время похода.",
                            "target_lang_translations": {
                                "en": "We are used to <sharing> food during the hike."
                            }
                        }
                    ],
                    "common_phrases": [
                        "делиться едой",
                        "делиться всем",
                        "делиться деньгами"
                    ],
                    "target_lang_definitions": {
                        "en": "To give a portion of what one has to others for joint use or consumption."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "share",
                                "target_lang_sense_clarification": "possessions or resources"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fe23e1b-f64a-3d7b-bf7c-c2384bd8ca63",
                    "sense_definition": "Быть кратным какому-либо числу, то есть давать целое число при делении (в математике).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "division_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Число десять <делится> на пять без остатка.",
                            "target_lang_translations": {
                                "en": "The number ten <is divisible> by five without a remainder."
                            }
                        },
                        {
                            "text": "Это сложное число не <делится> на три.",
                            "target_lang_translations": {
                                "en": "This complex number <is not divisible> by three."
                            }
                        }
                    ],
                    "common_phrases": [
                        "делиться на два",
                        "делиться без остатка",
                        "делиться на три"
                    ],
                    "target_lang_definitions": {
                        "en": "To be a multiple of a number; to result in a whole number when divided (in mathematics)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be divisible",
                                "target_lang_sense_clarification": "mathematics"
                            },
                            {
                                "target_lang_word": "divide",
                                "target_lang_sense_clarification": "mathematics, intransitive"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47ff507f-3699-3fef-a5b9-dc840b4420cc",
                    "sense_definition": "Рассказывать кому-либо о своих мыслях, чувствах, планах или новостях.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "share_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она любит <делиться> впечатлениями после каждого путешествия.",
                            "target_lang_translations": {
                                "en": "She loves <sharing> her impressions after every trip."
                            }
                        },
                        {
                            "text": "Коллеги часто <делятся> опытом друг с другом.",
                            "target_lang_translations": {
                                "en": "Colleagues often <share> experience with each other."
                            }
                        }
                    ],
                    "synonyms": [
                        "рассказывать",
                        "сообщать"
                    ],
                    "common_phrases": [
                        "делиться впечатлениями",
                        "делиться опытом",
                        "делиться новостями",
                        "делиться секретами"
                    ],
                    "target_lang_definitions": {
                        "en": "To tell someone about one's thoughts, feelings, plans, or news."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "share",
                                "target_lang_sense_clarification": "thoughts or feelings"
                            },
                            {
                                "target_lang_word": "confide",
                                "target_lang_sense_clarification": "secrets or personal matters"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7eb7e0c9-7b5b-377c-8664-4aac81718e7c",
                    "sense_definition": "Распадаться на отдельные части или группы; разъединяться.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "division_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Большая река <делится> на несколько маленьких рукавов.",
                            "target_lang_translations": {
                                "en": "The large river <splits> into several small branches."
                            }
                        },
                        {
                            "text": "Класс <делится> на две группы для урока английского языка.",
                            "target_lang_translations": {
                                "en": "The class <divides> into two groups for the English lesson."
                            }
                        }
                    ],
                    "synonyms": [
                        "разделяться",
                        "распадаться",
                        "разъединяться"
                    ],
                    "antonyms": [
                        "сохраняться"
                    ],
                    "common_phrases": [
                        "делиться на группы",
                        "делиться на части",
                        "делиться на лагеря"
                    ],
                    "target_lang_definitions": {
                        "en": "To break apart into separate parts or groups; to separate."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "divide",
                                "target_lang_sense_clarification": "into parts or groups"
                            },
                            {
                                "target_lang_word": "split",
                                "target_lang_sense_clarification": "separate or branch off"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
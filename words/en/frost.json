                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ма́товый фи́льтр",
                                "target_lang_sense_clarification": "для рассеивания света"
                            },
                            {
                                "target_lang_word": "фрост",
                                "target_lang_sense_clarification": "профессиональный жаргон"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ea20d2de-355a-3dbe-82e7-9fb9414b301f",
                    "sense_definition": "To cover a cake, cookie, or cupcake with a sweet, creamy mixture.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "food_prep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to <w>frost</w> the birthday cake with chocolate buttercream.",

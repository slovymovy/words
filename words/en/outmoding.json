    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c834fe2e-c4af-382f-8de7-4e7e1fb9ecba",
                    "sense_definition": "Present participle and gerund of <w>outmode</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "New technologies are rapidly <w>outmoding</w> the devices we use today.",
                            "target_lang_translations": {
                                "ru": "Новые технологии стремительно <w>делают устаревшими</w> устройства, которые мы используем сегодня."
                            }
                        },
                        {
                            "text": "The company was criticized for <w>outmoding</w> its products too quickly to force sales.",
                            "target_lang_translations": {
                                "ru": "Компанию критиковали за слишком быстрое <w>искусственное устаревание</w> своей продукции ради стимулирования продаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "superseding",
                        "dating",
                        "antiquating"
                    ],
                    "antonyms": [
                        "updating",
                        "modernizing",
                        "renewing"
                    ],
                    "common_phrases": [
                        "rapidly outmoding",
                        "process of outmoding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола outmode: делать что-либо старомодным, устаревшим или неактуальным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делать устаревшим",
                                "target_lang_sense_clarification": "приводить к потере актуальности"
                            },
                            {
                                "target_lang_word": "вытеснять",
                                "target_lang_sense_clarification": "заменять собой как более современным"
                            },
                            {
                                "target_lang_word": "лишать актуальности",
                                "target_lang_sense_clarification": "делать несоответствующим современным требованиям"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e36d1907-1f28-36a0-8cb0-5f05e92f0ce0",
                    "sense_definition": "The act or process of something becoming outmoded or obsolete.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rapid <w>outmoding</w> of computer hardware makes it difficult to stay current.",
                            "target_lang_translations": {
                                "ru": "Быстрое <w>устаревание</w> компьютерного оборудования затрудняет поддержание его актуальности."
                            }
                        },
                        {
                            "text": "Historians discussed the <w>outmoding</w> of feudal systems in Europe.",
                            "target_lang_translations": {
                                "ru": "Историки обсуждали <w>отмирание</w> феодальных систем в Европе."
                            }
                        }
                    ],
                    "synonyms": [
                        "obsolescence"
                    ],
                    "antonyms": [
                        "modernization",
                        "updating"
                    ],
                    "common_phrases": [
                        "rapid outmoding",
                        "outmoding of traditions"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт или процесс, в ходе которого что-либо становится устаревшим, выходит из моды или употребления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устаревание",
                                "target_lang_sense_clarification": "процесс потери современности или функциональности"
                            },
                            {
                                "target_lang_word": "выход из моды",
                                "target_lang_sense_clarification": "потеря популярности"
                            },
                            {
                                "target_lang_word": "отмирание",
                                "target_lang_sense_clarification": "постепенное исчезновение из практики (о традициях, системах)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
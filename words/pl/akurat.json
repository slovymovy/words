{
    "word_family": [
        "akuratnie",
        "akuratność"
    ],
    "entries": [
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "ec3b3bd3-6674-3930-b9af-c44068bd3fa8",
                    "sense_definition": "Służy do wskazania i wyróżnienia konkretnej osoby, rzeczy, miejsca lub czasu spośród innych; właśnie, dokładnie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emphasis_selection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dlaczego <akurat> ty musisz to robić?",
                            "target_lang_translations": {
                                "ru": "Почему <именно> ты должен это делать?"
                            }
                        },
                        {
                            "text": "Szukałem <akurat> tej książki w bibliotece.",
                            "target_lang_translations": {
                                "ru": "Я искал <как раз> эту книгу в библиотеке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dlaczego akurat ty",
                        "akurat ten",
                        "akurat wtedy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания и выделения конкретного лица, предмета, места или времени среди других; именно, точно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "именно",
                                "target_lang_sense_clarification": "выделение конкретного объекта"
                            },
                            {
                                "target_lang_word": "как раз",
                                "target_lang_sense_clarification": "уточнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79816277-771f-3fa8-97ec-d54915ac621a",
                    "sense_definition": "Służy do potwierdzenia dokładności informacji lub podkreślenia zbiegu okoliczności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emphasis_accuracy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam przy sobie <akurat> pięć złotych.",
                            "target_lang_translations": {
                                "ru": "У меня с собой <как раз> пять злотых."
                            }
                        },
                        {
                            "text": "Tak się <akurat> złożyło, że spotkałem go w sklepie.",
                            "target_lang_translations": {
                                "ru": "Так <как раз> сложилось, что я встретил его в магазине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tak się akurat składa",
                        "akurat tyle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служит для подтверждения точности информации или подчеркивания стечения обстоятельств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как раз",
                                "target_lang_sense_clarification": "совпадение обстоятельств"
                            },
                            {
                                "target_lang_word": "кстати",
                                "target_lang_sense_clarification": "уместность момента"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1f15f295-b09a-389f-b7a7-5822e6ee430d",
                    "sense_definition": "Właśnie w tym momencie; w chwili, o której mowa.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_moment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy zadzwoniłeś, <akurat> wychodziłem z domu.",
                            "target_lang_translations": {
                                "ru": "Когда ты позвонил, я <как раз> выходил из дома."
                            }
                        },
                        {
                            "text": "Nie mogę teraz rozmawiać, bo <akurat> jem obiad.",
                            "target_lang_translations": {
                                "ru": "Не могу сейчас говорить, потому что <как раз> обедаю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "akurat teraz",
                        "akurat dziś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Именно в этот момент; в то время, о котором идет речь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как раз",
                                "target_lang_sense_clarification": "в данный момент"
                            },
                            {
                                "target_lang_word": "именно",
                                "target_lang_sense_clarification": "точно тогда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfd08aad-233e-3823-9610-e53711bbd8f2",
                    "sense_definition": "W sam raz; pasujący idealnie pod względem rozmiaru, ilości lub właściwości.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "measure_fit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te buty są na mnie <akurat>.",
                            "target_lang_translations": {
                                "ru": "Эти туфли мне <в самый раз>."
                            }
                        },
                        {
                            "text": "Woda w jeziorze jest dzisiaj <akurat> do kąpieli.",
                            "target_lang_translations": {
                                "ru": "Вода в озере сегодня <в самый раз> для купания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jest akurat",
                        "leży akurat"
                    ],
                    "target_lang_definitions": {
                        "ru": "В самый раз; идеально подходящий по размеру, количеству или свойствам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в самый раз",
                                "target_lang_sense_clarification": "идеально подходит"
                            },
                            {
                                "target_lang_word": "впору",
                                "target_lang_sense_clarification": "о размере одежды/обуви"
                            },
                            {
                                "target_lang_word": "как раз",
                                "target_lang_sense_clarification": "подходит по размеру"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "f69380ea-da13-3cf8-a770-91089b91b960",
                    "sense_definition": "Wykrzyknik wyrażający powątpiewanie, niedowierzanie lub kategoryczną odmowę; ironiczne zaprzeczenie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "denial_irony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myślisz, że on ci pomoże? <Akurat>!",
                            "target_lang_translations": {
                                "ru": "Думаешь, он тебе поможет? <Как же>!"
                            }
                        },
                        {
                            "text": "– Pożyczysz mi stówę? – <Akurat>!",
                            "target_lang_translations": {
                                "ru": "– Одолжишь мне сотню? – <Ага, сейчас>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "ani mi się śni",
                        "jeszcze czego",
                        "nic z tego",
                        "nic z tych rzeczy",
                        "niedoczekanie",
                        "nie ma mowy",
                        "po moim trupie"
                    ],
                    "common_phrases": [
                        "ta akurat",
                        "jasne akurat"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, выражающее сомнение, недоверие или категорический отказ; ироническое отрицание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как же",
                                "target_lang_sense_clarification": "ироническое недоверие"
                            },
                            {
                                "target_lang_word": "ага",
                                "target_lang_sense_clarification": "сарказм"
                            },
                            {
                                "target_lang_word": "конечно",
                                "target_lang_sense_clarification": "ироническое согласие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

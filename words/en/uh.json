    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "75519b7e-e680-3678-a021-97b1fafe3fd1",
                    "sense_definition": "Used to express hesitation, uncertainty, or to indicate that one is thinking.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "hesitation_sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Uh</w>, I am not sure if I can come to the party."
                        },
                        {
                            "text": "<w>Uh</w>, who was that person you were talking to?"
                        }
                    ],
                    "synonyms": [
                        "um",
                        "er"
                    ],
                    "common_phrases": [
                        "uh, well",
                        "uh, wait"
                    ]
                },
                {
                    "sense_id": "a45109d3-f876-349f-8de2-db7c171ed916",
                    "sense_definition": "Used as a meaningless sound to fill a pause in conversation while deciding what to say next.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "hesitation_sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I would like to order, <w>uh</w>, a cheeseburger and fries."
                        },
                        {
                            "text": "<w>Uh</w>, let me check my calendar to see if I am free."
                        }
                    ],
                    "synonyms": [
                        "um",
                        "er"
                    ]
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7aaa222a-7ac2-30c8-80d4-d1e866aaefb1",
                    "sense_definition": "An instance of the hesitation sound \"uh\" used in speech.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nervous speaker filled his presentation with too many <w>uhs</w>."
                        },
                        {
                            "text": "She tried to speak clearly and avoid every unnecessary <w>uh</w>."
                        }
                    ]
                },
                {
                    "sense_id": "d006cf42-a487-30ae-9562-5894aadd933f",
                    "sense_definition": "An alternative form of <w>a</w> representing the fourth semiquaver of a beat in music counting.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musicians often count the rhythm of sixteenth notes as one, e, and, <w>uh</w>."
                        },
                        {
                            "text": "Make sure you play the note exactly on the <w>uh</w> of the second beat."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "alternative spelling"
                        }
                    ]
                }
            ]
        }

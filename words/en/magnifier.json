{
    "word_family": [
        "magnify",
        "magnification",
        "magnificent"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "59c318bc-2cf8-31cb-9198-b2905bd8a83e",
                    "sense_definition": "A device, usually consisting of a lens, used to make objects appear larger than they actually are.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She used a <w>magnifier</w> to examine the tiny details on the antique stamp.",
                            "target_lang_translations": {
                                "ru": "Она использовала <w>лупу</w>, чтобы рассмотреть мельчайшие детали на старинной марке."
                            }
                        },
                        {
                            "text": "The jeweler inspected the diamond ring through a powerful <w>magnifier</w>.",
                            "target_lang_translations": {
                                "ru": "Ювелир осмотрел кольцо с бриллиантом через мощную <w>лупу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "magnifying glass",
                        "lens",
                        "loupe"
                    ],
                    "common_phrases": [
                        "screen magnifier",
                        "handheld magnifier",
                        "pocket magnifier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство, обычно состоящее из линзы, используемое для визуального увеличения объектов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лупа",
                                "target_lang_sense_clarification": "оптический прибор"
                            },
                            {
                                "target_lang_word": "увеличительное стекло",
                                "target_lang_sense_clarification": "линза"
                            },
                            {
                                "target_lang_word": "увеличитель",
                                "target_lang_sense_clarification": "прибор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b599b584-7803-3d7e-8bce-0b3472fbc35c",
                    "sense_definition": "A person who praises or glorifies someone or something highly.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was known as a great <w>magnifier</w> of the king's virtues in his speeches.",
                            "target_lang_translations": {
                                "ru": "Он был известен как великий <w>восхвалитель</w> добродетелей короля в своих речах."
                            }
                        },
                        {
                            "text": "The poet acted as a <w>magnifier</w> of nature's beauty through his verses.",
                            "target_lang_translations": {
                                "ru": "Поэт выступал как <w>восхвалитель</w> красоты природы в своих стихах."
                            }
                        }
                    ],
                    "synonyms": [
                        "praiser",
                        "extoller"
                    ],
                    "antonyms": [
                        "detractor",
                        "critic"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Refers to the older sense of 'magnify' meaning to extol or praise."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который превозносит или прославляет кого-либо или что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восхвалитель",
                                "target_lang_sense_clarification": "тот, кто хвалит"
                            },
                            {
                                "target_lang_word": "превозноситель",
                                "target_lang_sense_clarification": "книжное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "6337fb27-ee4a-3860-8406-81cb9b41d761",
                    "sense_definition": "De hoeveelheid tijd die iets in beslag neemt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>lengte</w> van de film was iets te lang voor de kinderen.",
                            "target_lang_translations": {
                                "en": "The <w>length</w> of the movie was a bit too long for the children.",
                                "ru": "<w>Продолжительность</w> фильма была немного велика для детей."
                            }
                        },
                        {
                            "text": "We zullen elkaar tot in <w>lengte</w> van dagen blijven steunen.",
                            "target_lang_translations": {
                                "en": "We will continue to support each other for the <w>length</w> of days.",

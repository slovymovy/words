                    ],
                    "synonyms": [
                        "cleared",
                        "vacated",
                        "emptied"
                    ],
                    "antonyms": [
                        "nonevacuated"
                    ],
                    "common_phrases": [
                        "evacuated residents",
                        "evacuated area",
                        "evacuated zone"
                    ],
                    "target_lang_definitions": {
                        "ru": "О людях, вывезенных в безопасное место, или о территории, откуда вывезли людей."

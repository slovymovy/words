                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at all",
                                "target_lang_sense_clarification": "used with negatives"
                            },
                            {
                                "target_lang_word": "quite",
                                "target_lang_sense_clarification": "in the phrase 'not quite'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c09f3c9-ec47-32e4-b386-0b8187347a32",

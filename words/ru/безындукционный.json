    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ae5a18e9-243f-3288-9adf-e2b199a14927",
                    "sense_definition": "Не обладающий индуктивностью или имеющий пренебрежимо малую индуктивность (обычно об элементах электрических цепей).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "electronics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для точных измерений в схему был добавлен специальный <w>безындукционный</w> резистор.",
                            "target_lang_translations": {
                                "en": "For precise measurements, a special <w>non-inductive</w> resistor was added to the circuit."
                            }
                        },
                        {
                            "text": "<w>Безындукционная</w> намотка катушки позволяет минимизировать помехи на высоких частотах.",
                            "target_lang_translations": {
                                "en": "<w>Non-inductive</w> coil winding allows minimizing interference at high frequencies."
                            }
                        }
                    ],
                    "synonyms": [
                        "неиндуктивный"
                    ],
                    "antonyms": [
                        "индуктивный"
                    ],
                    "common_phrases": [
                        "безындукционный резистор",
                        "безындукционная намотка",
                        "безындукционный кабель",
                        "безындукционный шунт"
                    ],
                    "target_lang_definitions": {
                        "en": "Possessing no inductance or having negligibly small inductance (usually regarding electric circuit elements)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "non-inductive",
                                "target_lang_sense_clarification": "having negligible inductance"
                            },
                            {
                                "target_lang_word": "noninductive",
                                "target_lang_sense_clarification": "alternative spelling"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02a24fe7-571d-3b2c-be5d-6484ffdc41ba",
                    "sense_definition": "Не использующий явление электромагнитной индукции; не основанный на индукции.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physics_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "По принципу действия эти сложные механизмы делятся на индукционные и <w>безындукционные</w>.",
                            "target_lang_translations": {
                                "en": "According to their operating principle, these complex mechanisms are divided into inductive and <w>non-inductive</w> ones."
                            }
                        },
                        {
                            "text": "В лаборатории исследовали новый <w>безындукционный</w> метод генерации разряда.",
                            "target_lang_translations": {
                                "en": "The laboratory investigated a new <w>non-inductive</w> method of discharge generation."
                            }
                        }
                    ],
                    "antonyms": [
                        "индукционный"
                    ],
                    "common_phrases": [
                        "безындукционный разряд",
                        "безындукционный механизм",
                        "безындукционный нагрев"
                    ],
                    "target_lang_definitions": {
                        "en": "Not utilizing the phenomenon of electromagnetic induction; not based on induction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "non-inductive",
                                "target_lang_sense_clarification": "not based on induction"
                            },
                            {
                                "target_lang_word": "induction-free",
                                "target_lang_sense_clarification": "process description"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "5676407b-a5c3-3081-b08d-3489a3c8d990",
                    "sense_definition": "The plural reflexive form of <w>you</w>, used as the object when the subject is the same group of people.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should be proud of <w>yourselves</w> for finishing the difficult project on time.",
                            "target_lang_translations": {
                                "ru": "Вы должны гордиться <w>собой</w> за то, что закончили этот сложный проект вовремя."
                            }
                        },
                        {
                            "text": "Please come in and make <w>yourselves</w> at home in the living room.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проходите и чувствуйте <w>себя</w> как дома в гостиной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "enjoy yourselves",
                        "help yourselves",
                        "behave yourselves",
                        "amongst yourselves",
                        "make yourselves at home"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural reflexive of you"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвратная форма местоимения «вы» во множественном числе; используется, когда действие направлено на самих действующих лиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "возвратное местоимение"
                            },
                            {
                                "target_lang_word": "собой",
                                "target_lang_sense_clarification": "форма творительного падежа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0344f387-c2eb-3095-b0b8-be3f3c81a36e",
                    "sense_definition": "Used to emphasize the group of people being addressed.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You said it <w>yourselves</w> that we needed to change our strategy.",
                            "target_lang_translations": {
                                "ru": "Вы <w>сами</w> сказали, что нам нужно изменить стратегию."
                            }
                        },
                        {
                            "text": "If you <w>yourselves</w> do not believe in the plan, nobody else will follow you.",
                            "target_lang_translations": {
                                "ru": "Если вы <w>сами</w> не верите в этот план, никто другой за вами не пойдет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "you yourselves"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления значения подлежащего (группы людей), к которому обращаются."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сами",
                                "target_lang_sense_clarification": "усилительное местоимение (множественное число)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

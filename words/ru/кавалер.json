            "senses": [
                {
                    "sense_id": "8e134074-3dfe-33ab-8bca-95b0a4ed8e6b",
                    "sense_definition": "Мужчина, танцующий в паре с дамой.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_partner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дамы приглашают <w>кавалеров</w> на белый танец.",
                            "target_lang_translations": {
                                "en": "Ladies invite <w>gentlemen</w> for the ladies' choice dance."
                            }
                        },
                        {
                            "text": "Она осталась без <w>кавалера</w> во время вальса.",
                            "target_lang_translations": {
                                "en": "She was left without a <w>partner</w> during the waltz."
                            }
                        }
                    ],
                    "synonyms": [
                        "партнёр"
                    ],
                    "common_phrases": [

                },
                {
                    "sense_id": "dc66a121-715a-3ae8-bcc7-8cf96dcea06c",
                    "sense_definition": "De manier of hoek waaronder licht of straling op een oppervlak valt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "incidence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hoek van <w>inval</w> bepaalt hoe het licht wordt weerkaatst.",
                            "target_lang_translations": {
                                "ru": "Угол <w>падения</w> определяет, как отражается свет."
                            }
                        },
                        {
                            "text": "Bij loodrechte <w>inval</w> is de reflectie maximaal.",
                            "target_lang_translations": {
                                "ru": "При перпендикулярном <w>падении</w> отражение максимально."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hoek van inval"
                    ],
                    "target_lang_definitions": {

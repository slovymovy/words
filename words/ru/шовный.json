            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "80309d14-c8e8-3466-b742-434f26f84e17",
                    "sense_definition": "Предназначенный для сшивания краев раны или соединения тканей (обычно в медицине).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хирург использовал качественный <w>шовный</w> материал для закрытия раны.",
                            "target_lang_translations": {
                                "en": "The surgeon used high-quality <w>suture</w> material to close the wound."
                            }
                        },
                        {
                            "text": "Медсестра подготовила стерильные <w>шовные</w> инструменты перед началом операции.",
                            "target_lang_translations": {
                                "en": "The nurse prepared sterile <w>suturing</w> instruments before the operation started."
                            }
                        }
                    ],
                    "common_phrases": [
                        "шовный материал",
                        "шовная нить"
                    ],
                    "target_lang_definitions": {
                        "en": "Intended for stitching wound edges or connecting tissues (usually in medicine)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suture",
                                "target_lang_sense_clarification": "attributive use (e.g., suture material)"
                            },
                            {
                                "target_lang_word": "surgical",
                                "target_lang_sense_clarification": "related to stitching wounds"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f699713f-0610-37b3-95d2-dccfa305752f",
                    "sense_definition": "Относящийся к месту соединения деталей, имеющий вид шва или выполненный с помощью шва (в технике и строительстве).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Контактная <w>шовная</w> сварка обеспечивает высокую герметичность труб.",
                            "target_lang_translations": {
                                "en": "Resistance <w>seam</w> welding ensures high pipe tightness."
                            }
                        },
                        {
                            "text": "На металлической конструкции была видна грубая <w>шовная</w> полоса.",
                            "target_lang_translations": {
                                "en": "A rough <w>seam</w> strip was visible on the metal structure."
                            }
                        }
                    ],
                    "antonyms": [
                        "бесшовный"
                    ],
                    "common_phrases": [
                        "шовная сварка",
                        "шовное соединение"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the junction of parts, having the appearance of a seam, or made using a seam (in technology and construction)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seam",
                                "target_lang_sense_clarification": "attributive use (e.g., seam welding)"
                            },
                            {
                                "target_lang_word": "joint",
                                "target_lang_sense_clarification": "related to the connection of parts"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "28ce197c-89ef-3b37-bd98-73adb94f7661",
                    "sense_definition": "Отсутствие доказательств, логической обоснованности или убедительности (теории, аргумента, мнения).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В ходе дискуссии стала очевидна полная <w>несостоятельность</w> его аргументов.",
                            "target_lang_translations": {
                                "en": "During the discussion, the complete <w>untenability</w> of his arguments became obvious."
                            }
                        },
                        {
                            "text": "Учёный доказал научную <w>несостоятельность</w> этой устаревшей теории.",
                            "target_lang_translations": {
                                "en": "The scientist proved the scientific <w>unsoundness</w> of this outdated theory."
                            }
                        }
                    ],
                    "synonyms": [
                        "необоснованность",
                        "бездоказательность",
                        "безосновательность",
                        "неубедительность"
                    ],
                    "antonyms": [
                        "состоятельность",
                        "состоятельность",
                        "состоятельность"
                    ],
                    "common_phrases": [
                        "научная несостоятельность",
                        "полная несостоятельность",
                        "доказать несостоятельность",
                        "несостоятельность доводов"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "книжное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The lack of evidence, logical validity, or persuasiveness (of a theory, argument, or opinion)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "untenability",
                                "target_lang_sense_clarification": "incapability of being defended or maintained"
                            },
                            {
                                "target_lang_word": "groundlessness",
                                "target_lang_sense_clarification": "lack of foundation or evidence"
                            },
                            {
                                "target_lang_word": "unsoundness",
                                "target_lang_sense_clarification": "lack of logical validity or strength"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c4844b6-cdb2-34e9-b94e-52398153fcbb",
                    "sense_definition": "Официально признанная неспособность должника удовлетворить требования кредиторов и оплатить обязательные платежи.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "financial_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компания была вынуждена объявить о своей <w>несостоятельности</w> из-за огромных долгов.",
                            "target_lang_translations": {
                                "en": "The company was forced to declare its <w>insolvency</w> due to huge debts."
                            }
                        },
                        {
                            "text": "Суд рассматривает дело о финансовой <w>несостоятельности</w> крупного банка.",
                            "target_lang_translations": {
                                "en": "The court is considering the case regarding the financial <w>insolvency</w> of a large bank."
                            }
                        }
                    ],
                    "synonyms": [
                        "банкротство",
                        "неплатежеспособность"
                    ],
                    "antonyms": [
                        "состоятельность",
                        "состоятельность"
                    ],
                    "common_phrases": [
                        "объявить о несостоятельности",
                        "финансовая несостоятельность",
                        "признать несостоятельность",
                        "дело о несостоятельности"
                    ],
                    "target_lang_definitions": {
                        "en": "Officially recognized inability of a debtor to satisfy creditor claims and make mandatory payments."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "insolvency",
                                "target_lang_sense_clarification": "financial inability to pay debts"
                            },
                            {
                                "target_lang_word": "bankruptcy",
                                "target_lang_sense_clarification": "legal status of being unable to repay debts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1aaa92d-a09d-3f10-85bc-84bddec1e971",
                    "sense_definition": "Отсутствие материального достатка или богатства.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "wealth_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его материальная <w>несостоятельность</w> не позволяла ему часто путешествовать.",
                            "target_lang_translations": {
                                "en": "His material <w>lack of means</w> did not allow him to travel often."
                            }
                        },
                        {
                            "text": "Семья долго скрывала свою <w>несостоятельность</w> от богатых соседей.",
                            "target_lang_translations": {
                                "en": "The family hid their <w>poverty</w> from their rich neighbors for a long time."
                            }
                        }
                    ],
                    "synonyms": [
                        "бедность",
                        "нужда"
                    ],
                    "antonyms": [
                        "состоятельность"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "в современном языке чаще используется слово «бедность»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Lack of material wealth or prosperity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "poverty",
                                "target_lang_sense_clarification": "state of being poor"
                            },
                            {
                                "target_lang_word": "lack of means",
                                "target_lang_sense_clarification": "insufficient financial resources"
                            }
                        ]
                    }
                }
            ]
        }
    ]

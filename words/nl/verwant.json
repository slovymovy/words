    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f567a16a-be3c-34f6-95af-ed8f42ef2787",
                    "sense_definition": "Door familiebanden met elkaar verbonden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "family_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is nauw <w>verwant</w> aan de koninklijke familie.",
                            "target_lang_translations": {
                                "en": "She is closely <w>related</w> to the royal family.",
                                "ru": "Она состоит в близком <w>родстве</w> с королевской семьей."
                            }
                        },
                        {
                            "text": "Wij zijn in de verte aan elkaar <w>verwant</w>.",
                            "target_lang_translations": {
                                "en": "We are distantly <w>related</w> to each other.",
                                "ru": "Мы состоим в дальнем <w>родстве</w> друг с другом."
                            }
                        }
                    ],
                    "synonyms": [
                        "geparenteerd"
                    ],
                    "common_phrases": [
                        "nauw verwant",
                        "in de verte verwant"
                    ],
                    "target_lang_definitions": {
                        "en": "Connected to each other by family ties.",
                        "ru": "Связанный друг с другом семейными узами."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "related",
                                "target_lang_sense_clarification": "connected by blood or marriage"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "родственный",
                                "target_lang_sense_clarification": "связанный кровным родством"
                            },
                            {
                                "target_lang_word": "состоящий в родстве",
                                "target_lang_sense_clarification": "находящийся в родственных отношениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a827cc6-bd66-34e8-9358-070b2426e09e",
                    "sense_definition": "Een duidelijke overeenkomst of relatie vertonend in aard of uiterlijk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "general_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De twee schrijvers behandelen <w>verwante</w> thema's in hun boeken.",
                            "target_lang_translations": {
                                "en": "The two writers cover <w>related</w> themes in their books.",
                                "ru": "Два писателя рассматривают <w>родственные</w> темы в своих книгах."
                            }
                        },
                        {
                            "text": "Zij zochten naar <w>verwante</w> zielen om hun passie mee te delen.",
                            "target_lang_translations": {
                                "en": "They were looking for <w>kindred</w> spirits to share their passion with.",
                                "ru": "Они искали <w>родственные</w> души, чтобы разделить с ними свою страсть."
                            }
                        }
                    ],
                    "synonyms": [
                        "gelijksoortig",
                        "gelijkend"
                    ],
                    "common_phrases": [
                        "verwante zielen",
                        "verwante onderwerpen"
                    ],
                    "target_lang_definitions": {
                        "en": "Showing a clear similarity or relationship in nature or appearance.",
                        "ru": "Демонстрирующий явное сходство или связь по характеру или внешнему виду."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "related",
                                "target_lang_sense_clarification": "connected or similar in nature"
                            },
                            {
                                "target_lang_word": "kindred",
                                "target_lang_sense_clarification": "similar in spirit or character"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "родственный",
                                "target_lang_sense_clarification": "близкий по духу или характеру"
                            },
                            {
                                "target_lang_word": "схожий",
                                "target_lang_sense_clarification": "имеющий сходство"
                            },
                            {
                                "target_lang_word": "близкий",
                                "target_lang_sense_clarification": "похожий по сути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3f14723-e8e1-35b5-b880-01a76216751f",
                    "sense_definition": "Een gemeenschappelijke etymologische oorsprong hebbend.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ling_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze twee woorden zijn etymologisch <w>verwant</w>.",
                            "target_lang_translations": {
                                "en": "These two words are etymologically <w>related</w>.",
                                "ru": "Эти два слова этимологически <w>родственны</w>."
                            }
                        },
                        {
                            "text": "Het Nederlands is nauw <w>verwant</w> aan het Duits.",
                            "target_lang_translations": {
                                "en": "Dutch is closely <w>related</w> to German.",
                                "ru": "Нидерландский язык тесно <w>родственен</w> немецкому."
                            }
                        }
                    ],
                    "common_phrases": [
                        "etymologisch verwant"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a common etymological origin.",
                        "ru": "Имеющий общее этимологическое происхождение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "related",
                                "target_lang_sense_clarification": "belonging to the same language family"
                            },
                            {
                                "target_lang_word": "cognate",
                                "target_lang_sense_clarification": "descended from the same original word"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "родственный",
                                "target_lang_sense_clarification": "принадлежащий к той же языковой семье"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "105ff7dc-5362-3028-8e8d-0becbacc0f82",
                    "sense_definition": "Iemand met wie men door familiebanden verbonden is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "family_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Al zijn naaste <w>verwanten</w> waren aanwezig op het feest.",
                            "target_lang_translations": {
                                "en": "All his close <w>relatives</w> were present at the party.",
                                "ru": "Все его близкие <w>родственники</w> присутствовали на празднике."
                            }
                        },
                        {
                            "text": "Zij is een verre <w>verwante</w> van mijn moeder.",
                            "target_lang_translations": {
                                "en": "She is a distant <w>relative</w> of my mother.",
                                "ru": "Она дальняя <w>родственница</w> моей матери."
                            }
                        }
                    ],
                    "synonyms": [
                        "familielid",
                        "bloedverwant"
                    ],
                    "common_phrases": [
                        "naaste verwanten",
                        "verre verwanten"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone with whom one is connected by family ties.",
                        "ru": "Человек, с которым кто-либо связан семейными узами."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "relative",
                                "target_lang_sense_clarification": "a member of your family"
                            },
                            {
                                "target_lang_word": "relation",
                                "target_lang_sense_clarification": "a person who is related to you"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "родственник",
                                "target_lang_sense_clarification": "член семьи (мужчина)"
                            },
                            {
                                "target_lang_word": "родственница",
                                "target_lang_sense_clarification": "член семьи (женщина)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dae0d232-adc8-32b5-a135-3accd71df7e9",
                    "sense_definition": "Een woord dat dezelfde etymologische oorsprong heeft als een ander woord.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "ling_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De taalkundige zocht naar een <w>verwant</w> van dit woord in het Latijn.",
                            "target_lang_translations": {
                                "en": "The linguist looked for a <w>cognate</w> of this word in Latin.",
                                "ru": "Лингвист искал <w>родственное слово</w> для этого слова в латыни."
                            }
                        },
                        {
                            "text": "Dit Engelse woord is een <w>verwant</w> van een Duitse term.",
                            "target_lang_translations": {
                                "en": "This English word is a <w>cognate</w> of a German term.",
                                "ru": "Это английское слово — <w>когнат</w> немецкого термина."
                            }
                        }
                    ],
                    "synonyms": [
                        "cognaat"
                    ],
                    "target_lang_definitions": {
                        "en": "A word that has the same etymological origin as another word.",
                        "ru": "Слово, имеющее то же этимологическое происхождение, что и другое слово."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cognate",
                                "target_lang_sense_clarification": "linguistics term"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "родственное слово",
                                "target_lang_sense_clarification": "слово с общим корнем"
                            },
                            {
                                "target_lang_word": "когнат",
                                "target_lang_sense_clarification": "лингвистический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

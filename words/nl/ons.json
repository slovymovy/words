    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "5a07ceb4-1185-3146-b0f5-2af036106546",
                    "sense_definition": "De vorm van 'wij' wanneer het lijdend of meewerkend voorwerp is, of na een voorzetsel staat.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "wij_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft <w>ons</w> gisteren in de stad gezien.",
                            "target_lang_translations": {
                                "en": "He saw <w>us</w> in the city yesterday.",
                                "ru": "Он видел <w>нас</w> вчера в городе."
                            }
                        },
                        {
                            "text": "Wil je dat boek alsjeblieft aan <w>ons</w> geven?",
                            "target_lang_translations": {
                                "en": "Could you please give that book to <w>us</w>?",
                                "ru": "Ты не мог бы дать эту книгу <w>нам</w>?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "bij ons",
                        "met ons",
                        "voor ons",
                        "onder ons"
                    ],
                    "target_lang_definitions": {
                        "en": "The form of 'we' when it is the direct or indirect object, or when placed after a preposition.",
                        "ru": "Форма местоимения \"мы\", используемая в качестве прямого или косвенного дополнения, либо после предлога."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "us",
                                "target_lang_sense_clarification": "object pronoun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нас",
                                "target_lang_sense_clarification": "винительный/родительный падеж (кого? чего?)"
                            },
                            {
                                "target_lang_word": "нам",
                                "target_lang_sense_clarification": "дательный падеж (кому?)"
                            },
                            {
                                "target_lang_word": "нами",
                                "target_lang_sense_clarification": "творительный падеж (кем?)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b14d7e93-9822-3d9f-b411-f7168c5b9e5e",
                    "sense_definition": "Het wederkerend voornaamwoord dat verwijst naar 'wij' als onderwerp.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "wij_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij moeten <w>ons</w> haasten om de trein te halen.",
                            "target_lang_translations": {
                                "en": "We must hurry <w>ourselves</w> to catch the train.",
                                "ru": "Мы должны поторопить<w>ся</w>, чтобы успеть на поезд."
                            }
                        },
                        {
                            "text": "We hebben <w>ons</w> prima vermaakt op het feest.",
                            "target_lang_translations": {
                                "en": "We enjoyed <w>ourselves</w> just fine at the party.",
                                "ru": "Мы отлично повеселили<w>сь</w> на вечеринке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ons vergissen",
                        "ons wassen",
                        "ons verheugen"
                    ],
                    "target_lang_definitions": {
                        "en": "The reflexive pronoun referring to 'we' as the subject.",
                        "ru": "Возвратное местоимение, относящееся к подлежащему \"мы\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ourselves",
                                "target_lang_sense_clarification": "reflexive pronoun"
                            },
                            {
                                "target_lang_word": "us",
                                "target_lang_sense_clarification": "colloquial/dialect reflexive"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "возвратное местоимение"
                            },
                            {
                                "target_lang_word": "-ся",
                                "target_lang_sense_clarification": "возвратный суффикс глагола"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "cc3f937d-4be8-3018-9666-70dbfb98840d",
                    "sense_definition": "Bezittelijk voornaamwoord dat aangeeft dat iets van 'wij' is; gebruikt bij onzijdige woorden in het enkelvoud.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "wij_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is <w>ons</w> huis en wij wonen hier al jaren.",
                            "target_lang_translations": {
                                "en": "This is <w>our</w> house and we have lived here for years.",
                                "ru": "Это <w>наш</w> дом, и мы живем здесь уже много лет."
                            }
                        },
                        {
                            "text": "Het is <w>ons</w> doel om de wedstrijd te winnen.",
                            "target_lang_translations": {
                                "en": "It is <w>our</w> goal to win the match.",
                                "ru": "Это <w>наша</w> цель — выиграть матч."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ons huis",
                        "ons land",
                        "ons geluk"
                    ],
                    "target_lang_definitions": {
                        "en": "Possessive pronoun indicating that something belongs to 'us'; used with neuter singular nouns.",
                        "ru": "Притяжательное местоимение, указывающее на принадлежность \"нам\"; используется с существительными среднего рода (в голландском)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "our",
                                "target_lang_sense_clarification": "possessive determiner"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "наш",
                                "target_lang_sense_clarification": "мужской род"
                            },
                            {
                                "target_lang_word": "наше",
                                "target_lang_sense_clarification": "средний род"
                            },
                            {
                                "target_lang_word": "наша",
                                "target_lang_sense_clarification": "женский род"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a67f3d5e-5074-3179-8190-b267740c72b5",
                    "sense_definition": "Een gewichtsmaat van honderd gram.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "weight_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik van u drie <w>ons</w> jonge kaas?",
                            "target_lang_translations": {
                                "en": "May I have three <w>hundred grams</w> of young cheese from you?",
                                "ru": "Можно мне триста <w>грамм</w> молодого сыра?"
                            }
                        },
                        {
                            "text": "Voor dit recept heb je twee <w>ons</w> suiker nodig.",
                            "target_lang_translations": {
                                "en": "For this recipe you need two <w>hundred grams</w> of sugar.",
                                "ru": "Для этого рецепта тебе понадобится двести <w>грамм</w> сахара."
                            }
                        }
                    ],
                    "synonyms": [
                        "hectogram"
                    ],
                    "common_phrases": [
                        "een onsje meer",
                        "twee ons"
                    ],
                    "target_lang_definitions": {
                        "en": "A unit of weight equal to one hundred grams.",
                        "ru": "Мера веса, равная ста граммам."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "100 grams",
                                "target_lang_sense_clarification": "metric weight unit"
                            },
                            {
                                "target_lang_word": "hectogram",
                                "target_lang_sense_clarification": "formal measurement unit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сто грамм",
                                "target_lang_sense_clarification": "мера веса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87316a87-a3f5-3c6a-874e-8095ed11dc48",
                    "sense_definition": "Een historische gewichtsmaat met variërend gewicht, gebruikt vóór de invoering van het decimale stelsel.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "weight_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude apothekersboeken weegt een <w>ons</w> vaak minder dan honderd gram.",
                            "target_lang_translations": {
                                "en": "In old apothecary books an <w>ounce</w> often weighs less than a hundred grams.",
                                "ru": "В старых аптекарских книгах <w>унция</w> часто весит меньше ста грамм."
                            }
                        },
                        {
                            "text": "De handelaar gebruikte een oud <w>ons</w> als maatstaf.",
                            "target_lang_translations": {
                                "en": "The merchant used an old <w>ounce</w> as a standard.",
                                "ru": "Торговец использовал старую <w>унцию</w> в качестве меры."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historische maat"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A historical unit of weight with varying mass, used before the introduction of the decimal system.",
                        "ru": "Историческая мера веса с варьирующимся значением, использовавшаяся до введения метрической системы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ounce",
                                "target_lang_sense_clarification": "historical unit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "унция",
                                "target_lang_sense_clarification": "историческая мера"
                            }
                        ]
                    }
                }
            ]
        }

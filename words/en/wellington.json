    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "10c669c2-fb18-33fd-872a-8b601d3d7281",
                    "sense_definition": "The capital city of New Zealand.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "place_nz",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We are planning a trip to <w>Wellington</w> next summer.",
                            "target_lang_translations": {
                                "ru": "Мы планируем поездку в <w>Веллингтон</w> следующим летом."
                            }
                        },
                        {
                            "text": "The windy weather in <w>Wellington</w> is famous throughout the country.",
                            "target_lang_translations": {
                                "ru": "Ветреная погода в <w>Веллингтоне</w> известна по всей стране."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Столица Новой Зеландии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Веллингтон",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9dadc5b-afb9-3342-a1ad-ceaef8b2c156",
                    "sense_definition": "The Government of New Zealand (used metonymically).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "place_nz",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "Decisions made by <w>Wellington</w> have affected the rural economy.",
                            "target_lang_translations": {
                                "ru": "Решения, принятые <w>Веллингтоном</w>, повлияли на сельскую экономику."
                            }
                        },
                        {
                            "text": "The protesters marched to demand answers from <w>Wellington</w>.",
                            "target_lang_translations": {
                                "ru": "Протестующие вышли на марш, чтобы потребовать ответов от <w>Веллингтона</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Beehive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Правительство Новой Зеландии (используется метонимически)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Веллингтон",
                                "target_lang_sense_clarification": "правительство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98bd0fae-1b37-3140-b671-e6b3ac03f603",
                    "sense_definition": "Arthur Wellesley (1st Duke of Wellington), a British soldier and statesman.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "The Duke of <w>Wellington</w> is best known for defeating Napoleon at Waterloo.",
                            "target_lang_translations": {
                                "ru": "Герцог <w>Веллингтон</w> наиболее известен победой над Наполеоном при Ватерлоо."
                            }
                        },
                        {
                            "text": "Statues of <w>Wellington</w> can be found in many British cities.",
                            "target_lang_translations": {
                                "ru": "Статуи <w>Веллингтона</w> можно найти во многих британских городах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Duke of Wellington"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical figure"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Артур Уэлсли (1-й герцог Веллингтон), британский полководец и государственный деятель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Веллингтон",
                                "target_lang_sense_clarification": "историческая личность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42e098bb-88c0-32fa-8316-9119a6b63386",
                    "sense_definition": "A surname derived from place names in England.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Wellington</w> will see you in his office now.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Веллингтон</w> примет вас в своем кабинете сейчас."
                            }
                        },
                        {
                            "text": "She traced her family tree back to a <w>Wellington</w> in the 18th century.",
                            "target_lang_translations": {
                                "ru": "Она проследила свое генеалогическое древо до одного из <w>Веллингтонов</w>, жившего в XVIII веке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия английского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Веллингтон",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ce65448d-cd82-3895-b5e5-ee4c51887592",
                    "sense_definition": "A waterproof rubber boot, usually reaching below the knee.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "footwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should wear your <w>Wellingtons</w> if you are going to walk in the mud.",
                            "target_lang_translations": {
                                "ru": "Тебе следует надеть <w>резиновые сапоги</w>, если собираешься ходить по грязи."
                            }
                        },
                        {
                            "text": "He bought a new pair of green <w>Wellingtons</w> for gardening.",
                            "target_lang_translations": {
                                "ru": "Он купил новую пару зеленых <w>резиновых сапог</w> для работы в саду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pair of Wellingtons",
                        "wear Wellingtons"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Водонепроницаемые резиновые сапоги, обычно доходящие до колена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резиновые сапоги",
                                "target_lang_sense_clarification": "обувь"
                            },
                            {
                                "target_lang_word": "веллингтоны",
                                "target_lang_sense_clarification": "тип сапог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "542b3b2b-7342-3329-a034-39723fea4079",
                    "sense_definition": "A dish consisting of meat (usually beef) coated with pâté and wrapped in puff pastry.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We ordered the Beef <w>Wellington</w> for our anniversary dinner.",
                            "target_lang_translations": {
                                "ru": "Мы заказали говядину <w>Веллингтон</w> на наш юбилейный ужин."
                            }
                        },
                        {
                            "text": "Preparing a perfect <w>Wellington</w> takes a lot of culinary skill.",
                            "target_lang_translations": {
                                "ru": "Приготовление идеального <w>Веллингтона</w> требует большого кулинарного мастерства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Beef Wellington"
                    ],
                    "target_lang_definitions": {
                        "ru": "Блюдо из мяса (обычно говядины), покрытого паштетом и запеченного в слоеном тесте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Веллингтон",
                                "target_lang_sense_clarification": "кулинария"
                            }
                        ]
                    }
                }
            ]
        }

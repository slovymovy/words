                {
                    "sense_id": "55b807e2-38f5-3f82-89b0-56466ff433d9",
                    "sense_definition": "Een aanwijzing, bevel of voorschrift dat aangeeft hoe iets gedaan moet worden of hoe men moet handelen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "directive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lees de <w>instructies</w> in de handleiding goed door voordat je begint.",
                            "target_lang_translations": {
                                "en": "Read the <w>instructions</w> in the manual carefully before you start.",
                                "ru": "Внимательно прочитайте <w>инструкции</w> в руководстве, прежде чем начать."
                            }
                        },
                        {
                            "text": "De medewerkers kregen de <w>instructie</w> om het gebouw onmiddellijk te verlaten.",
                            "target_lang_translations": {
                                "en": "The employees received the <w>instruction</w> to leave the building immediately.",
                                "ru": "Сотрудники получили <w>указание</w> немедленно покинуть здание."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanwijzing",
                        "bevel",
                        "order",
                        "voorschrift"
                    ],
                    "common_phrases": [
                        "instructies opvolgen",
                        "instructies geven",
                        "volgens de instructies"
                    ],
                    "target_lang_definitions": {
                        "en": "A direction, order, or rule indicating how something must be done or how one must act.",
                        "ru": "Указание, приказ или предписание, определяющее, как что-либо должно быть сделано или как следует действовать."

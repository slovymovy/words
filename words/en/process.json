                },
                {
                    "sense_id": "6b0b696e-0479-3b6a-b5b9-5119cdbe3a62",
                    "sense_definition": "A summons or writ requiring a person to appear in court.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer argued that the legal <w>process</w> had not been served correctly.",

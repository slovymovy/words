                            {
                                "target_lang_word": "окружить",
                                "target_lang_sense_clarification": "результат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57366e01-9027-3dbd-a9aa-065d6888f844",
                    "sense_definition": "To sound or seem to have a particular quality, often used regarding truth or falsehood.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "seem_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His excuse did not <w>ring</w> true to the teacher.",
                            "target_lang_translations": {
                                "ru": "Его оправдание не <w>звучало</w> правдоподобно для учителя."
                            }
                        },
                        {
                            "text": "Her confident words <w>rang</w> with authority.",
                            "target_lang_translations": {
                                "ru": "В ее уверенных словах <w>звучала</w> властность."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ring true",
                        "ring hollow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звучать или казаться обладающим определенным качеством (часто о правде или лжи)."

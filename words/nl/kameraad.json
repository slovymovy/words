{
    "word_family": [
        "kameraadschap",
        "kameraadschappelijk"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "01b7d02c-a201-3e7f-8c1d-ba9b33e88e92",
                    "sense_definition": "Een goede vriend of maat met wie men een vriendschappelijke band heeft.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "friendship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samen met zijn <w>kameraad</w> is hij teruggekeerd naar de plek waar ze elkaar ontmoetten.",
                            "target_lang_translations": {
                                "en": "Together with his <w>buddy</w>, he returned to the place where they met.",
                                "ru": "Вместе со своим <w>товарищем</w> он вернулся на то место, где они встретились."
                            }
                        },
                        {
                            "text": "Mijn beste <w>kameraad</w> en ik gaan elke zaterdag samen sporten.",
                            "target_lang_translations": {
                                "en": "My best <w>pal</w> and I go play sports together every Saturday.",
                                "ru": "Мой лучший <w>приятель</w> и я каждую субботу вместе занимаемся спортом."
                            }
                        }
                    ],
                    "synonyms": [
                        "vriend",
                        "makker",
                        "maat"
                    ],
                    "antonyms": [
                        "vijand"
                    ],
                    "common_phrases": [
                        "goede kameraad",
                        "oude kameraad"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often implies a close, informal friendship"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A good friend or mate with whom one has a close, friendly bond.",
                        "ru": "Хороший друг или приятель, с которым человека связывают дружеские отношения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "buddy",
                                "target_lang_sense_clarification": "informal friend"
                            },
                            {
                                "target_lang_word": "pal",
                                "target_lang_sense_clarification": "close friend"
                            },
                            {
                                "target_lang_word": "mate",
                                "target_lang_sense_clarification": "friend (informal)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "товарищ",
                                "target_lang_sense_clarification": "друг, приятель"
                            },
                            {
                                "target_lang_word": "приятель",
                                "target_lang_sense_clarification": "близкий знакомый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "389aabb0-73b8-30c5-89f2-ee877eb26a4d",
                    "sense_definition": "Een medestrijder in een oorlog of een partijgenoot binnen een socialistische of communistische beweging.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "political_military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaat droeg zijn gewonde <w>kameraad</w> naar een veilige plek achter de linies.",
                            "target_lang_translations": {
                                "en": "The soldier carried his wounded <w>comrade</w> to a safe place behind the lines.",
                                "ru": "Солдат отнес своего раненого <w>товарища</w> в безопасное место за линией фронта."
                            }
                        },
                        {
                            "text": "Tijdens de partijbijeenkomst sprak de leider zijn <w>kameraden</w> toe met veel passie.",
                            "target_lang_translations": {
                                "en": "During the party meeting, the leader addressed his <w>comrades</w> with great passion.",
                                "ru": "Во время партийного собрания лидер с большой страстью обратился к своим <w>товарищам</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "medestrijder",
                        "partijgenoot"
                    ],
                    "antonyms": [
                        "tegenstander"
                    ],
                    "common_phrases": [
                        "beste kameraden"
                    ],
                    "target_lang_definitions": {
                        "en": "A fellow fighter in a war or a party member within a socialist or communist movement.",
                        "ru": "Боевой товарищ на войне или однопартиец в социалистическом или коммунистическом движении."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "comrade",
                                "target_lang_sense_clarification": "fellow soldier or political ally"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "товарищ",
                                "target_lang_sense_clarification": "обращение в партии или армии"
                            },
                            {
                                "target_lang_word": "соратник",
                                "target_lang_sense_clarification": "боевой товарищ, единомышленник"
                            }
                        ]
                    }
                }
            ]
        }
    ]

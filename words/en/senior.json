                    ],
                    "antonyms": [
                        "junior"
                    ],
                    "common_phrases": [
                        "senior management",
                        "senior partner",
                        "senior executive",
                        "senior official"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занимающий более высокое положение, ранг или имеющий больший авторитет в организации или профессии."
                    },
                    "translations": {

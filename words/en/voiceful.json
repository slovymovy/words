{
    "word_family": [
        "voice",
        "voiceless",
        "voicing"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5a8bdd95-61d1-3d01-aba8-ff5f45c5fd13",
                    "sense_definition": "Possessing a voice or producing a sound, often used to describe inanimate objects in a poetic way.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sound_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The poet sat by the <w>voiceful</w> sea and listened to the waves.",
                            "target_lang_translations": {
                                "ru": "Поэт сидел у <w>говорливого</w> моря и слушал волны."
                            }
                        },
                        {
                            "text": "She played a melody on the <w>voiceful</w> strings of her harp.",
                            "target_lang_translations": {
                                "ru": "Она играла мелодию на <w>звучных</w> струнах своей арфы."
                            }
                        }
                    ],
                    "synonyms": [
                        "vocal",
                        "sounding",
                        "resonant",
                        "sonorous"
                    ],
                    "antonyms": [
                        "voiceless",
                        "silent",
                        "mute"
                    ],
                    "common_phrases": [
                        "voiceful sea",
                        "voiceful stream"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий голосом или издающий звук (часто используется в поэтическом контексте для описания неодушевленных предметов)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звучный",
                                "target_lang_sense_clarification": "издающий громкий, чистый звук; резонирующий"
                            },
                            {
                                "target_lang_word": "говорливый",
                                "target_lang_sense_clarification": "издающий звуки, напоминающие речь; часто используется в поэзии о природе"
                            },
                            {
                                "target_lang_word": "голосистый",
                                "target_lang_sense_clarification": "обладающий сильным голосом или звуком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92440421-cf03-3a0f-bf21-3415d31fbf37",
                    "sense_definition": "Filled with loud sounds or the noise of many voices.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sound_volume",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>voiceful</w> crowd cheered loudly as the team entered the stadium.",
                            "target_lang_translations": {
                                "ru": "<w>Многоголосая</w> толпа громко приветствовала команду, когда та вышла на стадион."
                            }
                        },
                        {
                            "text": "A <w>voiceful</w> protest filled the city square with noise and energy.",
                            "target_lang_translations": {
                                "ru": "<w>Громкоголосый</w> протест наполнил городскую площадь шумом и энергией."
                            }
                        }
                    ],
                    "synonyms": [
                        "clamorous",
                        "vociferous",
                        "noisy"
                    ],
                    "antonyms": [
                        "quiet",
                        "silent"
                    ],
                    "common_phrases": [
                        "voiceful crowd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наполненный громкими звуками или шумом множества голосов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "многоголосый",
                                "target_lang_sense_clarification": "состоящий из множества голосов"
                            },
                            {
                                "target_lang_word": "шумный",
                                "target_lang_sense_clarification": "производящий много шума"
                            },
                            {
                                "target_lang_word": "громкоголосый",
                                "target_lang_sense_clarification": "характеризующийся громкими голосами"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
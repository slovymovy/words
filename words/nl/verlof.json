                {
                    "sense_id": "50961cce-c267-3a08-a676-071b694c47a5",
                    "sense_definition": "Een periode waarin een werknemer toestemming heeft om niet te werken, bijvoorbeeld wegens vakantie, persoonlijke omstandigheden of de geboorte van een kind",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "work_absence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn <w>verlof</w> begint op de eerste zomerdag.",
                            "target_lang_translations": {
                                "ru": "Мой <w>отпуск</w> начинается в первый день лета.",
                                "en": "My <w>leave</w> starts on the first day of summer."
                            }
                        },
                        {
                            "text": "Hij neemt onbetaald <w>verlof</w> om een lange reis te maken.",
                            "target_lang_translations": {
                                "ru": "Он берет неоплачиваемый <w>отпуск</w>, чтобы отправиться в долгое путешествие.",
                                "en": "He is taking unpaid <w>leave</w> to go on a long trip."
                            }
                        }
                    ],
                    "synonyms": [
                        "toestemming"
                    ],
                    "common_phrases": [
                        "met verlof gaan",
                        "onbetaald verlof",
                        "zwangerschapsverlof",
                        "ouderschapsverlof"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени, когда работнику разрешено не работать, например, из-за отпуска, личных обстоятельств или рождения ребенка",
                        "en": "A period of time when an employee is allowed to be absent from work, for example due to vacation, personal circumstances, or the birth of a child"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отпуск",
                                "target_lang_sense_clarification": "освобождение от работы"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "time off work"
                            },
                            {
                                "target_lang_word": "leave of absence",
                                "target_lang_sense_clarification": "formal period off"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22fcc06e-1c0e-3c4b-9192-15fdce3fd6e7",
                    "sense_definition": "Toestemming om tijdelijk afwezig te zijn van militaire dienst",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "work_absence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaat ging met <w>verlof</w> naar huis om zijn familie te bezoeken.",
                            "target_lang_translations": {
                                "ru": "Солдат отправился в <w>отпуск</w> домой, чтобы навестить семью.",
                                "en": "The soldier went home on <w>leave</w> to visit his family."
                            }
                        },
                        {
                            "text": "Na een kort <w>verlof</w> moest hij zich weer melden op de kazerne.",
                            "target_lang_translations": {
                                "ru": "После короткого <w>увольнения</w> он должен был снова явиться в казарму.",
                                "en": "After a short <w>furlough</w>, he had to report back to the barracks."
                            }
                        }
                    ],
                    "common_phrases": [
                        "met verlof zijn",
                        "groot verlof"
                    ],
                    "target_lang_definitions": {

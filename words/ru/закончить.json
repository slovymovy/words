    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "19768d43-9aaf-3233-8add-52dc1a145696",
                    "sense_definition": "Довести что-либо до конца, завершить выполнение какого-либо дела или действия.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Когда ты <закончишь> работу?",
                            "target_lang_translations": {
                                "en": "When will you <finish> work?"
                            }
                        },
                        {
                            "text": "Мы должны <закончить> этот проект до конца недели.",
                            "target_lang_translations": {
                                "en": "We must <finish> this project by the end of the week."
                            }
                        }
                    ],
                    "synonyms": [
                        "завершить",
                        "окончить",
                        "кончить",
                        "прекратить"
                    ],
                    "antonyms": [
                        "начать"
                    ],
                    "common_phrases": [
                        "закончить работу",
                        "закончить разговор",
                        "закончить дело",
                        "закончить ремонт"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring something to an end, to complete a task or action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "finish",
                                "target_lang_sense_clarification": "to bring a task or activity to an end"
                            },
                            {
                                "target_lang_word": "complete",
                                "target_lang_sense_clarification": "to finish making or doing something"
                            },
                            {
                                "target_lang_word": "end",
                                "target_lang_sense_clarification": "to bring to a conclusion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40f271e5-99e8-38f9-93f6-76420d99ce9a",
                    "sense_definition": "Завершить курс обучения в учебном заведении (школе, университете и т. п.).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <закончил> Московский государственный университет.",
                            "target_lang_translations": {
                                "en": "He <graduated> from Moscow State University."
                            }
                        },
                        {
                            "text": "Моя сестра успешно <закончила> школу в прошлом году.",
                            "target_lang_translations": {
                                "en": "My sister successfully <finished> school last year."
                            }

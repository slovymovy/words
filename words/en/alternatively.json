    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "319499aa-0bb8-3d91-8056-b7dae651576e",
                    "sense_definition": "Used to suggest another possibility or choice different from the one just mentioned.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "choice_transition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We could go to the cinema tonight, or <w>alternatively</w>, we could stay home and watch a movie.",
                            "target_lang_translations": {
                                "ru": "Мы могли бы пойти сегодня вечером в кино или, <w>как вариант</w>, остаться дома и посмотреть фильм."
                            }
                        },
                        {
                            "text": "You can take the bus to the station; <w>alternatively</w>, you might prefer to walk.",
                            "target_lang_translations": {
                                "ru": "Вы можете доехать до вокзала на автобусе; <w>в качестве альтернативы</w>, вы можете предпочесть прогуляться."
                            }
                        }
                    ],
                    "synonyms": [
                        "otherwise",
                        "instead",
                        "on the other hand"
                    ],
                    "common_phrases": [
                        "or alternatively"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы предложить другую возможность или выбор, отличный от только что упомянутого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как вариант",
                                "target_lang_sense_clarification": "вводное слово для предложения опции"
                            },
                            {
                                "target_lang_word": "в качестве альтернативы",
                                "target_lang_sense_clarification": "формальный вариант"
                            },
                            {
                                "target_lang_word": "или же",
                                "target_lang_sense_clarification": "союз для введения другого варианта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8061cbf8-6127-392b-9ea2-eeceac26fecb",
                    "sense_definition": "In a way that is different from the usual, traditional, or established way.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many young people are choosing to live <w>alternatively</w> to reduce their impact on the environment.",
                            "target_lang_translations": {
                                "ru": "Многие молодые люди выбирают жить <w>альтернативно</w>, чтобы снизить свое влияние на окружающую среду."
                            }
                        },
                        {
                            "text": "The teacher encouraged the students to think <w>alternatively</w> when solving the complex puzzle.",
                            "target_lang_translations": {
                                "ru": "Учитель призвал учеников мыслить <w>нестандартно</w> при решении сложной головоломки."
                            }
                        }
                    ],
                    "synonyms": [
                        "differently",
                        "unconventionally"
                    ],
                    "antonyms": [
                        "conventionally",
                        "traditionally"
                    ],
                    "common_phrases": [
                        "live alternatively",
                        "think alternatively"
                    ],
                    "target_lang_definitions": {

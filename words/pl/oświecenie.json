    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aa9be8d7-7300-3ebf-8b9f-43e63c52ee83",
                    "sense_definition": "epoka w kulturze europejskiej XVIII wieku, charakteryzująca się kultem rozumu, rozwojem nauki i krytycyzmem wobec tradycji",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_historical_era",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W epoce <w>oświecenia</w> nastąpił gwałtowny rozwój nauk przyrodniczych.",
                            "target_lang_translations": {
                                "ru": "В эпоху <w>Просвещения</w> произошел бурный расцвет естественных наук."
                            }
                        },
                        {
                            "text": "Literatura <w>oświecenia</w> często pełniła funkcje dydaktyczne i wychowawcze.",
                            "target_lang_translations": {
                                "ru": "Литература <w>Просвещения</w> часто выполняла дидактические и воспитательные функции."
                            }
                        }
                    ],
                    "synonyms": [
                        "Oświecenie",
                        "postęp"
                    ],
                    "antonyms": [
                        "romantyzm",
                        "średniowiecze"
                    ],
                    "common_phrases": [
                        "epoka oświecenia",
                        "idee oświecenia",
                        "filozofia oświecenia",
                        "polskie oświecenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "эпоха в европейской культуре XVIII века, характеризующаяся культом разума, развитием науки и критикой традиций"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Просвещение",
                                "target_lang_sense_clarification": "историческая эпоха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "496a1197-49c3-33ae-99ce-9750634b4360",
                    "sense_definition": "stan głębokiego zrozumienia rzeczywistości, jasności umysłu lub poznania prawdy, często w kontekście duchowym",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_spiritual_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielu latach medytacji mnich w końcu osiągnął <w>oświecenie</w>.",
                            "target_lang_translations": {
                                "ru": "После многих лет медитации монах наконец достиг <w>просветления</w>."
                            }
                        },
                        {
                            "text": "Doznał nagłego <w>oświecenia</w> i zrozumiał, jak rozwiązać ten trudny problem.",
                            "target_lang_translations": {
                                "ru": "На него снизошло внезапное <w>озарение</w>, и он понял, как решить эту трудную проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "postęp",
                        "oświecenie"
                    ],
                    "antonyms": [
                        "niewiedza",
                        "ignorancja",
                        "ciemnota"
                    ],
                    "common_phrases": [
                        "doznać oświecenia",
                        "duchowe oświecenie",
                        "droga do oświecenia",
                        "stan oświecenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "состояние глубокого понимания реальности, ясности ума или познания истины, часто в духовном контексте"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просветление",
                                "target_lang_sense_clarification": "духовное состояние"
                            },
                            {
                                "target_lang_word": "озарение",
                                "target_lang_sense_clarification": "внезапное понимание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65c57fce-30ae-3f16-9261-afad248106db",
                    "sense_definition": "rzeczownik odczasownikowy od <w>oświecić</w>: czynność przekazywania wiedzy, wyjaśniania czegoś lub uświadamiania kogoś",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_action_educate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziękuję ci za <w>oświecenie</w> mnie w kwestii nowych przepisów podatkowych.",
                            "target_lang_translations": {
                                "ru": "Спасибо за <w>разъяснение</w> мне вопроса о новых налоговых правилах."
                            }
                        },
                        {
                            "text": "Jego życiową misją było <w>oświecenie</w> społeczeństwa i walka z zabobonami.",
                            "target_lang_translations": {
                                "ru": "Его жизненной миссией было <w>просвещение</w> общества и борьба с суевериями."
                            }
                        }
                    ],
                    "synonyms": [

                {
                    "sense_id": "ea179303-2174-3261-a94d-0c9aa28839da",
                    "sense_definition": "Toestand van last, ongemak of belemmering die men ondervindt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hinder_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bewoners ondervinden veel <w>hinder</w> van het lawaai van de snelweg.",
                            "target_lang_translations": {
                                "en": "The residents experience a lot of <w>nuisance</w> from the noise of the highway.",
                                "ru": "Жители испытывают большие <w>неудобства</w> из-за шума с автострады."
                            }
                        },
                        {
                            "text": "Excuses voor de <w>hinder</w> tijdens de werkzaamheden aan het spoor.",
                            "target_lang_translations": {
                                "en": "Apologies for the <w>inconvenience</w> during the work on the tracks.",
                                "ru": "Приносим извинения за <w>неудобства</w> во время работ на путях."
                            }
                        }
                    ],
                    "synonyms": [
                        "overlast",
                        "ongemak",
                        "belemmering",
                        "last"
                    ],
                    "common_phrases": [
                        "hinder ondervinden",
                        "hinder veroorzaken"
                    ],
                    "target_lang_definitions": {

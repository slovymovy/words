    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "eeb5da29-da1b-3f71-8bc4-b1d04ee97858",
                    "sense_definition": "Opuszczać jakieś miejsce, udając się w podróż środkiem transportu",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "leaving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jutro rano <wyjeżdżamy> na długo oczekiwane wakacje.",
                            "target_lang_translations": {
                                "ru": "Завтра утром мы <выезжаем> в долгожданный отпуск."
                            }
                        },
                        {
                            "text": "Samochód powoli <wyjeżdżał> z garażu na ulicę.",
                            "target_lang_translations": {
                                "ru": "Машина медленно <выезжала> из гаража на улицу."
                            }
                        }
                    ],
                    "antonyms": [
                        "wjeżdżać"
                    ],
                    "common_phrases": [
                        "wyjeżdżać za granicę",
                        "wyjeżdżać na wakacje",
                        "wyjeżdżać z domu",
                        "wyjeżdżać w delegację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покидать какое-либо место, отправляясь в путь на транспортном средстве"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выезжать",
                                "target_lang_sense_clarification": "покидать пределы чего-либо на транспорте"
                            },
                            {
                                "target_lang_word": "уезжать",
                                "target_lang_sense_clarification": "отправляться в другое место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01780839-4801-3343-b8b0-4b0aa92ad547",
                    "sense_definition": "Przemieszczać się pojazdem na wyższy poziom lub na otwartą przestrzeń",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "moving_up_out",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rowerzysta z trudem <wyjeżdżał> na strome wzgórze za miastem.",
                            "target_lang_translations": {
                                "ru": "Велосипедист с трудом <взбирался> на крутой холм за городом."
                            }
                        },
                        {
                            "text": "Kierowca ostrożnie <wyjeżdżał> na prostą drogę po pokonaniu zakrętu.",
                            "target_lang_translations": {
                                "ru": "Водитель осторожно <выезжал> на прямую дорогу после поворота."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyjeżdżać na górę",
                        "wyjeżdżać na prostą",
                        "wyjeżdżać na prowadzenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться на транспортном средстве вверх или выбираться на открытое пространство"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выезжать",
                                "target_lang_sense_clarification": "выбираться на открытое место или дорогу"
                            },
                            {
                                "target_lang_word": "взбираться",
                                "target_lang_sense_clarification": "подниматься вверх (на гору) на транспорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80030a43-1624-3475-9ab5-3599a335da44",
                    "sense_definition": "Odzywać się niespodziewanie, mówiąc coś niestosownego, dziwnego lub niemądrego",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "speaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat zawsze <wyjeżdża> z głupimi żartami w najmniej odpowiednim momencie.",
                            "target_lang_translations": {
                                "ru": "Мой брат всегда <вылезает> с глупыми шутками в самый неподходящий момент."
                            }
                        },
                        {
                            "text": "Nie <wyjeżdżaj> mi tu z pretensjami, kiedy próbuję ci pomóc.",
                            "target_lang_translations": {
                                "ru": "Не <лезь> ко мне с претензиями, когда я пытаюсь тебе помочь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyjeżdżać z tekstem",
                        "wyjeżdżać z pretensjami",
                        "wyjeżdżać z propozycją"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неожиданно высказываться, говоря что-то неуместное, странное или глупое"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вылезать",
                                "target_lang_sense_clarification": "разг. выступать с чем-то неуместным"
                            },
                            {
                                "target_lang_word": "лезть",
                                "target_lang_sense_clarification": "разг. приставать с разговорами или претензиями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3300d964-d0fd-39f6-b5cc-089cf9ecf73a",
                    "sense_definition": "Tracić objętość lub właściwości poprzez parowanie (zwykle o alkoholu lub zapachu)",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "evaporating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gotuj potrawę na małym ogniu, żeby cały aromat nie <wyjeżdżał>.",
                            "target_lang_translations": {
                                "ru": "Готовь блюдо на медленном огне, чтобы весь аромат не <улетучивался>."
                            }
                        },
                        {
                            "text": "Jeśli nie zakorkujesz wina, alkohol szybko z niego <wyjedzie>.",
                            "target_lang_translations": {
                                "ru": "Если не закупоришь вино, алкоголь быстро из него <выдохнется>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ulatniać się",
                        "parować",
                        "wylatywać"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Терять объем или свойства из-за испарения (обычно об алкоголе или запахе)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдыхаться",
                                "target_lang_sense_clarification": "терять запах или крепость"
                            },
                            {
                                "target_lang_word": "улетучиваться",
                                "target_lang_sense_clarification": "испаряться, исчезать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd323766-51e4-319e-9426-b3c33157eed9",
                    "sense_definition": "Uderzać kogoś gwałtownie",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hitting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tamtej dzielnicy można oberwać, jak ktoś ci <wyjedzie> z pięści.",
                            "target_lang_translations": {
                                "ru": "В том районе можно отхватить, если кто-то <заедет> тебе кулаком."
                            }
                        },
                        {
                            "text": "Agresywny napastnik <wyjeżdżał> ludziom z łapami bez powodu.",
                            "target_lang_translations": {
                                "ru": "Агрессивный нападающий <лез> к людям с кулаками без причины."
                            }
                        }
                    ],
                    "synonyms": [
                        "walić",
                        "uderzać"
                    ],
                    "common_phrases": [
                        "wyjeżdżać z liścia",
                        "wyjeżdżać z pięści"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно и резко ударить кого-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заехать",
                                "target_lang_sense_clarification": "сленг. ударить (обычно по лицу)"
                            },
                            {
                                "target_lang_word": "врезать",
                                "target_lang_sense_clarification": "сленг. сильно ударить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "listener",
        "listening",
        "listenable",
        "relisten",
        "unlistenable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "914b55b2-5834-30f9-8bda-1cb99477d7d6",
                    "sense_definition": "To use one's ears to hear sound intentionally; to pay attention to a sound.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "hearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I like to <w>listen</w> to music on my way to work.",
                            "target_lang_translations": {
                                "ru": "Мне нравится <w>слушать</w> музыку по дороге на работу."
                            }
                        },
                        {
                            "text": "Please <w>listen</w> carefully to the instructions before you begin.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, внимательно <w>прослушайте</w> инструкции, прежде чем начать."
                            }
                        }
                    ],
                    "synonyms": [
                        "pay attention",
                        "attend"
                    ],
                    "antonyms": [
                        "ignore",
                        "disregard"
                    ],
                    "common_phrases": [
                        "listen to music",
                        "listen carefully",
                        "listen for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать слух, чтобы намеренно слышать звуки; обращать внимание на звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слу́шать",
                                "target_lang_sense_clarification": "воспринимать звуковую информацию"
                            },
                            {
                                "target_lang_word": "послу́шать",
                                "target_lang_sense_clarification": "провести некоторое время, слушая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f392f780-5ef4-31ed-a163-3079fd4f10f5",
                    "sense_definition": "To accept advice or obey instructions; to agree to do what someone says.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "obeying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Good children <w>listen</w> to their parents and teachers.",
                            "target_lang_translations": {
                                "ru": "Хорошие дети <w>слушаются</w> своих родителей и учителей."
                            }
                        },
                        {
                            "text": "He refused to <w>listen</w> to my advice about saving money.",
                            "target_lang_translations": {
                                "ru": "Он отказался <w>слушать</w> мой совет о том, как сберечь деньги."
                            }
                        }
                    ],
                    "synonyms": [
                        "heed",
                        "mind"
                    ],
                    "antonyms": [
                        "ignore",
                        "disregard"
                    ],
                    "common_phrases": [
                        "listen to reason",
                        "listen to advice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать советы или подчиняться инструкциям; соглашаться делать то, что говорят другие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слу́шаться",
                                "target_lang_sense_clarification": "подчиняться, повиноваться"
                            },
                            {
                                "target_lang_word": "слу́шать",
                                "target_lang_sense_clarification": "следовать чьим-то советам"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fcaa12d8-920b-3e8e-b679-6215fa8a6f82",
                    "sense_definition": "An act of listening to something, or a count of how many times a digital audio file has been played.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Give the engine a <w>listen</w> and tell me if it sounds strange.",
                            "target_lang_translations": {
                                "ru": "<w>Послушай</w> двигатель и скажи мне, не странно ли он звучит."
                            }
                        },
                        {
                            "text": "The new song already has over a million <w>listens</w> on the streaming platform.",
                            "target_lang_translations": {
                                "ru": "У новой песни уже более миллиона <w>прослушиваний</w> на стриминговой платформе."
                            }
                        }
                    ],
                    "synonyms": [
                        "play"
                    ],
                    "common_phrases": [
                        "give a listen",
                        "have a listen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт прослушивания чего-либо или количество воспроизведений цифрового аудиофайла."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }
    ]
}
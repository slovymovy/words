                    "frequency": "Middle",
                    "semantic_group_id": "progression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police officer told the crowd to move <w>along</w>.",
                            "target_lang_translations": {
                                "ru": "Полицейский велел толпе проходить <w>дальше</w>."
                            }

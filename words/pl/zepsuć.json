{
    "word_family": [
        "psuć",
        "popsuć",
        "zepsucie",
        "zepsuty"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "94800467-2d48-3654-aeaf-8cd0bac9abc4",
                    "sense_definition": "Zniszczyć lub uszkodzić przedmiot w sposób uniemożliwiający jego działanie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "uszkodzenie_mechaniczne",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój syn <w>zepsuł</w> już wiele zabawek.",
                            "target_lang_translations": {
                                "ru": "Мой сын <w>сломал</w> уже много игрушек."
                            }
                        },
                        {
                            "text": "Uważaj, żeby nie <w>zepsuć</w> tego delikatnego mechanizmu.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, чтобы не <w>испортить</w> этот хрупкий механизм."
                            }
                        }
                    ],
                    "synonyms": [
                        "uszkodzić",
                        "zniszczyć"
                    ],
                    "antonyms": [
                        "naprawić"
                    ],
                    "common_phrases": [
                        "zepsuć zabawkę",
                        "zepsuć zegarek",
                        "zepsuć urządzenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уничтожить или повредить предмет так, что он перестает работать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сломать",
                                "target_lang_sense_clarification": "о физическом повреждении предмета"
                            },
                            {
                                "target_lang_word": "испортить",
                                "target_lang_sense_clarification": "повредить, сделать непригодным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a0eb863-c5c7-3d14-93a3-892b27f871ff",
                    "sense_definition": "O sprzęcie lub maszynie: przestać działać poprawnie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "uszkodzenie_mechaniczne",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary telewizor znów się <w>zepsuł</w>.",
                            "target_lang_translations": {
                                "ru": "Этот старый телевизор снова <w>сломался</w>."
                            }
                        },
                        {
                            "text": "W moim samochodzie <w>zepsuł</w> się silnik.",
                            "target_lang_translations": {
                                "ru": "В моей машине <w>сломался</w> двигатель."
                            }
                        }
                    ],
                    "synonyms": [
                        "popsuć się"
                    ],
                    "common_phrases": [
                        "zepsuć się nagle",
                        "samochód się zepsuł",
                        "silnik się zepsuł"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О технике или машине: перестать работать должным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сломаться",
                                "target_lang_sense_clarification": "о механизмах и приборах"
                            },
                            {
                                "target_lang_word": "испортиться",
                                "target_lang_sense_clarification": "прийти в негодность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebf05878-e210-38f0-a7dd-816fcc90e010",
                    "sense_definition": "Zmienić coś na gorsze, np. nastrój, plany lub efekt pracy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pogorszenie_stanu",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Źle dobrany przykład <w>zepsuł</w> jego wystąpienie.",
                            "target_lang_translations": {
                                "ru": "Плохо подобранный пример <w>испортил</w> его выступление."
                            }
                        },
                        {
                            "text": "Nie pozwól, by drobne problemy <w>zepsuły</w> ci humor.",
                            "target_lang_translations": {
                                "ru": "Не позволяй мелким проблемам <w>испортить</w> тебе настроение."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmarnować",
                        "zrujnować"
                    ],
                    "antonyms": [
                        "naprawić"
                    ],
                    "common_phrases": [
                        "zepsuć humor",
                        "zepsuć niespodziankę",
                        "zepsuć plany",
                        "zepsuć opinię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить что-то к худшему, например, настроение, планы или результат работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испортить",
                                "target_lang_sense_clarification": "настроение, планы, дело"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f42198ce-17f1-38bb-aa9e-e75050654bee",
                    "sense_definition": "O jedzeniu: ulec rozkładowi i nie nadawać się do spożycia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "psucie_jedzenia",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Owoce <w>zepsuły</w> się, bo nikt ich nie kupował.",
                            "target_lang_translations": {
                                "ru": "Фрукты <w>испортились</w>, так как их никто не покупал."
                            }
                        },
                        {
                            "text": "Zupa <w>zepsuła</w> się, ponieważ stała w cieple przez całą noc.",
                            "target_lang_translations": {
                                "ru": "Суп <w>испортился</w>, так как стоял в тепле всю ночь."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgnić"
                    ],
                    "common_phrases": [
                        "mięso się zepsuło",
                        "mleko się zepsuło",
                        "jedzenie się zepsuło"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О еде: испортиться и стать непригодным для употребления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испортиться",
                                "target_lang_sense_clarification": "о продуктах"
                            },
                            {
                                "target_lang_word": "пропасть",
                                "target_lang_sense_clarification": "разговорное, о еде"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
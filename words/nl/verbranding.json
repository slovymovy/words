                {
                    "sense_id": "8195d3de-b02b-3059-8310-634ff1615177",
                    "sense_definition": "Het proces van het vernietigen of verteren van iets door vuur.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>verbranding</w> van afval is slecht voor het milieu.",
                            "target_lang_translations": {
                                "en": "The <w>incineration</w> of waste is bad for the environment.",
                                "ru": "<w>Сжигание</w> отходов вредно для окружающей среды."
                            }
                        },
                        {
                            "text": "Na de <w>verbranding</w> van het hout bleef er alleen as over.",
                            "target_lang_translations": {
                                "en": "After the <w>burning</w> of the wood, only ash remained.",
                                "ru": "После <w>сжигания</w> дров осталась только зола."
                            }
                        }
                    ],
                    "common_phrases": [
                        "verbranding van afval"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of burning something or being burned by fire.",
                        "ru": "Процесс уничтожения или поглощения чего-либо огнем."
                    },
                    "translations": {

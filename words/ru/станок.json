    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "334ffc9d-934d-36f0-9f93-9b64cda3c343",
                    "sense_definition": "Приспособление для бритья со сменными лезвиями (безопасная бритва).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hygiene_tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужно купить новый бритвенный <w>станок</w> и пену для бритья.",
                            "target_lang_translations": {
                                "en": "I need to buy a new <w>razor</w> and shaving foam."
                            }
                        },
                        {
                            "text": "Этот одноразовый <w>станок</w> очень удобен в путешествиях.",
                            "target_lang_translations": {
                                "en": "This disposable <w>razor</w> is very convenient for traveling."
                            }
                        }
                    ],
                    "common_phrases": [
                        "бритвенный станок",
                        "одноразовый станок",
                        "лезвия для станка"
                    ],
                    "target_lang_definitions": {
                        "en": "A device for shaving with replaceable blades (safety razor)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "razor",
                                "target_lang_sense_clarification": "shaving tool"
                            },
                            {
                                "target_lang_word": "safety razor",
                                "target_lang_sense_clarification": "specific type"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72d6d676-7dcb-32e7-9bfa-25a2ec87e2ab",
                    "sense_definition": "Машина или механизм для обработки различных материалов (металла, дерева) и изготовления изделий.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "machinery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На заводе установили новый токарный <w>станок</w> с программным управлением.",
                            "target_lang_translations": {
                                "en": "A new CNC <w>lathe</w> was installed at the factory."
                            }
                        },
                        {
                            "text": "Рабочий простоял у <w>станка</w> всю смену, вытачивая детали.",
                            "target_lang_translations": {
                                "en": "The worker stood at the <w>machine</w> all shift, turning parts."
                            }
                        }
                    ],
                    "synonyms": [
                        "машина"
                    ],
                    "common_phrases": [
                        "токарный станок",
                        "фрезерный станок",
                        "печатный станок",
                        "стоять у станка"
                    ],
                    "target_lang_definitions": {
                        "en": "A machine or mechanism for processing various materials (metal, wood) and manufacturing products."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "machine tool",
                                "target_lang_sense_clarification": "industrial equipment"
                            },
                            {
                                "target_lang_word": "lathe",
                                "target_lang_sense_clarification": "for turning"
                            },
                            {
                                "target_lang_word": "machine",
                                "target_lang_sense_clarification": "general"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cc93370-e3e5-3c56-b052-7d6b9e901ab9",
                    "sense_definition": "Специальная опора в виде горизонтального бруса для тренировок в балете или гимнастике.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Юные балерины каждый день начинают с разминки у <w>станка</w>.",
                            "target_lang_translations": {
                                "en": "Young ballerinas start every day with a warm-up at the <w>barre</w>."
                            }
                        },
                        {
                            "text": "В танцевальном зале вдоль стены закреплён длинный деревянный <w>станок</w>.",
                            "target_lang_translations": {
                                "en": "A long wooden <w>barre</w> is fixed along the wall in the dance hall."
                            }
                        }
                    ],
                    "common_phrases": [
                        "хореографический станок",
                        "балетный станок",
                        "заниматься у станка"
                    ],
                    "target_lang_definitions": {
                        "en": "A special support in the form of a horizontal bar for training in ballet or gymnastics."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "barre",
                                "target_lang_sense_clarification": "ballet"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77038828-b23b-3038-8b22-8120c9a8d873",
                    "sense_definition": "Основание или опора, на которой закрепляется артиллерийское орудие или пулемёт для ведения огня.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пулемётный <w>станок</w> обеспечивал устойчивость оружия при стрельбе очередями.",
                            "target_lang_translations": {
                                "en": "The machine gun <w>mount</w> ensured the weapon's stability when firing bursts."
                            }
                        },
                        {
                            "text": "Солдаты быстро сняли орудие со <w>станка</w> и подготовили его к транспортировке.",
                            "target_lang_translations": {
                                "en": "The soldiers quickly removed the gun from the <w>mount</w> and prepared it for transport."
                            }
                        }
                    ],
                    "synonyms": [
                        "лафет",
                        "станина"
                    ],
                    "common_phrases": [
                        "пулемётный станок",
                        "орудийный станок",
                        "треножный станок"
                    ],
                    "target_lang_definitions": {
                        "en": "A base or support on which an artillery piece or machine gun is fixed for firing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mount",
                                "target_lang_sense_clarification": "military"
                            },
                            {
                                "target_lang_word": "gun carriage",
                                "target_lang_sense_clarification": "mobile support"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c94d1319-e701-334d-b15b-0401cd8b25ea",

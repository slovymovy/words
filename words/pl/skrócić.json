    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fba86b8a-77e4-35ed-93e4-70817c500dfa",
                    "sense_definition": "Zmniejszyć wymiar fizyczny czegoś; sprawić, że coś staje się krótsze.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę iść do krawca, żeby <w>skrócił</w> mi spodnie, bo są za długie.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти к портному, чтобы он <w>укоротил</w> мне брюки, потому что они слишком длинные."
                            }
                        },
                        {
                            "text": "Fryzjer <w>skrócił</w> jej włosy o kilka centymetrów, zmieniając całkowicie fryzurę.",
                            "target_lang_translations": {
                                "ru": "Парикмахер <w>укоротил</w> ей волосы на несколько сантиметров, полностью изменив прическу."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyciąć",
                        "obciąć",
                        "ściąć"
                    ],
                    "antonyms": [
                        "wydłużyć"
                    ],
                    "common_phrases": [
                        "skrócić włosy",
                        "skrócić spodnie",
                        "skrócić rękawy",
                        "skrócić o połowę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшить физический размер чего-либо; сделать что-то короче."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укоротить",
                                "target_lang_sense_clarification": "сделать короче (одежду, предметы)"
                            },
                            {
                                "target_lang_word": "подшить",
                                "target_lang_sense_clarification": "подогнуть и зашить (брюки, юбку)"
                            },
                            {
                                "target_lang_word": "подстричь",
                                "target_lang_sense_clarification": "обрезать волосы или растения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5721971-8718-31aa-8e5a-23cd9fdb7c4d",
                    "sense_definition": "Sprawić, że czynność, zdarzenie lub okres trwa krócej niż planowano.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musieliśmy <w>skrócić</w> nasze wakacje z powodu nagłej choroby.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <w>сократить</w> наш отпуск из-за внезапной болезни."
                            }
                        },
                        {
                            "text": "Nowoczesna technologia pozwala znacznie <w>skrócić</w> czas produkcji tego urządzenia.",
                            "target_lang_translations": {
                                "ru": "Современные технологии позволяют значительно <w>сократить</w> время производства этого устройства."
                            }
                        }
                    ],
                    "antonyms": [
                        "przedłużyć"
                    ],
                    "common_phrases": [
                        "skrócić czas",
                        "skrócić życie",
                        "skrócić wizytę",
                        "skrócić termin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать так, чтобы действие, событие или период длились меньше запланированного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сократить",
                                "target_lang_sense_clarification": "уменьшить длительность или количество"
                            },
                            {
                                "target_lang_word": "урезать",
                                "target_lang_sense_clarification": "сильно сократить (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbaf157d-bcb4-3113-8f60-4a3f1fa1f5a1",
                    "sense_definition": "Stać się krótszym pod względem wymiaru lub czasu trwania (używane z zaimkiem \"się\").",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą dzień znacznie się <w>skrócił</w> i szybko robi się ciemno.",
                            "target_lang_translations": {
                                "ru": "Зимой день значительно <w>сократился</w>, и быстро темнеет."
                            }
                        },
                        {
                            "text": "Lista oczekujących na zabieg w końcu się <w>skróciła</w> dzięki nowym przepisom.",
                            "target_lang_translations": {
                                "ru": "Список ожидающих процедуру наконец <w>сократился</w> благодаря новым правилам."
                            }
                        }
                    ],
                    "antonyms": [
                        "wydłużyć się"
                    ],
                    "common_phrases": [
                        "dzień się skrócił",
                        "dystans się skrócił"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать короче по размеру или продолжительности (возвратная форма)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сократиться",
                                "target_lang_sense_clarification": "уменьшиться (о времени, расстоянии, списке)"
                            },
                            {
                                "target_lang_word": "укоротиться",
                                "target_lang_sense_clarification": "стать короче физически"
                            },
                            {
                                "target_lang_word": "стать короче",
                                "target_lang_sense_clarification": "общая фраза"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2a10483-5530-3982-a470-716398f99bd7",
                    "sense_definition": "Ograniczyć treść tekstu lub wypowiedzi, czyniąc ją bardziej zwięzłą, ale zachowując sens.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "text_editing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Redaktor poprosił autora, aby <w>skrócił</w> artykuł o dwie strony.",
                            "target_lang_translations": {
                                "ru": "Редактор попросил автора <w>сократить</w> статью на две страницы."
                            }
                        },
                        {
                            "text": "Musisz <w>skrócić</w> swoją prezentację, ponieważ mamy mało czasu na spotkanie.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>сократить</w> свою презентацию, так как у нас мало времени на встречу."
                            }
                        }
                    ],
                    "synonyms": [
                        "streścić",
                        "skondensować"
                    ],
                    "antonyms": [
                        "rozwinąć",
                        "rozszerzyć"
                    ],
                    "common_phrases": [
                        "skrócić tekst",
                        "skrócić wypowiedź",
                        "skrócić artykuł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограничить содержание текста или высказывания, сделав его более кратким, но сохранив смысл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сократить",
                                "target_lang_sense_clarification": "сделать текст короче"
                            },
                            {
                                "target_lang_word": "урезать",
                                "target_lang_sense_clarification": "удалить часть содержания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0847162-cc90-3b35-bb63-019067eb29fc",
                    "sense_definition": "Podzielić licznik i mianownik ułamka przez tę samą liczbę całkowitą, aby uprościć jego zapis.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "mathematics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel matematyki pokazał uczniom, jak poprawnie <w>skrócić</w> ułamek.",
                            "target_lang_translations": {
                                "ru": "Учитель математики показал ученикам, как правильно <w>сократить</w> дробь."
                            }
                        },
                        {
                            "text": "Jeśli podzielisz obie liczby przez dwa, uda ci się <w>skrócić</w> ten wynik.",
                            "target_lang_translations": {
                                "ru": "Если разделишь оба числа на два, тебе удастся <w>сократить</w> этот результат."
                            }
                        }
                    ],
                    "antonyms": [
                        "rozszerzyć"
                    ],
                    "common_phrases": [
                        "skrócić ułamek"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "matematyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разделить числитель и знаменатель дроби на одно и то же целое число для упрощения записи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сократить",
                                "target_lang_sense_clarification": "математический термин (дробь)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
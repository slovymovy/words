{
    "word_family": [
        "gevoel",
        "voelen",
        "gevoeligheid"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "36ba74fe-ffb0-3831-af0f-f897cb7e0304",
                    "sense_definition": "Snel emotioneel geraakt of beïnvloed door wat er gebeurt of gezegd wordt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotional_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is een <w>gevoelige</w> jongen en raakt snel gekwetst.",
                            "target_lang_translations": {
                                "en": "He is a <w>sensitive</w> boy and gets hurt easily.",
                                "ru": "Он <w>чувствительный</w> мальчик, и его легко обидеть."
                            }
                        },
                        {
                            "text": "Zij is erg <w>gevoelig</w> voor de kritiek van anderen.",
                            "target_lang_translations": {
                                "en": "She is very <w>sensitive</w> to criticism from others.",
                                "ru": "Она очень <w>чувствительна</w> к критике со стороны других."
                            }
                        }
                    ],
                    "synonyms": [
                        "sensitief"
                    ],
                    "antonyms": [
                        "ongevoelig",
                        "gevoelloos"
                    ],
                    "common_phrases": [
                        "een gevoelig mens",
                        "gevoelig voor kritiek",
                        "een gevoelige snaar"
                    ],
                    "target_lang_definitions": {
                        "en": "Easily emotionally touched or influenced by what happens or is said.",
                        "ru": "Быстро поддающийся эмоциям или влиянию происходящего."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sensitive",
                                "target_lang_sense_clarification": "easily offended or emotionally hurt"
                            },
                            {
                                "target_lang_word": "emotional",
                                "target_lang_sense_clarification": "having strong feelings"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "чувствительный",
                                "target_lang_sense_clarification": "об эмоциональном состоянии"
                            },
                            {
                                "target_lang_word": "ранимый",
                                "target_lang_sense_clarification": "легко обижаемый"
                            },
                            {
                                "target_lang_word": "восприимчивый",
                                "target_lang_sense_clarification": "легко поддающийся влиянию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5495fa3-619e-38d9-b442-aea01ec594cf",
                    "sense_definition": "Pijnlijk of teer als je het aanraakt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die oude wond is nog steeds <w>gevoelig</w> na al die tijd.",
                            "target_lang_translations": {
                                "en": "That old wound is still <w>tender</w> after all this time.",
                                "ru": "Эта старая рана всё ещё <w>болезненна</w> спустя столько времени."
                            }
                        },
                        {
                            "text": "Mijn tand is erg <w>gevoelig</w> als ik koud water drink.",
                            "target_lang_translations": {
                                "en": "My tooth is very <w>sensitive</w> when I drink cold water.",
                                "ru": "Мой зуб очень <w>чувствителен</w>, когда я пью холодную воду."
                            }
                        }
                    ],
                    "antonyms": [
                        "ongevoelig"
                    ],
                    "common_phrases": [
                        "een gevoelige plek",
                        "gevoelige tanden",
                        "een gevoelige huid"
                    ],
                    "target_lang_definitions": {
                        "en": "Painful or tender when touched.",
                        "ru": "Болезненный или нежный при прикосновении."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sensitive",
                                "target_lang_sense_clarification": "reacting to physical stimuli with pain"
                            },
                            {
                                "target_lang_word": "tender",
                                "target_lang_sense_clarification": "painful or sore when touched"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "чувствительный",
                                "target_lang_sense_clarification": "о зубах или коже"
                            },
                            {
                                "target_lang_word": "болезненный",
                                "target_lang_sense_clarification": "причиняющий боль при касании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0bdd47d-25ac-39b0-a72e-8662d7d53c8d",
                    "sense_definition": "Wat voorzichtigheid vraagt omdat het pijnlijk, moeilijk of emotioneel ligt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "delicate_matter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is het <w>gevoeligste</w> onderwerp voor haar, dus wees voorzichtig met wat je zegt.",
                            "target_lang_translations": {
                                "en": "This is the most <w>sensitive</w> subject for her, so be careful what you say.",
                                "ru": "Это самая <w>болезненная</w> тема для неё, поэтому будь осторожен в словах."
                            }
                        },
                        {
                            "text": "De politieke situatie in dat land is momenteel erg <w>gevoelig</w>.",
                            "target_lang_translations": {
                                "en": "The political situation in that country is currently very <w>delicate</w>.",
                                "ru": "Политическая ситуация в этой стране сейчас очень <w>щекотливая</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een gevoelig onderwerp",
                        "een gevoelige kwestie",
                        "gevoelige informatie"
                    ],
                    "target_lang_definitions": {
                        "en": "Requiring caution because the matter is painful, difficult, or emotional.",
                        "ru": "Требующий осторожности из-за сложности или эмоциональной напряженности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sensitive",
                                "target_lang_sense_clarification": "requiring tact or secrecy"
                            },
                            {
                                "target_lang_word": "delicate",
                                "target_lang_sense_clarification": "requiring careful handling"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "щекотливый",
                                "target_lang_sense_clarification": "о теме или ситуации"
                            },
                            {
                                "target_lang_word": "деликатный",
                                "target_lang_sense_clarification": "требующий такта"
                            },
                            {
                                "target_lang_word": "болезненный",
                                "target_lang_sense_clarification": "острой или неприятной теме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e897f977-1bb8-3ff3-9f8c-accaa3284c57",
                    "sense_definition": "In staat om heel goed dingen te voelen, waar te nemen of erop te reageren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je vingertoppen zijn veel <w>gevoeliger</w> dan je ellebogen.",
                            "target_lang_translations": {
                                "en": "Your fingertips are much more <w>sensitive</w> than your elbows.",
                                "ru": "Твои кончики пальцев гораздо <w>чувствительнее</w>, чем локти."
                            }
                        },
                        {
                            "text": "De babyhuid is veel <w>gevoeliger</w> voor irritaties dan de huid van volwassenen.",
                            "target_lang_translations": {
                                "en": "Baby skin is much more <w>sensitive</w> to irritation than adult skin.",
                                "ru": "Кожа младенца гораздо <w>чувствительнее</w> к раздражениям, чем кожа взрослых."
                            }
                        }
                    ],
                    "antonyms": [
                        "ongevoelig"
                    ],
                    "common_phrases": [
                        "gevoelige oren",
                        "gevoelig voor licht"
                    ],
                    "target_lang_definitions": {
                        "en": "Able to feel, perceive, or react to things very well.",
                        "ru": "Способный тонко чувствовать, воспринимать или реагировать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sensitive",
                                "target_lang_sense_clarification": "quick to detect or respond to stimuli"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "чувствительный",
                                "target_lang_sense_clarification": "об органах чувств"
                            },
                            {
                                "target_lang_word": "восприимчивый",
                                "target_lang_sense_clarification": "к внешним раздражителям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55d29b9e-9354-310e-ac1c-68430fb633ed",
                    "sense_definition": "Wat snel reageert op kleine veranderingen of invloeden (vaak over apparatuur).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "technical_response",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze meting is niet <w>gevoelig</w> genoeg om het verschil te zien.",
                            "target_lang_translations": {
                                "en": "This measurement is not <w>sensitive</w> enough to see the difference.",
                                "ru": "Это измерение недостаточно <w>чувствительное</w>, чтобы заметить разницу."
                            }
                        },
                        {
                            "text": "De sensor is zeer <w>gevoelig</w> voor beweging in het donker.",
                            "target_lang_translations": {
                                "en": "The sensor is very <w>sensitive</w> to movement in the dark.",
                                "ru": "Датчик очень <w>чувствителен</w> к движению в темноте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gevoelige apparatuur",
                        "gevoelige instrumenten"
                    ],
                    "target_lang_definitions": {
                        "en": "Reacting quickly to small changes or influences (often regarding equipment).",
                        "ru": "Быстро реагирующий на малейшие изменения или воздействия (об аппаратуре)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sensitive",
                                "target_lang_sense_clarification": "highly responsive to small changes"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "чувствительный",
                                "target_lang_sense_clarification": "о приборах и датчиках"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
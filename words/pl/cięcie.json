                        ]
                    }
                },
                {
                    "sense_id": "4d1b3904-7ff6-3e97-b812-19eea8ed400f",
                    "sense_definition": "rana na ciele powstała w wyniku naruszenia tkanki ostrym przedmiotem",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "result_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz opatrzył głębokie <cięcie> na dłoni pacjenta i założył szwy.",
                            "target_lang_translations": {
                                "ru": "Врач обработал глубокий <порез> на ладони пациента и наложил швы."
                            }

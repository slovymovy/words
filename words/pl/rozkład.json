    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "51adfcd2-7c09-363a-9b4f-4ef808c09087",
                    "sense_definition": "Plan godzin odjazdów i przyjazdów środków transportu lub harmonogram zajęć i czynności ułożony w porządku czasowym.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "schedule",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę sprawdzić aktualny <w>rozkład</w> jazdy pociągów do Warszawy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно проверить актуальное <w>расписание</w> поездов до Варшавы."
                            }
                        },
                        {
                            "text": "Nasz <w>rozkład</w> dnia w szkole zmienił się w tym semestrze.",
                            "target_lang_translations": {
                                "ru": "Наше школьное <w>расписание</w> изменилось в этом семестре."
                            }
                        }
                    ],
                    "synonyms": [
                        "harmonogram",
                        "plan",
                        "program"
                    ],
                    "common_phrases": [
                        "rozkład jazdy",
                        "rozkład zajęć",
                        "rozkład dnia",
                        "według rozkładu"
                    ],
                    "target_lang_definitions": {
                        "ru": "График времени отправления и прибытия транспортных средств или план занятий и мероприятий, составленный в хронологическом порядке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расписание",
                                "target_lang_sense_clarification": "транспорта, уроков"
                            },
                            {
                                "target_lang_word": "распорядок",
                                "target_lang_sense_clarification": "дня"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cae8137-8f18-3b52-959f-8bb5d803630a",
                    "sense_definition": "Sposób rozmieszczenia lub ułożenia elementów w przestrzeni, na przykład pomieszczeń w budynku.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To mieszkanie ma bardzo funkcjonalny <w>rozkład</w> pomieszczeń.",
                            "target_lang_translations": {
                                "ru": "У этой квартиры очень функциональная <w>планировка</w> помещений."
                            }
                        },
                        {
                            "text": "Architekt zaproponował nowy <w>rozkład</w> mebli w salonie.",
                            "target_lang_translations": {
                                "ru": "Архитектор предложил новое <w>расположение</w> мебели в гостиной."
                            }
                        }
                    ],
                    "synonyms": [
                        "plan"
                    ],
                    "common_phrases": [
                        "rozkład mieszkania",
                        "rozkład pomieszczeń",
                        "rozkład sił"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ размещения или расположения элементов в пространстве, например, комнат в здании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "планировка",
                                "target_lang_sense_clarification": "помещений, квартиры"
                            },
                            {
                                "target_lang_word": "расположение",
                                "target_lang_sense_clarification": "мебели, элементов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "799c5346-9617-382b-a6d7-9a88c5d7f210",
                    "sense_definition": "Proces biologicznego lub chemicznego niszczenia materii organicznej; gnicie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "decay_bio",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lesie znaleziono zwierzę w stanie zaawansowanego <w>rozkładu</w>.",
                            "target_lang_translations": {
                                "ru": "В лесу нашли животное в состоянии сильного <w>разложения</w>."
                            }
                        },
                        {
                            "text": "Wysoka temperatura przyspiesza <w>rozkład</w> resztek jedzenia.",
                            "target_lang_translations": {
                                "ru": "Высокая температура ускоряет <w>разложение</w> остатков еды."
                            }
                        }
                    ],
                    "synonyms": [
                        "gnicie",
                        "rozpad"
                    ],
                    "common_phrases": [
                        "stan rozkładu",
                        "ulec rozkładowi",
                        "proces rozkładu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс биологического или химического разрушения органической материи; гниение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разложение",
                                "target_lang_sense_clarification": "биологический процесс"
                            },
                            {
                                "target_lang_word": "гниение",
                                "target_lang_sense_clarification": "распад органики"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a15b2ad-dc20-33c9-a153-b3965e8f15e2",
                    "sense_definition": "Całkowity upadek, dezorganizacja lub pogorszenie się stanu jakiejś struktury społecznej, moralnej lub politycznej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "decay_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Historycy analizują przyczyny, które spowodowały <w>rozkład</w> imperium rzymskiego.",
                            "target_lang_translations": {
                                "ru": "Историки анализируют причины, вызвавшие <w>распад</w> Римской империи."
                            }
                        },
                        {
                            "text": "Sąd orzekł trwały i zupełny <w>rozkład</w> pożycia małżeńskiego.",
                            "target_lang_translations": {
                                "ru": "Суд признал полный и окончательный <w>распад</w> супружеской жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "upadek",
                        "erozja",
                        "rozpad",
                        "bankructwo"
                    ],
                    "common_phrases": [

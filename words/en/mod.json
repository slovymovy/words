        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b7cdaded-8c33-3bba-8c24-e14fb0af3ac8",
                    "sense_definition": "Crazy, insane, or extremely impressive.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That party last night was absolutely <mod>.",
                            "target_lang_translations": {
                                "ru": "Вчерашняя вечеринка была абсолютно <безумной>."
                            }

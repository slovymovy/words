                    ],
                    "target_lang_definitions": {
                        "ru": "Угощать кого-либо обедом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угощать обедом",
                                "target_lang_sense_clarification": "оплатить обед для кого-то"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "85c8f148-6ceb-3a34-b483-51f82f4b236b",
                    "sense_definition": "Postać biblijna, najstarszy z trzech synów Noego, uważany za praojca ludów semickich.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biblical_figure",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Według przekazów biblijnych <w>Sem</w> przeżył potop na arce wraz z rodziną.",
                            "target_lang_translations": {
                                "ru": "Согласно библейским преданиям, <w>Сим</w> пережил потоп на ковчеге вместе с семьей."
                            }
                        },
                        {
                            "text": "Tradycja głosi, że to właśnie <w>Sem</w> dał początek wielu narodom Bliskiego Wschodu.",
                            "target_lang_translations": {
                                "ru": "Предание гласит, что именно <w>Сим</w> стал родоначальником многих народов Ближнего Востока."
                            }
                        }
                    ],
                    "common_phrases": [
                        "synowie Sema",
                        "potomstwo Sema"
                    ],
                    "target_lang_definitions": {
                        "ru": "Библейский персонаж, старший из трех сыновей Ноя, считающийся прародителем семитских народов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сим",
                                "target_lang_sense_clarification": "библейский персонаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9121894f-e85d-38d9-8bfa-90061cd614db",
                    "sense_definition": "Męskie imię pochodzenia hebrajskiego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "given_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Rodzice wybrali dla syna krótkie i oryginalne imię <w>Sem</w>.",
                            "target_lang_translations": {
                                "ru": "Родители выбрали для сына короткое и оригинальное имя <w>Сем</w>."
                            }
                        },
                        {
                            "text": "Mój kolega <w>Sem</w> często musi tłumaczyć pochodzenie swojego imienia.",
                            "target_lang_translations": {
                                "ru": "Моему коллеге <w>Сему</w> часто приходится объяснять происхождение своего имени."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя еврейского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сем",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "15c0cf1d-8210-3946-a226-c29903ae7a62",
                    "sense_definition": "Skrót od wyrażenia <w>siła elektromotoryczna</w> – wielkość fizyczna charakteryzująca źródło energii w obwodzie elektrycznym.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physics_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W idealnym źródle napięcia <w>SEM</w> jest równa napięciu na zaciskach.",
                            "target_lang_translations": {
                                "ru": "В идеальном источнике напряжения <w>ЭДС</w> равна напряжению на зажимах."
                            }
                        },
                        {
                            "text": "Uczniowie obliczali <w>SEM</w> ogniwa na lekcji fizyki.",
                            "target_lang_translations": {
                                "ru": "Ученики вычисляли <w>ЭДС</w> элемента на уроке физики."
                            }
                        }
                    ],
                    "synonyms": [
                        "siła elektromotoryczna"
                    ],
                    "common_phrases": [
                        "SEM ogniwa",
                        "SEM indukcji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от «электродвижущая сила» — физическая величина, характеризующая источник энергии в электрической цепи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ЭДС",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

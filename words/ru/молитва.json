    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aacfe1b1-ff87-37b4-bcdf-9069f1e0e638",
                    "sense_definition": "Обращение верующего к Богу, святым или высшим силам с просьбой, благодарностью или хвалой.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Верующие собрались в храме на общую <w>молитву</w>.",
                            "target_lang_translations": {
                                "en": "The believers gathered in the temple for common <w>prayer</w>."
                            }
                        },
                        {
                            "text": "Её искренняя <w>молитва</w> о здоровье близких была услышана.",
                            "target_lang_translations": {
                                "en": "Her sincere <w>prayer</w> for the health of her loved ones was heard."
                            }
                        }
                    ],
                    "synonyms": [
                        "моление"
                    ],
                    "common_phrases": [
                        "утренняя молитва",
                        "вечерняя молитва",
                        "стоять на молитве"
                    ],
                    "target_lang_definitions": {
                        "en": "An appeal by a believer to God, saints, or higher powers with a request, gratitude, or praise."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prayer",
                                "target_lang_sense_clarification": "act of communicating with God"
                            },
                            {
                                "target_lang_word": "supplication",
                                "target_lang_sense_clarification": "humble request or entreaty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6709c91-1d92-3688-848c-bc850bf9346f",
                    "sense_definition": "Религиозный текст, который читают или произносят при обращении к Богу.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый вечер перед сном бабушка читала одну и ту же <w>молитву</w>.",
                            "target_lang_translations": {
                                "en": "Every evening before sleep, grandmother read the same <w>prayer</w>."
                            }
                        },
                        {
                            "text": "Священник открыл книгу и начал произносить древнюю <w>молитву</w>.",
                            "target_lang_translations": {
                                "en": "The priest opened the book and began to recite an ancient <w>prayer</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "читать молитву",
                        "знать молитву",
                        "слова молитвы"
                    ],
                    "target_lang_definitions": {
                        "en": "A religious text that is read or recited when addressing God."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prayer",
                                "target_lang_sense_clarification": "specific words or text"
                            }
                        ]
                    }
                }
            ]
        }

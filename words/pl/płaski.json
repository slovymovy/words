            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5fe4cc60-304e-3adf-ba0b-1669466edb30",
                    "sense_definition": "Mający równą powierzchnię, pozbawiony wzniesień, wgłębień lub wybrzuszeń.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Holandia jest znana z tego, że jest bardzo <w>płaskim</w> krajem.",
                            "target_lang_translations": {
                                "ru": "Нидерланды известны тем, что являются очень <w>плоской</w> страной."
                            }
                        },
                        {
                            "text": "Musimy znaleźć duży i <w>płaski</w> kamień, żeby postawić na nim kubek.",
                            "target_lang_translations": {
                                "ru": "Нам нужно найти большой и <w>плоский</w> камень, чтобы поставить на него кружку."
                            }
                        }
                    ],
                    "synonyms": [
                        "równy"
                    ],
                    "antonyms": [
                        "wypukły",
                        "pofałdowany",
                        "górzysty",
                        "stromy"
                    ],
                    "common_phrases": [
                        "płaski teren",
                        "płaski brzuch",
                        "płaski dach",
                        "płaski ekran"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий ровную поверхность, лишенный возвышенностей, углублений или выпуклостей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плоский",
                                "target_lang_sense_clarification": "о поверхности"
                            },
                            {
                                "target_lang_word": "ровный",
                                "target_lang_sense_clarification": "гладкий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "816770a8-6faa-305f-98c1-1697d0d78175",
                    "sense_definition": "O naczyniach: mający niewielką głębokość, służący zazwyczaj do serwowania dań głównych (nie zup).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę podać drugie danie na dużym, <w>płaskim</w> talerzu.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подайте второе блюдо на большой <w>плоской</w> тарелке."
                            }
                        },
                        {
                            "text": "W szafce kuchennej brakuje nam czystych, <w>płaskich</w> talerzy.",
                            "target_lang_translations": {
                                "ru": "В кухонном шкафу нам не хватает чистых <w>плоских</w> тарелок."
                            }
                        }
                    ],
                    "synonyms": [
                        "płytki"
                    ],
                    "antonyms": [
                        "głęboki"
                    ],
                    "common_phrases": [
                        "płaski talerz"
                    ],
                    "target_lang_definitions": {
                        "ru": "О посуде: имеющий небольшую глубину, обычно используемый для подачи вторых блюд (не супов)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелкий",
                                "target_lang_sense_clarification": "неглубокий, о тарелке"
                            },
                            {
                                "target_lang_word": "плоский",
                                "target_lang_sense_clarification": "о форме посуды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aab4cebb-4a24-3db9-a02b-2bc845689744",
                    "sense_definition": "O żarcie, wypowiedzi lub intelekcie: mało ambitny, pozbawiony głębszej treści, prymitywny.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>płaski</w> dowcip sprawił, że wszyscy przy stole poczuli się zażenowani.",
                            "target_lang_translations": {
                                "ru": "Его <w>плоская</w> шутка заставила всех за столом почувствовать смущение."
                            }
                        },
                        {
                            "text": "Nie lubię oglądać komedii, w których dominuje taki <w>płaski</w> humor.",
                            "target_lang_translations": {
                                "ru": "Я не люблю смотреть комедии, в которых преобладает такой <w>плоский</w> юмор."
                            }
                        }
                    ],
                    "synonyms": [
                        "banalny",
                        "płytki"
                    ],
                    "antonyms": [
                        "wyszukany",
                        "niebanalny",
                        "głęboki",
                        "nieprzeciętny"
                    ],
                    "common_phrases": [
                        "płaski żart",
                        "płaski dowcip"
                    ],
                    "target_lang_definitions": {
                        "ru": "О шутке, высказывании или интеллекте: малоамбициозный, лишенный глубокого содержания, примитивный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плоский",
                                "target_lang_sense_clarification": "о юморе"
                            },
                            {
                                "target_lang_word": "примитивный",
                                "target_lang_sense_clarification": "банальный"
                            }
                        ]
                    }
                }
            ]
        },
        {

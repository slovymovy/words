                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "drink",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bought a bottle of orange <w>pop</w> for the picnic.",
                            "target_lang_translations": {
                                "ru": "Мы купили бутылку апельсиновой <w>газировки</w> для пикника."

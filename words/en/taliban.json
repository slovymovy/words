                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Security forces arrested a suspected <w>Taliban</w> near the border crossing.",
                            "target_lang_translations": {

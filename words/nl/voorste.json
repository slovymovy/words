    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0d346321-cf9d-38ef-bff1-6515a1ca58db",
                    "sense_definition": "Zich bevindend op de meest vooruitgeschoven plaats; aan het begin of de voorkant van iets.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij hadden geluk en konden op de <w>voorste</w> rij zitten.",
                            "target_lang_translations": {
                                "en": "We were lucky and were able to sit in the <w>front</w> row.",
                                "ru": "Нам повезло, и мы смогли сесть в <w>первый</w> ряд."
                            }
                        },
                        {
                            "text": "Het <w>voorste</w> deel van de auto was beschadigd door het ongeluk.",
                            "target_lang_translations": {
                                "en": "The <w>front</w> part of the car was damaged by the accident.",
                                "ru": "<w>Передняя</w> часть автомобиля была повреждена в результате аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "eerste"
                    ],
                    "antonyms": [
                        "achterste",
                        "laatste"
                    ],
                    "common_phrases": [
                        "de voorste rij",
                        "het voorste deel",
                        "de voorste poten"
                    ],
                    "target_lang_definitions": {
                        "en": "Located at the most forward position; at the beginning or front of something.",
                        "ru": "Находящийся в самом передовом положении; в начале или передней части чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "front",
                                "target_lang_sense_clarification": "relating to the position ahead or at the beginning"
                            },
                            {
                                "target_lang_word": "foremost",
                                "target_lang_sense_clarification": "most prominent or leading in position"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "передний",
                                "target_lang_sense_clarification": "относящийся к передней части"
                            },
                            {
                                "target_lang_word": "первый",
                                "target_lang_sense_clarification": "находящийся в начале ряда"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "608c54a3-f60e-3c9e-afa6-dacaea276282",
                    "sense_definition": "De persoon of het ding dat zich helemaal vooraan bevindt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_position_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>voorste</w> in de rij mocht als eerste naar binnen.",
                            "target_lang_translations": {
                                "en": "The <w>one at the front</w> of the line was allowed to enter first.",
                                "ru": "<w>Первому</w> в очереди разрешили войти первым."
                            }
                        },
                        {
                            "text": "Tijdens de wedstrijd viel de <w>voorste</w> plotseling, waardoor de anderen moesten uitwijken.",
                            "target_lang_translations": {
                                "en": "During the race, the <w>front one</w> fell suddenly, causing the others to have to swerve.",
                                "ru": "Во время соревнования <w>лидер</w> внезапно упал, из-за чего остальным пришлось сворачивать."
                            }
                        }
                    ],
                    "synonyms": [
                        "eerste"
                    ],
                    "antonyms": [
                        "achterste"
                    ],
                    "target_lang_definitions": {
                        "en": "The person or thing that is at the very front.",
                        "ru": "Человек или предмет, который находится в самом переди."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "front one",
                                "target_lang_sense_clarification": "the person or object in the leading position"
                            },
                            {
                                "target_lang_word": "one at the front",
                                "target_lang_sense_clarification": "referring to the leading entity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "первый",
                                "target_lang_sense_clarification": "тот, кто находится впереди всех"
                            },
                            {
                                "target_lang_word": "лидер",
                                "target_lang_sense_clarification": "в спортивном контексте"
                            }
                        ]
                    }
                }
            ]
        }
    ]

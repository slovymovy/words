    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "afdc7a21-8b14-3ee2-b5d4-53452d4204a8",
                    "sense_definition": "Быть одетым во что-либо или иметь что-либо на себе (одежду, обувь, аксессуары, прическу)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "wear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой люди обычно <носят> тёплые шапки и шарфы.",
                            "target_lang_translations": {
                                "en": "In winter, people usually <wear> warm hats and scarves."
                            }
                        },
                        {
                            "text": "Мой брат любит <носить> джинсы и футболки каждый день.",
                            "target_lang_translations": {
                                "en": "My brother likes to <wear> jeans and T-shirts every day."
                            }
                        },
                        {
                            "text": "Она начала <носить> очки, когда пошла в школу.",
                            "target_lang_translations": {
                                "en": "She started to <wear> glasses when she started school."
                            }
                        }
                    ],
                    "common_phrases": [
                        "носить очки",
                        "носить бороду",
                        "носить кольцо",
                        "носить форму",
                        "носить траур"
                    ],
                    "target_lang_definitions": {
                        "en": "To be dressed in something or have something on oneself (clothes, shoes, accessories, hairstyle)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wear",
                                "target_lang_sense_clarification": "have clothing or accessories on one's body"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81f4ffc3-38b8-3272-8e80-1bcd42573fea",
                    "sense_definition": "Перемещать кого-либо или что-либо, держа в руках или на себе (обычно неоднократно, туда-сюда или в разных направлениях)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Официант должен быстро <носить> еду клиентам в зале.",
                            "target_lang_translations": {
                                "en": "The waiter must quickly <carry> food to customers in the dining hall."
                            }
                        },
                        {
                            "text": "Нам пришлось <носить> воду из колодца весь день.",
                            "target_lang_translations": {
                                "en": "We had to <carry> water from the well all day."
                            }
                        },
                        {
                            "text": "Мать любит <носить> своего ребёнка на руках.",
                            "target_lang_translations": {
                                "en": "The mother loves to <carry> her child in her arms."
                            }
                        }
                    ],
                    "synonyms": [
                        "таскать"
                    ],
                    "common_phrases": [
                        "носить на руках",
                        "носить воду",
                        "носить тяжести",
                        "носить с собой"
                    ],
                    "target_lang_definitions": {
                        "en": "To move someone or something while holding them in one's hands or on oneself (usually repeatedly or in different directions)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "hold and transport something from one place to another"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1a4a595-72ea-3960-ac0d-2a73bacba625",
                    "sense_definition": "Иметь, обладать каким-либо признаком, именем или свойством",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "possess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта центральная улица будет <носить> имя известного поэта.",
                            "target_lang_translations": {
                                "en": "This central street will <bear> the name of a famous poet."
                            }
                        },
                        {
                            "text": "Их встреча <носила> чисто деловой характер.",
                            "target_lang_translations": {
                                "en": "Their meeting <bore> a purely business character."
                            }
                        },
                        {
                            "text": "Многие женщины после свадьбы начинают <носить> фамилию мужа.",
                            "target_lang_translations": {
                                "en": "Many women start to <bear> their husband's surname after the wedding."
                            }
                        }
                    ],
                    "common_phrases": [
                        "носить имя",
                        "носить фамилию",
                        "носить характер",
                        "носить название",
                        "носить звание"
                    ],
                    "target_lang_definitions": {
                        "en": "To have or possess a certain feature, name, or quality"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bear",
                                "target_lang_sense_clarification": "have or display a name, mark, or characteristic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "827fb0f4-81ab-30d0-ae8c-c2bd740b69f9",
                    "sense_definition": "Быть беременной, вынашивать ребёнка",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "pregnancy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она <носит> под сердцем долгожданного сына.",
                            "target_lang_translations": {
                                "en": "She is <carrying> a long-awaited son under her heart."
                            }
                        },
                        {
                            "text": "Женщинам бывает трудно <носить> ребёнка на последних месяцах.",
                            "target_lang_translations": {
                                "en": "It can be difficult for women to <carry> a child in the last months."
                            }
                        }
                    ],
                    "common_phrases": [
                        "носить ребёнка",
                        "носить под сердцем",
                        "носить плод"
                    ],
                    "target_lang_definitions": {
                        "en": "To be pregnant, to carry a child"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "be pregnant with a child"
                            },
                            {
                                "target_lang_word": "bear",
                                "target_lang_sense_clarification": "carry offspring in the womb"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cf9a486-1784-3703-8ac1-e7bc3afffa12",
                    "sense_definition": "Быстро перемещать или гнать силой потока (о ветре, воде)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "drive_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный ветер <носит> сухие листья по всему парку.",
                            "target_lang_translations": {
                                "en": "A strong wind <carries> dry leaves all over the park."
                            }
                        },
                        {
                            "text": "Лодку <носило> по волнам во время страшного шторма.",
                            "target_lang_translations": {
                                "en": "The boat was <carried> along the waves during the terrible storm."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ветер носит",
                        "где тебя носит"
                    ],
                    "target_lang_definitions": {
                        "en": "To move quickly or drive by the force of a flow (of wind or water)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "move something by the force of wind or water"
                            }
                        ]
                    }
                }
            ]
        }
    ]

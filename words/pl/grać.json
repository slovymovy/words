                {
                    "sense_id": "3773318d-0f55-3691-99b6-a6968dfc15ea",
                    "sense_definition": "Uczestniczyć w grze sportowej, towarzyskiej lub komputerowej, postępując zgodnie z jej zasadami.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "games_sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi przyjaciele uwielbiają <w>grać</w> w piłkę nożną w każdy weekend.",
                            "target_lang_translations": {
                                "ru": "Мои друзья обожают <w>играть</w> в футбол каждые выходные."
                            }
                        },
                        {
                            "text": "Czy chciałbyś <w>grać</w> z nami w karty dzisiaj wieczorem?",
                            "target_lang_translations": {
                                "ru": "Хотел бы ты <w>поиграть</w> с нами в карты сегодня вечером?"
                            }
                        }

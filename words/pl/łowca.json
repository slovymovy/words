            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1fdf0ddd-fc97-3c6f-b7e1-9fc886db7c6f",
                    "sense_definition": "Osoba zajmująca się polowaniem na dzikie zwierzęta w celu zdobycia pożywienia, skór lub trofeów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "hunter_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony <w>łowca</w> potrafi bezszelestnie poruszać się po lesie.",
                            "target_lang_translations": {
                                "ru": "Опытный <w>охотник</w> умеет бесшумно передвигаться по лесу."
                            }
                        },
                        {
                            "text": "W dawnych czasach każdy mężczyzna w plemieniu musiał być dobrym <w>łowcą</w>.",
                            "target_lang_translations": {
                                "ru": "В давние времена каждый мужчина в племени должен был быть хорошим <w>охотником</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "myśliwy"
                    ],
                    "common_phrases": [
                        "wielki łowca",
                        "nocny łowca",
                        "zwinny łowca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, занимающийся охотой на диких животных ради пропитания, шкур или трофеев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охотник",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "ловец",
                                "target_lang_sense_clarification": "тот, кто ловит зверей или птиц; часто в архаичном или специфическом контексте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31cbc144-91b5-3326-a299-f52fce36e2fd",
                    "sense_definition": "Osoba, która aktywnie i z determinacją poszukuje kogoś lub czegoś, często dla zysku, sławy lub z pasji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hunter_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany <w>łowca</w> nagród przybył do miasta w poszukiwaniu zbiega.",
                            "target_lang_translations": {
                                "ru": "Известный <w>охотник</w> за головами прибыл в город в поисках беглеца."
                            }
                        },
                        {
                            "text": "Firma wysłała swojego najlepszego <w>łowcę</w> talentów na uniwersytet.",
                            "target_lang_translations": {
                                "ru": "Фирма отправила своего лучшего <w>охотника</w> за талантами в университет."
                            }
                        }
                    ],
                    "synonyms": [
                        "poszukiwacz",
                        "tropiciel"
                    ],
                    "common_phrases": [
                        "łowca nagród",
                        "łowca głów",
                        "łowca talentów",
                        "łowca burz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который активно и решительно ищет кого-то или что-то, часто ради выгоды или профессионально."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охотник",
                                "target_lang_sense_clarification": "в устойчивых выражениях (за головами, за талантами)"
                            },
                            {
                                "target_lang_word": "ловец",
                                "target_lang_sense_clarification": "в переносных значениях (ловец душ, ловец снов)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc4ec7d4-34e3-3ead-a1f8-2a6559a15f98",
                    "sense_definition": "Forma fleksyjna słowa <w>łowiec</w> (dopełniacz lub biernik liczby pojedynczej).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Król wezwał swojego najlepszego <w>łowca</w> na zamek.",
                            "target_lang_translations": {
                                "ru": "Король призвал своего лучшего <w>ловчего</w> в замок."
                            }
                        },
                        {
                            "text": "Nie było tam już tego starego <w>łowca</w>, którego spotkaliśmy wcześniej.",
                            "target_lang_translations": {
                                "ru": "Там уже не было того старого <w>ловчего</w>, которого мы встретили ранее."
                            }
                        }
                    ],
                    "traits": [
                        {

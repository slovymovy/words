{
    "word_family": [
        "część",
        "cząsteczka",
        "cząstkowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "067aca39-1fc8-33e6-a8f4-bff6bc7c8f44",
                    "sense_definition": "Mały fragment, element lub drobna ilość stanowiąca część większej całości (często w sensie abstrakcyjnym).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "part_of_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdej plotce znajduje się zazwyczaj choć jedna <w>cząstka</w> prawdy.",
                            "target_lang_translations": {
                                "ru": "В каждой сплетне обычно содержится хотя бы одна <w>частица</w> правды."
                            }
                        },
                        {
                            "text": "Włożył w ten projekt całe serce i <w>cząstkę</w> swojej duszy.",
                            "target_lang_translations": {
                                "ru": "Он вложил в этот проект всё сердце и <w>частичку</w> своей души."
                            }
                        }
                    ],
                    "synonyms": [
                        "fragment",
                        "element",
                        "część"
                    ],
                    "antonyms": [
                        "całość"
                    ],
                    "common_phrases": [
                        "cząstka prawdy",
                        "cząstka nadziei",
                        "cząstka siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький фрагмент, элемент или небольшое количество, являющееся частью большего целого (часто в абстрактном смысле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "частица",
                                "target_lang_sense_clarification": "малая часть чего-либо, фрагмент"
                            },
                            {
                                "target_lang_word": "частичка",
                                "target_lang_sense_clarification": "уменьшительное; очень маленькая часть, часто об абстрактных понятиях (души, тепла)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfc8c02f-a0a1-3e49-8bfb-348715dcda61",
                    "sense_definition": "Podstawowy składnik materii, taki jak elektron, proton czy neutron.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physics_particle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naukowcy odkryli nową <w>cząstkę</w> elementarną podczas eksperymentu.",
                            "target_lang_translations": {
                                "ru": "Учёные открыли новую элементарную <w>частицу</w> в ходе эксперимента."
                            }
                        },
                        {
                            "text": "Każda naładowana <w>cząstka</w> reaguje na pole magnetyczne.",
                            "target_lang_translations": {
                                "ru": "Каждая заряженная <w>частица</w> реагирует на магнитное поле."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cząstka elementarna",
                        "cząstka alfa",
                        "fizyka cząstek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основной компонент материи, такой как электрон, протон или нейтрон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "частица",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e62f236-6a07-36ac-a70e-e3c551f34cfc",
                    "sense_definition": "Naturalnie oddzielająca się część miąższu owocu cytrusowego.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fruit_segment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko zjadło ze smakiem ostatnią <w>cząstkę</w> słodkiej mandarynki.",
                            "target_lang_translations": {
                                "ru": "Ребёнок с аппетитом съел последнюю <w>дольку</w> сладкого мандарина."
                            }
                        },
                        {
                            "text": "Do dekoracji tortu możesz użyć obranych <w>cząstek</w> pomarańczy.",
                            "target_lang_translations": {
                                "ru": "Для украшения торта можно использовать очищенные <w>дольки</w> апельсина."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cząstka pomarańczy",
                        "cząstka mandarynki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественно отделяющаяся часть мякоти цитрусового фрукта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "долька",
                                "target_lang_sense_clarification": "сегмент цитрусового фрукта"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
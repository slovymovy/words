{
    "word_family": [
        "objection",
        "objective",
        "objectively",
        "objectivity",
        "objector"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "617a2479-23b2-3e55-92e9-6de676afef70",
                    "sense_definition": "A material thing that can be seen and touched.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A bright <w>object</w> appeared in the sky.",
                            "target_lang_translations": {
                                "ru": "В небе появился яркий <w>объект</w>."
                            }
                        },
                        {
                            "text": "Please do not touch the <w>objects</w> on the shelf.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не трогайте <w>предметы</w> на полке."
                            }
                        }
                    ],
                    "synonyms": [
                        "thing",
                        "item",
                        "article"
                    ],
                    "common_phrases": [
                        "foreign object",
                        "physical object",
                        "unidentified flying object"
                    ],
                    "target_lang_definitions": {
                        "ru": "Материальная вещь, которую можно увидеть и потрогать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предмет",
                                "target_lang_sense_clarification": "физический объект"
                            },
                            {
                                "target_lang_word": "объект",
                                "target_lang_sense_clarification": "материальное тело"
                            },
                            {
                                "target_lang_word": "вещь",
                                "target_lang_sense_clarification": "предмет обихода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07620835-9dc4-3820-90eb-f51cb39c7751",
                    "sense_definition": "A goal or purpose that someone tries to achieve.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>object</w> of the game is to score the most points.",
                            "target_lang_translations": {
                                "ru": "<w>Цель</w> игры — набрать наибольшее количество очков."
                            }
                        },
                        {
                            "text": "Her main <w>object</w> in life was to become a doctor.",
                            "target_lang_translations": {
                                "ru": "Ее главной <w>целью</w> в жизни было стать врачом."
                            }
                        }
                    ],
                    "synonyms": [
                        "target",
                        "thing",
                        "item"
                    ],
                    "common_phrases": [
                        "object of the game",
                        "main object"
                    ],
                    "target_lang_definitions": {
                        "ru": "Цель или намерение, которого кто-то пытается достичь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цель",
                                "target_lang_sense_clarification": "намерение, задача"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b367a6d4-2014-35b4-87c2-99a71996d619",
                    "sense_definition": "A person or thing that attracts specific attention, feeling, or action.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She became the <w>object</w> of his affection.",
                            "target_lang_translations": {
                                "ru": "Она стала <w>объектом</w> его привязанности."
                            }
                        },
                        {
                            "text": "The strange statue was an <w>object</w> of curiosity for the tourists.",
                            "target_lang_translations": {
                                "ru": "Странная статуя была <w>предметом</w> любопытства туристов."
                            }
                        }
                    ],
                    "synonyms": [
                        "target"
                    ],
                    "common_phrases": [
                        "object of desire",
                        "object of ridicule"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или вещь, привлекающие особое внимание, чувство или действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объект",
                                "target_lang_sense_clarification": "внимания, любви"
                            },
                            {
                                "target_lang_word": "предмет",
                                "target_lang_sense_clarification": "разговора, спора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1df0bf78-c450-309c-9960-2594b885f2ad",
                    "sense_definition": "A noun or phrase in a sentence that receives the action of a verb.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the sentence 'I kicked the ball', 'the ball' is the <w>object</w>.",
                            "target_lang_translations": {
                                "ru": "В предложении «Я пнул мяч», «мяч» — это <w>дополнение</w>."
                            }
                        },
                        {
                            "text": "Transitive verbs always require a direct <w>object</w>.",
                            "target_lang_translations": {
                                "ru": "Переходные глаголы всегда требуют прямого <w>дополнения</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "direct object",
                        "indirect object"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существительное или фраза в предложении, на которую направлено действие глагола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дополнение",
                                "target_lang_sense_clarification": "член предложения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "493e5b93-13e7-3c14-80b2-2f5d85e35f16",
                    "sense_definition": "A distinct unit of data in computer programming, often containing both variables and functions.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The code creates a new <w>object</w> for each user.",
                            "target_lang_translations": {
                                "ru": "Код создает новый <w>объект</w> для каждого пользователя."
                            }
                        },
                        {
                            "text": "You must define the <w>object</w> before you can use it in the script.",
                            "target_lang_translations": {
                                "ru": "Вы должны определить <w>объект</w>, прежде чем сможете использовать его в скрипте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "object-oriented programming"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная единица данных в программировании, часто содержащая как переменные, так и функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объект",
                                "target_lang_sense_clarification": "в программировании"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8fd45e0a-beb8-3ae4-b06f-42a14b9cf912",
                    "sense_definition": "To express disagreement with or opposition to something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>object</w> to being treated like a child.",
                            "target_lang_translations": {
                                "ru": "Я <w>возражаю</w> против того, чтобы со мной обращались как с ребенком."
                            }
                        },
                        {
                            "text": "No one <w>objected</w> to the plan, so we proceeded immediately.",
                            "target_lang_translations": {
                                "ru": "Никто не <w>возразил</w> против плана, поэтому мы немедленно приступили к делу."
                            }
                        }
                    ],
                    "synonyms": [
                        "take exception",
                        "demur"
                    ],
                    "common_phrases": [
                        "object to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать несогласие или противодействие чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возражать",
                                "target_lang_sense_clarification": "высказывать несогласие"
                            },
                            {
                                "target_lang_word": "протестовать",
                                "target_lang_sense_clarification": "выражать решительное несогласие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
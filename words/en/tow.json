    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e2774cdc-8194-3404-87dc-2483375092c6",
                    "sense_definition": "To pull a vehicle, boat, or other object behind another vehicle using a rope, chain, or bar.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "towing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If your car breaks down on the highway, you will need to call someone to <w>tow</w> it to a garage.",
                            "target_lang_translations": {
                                "ru": "Если ваша машина сломается на шоссе, вам нужно будет вызвать кого-нибудь, чтобы <w>отбуксировать</w> её в автосервис."
                            }
                        },
                        {
                            "text": "The tugboat began to <w>tow</w> the large barge out of the harbor.",
                            "target_lang_translations": {
                                "ru": "Буксир начал <w>тянуть</w> большую баржу из гавани."
                            }
                        }
                    ],
                    "antonyms": [
                        "push"
                    ],
                    "common_phrases": [
                        "tow away",
                        "tow a car",
                        "tow a trailer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тянуть транспортное средство, лодку или другой объект за другим транспортным средством с помощью веревки, цепи или жесткой сцепки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "буксировать",
                                "target_lang_sense_clarification": "транспортировать на буксире"
                            },
                            {
                                "target_lang_word": "тянуть",
                                "target_lang_sense_clarification": "перемещать с усилием"
                            },
                            {
                                "target_lang_word": "тащить",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34a70040-44b4-37d4-b12c-858efec0fed0",
                    "sense_definition": "To drive directly in front of another vehicle to shield it from wind resistance and help it gain speed.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "slipstream",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lead driver agreed to <w>tow</w> his teammate down the long straight to improve his qualifying time.",
                            "target_lang_translations": {
                                "ru": "Ведущий гонщик согласился <w>тащить</w> своего товарища по команде на длинной прямой, чтобы улучшить его время в квалификации."
                            }
                        },
                        {
                            "text": "In cycling, riders often take turns to <w>tow</w> the group and save energy for the others.",
                            "target_lang_translations": {
                                "ru": "В велоспорте гонщики часто по очереди <w>тащат</w> группу, чтобы сберечь силы остальным."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "racing context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ехать непосредственно перед другим транспортным средством, чтобы защитить его от сопротивления ветра и помочь ему набрать скорость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тащить",
                                "target_lang_sense_clarification": "спортивный жаргон"
                            },
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "лидировать в гонке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ab1f8415-0f1b-3f47-85c9-d1e2a3f424ad",
                    "sense_definition": "The act of pulling a vehicle or object behind another, or the assistance provided by a tow truck.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "towing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My engine wouldn't start, so I had to call the insurance company for a <w>tow</w>.",
                            "target_lang_translations": {
                                "ru": "Мой двигатель не заводился, поэтому мне пришлось позвонить в страховую компанию, чтобы заказать <w>буксировку</w>."
                            }
                        },
                        {
                            "text": "The captain radioed the port authority to request a <w>tow</w> into the dock.",
                            "target_lang_translations": {
                                "ru": "Капитан связался по радио с администрацией порта, чтобы запросить <w>буксировку</w> в док."
                            }
                        }
                    ],
                    "common_phrases": [
                        "need a tow",
                        "give a tow",
                        "call for a tow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по буксировке транспортного средства или помощь, предоставляемая эвакуатором."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "буксировка",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "буксир",
                                "target_lang_sense_clarification": "помощь в транспортировке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8eb67e5c-c5ee-3f77-84d6-da6889e0f22a",
                    "sense_definition": "A rope, cable, or chain used for pulling a vehicle or vessel.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "towing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy <w>tow</w> snapped under the strain of pulling the stuck truck.",
                            "target_lang_translations": {
                                "ru": "Тяжелый <w>буксировочный трос</w> лопнул от напряжения при вытягивании застрявшего грузовика."
                            }
                        },
                        {
                            "text": "Sailors secured the <w>tow</w> to the bow of the boat before starting the engine.",
                            "target_lang_translations": {
                                "ru": "Моряки закрепили <w>буксир</w> на носу лодки перед запуском двигателя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on tow",
                        "under tow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Веревка, трос или цепь, используемые для буксировки транспортного средства или судна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "буксир",
                                "target_lang_sense_clarification": "трос"
                            },
                            {
                                "target_lang_word": "буксировочный трос",
                                "target_lang_sense_clarification": "специальный кабель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99e191e0-471f-37d8-adc4-35e4ec2fa6ae",
                    "sense_definition": "An increase in speed gained by driving closely behind another vehicle to reduce wind resistance.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "slipstream",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He managed to overtake the leader by catching a <w>tow</w> on the final straight.",
                            "target_lang_translations": {
                                "ru": "Ему удалось обогнать лидера, сев ему на <w>буксир</w> на финишной прямой."
                            }
                        },
                        {
                            "text": "Without a <w>tow</w> from the car ahead, he couldn't reach his top speed.",
                            "target_lang_translations": {
                                "ru": "Без <w>воздушного мешка</w> от идущей впереди машины он не мог развить максимальную скорость."
                            }
                        }
                    ],
                    "common_phrases": [
                        "catch a tow",
                        "get a tow"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "racing context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличение скорости, получаемое за счет движения вплотную за другим транспортным средством для снижения сопротивления ветра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воздушный мешок",
                                "target_lang_sense_clarification": "аэродинамический эффект"
                            },
                            {
                                "target_lang_word": "буксир",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "171d235f-b214-3032-ae44-603bf5a9bc40",
                    "sense_definition": "Short, coarse fibres of flax, hemp, or jute that are separated from the finer fibres and used for making coarse cloth or rope.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fibres",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The spinner separated the fine linen from the rough <w>tow</w>.",
                            "target_lang_translations": {
                                "ru": "Прядильщик отделял тонкий лен от грубой <w>пакли</w>."
                            }
                        },
                        {
                            "text": "In the past, <w>tow</w> was often used to caulk the seams of wooden ships.",
                            "target_lang_translations": {
                                "ru": "В прошлом <w>пакля</w> часто использовалась для конопачения швов деревянных кораблей."
                            }
                        }
                    ],
                    "synonyms": [
                        "hards",
                        "oakum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткие грубые волокна льна, конопли или джута, отделенные от более тонких волокон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пакля",
                                "target_lang_sense_clarification": "грубое волокно"
                            },
                            {
                                "target_lang_word": "кудель",
                                "target_lang_sense_clarification": "волокно для прядения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9b96eb3-ee76-338c-9427-83f6a6f360ee",
                    "sense_definition": "A type of anti-tank missile that is tube-launched, optically tracked, and wire-guided.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "missile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers set up a <w>TOW</w> to defend the position against armored vehicles.",
                            "target_lang_translations": {
                                "ru": "Солдаты установили <w>ПТУР</w> для защиты позиции от бронетехники."
                            }
                        },
                        {
                            "text": "Military supplies included several <w>TOWs</w> for the infantry unit.",
                            "target_lang_translations": {
                                "ru": "Военные поставки включали несколько <w>ПТУР</w> для пехотного подразделения."
                            }
                        }
                    ],
                    "synonyms": [
                        "TOW missile"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "military"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип противотанковой управляемой ракеты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ПТУР",
                                "target_lang_sense_clarification": "военный термин"
                            },
                            {
                                "target_lang_word": "ТОУ",
                                "target_lang_sense_clarification": "название системы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "1d713969-fb29-3224-8bba-edb1c6f5a040",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Tow</w> has lived in this neighborhood for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Тоу</w> живет в этом районе уже более двадцати лет."
                            }
                        },
                        {
                            "text": "We are looking for a file belonging to a client named <w>Tow</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ищем файл, принадлежащий клиенту по фамилии <w>Тоу</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {

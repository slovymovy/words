            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0a37998e-63a3-3430-a95e-a524cf118148",
                    "sense_definition": "Iemand of iets ergens aan laten deelnemen of erin mengen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar probeert alle leerlingen actief bij de les te <w>betrekken</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель старается активно <w>вовлекать</w> всех учеников в урок."
                            }
                        },
                        {
                            "text": "Wij willen graag de buurtbewoners bij het nieuwe project <w>betrekken</w>.",
                            "target_lang_translations": {
                                "ru": "Мы хотели бы <w>привлечь</w> местных жителей к новому проекту."
                            }
                        }
                    ],
                    "synonyms": [
                        "erbij halen",
                        "inschakelen",
                        "mengen"
                    ],
                    "antonyms": [
                        "buitensluiten",
                        "uitsluiten"
                    ],
                    "common_phrases": [
                        "iemand ergens bij betrekken",
                        "nauw betrokken zijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вовлекать кого-либо в деятельность или заставлять участвовать в чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вовлекать",
                                "target_lang_sense_clarification": "побуждать к участию"
                            },
                            {
                                "target_lang_word": "привлекать",
                                "target_lang_sense_clarification": "приглашать к участию в деле"
                            },
                            {
                                "target_lang_word": "приобщать",
                                "target_lang_sense_clarification": "знакомить, включать в круг участников"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a63a5970-bcdc-367d-bfcf-6cfb986c45ba",
                    "sense_definition": "In verband brengen met iets anders; toepassen op.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet die algemene kritiek niet direct op jezelf <w>betrekken</w>.",
                            "target_lang_translations": {
                                "ru": "Ты не должен сразу <w>относить</w> эту общую критику к себе."
                            }
                        },
                        {
                            "text": "De spreker wist het onderwerp goed op de actualiteit te <w>betrekken</w>.",
                            "target_lang_translations": {
                                "ru": "Оратор сумел удачно <w>увязать</w> тему с текущими событиями."
                            }

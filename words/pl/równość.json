                {
                    "sense_id": "228eb9ca-56f1-3989-bab3-f27231364d7d",
                    "sense_definition": "Cecha powierzchni, która jest gładka, płaska i pozbawiona wybojów",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_attribute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Inżynierowie dokładnie sprawdzali <w>równość</w> nowo wylanego asfaltu na autostradzie.",
                            "target_lang_translations": {
                                "ru": "Инженеры тщательно проверяли <w>ровность</w> недавно уложенного асфальта на автостраде."
                            }

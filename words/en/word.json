                    "synonyms": [
                        "vocable",
                        "lexeme"
                    ],
                    "common_phrases": [
                        "in other words",
                        "word for word",
                        "by word of mouth",
                        "too for words"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наименьшая единица языка, имеющая определенное значение и способная употребляться самостоятельно, будь то в устной или письменной речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сло́во",
                                "target_lang_sense_clarification": "единица языка"

            "senses": [
                {
                    "sense_id": "6adf0dde-5dc5-3c8f-8bdf-ca48c428a21c",
                    "sense_definition": "Nieokreślona osoba, nieznany człowiek; słowo używane, gdy nie wiemy, o kogo chodzi, lub nie chcemy podawać tożsamości.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "indefinite_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy <ktoś> mógłby tu posprzątać?",
                            "target_lang_translations": {
                                "ru": "Может ли <w>кто-нибудь</w> здесь убраться?"
                            }
                        },
                        {
                            "text": "Czekam, aż <ktoś> odbierze telefon w recepcji.",
                            "target_lang_translations": {
                                "ru": "Я жду, когда <w>кто-то</w> ответит на звонок на ресепшене."
                            }
                        }
                    ],
                    "synonyms": [
                        "ktokolwiek"
                    ],
                    "antonyms": [
                        "nikt"
                    ],
                    "common_phrases": [
                        "ktoś inny",
                        "ktoś obcy",
                        "ktoś bliski",
                        "ktoś taki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неопределённое лицо, неизвестный человек; слово используется, когда мы не знаем, о ком идет речь, или не хотим раскрывать личность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кто-то",
                                "target_lang_sense_clarification": "конкретный, но неизвестный или неназываемый человек"
                            },
                            {
                                "target_lang_word": "кто-нибудь",
                                "target_lang_sense_clarification": "любой, безразлично кто (часто в вопросах и просьбах)"
                            },
                            {
                                "target_lang_word": "некто",
                                "target_lang_sense_clarification": "некая личность (более книжное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cc30f8a-dc0b-34c7-83d3-ccaa0527e90e",
                    "sense_definition": "Osoba ważna, znacząca, która coś osiągnęła lub cieszy się szacunkiem.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "important_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz wykładowca to jest dopiero <ktoś> – rok temu dostał Nobla!",
                            "target_lang_translations": {
                                "ru": "Наш преподаватель — это <w>личность</w>: год назад он получил Нобелевскую премию!"
                            }
                        },
                        {
                            "text": "Musisz się bardzo starać, żeby zostać <kimś> w świecie biznesu.",
                            "target_lang_translations": {
                                "ru": "Ты должен очень стараться, чтобы стать <w>кем-то</w> в мире бизнеса."
                            }
                        }
                    ],
                    "antonyms": [
                        "nikt"
                    ],
                    "common_phrases": [
                        "być kimś",
                        "uważać się za kogoś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Важная, значимая персона, которая чего-то достигла или пользуется уважением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кто-то",
                                "target_lang_sense_clarification": "в значении «важная персона» (обычно: стать кем-то)"
                            },
                            {
                                "target_lang_word": "личность",
                                "target_lang_sense_clarification": "выдающийся человек"
                            },
                            {
                                "target_lang_word": "человек",
                                "target_lang_sense_clarification": "с большой буквы, значимый человек"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5c9d2b97-fb5f-32d7-a97c-3c3927bb3831",
                    "sense_definition": "Osoba bliżej nieznana lub nieujawniona przez mówiącego, traktowana jako rzeczownik odmienny.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "indefinite_person_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jakiś tajemniczy <ktoś> zostawił ci kwiaty pod drzwiami.",
                            "target_lang_translations": {
                                "ru": "Какой-то таинственный <w>некто</w> оставил тебе цветы под дверью."

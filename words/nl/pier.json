{
    "word_family": [
        "pierenbad",
        "pierenwaaien"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8281b6fa-abd3-3089-b41f-c1b8353642cc",
                    "sense_definition": "Een constructie die vanuit het land de zee of een rivier in loopt, gebruikt als golfbreker, wandelpad of aanlegplaats.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We maakten een lange wandeling over de <w>pier</w> van Scheveningen.",
                            "target_lang_translations": {
                                "ru": "Мы совершили долгую прогулку по <w>пирсу</w> Схевенингена."
                            }
                        },
                        {
                            "text": "De vissersboot legde aan bij de stenen <w>pier</w> om de vangst te lossen.",
                            "target_lang_translations": {
                                "ru": "Рыбацкая лодка причалила к каменному <w>молу</w>, чтобы выгрузить улов."
                            }
                        }
                    ],
                    "synonyms": [
                        "havenhoofd",
                        "steiger",
                        "golfbreker"
                    ],
                    "common_phrases": [
                        "de pier opgaan",
                        "wandelen op de pier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сооружение, уходящее от берега в море или реку, используемое как волнорез, прогулочная зона или причал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пирс",
                                "target_lang_sense_clarification": "сооружение над водой"
                            },
                            {
                                "target_lang_word": "мол",
                                "target_lang_sense_clarification": "волнорез"
                            },
                            {
                                "target_lang_word": "причал",
                                "target_lang_sense_clarification": "место швартовки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "403edb12-96ec-3e70-bf53-59bd0317f7d2",
                    "sense_definition": "Een overdekte loopbrug op een luchthaven die de terminal met een vliegtuig verbindt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Passagiers moeten zich naar <w>pier</w> D begeven voor hun vlucht naar Londen.",
                            "target_lang_translations": {
                                "ru": "Пассажиры должны пройти на <w>пирс</w> D для посадки на рейс в Лондон."
                            }
                        },
                        {
                            "text": "Vanaf de <w>pier</w> konden we het vliegtuig al zien staan.",
                            "target_lang_translations": {
                                "ru": "С <w>пирса</w> мы уже видели стоящий самолет."
                            }
                        }
                    ],
                    "synonyms": [
                        "gate",
                        "loopbrug"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крытый переход в аэропорту, соединяющий терминал с самолетом или выходами на посадку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пирс",
                                "target_lang_sense_clarification": "часть терминала аэропорта"
                            },
                            {
                                "target_lang_word": "галерея",
                                "target_lang_sense_clarification": "посадочный переход"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3646d628-6cc5-3f94-a7d3-bcdd5b9aa99b",
                    "sense_definition": "Een regenworm, een langwerpig diertje dat in de aarde leeft; vaak gebruikt als aas bij het vissen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De visser deed een dikke <w>pier</w> aan de haak om een vis te vangen.",
                            "target_lang_translations": {
                                "ru": "Рыбак насадил жирного <w>червяка</w> на крючок, чтобы поймать рыбу."
                            }
                        },
                        {
                            "text": "Na de zware regenbui kroop er een <w>pier</w> over de natte stoep.",
                            "target_lang_translations": {
                                "ru": "После сильного ливня по мокрому тротуару полз <w>дождевой червь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "regenworm",
                        "worm"
                    ],
                    "common_phrases": [
                        "zo dood als een pier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дождевой червь, обитающий в земле; часто используется как наживка при рыбалке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дождевой червь",
                                "target_lang_sense_clarification": "животное"
                            },
                            {
                                "target_lang_word": "червяк",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "f654c1cc-61bf-363b-82e3-ded8f9d8e569",
                    "sense_definition": "Een jongensnaam, oorspronkelijk een verkleinvorm van Pieter.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mijn buurman heet <w>Pier</w> en hij komt uit Friesland.",
                            "target_lang_translations": {
                                "ru": "Моего соседа зовут <w>Пир</w>, и он родом из Фрисландии."
                            }
                        },
                        {
                            "text": "<w>Pier</w> en Jan gaan samen naar de voetbalwedstrijd.",
                            "target_lang_translations": {
                                "ru": "<w>Пир</w> и Ян вместе идут на футбольный матч."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя, изначально уменьшительная форма от имени Питер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пир",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d8df7929-74f5-38ed-a099-3b3f3ba9873a",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>pieren</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>pier</w> met mijn ogen tegen het felle zonlicht.",
                            "target_lang_translations": {
                                "ru": "Я <w>щурюсь</w> от яркого солнечного света."
                            }
                        },
                        {
                            "text": "Als ik mijn bril niet op heb, <w>pier</w> ik om de letters te kunnen lezen.",
                            "target_lang_translations": {
                                "ru": "Когда я без очков, я <w>щурюсь</w>, чтобы разглядеть буквы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени глагола 'pieren' (щуриться, всматриваться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щурюсь",
                                "target_lang_sense_clarification": "сужать глаза"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27e071c1-a72e-34d7-ac79-73d7d213e67d",
                    "sense_definition": "Gebiedende wijs van <w>pieren</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Pier</w> niet zo naar dat kleine schermpje, zet je bril op!",
                            "target_lang_translations": {
                                "ru": "Не <w>щурься</w> так на этот маленький экран, надень очки!"
                            }
                        },
                        {
                            "text": "<w>Pier</w> door het sleutelgat om te zien wie er binnen is.",
                            "target_lang_translations": {
                                "ru": "<w>Загляни</w> в замочную скважину, чтобы увидеть, кто внутри."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола 'pieren'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щурься",
                                "target_lang_sense_clarification": "не сужай глаза"
                            },
                            {
                                "target_lang_word": "всматривайся",
                                "target_lang_sense_clarification": "гляди пристально"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
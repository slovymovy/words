            "senses": [
                {
                    "sense_id": "958f3050-fd89-395d-bfc4-c6f9df8547a4",
                    "sense_definition": "Een toestand van grote rijkdom of het uitbundig uitgeven van geld.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",

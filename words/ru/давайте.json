    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "36f8962b-c80f-371f-a05c-334c1896b18e",
                    "sense_definition": "Используется для побуждения к совместному действию, соответствует английскому «let's».",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "auxiliary_particle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Давайте</w> пойдём в кино сегодня вечером.",
                            "target_lang_translations": {
                                "en": "<w>Let's</w> go to the cinema tonight."
                            }
                        },
                        {
                            "text": "<w>Давайте</w> говорить только по-русски.",
                            "target_lang_translations": {
                                "en": "<w>Let's</w> speak only Russian."
                            }
                        }
                    ],
                    "common_phrases": [
                        "давайте познакомимся",
                        "давайте жить дружно",
                        "давайте начнём"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to encourage joint action, corresponds to English \"let's\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let's",
                                "target_lang_sense_clarification": "suggestion for joint action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b55c4ea-4468-3dc6-9a52-7eb617e57a3b",
                    "sense_definition": "Форма повелительного наклонения глагола <w>давать</w> во множественном числе или при вежливом обращении.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "action_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Давайте</w> мне вашу руку, здесь скользко.",
                            "target_lang_translations": {
                                "en": "<w>Give</w> me your hand, it's slippery here."
                            }
                        },
                        {
                            "text": "Не <w>давайте</w> собаке сладкое.",
                            "target_lang_translations": {
                                "en": "Don't <w>give</w> the dog sweets."
                            }
                        }
                    ],
                    "synonyms": [
                        "вручайте",
                        "передавайте"
                    ],
                    "antonyms": [
                        "берите",
                        "забирайте"
                    ],
                    "common_phrases": [
                        "давайте сюда",
                        "давайте пять"
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative form of the verb \"давать\" (to give) in plural or polite address."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "imperative plural/polite"
                            },
                            {
                                "target_lang_word": "hand",
                                "target_lang_sense_clarification": "to pass something to someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75ecd7b3-b09f-3af1-b2ea-3a6be13ef4ce",
                    "sense_definition": "Выражает предложение говорящего выполнить действие самостоятельно (помощь, инициатива).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "auxiliary_particle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Давайте</w> я помогу вам донести сумки.",
                            "target_lang_translations": {
                                "en": "<w>Let</w> me help you carry the bags."
                            }
                        },
                        {
                            "text": "<w>Давайте</w> я объясню это правило ещё раз.",
                            "target_lang_translations": {
                                "en": "<w>Let</w> me explain this rule one more time."
                            }
                        }
                    ],
                    "synonyms": [
                        "разрешите",
                        "позвольте"
                    ],
                    "common_phrases": [
                        "давайте я сделаю",
                        "давайте я сам"
                    ],
                    "target_lang_definitions": {
                        "en": "Expresses an offer by the speaker to perform an action independently (help, initiative)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let me",
                                "target_lang_sense_clarification": "offer to do something"
                            },
                            {
                                "target_lang_word": "allow me",
                                "target_lang_sense_clarification": "polite offer"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
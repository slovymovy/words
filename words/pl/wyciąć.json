    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7a99f3b9-67ae-3894-9d8e-93592f4a3e24",
                    "sense_definition": "Usunąć fragment materiału lub nadać czemuś kształt za pomocą ostrego narzędzia, na przykład nożyczek lub noża.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_cutting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <wyciąć> ten kupon z dzisiejszej gazety.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <вырезать> этот купон из сегодняшней газеты."
                            }
                        },
                        {
                            "text": "Dziecko starało się precyzyjnie <wyciąć> serce z czerwonego papieru.",
                            "target_lang_translations": {
                                "ru": "Ребенок старался аккуратно <вырезать> сердце из красной бумаги."
                            }
                        }
                    ],
                    "synonyms": [
                        "wystrzyc"
                    ],
                    "common_phrases": [
                        "wyciąć nożyczkami",
                        "wyciąć z papieru",
                        "wyciąć dziurę",
                        "wyciąć wzór"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалить часть материала или придать чему-либо форму с помощью острого инструмента, например, ножниц или ножа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вырезать",
                                "target_lang_sense_clarification": "ножницами, из бумаги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91098a1f-0662-3918-a704-96925c547721",
                    "sense_definition": "Usunąć zaznaczony fragment tekstu, obrazu lub plik w programie komputerowym, aby przenieść go w inne miejsce (zwykle do schowka).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "digital_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Możesz <wyciąć> ten akapit i wkleić go na końcu dokumentu.",
                            "target_lang_translations": {
                                "ru": "Ты можешь <вырезать> этот абзац и вставить его в конце документа."
                            }
                        },
                        {
                            "text": "Zaznacz plik, który chcesz <wyciąć>, a następnie przenieś go do innego folderu.",
                            "target_lang_translations": {
                                "ru": "Выдели файл, который хочешь <вырезать>, а затем перемести его в другую папку."
                            }
                        }
                    ],
                    "synonyms": [
                        "usunąć"
                    ],
                    "antonyms": [
                        "wkleić"
                    ],
                    "common_phrases": [
                        "wyciąć i wkleić",
                        "wyciąć tekst",
                        "wyciąć plik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалить выделенный фрагмент текста, изображения или файл в программе для перемещения в другое место (обычно в буфер обмена)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вырезать",
                                "target_lang_sense_clarification": "команда в компьютере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77a89639-5e8d-3a89-b8b0-c15c68b276fc",
                    "sense_definition": "Całkowicie usunąć drzewa, krzewy lub inne rośliny z danego obszaru poprzez ich ścięcie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "nature_clearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Władze miasta zdecydowały, że trzeba <wyciąć> stare drzewa w parku ze względów bezpieczeństwa.",
                            "target_lang_translations": {
                                "ru": "Городские власти решили, что нужно <вырубить> старые деревья в парке из соображений безопасности."
                            }
                        },
                        {
                            "text": "Musimy <wyciąć> te gęste krzaki, ponieważ zasłaniają nam widok z okna.",
                            "target_lang_translations": {
                                "ru": "Мы должны <вырубить> эти густые кусты, так как они закрывают нам вид из окна."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykarczować",
                        "wyrąbać"
                    ],
                    "antonyms": [
                        "zasadzić",
                        "posadzić"
                    ],
                    "common_phrases": [
                        "wyciąć las",
                        "wyciąć drzewa",
                        "wyciąć krzaki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью удалить деревья, кустарники или другие растения с определенной территории путем их спиливания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вырубить",
                                "target_lang_sense_clarification": "деревья, лес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a325e30-d372-39b9-9e14-abecd1c0cf0b",
                    "sense_definition": "Ograniczyć kadr zdjęcia lub grafiki, usuwając jego zbędne zewnętrzne fragmenty.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "image_editing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Grafik musiał <wyciąć> postać z tła, aby umieścić ją na plakacie.",
                            "target_lang_translations": {
                                "ru": "Графический дизайнер должен был <вырезать> фигуру из фона, чтобы поместить её на плакат."
                            }
                        },
                        {
                            "text": "W tym programie możesz łatwo <wyciąć> niechciane elementy ze zdjęcia.",
                            "target_lang_translations": {
                                "ru": "В этой программе можно легко <вырезать> нежелательные элементы с фотографии."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykadrować"
                    ],
                    "common_phrases": [
                        "wyciąć tło",
                        "wyciąć ze zdjęcia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограничить кадр изображения или удалить лишние внешние фрагменты в графическом редакторе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вырезать",
                                "target_lang_sense_clarification": "объект на фото, фон"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
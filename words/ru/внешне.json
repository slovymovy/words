    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "5c645a77-a8d1-3fce-8f33-653b0da0677e",
                    "sense_definition": "Судя по наружному виду, по внешности.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хотя <w>внешне</w> всё выглядело благополучно, проблемы в семье оставались.",
                            "target_lang_translations": {
                                "en": "Although <w>outwardly</w> everything looked fine, problems in the family remained."
                            }
                        },
                        {
                            "text": "В этом костюме он <w>внешне</w> очень похож на своего дедушку.",
                            "target_lang_translations": {
                                "en": "In this suit, he is <w>outwardly</w> very similar to his grandfather."
                            }
                        },
                        {
                            "text": "<w>Внешне</w> он спокоен, но внутри сильно переживает.",
                            "target_lang_translations": {
                                "en": "<w>Outwardly</w> he is calm, but inside he is deeply worried."
                            }
                        }
                    ],
                    "synonyms": [
                        "с виду"
                    ],
                    "antonyms": [
                        "внутренне"
                    ],
                    "common_phrases": [
                        "внешне похож",
                        "выглядеть внешне",
                        "внешне спокойный",
                        "измениться внешне"
                    ],
                    "target_lang_definitions": {
                        "en": "Judging by the outward appearance or looks; often contrasted with internal reality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "outwardly",
                                "target_lang_sense_clarification": "regarding the external appearance, especially in contrast to inner feelings"
                            },
                            {
                                "target_lang_word": "externally",
                                "target_lang_sense_clarification": "relating to the outside or surface"
                            },
                            {
                                "target_lang_word": "seemingly",
                                "target_lang_sense_clarification": "appearing to be the case, though possibly not in reality"
                            },
                            {
                                "target_lang_word": "in appearance",
                                "target_lang_sense_clarification": "with regard to how someone or something looks"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5e050ef5-a1e6-33a9-8f9b-e88f4fd7025d",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>внешний</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это сходство лишь <w>внешне</w>, а по сути они совершенно разные.",
                            "target_lang_translations": {
                                "en": "This resemblance is only <w>external</w>, but in essence they are completely different."
                            }
                        },
                        {
                            "text": "Здание <w>внешне</w> и неприглядно, но очень удобно внутри.",
                            "target_lang_translations": {
                                "en": "The building is <w>on the outside</w> unsightly, but very comfortable inside."
                            }
                        }
                    ],

            "senses": [
                {
                    "sense_id": "ad38f565-924e-322b-aea3-41889c690f03",
                    "sense_definition": "Выдающийся, блестящий успех, значительная победа или торжество.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Премьера нового спектакля обернулась полным <w>триумфом</w> для всей труппы.",
                            "target_lang_translations": {
                                "en": "The premiere of the new play turned out to be a complete <w>triumph</w> for the entire troupe."
                            }
                        },
                        {
                            "text": "Спортсмен вернулся на родину с <w>триумфом</w> после победы на чемпионате мира.",
                            "target_lang_translations": {
                                "en": "The athlete returned to his homeland in <w>triumph</w> after winning the World Championship."
                            }
                        }
                    ],
                    "synonyms": [
                        "успех",
                        "победа",
                        "фурор",
                        "торжество"
                    ],
                    "antonyms": [
                        "провал",
                        "поражение",
                        "фиаско",
                        "крах"
                    ],
                    "common_phrases": [
                        "полный триумф",
                        "настоящий триумф",
                        "грандиозный триумф"
                    ],
                    "target_lang_definitions": {
                        "en": "An outstanding, brilliant success, a significant victory or celebration."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "triumph",
                                "target_lang_sense_clarification": "great success or victory"
                            },
                            {
                                "target_lang_word": "success",
                                "target_lang_sense_clarification": "achievement of a goal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53f4b76d-45ab-339f-a0ef-aa5bbdd67376",
                    "sense_definition": "Торжественный въезд в столицу полководца-победителя и его войска (в Древнем Риме).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сенат позволил полководцу отпраздновать <w>триумф</w> в Риме.",
                            "target_lang_translations": {
                                "en": "The Senate allowed the general to celebrate a <w>triumph</w> in Rome."
                            }
                        },
                        {
                            "text": "Во время <w>триумфа</w> за колесницей победителя шли пленные враги.",
                            "target_lang_translations": {
                                "en": "During the <w>triumph</w>, captured enemies walked behind the victor's chariot."
                            }
                        }
                    ],
                    "common_phrases": [
                        "римский триумф"
                    ],
                    "target_lang_definitions": {
                        "en": "A ceremonial entry into the capital by a victorious commander and his army (in Ancient Rome)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "triumph",
                                "target_lang_sense_clarification": "Roman ceremonial procession"
                            }
                        ]
                    }
                }
            ]
        }

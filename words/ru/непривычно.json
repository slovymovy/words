                    "sense_definition": "Краткая форма среднего рода прилагательного <w>непривычный</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это ощущение для меня ново и <w>непривычно</w>.",
                            "target_lang_translations": {
                                "en": "This sensation is new and <w>unusual</w> for me."
                            }
                        },
                        {
                            "text": "Поведение сотрудника было странно и <w>непривычно</w> для всех.",
                            "target_lang_translations": {
                                "en": "The employee's behavior was strange and <w>unusual</w> for everyone."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short neuter singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Not common, ordinary, or customary (describing a noun or situation)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unusual",
                                "target_lang_sense_clarification": "not commonly occurring or done"
                            },
                            {
                                "target_lang_word": "unaccustomed",
                                "target_lang_sense_clarification": "not familiar or usual"
                            }
                        ]
                    }
                }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "086a33dc-d0a9-32b7-97b5-ad9faa5b1bf0",
                    "sense_definition": "Umieścić coś ostro zakończonego wewnątrz czegoś innego, używając siły, uderzenia lub nacisku.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz mocno <w>wbić</w> ten gwóźdź w ścianę, żeby obraz nie spadł.",
                            "target_lang_translations": {
                                "ru": "Ты должен сильно <w>вбить</w> этот гвоздь в стену, чтобы картина не упала."
                            }
                        },
                        {
                            "text": "Ogrodnik <w>wbił</w> łopatę w ziemię i zaczął kopać dół pod drzewo.",
                            "target_lang_translations": {
                                "ru": "Садовник <w>воткнул</w> лопату в землю и начал копать яму для дерева."
                            }
                        }
                    ],
                    "synonyms": [
                        "wetknąć",
                        "wsadzić",
                        "osadzić"
                    ],
                    "antonyms": [
                        "wyciągnąć",
                        "wyjąć"
                    ],
                    "common_phrases": [
                        "wbić gwóźdź",
                        "wbić nóż w plecy",
                        "wbić klin",
                        "wbić wzrok w ziemię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить острый предмет внутрь чего-либо, применяя силу, удар или давление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вбить",
                                "target_lang_sense_clarification": "гвоздь, клин, сваю"
                            },
                            {
                                "target_lang_word": "воткнуть",
                                "target_lang_sense_clarification": "лопату, нож, иглу"
                            },
                            {
                                "target_lang_word": "вонзить",
                                "target_lang_sense_clarification": "оружие, когти (более интенсивно)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "125718f2-5189-3272-a5e0-e387fb515c20",
                    "sense_definition": "Wejść do jakiegoś pomieszczenia, odwiedzić kogoś lub dołączyć do wydarzenia, często bez zapowiedzi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Możemy <w>wbić</w> do ciebie wieczorem na pizzę i film?",
                            "target_lang_translations": {
                                "ru": "Мы можем <w>заскочить</w> к тебе вечером на пиццу и фильм?"
                            }
                        },
                        {
                            "text": "Marek postanowił <w>wbić</w> na imprezę, mimo że nie był oficjalnie zaproszony.",
                            "target_lang_translations": {
                                "ru": "Марек решил <w>завалиться</w> на вечеринку, хотя не был официально приглашён."
                            }
                        }
                    ],
                    "synonyms": [
                        "wejść",
                        "wpaść",
                        "odwiedzić"
                    ],
                    "antonyms": [
                        "wyjść"
                    ],
                    "common_phrases": [
                        "wbić na chatę",
                        "wbić na imprezę",
                        "wbić do środka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used commonly in informal speech among young people"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Зайти куда-либо, навестить кого-то или присоединиться к мероприятию, часто без приглашения (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завалиться",
                                "target_lang_sense_clarification": "прийти без церемоний, нагрянуть (разг.)"
                            },
                            {
                                "target_lang_word": "заскочить",
                                "target_lang_sense_clarification": "ненадолго зайти, забежать (разг.)"
                            },
                            {
                                "target_lang_word": "ворваться",
                                "target_lang_sense_clarification": "стремительно войти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ebbcd8a-0b06-3174-9baa-fbc3dfac40bc",
                    "sense_definition": "Dostać się do środka czegoś z trudem, wcisnąć się w tłum lub ubrać się w ciasne ubranie (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po świętach na pewno nie uda mi się <w>wbić</w> w moje ulubione dżinsy.",
                            "target_lang_translations": {
                                "ru": "После праздников мне точно не удастся <w>влезть</w> в мои любимые джинсы."
                            }
                        },
                        {
                            "text": "Pasażerowie próbowali <w>wbić</w> się do zatłoczonego autobusu w godzinach szczytu.",
                            "target_lang_translations": {
                                "ru": "Пассажиры пытались <w>втиснуться</w> в переполненный автобус в час пик."
                            }
                        }
                    ],
                    "synonyms": [
                        "wcisnąć się",
                        "wdrapać się"
                    ],
                    "common_phrases": [
                        "wbić się w garnitur",
                        "wbić się w kolejkę",
                        "wbić się w tłum"
                    ],
                    "target_lang_definitions": {
                        "ru": "С трудом попасть внутрь, протиснуться в толпе или надеть тесную одежду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влезть",
                                "target_lang_sense_clarification": "в одежду, в транспорт"
                            },
                            {
                                "target_lang_word": "втиснуться",
                                "target_lang_sense_clarification": "в толпу, в узкое пространство"
                            }
                        ]
                    }
                }
            ]
        }
    ]

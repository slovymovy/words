    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e3070a3-2c2f-3729-84e9-3ef06e47d17a",
                    "sense_definition": "Część ciała człowieka lub zwierzęcia, zazwyczaj znajdująca się na górze lub z przodu, zawierająca mózg oraz główne narządy zmysłów.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą zawsze noszę ciepłą czapkę na <w>głowie</w>, żeby nie zmarznąć.",
                            "target_lang_translations": {
                                "ru": "Зимой я всегда ношу теплую шапку на <w>голове</w>, чтобы не замерзнуть."
                            }
                        },
                        {
                            "text": "Pielęgniarka delikatnie opatrzyła ranę na jego <w>głowie</w> po wypadku.",
                            "target_lang_translations": {
                                "ru": "Медсестра осторожно перевязала рану на его <w>голове</w> после аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "główka",
                        "czerep",
                        "łeb"
                    ],
                    "antonyms": [
                        "nogi"
                    ],
                    "common_phrases": [
                        "od stóp do głów",
                        "kręcić głową",
                        "kiwać głową",
                        "ból głowy",
                        "mieć coś na głowie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть тела человека или животного, обычно находящаяся вверху или спереди, содержащая мозг и основные органы чувств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голова",
                                "target_lang_sense_clarification": "часть тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca107c33-52f6-389d-8944-f21f6cc53123",
                    "sense_definition": "Siedlisko myśli, pamięci, zdolności intelektualnych i rozsądku; umysł.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę chwilę odpocząć, bo mam dzisiaj straszny mętlik w <w>głowie</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно немного отдохнуть, потому что у меня сегодня страшная каша в <w>голове</w>."
                            }
                        },
                        {
                            "text": "To genialne rozwiązanie w ogóle nie przyszło mi wcześniej do <w>głowy</w>.",
                            "target_lang_translations": {
                                "ru": "Это гениальное решение вообще не приходило мне раньше в <w>голову</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozum",
                        "umysł",
                        "pamięć"
                    ],
                    "common_phrases": [
                        "mieć głowę na karku",
                        "stracić głowę",
                        "przyjść do głowy",
                        "wbić sobie do głowy",
                        "upść na głowę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Средоточие мыслей, памяти, интеллектуальных способностей и рассудка; ум."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голова",
                                "target_lang_sense_clarification": "ум, интеллект"
                            },
                            {
                                "target_lang_word": "ум",
                                "target_lang_sense_clarification": "рассудок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8f81b1e-6097-3384-b9f7-19855b11d732",
                    "sense_definition": "Pojedyncza osoba liczona jako jednostka w grupie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Koszt wycieczki wynosił sto złotych na <w>głowę</w>, co było dobrą ceną.",
                            "target_lang_translations": {
                                "ru": "Стоимость экскурсии составляла сто злотых с <w>человека</w>, что было хорошей ценой."
                            }
                        },
                        {
                            "text": "Organizatorzy przygotowali po dwa napoje na każdą <w>głowę</w> uczestniczącą w spotkaniu.",
                            "target_lang_translations": {
                                "ru": "Организаторы подготовили по два напитка на каждого <w>человека</w>, участвующего во встрече."
                            }
                        }
                    ],
                    "synonyms": [
                        "osoba"
                    ],
                    "common_phrases": [
                        "na głowę",
                        "na łeb na szyję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный человек, считаемый как единица в группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "человек",
                                "target_lang_sense_clarification": "единица счета людей"
                            },
                            {
                                "target_lang_word": "душа",
                                "target_lang_sense_clarification": "единица учета населения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b49453b3-5def-3618-9a62-a2a8ca0b43fb",
                    "sense_definition": "Osoba kierująca jakąś organizacją, państwem, rodziną lub grupą ludzi; przywódca.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "leader",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prezydent jest oficjalną <w>głową</w> państwa i reprezentuje kraj za granicą.",
                            "target_lang_translations": {
                                "ru": "Президент является официальным <w>главой</w> государства и представляет страну за рубежом."
                            }
                        },
                        {
                            "text": "Mój dziadek zawsze był uznawany za niekwestionowaną <w>głowę</w> naszej rodziny.",
                            "target_lang_translations": {
                                "ru": "Моего дедушку всегда считали бесспорным <w>главой</w> нашей семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywódca",
                        "szef",
                        "lider"
                    ],
                    "common_phrases": [
                        "głowa państwa",
                        "głowa rodziny",
                        "głowa kościoła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, возглавляющее организацию, государство, семью или группу людей; лидер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глава",
                                "target_lang_sense_clarification": "руководитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e406679-ad6a-3b17-b2df-c88a72bbaf13",
                    "sense_definition": "Część niektórych roślin, np. kapusty lub sałaty, uformowana w kulisty kształt ze ściśle przylegających liści.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem na targu wielką <w>głowę</w> kapusty, żeby ugotować świąteczny bigos.",
                            "target_lang_translations": {
                                "ru": "Я купил на рынке большой <w>кочан</w> капусты, чтобы приготовить праздничный бигос."
                            }
                        },
                        {
                            "text": "Rolnik załadował na wóz dorodne <w>głowy</w> sałaty prosto z pola.",
                            "target_lang_translations": {
                                "ru": "Фермер погрузил на повозку отборные <w>кочаны</w> салата прямо с поля."
                            }
                        }
                    ],
                    "synonyms": [
                        "główka"
                    ],
                    "common_phrases": [
                        "głowa kapusty",
                        "głowa cukru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть некоторых растений, например капусты или салата, сформированная в шарообразную форму из плотно прилегающих листьев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кочан",
                                "target_lang_sense_clarification": "капусты, салата"
                            },
                            {
                                "target_lang_word": "головка",
                                "target_lang_sense_clarification": "растения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d49418aa-4601-3813-bee5-7fbff26e0d4e",
                    "sense_definition": "Życie człowieka, często w kontekście wielkiego zagrożenia, poświęcenia lub odpowiedzialności.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "life",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz przysiągł, że będzie bronił króla nawet za cenę własnej <w>głowy</w>.",
                            "target_lang_translations": {
                                "ru": "Рыцарь поклялся, что будет защищать короля даже ценой собственной <w>головы</w>."
                            }
                        },
                        {
                            "text": "Jeśli ten plan nie zadziała, ręczę za to moją własną <w>głową</w>.",
                            "target_lang_translations": {
                                "ru": "Если этот план не сработает, я ручаюсь за это своей собственной <w>головой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "życie"
                    ],
                    "common_phrases": [
                        "przypłacić głową",
                        "dać głowę",
                        "ręczyć głową"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane głównie w zwrotach frazeologicznych"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жизнь человека, часто в контексте большой опасности, самопожертвования или ответственности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голова",
                                "target_lang_sense_clarification": "жизнь (в идиомах)"
                            },
                            {
                                "target_lang_word": "жизнь",
                                "target_lang_sense_clarification": "существование"
                            }
                        ]
                    }
                }
            ]
        },
        {

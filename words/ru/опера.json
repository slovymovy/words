                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Местные <опера> быстро раскрыли это запутанное дело.",
                            "target_lang_translations": {
                                "en": "The local <detectives> quickly solved this complicated case."
                            }

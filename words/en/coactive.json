{
    "word_family": [
        "coact",
        "coaction"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "735f6677-5951-3010-8590-8db7be008097",
                    "sense_definition": "Acting together or working in unison to achieve a result.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "acting_together",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>coactive</w> efforts of the different departments led to a quick resolution of the problem.",
                            "target_lang_translations": {
                                "ru": "<w>Совместные</w> усилия различных отделов привели к быстрому решению проблемы."
                            }
                        },
                        {
                            "text": "We observed a <w>coactive</w> effect when the two substances were mixed.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдали эффект <w>совместного действия</w> при смешивании двух веществ."
                            }
                        }
                    ],
                    "synonyms": [
                        "cooperative",
                        "collaborative",
                        "joint",
                        "concurrent"
                    ],
                    "antonyms": [
                        "independent",
                        "separate",
                        "individual"
                    ],
                    "common_phrases": [
                        "coactive effect",
                        "coactive participation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действующий совместно или работающий в унисон для достижения результата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совместный",
                                "target_lang_sense_clarification": "осуществляемый вместе"
                            },
                            {
                                "target_lang_word": "согласованный",
                                "target_lang_sense_clarification": "действующий в унисон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3588c12-229d-3e7d-87fa-65d5bbb784f4",
                    "sense_definition": "Serving to force or constrain someone to do something; compulsory.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "compelling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The state exercises <w>coactive</w> power to ensure that citizens obey the law.",
                            "target_lang_translations": {
                                "ru": "Государство использует <w>принудительную</w> власть для обеспечения соблюдения законов гражданами."
                            }
                        },
                        {
                            "text": "Without any <w>coactive</w> force behind it, the regulation was largely ignored.",
                            "target_lang_translations": {
                                "ru": "Без какой-либо <w>принудительной</w> силы, стоящей за ним, это предписание по большей части игнорировалось."
                            }
                        }
                    ],
                    "synonyms": [
                        "coercive",
                        "compulsory",
                        "mandatory",
                        "restrictive"
                    ],
                    "antonyms": [
                        "voluntary",
                        "optional"
                    ],
                    "common_phrases": [
                        "coactive power",
                        "coactive force"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служащий для принуждения или ограничения кого-либо в действиях; принудительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принудительный",
                                "target_lang_sense_clarification": "основанный на принуждении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
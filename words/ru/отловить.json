    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f1dd4cf9-d554-3210-accd-df9ee9e247a3",
                    "sense_definition": "Поймать, схватить или задержать кого-либо или что-либо (часто подразумевает поиск, преследование или использование специальных средств).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "catch_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Специальная служба должна <w>отловить</w> всех бродячих собак в этом районе для вакцинации.",
                            "target_lang_translations": {
                                "en": "The special service needs to <w>round up</w> all the stray dogs in this area for vaccination."
                            }
                        },
                        {
                            "text": "Полиции удалось быстро <w>отловить</w> сбежавшего преступника на вокзале.",
                            "target_lang_translations": {
                                "en": "The police managed to quickly <w>capture</w> the escaped criminal at the train station."
                            }
                        }
                    ],
                    "synonyms": [
                        "поймать",
                        "изловить",
                        "выловить",
                        "задержать"
                    ],
                    "antonyms": [
                        "выпустить",
                        "отпустить",
                        "упустить"
                    ],
                    "common_phrases": [
                        "отловить зверя",
                        "отловить нарушителя",
                        "отловить момент"
                    ],
                    "target_lang_definitions": {
                        "en": "To catch, seize, or detain someone or something (often implies searching, pursuit, or using special means)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "general action of capturing"
                            },
                            {
                                "target_lang_word": "capture",
                                "target_lang_sense_clarification": "to take into custody or control by force"
                            },
                            {
                                "target_lang_word": "round up",
                                "target_lang_sense_clarification": "to gather animals or people together"
                            },
                            {
                                "target_lang_word": "apprehend",
                                "target_lang_sense_clarification": "formal, specifically for criminals"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "449ec16a-3a6e-3451-979e-affc914726cc",
                    "sense_definition": "Закончить ловлю, завершить процесс поимки или выловить всё необходимое количество.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "catch_completion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рыбаки уже <w>отловили</w> свою норму и начали сматывать удочки.",
                            "target_lang_translations": {
                                "en": "The fishermen had already <w>caught</w> their quota and started reeling in their lines."
                            }
                        },
                        {
                            "text": "Когда мы <w>отловим</w> последних вредителей, можно будет спокойно сажать огород.",
                            "target_lang_translations": {
                                "en": "When we <w>catch</w> the last pests, we can plant the garden in peace."
                            }
                        }
                    ],
                    "synonyms": [
                        "выловить",
                        "наловить"
                    ],
                    "common_phrases": [
                        "отловить норму",
                        "отловить всех"
                    ],
                    "target_lang_definitions": {
                        "en": "To finish fishing or catching, to complete the process of capture, or to catch the required amount."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "to obtain a specific amount or quota"
                            },
                            {
                                "target_lang_word": "finish catching",
                                "target_lang_sense_clarification": "to complete the activity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dadf03b4-9a8d-35d1-a9ab-b3c5ff3d3c5a",
                    "sense_definition": "Провести определённое время, занимаясь ловлей кого-либо или чего-либо.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "catch_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы честно <w>отловили</w> всё утро, но так ничего и не поймали.",
                            "target_lang_translations": {
                                "en": "We honestly <w>fished</w> all morning, but didn't catch anything."
                            }
                        },
                        {
                            "text": "Охотники <w>отловили</w> в лесу целую неделю, прежде чем вернулись домой.",
                            "target_lang_translations": {
                                "en": "The hunters <w>spent</w> a whole week <w>trapping</w> in the forest before returning home."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies spending a duration of time"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To spend a certain amount of time engaged in catching or fishing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spend time fishing",
                                "target_lang_sense_clarification": "focus on the duration of the activity"
                            },
                            {
                                "target_lang_word": "fish",
                                "target_lang_sense_clarification": "used with a duration of time"
                            }
                        ]
                    }
                }
            ]
        }

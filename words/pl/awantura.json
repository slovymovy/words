    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "912c3f83-fbe2-37ef-ab91-ce4c6b36b181",
                    "sense_definition": "Gwałtowna, głośna kłótnia, ostry spór lub bójka, często wywołująca zamieszanie w miejscu publicznym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiedzi znowu zrobili w nocy straszną <w>awanturę</w>, która obudziła cały blok.",
                            "target_lang_translations": {
                                "ru": "Соседи ночью снова устроили ужасный <w>скандал</w>, который разбудил весь дом."
                            }
                        },
                        {
                            "text": "Nie chcę wywoływać niepotrzebnej <w>awantury</w> przy dzieciach.",
                            "target_lang_translations": {
                                "ru": "Я не хочу устраивать ненужной <w>сцены</w> при детях."
                            }
                        }
                    ],
                    "synonyms": [
                        "kłótnia",
                        "sprzeczka",
                        "burda",
                        "draka"
                    ],
                    "common_phrases": [
                        "zrobić awanturę",
                        "szukać awantury",
                        "awantura domowa",
                        "pijacka awantura"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громкая ссора, бурный спор или драка, часто вызывающие переполох в общественном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скандал",
                                "target_lang_sense_clarification": "шумная ссора, перепалка"
                            },
                            {
                                "target_lang_word": "дебош",
                                "target_lang_sense_clarification": "буйное поведение, нарушение порядка"
                            },
                            {
                                "target_lang_word": "сцена",
                                "target_lang_sense_clarification": "бурное выяснение отношений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6db8f880-850d-3c66-bf7c-51f0308f0583",
                    "sense_definition": "Wydarzenie wywołujące powszechne oburzenie, skandal lub zamieszanie, często w kontekście politycznym lub finansowym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "scandal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa ustawa wywołała wielką <w>awanturę</w> w mediach i parlamencie.",
                            "target_lang_translations": {
                                "ru": "Новый закон вызвал большой <w>скандал</w> в СМИ и парламенте."
                            }
                        },
                        {
                            "text": "Cała ta polityczna <w>awantura</w> zaszkodziła wizerunkowi partii.",
                            "target_lang_translations": {
                                "ru": "Весь этот политический <w>скандал</w> навредил имиджу партии."
                            }
                        }
                    ],
                    "synonyms": [
                        "skandal",
                        "afera",
                        "granda"
                    ],
                    "common_phrases": [
                        "awantura polityczna",
                        "wielka awantura",
                        "awantura o pieniądze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Событие, вызывающее всеобщее возмущение, скандал или замешательство, часто в политическом или финансовом контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скандал",
                                "target_lang_sense_clarification": "событие, получившее широкую огласку"
                            },
                            {
                                "target_lang_word": "шумиха",
                                "target_lang_sense_clarification": "бурное обсуждение, ажиотаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79ef0205-7898-3485-b530-123eff9cf545",
                    "sense_definition": "Niezwykłe, ryzykowne zdarzenie lub przygoda (znaczenie spotykane głównie w literaturze).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adventure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody rycerz wyruszył w świat w poszukiwaniu sławy i niejednej <w>awantury</w>.",
                            "target_lang_translations": {
                                "ru": "Молодой рыцарь отправился в странствия в поисках славы и не одной <w>авантюры</w>."
                            }
                        },
                        {
                            "text": "Ich podróż była pełna niebezpiecznych <w>awantur</w> i nagłych zwrotów akcji.",
                            "target_lang_translations": {
                                "ru": "Их путешествие было полно опасных <w>приключений</w> и внезапных поворотов сюжета."
                            }
                        }
                    ],
                    "synonyms": [
                        "przygoda"
                    ],
                    "common_phrases": [
                        "poszukiwacz awantur",
                        "miłosna awantura"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "używane w kontekście historycznym lub literackim"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Необычное, рискованное событие или приключение (значение встречается преимущественно в литературе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "авантюра",
                                "target_lang_sense_clarification": "рискованное похождение"
                            },
                            {
                                "target_lang_word": "приключение",
                                "target_lang_sense_clarification": "захватывающее происшествие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

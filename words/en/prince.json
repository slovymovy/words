    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "db32be7d-12a0-3863-bf42-ea1e2915a9b8",
                    "sense_definition": "A male member of a royal family, especially the son or grandson of a king or queen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "royalty_member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young <w>prince</w> waved to the crowd from the palace balcony.",
                            "target_lang_translations": {
                                "ru": "Молодой <w>принц</w> помахал толпе с балкона дворца."
                            }
                        },
                        {
                            "text": "Everyone hoped that the <w>prince</w> would one day become a wise king.",
                            "target_lang_translations": {
                                "ru": "Все надеялись, что однажды <w>принц</w> станет мудрым королем."
                            }
                        }
                    ],
                    "synonyms": [
                        "royal"
                    ],
                    "antonyms": [
                        "commoner"
                    ],
                    "common_phrases": [
                        "Crown Prince",
                        "Prince Charming",
                        "Prince of Wales"
                    ],
                    "target_lang_definitions": {
                        "ru": "Член королевской семьи мужского пола, особенно сын или внук короля или королевы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принц",
                                "target_lang_sense_clarification": "член королевской семьи"
                            },
                            {
                                "target_lang_word": "царевич",
                                "target_lang_sense_clarification": "сын царя"
                            },
                            {
                                "target_lang_word": "королевич",
                                "target_lang_sense_clarification": "сын короля (устар., поэт.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f83dd155-3a5e-3155-8912-1d7227e84945",
                    "sense_definition": "A male ruler or monarch of a country or state.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ruler",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Machiavelli wrote a famous book about how a <w>prince</w> should maintain power.",
                            "target_lang_translations": {
                                "ru": "Макиавелли написал известную книгу о том, как <w>государь</w> должен удерживать власть."
                            }
                        },
                        {
                            "text": "The <w>prince</w> governed his lands with absolute authority.",
                            "target_lang_translations": {
                                "ru": "<w>Князь</w> правил своими землями, обладая абсолютной властью."
                            }
                        }
                    ],
                    "synonyms": [
                        "monarch",
                        "ruler",
                        "sovereign",
                        "king"
                    ],
                    "antonyms": [
                        "subject"
                    ],
                    "common_phrases": [
                        "merchant prince",
                        "prince of peace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Правитель или монарх страны или государства мужского пола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "государь",
                                "target_lang_sense_clarification": "монарх, правитель"
                            },
                            {
                                "target_lang_word": "правитель",
                                "target_lang_sense_clarification": "глава государства"
                            },
                            {
                                "target_lang_word": "князь",
                                "target_lang_sense_clarification": "феодальный правитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8cd59ce-4ddf-3f2e-864b-7c0b0731a361",
                    "sense_definition": "The male ruler or head of a principality.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "ruler",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>prince</w> of Monaco attended the international gala.",
                            "target_lang_translations": {
                                "ru": "<w>Князь</w> Монако посетил международный гала-концерт."
                            }
                        },
                        {
                            "text": "As the head of a small state, the <w>prince</w> had to navigate complex diplomatic relations.",
                            "target_lang_translations": {
                                "ru": "Как глава небольшого государства, <w>князь</w> должен был разбираться в сложных дипломатических отношениях."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Правитель или глава княжества мужского пола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "князь",
                                "target_lang_sense_clarification": "правитель княжества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ee08310-2174-3157-9093-b119747e8c93",
                    "sense_definition": "A man who is considered the best, most important, or most talented in a particular group or field.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_best",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was considered a <w>prince</w> among jazz musicians for his incredible talent.",
                            "target_lang_translations": {
                                "ru": "Его считали <w>королем</w> среди джазовых музыкантов за его невероятный талант."
                            }
                        },
                        {
                            "text": "She described her late husband as a <w>prince</w> of a man, kind and generous to everyone.",
                            "target_lang_translations": {
                                "ru": "Она описывала своего покойного мужа как <w>идеал</w> мужчины, доброго и щедрого ко всем."
                            }
                        }
                    ],
                    "synonyms": [
                        "master",
                        "leader",
                        "giant"
                    ],
                    "common_phrases": [
                        "prince among men"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to praise someone highly"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который считается лучшим, самым важным или талантливым в определенной группе или области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "король",
                                "target_lang_sense_clarification": "лучший в своем деле"
                            },
                            {
                                "target_lang_word": "мастер",
                                "target_lang_sense_clarification": "выдающийся специалист"
                            },
                            {
                                "target_lang_word": "идеал",
                                "target_lang_sense_clarification": "совершенный образец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "185a6a1f-e0c2-3794-87ac-540c8bb38145",
                    "sense_definition": "A high title of nobility used in some European countries, ranking below a king but often above a duke.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "nobility_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Russian <w>prince</w> lost his estates during the revolution.",
                            "target_lang_translations": {
                                "ru": "Русский <w>князь</w> потерял свои поместья во время революции."
                            }
                        },
                        {
                            "text": "Although he held the title of <w>prince</w>, he had no political power in the modern republic.",
                            "target_lang_translations": {
                                "ru": "Хотя он носил титул <w>князя</w>, в современной республике у него не было политической власти."
                            }
                        }
                    ],
                    "synonyms": [
                        "nobleman",
                        "aristocrat"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "historical European contexts like France or Holy Roman Empire"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокий дворянский титул в некоторых европейских странах, рангом ниже короля, но часто выше герцога."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "князь",
                                "target_lang_sense_clarification": "дворянский титул"
                            }
                        ]
                    }
                }
            ]
        },
        {

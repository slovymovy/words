    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0ec8f82-674a-3016-be8f-4cf24fd0fa77",
                    "sense_definition": "Задняя часть туловища человека от шеи до поясницы или верхняя часть тела животного.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня сильно болит <w>спина</w> после долгой работы.",
                            "target_lang_translations": {
                                "en": "My <w>back</w> hurts a lot after working for a long time."
                            }
                        },
                        {
                            "text": "Он повернулся ко мне <w>спиной</w> и смотрел в окно.",
                            "target_lang_translations": {
                                "en": "He turned his <w>back</w> to me and looked out the window."
                            }
                        }
                    ],
                    "antonyms": [
                        "грудь",
                        "брюхо"
                    ],
                    "common_phrases": [
                        "за спиной",
                        "нож в спину",
                        "гнуть спину",
                        "повернуться спиной",
                        "прятаться за спину"
                    ],
                    "target_lang_definitions": {
                        "en": "The rear part of the human body from the neck to the lower back, or the upper part of an animal's body."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "back",
                                "target_lang_sense_clarification": "body part"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9cfb0ef-d466-33de-8dbc-b08156ffb2f1",
                    "sense_definition": "Позвоночник, хребет; центральная костная структура, поддерживающая тело.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач сказал, что у пациента повреждена <w>спина</w>.",
                            "target_lang_translations": {
                                "en": "The doctor said the patient's <w>back</w> is injured."
                            }
                        },
                        {
                            "text": "Гимнастика очень полезна для укрепления <w>спины</w>.",
                            "target_lang_translations": {
                                "en": "Gymnastics is very useful for strengthening the <w>back</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "позвоно́чник",
                        "хребе́т"
                    ],
                    "target_lang_definitions": {
                        "en": "The backbone or spine; the central bony structure supporting the body."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spine",
                                "target_lang_sense_clarification": "anatomy"
                            },
                            {
                                "target_lang_word": "backbone",
                                "target_lang_sense_clarification": "bone structure"
                            },
                            {
                                "target_lang_word": "back",
                                "target_lang_sense_clarification": "general reference"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0dec2cd3-05e9-310f-9f70-f2ccfcdfff46",
                    "sense_definition": "Часть одежды, которая закрывает спину.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На <w>спине</w> его куртки была яркая надпись.",
                            "target_lang_translations": {
                                "en": "There was a bright inscription on the <w>back</w> of his jacket."
                            }
                        },
                        {
                            "text": "Портной аккуратно разгладил ткань на <w>спине</w> пальто.",
                            "target_lang_translations": {
                                "en": "The tailor carefully smoothed the fabric on the <w>back</w> of the coat."
                            }
                        }
                    ],
                    "synonyms": [
                        "спинка"
                    ],
                    "target_lang_definitions": {
                        "en": "The part of a piece of clothing that covers the back."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "back",
                                "target_lang_sense_clarification": "of a garment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d4077aa-0ef2-3a4b-ae59-66bfbdd01a89",
                    "sense_definition": "Верхняя или обратная сторона предмета, транспортного средства.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Самолёт сделал резкий вираж и перевернулся на <w>спину</w>.",
                            "target_lang_translations": {
                                "en": "The plane made a sharp turn and flipped onto its <w>back</w>."
                            }
                        },
                        {
                            "text": "Пилот мастерски положил истребитель на <w>спину</w>.",
                            "target_lang_translations": {
                                "en": "The pilot skillfully put the fighter jet on its <w>back</w>."
                            }

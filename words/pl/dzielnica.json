    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "af89572d-d9e0-3cd5-ac37-eae95c80ebc8",
                    "sense_definition": "Część dużego miasta, wyodrębniona administracyjnie lub zwyczajowo, często posiadająca własną nazwę i charakter.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "city_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja <w>dzielnica</w> jest bardzo spokojna i pełna zieleni.",
                            "target_lang_translations": {
                                "ru": "Мой <w>район</w> очень спокойный и утопает в зелени."
                            }
                        },
                        {
                            "text": "W tej części miasta powstaje nowa <w>dzielnica</w> mieszkaniowa.",
                            "target_lang_translations": {
                                "ru": "В этой части города строится новый жилой <w>район</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "region"
                    ],
                    "common_phrases": [
                        "dzielnica mieszkaniowa",
                        "dzielnica przemysłowa",
                        "niebezpieczna dzielnica",
                        "dzielnica willowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть большого города, выделенная административно или по традиции, часто имеющая собственное название и характер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "район",
                                "target_lang_sense_clarification": "административная или условная часть города"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e044fc5-244f-32e7-9c90-83704a905fda",
                    "sense_definition": "Duży obszar kraju wyróżniający się odrębną historią, kulturą, geografią lub etnografią.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "region",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Śląsk to ważna <w>dzielnica</w> przemysłowa Polski.",
                            "target_lang_translations": {
                                "ru": "Силезия — это важный промышленный <w>регион</w> Польши."
                            }
                        },
                        {
                            "text": "Każda historyczna <w>dzielnica</w> kraju ma swoje unikalne tradycje.",
                            "target_lang_translations": {
                                "ru": "У каждой исторической <w>области</w> страны есть свои уникальные традиции."
                            }
                        }
                    ],
                    "synonyms": [
                        "region",
                        "część",
                        "część"
                    ],
                    "common_phrases": [
                        "dzielnica kraju",
                        "dzielnica historyczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная территория страны, отличающаяся особой историей, культурой, географией или этнографией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "область",
                                "target_lang_sense_clarification": "крупная часть страны или территории"
                            },
                            {
                                "target_lang_word": "регион",
                                "target_lang_sense_clarification": "территория с уникальными характеристиками"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1124df65-ef94-3195-8e8e-2cf1ed63bfe8",
                    "sense_definition": "Niezależne księstwo w średniowiecznej Polsce, powstałe w wyniku podziału państwa między synów władcy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_division",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy z synów króla otrzymał własną <w>dzielnicę</w> do rządzenia.",
                            "target_lang_translations": {
                                "ru": "Каждый из сыновей короля получил в правление собственный <w>удел</w>."
                            }
                        },
                        {
                            "text": "Walki o władzę nad główną <w>dzielnicą</w> trwały przez wiele lat.",
                            "target_lang_translations": {
                                "ru": "Борьба за власть над главным <w>уделом</w> длилась много лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rozbicie dzielnicowe",
                        "dzielnica senioralna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Независимое княжество в средневековой Польше, возникшее в результате раздела государства между сыновьями правителя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удел",
                                "target_lang_sense_clarification": "историческое владение князя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
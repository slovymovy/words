    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8982cbe0-fc38-3f6c-827b-8db128f6f4e3",
                    "sense_definition": "Op vaste momenten of vaak terugkerend.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij gaan <w>regelmatig</w> naar de bioscoop om een film te kijken.",
                            "target_lang_translations": {
                                "en": "We <w>regularly</w> go to the cinema to watch a movie.",
                                "ru": "Мы <w>регулярно</w> ходим в кино, чтобы посмотреть фильм."
                            }
                        },
                        {
                            "text": "Er wordt in dit gebouw <w>regelmatig</w> schoongemaakt.",
                            "target_lang_translations": {
                                "en": "This building is cleaned <w>regularly</w>.",
                                "ru": "В этом здании <w>регулярно</w> убирают."
                            }
                        }
                    ],
                    "synonyms": [
                        "geregeld"
                    ],
                    "antonyms": [
                        "onregelmatig"
                    ],
                    "common_phrases": [
                        "komt regelmatig voor",
                        "doe regelmatig"
                    ],
                    "target_lang_definitions": {
                        "en": "Happening at fixed moments or recurring often.",
                        "ru": "Происходящий в установленные моменты или часто повторяющийся."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "regularly",
                                "target_lang_sense_clarification": "at fixed intervals or frequently"
                            },
                            {
                                "target_lang_word": "frequently",
                                "target_lang_sense_clarification": "happening often"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "регулярно",
                                "target_lang_sense_clarification": "через равные промежутки времени"
                            },
                            {
                                "target_lang_word": "часто",
                                "target_lang_sense_clarification": "много раз, нередко"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "06fb1c2a-d699-30c7-8289-da0f03bbf19e",
                    "sense_definition": "Gekenmerkt door een vaste orde, maat of ritme; zonder afwijkingen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de operatie had de patiënt al snel weer een <w>regelmatige</w> hartslag.",
                            "target_lang_translations": {
                                "en": "After the surgery, the patient soon had a <w>regular</w> heartbeat again.",
                                "ru": "После операции у пациента быстро восстановился <w>ровный</w> сердечный ритм."
                            }
                        },
                        {
                            "text": "In de Nederlandse grammatica is dit een <w>regelmatig</w> werkwoord.",
                            "target_lang_translations": {
                                "en": "In Dutch grammar, this is a <w>regular</w> verb.",
                                "ru": "В нидерландской грамматике это <w>правильный</w> глагол."
                            }
                        }
                    ],
                    "synonyms": [
                        "regulier",
                        "systematisch"
                    ],
                    "antonyms": [
                        "onregelmatig"
                    ],
                    "common_phrases": [
                        "regelmatige hartslag",
                        "regelmatig patroon",
                        "regelmatig werkwoord"
                    ],
                    "target_lang_definitions": {
                        "en": "Characterized by a fixed order, measure, or rhythm; without deviations.",
                        "ru": "Характеризующийся постоянным порядком, размером или ритмом; без отклонений."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "regular",
                                "target_lang_sense_clarification": "having a constant pattern, rhythm, or following rules"
                            },
                            {
                                "target_lang_word": "steady",
                                "target_lang_sense_clarification": "constant and uniform"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "правильный",
                                "target_lang_sense_clarification": "следующий правилам или симметричный"
                            },
                            {
                                "target_lang_word": "ровный",
                                "target_lang_sense_clarification": "равномерный, без перебоев"
                            },
                            {
                                "target_lang_word": "ритмичный",
                                "target_lang_sense_clarification": "подчиненный ритму"

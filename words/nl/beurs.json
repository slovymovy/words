                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сумка",
                                "target_lang_sense_clarification": "анатомическая (слизистая/суставная)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bde32c26-a3ed-3a7c-bce2-5b8b30ea2e84",
                    "sense_definition": "Pijnlijk en gevoelig door stoten, vallen of overbelasting, vaak gepaard gaand met blauwe plekken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de val van de trap voelde zijn hele lichaam <w>beurs</w>.",
                            "target_lang_translations": {
                                "ru": "После падения с лестницы всё его тело казалось <w>разбитым</w>."
                            }
                        },
                        {
                            "text": "Ik heb een <w>beurse</w> plek op mijn been waar ik me heb gestoten.",
                            "target_lang_translations": {
                                "ru": "У меня на ноге <w>ушибленное</w> место, где я ударился."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich beurs voelen",
                        "een beurse plek",
                        "beurs geslagen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Болезненный и чувствительный от ударов, падения или перенапряжения, часто с синяками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ушибленный",
                                "target_lang_sense_clarification": "о месте на теле"
                            },
                            {
                                "target_lang_word": "разбитый",
                                "target_lang_sense_clarification": "об общем самочувствии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25c319e0-4bc6-3501-a378-4a898b9dbfb8",
                    "sense_definition": "Zacht en bruin geworden door ouderdom of beschadiging, specifiek gezegd van fruit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fruit_condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt deze <w>beurse</w> appel beter niet meer opeten.",
                            "target_lang_translations": {
                                "ru": "Это <w>побитое</w> яблоко лучше больше не есть."
                            }
                        },
                        {
                            "text": "Het fruit in de schaal is na een week helemaal <w>beurs</w> geworden.",
                            "target_lang_translations": {
                                "ru": "Фрукты в вазе за неделю стали совсем <w>мятыми</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beurs fruit",
                        "een beurse appel",
                        "beurse plekken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ставший мягким и коричневым от старости или повреждений (о фруктах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побитый",
                                "target_lang_sense_clarification": "с повреждениями"
                            },
                            {
                                "target_lang_word": "мятый",
                                "target_lang_sense_clarification": "мягкий от ударов"
                            }
                        ]
                    }
                }
            ]
        },

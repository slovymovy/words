            "senses": [
                {
                    "sense_id": "b459e516-988b-3338-af50-98a75cbeca24",
                    "sense_definition": "Duży, stojący mebel z drzwiami, służący do przechowywania ubrań lub innych przedmiotów.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy wymienić naszą <w>szafę</w>, ta już się rozpada.",
                            "target_lang_translations": {
                                "ru": "Нам нужно заменить наш <w>шкаф</w>, этот уже разваливается."
                            }
                        },
                        {
                            "text": "Włożyłem wszystkie czyste ubrania do <w>szafy</w> w sypialni.",
                            "target_lang_translations": {
                                "ru": "Я сложил всю чистую одежду в <w>шкаф</w> в спальне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szafa wnękowa",
                        "szafa na ubrania",
                        "pojemna szafa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой стоячий предмет мебели с дверцами, предназначенный для хранения одежды или других вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шкаф",
                                "target_lang_sense_clarification": "мебель"
                            },
                            {
                                "target_lang_word": "гардероб",
                                "target_lang_sense_clarification": "для одежды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc14c733-8b56-3318-badc-5641c3c28896",
                    "sense_definition": "Duża obudowa urządzenia technicznego, na przykład serwera lub aparatury sterowniczej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Technik otworzył <w>szafę</w> sterowniczą, aby naprawić usterkę.",
                            "target_lang_translations": {
                                "ru": "Техник открыл <w>шкаф</w> управления, чтобы устранить неисправность."
                            }
                        },
                        {
                            "text": "Ta <w>szafa</w> serwerowa jest wyposażona w nowoczesny system chłodzenia.",
                            "target_lang_translations": {
                                "ru": "Этот серверный <w>шкаф</w> оборудован современной системой охлаждения."

                    "synonyms": [
                        "set",
                        "established",
                        "permanent",
                        "constant",
                        "unchangeable"
                    ],
                    "antonyms": [
                        "variable",
                        "moveable",
                        "movable"
                    ],
                    "common_phrases": [
                        "fixed price",
                        "fixed rate",
                        "fixed income",
                        "fixed term"
                    ],
                    "target_lang_definitions": {

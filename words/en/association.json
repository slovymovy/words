    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "16b6f5cb-3fff-3559-98ab-63fbf6d08cd1",
                    "sense_definition": "A group of people organized for a joint purpose or common interest.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She joined the alumni <w>association</w> to stay in touch with her former classmates.",
                            "target_lang_translations": {
                                "ru": "Она вступила в <w>ассоциацию</w> выпускников, чтобы поддерживать связь с бывшими однокурсниками."
                            }
                        },
                        {
                            "text": "The local football <w>association</w> organizes matches every weekend for young players.",
                            "target_lang_translations": {
                                "ru": "Местная футбольная <w>ассоциация</w> каждые выходные организует матчи для молодых игроков."
                            }
                        }
                    ],
                    "synonyms": [
                        "ass'n"
                    ],
                    "common_phrases": [
                        "alumni association",
                        "football association",
                        "trade association",
                        "residents association"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, организованная для совместной цели или общих интересов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ассоциа́ция",
                                "target_lang_sense_clarification": "организация"
                            },
                            {
                                "target_lang_word": "объедине́ние",
                                "target_lang_sense_clarification": "группа лиц"
                            },
                            {
                                "target_lang_word": "сою́з",
                                "target_lang_sense_clarification": "альянс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "627f9ec7-56ae-303c-af6f-ce40252bf7fc",
                    "sense_definition": "A connection or relationship between things, people, or ideas.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The book was written in <w>association</w> with a team of expert historians.",
                            "target_lang_translations": {
                                "ru": "Книга была написана в <w>сотрудничестве</w> с командой профессиональных историков."
                            }
                        },
                        {
                            "text": "This perfume has a strong <w>association</w> with my childhood memories of the garden.",
                            "target_lang_translations": {
                                "ru": "Эти духи вызывают сильную <w>ассоциацию</w> с моими детскими воспоминаниями о саде."
                            }
                        }
                    ],
                    "synonyms": [
                        "connection"
                    ],
                    "common_phrases": [
                        "in association with",
                        "close association",
                        "mental association"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связь или отношение между вещами, людьми или идеями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ассоциа́ция",
                                "target_lang_sense_clarification": "психологическая связь"
                            },
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "отношение"
                            },
                            {
                                "target_lang_word": "сотрудничество",
                                "target_lang_sense_clarification": "совместная деятельность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db6b77d1-479b-300e-b7f4-6ca6b74681d5",
                    "sense_definition": "The act of mixing socially or cooperating with others.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The constitution guarantees freedom of <w>association</w> for all citizens of the country.",
                            "target_lang_translations": {
                                "ru": "Конституция гарантирует свободу <w>объединений</w> для всех граждан страны."
                            }
                        },
                        {
                            "text": "His <w>association</w> with known criminals led to an investigation by the police.",
                            "target_lang_translations": {
                                "ru": "Его <w>связь</w> с известными преступниками привела к полицейскому расследованию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "freedom of association",
                        "guilt by association"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по общению или сотрудничеству с другими людьми."
                    },
                    "translations": {
                        "ru": [

{
    "word_family": [
        "odniesienie",
        "odnośnik",
        "odnośny"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "57fcd2b1-74b9-3ed9-9232-daf3a7393a22",
                    "sense_definition": "Przynieść wzięty przedmiot z powrotem na miejsce, z którego pochodzi.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę jutro <odnieść> te książki do biblioteki.",
                            "target_lang_translations": {
                                "ru": "Я должен завтра <отнести> эти книги в библиотеку."
                            }
                        },
                        {
                            "text": "Proszę <odnieść> brudne naczynia do kuchni.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <отнеси> грязную посуду на кухню."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwrócić",
                        "oddać"
                    ],
                    "antonyms": [
                        "przynieść",
                        "wziąć",
                        "zabrać"
                    ],
                    "common_phrases": [
                        "odnieść na miejsce",
                        "odnieść książkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вернуть взятый предмет на место, откуда он был взят, или доставить его куда-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отнести",
                                "target_lang_sense_clarification": "доставить предмет в другое место"
                            },
                            {
                                "target_lang_word": "вернуть",
                                "target_lang_sense_clarification": "отдать обратно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8226b961-65a1-31e6-a927-c3d1b80fc48e",
                    "sense_definition": "Uzyskać określony skutek, stać się udziałem jakiegoś zdarzenia (np. sukcesu, zwycięstwa) lub doznać czegoś (np. obrażeń, porażki).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza drużyna <odniosła> wczoraj wspaniałe zwycięstwo w meczu.",
                            "target_lang_translations": {
                                "ru": "Наша команда вчера <одержала> великолепную победу в матче."
                            }
                        },
                        {
                            "text": "W wyniku wypadku kierowca <odniósł> poważne obrażenia.",
                            "target_lang_translations": {
                                "ru": "В результате аварии водитель <получил> серьезные травмы."
                            }
                        }
                    ],
                    "synonyms": [
                        "osiągnąć",
                        "zdobyć",
                        "doznać"
                    ],
                    "common_phrases": [
                        "odnieść sukces",
                        "odnieść zwycięstwo",
                        "odnieść wrażenie",
                        "odnieść obrażenia",
                        "odnieść porażkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добиться определенного результата (успеха, победы) или испытать что-то на себе (травмы, поражение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одержать",
                                "target_lang_sense_clarification": "победу"
                            },
                            {
                                "target_lang_word": "получить",
                                "target_lang_sense_clarification": "травму, ранение"
                            },
                            {
                                "target_lang_word": "добиться",
                                "target_lang_sense_clarification": "успеха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "645137e3-188a-3c5f-ace9-6b1e9bfd66f6",
                    "sense_definition": "Wskazać na związek jednej rzeczy z drugą lub nawiązać do czegoś w wypowiedzi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trudno <odnieść> te wyniki do sytuacji z poprzedniego roku.",
                            "target_lang_translations": {
                                "ru": "Трудно <соотнести> эти результаты с ситуацией прошлого года."
                            }
                        },
                        {
                            "text": "Prezes krótko <odniósł> się do zarzutów stawianych przez media.",
                            "target_lang_translations": {
                                "ru": "Президент кратко <отреагировал> на обвинения, выдвинутые СМИ."
                            }
                        }
                    ],
                    "synonyms": [
                        "nawiązać",
                        "powiązać",
                        "porównać"
                    ],
                    "common_phrases": [
                        "odnieść się do",
                        "odnieść skutek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указать на связь одной вещи с другой или сослаться на что-то в высказывании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соотнести",
                                "target_lang_sense_clarification": "сравнить или связать факты"
                            },
                            {
                                "target_lang_word": "сослаться",
                                "target_lang_sense_clarification": "упомянуть или отреагировать на что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "086dd0dc-58a6-385b-8f0d-e28e90750184",
                    "sense_definition": "Okazać komuś lub czemuś określone uczucia lub postawę; zareagować w pewien sposób.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel <odniósł> się do nowego ucznia z dużą życzliwością.",
                            "target_lang_translations": {
                                "ru": "Учитель <отнесся> к новому ученику с большой доброжелательностью."
                            }
                        },
                        {
                            "text": "Nie wiem, jak mam <odnieść> się do tej zaskakującej propozycji.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, как <отнестись> к этому неожиданному предложению."
                            }
                        }
                    ],
                    "synonyms": [
                        "ustosunkować się",
                        "potraktować"
                    ],
                    "common_phrases": [
                        "odnieść się z szacunkiem",
                        "odnieść się krytycznie",
                        "odnieść się wrogo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявить определенные чувства или отношение к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отнестись",
                                "target_lang_sense_clarification": "выразить отношение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
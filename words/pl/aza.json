            "senses": [
                {
                    "sense_id": "baf1c6cb-01fd-33e4-95c1-636f31f6bc7a",
                    "sense_definition": "Popularne imię nadawane psom, szczególnie sukom; często spotykane w polskiej literaturze i kulturze.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "names_animals",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "Nie dawaj <w>Azie</w> dropsów, ponieważ nie karmi się psa słodyczami.",
                            "target_lang_translations": {
                                "ru": "Не давай <w>Азе</w> леденцы, потому что собак не кормят сладостями."
                            }
                        },
                        {
                            "text": "Nasza wierna <w>Aza</w> zawsze szczeka, gdy ktoś obcy zbliża się do bramy.",
                            "target_lang_translations": {
                                "ru": "Наша верная <w>Аза</w> всегда лает, когда кто-то чужой приближается к воротам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wierna Aza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Популярная кличка для собак, особенно самок; часто встречается в польской литературе и культуре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Аза",
                                "target_lang_sense_clarification": "кличка собаки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d2387cd-9d7c-3c59-8563-f90ed93b14b2",
                    "sense_definition": "Imię męskie pochodzenia biblijnego; w Starym Testamencie imię to nosił król Judy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "names_people",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "A nie było wojny aż do roku trzydziestego i piątego królowania <w>Azy</w>.",
                            "target_lang_translations": {
                                "ru": "И не было войны до тридцать пятого года царствования <w>Асы</w>."
                            }
                        },
                        {
                            "text": "Król <w>Aza</w> zasłynął z reform religijnych przeprowadzonych w swoim królestwie.",
                            "target_lang_translations": {
                                "ru": "Царь <w>Аса</w> прославился религиозными реформами, проведенными в своем царстве."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя библейского происхождения; в Ветхом Завете это имя носил царь Иудеи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Аса",
                                "target_lang_sense_clarification": "библейский царь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4cf50dd1-d0e1-33d0-90b0-c9b48e691aea",
                    "sense_definition": "Rzadko spotykane imię żeńskie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "names_people",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Zrozumiał jeno, że nie chce i nie może się zgodzić na to, aby <w>Azie</w> cośkolwiek zawdzięczał.",
                            "target_lang_translations": {
                                "ru": "Он понял лишь, что не хочет и не может согласиться с тем, чтобы быть чем-либо обязанным <w>Азе</w>."
                            }
                        },
                        {
                            "text": "Na liście gości znalazła się tajemnicza kobieta o imieniu <w>Aza</w>.",
                            "target_lang_translations": {
                                "ru": "В списке гостей оказалась таинственная женщина по имени <w>Аза</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Редкое женское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Аза",
                                "target_lang_sense_clarification": "женское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e0e2c89-ae8f-3b88-b74f-43d715208dca",
                    "sense_definition": "Miejscowość położona w Hiszpanii.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "names_places",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Według starej legendy Joanna z <w>Azy</w> słynęła z wielkiej troski o biednych.",
                            "target_lang_translations": {
                                "ru": "Согласно старой легенде, Иоанна де <w>Аза</w> славилась великой заботой о бедных."
                            }
                        },
                        {
                            "text": "Podczas podróży po Półwyspie Iberyjskim odwiedziliśmy małe miasteczko <w>Aza</w>.",
                            "target_lang_translations": {
                                "ru": "Во время путешествия по Пиренейскому полуострову мы посетили маленький городок <w>Аса</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Населенный пункт в Испании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Аза",
                                "target_lang_sense_clarification": "географическое название"
                            },
                            {
                                "target_lang_word": "Аса",
                                "target_lang_sense_clarification": "город в Испании"
                            }
                        ]
                    }
                }
            ]
        },
        {

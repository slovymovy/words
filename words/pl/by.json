    "entries": [
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "a48dcb70-1af3-3f3d-98ad-d3dbb79c5fb5",
                    "sense_definition": "Służy do tworzenia trybu przypuszczającego, wyrażającego warunek, hipotezę, życzenie, możliwość lub łagodzenie wypowiedzi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "conditional_mood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "On <w>by</w> nigdy nie zrobił czegoś takiego celowo.",
                            "target_lang_translations": {
                                "ru": "Он <w>бы</w> никогда не сделал ничего подобного намеренно."
                            }
                        },
                        {
                            "text": "Trzeba <w>by</w> się zastanowić nad lepszym rozwiązaniem.",
                            "target_lang_translations": {
                                "ru": "Надо <w>бы</w> подумать над лучшим решением."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trzeba by",
                        "można by",
                        "warto by",
                        "dałoby się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служит для образования сослагательного наклонения, выражает условие, гипотезу или вежливость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бы",
                                "target_lang_sense_clarification": "частица сослагательного наклонения"
                            },
                            {
                                "target_lang_word": "б",
                                "target_lang_sense_clarification": "краткая форма частицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a482bb8-1999-3880-b53e-815378a5963e",
                    "sense_definition": "Partykuła wyznaczająca minimalny zakres lub stopień czegoś; rzadziej używany synonim słowa „przynajmniej”.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "limitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Daj <w>by</w> znak, że wszystko u ciebie w porządku.",
                            "target_lang_translations": {
                                "ru": "Подай <w>хотя бы</w> знак, что у тебя всё в порядке."
                            }
                        },
                        {
                            "text": "Wypij <w>by</w> łyk wody, zanim wyjdziesz.",
                            "target_lang_translations": {
                                "ru": "Выпей <w>хотя бы</w> глоток воды, прежде чем уйти."
                            }
                        }
                    ],
                    "synonyms": [
                        "przynajmniej"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "usage is archaic or dialectal in modern Polish"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Частица, указывающая на минимальный предел; редкий синоним «хотя бы»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хотя бы",
                                "target_lang_sense_clarification": "ограничительная частица"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "6aba5767-103a-327e-84cc-c11c594af657",
                    "sense_definition": "Wprowadza zdanie podrzędne celu, wskazując na zamiar lub cel wykonywanej czynności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zadzwoniłem do ciebie, <w>by</w> zapytać o zdrowie.",
                            "target_lang_translations": {
                                "ru": "Я позвонил тебе, <w>чтобы</w> спросить о здоровье."
                            }
                        },
                        {
                            "text": "Musisz dużo ćwiczyć, <w>by</w> osiągnąć sukces w sporcie.",
                            "target_lang_translations": {
                                "ru": "Ты должен много тренироваться, <w>чтобы</w> добиться успеха в спорте."
                            }
                        }
                    ],
                    "synonyms": [
                        "żeby",
                        "aby",
                        "ażeby"
                    ],
                    "common_phrases": [
                        "po to, by",
                        "tylko po to, by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вводит придаточное цели, указывая на намерение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чтобы",
                                "target_lang_sense_clarification": "союз цели"
                            },
                            {
                                "target_lang_word": "для того, чтобы",
                                "target_lang_sense_clarification": "составной союз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c9bdbf0-ff4a-3c3f-9a32-08b2b92f90d2",
                    "sense_definition": "Wprowadza zdanie podrzędne dopełnieniowe po czasownikach wyrażających wolę, prośbę, rozkaz, obawę lub wątpliwość.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "object_clause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice chcieliby, <w>by</w> ich dzieci były szczęśliwe.",
                            "target_lang_translations": {
                                "ru": "Родители хотели бы, <w>чтобы</w> их дети были счастливы."
                            }
                        },
                        {
                            "text": "Nie sądzę, <w>by</w> to był dobry moment na rozmowę.",
                            "target_lang_translations": {
                                "ru": "Не думаю, <w>чтобы</w> это был подходящий момент для разговора."
                            }
                        }
                    ],
                    "synonyms": [
                        "żeby",
                        "aby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вводит изъяснительное придаточное после глаголов волеизъявления или сомнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чтобы",
                                "target_lang_sense_clarification": "изъяснительный союз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5db8c43-8a7b-3118-afd5-a12773987d41",
                    "sense_definition": "Wprowadza zdanie wyrażające silne życzenie lub pragnienie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "wish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>By</w> ten dzień wreszcie się skończył!",
                            "target_lang_translations": {
                                "ru": "<w>Хоть бы</w> этот день наконец закончился!"
                            }
                        },
                        {
                            "text": "<w>By</w>m tylko zdążył na ostatni pociąg do domu.",
                            "target_lang_translations": {
                                "ru": "<w>Лишь бы</w> я успел на последний поезд домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "żeby",
                        "aby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает сильное желание или надежду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хоть бы",
                                "target_lang_sense_clarification": "желание"
                            },
                            {
                                "target_lang_word": "лишь бы",
                                "target_lang_sense_clarification": "желание"
                            },
                            {
                                "target_lang_word": "чтоб",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8aa35ee-acd8-3412-8e71-cbe93c255e9b",
                    "sense_definition": "Wprowadza zdanie skutkowe, łącząc wydarzenie z jego następstwem, często nieoczekiwanym lub ironicznym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wyszedł z domu w pośpiechu, <w>by</w> nigdy już tam nie wrócić.",
                            "target_lang_translations": {
                                "ru": "Он в спешке вышел из дома, <w>чтобы</w> никогда больше туда не вернуться."
                            }
                        },
                        {
                            "text": "Szukali schronienia przed deszczem, <w>by</w> trafić do starej jaskini.",
                            "target_lang_translations": {
                                "ru": "Они искали укрытия от дождя, <w>чтобы</w> попасть в старую пещеру."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often literary or storytelling context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вводит придаточное следствия, связывая событие с его результатом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чтобы",
                                "target_lang_sense_clarification": "союз следствия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "520f78dc-155d-3e70-8534-3b597d51eb1e",
                    "sense_definition": "A colloquial or dialectal form of <w>one</w>, referring to a person.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "referring_to_one",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The little <w>'un</w> was playing happily in the garden with a ball.",
                            "target_lang_translations": {
                                "ru": "Этот <w>малыш</w> счастливо играл в саду с мячом."
                            }
                        },
                        {
                            "text": "He is a wrong <w>'un</w>, so you should probably stay away from him.",
                            "target_lang_translations": {
                                "ru": "Он мутный <w>тип</w>, так что тебе, вероятно, стоит держаться от него подальше."
                            }
                        }
                    ],
                    "synonyms": [
                        "one"
                    ],
                    "common_phrases": [
                        "young 'un",
                        "little 'un",
                        "wrong 'un",
                        "clever 'un"
                    ],
                    "traits": [

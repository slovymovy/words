{
    "word_family": [
        "mooiheid"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6a40b64d-6bea-3cff-ace2-6e41c7030e90",
                    "sense_definition": "Aangenaam om naar te kijken of te luisteren; esthetisch aantrekkelijk.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "aesthetic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij heeft een heel <w>mooi</w> gezicht en prachtige ogen.",
                            "target_lang_translations": {
                                "en": "She has a very <w>beautiful</w> face and lovely eyes.",
                                "ru": "У неё очень <w>красивое</w> лицо и прекрасные глаза."
                            }
                        },
                        {
                            "text": "Wat een <w>mooi</w> schilderij hangt daar aan de muur.",
                            "target_lang_translations": {
                                "en": "What a <w>beautiful</w> painting is hanging on the wall there.",
                                "ru": "Какая <w>красивая</w> картина висит там на стене."
                            }
                        }
                    ],
                    "synonyms": [
                        "aantrekkelijk",
                        "fraai",
                        "knap",
                        "schoon"
                    ],
                    "common_phrases": [
                        "een mooie vrouw",
                        "een mooi huis",
                        "mooi om te zien"
                    ],
                    "target_lang_definitions": {
                        "en": "Pleasant to look at or listen to; aesthetically attractive.",
                        "ru": "Приятный на вид или слух; эстетически привлекательный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beautiful",
                                "target_lang_sense_clarification": "possessing beauty, impressing the eye"
                            },
                            {
                                "target_lang_word": "pretty",
                                "target_lang_sense_clarification": "pleasant to the sight, especially of people or objects"
                            },
                            {
                                "target_lang_word": "lovely",
                                "target_lang_sense_clarification": "delightful for beauty or harmony"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "красивый",
                                "target_lang_sense_clarification": "обладающий красотой, приятный глазу"
                            },
                            {
                                "target_lang_word": "прекрасный",
                                "target_lang_sense_clarification": "очень красивый, восхитительный"
                            },
                            {
                                "target_lang_word": "симпатичный",
                                "target_lang_sense_clarification": "милый, приятный внешне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88bd6299-eb85-3236-8650-7e728eed8a53",
                    "sense_definition": "Prettig om te ervaren, van goede kwaliteit of gunstig.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pleasant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is vandaag erg <w>mooi</w> weer om buiten te wandelen.",
                            "target_lang_translations": {
                                "en": "It is very <w>nice</w> weather today for walking outside.",
                                "ru": "Сегодня очень <w>хорошая</w> погода для прогулки."
                            }
                        },
                        {
                            "text": "Dat heb je goed gedaan, <w>mooi</w> werk!",
                            "target_lang_translations": {
                                "en": "You did that well, <w>nice</w> work!",
                                "ru": "Ты хорошо справился, <w>отличная</w> работа!"
                            }
                        }
                    ],
                    "synonyms": [
                        "fraai",
                        "aantrekkelijk"
                    ],
                    "common_phrases": [
                        "mooi weer",
                        "mooi werk",
                        "een mooie gelegenheid"
                    ],
                    "target_lang_definitions": {
                        "en": "Pleasant to experience, of good quality, or favorable.",
                        "ru": "Приятный по ощущениям, хорошего качества или благоприятный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nice",
                                "target_lang_sense_clarification": "pleasant, satisfactory"
                            },
                            {
                                "target_lang_word": "beautiful",
                                "target_lang_sense_clarification": "affording pleasure (e.g. weather)"
                            },
                            {
                                "target_lang_word": "good",
                                "target_lang_sense_clarification": "of high quality or merit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хороший",
                                "target_lang_sense_clarification": "качественный или благоприятный (например, о погоде)"
                            },
                            {
                                "target_lang_word": "отличный",
                                "target_lang_sense_clarification": "очень хороший, превосходный"
                            },
                            {
                                "target_lang_word": "приятный",
                                "target_lang_sense_clarification": "доставляющий удовольствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af14bb23-8523-3cc2-b6df-a8954f93aea4",
                    "sense_definition": "Vervelend, tegenvallend of onaangenaam (ironisch gebruikt).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ironic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nou wordt 'ie <w>mooi</w>, dat had ik totaal niet verwacht!",
                            "target_lang_translations": {
                                "en": "Now that's a <w>fine</w> one, I didn't expect that at all!",
                                "ru": "Ну это просто <w>прекрасно</w>, я этого совсем не ожидал!"
                            }
                        },
                        {
                            "text": "Dat is een <w>mooie</w> boel, nu zitten we met de problemen.",
                            "target_lang_translations": {
                                "en": "That is a <w>fine</w> mess, now we are stuck with the problems.",
                                "ru": "Это <w>хорошенькое</w> дело, теперь у нас проблемы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nou wordt ie mooi",
                        "een mooie boel"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used ironically to mean the opposite"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Annoying, disappointing, or unpleasant (used ironically).",
                        "ru": "Досадный или неприятный (используется с иронией)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fine",
                                "target_lang_sense_clarification": "used ironically to mean bad or problematic"
                            },
                            {
                                "target_lang_word": "pretty",
                                "target_lang_sense_clarification": "awkward or unpleasant (ironic, e.g. a pretty mess)"
                            },
                            {
                                "target_lang_word": "nice",
                                "target_lang_sense_clarification": "used ironically to mean the opposite"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хорошенький",
                                "target_lang_sense_clarification": "иронично: плохой, проблемный (напр. хорошенькое дело)"
                            },
                            {
                                "target_lang_word": "прекрасный",
                                "target_lang_sense_clarification": "иронично: ужасный или нелепый"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4bb0e1b5-0054-30ec-b825-bc98c2e3c3f1",
                    "sense_definition": "In aanzienlijke mate; behoorlijk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je zit er <w>mooi</w> naast als je denkt dat ik dat ga doen.",
                            "target_lang_translations": {
                                "en": "You are <w>pretty</w> wrong if you think I'm going to do that.",
                                "ru": "Ты <w>сильно</w> ошибаешься, если думаешь, что я это сделаю."
                            }
                        },
                        {
                            "text": "Het begint hier al <w>mooi</w> druk te worden.",
                            "target_lang_translations": {
                                "en": "It's starting to get <w>pretty</w> busy here.",
                                "ru": "Здесь становится уже <w>довольно</w> людно."
                            }
                        }
                    ],
                    "synonyms": [
                        "aardig",
                        "behoorlijk",
                        "nogal"
                    ],
                    "common_phrases": [
                        "mooi mis",
                        "mooi op tijd"
                    ],
                    "target_lang_definitions": {
                        "en": "To a considerable degree; quite.",
                        "ru": "В значительной степени; довольно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pretty",
                                "target_lang_sense_clarification": "somewhat, fairly, quite"
                            },
                            {
                                "target_lang_word": "quite",
                                "target_lang_sense_clarification": "to a considerable extent"
                            },
                            {
                                "target_lang_word": "nicely",
                                "target_lang_sense_clarification": "satisfactorily or sufficiently"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "довольно",
                                "target_lang_sense_clarification": "в достаточной степени"
                            },
                            {
                                "target_lang_word": "изрядно",
                                "target_lang_sense_clarification": "сильно, заметно"
                            },
                            {
                                "target_lang_word": "здорово",
                                "target_lang_sense_clarification": "разг.: очень, сильно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bae78175-bd97-3f66-8fd5-1a1e53360edd",
                    "sense_definition": "Het esthetische aspect; de verfraaiing.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben de taart puur voor het <w>mooi</w> versierd met slagroom.",
                            "target_lang_translations": {
                                "en": "We decorated the cake with whipped cream purely for <w>show</w>.",
                                "ru": "Мы украсили торт взбитыми сливками чисто для <w>красоты</w>."
                            }
                        },
                        {
                            "text": "Dat hek staat er alleen voor het <w>mooi</w>, het houdt niemand tegen.",
                            "target_lang_translations": {
                                "en": "That fence is only there for <w>decoration</w>, it doesn't stop anyone.",
                                "ru": "Этот забор стоит только для <w>вида</w>, он никого не останавливает."
                            }
                        }
                    ],
                    "common_phrases": [
                        "voor het mooi"
                    ],
                    "target_lang_definitions": {
                        "en": "The aesthetic aspect; the embellishment.",
                        "ru": "Эстетический аспект; то, что служит для украшения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "appearance or display"
                            },
                            {
                                "target_lang_word": "decoration",
                                "target_lang_sense_clarification": "the act of making something look better"
                            },
                            {
                                "target_lang_word": "looks",
                                "target_lang_sense_clarification": "visual appearance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "красота",
                                "target_lang_sense_clarification": "эстетическая привлекательность"
                            },
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "внешний облик, показуха"
                            }
                        ]
                    }
                }
            ]
        }
    ]

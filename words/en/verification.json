{
    "word_family": [
        "verify",
        "verifier",
        "verifiable",
        "unverified",
        "unverifiable"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2235fcf0-683c-357e-80d5-9acef8bc95bd",
                    "sense_definition": "The act or process of checking that something is true, accurate, or valid.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bank requires <w>verification</w> of your identity before opening a new account.",
                            "target_lang_translations": {
                                "ru": "Банк требует <w>подтверждения</w> вашей личности перед открытием нового счета."
                            }
                        },
                        {
                            "text": "Please complete the email <w>verification</w> process to activate your profile.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, завершите процесс <w>верификации</w> электронной почты, чтобы активировать свой профиль."
                            }
                        }
                    ],
                    "synonyms": [
                        "checking",
                        "validation",
                        "authentication"
                    ],
                    "common_phrases": [
                        "identity verification",
                        "email verification",
                        "verification process",
                        "data verification"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс проверки того, что что-то является истинным, точным или действительным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверка",
                                "target_lang_sense_clarification": "общая"
                            },
                            {
                                "target_lang_word": "верификация",
                                "target_lang_sense_clarification": "формальная или техническая"
                            },
                            {
                                "target_lang_word": "подтверждение",
                                "target_lang_sense_clarification": "удостоверение правильности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2cc0532-c647-3eda-be9e-2ad0d1df744c",
                    "sense_definition": "Evidence or confirmation that establishes the truth or accuracy of something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The detective needs <w>verification</w> of your whereabouts last night.",
                            "target_lang_translations": {
                                "ru": "Детективу нужно <w>подтверждение</w> вашего местонахождения прошлой ночью."
                            }
                        },
                        {
                            "text": "We are waiting for written <w>verification</w> of his employment history from the company.",
                            "target_lang_translations": {
                                "ru": "Мы ждем письменного <w>подтверждения</w> истории его трудоустройства от компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "confirmation",
                        "proof",
                        "evidence"
                    ],
                    "common_phrases": [
                        "independent verification",
                        "written verification",
                        "visual verification"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доказательство или подтверждение, устанавливающее истинность или точность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подтверждение",
                                "target_lang_sense_clarification": "доказательство истинности"
                            },
                            {
                                "target_lang_word": "доказательство",
                                "target_lang_sense_clarification": "свидетельство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ec5ccd9-e399-381f-98dc-30042862ca00",
                    "sense_definition": "A formal phrase used in concluding a legal plea to denote confirmation by evidence.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer included a formal <w>verification</w> at the end of the complaint.",
                            "target_lang_translations": {
                                "ru": "Юрист включил официальное <w>заверение</w> в конце жалобы."
                            }
                        },
                        {
                            "text": "Without proper <w>verification</w>, the plea may be dismissed by the court.",
                            "target_lang_translations": {
                                "ru": "Без надлежащего <w>удостоверения</w> иск может быть отклонен судом."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Формальная фраза, используемая в конце юридического заявления для подтверждения доказательствами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заверение",
                                "target_lang_sense_clarification": "юридическое"
                            },
                            {
                                "target_lang_word": "удостоверение",
                                "target_lang_sense_clarification": "официальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbf4ca69-981b-380c-8fac-47e9b26100e2",
                    "sense_definition": "The operation of testing an equation to see whether it truly expresses the conditions of a problem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The student performed a <w>verification</w> of the calculated roots to ensure accuracy.",
                            "target_lang_translations": {
                                "ru": "Студент выполнил <w>проверку</w> вычисленных корней, чтобы убедиться в точности."
                            }
                        },
                        {
                            "text": "Mathematical <w>verification</w> is essential to confirm that the formula works for all values.",
                            "target_lang_translations": {
                                "ru": "Математическая <w>проверка</w> необходима, чтобы подтвердить, что формула работает для всех значений."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Операция проверки уравнения, чтобы увидеть, действительно ли оно выражает условия задачи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверка",
                                "target_lang_sense_clarification": "математическая"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
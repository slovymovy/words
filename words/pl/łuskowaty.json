    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "56d51ea1-c6f2-3dfa-b742-a67e7a19c725",
                    "sense_definition": "Pokryty łuskami, czyli małymi, płaskimi płytkami, które chronią ciało niektórych zwierząt lub powierzchnię roślin.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "texture_covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wąż ma długi, <w>łuskowaty</w> tułów, który ułatwia mu poruszanie się po piasku.",
                            "target_lang_translations": {
                                "ru": "У змеи длинное <w>чешуйчатое</w> туловище, которое помогает ей передвигаться по песку."
                            }
                        },
                        {
                            "text": "Chora skóra stała się sucha, szorstka i <w>łuskowata</w>.",
                            "target_lang_translations": {
                                "ru": "Больная кожа стала сухой, шершавой и <w>чешуйчатой</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "gładki"
                    ],
                    "common_phrases": [
                        "łuskowata skóra",
                        "łuskowaty ogon",
                        "łuskowaty tułów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрытый чешуей — маленькими плоскими пластинками, защищающими тело некоторых животных или поверхность растений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чешуйчатый",
                                "target_lang_sense_clarification": "покрытый чешуей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65c33239-c2b7-321a-87d3-1a0367acbcc1",
                    "sense_definition": "Mający kształt, strukturę lub wygląd przypominający łuskę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "appearance_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na powierzchni kamienia widoczny był nietypowy, <w>łuskowaty</w> wzór.",
                            "target_lang_translations": {
                                "ru": "На поверхности камня был виден необычный <w>чешуйчатый</w> узор."
                            }
                        },
                        {
                            "text": "Ten minerał charakteryzuje się <w>łuskowatą</w> budową, przez co łatwo się rozwarstwia.",
                            "target_lang_translations": {
                                "ru": "Этот минерал характеризуется <w>чешуйчатым</w> строением, из-за чего легко расслаивается."
                            }
                        }
                    ],
                    "common_phrases": [
                        "łuskowaty wzór",
                        "łuskowata faktura",
                        "łuskowata budowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий форму, структуру или внешний вид, напоминающий чешую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чешуйчатый",
                                "target_lang_sense_clarification": "напоминающий чешую"
                            }
                        ]
                    }
                }
            ]
        }
    ]

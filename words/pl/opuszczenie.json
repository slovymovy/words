    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3cf92e4e-3ce1-369f-ac68-f25256e21368",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>opuścić</w>: czynność polegająca na wyjściu z jakiegoś miejsca, skierowaniu czegoś w dół lub pominięciu czegoś w tekście lub działaniu.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "action_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagłe <opuszczenie> sali przez dyrektora zaskoczyło wszystkich zebranych.",
                            "target_lang_translations": {
                                "ru": "Внезапный <уход> директора из зала удивил всех собравшихся."
                            }
                        },
                        {
                            "text": "<Opuszczenie> rolet w oknach pozwoliło na rozpoczęcie prezentacji multimedialnej.",
                            "target_lang_translations": {
                                "ru": "<Опускание> жалюзи на окнах позволило начать мультимедийную презентацию."
                            }
                        },
                        {
                            "text": "Zauważyłem przypadkowe <opuszczenie> jednego ważnego zdania w tłumaczeniu.",
                            "target_lang_translations": {
                                "ru": "Я заметил случайный <пропуск> одного важного предложения в переводе."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyjście",
                        "obniżenie",
                        "pominięcie",
                        "wyjazd"
                    ],
                    "antonyms": [
                        "wejście",
                        "podniesienie",
                        "uwzględnienie",
                        "przybycie"
                    ],
                    "common_phrases": [
                        "opuszczenie miejsca pracy",
                        "opuszczenie rąk",
                        "opuszczenie głowy",
                        "opuszczenie flagi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отглагольное существительное от <w>opuścić</w>: действие, заключающееся в выходе из какого-либо места, перемещении чего-либо вниз или пропуске чего-либо в тексте или действии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опускание",
                                "target_lang_sense_clarification": "перемещение вниз"
                            },
                            {
                                "target_lang_word": "пропуск",
                                "target_lang_sense_clarification": "в тексте или списке"
                            },
                            {
                                "target_lang_word": "уход",
                                "target_lang_sense_clarification": "из помещения"
                            },
                            {
                                "target_lang_word": "покидание",
                                "target_lang_sense_clarification": "оставление места (книжн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1cf4360-2f16-3462-b612-23afa769429c",
                    "sense_definition": "Pozostawienie kogoś lub czegoś własnemu losowi bez opieki; także stan osamotnienia wynikający z bycia porzuconym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abandonment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko bardzo mocno przeżyło <opuszczenie> przez biologicznych rodziców.",
                            "target_lang_translations": {
                                "ru": "Ребенок очень тяжело пережил <оставление> биологическими родителями."
                            }
                        },
                        {
                            "text": "Czuła głębokie <opuszczenie> i smutek po wyjeździe swoich dorosłych dzieci.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала глубокую <покинутость> и грусть после отъезда своих взрослых детей."
                            }
                        }
                    ],
                    "synonyms": [
                        "porzucenie",
                        "osamotnienie"
                    ],
                    "antonyms": [
                        "opieka",
                        "wsparcie",
                        "towarzyszenie"
                    ],
                    "common_phrases": [
                        "poczucie opuszczenia",
                        "opuszczenie przez bliskich",
                        "opuszczenie rodziny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставление кого-либо или чего-либо на произвол судьбы без присмотра; также состояние одиночества, возникающее в результате того, что человека покинули."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покинутость",

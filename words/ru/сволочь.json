    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8adb2f23-2823-3f12-8cf4-977aec59cc47",
                    "sense_definition": "Скверный, подлый человек; негодяй, мерзавец",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "insult_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта <w>сволочь</w> снова заняла моё парковочное место у дома.",
                            "target_lang_translations": {
                                "en": "That <w>bastard</w> took my parking spot near the house again."
                            }
                        },
                        {
                            "text": "Я не ожидал, что он окажется такой редкой <w>сволочью</w> и предаст нас.",
                            "target_lang_translations": {
                                "en": "I didn't expect him to turn out to be such a rare <w>scumbag</w> and betray us."
                            }
                        }
                    ],
                    "synonyms": [
                        "мерзавец",
                        "скотина",
                        "подлец",
                        "подонок",
                        "тварь",
                        "мразь",
                        "мразота",
                        "падло"
                    ],
                    "common_phrases": [
                        "редкая сволочь",
                        "порядочная сволочь",
                        "последняя сволочь"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "бранное слово"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A nasty, vile person; a scoundrel or bastard"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bastard",
                                "target_lang_sense_clarification": "offensive, a mean or despicable person"
                            },
                            {
                                "target_lang_word": "scumbag",
                                "target_lang_sense_clarification": "slang, a despicable person"
                            },
                            {
                                "target_lang_word": "scum",
                                "target_lang_sense_clarification": "a reprehensible person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a376fa07-25d8-3315-9923-1ae0689f738f",
                    "sense_definition": "Дрянные, подлые люди как группа; сброд",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "insult_collective",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этот дешёвый бар по вечерам набивается всякая пьяная <w>сволочь</w>.",
                            "target_lang_translations": {
                                "en": "All kinds of drunken <w>scum</w> cram into this cheap bar in the evenings."
                            }
                        },
                        {
                            "text": "Не стоит туда ходить, там собирается одна <w>сволочь</w> со всего района.",
                            "target_lang_translations": {
                                "en": "It's not worth going there, only <w>riff-raff</w> from the whole neighborhood gathers there."
                            }
                        }
                    ],
                    "synonyms": [
                        "сволота",
                        "сброд"
                    ],
                    "common_phrases": [
                        "всякая сволочь"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "собирательное значение"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Vile, despicable people as a group; riff-raff"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scum",
                                "target_lang_sense_clarification": "collective, worthless or evil people"
                            },
                            {
                                "target_lang_word": "riff-raff",
                                "target_lang_sense_clarification": "disreputable or undesirable people"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "359d35a5-249a-3049-a920-be76cf044dd1",
                    "sense_definition": "Перетащить, переместить что-либо тяжёлое волоком или с трудом",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "drag_move",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочим пришлось <w>сволочь</w> все спиленные ветки в один большой овраг.",
                            "target_lang_translations": {
                                "en": "The workers had to <w>drag</w> all the cut branches into one big ravine."
                            }
                        },
                        {
                            "text": "Мы с огромным трудом смогли <w>сволочь</w> этот камень с дороги.",
                            "target_lang_translations": {
                                "en": "We were able to <w>haul</w> this stone off the road with great difficulty."
                            }
                        }
                    ],
                    "synonyms": [
                        "перетащить",
                        "переместить"
                    ],
                    "common_phrases": [
                        "сволочь в кучу",
                        "еле сволочь"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To drag or move something heavy by pulling it"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drag",
                                "target_lang_sense_clarification": "to pull along a surface"
                            },
                            {
                                "target_lang_word": "haul",
                                "target_lang_sense_clarification": "to pull or transport with effort"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49b3b114-5e8d-3a9a-8e0b-d26d677b3802",
                    "sense_definition": "Незаметно утащить; украсть",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "steal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кто-то умудрился <w>сволочь</w> мою ручку со стола, пока я выходил.",
                            "target_lang_translations": {
                                "en": "Someone managed to <w>swipe</w> my pen from the table while I was out."
                            }
                        },
                        {
                            "text": "Ему удалось <w>сволочь</w> пару яблок с прилавка, когда продавец отвернулся.",
                            "target_lang_translations": {
                                "en": "He managed to <w>pinch</w> a couple of apples from the counter when the seller turned away."
                            }
                        }
                    ],
                    "synonyms": [
                        "украсть",
                        "утащить",
                        "стащить"
                    ],
                    "common_phrases": [
                        "сволочь деньги"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To steal or swipe something unnoticed"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swipe",
                                "target_lang_sense_clarification": "informal, to steal"
                            },
                            {
                                "target_lang_word": "pinch",
                                "target_lang_sense_clarification": "informal, to steal"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
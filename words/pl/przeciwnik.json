    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "200a6ab0-328f-33e8-a066-2b7640a6a89d",
                    "sense_definition": "Osoba lub drużyna, z którą rywalizuje się w sporcie, grze lub innej formie współzawodnictwa.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "competition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym meczu <w>przeciwnicy</w> strzelili nam pięć goli.",
                            "target_lang_translations": {
                                "ru": "В этом матче <w>соперники</w> забили нам пять голов."
                            }
                        },
                        {
                            "text": "Nasz <w>przeciwnik</w> okazał się znacznie silniejszy, niż myśleliśmy przed zawodami.",
                            "target_lang_translations": {
                                "ru": "Наш <w>противник</w> оказался значительно сильнее, чем мы думали перед соревнованиями."
                            }
                        }
                    ],
                    "synonyms": [
                        "rywal",
                        "oponent",
                        "nieprzyjaciel"
                    ],
                    "antonyms": [
                        "towarzysz",
                        "swój",
                        "zwolennik"
                    ],
                    "common_phrases": [
                        "pokonać przeciwnika",
                        "trudny przeciwnik",
                        "godny przeciwnik",
                        "słaby przeciwnik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или команда, с которыми ведется соперничество в спорте, игре или другом состязании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соперник",
                                "target_lang_sense_clarification": "конкурент в спорте или игре"
                            },
                            {
                                "target_lang_word": "противник",
                                "target_lang_sense_clarification": "противоборствующая сторона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fc8fc24-e18e-3d6f-8d38-871ce7d01b98",
                    "sense_definition": "Osoba, która nie zgadza się z czymś i występuje przeciwko jakiejś idei, planowi lub poglądowi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ideology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Przeciwnicy</w> wyścigu zbrojeń zablokowali całe miasto i wiele osób nie mogło dojechać do pracy.",
                            "target_lang_translations": {
                                "ru": "<w>Противники</w> гонки вооружений заблокировали весь город, и многие люди не могли доехать до работы."
                            }
                        },
                        {
                            "text": "Mój ojciec jest zagorzałym <w>przeciwnikiem</w> palenia papierosów w miejscach publicznych.",
                            "target_lang_translations": {
                                "ru": "Мой отец — ярый <w>противник</w> курения в общественных местах."
                            }
                        }
                    ],
                    "synonyms": [
                        "oponent"
                    ],
                    "antonyms": [
                        "zwolennik"
                    ],
                    "common_phrases": [
                        "przeciwnik polityczny",
                        "zagorzały przeciwnik",
                        "przeciwnik zmian",
                        "głosy przeciwników"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто не согласен с чем-либо и выступает против какой-либо идеи, плана или взгляда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противник",
                                "target_lang_sense_clarification": "тот, кто выступает против чего-либо"
                            },
                            {
                                "target_lang_word": "оппонент",
                                "target_lang_sense_clarification": "в споре или дискуссии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adfffa37-97d3-3ac5-8244-bc4c71cc28fa",
                    "sense_definition": "Osoba wrogo nastawiona, z którą toczy się poważny spór lub walkę; wróg.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hostility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wojnie trzeba szanować swojego <w>przeciwnika</w>, by nie popełnić błędu.",
                            "target_lang_translations": {
                                "ru": "На войне нужно уважать своего <w>противника</w>, чтобы не совершить ошибку."
                            }
                        },
                        {
                            "text": "Stała się moim <w>przeciwnikiem</w>, gdy zaczęliśmy walczyć o ten sam awans w firmie.",
                            "target_lang_translations": {
                                "ru": "Она стала моим <w>противником</w>, когда мы начали бороться за одно и то же повышение в фирме."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieprzyjaciel",
                        "adwersarz",
                        "rywal"
                    ],
                    "antonyms": [
                        "przyjaciel",
                        "towarzysz"
                    ],
                    "common_phrases": [
                        "śmiertelny przeciwnik",
                        "zacięty przeciwnik",
                        "główny przeciwnik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Враждебно настроенный человек, с которым ведется серьезный спор или борьба; враг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "враг",
                                "target_lang_sense_clarification": "недруг"
                            },
                            {
                                "target_lang_word": "противник",
                                "target_lang_sense_clarification": "враждебная сторона, неприятель"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "swąd",
        "zaswędzić",
        "swędzenie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "72b5cd05-0b0d-32d2-9e31-6d8bff0a2e6f",
                    "sense_definition": "Dawać odczucie nieprzyjemnego podrażnienia skóry, które wywołuje chęć drapania.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "itch_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miejsce po ukąszeniu komara bardzo <w>swędzi</w> i robi się czerwone.",
                            "target_lang_translations": {
                                "ru": "Место укуса комара очень <w>чешется</w> и краснеет."
                            }
                        },
                        {
                            "text": "Jeśli plecy cię <w>swędzą</w>, mogę cię podrapać.",
                            "target_lang_translations": {
                                "ru": "Если у тебя <w>чешется</w> спина, я могу тебя почесать."
                            }
                        }
                    ],
                    "synonyms": [
                        "swędzieć",
                        "świerzbieć"
                    ],
                    "common_phrases": [
                        "nos swędzi",
                        "skóra swędzi",
                        "plecy swędzą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать ощущение зуда, раздражения кожи, желание почесаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чесаться",
                                "target_lang_sense_clarification": "испытывать зуд"
                            },
                            {
                                "target_lang_word": "зудеть",
                                "target_lang_sense_clarification": "вызывать зуд, ныть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf6b454e-c0f5-3639-90e9-3d3a4871eb03",
                    "sense_definition": "Odczuwać natrętną, trudną do powstrzymania chęć zrobienia lub powiedzenia czegoś; męczyć kogoś taką potrzebą.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "itch_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Język go <w>swędzi</w>, żeby wyjawić wszystkim ten sekret.",
                            "target_lang_translations": {
                                "ru": "У него язык <w>чешется</w> раскрыть всем этот секрет."
                            }
                        },
                        {
                            "text": "Ręka mnie <w>swędzi</w>, żeby mu w końcu przyłożyć za to zachowanie.",
                            "target_lang_translations": {
                                "ru": "У меня руки <w>чешутся</w> наконец врезать ему за это поведение."
                            }
                        }
                    ],
                    "synonyms": [
                        "świerzbieć",
                        "nękać"
                    ],
                    "common_phrases": [
                        "język swędzi",
                        "ręka swędzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать навязчивое, трудно сдерживаемое желание сделать или сказать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чесаться",
                                "target_lang_sense_clarification": "в идиомах: руки/язык чешутся"
                            },
                            {
                                "target_lang_word": "не терпеться",
                                "target_lang_sense_clarification": "сильно хотеть сделать что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97055c96-7d85-3919-801f-b35548be1ec4",
                    "sense_definition": "Wydzielać nieprzyjemny zapach spalenizny lub dym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Coś w kuchni <w>swędzi</w>, chyba przypaliłeś mleko na gazie.",
                            "target_lang_translations": {
                                "ru": "Что-то на кухне <w>чадит</w>, кажется, ты прижег молоко на плите."
                            }
                        },
                        {
                            "text": "Stary piec zaczął <w>swędzić</w> i dymić na cały dom.",
                            "target_lang_translations": {
                                "ru": "Старая печь начала <w>чадить</w> и дымить на весь дом."
                            }
                        }
                    ],
                    "synonyms": [
                        "kopcić"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "often used regionally or colloquially instead of 'swądzić'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать неприятный запах гари или дым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чадить",
                                "target_lang_sense_clarification": "издавать дым и гарь"
                            },
                            {
                                "target_lang_word": "пахнуть гарью",
                                "target_lang_sense_clarification": "издавать запах горелого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1544c5a5-ee2e-3fb1-b747-9ed75fbe3201",
                    "sense_definition": "Martwić się czymś, niepokoić lub denerwować.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <w>swędź</w> się na zapas problemami, które jeszcze nie nadeszły.",
                            "target_lang_translations": {
                                "ru": "Не <w>беспокойся</w> заранее о проблемах, которые еще не наступили."
                            }
                        },
                        {
                            "text": "Gospodyni <w>swędzi</w> się o to, czy wystarczy jedzenia dla gości.",
                            "target_lang_translations": {
                                "ru": "Хозяйка <w>беспокоится</w> о том, хватит ли еды для гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "alterować się"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "or dialectal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Волноваться, беспокоиться или нервничать из-за чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беспокоиться",
                                "target_lang_sense_clarification": "испытывать тревогу"
                            },
                            {
                                "target_lang_word": "тревожиться",
                                "target_lang_sense_clarification": "волноваться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
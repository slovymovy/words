                            {
                                "target_lang_word": "иметь",
                                "target_lang_sense_clarification": "обладать признаком или свойством"
                            },
                            {
                                "target_lang_word": "носить",
                                "target_lang_sense_clarification": "иметь на себе (имя, следы, отпечаток)"
                            }
                        ]

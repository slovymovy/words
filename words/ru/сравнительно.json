    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "32ddfd7e-a30b-389d-8a72-e7751cfdcbd6",
                    "sense_definition": "Относительно, в некоторой степени; не очень сильно или много по сравнению с чем-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы закончили этот проект <w>сравнительно</w> быстро и без ошибок.",
                            "target_lang_translations": {
                                "en": "We finished this project <w>relatively</w> quickly and without errors."
                            }
                        },
                        {
                            "text": "В этом году зима была <w>сравнительно</w> тёплой и бесснежной.",
                            "target_lang_translations": {
                                "en": "This year the winter was <w>relatively</w> warm and snowless."
                            }
                        }
                    ],
                    "synonyms": [
                        "относительно",
                        "довольно"
                    ],
                    "antonyms": [
                        "абсолютно"
                    ],
                    "common_phrases": [
                        "сравнительно недавно",
                        "сравнительно легко",
                        "сравнительно недорого",
                        "сравнительно небольшой"
                    ],
                    "target_lang_definitions": {
                        "en": "Relatively, to a certain extent; not very strongly or much compared to something else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "relatively",
                                "target_lang_sense_clarification": "in relation to a standard or other cases"
                            },
                            {
                                "target_lang_word": "comparatively",
                                "target_lang_sense_clarification": "when compared to others"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58ea05b9-9e3a-3489-8661-155040a75e58",
                    "sense_definition": "Путём сравнения, при помощи сопоставления.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автор предлагает рассматривать эти исторические события <w>сравнительно</w>.",
                            "target_lang_translations": {
                                "en": "The author suggests viewing these historical events <w>comparatively</w>."
                            }
                        },
                        {
                            "text": "Эту проблему можно изучать <w>сравнительно</w>, сопоставляя разные подходы.",
                            "target_lang_translations": {
                                "en": "This problem can be studied <w>comparatively</w>, by juxtaposing different approaches."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "By way of comparison, through the method of juxtaposition."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "comparatively",
                                "target_lang_sense_clarification": "using a comparative method"
                            }
                        ]
                    }
                }
            ]
        }
    ]

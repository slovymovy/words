            "senses": [
                {
                    "sense_id": "18f62bd5-a41e-3fb3-a8cd-fe8872869b1e",
                    "sense_definition": "Krótkie narzędzie ręczne składające się z ostrza i rękojeści, służące do krojenia lub cięcia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "tool_weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kroję świeży chleb ostrym <nożem> na śniadanie.",
                            "target_lang_translations": {
                                "ru": "Я режу свежий хлеб острым <ножом> на завтрак."
                            }
                        },
                        {
                            "text": "Proszę, podaj mi <nóż> i widelec, abym mógł zjeść obiad.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, дай мне <нож> и вилку, чтобы я мог пообедать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ostry nóż",
                        "nóż kuchenny",
                        "nóż do chleba",
                        "tępy nóż"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий ручной инструмент, состоящий из лезвия и рукоятки, предназначенный для резки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нож",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]

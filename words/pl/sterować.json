            "pos": "verb",
            "senses": [
                {
                    "sense_id": "53b274e8-bf9b-3e19-946a-4d8fd936065f",
                    "sense_definition": "Wyznaczać i utrzymywać kierunek ruchu pojazdu, zazwyczaj statku wodnego lub powietrznego, używając do tego steru.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kapitan musi pewnie <w>sterować</w> statkiem, aby ominąć niebezpieczne skały.",
                            "target_lang_translations": {
                                "ru": "Капитан должен уверенно <w>управлять</w> судном, чтобы обойти опасные скалы."
                            }
                        },
                        {
                            "text": "Mój dziadek uczył mnie, jak <w>sterować</w> małą łódką na jeziorze.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка учил меня, как <w>управлять</w> маленькой лодкой на озере."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierować",
                        "manewrować"
                    ],
                    "common_phrases": [
                        "sterować statkiem",
                        "sterować łodzią",
                        "sterować okrętem",
                        "ręcznie sterować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определять и поддерживать направление движения транспортного средства, обычно судна или самолета, используя руль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "транспортным средством"
                            },
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "судно или машину"
                            },
                            {
                                "target_lang_word": "рулить",
                                "target_lang_sense_clarification": "использовать руль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05b7155c-7f82-3be7-b23a-903738ca570d",
                    "sense_definition": "Kontrolować działanie urządzenia, maszyny lub systemu, często wydając polecenia za pomocą aparatury elektronicznej lub komputera.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "technical_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowoczesna aplikacja pozwala <w>sterować</w> ogrzewaniem w domu z dowolnego miejsca na świecie.",
                            "target_lang_translations": {
                                "ru": "Современное приложение позволяет <w>управлять</w> отоплением в доме из любой точки мира."
                            }
                        },
                        {
                            "text": "Operatorzy mogą <w>sterować</w> dronem za pomocą specjalnego pilota radiowego.",
                            "target_lang_translations": {
                                "ru": "Операторы могут <w>управлять</w> дроном с помощью специального радиопульта."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierować"
                    ],
                    "common_phrases": [
                        "sterować zdalnie",
                        "sterować pilotem",
                        "sterować głosem",
                        "sterować systemem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Контролировать работу устройства, машины или системы, часто отдавая команды с помощью электроники или компьютера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "устройством или системой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c8112d9-41a2-3c93-82ca-d1c4ca57bafb",
                    "sense_definition": "Mieć wpływ na przebieg zdarzeń, zachowanie ludzi lub funkcjonowanie organizacji; zarządzać czymś lub kimś, czasem w sposób ukryty.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie powinieneś pozwalać emocjom <w>sterować</w> twoimi ważnymi decyzjami życiowymi.",
                            "target_lang_translations": {
                                "ru": "Ты не должен позволять эмоциям <w>управлять</w> твоими важными жизненными решениями."
                            }
                        },
                        {
                            "text": "Polityk próbował <w>sterować</w> opinią publiczną poprzez media społecznościowe.",
                            "target_lang_translations": {
                                "ru": "Политик пытался <w>управлять</w> общественным мнением через социальные сети."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierować",
                        "manipulować",
                        "być u steru"
                    ],
                    "common_phrases": [
                        "sterować ludźmi",
                        "sterować rynkiem",
                        "sterować z tylnego siedzenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Влиять на ход событий, поведение людей или работу организации; руководить кем-либо или чем-либо, иногда скрыто."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "людьми или процессами"
                            },
                            {
                                "target_lang_word": "манипулировать",
                                "target_lang_sense_clarification": "скрыто влиять"
                            },
                            {
                                "target_lang_word": "руководить",
                                "target_lang_sense_clarification": "возглавлять"
                            }
                        ]
                    }
                }
            ]
        }
    ]

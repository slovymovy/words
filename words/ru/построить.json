                    ],
                    "common_phrases": [
                        "построить в шеренгу",
                        "построить отряд",
                        "построить солдат"
                    ],
                    "target_lang_definitions": {
                        "en": "To arrange people (usually military) in a formation or specific order."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "line up",
                                "target_lang_sense_clarification": "to arrange people in a row or queue"
                            },
                            {
                                "target_lang_word": "form up",
                                "target_lang_sense_clarification": "military term for arranging troops in formation"
                            }
                        ]
                    }
                }
            ]

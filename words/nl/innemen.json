                {
                    "sense_id": "3270c7df-0ce0-3678-9e97-f25e47f01689",
                    "sense_definition": "Medicijnen of voedsel inslikken of tot zich nemen",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ingest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet deze pillen twee keer per dag met een glas water <w>innemen</w>.",
                            "target_lang_translations": {
                                "en": "You must <w>take</w> these pills twice a day with a glass of water.",
                                "ru": "Вы должны <w>принимать</w> эти таблетки два раза в день, запивая стаканом воды."
                            }
                        },
                        {
                            "text": "De dokter adviseerde hem om het medicijn direct na de maaltijd <w>in</w> te <w>nemen</w>.",
                            "target_lang_translations": {
                                "en": "The doctor advised him to <w>take</w> the medicine immediately after the meal.",
                                "ru": "Врач посоветовал ему <w>принимать</w> лекарство сразу после еды."
                            }
                        }
                    ],
                    "synonyms": [
                        "slikken",
                        "gebruiken"
                    ],
                    "antonyms": [
                        "uitspugen"
                    ],
                    "common_phrases": [
                        "medicijnen innemen",
                        "een pil innemen"
                    ],
                    "target_lang_definitions": {
                        "en": "To swallow or consume medicine or food",
                        "ru": "Принимать лекарства или пищу, проглатывать"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "medicine"
                            },
                            {

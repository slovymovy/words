            "senses": [
                {
                    "sense_id": "a46d9b81-283b-3cb3-b3b7-c0b105cfad0c",
                    "sense_definition": "Druga osoba liczby pojedynczej trybu rozkazującego od czasownika <w>pojebać</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważaj, żebyś przez przypadek nie <w>pojeb</w> tych wszystkich kabli przy podłączaniu.",
                            "target_lang_translations": {
                                "ru": "Смотри, случайно не <w>перепутай</w> все эти кабели при подключении."
                            }
                        },
                        {
                            "text": "Tylko nie <w>pojeb</w> znowu zamówień, bo szef się wścieknie.",
                            "target_lang_translations": {
                                "ru": "Только не <w>перепутай</w> снова заказы, а то шеф взбесится."

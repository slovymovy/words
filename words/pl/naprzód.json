    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "cf541c1a-534d-32b5-982c-2d2f4ced9078",
                    "sense_definition": "W kierunku przed siebie; w stronę, w którą jest się zwróconym twarzą.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wykonaj jeden krok <w>naprzód</w>, a następnie obrót w lewo.",
                            "target_lang_translations": {
                                "ru": "Сделай один шаг <w>вперёд</w>, а затем поворот налево."
                            }
                        },
                        {
                            "text": "Musimy iść ciągle <w>naprzód</w>, żeby dotrzeć do celu przed zmrokiem.",
                            "target_lang_translations": {
                                "ru": "Мы должны идти постоянно <w>вперёд</w>, чтобы добраться до цели до наступления темноты."
                            }
                        }
                    ],
                    "synonyms": [
                        "do przodu",
                        "w przód",
                        "przed siebie"
                    ],
                    "antonyms": [
                        "do tyłu",
                        "wstecz",
                        "w tył",
                        "za siebie"
                    ],
                    "common_phrases": [
                        "krok naprzód",
                        "iść naprzód",
                        "patrzeć naprzód"
                    ],
                    "target_lang_definitions": {
                        "ru": "В направлении перед собой; в ту сторону, куда обращено лицо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вперёд",
                                "target_lang_sense_clarification": "направление движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3aef3c1-e9f6-3b5c-8c19-ceb36c9cb149",
                    "sense_definition": "Z wyprzedzeniem czasowym; zanim coś nastąpi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zapłacił mi czynsz za mieszkanie trzy miesiące <w>naprzód</w>.",
                            "target_lang_translations": {
                                "ru": "Он заплатил мне за аренду квартиры за три месяца <w>вперёд</w>."
                            }
                        },
                        {
                            "text": "Nie martw się na zapas i nie dziękuj mi <w>naprzód</w>.",
                            "target_lang_translations": {
                                "ru": "Не волнуйся про запас и не благодари меня <w>заранее</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "z góry",
                        "zawczasu",
                        "z wyprzedzeniem"
                    ],
                    "antonyms": [
                        "po fakcie"
                    ],
                    "common_phrases": [
                        "płacić naprzód",
                        "dziękować naprzód"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заблаговременно; до наступления события или с опережением во времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вперёд",
                                "target_lang_sense_clarification": "на будущее время"
                            },
                            {
                                "target_lang_word": "заранее",
                                "target_lang_sense_clarification": "заблаговременно"
                            },
                            {
                                "target_lang_word": "наперёд",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ef78a33-c441-3de1-a037-5ca52e1e8025",
                    "sense_definition": "Na czele grupy lub przed innymi osobami czy obiektami.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci niech wyjdą <w>naprzód</w>, inaczej nie będzie ich widać na zdjęciu.",
                            "target_lang_translations": {
                                "ru": "Пусть дети выйдут <w>вперёд</w>, иначе их не будет видно на фотографии."
                            }
                        },
                        {
                            "text": "Wysunął się <w>naprzód</w>, aby lepiej widzieć scenę.",
                            "target_lang_translations": {
                                "ru": "Он выдвинулся <w>вперёд</w>, чтобы лучше видеть сцену."
                            }
                        }
                    ],
                    "synonyms": [
                        "do pierwszego rzędu"
                    ],
                    "common_phrases": [
                        "wyjść naprzód",
                        "wysunąć się naprzód"
                    ],
                    "target_lang_definitions": {
                        "ru": "На передний план; положение перед другими людьми или объектами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вперёд",
                                "target_lang_sense_clarification": "на передний план"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c3308ab-b5b9-3623-aeaf-ef81ff317d39",
                    "sense_definition": "Jako pierwsza czynność w kolejności; najpierw.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wojownicy zbliżyli się do siebie i poczęli <w>naprzód</w> się lżyć wzajemnie.",
                            "target_lang_translations": {
                                "ru": "Воины сблизились и начали <w>сначала</w> оскорблять друг друга."
                            }
                        },
                        {
                            "text": "Musimy <w>naprzód</w> ustalić plan działania, a dopiero potem ruszać.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>сначала</w> определить план действий, и только потом выдвигаться."
                            }
                        }
                    ],
                    "synonyms": [
                        "najpierw",
                        "na początku",
                        "przede wszystkim",
                        "po pierwsze",
                        "wpierw",
                        "na wstępie"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie częściej używa się słowa 'najpierw'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В качестве первого действия; прежде всего (устаревшее или книжное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сначала",
                                "target_lang_sense_clarification": "в первую очередь"
                            },
                            {
                                "target_lang_word": "прежде",
                                "target_lang_sense_clarification": "перед тем как"
                            },
                            {
                                "target_lang_word": "сперва",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "f250438a-d786-343b-8514-c620b3df15c0",
                    "sense_definition": "Komenda lub okrzyk wzywający do ruszenia z miejsca lub ataku.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kapitan statku krzyknął głośno do załogi: „Cała <w>naprzód</w>!”",
                            "target_lang_translations": {
                                "ru": "Капитан корабля громко крикнул команде: «Полный <w>вперёд</w>!»"
                            }
                        },
                        {
                            "text": "<w>Naprzód</w>, moi dzielni żołnierze, do ataku!",
                            "target_lang_translations": {
                                "ru": "<w>Вперёд</w>, мои храбрые солдаты, в атаку!"
                            }
                        }
                    ],
                    "synonyms": [
                        "naprzód marsz"
                    ],
                    "common_phrases": [
                        "cała naprzód"
                    ],
                    "target_lang_definitions": {
                        "ru": "Команда или возглас, призывающий к движению или атаке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вперёд",
                                "target_lang_sense_clarification": "команда"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
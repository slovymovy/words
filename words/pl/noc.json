    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1776d1e2-60a6-3fc8-acab-450dc876ba28",
                    "sense_definition": "Pora doby od zachodu do wschodu słońca, kiedy na zewnątrz jest ciemno.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W <w>nocy</w> na niebie widać tysiące gwiazd.",
                            "target_lang_translations": {
                                "ru": "<w>Ночью</w> на небе видны тысячи звёзд."
                            }
                        },
                        {
                            "text": "Dzieci powinny spać w <w>nocy</w>, aby wypocząć.",
                            "target_lang_translations": {
                                "ru": "Дети должны спать <w>ночью</w>, чтобы отдохнуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "nocka"
                    ],
                    "antonyms": [
                        "dzień"
                    ],
                    "common_phrases": [
                        "w nocy",
                        "dobra noc",
                        "całą noc",
                        "późna noc"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время суток от заката до рассвета, когда на улице темно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ночь",
                                "target_lang_sense_clarification": "время суток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cec4ceee-659c-3aa6-afd5-5ee4e015b64d",
                    "sense_definition": "Ciemność panująca na zewnątrz z powodu braku światła słonecznego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gęsta <w>noc</w> utrudniała nam powrót do domu.",
                            "target_lang_translations": {
                                "ru": "Густая <w>тьма</w> затрудняла нам возвращение домой."
                            }
                        },
                        {
                            "text": "Za oknem panowała nieprzenikniona <w>noc</w>.",
                            "target_lang_translations": {
                                "ru": "За окном стояла непроглядная <w>ночь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciemność"
                    ],
                    "common_phrases": [
                        "czarna noc",
                        "ciemna noc"
                    ],
                    "target_lang_definitions": {
                        "ru": "Темнота, царящая на улице из-за отсутствия солнечного света."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тьма",
                                "target_lang_sense_clarification": "темнота"
                            },
                            {
                                "target_lang_word": "мрак",
                                "target_lang_sense_clarification": "густая темнота"
                            },
                            {
                                "target_lang_word": "ночь",
                                "target_lang_sense_clarification": "как синоним темноты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1778ae46-6676-3aec-8024-0f6c732e9fcd",
                    "sense_definition": "Wydarzenie kulturalne, rozrywkowe lub towarzyskie trwające w godzinach nocnych.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wybieramy się ze znajomymi na <w>noc</w> horrorów do kina.",
                            "target_lang_translations": {
                                "ru": "Мы с друзьями идем на <w>ночь</w> ужасов в кино."
                            }
                        },
                        {
                            "text": "Tegoroczna <w>noc</w> muzeów przyciągnęła tłumy turystów.",
                            "target_lang_translations": {
                                "ru": "В этом году <w>ночь</w> музеев привлекла толпы туристов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "noc muzeów",
                        "noc filmowa",
                        "noc poślubna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Культурное или развлекательное мероприятие, проходящее в ночное время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ночь",
                                "target_lang_sense_clarification": "мероприятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "276f5cd7-1f87-39b1-ac0e-49c6f3eec550",
                    "sense_definition": "Okres w historii postrzegany jako czas upadku, zła i tragicznych wydarzeń.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To była długa <w>noc</w> dla naszego kraju.",
                            "target_lang_translations": {
                                "ru": "Это была долгая <w>ночь</w> для нашей страны."
                            }
                        },
                        {
                            "text": "Mamy nadzieję, że ta polityczna <w>noc</w> wkrótce przeminie.",
                            "target_lang_translations": {
                                "ru": "Мы надеемся, что эта политическая <w>ночь</w> скоро пройдет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "długa noc",
                        "noc okupacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исторический период, воспринимаемый как время упадка, зла и трагических событий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ночь",
                                "target_lang_sense_clarification": "метафора тяжелого времени"
                            },
                            {
                                "target_lang_word": "мрак",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c695c65d-2b6c-38ae-8a29-b9e9820f3ea9",
                    "sense_definition": "Stan silnego oszołomienia lub otępienia umysłu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po uderzeniu w głowę ogarnęła go czarna <w>noc</w>.",
                            "target_lang_translations": {
                                "ru": "После удара по голове его накрыла черная <w>тьма</w>."
                            }
                        },
                        {
                            "text": "Czuł, jakby jego myśli spowiła <w>noc</w>.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал, как будто его мысли окутала <w>тьма</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "oszołomienie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние сильного помрачения сознания или отупения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тьма",
                                "target_lang_sense_clarification": "помутнение сознания"
                            },
                            {
                                "target_lang_word": "пелена",
                                "target_lang_sense_clarification": "затмение чувств"
                            }
                        ]
                    }
                }
            ]
        }

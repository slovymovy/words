    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "200470c9-a415-315b-b45d-d58d66079391",
                    "sense_definition": "Przechodzić z zewnątrz do wewnątrz jakiegoś pomieszczenia, budynku, pojazdu lub obszaru.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_in",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <w>wejść</w> do środka i zamknąć za sobą drzwi.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>войдите</w> внутрь и закройте за собой дверь."
                            }
                        },
                        {
                            "text": "Pasażerowie powoli <w>wchodzą</w> do pociągu stojącego na peronie.",
                            "target_lang_translations": {
                                "ru": "Пассажиры медленно <w>входят</w> в поезд, стоящий на платформе."
                            }
                        }
                    ],
                    "synonyms": [
                        "wkraczać",
                        "włazić"
                    ],
                    "antonyms": [
                        "wychodzić"
                    ],
                    "common_phrases": [
                        "wchodzić do domu",
                        "wchodzić do środka",
                        "wchodzić do pokoju",
                        "wchodzić bez pukania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться снаружи внутрь какого-либо помещения, здания, транспортного средства или территории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "входить",
                                "target_lang_sense_clarification": "внутрь помещения"
                            },
                            {
                                "target_lang_word": "заходить",
                                "target_lang_sense_clarification": "ненадолго или по пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e99498d-8244-30e2-a168-39d2f5b50afc",
                    "sense_definition": "Przemieszczać się w górę, na wyższy poziom lub na szczyt czegoś, używając nóg.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_up",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie muszę <w>wchodzić</w> po schodach na czwarte piętro.",
                            "target_lang_translations": {
                                "ru": "Мне приходится каждый день <w>подниматься</w> по лестнице на четвёртый этаж."
                            }
                        },
                        {
                            "text": "Turyści chętnie <w>wchodzą</w> na szczyt tej wysokiej góry.",
                            "target_lang_translations": {
                                "ru": "Туристы охотно <w>поднимаются</w> на вершину этой высокой горы."
                            }
                        }
                    ],
                    "synonyms": [
                        "zob. wspinać się",
                        "włazić"
                    ],
                    "antonyms": [
                        "schodzić",
                        "złazić"
                    ],
                    "common_phrases": [
                        "wchodzić po schodach",
                        "wchodzić na górę",
                        "wchodzić na szczyt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться вверх, на более высокий уровень или на вершину чего-либо, используя ноги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подниматься",
                                "target_lang_sense_clarification": "наверх, по лестнице"
                            },
                            {
                                "target_lang_word": "всходить",
                                "target_lang_sense_clarification": "на вершину, на гору"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1055fae-7557-3aee-9bf6-19e77f5631fa",
                    "sense_definition": "Otwierać stronę internetową lub uzyskiwać dostęp do systemu komputerowego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rzadko <w>wchodzę</w> na portale społecznościowe w godzinach pracy.",
                            "target_lang_translations": {
                                "ru": "Я редко <w>захожу</w> в социальные сети в рабочее время."
                            }
                        },
                        {
                            "text": "Musisz <w>wejść</w> w ustawienia, aby zmienić hasło.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>зайти</w> в настройки, чтобы изменить пароль."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wchodzić na stronę",
                        "wchodzić na konto",
                        "wchodzić w link"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открывать веб-страницу или получать доступ к компьютерной системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заходить",
                                "target_lang_sense_clarification": "на сайт, в соцсети"
                            },
                            {
                                "target_lang_word": "входить",
                                "target_lang_sense_clarification": "в систему, логиниться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7352ef75-c5a7-3889-8537-388e4f6eb61f",
                    "sense_definition": "Stawać się członkiem jakiejś organizacji, grupy, zespołu lub spółki.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "participation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polska <w>weszła</w> do Unii Europejskiej w dwa tysiące czwartym roku.",
                            "target_lang_translations": {
                                "ru": "Польша <w>вступила</w> в Европейский союз в две тысячи четвёртом году."
                            }
                        },
                        {
                            "text": "Mój brat chce <w>wejść</w> do spółki z doświadczonym inwestorem.",
                            "target_lang_translations": {
                                "ru": "Мой брат хочет <w>войти</w> в долю с опытным инвестором."
                            }
                        }
                    ],
                    "synonyms": [
                        "przystępować"
                    ],
                    "antonyms": [
                        "wychodzić"
                    ],
                    "common_phrases": [
                        "wchodzić do gry",
                        "wchodzić do spółki",
                        "wchodzić w układ"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться членом какой-либо организации, группы, команды или компании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вступать",
                                "target_lang_sense_clarification": "в организацию, в союз"
                            },
                            {
                                "target_lang_word": "входить",
                                "target_lang_sense_clarification": "в состав группы, в долю"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7af10a6e-b179-34e7-bbdd-3d5f973a4848",
                    "sense_definition": "Stawiać stopę na czymś lub w czymś; umieszczać nogi na jakiejś powierzchni.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_up",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę nie <w>wchodzić</w> na trawnik, bo trawa jest świeżo posiana.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>ходите</w> по газону, так как трава только что посеяна."
                            }
                        },
                        {
                            "text": "Dziecko przypadkiem <w>weszło</w> w głęboką kałużę po deszczu.",
                            "target_lang_translations": {
                                "ru": "Ребёнок случайно <w>наступил</w> в глубокую лужу после дождя."
                            }
                        }
                    ],
                    "synonyms": [
                        "włazić"
                    ],
                    "antonyms": [
                        "schodzić"
                    ],
                    "common_phrases": [
                        "wchodzić na drabinę",
                        "wchodzić na krzesło",
                        "wchodzić w kałużę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ставить ногу на что-то или во что-то; помещать ноги на какую-либо поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наступать",
                                "target_lang_sense_clarification": "на что-то, во что-то"
                            },
                            {
                                "target_lang_word": "заходить",
                                "target_lang_sense_clarification": "на территорию (напр. на газон)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82e5e6d2-e996-3361-a290-b1ff5bc1e16e",
                    "sense_definition": "Stanowić część jakiejś całości, zbioru lub struktury.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "composition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W skład tej komisji <w>wchodzi</w> pięciu niezależnych ekspertów.",
                            "target_lang_translations": {
                                "ru": "В состав этой комиссии <w>входят</w> пять независимых экспертов."
                            }
                        },
                        {
                            "text": "Do ceny biletu <w>wchodzi</w> również opłata za bagaż.",
                            "target_lang_translations": {
                                "ru": "В стоимость билета <w>входит</w> также плата за багаж."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wchodzić w skład",
                        "wchodzić w zakres"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть составной частью какого-либо целого, набора или структуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "входить",
                                "target_lang_sense_clarification": "в состав, в стоимость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7e9e4e5-645d-3071-963f-c3878b700f75",
                    "sense_definition": "Zaczynać znajdować się w określonym stanie, sytuacji lub fazie; rozpoczynać działanie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowe przepisy drogowe <w>wejdą</w> w życie od przyszłego miesiąca.",
                            "target_lang_translations": {
                                "ru": "Новые правила дорожного движения <w>вступят</w> в силу со следующего месяца."
                            }
                        },
                        {
                            "text": "To rozwiązanie jest zbyt drogie i w ogóle nie <w>wchodzi</w> w grę.",
                            "target_lang_translations": {
                                "ru": "Это решение слишком дорогое и вообще не <w>рассматривается</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wchodzić w życie",
                        "wchodzić w grę",
                        "wchodzić w nawyk",
                        "wchodzić w dorosłość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать находиться в определённом состоянии, ситуации или фазе; начинать действовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вступать",
                                "target_lang_sense_clarification": "в силу, в фазу"
                            },
                            {
                                "target_lang_word": "входить",
                                "target_lang_sense_clarification": "в привычку, в роль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1ea58db-bb9f-3eeb-b594-2bb1a6d0e10e",
                    "sense_definition": "Mieścić się w czymś; pasować rozmiarem do wnętrza czegoś.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fitting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te stare spodnie już na mnie nie <w>wchodzą</w>, bo przytyłem.",
                            "target_lang_translations": {
                                "ru": "Эти старые брюки на меня уже не <w>налезают</w>, так как я поправился."
                            }
                        },
                        {
                            "text": "Walizka jest tak pełna, że nic więcej do niej nie <w>wejdzie</w>.",
                            "target_lang_translations": {
                                "ru": "Чемодан так полон, что в него больше ничего не <w>влезет</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wchodzić na styk",
                        "nie wchodzić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещаться внутри чего-либо; подходить по размеру к внутреннему пространству чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влезать",
                                "target_lang_sense_clarification": "в ёмкость, внутрь"
                            },
                            {
                                "target_lang_word": "налезать",
                                "target_lang_sense_clarification": "об одежде"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
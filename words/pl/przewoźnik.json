        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e7a9a20-91e7-3edd-ae3e-ad34d9b324af",
                    "sense_definition": "Przedsiębiorstwo lub instytucja świadcząca usługi transportowe, zajmująca się zawodowo przewozem pasażerów lub towarów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transport_company",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polski <w>przewoźnik</w> powiększył swoją flotę o dwadzieścia nowych samolotów.",
                            "target_lang_translations": {
                                "ru": "Польский <w>перевозчик</w> увеличил свой флот на двадцать новых самолетов."
                            }
                        },
                        {
                            "text": "Tani <w>przewoźnik</w> oferuje bilety lotnicze w bardzo atrakcyjnych cenach.",
                            "target_lang_translations": {
                                "ru": "Бюджетный <w>перевозчик</w> предлагает авиабилеты по очень привлекательным ценам."
                            }
                        },
                        {
                            "text": "Prezes urzędu nałożył wysokie kary finansowe na nieuczciwych <w>przewoźników</w> kolejowych.",
                            "target_lang_translations": {
                                "ru": "Глава ведомства наложил высокие финансовые штрафы на недобросовестных железнодорожных <w>перевозчиков</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "firma transportowa",
                        "operator"
                    ],
                    "common_phrases": [
                        "przewoźnik lotniczy",
                        "przewoźnik kolejowy",
                        "narodowy przewoźnik",
                        "tani przewoźnik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предприятие или учреждение, предоставляющее транспортные услуги, профессионально занимающееся перевозкой пассажиров или грузов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевозчик",
                                "target_lang_sense_clarification": "транспортная компания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "039a23fd-85e4-3de9-89f4-6ca3d7c95e76",
                    "sense_definition": "Osoba, która zawodowo zajmuje się transportem i przewożeniem ładunków lub ludzi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy <w>przewoźnik</w> musi posiadać odpowiednią licencję na wykonywanie usług transportowych.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>перевозчик</w> должен иметь соответствующую лицензию на предоставление транспортных услуг."
                            }
                        },
                        {
                            "text": "Prywatny <w>przewoźnik</w> dostarczył nam zamówione meble prosto do domu.",
                            "target_lang_translations": {
                                "ru": "Частный <w>перевозчик</w> доставил нам заказанную мебель прямо домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "transportowiec",
                        "dostawca"
                    ],
                    "common_phrases": [
                        "prywatny przewoźnik",
                        "licencjonowany przewoźnik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, профессионально занимающееся транспортом и перевозкой грузов или людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевозчик",
                                "target_lang_sense_clarification": "лицо, оказывающее услуги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39603d2c-884c-367a-9db1-32a8ed5821b8",

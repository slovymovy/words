                    ]
                },
                {
                    "sense_id": "ad8b9d1f-ec8d-315e-b6b8-d605ee973df4",
                    "sense_definition": "Отправиться в путь, начать поход (обычно о группе людей или войске).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир приказал отряду <выступить> на рассвете, пока враг спит."
                        },
                        {
                            "text": "Экспедиция готова <выступить> в горы, как только закончится дождь."
                        }
                    ],
                    "synonyms": [
                        "отпра́виться"
                    ],
                    "common_phrases": [

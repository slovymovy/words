                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подтёк",
                                "target_lang_sense_clarification": "след от вытекшей жидкости"
                            },
                            {
                                "target_lang_word": "выделения",
                                "target_lang_sense_clarification": "медицинское; субстанция"
                            }
                        ]
                    }
                }
            ]
        }

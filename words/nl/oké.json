{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "b9b187f2-376c-3eb0-ae88-d0bb24449e92",
                    "sense_definition": "Wordt gebruikt om instemming te betuigen, goedkeuring te geven of te bevestigen dat men iets begrepen heeft.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ga je mee naar de film? <w>Oké</w>, ik pak mijn jas.",
                            "target_lang_translations": {
                                "en": "Are you coming to the movie? <w>OK</w>, I'll grab my coat.",
                                "ru": "Ты пойдешь в кино? <w>Ладно</w>, я возьму куртку."
                            }
                        },
                        {
                            "text": "<w>Oké</w>, ik begrijp wat je bedoelt en ik zal het regelen.",
                            "target_lang_translations": {
                                "en": "<w>Okay</w>, I understand what you mean and I'll arrange it.",
                                "ru": "<w>Хорошо</w>, я понимаю, что ты имеешь в виду, и я всё улажу."
                            }
                        }
                    ],
                    "synonyms": [
                        "okido",
                        "okidoki"
                    ],
                    "common_phrases": [
                        "is oké",
                        "oké dan"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to express agreement, give approval, or confirm that one has understood something.",
                        "ru": "Используется для выражения согласия, одобрения или подтверждения понимания."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "OK",
                                "target_lang_sense_clarification": "expressing agreement or understanding"
                            },
                            {
                                "target_lang_word": "okay",
                                "target_lang_sense_clarification": "alternative spelling"
                            },
                            {
                                "target_lang_word": "all right",
                                "target_lang_sense_clarification": "synonym for agreement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хорошо",
                                "target_lang_sense_clarification": "согласие"
                            },
                            {
                                "target_lang_word": "ладно",
                                "target_lang_sense_clarification": "одобрение"
                            },
                            {
                                "target_lang_word": "окей",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "договорились",
                                "target_lang_sense_clarification": "подтверждение договоренности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ccb9798-16fc-38b6-a49f-96b805aa1834",
                    "sense_definition": "Wordt gebruikt om iets toe te geven, een concessie te doen of een overgang in het gesprek te markeren.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Oké</w>, je hebt gelijk wat betreft de kosten, maar we moeten het toch doen.",
                            "target_lang_translations": {
                                "en": "<w>Okay</w>, you're right regarding the costs, but we still have to do it.",
                                "ru": "<w>Ладно</w>, ты прав насчет расходов, но мы всё равно должны это сделать."
                            }
                        },
                        {
                            "text": "<w>Oké</w>, laten we nu overgaan naar het volgende punt op de agenda.",
                            "target_lang_translations": {
                                "en": "<w>OK</w>, let's move on to the next item on the agenda.",
                                "ru": "<w>Хорошо</w>, давайте перейдем к следующему пункту повестки дня."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oké maar"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to admit something, make a concession, or mark a transition in the conversation.",
                        "ru": "Используется для выражения уступки или обозначения перехода к новой теме в разговоре."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "OK",
                                "target_lang_sense_clarification": "discourse marker or concession"
                            },
                            {
                                "target_lang_word": "okay",
                                "target_lang_sense_clarification": "alternative spelling"
                            },
                            {
                                "target_lang_word": "fine",
                                "target_lang_sense_clarification": "indicating reluctant agreement or concession"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ладно",
                                "target_lang_sense_clarification": "уступка"
                            },
                            {
                                "target_lang_word": "хорошо",
                                "target_lang_sense_clarification": "смена темы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6b1d8937-c43f-3b5f-bfe8-4f0e0767f71c",
                    "sense_definition": "Voldoende van kwaliteit, in orde of acceptabel; niet slecht maar ook niet uitstekend.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soep was <w>oké</w>, maar het dessert was echt geweldig.",
                            "target_lang_translations": {
                                "en": "The soup was <w>okay</w>, but the dessert was really amazing.",
                                "ru": "Суп был <w>нормальный</w>, но десерт был просто потрясающий."
                            }
                        },
                        {
                            "text": "Vind je het <w>oké</w> als ik vanavond wat later thuiskom?",
                            "target_lang_translations": {
                                "en": "Is it <w>okay</w> if I come home a bit later tonight?",
                                "ru": "Всё <w>в порядке</w>, если я сегодня приду домой попозже?"
                            }
                        }
                    ],
                    "synonyms": [
                        "aanvaardbaar",
                        "bevredigend"
                    ],
                    "antonyms": [
                        "onaanvaardbaar",
                        "slecht"
                    ],
                    "common_phrases": [
                        "helemaal oké",
                        "alles oké"
                    ],
                    "target_lang_definitions": {
                        "en": "Of sufficient quality, in order, or acceptable; not bad but not excellent.",
                        "ru": "Приемлемый по качеству, в порядке; неплохой, но и не отличный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "OK",
                                "target_lang_sense_clarification": "satisfactory or acceptable"
                            },
                            {
                                "target_lang_word": "okay",
                                "target_lang_sense_clarification": "alternative spelling"
                            },
                            {
                                "target_lang_word": "all right",
                                "target_lang_sense_clarification": "acceptable"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нормальный",
                                "target_lang_sense_clarification": "среднего качества"
                            },
                            {
                                "target_lang_word": "в порядке",
                                "target_lang_sense_clarification": "приемлемый"
                            },
                            {
                                "target_lang_word": "ничего",
                                "target_lang_sense_clarification": "сносный, сойдет"
                            }
                        ]
                    }
                }
            ]
        }

            "senses": [
                {
                    "sense_id": "b37e6f35-d304-30a6-bb70-a3515b261373",
                    "sense_definition": "Het begin of de eerste stap van een proces of handeling.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schets was de eerste <w>aanzet</w> tot het maken van een nieuw type auto.",
                            "target_lang_translations": {
                                "en": "The sketch was the first <w>step</w> towards making a new type of car.",
                                "ru": "Эскиз стал первым <w>шагом</w> к созданию нового типа автомобиля."
                            }
                        },
                        {
                            "text": "Dit plan vormt een goede <w>aanzet</w> voor verdere discussie.",
                            "target_lang_translations": {
                                "en": "This plan forms a good <w>starting point</w> for further discussion.",
                                "ru": "Этот план служит хорошим <w>началом</w> для дальнейшего обсуждения."
                            }
                        }
                    ],
                    "synonyms": [
                        "begin",
                        "start",
                        "aanvang"
                    ],
                    "antonyms": [
                        "einde",
                        "slot"
                    ],
                    "common_phrases": [
                        "de eerste aanzet",
                        "een aanzet tot"
                    ],
                    "target_lang_definitions": {
                        "en": "The beginning or the first step of a process or action.",
                        "ru": "Начало или первый шаг процесса или действия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "general beginning of something"
                            },
                            {
                                "target_lang_word": "impetus",
                                "target_lang_sense_clarification": "the force that makes something happen or happen more quickly"
                            },
                            {
                                "target_lang_word": "initiative",
                                "target_lang_sense_clarification": "an act or strategy intended to resolve a difficulty or improve a situation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "начало",
                                "target_lang_sense_clarification": "отправная точка"
                            },
                            {
                                "target_lang_word": "первый шаг",
                                "target_lang_sense_clarification": "начальное действие"
                            }
                        ]

{
    "word_family": [
        "publisher",
        "publication",
        "republish",
        "unpublished"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3257f6c6-a3c2-32ff-97b3-165fd029421b",
                    "sense_definition": "To prepare and issue a book, magazine, or other material for sale or distribution.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "publishing_industry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company plans to <w>publish</w> the new novel next month.",
                            "target_lang_translations": {
                                "ru": "Компания планирует <w>издать</w> новый роман в следующем месяце."
                            }
                        },
                        {
                            "text": "He has <w>published</w> several books on European history.",
                            "target_lang_translations": {
                                "ru": "Он <w>издал</w> несколько книг по истории Европы."
                            }
                        }
                    ],
                    "synonyms": [
                        "post"
                    ],
                    "common_phrases": [
                        "publish a book",
                        "publish a novel",
                        "publish a magazine",
                        "publish an article"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовить и выпускать книгу, журнал или другой материал для продажи или распространения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "издавать",
                                "target_lang_sense_clarification": "выпускать в свет (книги, журналы)"
                            },
                            {
                                "target_lang_word": "публиковать",
                                "target_lang_sense_clarification": "размещать в печати"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "991aabd4-0094-3fdd-9595-6ce610ae78b1",
                    "sense_definition": "To make information or a document generally known to the public.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "make_public",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government will <w>publish</w> the election results tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Правительство <w>опубликует</w> результаты выборов завтра утром."
                            }
                        },
                        {
                            "text": "They decided to <w>publish</w> the findings of the investigation online.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>опубликовать</w> результаты расследования в интернете."
                            }
                        }
                    ],
                    "synonyms": [
                        "disclose",
                        "make known",
                        "post"
                    ],
                    "common_phrases": [
                        "publish a report",
                        "publish findings",
                        "publish data",
                        "publish online"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать информацию или документ общеизвестными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнародовать",
                                "target_lang_sense_clarification": "делать всеобщим достоянием"
                            },
                            {
                                "target_lang_word": "опубликовать",
                                "target_lang_sense_clarification": "размещать в СМИ или интернете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9aa80489-4e07-3098-8d3a-bf0aa1421c85",
                    "sense_definition": "To be released or made available in a publication.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "publishing_industry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The story first <w>published</w> in a local newspaper before going national.",
                            "target_lang_translations": {
                                "ru": "История впервые <w>была опубликована</w> в местной газете, прежде чем стать известной на всю страну."
                            }
                        },
                        {
                            "text": "His latest article <w>published</w> on the website yesterday.",
                            "target_lang_translations": {
                                "ru": "Его последняя статья <w>вышла</w> на сайте вчера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "article published",
                        "story published"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть выпущенным или стать доступным в публикации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выходить",
                                "target_lang_sense_clarification": "появляться в печати"
                            },
                            {
                                "target_lang_word": "публиковаться",
                                "target_lang_sense_clarification": "быть изданным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0246602-39d1-3b4d-82b5-079915b9ca07",
                    "sense_definition": "To have one's work accepted and printed in a publication, often for academic or professional credit.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "publishing_industry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists must <w>publish</w> regularly to advance their careers.",
                            "target_lang_translations": {
                                "ru": "Ученые должны регулярно <w>публиковаться</w>, чтобы продвигаться по карьерной лестнице."
                            }
                        },
                        {
                            "text": "She hopes to <w>publish</w> in a leading medical journal this year.",
                            "target_lang_translations": {
                                "ru": "Она надеется <w>опубликоваться</w> в ведущем медицинском журнале в этом году."
                            }
                        }
                    ],
                    "common_phrases": [
                        "publish in a journal",
                        "publish or perish",
                        "need to publish"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публиковать свои работы в научных или профессиональных изданиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публиковаться",
                                "target_lang_sense_clarification": "печатать свои труды"
                            },
                            {
                                "target_lang_word": "печататься",
                                "target_lang_sense_clarification": "издавать свои работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76527af4-bbe9-37cc-822d-9efa01e3ce02",
                    "sense_definition": "To make data or an event available to other parts of a software system.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The server will <w>publish</w> updates to all connected clients immediately.",
                            "target_lang_translations": {
                                "ru": "Сервер немедленно <w>опубликует</w> обновления для всех подключенных клиентов."
                            }
                        },
                        {
                            "text": "You need to <w>publish</w> the message to the correct queue for processing.",
                            "target_lang_translations": {
                                "ru": "Вам нужно <w>опубликовать</w> сообщение в правильную очередь для обработки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "publish and subscribe",
                        "publish an event",
                        "publish a message"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical (computing)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать данные или событие доступными для других частей программной системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публиковать",
                                "target_lang_sense_clarification": "отправлять данные подписчикам"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        "source_sense_id": "9bf9f884-6928-39ee-b223-108790240744",
                        "source_sense_description": "Anordnung von Drähten oder Stäben in einem Rahmen",
                        "target_lang_word": "Gitter",
                        "target_lang_code": "de",
                        "target_sense_ids": [],
                        "cross_references": [
                            {
                                "target_word": "Gitter",

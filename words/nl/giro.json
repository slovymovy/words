                {
                    "sense_id": "82ff0170-8f58-35bc-a04b-2bc375b33693",
                    "sense_definition": "Een betaalsysteem waarmee geld tussen rekeningen wordt overgemaakt zonder contant geld, vaak geassocieerd met de voormalige postbank.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "financial_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb het geld gisteren via de <w>giro</w> naar je overgemaakt.",
                            "target_lang_translations": {
                                "en": "I transferred the money to you yesterday via the <w>giro</w>.",
                                "ru": "Я перевел тебе деньги вчера через <w>систему жиро</w>."
                            }
                        },
                        {
                            "text": "Vroeger hadden veel mensen een rekening bij de <w>giro</w> in plaats van bij een algemene bank.",
                            "target_lang_translations": {
                                "en": "In the past, many people had an account with the <w>giro</w> instead of a general bank.",
                                "ru": "Раньше у многих людей был счет в <w>системе жиро</w>, а не в обычном банке."
                            }
                        }
                    ],
                    "synonyms": [
                        "betaaldienst"
                    ],
                    "common_phrases": [
                        "via de giro",
                        "op de giro"
                    ],
                    "target_lang_definitions": {
                        "en": "A payment system used to transfer money between accounts without cash, often associated with the former postal bank.",
                        "ru": "Платежная система, используемая для перевода денег между счетами без использования наличных, часто ассоциируется с бывшим почтовым банком."
                    },

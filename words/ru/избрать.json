            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4f7ebea2-aa29-3c27-bf8d-160b065e1e58",
                    "sense_definition": "Выбрать кого-либо на какую-либо должность или для выполнения обязанностей путём голосования.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "choice_selection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Народ <избрал> нового президента большинством голосов.",
                            "target_lang_translations": {
                                "en": "The people <w>elected</w> a new president by a majority vote."
                            }
                        },
                        {
                            "text": "На собрании мы <избрали> председателя комитета.",
                            "target_lang_translations": {
                                "en": "At the meeting, we <w>elected</w> a committee chairman."
                            }
                        }
                    ],
                    "synonyms": [
                        "выбрать"
                    ],
                    "common_phrases": [
                        "избрать президента",
                        "избрать депутата",
                        "избрать председателя",
                        "избрать губернатора"
                    ],
                    "target_lang_definitions": {
                        "en": "To choose someone for a position or duty by voting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "elect",
                                "target_lang_sense_clarification": "to choose by voting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffa76b28-f031-3aba-a868-ed68eaa986f0",
                    "sense_definition": "Отдать предпочтение кому-либо или чему-либо из ряда возможных вариантов; выбрать (обычно о жизненном пути, профессии, методе).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "choice_selection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодой человек <избрал> медицину своей будущей профессией.",
                            "target_lang_translations": {
                                "en": "The young man <w>chose</w> medicine as his future profession."
                            }
                        },
                        {
                            "text": "Для решения этой сложной задачи они <избрали> правильную тактику.",
                            "target_lang_translations": {
                                "en": "To solve this complex problem, they <w>chose</w> the right tactic."
                            }
                        }
                    ],
                    "synonyms": [
                        "выбрать",
                        "предпочесть"
                    ],
                    "antonyms": [
                        "отвергнуть"
                    ],
                    "common_phrases": [
                        "избрать путь",
                        "избрать профессию",
                        "избрать тактику",
                        "избрать метод"
                    ],
                    "target_lang_definitions": {
                        "en": "To select or prefer something from a range of options (often regarding a life path, profession, or method)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "choose",
                                "target_lang_sense_clarification": "to select or pick"
                            }
                        ]
                    }
                }
            ]
        }
    ]

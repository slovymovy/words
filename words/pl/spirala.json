                {
                    "sense_id": "665d2dcb-3552-3d42-92e5-da40f99d250c",
                    "sense_definition": "Linia krzywa, która wielokrotnie okrąża punkt środkowy, stopniowo się od niego oddalając, lub przedmiot o takim kształcie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dym z komina układał się w delikatną <w>spiralę</w> na bezwietrznym niebie."
                        },
                        {
                            "text": "Dziecko narysowało na kartce kolorową <w>spiralę</w> za pomocą kredek."
                        }
                    ],
                    "common_phrases": [

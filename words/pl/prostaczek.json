            "pos": "noun",
            "senses": [
                {
                    "sense_id": "573790b5-e1b9-3292-b6e8-32dca52be897",
                    "sense_definition": "Człowiek prosty, niewykształcony, często naiwny lub pokorny; zdrobnienie od słowa <w>prostak</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "simple_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Święty Franciszek z Asyżu często określał siebie jako boży <w>prostaczek</w>.",
                            "target_lang_translations": {
                                "ru": "Святой Франциск Ассизский часто называл себя божьим <w>простачком</w>."
                            }
                        },
                        {
                            "text": "Ten biedny <w>prostaczek</w> nie rozumiał skomplikowanego języka urzędników.",
                            "target_lang_translations": {
                                "ru": "Этот бедный <w>простачок</w> не понимал сложного языка чиновников."
                            }
                        }
                    ],
                    "synonyms": [
                        "maluczki"
                    ],
                    "common_phrases": [
                        "boży prostaczek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простой, необразованный человек, часто наивный или смиренный; уменьшительная форма от слова prostak."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простачок",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное; наивный, простодушный человек"
                            },
                            {
                                "target_lang_word": "простак",
                                "target_lang_sense_clarification": "бесхитростный или недалекий человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f543b95-f904-3f6e-a2d3-d221ed526ccb",
                    "sense_definition": "Forma fleksyjna (dopełniacz liczby mnogiej) rzeczownika <w>prostaczka</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej grupie nie było żadnych <w>prostaczek</w>, same wykształcone kobiety.",
                            "target_lang_translations": {
                                "ru": "В этой группе не было никаких <w>простушек</w>, только образованные женщины."
                            }
                        },
                        {
                            "text": "Unikał towarzystwa wiejskich <w>prostaczek</w>, woląc damy z miasta.",
                            "target_lang_translations": {
                                "ru": "Он избегал общества деревенских <w>простушек</w>, предпочитая городских дам."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural of prostaczka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного prostaczka (простушка, необразованная женщина)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простушек",
                                "target_lang_sense_clarification": "форма слова \"простушка\" (родительный падеж мн. ч.)"
                            }
                        ]
                    }
                }
            ]
        }

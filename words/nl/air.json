                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "внешний облик, выражение лица, манера держаться"
                            },
                            {
                                "target_lang_word": "важность",
                                "target_lang_sense_clarification": "напускная значимость, высокомерие"
                            },
                            {
                                "target_lang_word": "манера",
                                "target_lang_sense_clarification": "способ держать себя"
                            }
                        ]
                    }
                },

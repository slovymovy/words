    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "93fd3182-d410-3aa5-83f5-6d490bb366dc",
                    "sense_definition": "Een insect dat op een vlinder lijkt en meestal 's nachts actief is; specifiek ook de soort waarvan de larven gaten in kleding maken",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "insect_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zitten gaten in mijn wollen trui door de <w>motten</w>.",
                            "target_lang_translations": {
                                "ru": "В моем шерстяном свитере дырки от <w>моли</w>."
                            }
                        },
                        {
                            "text": "Toen ik het licht aandeed, vloog er een <w>mot</w> door de kamer.",
                            "target_lang_translations": {
                                "ru": "Когда я включил свет, по комнате летала <w>моль</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "klerenmot",
                        "zeug"
                    ],
                    "target_lang_definitions": {
                        "ru": "Насекомое, похожее на бабочку, обычно активное ночью; в частности, вид, личинки которого прогрызают дырки в одежде"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "моль",
                                "target_lang_sense_clarification": "вредитель одежды"
                            },
                            {
                                "target_lang_word": "ночная бабочка",
                                "target_lang_sense_clarification": "насекомое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6aa3bbc-26d1-306a-860a-1315d55f0403",
                    "sense_definition": "Een situatie waarin mensen ruzie of onenigheid met elkaar hebben",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conflict_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft alweer <w>mot</w> met de buren over het lawaai.",
                            "target_lang_translations": {
                                "ru": "У него опять <w>ссора</w> с соседями из-за шума."
                            }
                        },
                        {
                            "text": "Als je <w>mot</w> zoekt, kun je dat krijgen.",
                            "target_lang_translations": {
                                "ru": "Если ты ищешь <w>ссоры</w>, ты её получишь."
                            }
                        }
                    ],
                    "synonyms": [
                        "klerenmot",
                        "zeug"
                    ],
                    "common_phrases": [
                        "mot hebben",
                        "mot zoeken"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, в которой люди ссорятся или имеют разногласия"
                    },
                    "translations": {

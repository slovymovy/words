                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "день рожде́ния",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "годовщи́на",
                                "target_lang_sense_clarification": "юбилей основания"
                            }
                        ]
                    }
                }
            ]
        },

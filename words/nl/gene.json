{
    "word_family": [
        "degene",
        "ginds",
        "ginder",
        "generzijds"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "47838890-6a0d-360a-bbf1-49abbec17cde",
                    "sense_definition": "Iemand; de persoon die bedoeld wordt (voornamelijk gebruikt in de samenstelling 'degene').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_ref",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is <w>degene</w> die verantwoordelijk is voor het project.",
                            "target_lang_translations": {
                                "ru": "Он — <w>тот</w>, кто отвечает за этот проект."
                            }
                        },
                        {
                            "text": "Ik zoek <w>degene</w> die mijn fiets heeft gevonden.",
                            "target_lang_translations": {
                                "ru": "Я ищу <w>того</w>, кто нашел мой велосипед."
                            }
                        }
                    ],
                    "common_phrases": [
                        "degene die"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "in samenstellingen"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот (человек), о котором идет речь; лицо, которое подразумевается (обычно используется в составе слова 'degene')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тот",
                                "target_lang_sense_clarification": "указательное местоимение, ссылающееся на человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c768a1df-b993-3ff6-a156-139c79acab78",
                    "sense_definition": "Die of dat daar; verwijst naar de andere of de verste van twee genoemde zaken of personen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "demonstrative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aan <w>gene</w> zijde van de rivier ligt een uitgestrekt bos.",
                            "target_lang_translations": {
                                "ru": "На <w>той</w> стороне реки раскинулся обширный лес."
                            }
                        },
                        {
                            "text": "Zowel deze als <w>gene</w> waren het niet eens met het besluit.",
                            "target_lang_translations": {
                                "ru": "Как этот, так и <w>тот</w> были не согласны с решением."
                            }
                        }
                    ],
                    "synonyms": [
                        "die",
                        "gindse",
                        "de andere"
                    ],
                    "antonyms": [
                        "deze"
                    ],
                    "common_phrases": [
                        "aan gene zijde",
                        "deze en gene"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "formeel"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот (там); указывает на другой или более удаленный из двух упомянутых предметов или лиц (часто в противопоставлении «этому»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тот",
                                "target_lang_sense_clarification": "дальний, противоположный «этому»"
                            },
                            {
                                "target_lang_word": "другой",
                                "target_lang_sense_clarification": "иной, находящийся на другой стороне"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "0b63b8e3-2912-351d-825b-23deeb28634c",
                    "sense_definition": "Verbogen vorm van <w>geen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "negation_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb <w>gene</w> tijd om naar dat gezeur te luisteren.",
                            "target_lang_translations": {
                                "ru": "У меня <w>нет</w> времени слушать это нытье."
                            }
                        },
                        {
                            "text": "Er is <w>gene</w> man overboord als we iets later vertrekken.",
                            "target_lang_translations": {
                                "ru": "<w>Никакой</w> беды не будет, если мы отправимся чуть позже."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Vlaams"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "standaardnederlands"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Склоняемая или устаревшая форма отрицательного местоимения «geen» (никакой, нет)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "никакой",
                                "target_lang_sense_clarification": "отрицательное местоимение"
                            },
                            {
                                "target_lang_word": "нет",
                                "target_lang_sense_clarification": "отсутствие чего-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "drijven",
        "overdrijving",
        "verdrijven",
        "afdrijven",
        "aandrijven"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0cf308b8-5a97-35aa-8014-148cb556a604",
                    "sense_definition": "Iets groter, mooier, erger of belangrijker voorstellen dan het in werkelijkheid is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "exaggerate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De actievoerder <w>overdrijft</w> nogal over het aantal aanwezige betogers.",
                            "target_lang_translations": {
                                "en": "The activist <w>exaggerates</w> quite a bit about the number of protesters present."
                            }
                        },
                        {
                            "text": "Je moet niet zo <w>overdrijven</w>, het probleem is veel kleiner dan je denkt.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>exaggerate</w> so much; the problem is much smaller than you think."
                            }
                        }
                    ],
                    "synonyms": [
                        "overgaan",
                        "voorbijgaan",
                        "weggaan",
                        "passeren"
                    ],
                    "antonyms": [
                        "bagatelliseren",
                        "afzwakken"
                    ],
                    "common_phrases": [
                        "zwaar overdrijven",
                        "niet overdrijven"
                    ],
                    "target_lang_definitions": {
                        "en": "To represent something as bigger, better, worse, or more important than it really is."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exaggerate",
                                "target_lang_sense_clarification": "to represent as larger, better, or worse than reality"
                            },
                            {
                                "target_lang_word": "overstate",
                                "target_lang_sense_clarification": "to state in too strong terms"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "beb2a5e9-6a23-38d0-ad99-f2baf11af1ef",
                    "sense_definition": "Voorbijgaan of verdwijnen, vaak gezegd van wolken, buien of gemoedstoestanden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pass_by",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Alle verdriet <w>drijft</w> na een zekere tijd weer <w>over</w>.",
                            "target_lang_translations": {
                                "en": "All grief <w>blows over</w> after a certain time."
                            }
                        },
                        {
                            "text": "Gelukkig is de zware onweersbui snel <w>overgedreven</w>.",
                            "target_lang_translations": {
                                "en": "Fortunately, the heavy thunderstorm <w>blew over</w> quickly."
                            }
                        }
                    ],
                    "synonyms": [
                        "overgaan",
                        "voorbijgaan",
                        "weggaan",
                        "weggaan"
                    ],
                    "common_phrases": [
                        "de bui drijft over"
                    ],
                    "target_lang_definitions": {
                        "en": "To pass by or disappear, often said of clouds, showers, or states of mind."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blow over",
                                "target_lang_sense_clarification": "to subside or pass by (of a storm or situation)"
                            },
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "to come to an end; to go by"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb95ae87-6822-3fdc-889d-6c6fcde3d0ed",
                    "sense_definition": "Door de wind of stroming drijvend de overkant bereiken.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "float_across",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je rubberbootje <w>drijft</w> <w>over</w> als je het niet snel gaat halen.",
                            "target_lang_translations": {
                                "en": "Your rubber dinghy will <w>drift over</w> if you don't go get it quickly."
                            }
                        },
                        {
                            "text": "De bal is via de sloot naar de buren <w>overgedreven</w>.",
                            "target_lang_translations": {
                                "en": "The ball <w>floated over</w> to the neighbors via the ditch."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To reach the other side by floating on wind or current."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drift over",
                                "target_lang_sense_clarification": "to be carried across by a current or air"
                            },
                            {
                                "target_lang_word": "float across",
                                "target_lang_sense_clarification": "to move to the other side on the surface of a liquid or in air"
                            },
                            {
                                "target_lang_word": "blow over",
                                "target_lang_sense_clarification": "to be driven across by the wind"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "yearly",
        "yearlong",
        "yearling"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c0931b10-a2ca-3e04-baeb-1889b3aa65be",
                    "sense_definition": "A unit of time equal to 12 months or 365 days, used to measure age or duration.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moved to this town exactly one <w>year</w> ago.",
                            "target_lang_translations": {
                                "ru": "Мы переехали в этот город ровно один <w>год</w> назад."
                            }
                        },
                        {
                            "text": "My younger brother is twenty <w>years</w> old.",
                            "target_lang_translations": {
                                "ru": "Моему младшему брату двадцать <w>лет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "twelvemonth"
                    ],
                    "common_phrases": [
                        "last year",
                        "next year",
                        "this year",
                        "every year",
                        "years old"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица времени, равная 12 месяцам или 365 дням, используемая для измерения возраста или продолжительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "год",
                                "target_lang_sense_clarification": "единица времени"
                            },
                            {
                                "target_lang_word": "лет",
                                "target_lang_sense_clarification": "форма мн. ч. (для возраста и сроков)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e74f79b-d5a8-312c-b8a9-af8759d8b026",
                    "sense_definition": "A specific period in a calendar identified by a number.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was born in the <w>year</w> 1995.",
                            "target_lang_translations": {
                                "ru": "Она родилась в 1995 <w>году</w>."
                            }
                        },
                        {
                            "text": "The <w>year</w> 2000 marked the beginning of a new millennium.",
                            "target_lang_translations": {
                                "ru": "2000 <w>год</w> ознаменовал начало нового тысячелетия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leap year",
                        "calendar year",
                        "new year"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный период в календаре, обозначаемый числом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "год",
                                "target_lang_sense_clarification": "календарный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c14f8c0-0bcd-32a0-947f-08ad9f47f94f",
                    "sense_definition": "A period of roughly twelve months associated with a particular activity, such as school, business, or finance.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company reported a loss for the financial <w>year</w>.",
                            "target_lang_translations": {
                                "ru": "Компания отчиталась об убытках за финансовый <w>год</w>."
                            }
                        },
                        {
                            "text": "She decided to take a gap <w>year</w> before starting university.",
                            "target_lang_translations": {
                                "ru": "Она решила взять академический <w>год</w> перед поступлением в университет."
                            }
                        }
                    ],
                    "synonyms": [
                        "annum"
                    ],
                    "common_phrases": [
                        "school year",
                        "fiscal year",
                        "academic year",
                        "gap year"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период около двенадцати месяцев, связанный с определенной деятельностью (учебой, бизнесом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "год",
                                "target_lang_sense_clarification": "отчетный или учебный период"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85b8a42c-70e8-3ac9-bd76-74b23a6cda5c",
                    "sense_definition": "A group of students of the same age or grade level in a school.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "school_grade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Every student in <w>year</w> ten must choose their electives.",
                            "target_lang_translations": {
                                "ru": "Каждый ученик в десятом <w>классе</w> должен выбрать факультативы."
                            }
                        },
                        {
                            "text": "He is the tallest boy in his <w>year</w>.",
                            "target_lang_translations": {
                                "ru": "Он самый высокий мальчик в своем <w>классе</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "final year",
                        "first year"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in UK, Australia, New Zealand"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа учащихся одного возраста или уровня обучения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "класс",
                                "target_lang_sense_clarification": "в школе"
                            },
                            {
                                "target_lang_word": "курс",
                                "target_lang_sense_clarification": "в университете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b0237de-3d17-31a0-b9c2-443dc2ec1368",
                    "sense_definition": "The time it takes for a planet or other celestial body to complete one orbit around a star.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A Martian <w>year</w> is almost twice as long as an Earth year.",
                            "target_lang_translations": {
                                "ru": "Марсианский <w>год</w> почти в два раза длиннее земного."
                            }
                        },
                        {
                            "text": "Astronomers calculate the length of a <w>year</w> on distant exoplanets.",
                            "target_lang_translations": {
                                "ru": "Астрономы вычисляют продолжительность <w>года</w> на далеких экзопланетах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "solar year",
                        "light year"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical or scientific context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Время, за которое небесное тело совершает полный оборот вокруг звезды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "год",
                                "target_lang_sense_clarification": "астрономический"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
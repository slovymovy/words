                        "послушай детка"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used as a form of address like 'baby' or 'babe'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A familiar or affectionate term of address for a girl or young woman"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "baby",
                                "target_lang_sense_clarification": "term of endearment"
                            },
                            {
                                "target_lang_word": "babe",
                                "target_lang_sense_clarification": "informal term of endearment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ceeea5b-c66c-3e99-9b04-290f2902832f",

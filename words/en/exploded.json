{
    "word_family": [
        "explode",
        "explosion",
        "explosive"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cfd62685-a805-3e05-a970-cbfd455130c3",
                    "sense_definition": "Simple past and past participle of <w>explode</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fireworks <w>exploded</w> in the night sky, creating a beautiful display.",
                            "target_lang_translations": {
                                "ru": "Фейерверк <w>взорвался</w> в ночном небе, создав красивое зрелище."
                            }
                        },
                        {
                            "text": "Her anger <w>exploded</w> when she found out the truth.",
                            "target_lang_translations": {
                                "ru": "Её гнев <w>вспыхнул</w>, когда она узнала правду."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past simple and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола explode."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взорвался",
                                "target_lang_sense_clarification": "о физическом взрыве"
                            },
                            {
                                "target_lang_word": "вспылил",
                                "target_lang_sense_clarification": "об эмоциях (переносно)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "014a4228-eb64-308c-a0a7-2a6ef80142d4",
                    "sense_definition": "Having been destroyed or broken apart by an explosion.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Investigators examined the remains of the <w>exploded</w> vehicle.",
                            "target_lang_translations": {
                                "ru": "Следователи осмотрели останки <w>взорвавшегося</w> автомобиля."
                            }
                        },
                        {
                            "text": "Debris from the <w>exploded</w> rocket was scattered over a wide area.",
                            "target_lang_translations": {
                                "ru": "Обломки <w>взорвавшейся</w> ракеты были разбросаны по большой территории."
                            }
                        }
                    ],
                    "synonyms": [
                        "blown up",
                        "shattered",
                        "destroyed"
                    ],
                    "antonyms": [
                        "intact",
                        "whole"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрушенный или разорванный на части взрывом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взорвавшийся",
                                "target_lang_sense_clarification": "разлетевшийся на куски (активное действие)"
                            },
                            {
                                "target_lang_word": "взорванный",
                                "target_lang_sense_clarification": "уничтоженный взрывом (результат)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ed76c70-3e89-3170-a668-5363de17fe35",
                    "sense_definition": "Shown with parts separated but in their correct relative positions to explain how they fit together.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_display",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The assembly manual contains an <w>exploded</w> diagram of the engine.",
                            "target_lang_translations": {
                                "ru": "Руководство по сборке содержит <w>покомпонентную</w> схему двигателя."
                            }
                        },
                        {
                            "text": "You should check the <w>exploded</w> view to see where this screw belongs.",
                            "target_lang_translations": {
                                "ru": "Тебе следует свериться с <w>разнесённым</w> видом, чтобы понять, откуда этот винт."
                            }
                        }
                    ],
                    "synonyms": [
                        "expanded"
                    ],
                    "antonyms": [
                        "assembled"
                    ],
                    "common_phrases": [
                        "exploded view",
                        "exploded diagram"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изображенный с разделенными частями для демонстрации устройства и порядка сборки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покомпонентный",
                                "target_lang_sense_clarification": "о схеме или диаграмме"
                            },
                            {
                                "target_lang_word": "разнесённый",
                                "target_lang_sense_clarification": "технический термин (например, разнесённый вид)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
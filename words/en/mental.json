        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bcffcac6-90bd-3102-b2b3-311b66f2fa7a",
                    "sense_definition": "Of or relating to the mind, intellect, or the process of thinking.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mind_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Solving complex puzzles requires a great deal of <w>mental</w> effort.",
                            "target_lang_translations": {
                                "ru": "Решение сложных головоломок требует больших <w>у́мственных</w> усилий."
                            }
                        },
                        {
                            "text": "She made a <w>mental</w> note to buy milk on her way home.",
                            "target_lang_translations": {
                                "ru": "Она сделала себе <w>мы́сленную</w> пометку купить молока по дороге домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "psychological",
                        "inner"
                    ],
                    "antonyms": [
                        "physical"
                    ],
                    "common_phrases": [
                        "mental health",
                        "mental state",
                        "mental image",
                        "mental age"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к разуму, интеллекту или процессу мышления."

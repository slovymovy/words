{
    "word_family": [
        "forevermore"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ae7159a4-56ca-3777-8118-43fdbd72e171",
                    "sense_definition": "For all future time; for eternity.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I promise that I will love you <w>forever</w>.",
                            "target_lang_translations": {
                                "ru": "Я обещаю, что буду любить тебя <w>вечно</w>."
                            }
                        },
                        {
                            "text": "Nothing in this world lasts <w>forever</w>.",
                            "target_lang_translations": {
                                "ru": "Ничто в этом мире не длится <w>вечно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "always",
                        "eternally",
                        "permanently",
                        "for good",
                        "evermore",
                        "for ever and ever",
                        "perpetually"
                    ],
                    "antonyms": [
                        "never"
                    ],
                    "common_phrases": [
                        "forever and ever",
                        "gone forever",
                        "last forever",
                        "live forever"
                    ],
                    "target_lang_definitions": {
                        "ru": "На все будущие времена; навечно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навсегда",
                                "target_lang_sense_clarification": "окончательно, безвозвратно"
                            },
                            {
                                "target_lang_word": "вечно",
                                "target_lang_sense_clarification": "бесконечно во времени"
                            },
                            {
                                "target_lang_word": "навеки",
                                "target_lang_sense_clarification": "поэтическое или усиленное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4febebd4-6573-3f92-8155-0c5e2993b0de",
                    "sense_definition": "For a very long time; a seeming eternity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It took <w>forever</w> to find a parking space downtown.",
                            "target_lang_translations": {
                                "ru": "Поиск парковочного места в центре занял <w>целую вечность</w>."
                            }
                        },
                        {
                            "text": "We had to wait <w>forever</w> for the bus to arrive.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось ждать автобуса <w>целую вечность</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "endlessly"
                    ],
                    "common_phrases": [
                        "take forever",
                        "wait forever"
                    ],
                    "target_lang_definitions": {
                        "ru": "В течение очень долгого времени; целая вечность (преувеличение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целую вечность",
                                "target_lang_sense_clarification": "разговорное преувеличение"
                            },
                            {
                                "target_lang_word": "очень долго",
                                "target_lang_sense_clarification": "нейтральное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "223093f2-fccc-3089-8461-f206885c5566",
                    "sense_definition": "Constantly or frequently, often describing an annoying habit.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "frequency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is <w>forever</w> losing his keys around the house.",
                            "target_lang_translations": {
                                "ru": "Он <w>вечно</w> теряет свои ключи где-то в доме."
                            }
                        },
                        {
                            "text": "They are <w>forever</w> arguing about money and bills.",
                            "target_lang_translations": {
                                "ru": "Они <w>постоянно</w> ссорятся из-за денег и счетов."
                            }
                        }
                    ],
                    "synonyms": [
                        "always",
                        "all the time",
                        "continually",
                        "incessantly",
                        "endlessly"
                    ],
                    "antonyms": [
                        "never"
                    ],
                    "common_phrases": [
                        "forever changing",
                        "forever complaining"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянно или очень часто (обычно о раздражающей привычке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вечно",
                                "target_lang_sense_clarification": "разговорное, с оттенком недовольства"
                            },
                            {
                                "target_lang_word": "постоянно",
                                "target_lang_sense_clarification": "регулярно, непрерывно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e99d4a73-5843-3548-9f59-c6e2ea65102c",
                    "sense_definition": "Permanent, lasting, or constant.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "permanence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The animal shelter helped the dog find a <w>forever</w> home.",
                            "target_lang_translations": {
                                "ru": "Приют помог собаке найти <w>постоянный</w> дом."
                            }
                        },
                        {
                            "text": "She is looking for a <w>forever</w> friend to share her life with.",
                            "target_lang_translations": {
                                "ru": "Она ищет друга <w>на всю жизнь</w>, чтобы разделить с ним судьбу."
                            }
                        }
                    ],
                    "synonyms": [
                        "permanently",
                        "lastingly"
                    ],
                    "common_phrases": [
                        "forever home",
                        "forever friend",
                        "forever war"
                    ],
                    "target_lang_definitions": {

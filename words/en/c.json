        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "50b034c9-82c9-3786-a097-e3e7e22ed676",
                    "sense_definition": "An academic grade given to a student, usually denoting average performance, better than a D and worse than a B.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "grades",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She studied hard but still only managed to get a <w>C</w> on the difficult math exam.",
                            "target_lang_translations": {
                                "ru": "Она усердно занималась, но всё равно смогла получить только <w>тройку</w> на сложном экзамене по математике."
                            }
                        },
                        {
                            "text": "His parents were not happy because his report card was full of <w>C</w> grades.",
                            "target_lang_translations": {
                                "ru": "Его родители были недовольны, потому что в его табеле были одни <w>тройки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get a C",
                        "straight Cs",
                        "C student"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "education"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Академическая оценка, означающая среднюю успеваемость (обычно соответствует «тройке» или «удовлетворительно»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тройка",
                                "target_lang_sense_clarification": "школьная оценка"
                            },
                            {
                                "target_lang_word": "удовлетворительно",
                                "target_lang_sense_clarification": "официальное название оценки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6932e97f-b0f8-33e1-9cfd-74277e7838de",
                    "sense_definition": "The first note in the C major scale, or the third note in the A minor scale.",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "26242022-55ac-3264-9884-177f59b8b939",
                    "sense_definition": "Eigendommen, geld of goederen ontvangen van iemand die overleden is, meestal een familielid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "inherit_property",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <w>erfde</w> een prachtig schilderij van haar oma.",
                            "target_lang_translations": {
                                "en": "She <w>inherited</w> a beautiful painting from her grandmother.",
                                "ru": "Она <w>унаследовала</w> прекрасную картину от своей бабушки."
                            }
                        },
                        {
                            "text": "Na het overlijden van zijn ouders zal hij het familiebedrijf <w>erven</w>.",
                            "target_lang_translations": {
                                "en": "After the death of his parents, he will <w>inherit</w> the family business.",
                                "ru": "После смерти родителей он <w>унаследует</w> семейный бизнес."
                            }
                        }
                    ],
                    "synonyms": [
                        "verkrijgen"
                    ],
                    "common_phrases": [
                        "geld erven",
                        "een huis erven",
                        "alles erven",
                        "schulden erven"
                    ],
                    "target_lang_definitions": {
                        "en": "To receive money, property, or possessions from someone who has died.",
                        "ru": "Получать имущество, деньги или вещи от умершего человека, обычно родственника."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inherit",
                                "target_lang_sense_clarification": "receive property or money"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "наследовать",
                                "target_lang_sense_clarification": "имущество"
                            },
                            {
                                "target_lang_word": "получить в наследство",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f697e67-54f5-3228-a6f1-fa5a834478c1",
                    "sense_definition": "Eigenschappen, kenmerken of talenten via de genen of opvoeding meekrijgen van ouders of voorouders.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "inherit_traits",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gelukkig heb ik niet de slechte gewoonten van mijn grootmoeder <w>geërfd</w>.",
                            "target_lang_translations": {
                                "en": "Fortunately, I have not <w>inherited</w> my grandmother's bad habits.",
                                "ru": "К счастью, я не <w>унаследовал</w> плохие привычки моей бабушки."
                            }
                        },
                        {
                            "text": "Zij heeft haar muzikale talent duidelijk van haar vader <w>geërfd</w>.",
                            "target_lang_translations": {
                                "en": "She clearly <w>inherited</w> her musical talent from her father.",
                                "ru": "Она явно <w>унаследовала</w> свой музыкальный талант от отца."
                            }
                        }
                    ],
                    "synonyms": [
                        "meekrijgen"
                    ],
                    "common_phrases": [
                        "talent erven",
                        "eigenschappen erven",
                        "genen erven"
                    ],
                    "target_lang_definitions": {
                        "en": "To derive physical or mental characteristics from one's parents or ancestors.",
                        "ru": "Получать свойства, характеристики или таланты через гены или воспитание от родителей или предков."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inherit",
                                "target_lang_sense_clarification": "receive traits or genes"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "унаследовать",
                                "target_lang_sense_clarification": "черты, таланты"
                            },
                            {
                                "target_lang_word": "перенять",
                                "target_lang_sense_clarification": "привычки, особенности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f6659408-d881-34ed-a198-9dd370ec81ae",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>erf</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boeren maakten hun <w>erven</w> schoon voor het grote oogstfeest.",
                            "target_lang_translations": {
                                "en": "The farmers cleaned their <w>yards</w> for the big harvest festival.",
                                "ru": "Фермеры убирали свои <w>дворы</w> перед большим праздником урожая."
                            }
                        },
                        {
                            "text": "Op de meeste <w>erven</w> in deze straat staan grote eikenbomen.",
                            "target_lang_translations": {
                                "en": "On most <w>yards</w> in this street, there are large oak trees.",
                                "ru": "В большинстве <w>дворов</w> на этой улице растут большие дубы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op de erven",
                        "boeren erven"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The grounds or areas of land surrounding houses or farms (plural form).",
                        "ru": "Множественное число существительного 'erf' (двор, участок земли при доме)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "yards",
                                "target_lang_sense_clarification": "grounds surrounding a building"
                            },
                            {
                                "target_lang_word": "premises",
                                "target_lang_sense_clarification": "property grounds"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "дворы",
                                "target_lang_sense_clarification": "территория у дома"
                            },
                            {
                                "target_lang_word": "участки",
                                "target_lang_sense_clarification": "земельные наделы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1c4e71e-b911-3052-ba88-640b26558168",
                    "sense_definition": "De gezamenlijke erfgenamen van een overledene.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De brief was gericht aan de weduwe en <w>erven</w> van de overledene.",
                            "target_lang_translations": {
                                "en": "The letter was addressed to the widow and <w>heirs</w> of the deceased.",
                                "ru": "Письмо было адресовано вдове и <w>наследникам</w> покойного."
                            }
                        },
                        {
                            "text": "De <w>erven</w> besloten het landgoed gezamenlijk te verkopen.",
                            "target_lang_translations": {
                                "en": "The <w>heirs</w> decided to sell the estate jointly.",
                                "ru": "<w>Наследники</w> решили совместно продать поместье."
                            }
                        }
                    ],
                    "synonyms": [
                        "erfgenamen"
                    ],
                    "common_phrases": [
                        "de erven van",
                        "weduwe en erven"
                    ],
                    "traits": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a6e38846-e4bb-3e8a-8447-9c3fb64cec24",
                    "sense_definition": "Osoba, która brudzi, nie dba o higienę lub nie zachowuje porządku.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "dirty_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Idź się umyć, ty mały <w>świntuchu</w>, bo jesteś cały w błocie.",
                            "target_lang_translations": {
                                "ru": "Иди помойся, ты маленький <w>свинтус</w>, а то ты весь в грязи."
                            }
                        },
                        {
                            "text": "Jaki z ciebie <w>świntuch</w>, znowu poplamiłeś nową koszulkę sosem.",
                            "target_lang_translations": {
                                "ru": "Какой же ты <w>грязнуля</w>, опять испачкал новую футболку соусом."
                            }
                        }
                    ],
                    "synonyms": [
                        "brudas",
                        "fleja",
                        "niechluj",
                        "syfiarz",
                        "prosię"
                    ],
                    "antonyms": [
                        "czyścioch"
                    ],
                    "common_phrases": [
                        "mały świntuch"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech, often towards children or messy people"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который пачкается, не следит за гигиеной или не соблюдает порядок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грязнуля",
                                "target_lang_sense_clarification": "о человеке, который не следит за чистотой"
                            },
                            {
                                "target_lang_word": "свинтус",
                                "target_lang_sense_clarification": "разговорное; о том, кто мусорит или пачкается"
                            },
                            {
                                "target_lang_word": "неряха",
                                "target_lang_sense_clarification": "о неопрятном человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcfeee0c-304d-3264-96ca-7857f563cd14",
                    "sense_definition": "Mężczyzna zachowujący się nieprzyzwoicie, często w kontekście seksualnym, lub opowiadający sprośne żarty.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "indecent_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź takim <w>świntuchem</w> i przestań opowiadać te sprośne kawały przy stole.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>пошляком</w> и перестань рассказывать эти сальные анекдоты за столом."
                            }
                        },
                        {
                            "text": "Wszyscy wiedzieli, że to stary <w>świntuch</w>, który lubi zaczepiać młode kobiety.",
                            "target_lang_translations": {
                                "ru": "Все знали, что это старый <w>развратник</w>, который любит приставать к молодым женщинам."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbereźnik",
                        "bezwstydnik",
                        "rozpustnik"
                    ],
                    "common_phrases": [
                        "stary świntuch",
                        "ty świntuchu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often pejorative or joking"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, ведущий себя непристойно, часто в сексуальном контексте, или рассказывающий пошлые шутки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пошляк",
                                "target_lang_sense_clarification": "человек, говорящий или делающий пошлости"
                            },
                            {
                                "target_lang_word": "бесстыдник",
                                "target_lang_sense_clarification": "человек без стыда, ведущий себя непристойно"
                            },
                            {
                                "target_lang_word": "развратник",
                                "target_lang_sense_clarification": "человек распутного поведения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "przygotowanie",
        "przygotowany",
        "gotować",
        "gotowy",
        "gotowość",
        "przygotowawczy",
        "przygotować"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "77c30783-893b-3fee-a5b1-47b7d76512be",
                    "sense_definition": "Robić coś, przyrządzać lub organizować w taki sposób, aby było to gotowe do użycia lub działania w określonym celu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "general_preparation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja mama codziennie rano wstaje wcześniej, aby <przygotowywać> śniadanie dla całej rodziny.",
                            "target_lang_translations": {
                                "ru": "Моя мама каждое утро встает пораньше, чтобы <готовить> завтрак для всей семьи."
                            }
                        },
                        {
                            "text": "Musimy dokładnie <przygotowywać> wszystkie dokumenty przed spotkaniem z ważnym klientem.",
                            "target_lang_translations": {
                                "ru": "Мы должны тщательно <подготавливать> все документы перед встречей с важным клиентом."
                            }
                        }
                    ],
                    "synonyms": [
                        "szykować",
                        "przyrządzać"
                    ],
                    "common_phrases": [
                        "przygotowywać posiłek",
                        "przygotowywać obiad",
                        "przygotowywać plan",
                        "przygotowywać miejsce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо, стряпать или организовывать таким образом, чтобы это было готово к использованию или действию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовить",
                                "target_lang_sense_clarification": "общий смысл, приготовление еды"
                            },
                            {
                                "target_lang_word": "подготавливать",
                                "target_lang_sense_clarification": "документы, предварительная работа"
                            },
                            {
                                "target_lang_word": "приготавливать",
                                "target_lang_sense_clarification": "приводить в готовность, создавать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cab992ea-ceeb-3406-b3ff-fe30e53bb1b6",
                    "sense_definition": "Podejmować działania i starania, aby stać się gotowym do jakiegoś wydarzenia lub zadania (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "self_preparation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczniowie zaczęli systematycznie <przygotowywać> się do końcowych egzaminów już w marcu.",
                            "target_lang_translations": {
                                "ru": "Ученики начали систематически <готовиться> к выпускным экзаменам уже в марте."
                            }
                        },
                        {
                            "text": "Zawsze muszę długo <przygotowywać> się do wyjścia na eleganckie przyjęcie.",
                            "target_lang_translations": {
                                "ru": "Мне всегда приходится долго <готовиться> к выходу на элегантный прием."
                            }
                        }
                    ],
                    "synonyms": [
                        "szykować"
                    ],
                    "common_phrases": [
                        "przygotowywać się do egzaminu",
                        "przygotowywać się do drogi",
                        "przygotowywać się do wyjścia",
                        "przygotowywać się do pracy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предпринимать действия и усилия, чтобы стать готовым к какому-либо событию или задаче (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовиться",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "подготавливаться",
                                "target_lang_sense_clarification": "процесс подготовки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4e55609-cada-33dc-a0ed-24cd933454b3",
                    "sense_definition": "Uprzedzać kogoś o nadchodzących wydarzeniach, często trudnych, aby pomóc mu się z nimi psychicznie oswoić.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_preparation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz starał się delikatnie <przygotowywać> pacjenta na konieczność przeprowadzenia trudnej operacji.",
                            "target_lang_translations": {
                                "ru": "Врач старался мягко <подготавливать> пациента к необходимости проведения сложной операции."
                            }
                        },
                        {
                            "text": "Rodzice powinni powoli <przygotowywać> dzieci na nadchodzące zmiany w ich życiu.",
                            "target_lang_translations": {
                                "ru": "Родители должны постепенно <подготавливать> детей к грядущим переменам в их жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "szykować",
                        "przyrządzać"
                    ],
                    "common_phrases": [
                        "przygotowywać na najgorsze",
                        "przygotowywać na zmiany",
                        "przygotowywać na złą wiadomość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предупреждать кого-либо о предстоящих событиях, часто трудных, чтобы помочь морально свыкнуться с ними."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подготавливать",
                                "target_lang_sense_clarification": "морально настраивать"
                            },
                            {
                                "target_lang_word": "настраивать",
                                "target_lang_sense_clarification": "психологическая подготовка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
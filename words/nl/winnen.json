    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bcc85c6b-7fce-33d8-9a80-f45fa69d1279",
                    "sense_definition": "Als beste eindigen in een wedstrijd, spel, strijd of competitie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "victory_success",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Onze voetbalclub wil de volgende wedstrijd absoluut <w>winnen</w>.",
                            "target_lang_translations": {
                                "ru": "Наш футбольный клуб безусловно хочет <w>выиграть</w> следующий матч."
                            }
                        },
                        {
                            "text": "Als je goed oefent, zul je vast een keer van mij <w>winnen</w>.",
                            "target_lang_translations": {
                                "ru": "Если будешь хорошо тренироваться, ты наверняка однажды у меня <w>выиграешь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zegevieren"
                    ],
                    "antonyms": [
                        "verliezen"
                    ],
                    "common_phrases": [
                        "de wedstrijd winnen",
                        "met gemak winnen",
                        "van iemand winnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать победителем в соревновании, игре или борьбе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побеждать",
                                "target_lang_sense_clarification": "одерживать верх над противником"
                            },
                            {
                                "target_lang_word": "выигрывать",
                                "target_lang_sense_clarification": "становиться победителем в игре или споре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6572e0f-d1b7-39c7-bd68-c3175aec132b",
                    "sense_definition": "Een prijs, geldbedrag of titel verkrijgen door een prestatie of door geluk.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "victory_success",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft gisteren een groot bedrag in de loterij <w>gewonnen</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера он <w>выиграл</w> крупную сумму в лотерею."
                            }
                        },
                        {
                            "text": "De atleet traint hard om een gouden medaille te <w>winnen</w>.",
                            "target_lang_translations": {
                                "ru": "Атлет усердно тренируется, чтобы <w>завоевать</w> золотую медаль."
                            }
                        }
                    ],
                    "synonyms": [
                        "behalen",
                        "binnenhalen",
                        "verdienen"
                    ],
                    "antonyms": [
                        "verliezen"
                    ],
                    "common_phrases": [
                        "de loterij winnen",
                        "goud winnen",
                        "een prijs winnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить приз, деньги или титул благодаря удаче или достижениям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выигрывать",
                                "target_lang_sense_clarification": "получать приз или деньги (например, в лотерее)"
                            },
                            {
                                "target_lang_word": "завоевывать",
                                "target_lang_sense_clarification": "получать награду или титул в результате усилий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c63fe5d-f1ac-3976-a02c-06aab0848ac8",
                    "sense_definition": "Iets immaterieels verkrijgen of vergroten, zoals tijd, invloed, ruimte of vertrouwen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "acquire_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten proberen wat tijd te <w>winnen</w> voordat we een beslissing nemen.",
                            "target_lang_translations": {
                                "ru": "Мы должны попытаться <w>выиграть</w> немного времени, прежде чем принимать решение."
                            }
                        },
                        {
                            "text": "Het nieuwe politieke idee begint langzaam terrein te <w>winnen</w> bij de kiezers.",
                            "target_lang_translations": {
                                "ru": "Новая политическая идея начинает медленно <w>завоевывать</w> позиции среди избирателей."
                            }
                        }
                    ],
                    "synonyms": [
                        "verkrijgen",
                        "verwerven"
                    ],
                    "antonyms": [
                        "verliezen"
                    ],
                    "common_phrases": [
                        "tijd winnen",
                        "vertrouwen winnen",
                        "terrein winnen",
                        "advies inwinnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить или увеличить что-то нематериальное, например время, влияние или доверие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выигрывать",
                                "target_lang_sense_clarification": "получать преимущество во времени"
                            },
                            {
                                "target_lang_word": "завоевывать",
                                "target_lang_sense_clarification": "приобретать доверие или авторитет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dec7328d-e507-3610-aa80-2a909fe55606",
                    "sense_definition": "Grondstoffen of natuurlijke hulpbronnen uit de aarde halen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "extraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf wil in dit gebied aardgas gaan <w>winnen</w>.",
                            "target_lang_translations": {
                                "ru": "Компания хочет начать <w>добывать</w> природный газ в этом районе."
                            }
                        },
                        {
                            "text": "In de mijnen werd vroeger veel steenkool <w>gewonnen</w> door harde arbeid.",
                            "target_lang_translations": {
                                "ru": "Раньше в шахтах тяжелым трудом <w>добывалось</w> много угля."
                            }
                        }
                    ],
                    "synonyms": [
                        "delven",
                        "ontginnen"
                    ],
                    "common_phrases": [
                        "olie winnen",
                        "gas winnen",
                        "grondstoffen winnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Извлекать полезные ископаемые или природные ресурсы из земли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добывать",
                                "target_lang_sense_clarification": "извлекать ресурсы из недр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9162f839-3311-3d39-bb26-ebc785358ff1",
                    "sense_definition": "Iemand overtuigen of gunstig stemmen om steun te krijgen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "persuasion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met zijn charmante glimlach wist hij iedereen voor zich te <w>winnen</w>.",
                            "target_lang_translations": {
                                "ru": "Своей очаровательной улыбкой он сумел <w>расположить</w> всех к себе."
                            }
                        },
                        {
                            "text": "Het was moeilijk om de commissie voor dit gewaagde plan te <w>winnen</w>.",
                            "target_lang_translations": {
                                "ru": "Было трудно <w>склонить</w> комиссию на сторону этого смелого плана."
                            }
                        }
                    ],
                    "synonyms": [
                        "overhalen",
                        "overtuigen"
                    ],
                    "common_phrases": [
                        "voor zich winnen",
                        "iemand winnen voor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убедить кого-то или расположить к себе для получения поддержки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "располагать к себе",
                                "target_lang_sense_clarification": "вызывать симпатию"
                            },
                            {
                                "target_lang_word": "склонять",
                                "target_lang_sense_clarification": "убеждать принять чью-то сторону"
                            }
                        ]
                    }
                }
            ]
        }
    ]

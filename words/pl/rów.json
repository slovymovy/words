            "senses": [
                {
                    "sense_id": "3d8c7d07-ae79-3115-9e5f-2ab6269b057e",
                    "sense_definition": "Podłużny dół wykopany w ziemi, zazwyczaj służący do odprowadzania wody lub jako przeszkoda.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pijany kierowca wjechał do przydrożnego <w>rowu</w>.",
                            "target_lang_translations": {
                                "ru": "Пьяный водитель съехал в придорожный <w>кювет</w>."
                            }
                        },
                        {
                            "text": "Woda z pól spływa do głębokiego <w>rowu</w> melioracyjnego.",
                            "target_lang_translations": {
                                "ru": "Вода с полей стекает в глубокую мелиоративную <w>канаву</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rów melioracyjny",
                        "rów przydrożny",
                        "kopać rów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолговатая яма, вырытая в земле, обычно служащая для отвода воды или в качестве препятствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канава",
                                "target_lang_sense_clarification": "для отвода воды"
                            },
                            {
                                "target_lang_word": "ров",
                                "target_lang_sense_clarification": "оборонительный или глубокий"
                            },
                            {
                                "target_lang_word": "кювет",
                                "target_lang_sense_clarification": "придорожный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bde2430e-09ed-3a0b-b94a-79a173d2d0aa",
                    "sense_definition": "Rozległe, podłużne obniżenie terenu lub dna oceanu powstałe w wyniku ruchów tektonicznych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rów Mariański to najgłębszy znany <w>rów</w> oceaniczny na Ziemi.",
                            "target_lang_translations": {
                                "ru": "Марианский <w>жёлоб</w> — это самый глубокий известный океанический <w>жёлоб</w> на Земле."
                            }
                        },
                        {
                            "text": "Wzdłuż granicy płyt tektonicznych powstał wielki <w>rów</w>.",
                            "target_lang_translations": {
                                "ru": "Вдоль границы тектонических плит образовался огромный <w>разлом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rów tektoniczny",
                        "rów oceaniczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обширное продольное понижение местности или дна океана, возникшее в результате тектонических движений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жёлоб",
                                "target_lang_sense_clarification": "океанический"
                            },
                            {
                                "target_lang_word": "грабен",
                                "target_lang_sense_clarification": "геологический термин"
                            },
                            {
                                "target_lang_word": "разлом",
                                "target_lang_sense_clarification": "тектонический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0329687-4e54-336d-9b89-6b10efaf30f5",
                    "sense_definition": "Wgłębienie między pośladkami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pochylił się i było mu widać <w>rów</w>.",
                            "target_lang_translations": {
                                "ru": "Он наклонился, и у него была видна <w>копилка</w>."
                            }
                        },
                        {
                            "text": "Podciągnij spodnie, bo widać ci <w>rów</w>.",
                            "target_lang_translations": {
                                "ru": "Подтяни штаны, а то <w>копилку</w> видно."

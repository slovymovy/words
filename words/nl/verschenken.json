                {
                    "sense_id": "e23445fc-fb53-31e4-8433-58de803bf73b",
                    "sense_definition": "Iets gratis aan iemand anders geven zonder er iets voor terug te vragen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij besloot zijn oude boekenverzameling aan de bibliotheek te <w>verschenken</w>.",
                            "target_lang_translations": {
                                "en": "He decided to <w>donate</w> his old book collection to the library.",
                                "ru": "Он решил <w>подарить</w> свою коллекцию старых книг библиотеке."
                            }
                        },
                        {
                            "text": "Ze hebben al hun overbodige meubels <w>verschonken</w> aan een goed doel.",
                            "target_lang_translations": {
                                "en": "They <w>gave away</w> all their surplus furniture to a charity.",
                                "ru": "Они <w>отдали</w> всю свою лишнюю мебель на благотворительность."
                            }
                        }
                    ],
                    "synonyms": [
                        "weggeven",
                        "cadeau doen",
                        "schenken"
                    ],
                    "antonyms": [
                        "houden",
                        "behouden",
                        "krijgen"
                    ],
                    "common_phrases": [
                        "iets te verschenken hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "To give something to someone else for free without asking for anything in return.",
                        "ru": "Дарить или отдавать что-либо бесплатно, не требуя ничего взамен."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give away",
                                "target_lang_sense_clarification": "to transfer possession freely"
                            },
                            {
                                "target_lang_word": "donate",
                                "target_lang_sense_clarification": "to give for a good cause or institution"
                            },
                            {
                                "target_lang_word": "gift",
                                "target_lang_sense_clarification": "to present as a gift"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "раздаривать",

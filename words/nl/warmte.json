        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "34705a0c-945f-3833-88b9-80555984cf37",
                    "sense_definition": "De toestand van warm weer of een hoge temperatuur.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "literal_heat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>warmte</w> was de laatste tijd moeilijk te verdragen.",
                            "target_lang_translations": {
                                "en": "The <w>heat</w> has been hard to bear lately.",
                                "ru": "В последнее время <w>жару</w> было трудно переносить."
                            }
                        },
                        {
                            "text": "We bleven binnen om aan de <w>warmte</w> van de middagzon te ontsnappen.",
                            "target_lang_translations": {
                                "en": "We stayed inside to escape the <w>heat</w> of the afternoon sun.",
                                "ru": "Мы остались внутри, чтобы спастись от <w>жары</w> полуденного солнца."
                            }
                        }
                    ],
                    "antonyms": [
                        "kou",
                        "koude"
                    ],
                    "common_phrases": [
                        "ondraaglijke warmte",
                        "last van de warmte"
                    ],
                    "target_lang_definitions": {
                        "en": "The condition of hot weather or high temperature.",
                        "ru": "Состояние теплой погоды или высокой температуры."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heat",
                                "target_lang_sense_clarification": "high temperature or hot weather"
                            },
                            {
                                "target_lang_word": "warmth",
                                "target_lang_sense_clarification": "the state of being warm"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "жара",
                                "target_lang_sense_clarification": "высокая температура воздуха"
                            },
                            {
                                "target_lang_word": "зной",
                                "target_lang_sense_clarification": "сильная жара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "838c9ba8-adf2-31c1-a3e9-834648774a2f",
                    "sense_definition": "Een aangenaam gevoel van hitte of energie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "literal_heat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een aangename <w>warmte</w> verspreidde zich door mijn lichaam na het drinken van de thee.",
                            "target_lang_translations": {
                                "en": "A pleasant <w>warmth</w> spread through my body after drinking the tea.",
                                "ru": "Приятное <w>тепло</w> разлилось по моему телу после того, как я выпил чаю."
                            }
                        },
                        {
                            "text": "Hij wreef in zijn handen om wat <w>warmte</w> te voelen in de koude lucht.",
                            "target_lang_translations": {
                                "en": "He rubbed his hands to feel some <w>warmth</w> in the cold air.",
                                "ru": "Он потер руки, чтобы почувствовать немного <w>тепла</w> на холодном воздухе."
                            }
                        }
                    ],
                    "antonyms": [
                        "kou"
                    ],
                    "common_phrases": [
                        "aangename warmte",
                        "warmte zoeken"
                    ],
                    "target_lang_definitions": {
                        "en": "A pleasant feeling of heat or energy.",
                        "ru": "Приятное ощущение тепла или энергии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "warmth",
                                "target_lang_sense_clarification": "pleasant sensation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тепло",
                                "target_lang_sense_clarification": "приятное ощущение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "896796ab-2b39-3d93-a450-cbac3ba2c1ac",
                    "sense_definition": "Een gevoel van hartelijkheid, genegenheid en vriendelijkheid in het contact met anderen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze werd met veel <w>warmte</w> ontvangen door haar nieuwe collega's.",
                            "target_lang_translations": {
                                "en": "She was received with much <w>warmth</w> by her new colleagues.",
                                "ru": "Новые коллеги приняли её с большой <w>теплотой</w>."
                            }
                        },
                        {
                            "text": "Er sprak een grote <w>warmte</w> uit zijn stem toen hij over zijn familie vertelde.",
                            "target_lang_translations": {
                                "en": "There was great <w>warmth</w> in his voice when he spoke about his family.",
                                "ru": "В его голосе звучала большая <w>теплота</w>, когда он рассказывал о своей семье."
                            }
                        }
                    ],
                    "synonyms": [
                        "hartelijkheid"
                    ],
                    "antonyms": [
                        "kou"
                    ],
                    "common_phrases": [
                        "warmte en genegenheid",
                        "menselijke warmte",
                        "warmte uitstralen"
                    ],
                    "target_lang_definitions": {
                        "en": "A feeling of friendliness, affection, and kindness in contact with others.",
                        "ru": "Чувство сердечности, привязанности и дружелюбия в общении с другими."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "warmth",
                                "target_lang_sense_clarification": "friendliness or affection"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "теплота",
                                "target_lang_sense_clarification": "душевное отношение"
                            },
                            {
                                "target_lang_word": "сердечность",
                                "target_lang_sense_clarification": "искренность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "697d4b3a-b213-3fd6-a543-1e74de0bcc43",
                    "sense_definition": "Een vorm van energie die ontstaat door de beweging van atomen en moleculen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_heat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aarde gaf na de zonnige dag nog steeds <w>warmte</w> af.",
                            "target_lang_translations": {
                                "en": "The earth was still giving off <w>heat</w> after the sunny day.",
                                "ru": "После солнечного дня земля всё ещё отдавала <w>тепло</w>."
                            }
                        },
                        {
                            "text": "Goede isolatie zorgt ervoor dat de <w>warmte</w> in het huis wordt vastgehouden.",
                            "target_lang_translations": {
                                "en": "Good insulation ensures that the <w>heat</w> is retained in the house.",
                                "ru": "Хорошая изоляция гарантирует, что <w>тепло</w> удерживается в доме."
                            }
                        }
                    ],

            "senses": [
                {
                    "sense_id": "4f94be84-1c96-31ab-a2a8-f073855be858",
                    "sense_definition": "Что-либо неблагоприятное, негативное, приносящее вред или огорчение.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не вижу в этом предложении ничего <w>плохого</w>.",
                            "target_lang_translations": {
                                "en": "I don't see anything <w>bad</w> in this proposal."
                            }
                        },
                        {
                            "text": "В жизни было много <w>плохого</w>, но мы старались сохранять оптимизм.",
                            "target_lang_translations": {
                                "en": "There were many <w>bad things</w> in life, but we tried to remain optimistic."
                            }
                        }
                    ],
                    "synonyms": [

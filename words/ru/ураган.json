    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0e1643e0-b3f7-38be-8ae7-01bd93f9aa3c",
                    "sense_definition": "Ветер разрушительной силы и значительной продолжительности, скорость которого превышает 32 м/с.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Метеорологи предупредили, что к побережью приближается мощный <w>ураган</w>.",
                            "target_lang_translations": {
                                "en": "Meteorologists warned that a powerful <w>hurricane</w> is approaching the coast."
                            }
                        },
                        {
                            "text": "Вчерашний <w>ураган</w> повалил много деревьев в городском парке.",
                            "target_lang_translations": {
                                "en": "Yesterday's <w>hurricane</w> knocked down many trees in the city park."
                            }
                        }
                    ],
                    "synonyms": [
                        "тропический циклон",
                        "шторм",
                        "торнадо"
                    ],
                    "common_phrases": [
                        "сильный ураган",
                        "последствия урагана",
                        "эпицентр урагана",
                        "разрушительный ураган"
                    ],
                    "target_lang_definitions": {
                        "en": "A wind of destructive force and significant duration, with a speed exceeding 32 m/s."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hurricane",
                                "target_lang_sense_clarification": "severe tropical cyclone"
                            },
                            {
                                "target_lang_word": "gale",
                                "target_lang_sense_clarification": "very strong wind"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38b880e1-a485-352c-bf9f-902939ab3719",
                    "sense_definition": "Чрезвычайно стремительное, бурное развитие чего-либо или проявление чего-либо с огромной силой (о чувствах, событиях).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После выступления артиста в зале поднялся настоящий <w>ураган</w> аплодисментов.",
                            "target_lang_translations": {
                                "en": "After the artist's performance, a real <w>storm</w> of applause rose in the hall."
                            }
                        },
                        {
                            "text": "В её душе бушевал <w>ураган</w> эмоций, с которым было трудно справиться.",
                            "target_lang_translations": {
                                "en": "A <w>hurricane</w> of emotions raged in her soul that was difficult to cope with."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ураган страстей",
                        "ураган огня",
                        "ураган событий"
                    ],
                    "target_lang_definitions": {
                        "en": "An extremely rapid, turbulent development or manifestation of something with immense force (regarding feelings or events)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hurricane",
                                "target_lang_sense_clarification": "figurative: powerful outburst"
                            },
                            {
                                "target_lang_word": "storm",
                                "target_lang_sense_clarification": "violent outburst of emotion or reaction"
                            },
                            {
                                "target_lang_word": "whirlwind",
                                "target_lang_sense_clarification": "tumultuous rush or chaotic sequence"
                            }
                        ]
                    }
                }
            ]
        }
    ]

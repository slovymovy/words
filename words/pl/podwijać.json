            "senses": [
                {
                    "sense_id": "f82121cf-064a-3253-b2b5-04a5c65d076a",
                    "sense_definition": "Zaginać brzeg materiału lub ubrania do góry lub pod spód, zazwyczaj aby je skrócić.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <w>podwijać</w> nogawki spodni, żeby nie zamoczyć ich w kałuży.",
                            "target_lang_translations": {
                                "ru": "Мне приходится <w>подворачивать</w> штанины брюк, чтобы не намочить их в луже."
                            }
                        },
                        {
                            "text": "Kiedy robi się gorąco, zawsze <w>podwijam</w> rękawy mojej koszuli.",
                            "target_lang_translations": {
                                "ru": "Когда становится жарко, я всегда <w>засучиваю</w> рукава моей рубашки."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakasywać",
                        "podkasywać"
                    ],
                    "antonyms": [
                        "rozwijać",
                        "opuszczać"
                    ],
                    "common_phrases": [
                        "podwijać rękawy",
                        "podwijać nogawki",
                        "podwijać spodnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Загибать край ткани или одежды вверх или внутрь, обычно чтобы укоротить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подворачивать",
                                "target_lang_sense_clarification": "одежду, края"
                            },
                            {
                                "target_lang_word": "засучивать",
                                "target_lang_sense_clarification": "рукава"
                            },
                            {
                                "target_lang_word": "подгибать",
                                "target_lang_sense_clarification": "укорачивать, подшивать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eeedb923-07d4-333a-9ba5-542118d5505a",
                    "sense_definition": "Zginać kończyny lub ogon i chować je pod siebie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestraszony pies zaczął <w>podwijać</w> ogon i uciekł do budy.",
                            "target_lang_translations": {
                                "ru": "Испуганный пес начал <w>поджимать</w> хвост и убежал в будку."
                            }
                        },
                        {
                            "text": "Lubię siedzieć w fotelu i <w>podwijać</w> nogi pod kocem.",
                            "target_lang_translations": {
                                "ru": "Я люблю сидеть в кресле и <w>поджимать</w> ноги под пледом."
                            }
                        }
                    ],
                    "synonyms": [
                        "podkulać",
                        "podkurczać"
                    ],
                    "common_phrases": [
                        "podwijać ogon",
                        "podwijać nogi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сгибать конечности или хвост и прятать их под себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поджимать",

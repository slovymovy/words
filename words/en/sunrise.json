                    "sense_definition": "To introduce or phase in a new technology or system.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company plans to <w>sunrise</w> the new software next month.",
                            "target_lang_translations": {
                                "ru": "Компания планирует <w>внедрить</w> новое программное обеспечение в следующем месяце."
                            }
                        },
                        {
                            "text": "They will <w>sunrise</w> the updated platform while maintaining the old one briefly.",
                            "target_lang_translations": {
                                "ru": "Они <w>запустят</w> обновленную платформу, ненадолго сохранив старую."
                            }
                        }
                    ],
                    "antonyms": [
                        "sunset"
                    ],
                    "common_phrases": [
                        "sunrise a project",
                        "sunrise new technology"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внедрять или поэтапно вводить новую технологию или систему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внедрять",
                                "target_lang_sense_clarification": "вводить в действие"
                            },
                            {
                                "target_lang_word": "запускать",
                                "target_lang_sense_clarification": "начинать использование"
                            }
                        ]
                    }
                }
            ]
        }

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9922967d-36f9-35a8-a0fd-f05fe3d36c47",
                    "sense_definition": "Proces powolnych, stopniowych zmian zachodzących w czymś, prowadzący do przekształcenia się w formę bardziej złożoną lub inną jakościowo.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "process_of_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego poglądy polityczne przeszły długą <w>ewolucję</w> na przestrzeni ostatnich lat.",
                            "target_lang_translations": {
                                "ru": "Его политические взгляды претерпели долгую <w>эволюцию</w> за последние годы."
                            }
                        },
                        {
                            "text": "Obserwujemy powolną <w>ewolucję</w> systemu edukacji, który dostosowuje się do nowych technologii.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдаем медленную <w>эволюцию</w> системы образования, которая приспосабливается к новым технологиям."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozwój"
                    ],
                    "antonyms": [
                        "rewolucja",
                        "regres"
                    ],
                    "common_phrases": [
                        "ewolucja poglądów",
                        "powolna ewolucja",
                        "drogą ewolucji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс медленных, постепенных изменений, ведущий к переходу в более сложную или качественно иную форму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эволюция",
                                "target_lang_sense_clarification": "процесс развития"
                            },
                            {
                                "target_lang_word": "развитие",
                                "target_lang_sense_clarification": "постепенное изменение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fd0cb9c-bba9-37b8-a50d-071081b9f4e1",
                    "sense_definition": "Proces biologiczny polegający na stopniowych zmianach cech dziedzicznych organizmów w kolejnych pokoleniach, prowadzący do powstawania nowych gatunków.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "process_of_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Karol Darwin sformułował teorię, która wyjaśnia <w>ewolucję</w> wszystkich żywych organizmów.",
                            "target_lang_translations": {
                                "ru": "Чарльз Дарвин сформулировал теорию, которая объясняет <w>эволюцию</w> всех живых организмов."
                            }
                        },
                        {
                            "text": "<w>Ewolucja</w> człowieka jest fascynującym tematem badań dla wielu naukowców.",
                            "target_lang_translations": {
                                "ru": "<w>Эволюция</w> человека является увлекательной темой исследований для многих ученых."
                            }
                        }
                    ],
                    "synonyms": [
                        "ewolucja naturalna"
                    ],
                    "antonyms": [
                        "inwolucja"
                    ],
                    "common_phrases": [
                        "teoria ewolucji",
                        "ewolucja gatunków",
                        "ogniwo ewolucji"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "biologia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Биологический процесс, заключающийся в постепенном изменении наследственных признаков организмов в последующих поколениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эволюция",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "982a2ba3-0d30-30ff-ad95-53221e08ac75",
                    "sense_definition": "Skomplikowana figura gimnastyczna, akrobatyczna, taneczna lub lotnicza, wymagająca dużej sprawności.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement_maneuver",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "12f71585-6ce1-3990-82c7-5e12b678af9a",
                    "sense_definition": "A piece of meat from the front of a bird's body, especially chicken or turkey.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I prefer grilled chicken <w>breast</w> with a fresh salad.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю куриную <w>грудку</w> на гриле со свежим салатом."
                            }
                        },
                        {
                            "text": "You should slice the turkey <w>breast</w> thinly for sandwiches.",
                            "target_lang_translations": {
                                "ru": "Тебе следует тонко нарезать <w>грудку</w> индейки для сэндвичей."
                            }
                        }
                    ],
                    "synonyms": [
                        "white meat"
                    ],
                    "antonyms": [
                        "thigh",
                        "wing",
                        "dark meat"
                    ],
                    "common_phrases": [
                        "chicken breast",
                        "turkey breast",
                        "duck breast"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок мяса с передней части туловища птицы, особенно курицы или индейки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грудка",
                                "target_lang_sense_clarification": "мясо птицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9859153d-62c1-3879-bd30-d860c1b5d7fd",
                    "sense_definition": "Either of the two soft, fleshy parts on the front of a woman's chest that can produce milk.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mother sat down quietly to breastfeed her baby.",
                            "target_lang_translations": {
                                "ru": "Мать тихо села, чтобы покормить ребенка грудью."
                            }
                        },
                        {
                            "text": "Doctors recommend regular exams to check for <w>breast</w> cancer.",
                            "target_lang_translations": {
                                "ru": "Врачи рекомендуют регулярные обследования для проверки на рак <w>груди</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "breast cancer",
                        "breast milk",
                        "breast pump"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из двух мягких, мясистых частей на передней части женской грудной клетки, способных вырабатывать молоко."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грудь",
                                "target_lang_sense_clarification": "молочная железа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e06bb54f-e4c8-307f-bc7e-2a1fac539837",
                    "sense_definition": "The upper front part of the human body between the neck and the stomach.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He held the sleeping child gently against his <w>breast</w>.",
                            "target_lang_translations": {
                                "ru": "Он нежно прижал спящего ребенка к своей <w>груди</w>."
                            }
                        },
                        {
                            "text": "The soldier proudly wore a shiny medal on his left <w>breast</w>.",
                            "target_lang_translations": {
                                "ru": "Солдат с гордостью носил блестящую медаль на левой стороне <w>груди</w>."
                            }

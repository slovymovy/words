                        "do ostatniej kropli",
                        "ani kropli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень маленькое, ничтожное количество какого-либо вещества или вещи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капля",

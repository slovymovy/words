                    "antonyms": [
                        "new",
                        "pristine"
                    ],
                    "common_phrases": [
                        "worn out",
                        "badly worn",
                        "worn smooth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поврежденный, ветхий или потертый в результате длительного использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изношенный",
                                "target_lang_sense_clarification": "о вещах, механизмах"

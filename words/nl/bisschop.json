    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1fa46ce2-ab78-39f5-9bb6-882f36edeaac",
                    "sense_definition": "Een christelijke geestelijke die aan het hoofd staat van een bisdom.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>bisschop</w> leidde de plechtige mis in de grote kathedraal.",
                            "target_lang_translations": {
                                "ru": "<w>Епископ</w> возглавил торжественную мессу в большом соборе."
                            }
                        },
                        {
                            "text": "Hij is al jaren een residerende <w>bisschop</w> in dit gebied.",
                            "target_lang_translations": {
                                "ru": "Он уже много лет является правящим <w>епископом</w> в этой области."
                            }
                        }
                    ],
                    "common_phrases": [
                        "katholieke bisschop",
                        "bisschop wijden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Христианский священнослужитель, возглавляющий епархию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "епископ",
                                "target_lang_sense_clarification": "церковный сан"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e228cd8c-fe4e-319d-a42c-8d9a720ba09c",
                    "sense_definition": "Een warme, zoete drank gemaakt van rode wijn en specerijen, vaak gedronken rond Sinterklaas.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op koude winteravonden drinken we graag een glas warme <w>bisschop</w>.",
                            "target_lang_translations": {
                                "ru": "Холодными зимними вечерами мы любим выпить стакан горячего <w>глинтвейна</w>."
                            }
                        },
                        {
                            "text": "Mijn grootmoeder maakte vroeger heerlijke <w>bisschop</w> met kaneel en kruidnagel.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка раньше готовила вкусный <w>глинтвейн</w> с корицей и гвоздикой."
                            }
                        }
                    ],
                    "synonyms": [
                        "loper",
                        "raadsheer"
                    ],
                    "common_phrases": [
                        "warme bisschop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Горячий сладкий напиток из красного вина и специй, который часто пьют в период празднования Синтерклааса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глинтвейн",
                                "target_lang_sense_clarification": "напиток"
                            }
                        ]
                    }
                }
            ]
        }
    ]

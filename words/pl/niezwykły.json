                            },
                            {
                                "target_lang_word": "выдающийся",
                                "target_lang_sense_clarification": "знаменитый, превосходный"
                            },
                            {
                                "target_lang_word": "исключительный",
                                "target_lang_sense_clarification": "редкостный, особенный"

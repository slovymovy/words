    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "33f13814-bdf3-3696-96bc-41c25a6759d9",
                    "sense_definition": "Powieść lub opowiadanie, których fabuła koncentruje się na zbrodni, śledztwie i wykrywaniu sprawcy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "literature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wakacje zabrałem ze sobą wciągający <w>kryminał</w> ulubionego autora.",
                            "target_lang_translations": {
                                "ru": "В отпуск я взял с собой захватывающий <w>детектив</w> любимого автора."
                            }
                        },
                        {
                            "text": "Moja babcia uwielbia czytać <w>kryminały</w> w długie, zimowe wieczory.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка обожает читать <w>детективы</w> долгими зимними вечерами."
                            }
                        }
                    ],
                    "synonyms": [
                        "powieść kryminalna"
                    ],
                    "common_phrases": [
                        "czytać kryminał",
                        "dobry kryminał",
                        "skandynawski kryminał",
                        "pisać kryminały"
                    ],
                    "target_lang_definitions": {
                        "ru": "Роман или рассказ, сюжет которого сосредоточен на преступлении, расследовании и выявлении преступника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детектив",
                                "target_lang_sense_clarification": "литературный жанр"
                            },
                            {
                                "target_lang_word": "детективный роман",
                                "target_lang_sense_clarification": "книга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "daf1f4c3-4126-34a2-8a47-a6823ba03a07",
                    "sense_definition": "Film fabularny o tematyce przestępczej, ukazujący działania policji, detektywów lub przestępców.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cinema",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj w telewizji leciał stary, amerykański <w>kryminał</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера по телевизору шёл старый американский <w>детектив</w>."
                            }
                        },
                        {
                            "text": "Nie lubię komedii romantycznych, wolę obejrzeć trzymający w napięciu <w>kryminał</w>.",
                            "target_lang_translations": {
                                "ru": "Я не люблю романтические комедии, предпочитаю смотреть напряжённый <w>детектив</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "film kryminalny"
                    ],
                    "common_phrases": [
                        "oglądać kryminał",
                        "serial i kryminał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Художественный фильм о преступлениях, показывающий работу полиции, детективов или преступников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детектив",
                                "target_lang_sense_clarification": "жанр кино"
                            },
                            {
                                "target_lang_word": "криминальный фильм",
                                "target_lang_sense_clarification": "кино"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39cabe77-c090-3f32-93af-ab363f0ec852",
                    "sense_definition": "Potoczne określenie zakładu karnego, miejsca odbywania kary pozbawienia wolności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "law_enforcement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za kradzież samochodu złodziej trafił na dwa lata do <w>kryminału</w>.",
                            "target_lang_translations": {
                                "ru": "За кражу машины вор попал на два года в <w>тюрьму</w>."
                            }
                        },
                        {
                            "text": "Mój sąsiad spędził w <w>kryminale</w> połowę swojego dorosłego życia.",
                            "target_lang_translations": {
                                "ru": "Мой сосед провёл в <w>тюрьме</w> половину своей взрослой жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "więzienie",
                        "paka"
                    ],
                    "common_phrases": [
                        "trafić do kryminału",
                        "siedzieć w kryminale",
                        "wsadzić do kryminału"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название исправительного учреждения, места лишения свободы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тюрьма",
                                "target_lang_sense_clarification": "место заключения"
                            },
                            {
                                "target_lang_word": "тюряга",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d42f2bdf-2dd2-3355-8c67-d8b9e0b2e952",
                    "sense_definition": "Sytuacja, czyn lub zjawisko oceniane jako skandaliczne, niedopuszczalne lub bardzo naganne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest po prostu <w>kryminał</w>, żeby kazać ludziom pracować w takich warunkach.",
                            "target_lang_translations": {
                                "ru": "Это просто <w>преступление</w> — заставлять людей работать в таких условиях."
                            }
                        },
                        {
                            "text": "Ceny mieszkań w centrum miasta to czysty <w>kryminał</w>.",
                            "target_lang_translations": {
                                "ru": "Цены на квартиры в центре города — это чистый <w>криминал</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przestępstwo"
                    ],
                    "common_phrases": [

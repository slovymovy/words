    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "da690140-fe0c-39e0-b502-33da00263fe1",
                    "sense_definition": "De naam dragen; als naam hebben.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "naming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>heet</w> Thomas en ik kom uit België.",
                            "target_lang_translations": {
                                "ru": "Меня <w>зовут</w> Томас, и я из Бельгии."
                            }
                        },
                        {
                            "text": "Hoe <w>heet</w> jouw broer?",
                            "target_lang_translations": {
                                "ru": "Как <w>зовут</w> твоего брата?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "heten naar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Носить имя; зваться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зваться",
                                "target_lang_sense_clarification": "иметь имя"
                            },
                            {
                                "target_lang_word": "называться",
                                "target_lang_sense_clarification": "носить имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d2abcae-79ee-3d42-a29f-25f89aca1a76",
                    "sense_definition": "Aangeduid worden met een bepaalde term (vaak in een andere taal).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "naming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hoe <w>heet</w> dit gerecht in het Italiaans?",
                            "target_lang_translations": {
                                "ru": "Как <w>называется</w> это блюдо по-итальянски?"
                            }
                        },
                        {
                            "text": "Dat kleine diertje <w>heet</w> een lieveheersbeestje.",
                            "target_lang_translations": {
                                "ru": "Этот маленький жучок <w>называется</w> божьей коровкой."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Называться; обозначаться определенным термином."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "называться",
                                "target_lang_sense_clarification": "иметь название (о предметах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d01aa65e-1f09-30ec-96d5-3ab1cd6a71a3",
                    "sense_definition": "Betekenen; neerkomen op.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat <w>heet</w> geluk hebben, want het had veel slechter kunnen aflopen.",
                            "target_lang_translations": {
                                "ru": "Это <w>называется</w> повезло, так как всё могло закончиться гораздо хуже."
                            }
                        },
                        {
                            "text": "Zonder te betalen weglopen <w>heet</w> diefstal.",
                            "target_lang_translations": {
                                "ru": "Уйти не заплатив <w>называется</w> кражей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Означать; являться по сути."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "называться",
                                "target_lang_sense_clarification": "считаться чем-либо"
                            },
                            {
                                "target_lang_word": "считаться",
                                "target_lang_sense_clarification": "расцениваться как"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7efb2cff-f888-3da0-a21b-95e16c87c4a8",
                    "sense_definition": "Iemand of iets noemen; een naam geven.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "naming_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>heten</w> u van harte welkom in ons restaurant.",
                            "target_lang_translations": {
                                "ru": "Мы сердечно <w>приветствуем</w> вас в нашем ресторане."
                            }
                        },
                        {
                            "text": "De directeur <w>heette</w> de nieuwe collega's welkom.",
                            "target_lang_translations": {
                                "ru": "Директор <w>поприветствовал</w> новых коллег."
                            }
                        }
                    ],
                    "synonyms": [
                        "noemen"
                    ],
                    "common_phrases": [
                        "welkom heten"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "buiten de uitdrukking 'welkom heten'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приветствовать (в выражении 'welkom heten')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приветствовать",
                                "target_lang_sense_clarification": "выражать приветствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12715c2d-2adc-3ca4-95d3-b98fec78ef25",
                    "sense_definition": "Beweerd worden; naar men zegt zijn.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reputation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>heet</w> dat de regering binnenkort nieuwe maatregelen aankondigt.",
                            "target_lang_translations": {
                                "ru": "<w>Говорят</w>, что правительство скоро объявит о новых мерах."
                            }
                        },
                        {
                            "text": "Hij <w>heet</w> een van de rijkste mensen van het dorp te zijn.",
                            "target_lang_translations": {
                                "ru": "Он <w>слывет</w> одним из самых богатых людей в деревне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het heet dat"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formeel"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Считаться; иметь репутацию (по слухам)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слыть",
                                "target_lang_sense_clarification": "иметь репутацию"
                            },
                            {
                                "target_lang_word": "говориться",
                                "target_lang_sense_clarification": "утверждаться (безличн.)"
                            }
                        ]
                    }
                }
            ]
        },

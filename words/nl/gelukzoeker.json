    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9c31e8ce-3333-35cd-ba4d-f5b020c1f481",
                    "sense_definition": "Iemand die naar een ander land trekt om daar een beter bestaan op te bouwen, vaak gebruikt met een negatieve bijklank om onderscheid te maken met vluchtelingen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "migrant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politicus maakte in zijn toespraak een scherp onderscheid tussen de vluchteling en de <w>gelukzoeker</w>.",
                            "target_lang_translations": {
                                "ru": "В своей речи политик провел четкое различие между беженцем и <w>искателем счастья</w>."
                            }
                        },
                        {
                            "text": "Hij wordt vaak onterecht weggezet als een economische <w>gelukzoeker</w> terwijl hij voor oorlog vluchtte.",
                            "target_lang_translations": {
                                "ru": "Его часто несправедливо называют экономическим <w>мигрантом</w>, хотя он бежал от войны."
                            }
                        }
                    ],
                    "synonyms": [
                        "migrant",
                        "fortuinzoeker"
                    ],
                    "antonyms": [
                        "vluchteling"
                    ],
                    "common_phrases": [
                        "economische gelukzoeker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, переезжающий в другую страну ради лучшей жизни; часто используется с негативным оттенком для отличия от беженцев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искатель счастья",
                                "target_lang_sense_clarification": "иронично об экономическом мигранте"
                            },
                            {
                                "target_lang_word": "экономический мигрант",
                                "target_lang_sense_clarification": "официальный термин"
                            },
                            {
                                "target_lang_word": "искатель лучшей жизни",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9eac9a49-576f-3835-8849-231273223a3c",
                    "sense_definition": "Iemand die op avontuur gaat of risico's neemt in de hoop rijkdom of succes te vinden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fortune_hunter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als een echte <w>gelukzoeker</w> reisde hij de wereld rond op zoek naar goud en roem.",
                            "target_lang_translations": {
                                "ru": "Как настоящий <w>ловец удачи</w>, он путешествовал по миру в поисках золота и славы."
                            }
                        },
                        {
                            "text": "De roman vertelt het verhaal van een jonge <w>gelukzoeker</w> die zijn fortuin probeert te maken in de grote stad.",
                            "target_lang_translations": {
                                "ru": "Роман рассказывает историю молодого <w>искателя счастья</w>, который пытается сколотить состояние в большом городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "avonturier",
                        "fortuinzoeker"
                    ],
                    "common_phrases": [
                        "jonge gelukzoeker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто отправляется на поиски приключений или рискует в надежде обрести богатство или успех."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ловец удачи",
                                "target_lang_sense_clarification": "ищущий богатства и успеха"
                            },
                            {
                                "target_lang_word": "искатель приключений",
                                "target_lang_sense_clarification": "авантюрист"
                            },
                            {
                                "target_lang_word": "охотник за удачей",
                                "target_lang_sense_clarification": "буквальный перевод"
                            }
                        ]
                    }
                }
            ]
        }
    ]

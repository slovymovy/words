            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2bf30300-9527-3f45-96c3-262a4dadb494",
                    "sense_definition": "Пластиковая карта, привязанная к банковскому счёту, используемая для безналичной оплаты и снятия наличных.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом магазине можно расплатиться <карточкой>.",
                            "target_lang_translations": {
                                "en": "You can pay with a <w>card</w> in this store."
                            }
                        },
                        {
                            "text": "Я забыл пин-код от своей зарплатной <карточки>.",
                            "target_lang_translations": {
                                "en": "I forgot the PIN for my salary <w>card</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "банковская карта",
                        "карта"
                    ],
                    "common_phrases": [
                        "оплатить карточкой",
                        "деньги на карточке",
                        "кредитная карточка"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "общеупотребительное название банковской карты"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A plastic card linked to a bank account, used for cashless payments and cash withdrawals."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "card",
                                "target_lang_sense_clarification": "payment method"
                            },
                            {
                                "target_lang_word": "bank card",
                                "target_lang_sense_clarification": "financial tool"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34925905-c9e0-3777-9d41-6f821306a6a0",
                    "sense_definition": "Небольшой лист плотной бумаги или картона, содержащий определённые сведения, записи или служащий документом.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач долго искал мою медицинскую <карточку> в регистратуре.",
                            "target_lang_translations": {
                                "en": "The doctor spent a long time looking for my medical <w>record</w> at the reception."
                            }
                        },
                        {
                            "text": "Он протянул мне свою визитную <карточку> с номером телефона.",
                            "target_lang_translations": {
                                "en": "He handed me his business <w>card</w> with a phone number."
                            }
                        }
                    ],
                    "common_phrases": [
                        "визитная карточка",
                        "медицинская карточка",
                        "заполнить карточку"
                    ],
                    "target_lang_definitions": {
                        "en": "A small sheet of thick paper or cardboard containing specific information, records, or serving as a document."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "card",
                                "target_lang_sense_clarification": "general document"
                            },
                            {
                                "target_lang_word": "business card",
                                "target_lang_sense_clarification": "contact information"
                            },
                            {
                                "target_lang_word": "record",
                                "target_lang_sense_clarification": "medical file"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e573e095-7644-38b0-baf6-c308a6d23f5a",
                    "sense_definition": "Билет для многократного проезда в общественном транспорте.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студенческая <карточка> позволяет значительно экономить на проезде в метро.",
                            "target_lang_translations": {
                                "en": "A student <w>card</w> allows you to save significantly on subway fares."
                            }
                        },
                        {
                            "text": "Приложите <карточку> к валидатору при входе в автобус.",
                            "target_lang_translations": {
                                "en": "Tap your <w>card</w> on the validator when entering the bus."
                            }
                        }
                    ],
                    "synonyms": [
                        "проездной",
                        "проездной билет"
                    ],
                    "common_phrases": [
                        "проездная карточка",
                        "деньги на карточке"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "преимущественно в Санкт-Петербурге"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A ticket for multiple journeys on public transport."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "travel card",
                                "target_lang_sense_clarification": "transport ticket"
                            },
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "subscription"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "437d7d27-6d71-352f-a466-18e8d1a1d800",
                    "sense_definition": "Фотографический снимок, обычно небольшого формата (часто о старых фотографиях).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "image",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка любила пересматривать старые <карточки> в семейном альбоме.",
                            "target_lang_translations": {
                                "en": "Grandma loved looking through old <w>photos</w> in the family album."
                            }
                        },
                        {
                            "text": "На этой <карточке> я получился очень серьёзным.",
                            "target_lang_translations": {
                                "en": "I turned out very serious in this <w>photo</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "фотокарточка"
                    ],
                    "common_phrases": [
                        "старая карточка",
                        "карточка на память"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "в современном языке чаще используется слово «фотография» или «фотка»"
                        }
                    ],
                    "target_lang_definitions": {

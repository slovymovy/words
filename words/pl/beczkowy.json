    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5b6a6e09-39ca-3cc2-aa11-8dc306583a06",
                    "sense_definition": "o napojach, zwłaszcza piwie lub winie: nalewany bezpośrednio z beczki, a nie z butelki czy puszki",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "beverages",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W upalny dzień najlepiej smakuje zimne piwo <w>beczkowe</w>.",
                            "target_lang_translations": {
                                "ru": "В жаркий день вкуснее всего холодное <w>разливное</w> пиво."
                            }
                        },
                        {
                            "text": "W tej restauracji serwują doskonałe wino <w>beczkowe</w> z lokalnej winnicy.",
                            "target_lang_translations": {
                                "ru": "В этом ресторане подают отличное <w>бочковое</w> вино с местной винодельни."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piwo beczkowe",
                        "wino beczkowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "О напитках, особенно пиве или вине: наливаемый непосредственно из бочки, а не из бутылки или банки"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разливной",
                                "target_lang_sense_clarification": "о напитках"
                            },
                            {
                                "target_lang_word": "бочковой",
                                "target_lang_sense_clarification": "из бочки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2b8d418-ea6e-349a-af93-61b4f90d279c",
                    "sense_definition": "związany z beczką, przechowywany w beczce lub z niej pochodzący (często o żywności)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na targu kupiłem pyszne ogórki <w>beczkowe</w>, które są idealne do obiadu.",
                            "target_lang_translations": {
                                "ru": "На рынке я купил вкусные <w>бочковые</w> огурцы, которые идеально подходят к обеду."
                            }
                        },
                        {
                            "text": "W starej piwnicy unosił się charakterystyczny zapach <w>beczkowy</w>.",
                            "target_lang_translations": {
                                "ru": "В старом подвале стоял характерный <w>бочковой</w> запах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ogórki beczkowe",
                        "kapusta beczkowa",
                        "sól beczkowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с бочкой, хранящийся в бочке или происходящий из неё (часто о продуктах питания)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бочковой",
                                "target_lang_sense_clarification": "приготовленный в бочке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c56d0d27-5fda-3406-9c42-bc974b5676e2",
                    "sense_definition": "mający kształt beczki lub przypominający beczkę",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zabytkowy kościół posiada piękne sklepienie <w>beczkowe</w> nad nawą główną.",
                            "target_lang_translations": {
                                "ru": "В старинной церкви над главным нефом есть красивый <w>цилиндрический</w> свод."
                            }
                        },
                        {
                            "text": "Lekarz zdiagnozował u pacjenta klatkę piersiową <w>beczkową</w>.",
                            "target_lang_translations": {
                                "ru": "Врач диагностировал у пациента <w>бочкообразную</w> грудную клетку."
                            }
                        }
                    ],
                    "synonyms": [
                        "beczkowaty"
                    ],
                    "common_phrases": [
                        "sklepienie beczkowe",
                        "klatka piersiowa beczkowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий форму бочки или напоминающий бочку"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бочкообразный",
                                "target_lang_sense_clarification": "по форме"
                            }
                        ]
                    }
                }
            ]
        }
    ]

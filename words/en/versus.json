    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "22819ac3-6e59-371a-9f02-c3058659067c",
                    "sense_definition": "Against; in opposition to, especially in a sports competition or conflict.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "opposition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The final match will be France <w>versus</w> Brazil.",
                            "target_lang_translations": {
                                "ru": "Финальный матч будет Франция <w>против</w> Бразилии."
                            }
                        },
                        {
                            "text": "It often feels like it is us <w>versus</w> them in this negotiation.",
                            "target_lang_translations": {
                                "ru": "В этих переговорах часто кажется, что это мы <w>против</w> них."
                            }
                        }
                    ],
                    "synonyms": [
                        "vs",
                        "vs."
                    ],
                    "common_phrases": [
                        "us versus them",
                        "good versus evil",
                        "David versus Goliath"
                    ],
                    "target_lang_definitions": {
                        "ru": "Против; в оппозиции к кому-либо, особенно в спортивных соревнованиях или конфликтах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "про́тив",
                                "target_lang_sense_clarification": "предлог для борьбы или соперничества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1e33640-0f3e-3e4f-b339-40f693470f51",
                    "sense_definition": "Compared with or considered as an alternative to.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to weigh the costs of buying <w>versus</w> renting a house.",
                            "target_lang_translations": {
                                "ru": "Нам нужно взвесить затраты на покупку <w>против</w> аренды дома."
                            }
                        },
                        {
                            "text": "The graph shows the sales figures for May <w>versus</w> June.",
                            "target_lang_translations": {
                                "ru": "График показывает цифры продаж за май <w>по сравнению с</w> июнем."
                            }
                        }
                    ],
                    "synonyms": [
                        "vs",
                        "vs."
                    ],
                    "common_phrases": [
                        "quality versus quantity",
                        "nature versus nurture",
                        "pros versus cons"
                    ],
                    "target_lang_definitions": {
                        "ru": "В сравнении с или рассматриваемый как альтернатива чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "про́тив",
                                "target_lang_sense_clarification": "в сопоставлении с чем-либо"
                            },
                            {
                                "target_lang_word": "по сравне́нию с",
                                "target_lang_sense_clarification": "сравнивая с чем-либо"
                            },
                            {
                                "target_lang_word": "в сравне́нии с",
                                "target_lang_sense_clarification": "при сравнении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c5d95ed-3b01-3cdf-9fbc-c200aac40f52",
                    "sense_definition": "Bringing a legal action against; used in the title of a court case to separate the plaintiff and the defendant.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "opposition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The landmark case of Brown <w>versus</w> Board of Education changed the school system.",
                            "target_lang_translations": {
                                "ru": "Знаковое дело «Браун <w>против</w> Совета по образованию» изменило школьную систему."
                            }
                        },
                        {
                            "text": "In the trial of the State <w>versus</w> Smith, the jury reached a verdict quickly.",
                            "target_lang_translations": {
                                "ru": "На суде «Штат <w>против</w> Смита» присяжные быстро вынесли вердикт."
                            }
                        }
                    ],
                    "synonyms": [
                        "v",
                        "v."
                    ],
                    "common_phrases": [
                        "Roe versus Wade"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возбуждение судебного иска против; используется в названии судебного дела для разделения истца и ответчика."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "про́тив",
                                "target_lang_sense_clarification": "в юридическом контексте"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "44a19396-97e4-3ea3-b119-f00d1a56bec3",
                    "sense_definition": "To face in competition or fight against.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My team <w>versused</w> the champions in the first round and lost.",
                            "target_lang_translations": {
                                "ru": "Моя команда <w>играла против</w> чемпионов в первом раунде и проиграла."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e1a294af-da93-3fbf-8cb9-f8d3d24fb4a1",
                    "sense_definition": "Suma wpływów pieniężnych (przychodów) pomniejszona o koszty ich uzyskania; środki uzyskane z pracy, działalności gospodarczej lub majątku.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej miesięczny <w>dochód</w> pozwala na spokojne i wygodne życie.",
                            "target_lang_translations": {
                                "ru": "Её ежемесячный <w>доход</w> позволяет вести спокойную и комфортную жизнь."
                            }
                        },
                        {
                            "text": "Firma przeznaczyła część zysku na inwestycje, aby zwiększyć przyszły <w>dochód</w>.",
                            "target_lang_translations": {
                                "ru": "Компания выделила часть прибыли на инвестиции, чтобы увеличить будущий <w>доход</w>."
                            }
                        },
                        {
                            "text": "Każdy obywatel musi raz w roku rozliczyć swój <w>dochód</w> w urzędzie skarbowym.",
                            "target_lang_translations": {
                                "ru": "Каждый гражданин обязан раз в год декларировать свой <w>доход</w> в налоговой инспекции."
                            }
                        }
                    ],
                    "synonyms": [
                        "zysk",
                        "przychód",
                        "wpływ",
                        "intrata"
                    ],
                    "antonyms": [
                        "rozchód"
                    ],
                    "common_phrases": [
                        "dochód netto",
                        "dochód brutto",
                        "dochód narodowy",
                        "źródło dochodu",
                        "osiągać dochód"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежные средства, полученные в результате работы, хозяйственной деятельности или от имущества; финансовые поступления за вычетом расходов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доход",
                                "target_lang_sense_clarification": "денежные поступления"
                            },
                            {
                                "target_lang_word": "прибыль",
                                "target_lang_sense_clarification": "финансовая выгода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca0d1568-d5b1-3700-bf41-51acd06810c0",
                    "sense_definition": "Fakt dotarcia do określonego miejsca; przybycie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szczęśliwy <w>dochód</w> podróżników do celu ucieszył wszystkich oczekujących.",
                            "target_lang_translations": {
                                "ru": "Благополучное <w>прибытие</w> путешественников к цели обрадовало всех ожидающих."
                            }
                        },
                        {
                            "text": "Opóźniony <w>dochód</w> wojsk królewskich zmienił losy całej bitwy.",
                            "target_lang_translations": {
                                "ru": "Запоздалый <w>приход</w> королевских войск изменил исход всей битвы."
                            }
                        }
                    ],
                    "synonyms": [
                        "przybycie"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie używa się słów 'przybycie' lub 'dojście'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Факт достижения определенного места; прибытие (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прибытие",
                                "target_lang_sense_clarification": "достижение места"
                            },
                            {
                                "target_lang_word": "приход",
                                "target_lang_sense_clarification": "появление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34ec9173-4758-3878-96fe-13488c898878",
                    "sense_definition": "Mieszanina wyższych alkoholi powstająca jako produkt uboczny fermentacji alkoholowej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "chemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niewłaściwie przeprowadzona destylacja sprawiła, że w alkoholu pozostał szkodliwy <w>dochód</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за неправильно проведенной дистилляции в спирте осталось вредное <w>сивушное масло</w>."
                            }
                        },
                        {
                            "text": "Ten bimber ma nieprzyjemny zapach, ponieważ zawiera <w>dochód</w>.",
                            "target_lang_translations": {
                                "ru": "У этого самогона неприятный запах, так как он содержит <w>сивуху</w>."
                            }
                        }
                    ],
                    "synonyms": [

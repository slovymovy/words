    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "20a3b8f1-caeb-3c45-8b28-b6df26c350c1",
                    "sense_definition": "Wyznaczone miejsce w sklepie, urzędzie, kinie lub na dworcu, gdzie dokonuje się płatności lub kupuje bilety.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "place_of_payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę zapłacić za zakupy w <kasie> przy wyjściu.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, оплатите покупки на <w>кассе</w> у выхода."
                            }
                        },
                        {
                            "text": "Kolejka do <kasy> biletowej była bardzo długa.",
                            "target_lang_translations": {
                                "ru": "Очередь в билетную <w>кассу</w> была очень длинной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kasa biletowa",
                        "podejść do kasy",
                        "stać w kasie",
                        "kasa pierwszeństwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специально отведенное место в магазине, учреждении, кинотеатре или на вокзале, где производятся платежи или покупаются билеты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "касса",
                                "target_lang_sense_clarification": "место оплаты или продажи билетов"
                            },
                            {
                                "target_lang_word": "окошко",
                                "target_lang_sense_clarification": "место обслуживания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f86db300-d9fa-39d9-ade1-c6c2cbd9a131",
                    "sense_definition": "Potoczne określenie na pieniądze, środki finansowe lub gotówkę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "money_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mogę iść do kina, bo nie mam <kasy>.",
                            "target_lang_translations": {
                                "ru": "Я не могу пойти в кино, потому что у меня нет <w>денег</w>."
                            }
                        },
                        {
                            "text": "On zarabia kupę <kasy> w nowej firmie.",
                            "target_lang_translations": {
                                "ru": "Он зарабатывает кучу <w>денег</w> в новой фирме."
                            }
                        }
                    ],
                    "synonyms": [
                        "pieniądze",
                        "forsa",
                        "hajs",
                        "gotówka",
                        "szmal",
                        "mamona"
                    ],
                    "common_phrases": [
                        "nie mieć kasy",
                        "kupa kasy",
                        "robić kasę",
                        "wydać kasę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }

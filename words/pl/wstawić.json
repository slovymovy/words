                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заступиться",
                                "target_lang_sense_clarification": "встать на защиту чьих-либо интересов"
                            },
                            {
                                "target_lang_word": "похлопотать",
                                "target_lang_sense_clarification": "обратиться с просьбой ради кого-то"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "aad39824-7ecd-3f82-8858-8d9ea4795fa2",
                    "sense_definition": "Simple past tense of <w>fly</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The large bird <flew> silently over the trees.",
                            "target_lang_translations": {
                                "ru": "Большая птица бесшумно <w>пролетела</w> над деревьями."
                            }
                        },
                        {
                            "text": "We <flew> to Paris for our summer vacation last year.",
                            "target_lang_translations": {
                                "ru": "В прошлом году мы <w>полетели</w> в Париж в летний отпуск."
                            }
                        },
                        {
                            "text": "Time <flew> so fast that we barely noticed the sun going down.",
                            "target_lang_translations": {
                                "ru": "Время <w>пролетело</w> так быстро, что мы едва заметили, как село солнце."
                            }
                        }
                    ],
                    "common_phrases": [
                        "flew away",
                        "flew open",
                        "time flew",
                        "flew off",
                        "flew over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время от глагола fly (летать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "летел",
                                "target_lang_sense_clarification": "процесс полета"
                            },
                            {
                                "target_lang_word": "полетел",
                                "target_lang_sense_clarification": "начало полета или направление"
                            },
                            {
                                "target_lang_word": "пролетел",
                                "target_lang_sense_clarification": "о времени или преодолении расстояния"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e95b2ce8-25a1-322c-a04d-3f6e38b33314",
                    "sense_definition": "Simple past tense of <w>flay</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the old legend, the warrior <flew> his enemy after the battle.",
                            "target_lang_translations": {
                                "ru": "В старой легенде воин <w>содрал кожу</w> со своего врага после битвы."
                            }
                        },
                        {
                            "text": "He <flew> the bark from the tree to make a shelter.",
                            "target_lang_translations": {
                                "ru": "Он <w>ободрал</w> кору с дерева, чтобы сделать укрытие."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": "Standard past tense is 'flayed'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшая форма прошедшего времени от глагола flay (сдирать кожу)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "содрал кожу",
                                "target_lang_sense_clarification": "с кого-либо"
                            },
                            {
                                "target_lang_word": "ободрал",
                                "target_lang_sense_clarification": "очистил от коры или кожи"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "06ee5b06-7ef1-3f6c-8928-bb552d827899",
                    "sense_definition": "The thick, dangling upper lip of certain breeds of dog.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bulldog had heavy <flews> that shook whenever he barked.",
                            "target_lang_translations": {
                                "ru": "У бульдога были тяжелые <w>брыли</w>, которые тряслись, когда он лаял."
                            }
                        },
                        {
                            "text": "You should clean the dog's <flews> regularly to prevent infection.",
                            "target_lang_translations": {
                                "ru": "Вам следует регулярно чистить <w>брыли</w> собаки, чтобы предотвратить инфекцию."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отвислые верхние губы у некоторых пород собак."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брыли",
                                "target_lang_sense_clarification": "губы собаки"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1d4d5850-33f2-32d4-b164-094925140d0e",
                    "sense_definition": "Наполнить ёмкость или резервуар жидкостью доверху.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель остановился на заправке, чтобы <w>залить</w> полный бак бензина.",
                            "target_lang_translations": {
                                "en": "The driver stopped at the gas station to <w>fill up</w> a full tank of gas."
                            }
                        },
                        {
                            "text": "Нужно <w>залить</w> воду в чайник перед тем, как его включить.",
                            "target_lang_translations": {
                                "en": "You need to <w>pour</w> water into the kettle before turning it on."
                            }
                        }
                    ],
                    "synonyms": [
                        "наполнить"
                    ],
                    "common_phrases": [
                        "залить бак",
                        "залить бензин",
                        "залить воды"
                    ],
                    "target_lang_definitions": {
                        "en": "To fill a container or reservoir with liquid to the top."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fill up",
                                "target_lang_sense_clarification": "to make a container full"
                            },
                            {
                                "target_lang_word": "pour",
                                "target_lang_sense_clarification": "to cause liquid to flow into a container"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d2f7661-134d-3130-841a-35eb04fe129e",
                    "sense_definition": "Испачкать или испортить поверхность, случайно пролив на неё жидкость.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я случайно <w>залил</w> важные документы горячим кофе.",
                            "target_lang_translations": {
                                "en": "I accidentally <w>spilled</w> hot coffee <w>on</w> important documents."
                            }
                        },
                        {
                            "text": "Гость неловко повернулся и <w>залил</w> белую скатерть красным вином.",
                            "target_lang_translations": {
                                "en": "The guest turned awkwardly and <w>stained</w> the white tablecloth with red wine."
                            }
                        }
                    ],
                    "synonyms": [
                        "испачкать"
                    ],
                    "common_phrases": [
                        "залить скатерть",
                        "залить стол",
                        "залить платье"
                    ],
                    "target_lang_definitions": {
                        "en": "To stain or ruin a surface by accidentally spilling liquid on it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spill ... on",
                                "target_lang_sense_clarification": "accidental action of liquid falling on something"
                            },
                            {
                                "target_lang_word": "stain",
                                "target_lang_sense_clarification": "to mark or spoil a surface with liquid"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1d42e48-dc85-335a-a602-bd91b799d9c9",
                    "sense_definition": "Покрыть водой значительное пространство (о природных явлениях) или затопить помещение.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "flood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Весенний паводок <w>залил</w> все прибрежные луга и дороги.",
                            "target_lang_translations": {
                                "en": "The spring flood <w>flooded</w> all the coastal meadows and roads."
                            }
                        },
                        {
                            "text": "У нас прорвало трубу, и мы <w>залили</w> соседей снизу.",
                            "target_lang_translations": {
                                "en": "A pipe burst at our place, and we <w>flooded</w> the neighbors below."
                            }
                        }
                    ],
                    "synonyms": [
                        "затопить"
                    ],
                    "antonyms": [
                        "осушить"
                    ],
                    "common_phrases": [
                        "залить соседей",
                        "залить луга",
                        "залить водой"
                    ],
                    "target_lang_definitions": {
                        "en": "To cover a significant area with water (regarding natural phenomena) or to flood a room."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flood",
                                "target_lang_sense_clarification": "to overflow with water"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e92168a-1b9d-302b-9e2e-3e9f908aa172",
                    "sense_definition": "Потушить огонь, вылив на него большое количество воды.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "extinguish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы тщательно <w>залили</w> костёр перед уходом из леса.",
                            "target_lang_translations": {
                                "en": "The tourists thoroughly <w>doused</w> the campfire before leaving the forest."
                            }
                        },
                        {
                            "text": "Пожарным удалось быстро <w>залить</w> огонь в горящем доме.",
                            "target_lang_translations": {
                                "en": "Firefighters managed to quickly <w>put out</w> the fire in the burning house."
                            }
                        }
                    ],
                    "synonyms": [
                        "потушить"
                    ],
                    "common_phrases": [
                        "залить костёр",
                        "залить огонь",
                        "залить пожар"
                    ],
                    "target_lang_definitions": {
                        "en": "To extinguish a fire by pouring a large amount of water on it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "douse",
                                "target_lang_sense_clarification": "to pour liquid over fire to stop it"
                            },
                            {
                                "target_lang_word": "put out",
                                "target_lang_sense_clarification": "to extinguish a fire"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d51fd8b-7864-3e48-8886-77342ca6cb33",
                    "sense_definition": "Загрузить файлы, видео или фотографии в интернет или на сервер.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "upload",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Блогер обещал <w>залить</w> новое видео на свой канал сегодня вечером.",
                            "target_lang_translations": {
                                "en": "The blogger promised to <w>upload</w> a new video to his channel this evening."
                            }
                        },
                        {
                            "text": "Системный администратор забыл <w>залить</w> обновления на рабочий сервер.",
                            "target_lang_translations": {
                                "en": "The system administrator forgot to <w>upload</w> updates to the work server."
                            }
                        }
                    ],
                    "synonyms": [
                        "загрузить",
                        "выложить"
                    ],
                    "common_phrases": [
                        "залить видео",
                        "залить фотки",
                        "залить на сервер"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet/computer slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To upload files, videos, or photos to the internet or a server."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "upload",
                                "target_lang_sense_clarification": "to transfer data to a server or internet"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3c4b3a3-71ea-3785-9353-ce91424e82ba",
                    "sense_definition": "Заполнить собой всё пространство (о свете, звуке, запахе или чувствах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Яркое утреннее солнце <w>залило</w> всю спальню тёплым светом.",
                            "target_lang_translations": {
                                "en": "Bright morning sun <w>flooded</w> the whole bedroom with warm light."
                            }
                        },
                        {
                            "text": "Громкая музыка внезапно <w>залила</w> концертный зал.",
                            "target_lang_translations": {
                                "en": "Loud music suddenly <w>filled</w> the concert hall."
                            }
                        }
                    ],
                    "common_phrases": [
                        "свет залил комнату",
                        "румянец залил щёки"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "metaphorical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To fill a space completely (referring to light, sound, smell, or feelings)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flood",
                                "target_lang_sense_clarification": "metaphorical: to fill a space overwhelmingly"
                            },
                            {
                                "target_lang_word": "fill",
                                "target_lang_sense_clarification": "to occupy the entire space"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af1ba93d-fdd2-3548-a5ae-cabb7df71337",
                    "sense_definition": "Покрыть поверхность жидким строительным материалом, который затем затвердевает.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочие должны <w>залить</w> фундамент для нового дома до начала дождей.",
                            "target_lang_translations": {
                                "en": "The workers must <w>pour</w> the foundation for the new house before the rains start."
                            }
                        },
                        {
                            "text": "Строители решили <w>залить</w> пол бетоном для выравнивания.",
                            "target_lang_translations": {
                                "en": "The builders decided to <w>pour</w> the floor with concrete to level it."
                            }
                        }
                    ],
                    "synonyms": [
                        "забетонировать"
                    ],
                    "common_phrases": [
                        "залить фундамент",
                        "залить пол",
                        "залить бетоном"
                    ],
                    "target_lang_definitions": {
                        "en": "To cover a surface with liquid construction material which then hardens."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pour",
                                "target_lang_sense_clarification": "construction: to dispense concrete or cement"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
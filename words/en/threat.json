{
    "word_family": [
        "threaten",
        "threatening",
        "threateningly"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cf2c2303-65b9-32fd-a40b-dd1abfaae3ce",
                    "sense_definition": "An expression of intent to injure or punish another.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He made a serious <w>threat</w> to quit his job if his demands were not met.",
                            "target_lang_translations": {
                                "ru": "Он высказал серьезную <w>угрозу</w> уволиться, если его требования не будут выполнены."
                            }
                        },
                        {
                            "text": "The police investigated the anonymous death <w>threat</w> sent to the politician.",
                            "target_lang_translations": {
                                "ru": "Полиция расследовала анонимную <w>угрозу</w> убийством, отправленную политику."
                            }
                        }
                    ],
                    "synonyms": [
                        "intimidation",
                        "warning",
                        "menace"
                    ],
                    "common_phrases": [
                        "death threat",
                        "empty threat",
                        "carry out a threat",
                        "make a threat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение намерения причинить вред или наказать другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угроза",
                                "target_lang_sense_clarification": "запугивание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fe83eb7-c858-33d7-a4fb-ef884fc2811a",
                    "sense_definition": "A person or object that is regarded as a danger; a menace.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "danger_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pollution poses a major <w>threat</w> to the health of our oceans.",
                            "target_lang_translations": {
                                "ru": "Загрязнение представляет серьезную <w>угрозу</w> для здоровья наших океанов."
                            }
                        },
                        {
                            "text": "The new virus is considered a global security <w>threat</w>.",
                            "target_lang_translations": {
                                "ru": "Новый вирус считается глобальной <w>угрозой</w> безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "menace"
                    ],
                    "common_phrases": [
                        "pose a threat",
                        "security threat",
                        "major threat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или предмет, который считается опасным; источник опасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угроза",
                                "target_lang_sense_clarification": "источник опасности"
                            },
                            {
                                "target_lang_word": "опасность",
                                "target_lang_sense_clarification": "риск"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e1d7f39-53f0-3c8c-9ba3-94d83dc55817",
                    "sense_definition": "An indication of potential or imminent danger.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "danger_indication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The constant <w>threat</w> of rain forced the organizers to move the event indoors.",
                            "target_lang_translations": {
                                "ru": "Постоянная <w>угроза</w> дождя вынудила организаторов перенести мероприятие в помещение."
                            }
                        },
                        {
                            "text": "Many animal species are currently under <w>threat</w> of extinction.",
                            "target_lang_translations": {
                                "ru": "Многие виды животных в настоящее время находятся под <w>угрозой</w> вымирания."
                            }
                        }
                    ],
                    "synonyms": [
                        "warning",
                        "menace"
                    ],
                    "antonyms": [
                        "protection"
                    ],
                    "common_phrases": [
                        "threat of rain",
                        "under threat",
                        "imminent threat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Признак возможной или неизбежной опасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угроза",
                                "target_lang_sense_clarification": "вероятность беды"
                            },
                            {
                                "target_lang_word": "опасность",
                                "target_lang_sense_clarification": "риск возникновения ущерба"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ec7f2bc4-0a58-3ca1-9317-75855001755a",
                    "sense_definition": "To threaten.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dark storm clouds did <w>threat</w> the weary travelers with heavy rain.",
                            "target_lang_translations": {
                                "ru": "Темные грозовые тучи <w>грозили</w> уставшим путникам сильным ливнем."
                            }
                        },
                        {
                            "text": "He dared to <w>threat</w> the king with open rebellion.",
                            "target_lang_translations": {
                                "ru": "Он осмелился <w>угрожать</w> королю открытым восстанием."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Standard modern form is 'threaten'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Угрожать; грозить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угрожать",
                                "target_lang_sense_clarification": "обещать причинить вред"
                            },
                            {
                                "target_lang_word": "грозить",
                                "target_lang_sense_clarification": "предвещать опасность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6064a934-aa04-329d-92fb-edd4c3845539",
                    "sense_definition": "To press; urge; compel.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do not <w>threat</w> me to act against my own conscience.",
                            "target_lang_translations": {
                                "ru": "Не <w>принуждай</w> меня действовать против моей совести."
                            }
                        },
                        {
                            "text": "They tried to <w>threat</w> him into signing the document.",
                            "target_lang_translations": {
                                "ru": "Они пытались <w>заставить</w> его подписать документ."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": ""
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Давить; побуждать; принуждать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принуждать",
                                "target_lang_sense_clarification": "заставлять силой"
                            },
                            {
                                "target_lang_word": "заставлять",
                                "target_lang_sense_clarification": "вынуждать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "0da9af3d-75ad-3eca-b9ca-23debb187ad6",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Threat</w> has worked in the accounting department for ten years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Трет</w> проработал в бухгалтерии десять лет."
                            }
                        },
                        {
                            "text": "I was introduced to a woman named Sarah <w>Threat</w> at the conference.",
                            "target_lang_translations": {
                                "ru": "На конференции меня представили женщине по имени Сара <w>Трет</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Трет",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
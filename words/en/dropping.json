                    "synonyms": [
                        "dung",
                        "excrement",
                        "feces",
                        "scat"
                    ],
                    "common_phrases": [
                        "bird droppings",
                        "mouse droppings",
                        "animal droppings"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердые отходы жизнедеятельности животного или птицы."

                    "sense_id": "b24d7ad8-c78c-39ba-8704-70b07a286534",
                    "sense_definition": "Jedna z możliwości, którą można wybrać w danej sytuacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "choice",
                    "name_type": "no",

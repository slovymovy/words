    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d9de9fc1-7677-37c3-9df6-064d4340d9e8",
                    "sense_definition": "Forma liczby mnogiej rzeczownika <w>dzwonek</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na leśnej polanie zakwitły fioletowe <dzwonki>.",
                            "target_lang_translations": {
                                "ru": "На лесной поляне расцвели фиолетовые <колокольчики>."
                            }
                        },
                        {
                            "text": "Zimą słychać było wesołe <dzwonki> u sań.",
                            "target_lang_translations": {
                                "ru": "Зимой были слышны веселые <бубенчики> саней."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dzwonki sań",
                        "polne dzwonki"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "liczba mnoga"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма множественного числа существительного \"dzwonek\" (колокольчик, звонок)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колокольчики",
                                "target_lang_sense_clarification": "цветы или маленькие звонки"
                            },
                            {
                                "target_lang_word": "бубенчики",
                                "target_lang_sense_clarification": "на упряжи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abe68733-ae24-3645-bcb4-68fbef2bdee5",
                    "sense_definition": "Instrument muzyczny, w którym dźwięk wydobywa się przez uderzanie pałeczkami w metalowe płytki.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci w szkole uczyły się grać proste melodie na <dzwonkach>.",
                            "target_lang_translations": {
                                "ru": "Дети в школе учились играть простые мелодии на <металлофоне>."
                            }
                        },
                        {
                            "text": "W orkiestrze perkusista obsługiwał bębny i <dzwonki>.",
                            "target_lang_translations": {
                                "ru": "В оркестре перкуссионист играл на барабанах и <колокольчиках>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cymbałki"
                    ],
                    "common_phrases": [
                        "grać na dzwonkach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальный инструмент, в котором звук извлекается ударами палочек по металлическим пластинкам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "металлофон",
                                "target_lang_sense_clarification": "школьный инструмент"
                            },
                            {
                                "target_lang_word": "колокольчики",
                                "target_lang_sense_clarification": "оркестровый инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "255e2db0-506d-3a4a-b278-79f1a17219b5",
                    "sense_definition": "Rodzaj roślin z rodziny jasnotowatych, często uprawianych jako rośliny ozdobne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Florystka dodała do bukietu zielone <dzwonki> irlandzkie.",
                            "target_lang_translations": {
                                "ru": "Флористка добавила в букет зеленые ирландские <колокольчики>."
                            }
                        },
                        {
                            "text": "W naszym ogrodzie botanicznym rosną rzadkie <dzwonki>.",
                            "target_lang_translations": {
                                "ru": "В нашем ботаническом саду растут редкие <колокольчики>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gniazdeczka"
                    ],
                    "common_phrases": [
                        "dzwonki irlandzkie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид растений семейства яснотковых, часто выращиваемых как декоративные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ирландские колокольчики",
                                "target_lang_sense_clarification": "растение"
                            },
                            {
                                "target_lang_word": "молюцелла",
                                "target_lang_sense_clarification": "ботаническое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3af9236c-63c9-38bf-92c1-f93f1247c495",
                    "sense_definition": "Potoczne określenie na męskie genitalia.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas meczu piłkarz dostał piłką w <dzwonki>.",
                            "target_lang_translations": {
                                "ru": "Во время матча футболисту попали мячом по <бубенцам>."
                            }
                        },
                        {
                            "text": "Uważaj na swoje <dzwonki>, gdy przeskakujesz przez ten płot.",
                            "target_lang_translations": {
                                "ru": "Береги свои <бубенцы>, когда перепрыгиваешь через этот забор."
                            }
                        }
                    ],
                    "synonyms": [
                        "klejnoty",
                        "orzeszki"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "anatomicznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название мужских гениталий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бубенцы",
                                "target_lang_sense_clarification": "сленг, иронично"
                            },
                            {
                                "target_lang_word": "яйца",
                                "target_lang_sense_clarification": "грубое"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "półkowy",
        "półeczka",
        "póła"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5920386c-71e9-3a7e-8e5e-7eb12c0b976a",
                    "sense_definition": "Pozioma deska przymocowana do ściany lub stanowiąca element mebla, służąca do przechowywania lub eksponowania przedmiotów.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "furniture_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Postawiłem wazon z kwiatami na górnej <w>półce</w>.",
                            "target_lang_translations": {
                                "ru": "Я поставил вазу с цветами на верхнюю <w>полку</w>."
                            }
                        },
                        {
                            "text": "Wszystkie moje książki leżą na wiszącej <w>półce</w> nad biurkiem.",
                            "target_lang_translations": {
                                "ru": "Все мои книги лежат на подвесной <w>полке</w> над письменным столом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "półka na książki",
                        "wisząca półka",
                        "górna półka",
                        "odłożyć na półkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Горизонтальная доска, прикрепленная к стене или являющаяся частью мебели, используемая для хранения предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полка",
                                "target_lang_sense_clarification": "часть мебели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45b4bfaf-03db-3445-9755-a55d1f8d7cca",
                    "sense_definition": "Stojący mebel składający się z kilku poziomych desek ułożonych jedna nad drugą, służący do przechowywania rzeczy.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "furniture_objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiliśmy nową <w>półkę</w> na buty do przedpokoju.",
                            "target_lang_translations": {
                                "ru": "Мы купили новую <w>полку</w> для обуви в прихожую."
                            }
                        },
                        {
                            "text": "Ta drewniana <w>półka</w> idealnie pasuje do wystroju salonu.",
                            "target_lang_translations": {
                                "ru": "Эта деревянная <w>полка</w> идеально подходит к интерьеру гостиной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "półka na buty",
                        "półka łazienkowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет мебели, состоящий из нескольких горизонтальных уровней для хранения вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полка",
                                "target_lang_sense_clarification": "предмет мебели"
                            },
                            {
                                "target_lang_word": "стеллаж",
                                "target_lang_sense_clarification": "конструкция с полками"
                            },
                            {
                                "target_lang_word": "этажерка",
                                "target_lang_sense_clarification": "легкая мебель с полками"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85934cca-5ba4-3313-89e0-5c0b40ea29b9",
                    "sense_definition": "Płaski, poziomy występ skalny w górach lub na urwisku.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "geological_features",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Alpiniści spędzili noc na wąskiej <w>półce</w> skalnej.",
                            "target_lang_translations": {
                                "ru": "Альпинисты провели ночь на узкой скальной <w>полке</w>."
                            }
                        },
                        {
                            "text": "Orzeł wylądował na kamiennej <w>półce</w> wysoko w górach.",
                            "target_lang_translations": {
                                "ru": "Орел приземлился на каменный <w>уступ</w> высоко в горах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skalna półka",
                        "półka lodowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоский горизонтальный выступ на скале или обрыве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уступ",
                                "target_lang_sense_clarification": "выступ"
                            },
                            {
                                "target_lang_word": "полка",
                                "target_lang_sense_clarification": "геологический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c37be090-8385-3dd5-84d3-59581c3c2084",
                    "sense_definition": "Kawałek drewna powstały przez rozłupanie kloca na połowy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "wood_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz wrzucił do pieca dużą <w>półkę</w> drewna.",
                            "target_lang_translations": {
                                "ru": "Хозяин бросил в печь большое <w>полено</w>."
                            }
                        },
                        {
                            "text": "Na podwórku leżały pocięte <w>półki</w> przygotowane na opał.",
                            "target_lang_translations": {
                                "ru": "Во дворе лежали нарезанные <w>поленья</w>, заготовленные на дрова."
                            }
                        }
                    ],
                    "synonyms": [
                        "szczapa"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "używane w niektórych gwarach lub dawniej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок дерева, полученный раскалыванием полена пополам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полено",
                                "target_lang_sense_clarification": "дрова"
                            },
                            {
                                "target_lang_word": "плаха",
                                "target_lang_sense_clarification": "расколотое бревно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b03ddc77-0b96-3492-bb74-f5a862a273c2",
                    "sense_definition": "Jedna z części materiału, z których zszyta jest spódnica.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej ludowa spódnica składała się z sześciu kolorowych <w>półek</w>.",
                            "target_lang_translations": {
                                "ru": "Ее народная юбка состояла из шести цветных <w>клиньев</w>."
                            }
                        },
                        {
                            "text": "Krawcowa starannie zszyła ze sobą dwie <w>półki</w> materiału.",
                            "target_lang_translations": {
                                "ru": "Портниха тщательно сшила между собой два <w>полотнища</w> ткани."
                            }
                        }
                    ],
                    "synonyms": [
                        "bryt"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "określenie rzadkie lub gwarowe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из частей ткани, из которых сшита юбка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клин",
                                "target_lang_sense_clarification": "часть одежды"
                            },
                            {
                                "target_lang_word": "полотнище",
                                "target_lang_sense_clarification": "кусок ткани"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e22dceb0-ff68-305e-84a5-0351c2b29f6e",
                    "sense_definition": "Materiałowa powłoczka nakładana na poduszkę.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "bedding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia wyhaftowała piękne wzory na białej <w>półce</w>.",
                            "target_lang_translations": {
                                "ru": "Бабушка вышила красивые узоры на белой <w>наволочке</w>."
                            }
                        },
                        {
                            "text": "Muszę zdjąć brudną <w>półkę</w> i wyprać ją w gorącej wodzie.",
                            "target_lang_translations": {
                                "ru": "Мне нужно снять грязную <w>наволочку</w> и постирать ее в горячей воде."
                            }
                        }
                    ],
                    "synonyms": [
                        "poszewka",
                        "powłoczka"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "występuje w gwarach lub języku regionalnym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чехол из ткани, надеваемый на подушку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наволочка",
                                "target_lang_sense_clarification": "постельное белье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36a0380e-fbb5-396d-a037-c86f4252ae8c",
                    "sense_definition": "Dawna jednostka miary objętości lub wagi (ósemka).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measures",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na targu kupiła jedną <w>półkę</w> mąki pszennej.",
                            "target_lang_translations": {
                                "ru": "На рынке она купила одну <w>мерку</w> пшеничной муки."
                            }
                        },
                        {
                            "text": "To stare naczynie mieściło dokładnie jedną <w>półkę</w> ziarna.",
                            "target_lang_translations": {
                                "ru": "Этот старый сосуд вмещал ровно одну <w>мерку</w> зерна."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Śląsk Cieszyński"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "dawna miara"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная мера объема или веса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мерка",
                                "target_lang_sense_clarification": "единица измерения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "lounger",
        "loungewear"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0f45d2e4-08de-37e1-b34a-9af193f7a5ac",
                    "sense_definition": "The living room or sitting room of a house.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "room_home",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The whole family sat in the <w>lounge</w> watching television.",
                            "target_lang_translations": {
                                "ru": "Вся семья сидела в <w>гостиной</w> и смотрела телевизор."
                            }
                        },
                        {
                            "text": "She bought a new carpet for the <w>lounge</w>.",
                            "target_lang_translations": {
                                "ru": "Она купила новый ковер для <w>гостиной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sitting room",
                        "parlour"
                    ],
                    "common_phrases": [
                        "in the lounge"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British, Australian, New Zealand"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жилая комната в доме или квартире, предназначенная для отдыха и приема гостей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гостиная",
                                "target_lang_sense_clarification": "комната"
                            },
                            {
                                "target_lang_word": "зал",
                                "target_lang_sense_clarification": "разг. большая комната"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "011417a0-eba9-3d23-83cc-71b42982f282",
                    "sense_definition": "A public waiting room in an office, airport, or hotel.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "room_public",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We waited for our flight in the departure <w>lounge</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ждали своего рейса в <w>зале ожидания</w> вылета."
                            }
                        },
                        {
                            "text": "Business class passengers have access to a private <w>lounge</w>.",
                            "target_lang_translations": {
                                "ru": "Пассажиры бизнес-класса имеют доступ в частный <w>лаундж</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "departure lounge",
                        "airport lounge",
                        "VIP lounge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зал ожидания в аэропорту, отеле или офисном здании; зона отдыха для пассажиров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зал ожидания",
                                "target_lang_sense_clarification": "в аэропорту или на вокзале"
                            },
                            {
                                "target_lang_word": "лаундж",
                                "target_lang_sense_clarification": "зона повышенного комфорта"
                            },
                            {
                                "target_lang_word": "холл",
                                "target_lang_sense_clarification": "вестибюль отеля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "557301b5-822a-3e9e-a9cb-08a2c40a2236",
                    "sense_definition": "An establishment, similar to a bar, that serves alcohol and offers a relaxed atmosphere.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "room_public",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They met for a drink at a quiet cocktail <w>lounge</w> downtown.",
                            "target_lang_translations": {
                                "ru": "Они встретились, чтобы выпить, в тихом коктейль-<w>баре</w> в центре города."
                            }
                        },
                        {
                            "text": "The hotel <w>lounge</w> features live jazz music every Friday night.",
                            "target_lang_translations": {
                                "ru": "В <w>холле</w> отеля каждую пятницу играет живой джаз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cocktail lounge",
                        "hotel lounge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заведение, похожее на бар, где подают напитки и царит расслабленная атмосфера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бар",
                                "target_lang_sense_clarification": "заведение с напитками"
                            },
                            {
                                "target_lang_word": "лаундж-бар",
                                "target_lang_sense_clarification": "бар с мягкой мебелью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3611ebf-6902-32ab-a768-10134b2e872a",
                    "sense_definition": "A large comfortable seat for two or more people; a sofa or couch.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He fell asleep on the <w>lounge</w> after a long day at work.",
                            "target_lang_translations": {
                                "ru": "Он заснул на <w>диване</w> после долгого рабочего дня."
                            }
                        },
                        {
                            "text": "We need to buy a new leather <w>lounge</w> for the apartment.",
                            "target_lang_translations": {
                                "ru": "Нам нужно купить новый кожаный <w>диван</w> в квартиру."
                            }
                        }
                    ],
                    "synonyms": [
                        "parlour",
                        "sitting room"
                    ],
                    "common_phrases": [
                        "chaise lounge",
                        "lounge suite"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Australian, US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой удобный диван или кушетка для двух и более человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "диван",
                                "target_lang_sense_clarification": "мебель"
                            },
                            {
                                "target_lang_word": "кушетка",
                                "target_lang_sense_clarification": "мебель для лежания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f4c0bf88-b3a5-3bec-ba1f-5b79938667b3",
                    "sense_definition": "To relax or spend time lazily; to sit or lie in a comfortable, indolent manner.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "action_relax",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We like to spend our Sundays <w>lounging</w> about at home in our pyjamas.",
                            "target_lang_translations": {
                                "ru": "Мы любим проводить воскресенья, <w>бездельничая</w> дома в пижамах."
                            }
                        },
                        {
                            "text": "The cat was <w>lounging</w> on the windowsill in the warm sun.",
                            "target_lang_translations": {
                                "ru": "Кот <w>нежился</w> на подоконнике на теплом солнышке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lounge around",
                        "lounge about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расслабляться, проводить время в ленивой неге; сидеть или лежать в удобной позе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бездельничать",
                                "target_lang_sense_clarification": "ничего не делать"
                            },
                            {
                                "target_lang_word": "валяться",
                                "target_lang_sense_clarification": "разг. лежать"
                            },
                            {
                                "target_lang_word": "отдыхать",
                                "target_lang_sense_clarification": "расслабляться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1da4237b-014f-3afd-a097-05b3e8647f7e",
                    "sense_definition": "To walk or go in a leisurely manner.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_move",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>lounged</w> down the street without a care in the world.",
                            "target_lang_translations": {
                                "ru": "Он неторопливо <w>брел</w> по улице, не зная забот."
                            }
                        },
                        {
                            "text": "She <w>lounged</w> past the shops, looking at the window displays.",
                            "target_lang_translations": {
                                "ru": "Она неспешно <w>прогуливалась</w> мимо магазинов, разглядывая витрины."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ходить неторопливой, расслабленной походкой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слоняться",
                                "target_lang_sense_clarification": "ходить без дела"
                            },
                            {
                                "target_lang_word": "прогуливаться",
                                "target_lang_sense_clarification": "гулять не спеша"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
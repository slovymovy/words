    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "480eaed9-f3ec-3b0c-84d1-2bc1f7489d3d",
                    "sense_definition": "An organizer, usually made of stiff paper or plastic, used to hold loose papers together.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I keep all my schoolwork in a yellow <w>folder</w>.",
                            "target_lang_translations": {
                                "ru": "Я храню все свои школьные работы в желтой <w>папке</w>."
                            }
                        },
                        {
                            "text": "Please put these signed documents into the file <w>folder</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, положи эти подписанные документы в <w>папку</w> для бумаг."
                            }
                        }
                    ],
                    "common_phrases": [
                        "file folder",
                        "manila folder",
                        "plastic folder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Органайзер, обычно из плотной бумаги или пластика, используемый для хранения и скрепления бумаг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "папка",
                                "target_lang_sense_clarification": "канцелярская"
                            },
                            {
                                "target_lang_word": "скоросшиватель",
                                "target_lang_sense_clarification": "папка с механизмом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "662e035a-5eee-3dc8-9fb3-99c6fbddc8aa",
                    "sense_definition": "A virtual container in a computer's file system where files and other subdirectories are stored.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My essays are in the <w>folder</w> named \"Schoolwork\".",
                            "target_lang_translations": {
                                "ru": "Мои эссе находятся в <w>папке</w> под названием «Школьные работы»."
                            }
                        },
                        {
                            "text": "You should create a new <w>folder</w> on the desktop for your photos.",
                            "target_lang_translations": {
                                "ru": "Тебе следует создать новую <w>папку</w> на рабочем столе для своих фотографий."
                            }
                        }
                    ],
                    "synonyms": [
                        "directory"
                    ],
                    "common_phrases": [
                        "new folder",
                        "open the folder",
                        "subfolder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Виртуальный контейнер в файловой системе компьютера, где хранятся файлы и другие подкаталоги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "папка",
                                "target_lang_sense_clarification": "компьютерная"
                            },
                            {
                                "target_lang_word": "каталог",
                                "target_lang_sense_clarification": "файловая система"
                            },
                            {
                                "target_lang_word": "директория",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21f66947-7071-3d79-a471-3ce71dd5eda2",
                    "sense_definition": "A knife with a blade that folds into the handle; a pocketknife.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He carried a small <w>folder</w> in his pocket during the camping trip.",
                            "target_lang_translations": {
                                "ru": "В походе он носил в кармане маленький <w>складной нож</w>."
                            }
                        },
                        {
                            "text": "The tactical <w>folder</w> has a locking mechanism for safety.",
                            "target_lang_translations": {
                                "ru": "Тактический <w>складной нож</w> имеет запорный механизм для безопасности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tactical folder",
                        "pocket folder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нож с лезвием, которое складывается в рукоятку; перочинный нож."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "складной нож",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc8eef81-43c7-333d-964d-af5bffd5f2e3",
                    "sense_definition": "A machine or person that folds items, such as paper or laundry.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The automatic <w>folder</w> processed thousands of letters in an hour.",
                            "target_lang_translations": {
                                "ru": "Автоматический <w>фальцовщик</w> обрабатывал тысячи писем в час."
                            }
                        },
                        {
                            "text": "She worked as a laundry <w>folder</w> at the large hotel.",
                            "target_lang_translations": {
                                "ru": "Она работала <w>укладчицей</w> белья в большом отеле."
                            }
                        }
                    ],
                    "common_phrases": [
                        "paper folder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Машина или человек, занимающийся складыванием предметов, например бумаги или белья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фальцовщик",
                                "target_lang_sense_clarification": "полиграфия"
                            },
                            {
                                "target_lang_word": "укладчик",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "37c64fdc-eee6-3f14-8a07-70a2dc0f83ca",
                    "sense_definition": "Период сдачи экзаменов в высших и средних специальных учебных заведениях, который обычно проходит два раза в год (зимой и летом).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "exams",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студенты проводят много времени в библиотеке, когда начинается зимняя <w>сессия</w>.",
                            "target_lang_translations": {
                                "en": "Students spend a lot of time in the library when the winter <w>exams</w> begin."
                            }
                        },
                        {
                            "text": "Если ты не сдашь <w>сессию</w> вовремя, тебя могут отчислить из университета.",
                            "target_lang_translations": {
                                "en": "If you don't pass your <w>exams</w> on time, you might be expelled from the university."
                            }
                        }
                    ],
                    "antonyms": [
                        "каникулы"
                    ],
                    "common_phrases": [
                        "зимняя сессия",
                        "летняя сессия",
                        "сдать сессию",
                        "завалить сессию",
                        "во время сессии"
                    ],
                    "target_lang_definitions": {
                        "en": "The period of examinations at universities and colleges, usually held twice a year (in winter and summer)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exams",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "exam period",
                                "target_lang_sense_clarification": "time frame"
                            },
                            {
                                "target_lang_word": "finals",
                                "target_lang_sense_clarification": "end-of-term exams"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c51cb992-80df-3d78-afc8-c1c1be7b6203",
                    "sense_definition": "Период, в течение которого происходят регулярные заседания законодательного органа, суда или международной организации.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "meetings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Очередная <w>сессия</w> парламента будет посвящена обсуждению нового бюджета страны.",
                            "target_lang_translations": {
                                "en": "The next parliamentary <w>session</w> will be dedicated to discussing the country's new budget."
                            }
                        },
                        {
                            "text": "На выездную <w>сессию</w> суда приехали журналисты из разных городов.",
                            "target_lang_translations": {
                                "en": "Journalists from different cities arrived for the visiting court <w>session</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "заседание",
                        "собрание",
                        "съезд"
                    ],
                    "common_phrases": [
                        "выездная сессия",
                        "сессия парламента",
                        "открыть сессию",
                        "пленарная сессия"
                    ],
                    "target_lang_definitions": {
                        "en": "A period during which regular meetings of a legislative body, court, or international organization take place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "session",
                                "target_lang_sense_clarification": "official meeting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e09b8466-d5ce-3d74-b746-76417c2a8aa5",
                    "sense_definition": "Ограниченный во времени период активного взаимодействия пользователя с программой, сайтом или системой.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ваша <w>сессия</w> истекла, поэтому вам нужно снова ввести логин и пароль.",
                            "target_lang_translations": {
                                "en": "Your <w>session</w> has expired, so you need to enter your login and password again."
                            }
                        },
                        {
                            "text": "Администратор сервера был вынужден принудительно завершить зависшую <w>сессию</w>.",
                            "target_lang_translations": {
                                "en": "The server administrator was forced to forcibly terminate the hung <w>session</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "сеанс"
                    ],
                    "common_phrases": [
                        "игровая сессия",
                        "сессия пользователя",
                        "завершить сессию",
                        "истечение сессии"
                    ],
                    "target_lang_definitions": {
                        "en": "A time-limited period of active interaction between a user and a program, website, or system."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "session",
                                "target_lang_sense_clarification": "computing"
                            }
                        ]
                    }
                }
            ]
        }

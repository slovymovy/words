{
    "word_family": [
        "ziel",
        "bezielen",
        "bezieling"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e50eabd-3c5d-3641-be5c-30ba2cb1e582",
                    "sense_definition": "Het innerlijke, emotionele en psychische bestaan van een persoon; de wereld van iemands gedachten en gevoelens.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "inner_life",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De biografie beschrijft het complexe <w>zielenleven</w> van de beroemde schilder.",
                            "target_lang_translations": {
                                "ru": "Биография описывает сложную <w>душевную жизнь</w> знаменитого художника."
                            }
                        },
                        {
                            "text": "Zij heeft een rijk <w>zielenleven</w> en denkt diep na over haar emoties.",
                            "target_lang_translations": {
                                "ru": "У нее богатый <w>внутренний мир</w>, и она глубоко размышляет о своих эмоциях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rijk zielenleven",
                        "gekweld zielenleven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутреннее, эмоциональное и психическое существование человека; мир мыслей и чувств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "душевная жизнь",
                                "target_lang_sense_clarification": "эмоционально-психическое состояние"
                            },
                            {
                                "target_lang_word": "внутренний мир",
                                "target_lang_sense_clarification": "совокупность мыслей и переживаний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15b9acb1-6b8a-3ede-8bee-290960e7efc9",
                    "sense_definition": "Het religieuze of spirituele bestaan van een persoon, met betrekking tot het heil van de ziel.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "inner_life",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor de monnik is het <w>zielenleven</w> belangrijker dan aards bezit.",
                            "target_lang_translations": {
                                "ru": "Для монаха <w>духовная жизнь</w> важнее земных благ."
                            }
                        },
                        {
                            "text": "De priester sprak tijdens de preek over de zorg voor het <w>zielenleven</w>.",
                            "target_lang_translations": {
                                "ru": "Во время проповеди священник говорил о заботе о <w>жизни души</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "spiritualiteit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Религиозное или духовное бытие человека, связанное со спасением души."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "духовная жизнь",
                                "target_lang_sense_clarification": "религиозный аспект"
                            },
                            {
                                "target_lang_word": "жизнь души",
                                "target_lang_sense_clarification": "состояние души в контексте веры"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
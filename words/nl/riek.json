            "senses": [
                {
                    "sense_id": "1e7e6a1c-fff3-34dd-9da8-603e5d0dd78a",
                    "sense_definition": "Een handwerktuig met een lange steel en tanden, gebruikt om bladeren, gras of mest te verplaatsen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer gebruikte een <w>riek</w> om het hooi in de stal te leggen.",
                            "target_lang_translations": {
                                "en": "The farmer used a <w>pitchfork</w> to put the hay in the stable.",
                                "ru": "Фермер использовал <w>вилы</w>, чтобы уложить сено в стойло."
                            }
                        },
                        {
                            "text": "Pas op dat je niemand raakt met de scherpe punten van de <w>riek</w>.",
                            "target_lang_translations": {
                                "en": "Be careful not to hit anyone with the sharp points of the <w>pitchfork</w>.",
                                "ru": "Осторожно, не задень никого острыми зубьями <w>вил</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mestvork"
                    ],
                    "common_phrases": [
                        "met een riek"
                    ],
                    "target_lang_definitions": {
                        "en": "A hand tool with a long handle and tines, used to move leaves, grass, or manure.",
                        "ru": "Ручной инструмент с длинной рукояткой и зубьями, используемый для перемещения листьев, травы или навоза."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pitchfork",
                                "target_lang_sense_clarification": "agricultural tool for lifting hay or manure"
                            },
                            {
                                "target_lang_word": "fork",
                                "target_lang_sense_clarification": "short for pitchfork or garden fork"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вилы",
                                "target_lang_sense_clarification": "сельскохозяйственный инструмент"
                            }
                        ]
                    }
                }
            ]
        },
        {

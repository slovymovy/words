{
    "word_family": [
        "pułapkowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "67fa0f1e-6524-3bb9-ab14-85c492a6c2d9",
                    "sense_definition": "Urządzenie lub mechanizm służący do chwytania zwierząt, na przykład myszy lub ptaków.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy kupić nową <w>pułapkę</w> na myszy, ponieważ stara się zepsuła.",
                            "target_lang_translations": {
                                "ru": "Нам нужно купить новую <w>ловушку</w> для мышей, так как старая сломалась."
                            }
                        },
                        {
                            "text": "Myśliwy zastawił w lesie metalową <w>pułapkę</w> na dzikie zwierzęta.",
                            "target_lang_translations": {
                                "ru": "Охотник поставил в лесу металлический <w>капкан</w> на диких зверей."
                            }
                        }
                    ],
                    "synonyms": [
                        "sidła",
                        "potrzask"
                    ],
                    "common_phrases": [
                        "pułapka na myszy",
                        "wpaść w pułapkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство или механизм, служащий для ловли животных, например мышей или птиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ловушка",
                                "target_lang_sense_clarification": "приспособление для ловли"
                            },
                            {
                                "target_lang_word": "капкан",
                                "target_lang_sense_clarification": "механическое устройство для ловли зверей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39db05fc-e034-3f8e-ad80-1e02a7ba8f70",
                    "sense_definition": "Podstęp lub zasadzka przygotowana w celu schwytania kogoś lub oszukania przeciwnika.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja przygotowała na złodziei skuteczną <w>pułapkę</w> w opuszczonym magazynie.",
                            "target_lang_translations": {
                                "ru": "Полиция подготовила для воров эффективную <w>засаду</w> на заброшенном складе."
                            }
                        },
                        {
                            "text": "Nieroztropny generał wprowadził swoją armię prosto w <w>pułapkę</w> wroga.",
                            "target_lang_translations": {
                                "ru": "Неосмотрительный генерал завёл свою армию прямо во вражескую <w>западню</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zastawić pułapkę",
                        "wciągnąć w pułapkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хитрость или засада, подготовленная с целью поймать кого-либо или обмануть противника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "западня",
                                "target_lang_sense_clarification": "спланированная безвыходная ситуация"
                            },
                            {
                                "target_lang_word": "засада",
                                "target_lang_sense_clarification": "скрытое нападение"
                            },
                            {
                                "target_lang_word": "ловушка",
                                "target_lang_sense_clarification": "уловка, хитрость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da14f88e-5cda-3404-b7bb-cce18211acc9",
                    "sense_definition": "Trudna sytuacja życiowa bez wyjścia, z której nie można się łatwo uwolnić.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wzięcie kolejnej szybkiej pożyczki wpędziło go w poważną finansową <w>pułapkę</w>.",
                            "target_lang_translations": {
                                "ru": "Взятие очередного быстрого займа загнало его в серьёзную финансовую <w>ловушку</w>."
                            }
                        },
                        {
                            "text": "Młoda kobieta czuła się jak w <w>pułapce</w> z powodu toksycznego związku.",
                            "target_lang_translations": {
                                "ru": "Молодая женщина чувствовала себя как в <w>западне</w> из-за токсичных отношений."
                            }
                        }
                    ],
                    "synonyms": [
                        "matnia",
                        "sytuacja bez wyjścia"
                    ],
                    "common_phrases": [
                        "znaleźć się w pułapce",
                        "pułapka kredytowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудная жизненная ситуация, из которой сложно найти выход."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ловушка",
                                "target_lang_sense_clarification": "безвыходное положение"
                            },
                            {
                                "target_lang_word": "западня",
                                "target_lang_sense_clarification": "тяжелая ситуация без выхода"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
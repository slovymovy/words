            "senses": [
                {
                    "sense_id": "28ed8640-9027-390f-a831-346a6e74fc54",
                    "sense_definition": "A return to popularity, success, or fame after a period of obscurity or failure.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "return_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The famous band is planning to make a <w>comeback</w> with a new album next year.",
                            "target_lang_translations": {
                                "ru": "Известная группа планирует <w>возвращение</w> с новым альбомом в следующем году."
                            }
                        },
                        {
                            "text": "Fashion styles from the eighties are making a <w>comeback</w> among teenagers today.",
                            "target_lang_translations": {
                                "ru": "Модные стили восьмидесятых сегодня переживают <w>возвращение</w> популярности среди подростков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "make a comeback",
                        "stage a comeback",
                        "career comeback"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвращение к популярности, успеху или славе после периода безвестности или неудач."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возвращение",
                                "target_lang_sense_clarification": "к славе, успеху"
                            },
                            {
                                "target_lang_word": "камбэк",
                                "target_lang_sense_clarification": "разг. в шоу-бизнесе или моде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "068af6a6-39e3-3e49-8d19-5e21cac8433e",
                    "sense_definition": "An instance of winning or drawing a game or competition after being in a losing position.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "return_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team produced a stunning <w>comeback</w> in the second half to win the match.",
                            "target_lang_translations": {
                                "ru": "Команда совершила потрясающий <w>камбэк</w> во втором тайме и выиграла матч."
                            }
                        },
                        {
                            "text": "Fans cheered wildly as the player completed an incredible <w>comeback</w> from a large point deficit.",
                            "target_lang_translations": {
                                "ru": "Болельщики неистово ликовали, когда игрок совершил невероятный <w>камбэк</w>, отыграв большое отставание."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stunning comeback",
                        "dramatic comeback",
                        "complete a comeback"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация в спорте, когда участник или команда отыгрывается и побеждает после отставания в счете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "камбэк",
                                "target_lang_sense_clarification": "спорт."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e709296-066d-37ec-ae2f-9deb1d0285ce",
                    "sense_definition": "A quick and clever reply to a critical remark.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She always has a witty <w>comeback</w> ready whenever someone insults her.",
                            "target_lang_translations": {
                                "ru": "У неё всегда наготове остроумный <w>ответ</w>, если кто-то её оскорбляет."
                            }
                        },
                        {
                            "text": "He could not think of a good <w>comeback</w> until the argument was already over.",
                            "target_lang_translations": {
                                "ru": "Он не мог придумать достойный <w>ответ</w>, пока спор уже не закончился."
                            }
                        }
                    ],
                    "synonyms": [
                        "retort",
                        "rejoinder"
                    ],
                    "common_phrases": [
                        "witty comeback",
                        "quick comeback",
                        "sharp comeback"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстрый и остроумный ответ на критическое замечание."
                    },
                    "translations": {
                        "ru": [
                            {

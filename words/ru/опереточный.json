    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c185c7fc-e349-36e6-b824-bbe03945d269",
                    "sense_definition": "Относящийся к оперетте — жанру музыкально-театрального искусства; свойственный оперетте.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В городе недавно открылся новый <w>опереточный</w> театр.",
                            "target_lang_translations": {
                                "en": "A new <w>operetta</w> theater recently opened in the city."
                            }
                        },
                        {
                            "text": "Она мечтала стать известной <w>опереточной</w> актрисой и выступать на большой сцене.",
                            "target_lang_translations": {
                                "en": "She dreamed of becoming a famous <w>operetta</w> actress and performing on the big stage."
                            }
                        }
                    ],
                    "common_phrases": [
                        "опереточный театр",
                        "опереточный жанр",
                        "опереточная музыка",
                        "опереточная актриса"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to operetta — a genre of musical-theatrical art; characteristic of operetta."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "operetta",
                                "target_lang_sense_clarification": "relating to the musical genre"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "592dfb54-4e9c-3025-80de-4c93ea9a798d",
                    "sense_definition": "Ненатуральный, наигранный, комичный или несерьёзный; такой, которого нельзя воспринимать всерьёз (напоминающий персонажей или ситуации из оперетты).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его гнев казался каким-то <w>опереточным</w> и совсем не страшным.",
                            "target_lang_translations": {
                                "en": "His anger seemed somewhat <w>farcical</w> and not at all scary."
                            }
                        },
                        {
                            "text": "Генерал с важным видом носил свой <w>опереточный</w> мундир, увешанный блестящими медалями.",
                            "target_lang_translations": {
                                "en": "The general wore his <w>operetta</w> uniform, hung with shiny medals, with an air of self-importance."
                            }
                        }
                    ],
                    "synonyms": [
                        "шутовской"
                    ],
                    "antonyms": [
                        "серьёзный",
                        "настоящий",
                        "натуральный"
                    ],
                    "common_phrases": [
                        "опереточный злодей",
                        "опереточный герой",
                        "опереточный вид",
                        "опереточная важность"
                    ],
                    "target_lang_definitions": {
                        "en": "Unnatural, affected, comical, or not serious; something that cannot be taken seriously (resembling characters or situations from an operetta)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "operetta",
                                "target_lang_sense_clarification": "figurative: ridiculous, exaggerated, or stereotypical"
                            },
                            {
                                "target_lang_word": "farcical",
                                "target_lang_sense_clarification": "absurd or comical"
                            },
                            {
                                "target_lang_word": "theatrical",
                                "target_lang_sense_clarification": "affected or unnatural"
                            }
                        ]
                    }
                }
            ]
        }
    ]

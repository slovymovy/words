                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перина",
                                "target_lang_sense_clarification": "пуховая постель"
                            },
                            {
                                "target_lang_word": "тряпье",
                                "target_lang_sense_clarification": "старые вещи, ветошь"
                            }

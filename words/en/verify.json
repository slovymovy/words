    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fa313063-a342-3b43-962c-97747daee09b",
                    "sense_definition": "To confirm or test the truth or accuracy of something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please check your email to <w>verify</w> your account details.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверьте свою электронную почту, чтобы <w>подтвердить</w> данные вашей учетной записи."
                            }
                        },
                        {
                            "text": "The auditors will <w>verify</w> the information provided in the financial report.",
                            "target_lang_translations": {
                                "ru": "Аудиторы <w>проверят</w> информацию, представленную в финансовом отчете."
                            }
                        }
                    ],
                    "synonyms": [
                        "confirm",
                        "check",
                        "validate",
                        "authenticate",
                        "corroborate"
                    ],
                    "antonyms": [
                        "disprove",
                        "invalidate",
                        "refute"
                    ],
                    "common_phrases": [
                        "verify the identity",
                        "verify the information",
                        "verify the results",
                        "verify the authenticity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подтверждать или проверять истинность или точность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверять",
                                "target_lang_sense_clarification": "устанавливать правильность или точность"
                            },
                            {
                                "target_lang_word": "подтверждать",
                                "target_lang_sense_clarification": "свидетельствовать о достоверности"
                            },
                            {
                                "target_lang_word": "доказывать",
                                "target_lang_sense_clarification": "обосновывать истинность фактами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c496e7b2-2a84-31ab-87ab-adee5d5d3fb1",
                    "sense_definition": "To affirm something formally, under oath.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The defendant had to <w>verify</w> his statement before the court proceedings could continue.",
                            "target_lang_translations": {
                                "ru": "Подсудимый должен был <w>подтвердить</w> свои показания, прежде чем судебное разбирательство могло продолжиться."
                            }
                        },
                        {
                            "text": "She refused to <w>verify</w> the document under oath without her lawyer present.",
                            "target_lang_translations": {
                                "ru": "Она отказалась <w>удостоверять</w> документ под присягой без присутствия своего адвоката."
                            }
                        }
                    ],
                    "synonyms": [
                        "attest",
                        "swear"
                    ],
                    "common_phrases": [
                        "verify under oath",
                        "verify a statement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально подтверждать что-либо, часто под присягой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удостоверять",
                                "target_lang_sense_clarification": "свидетельствовать подлинность"
                            },
                            {
                                "target_lang_word": "заверять",
                                "target_lang_sense_clarification": "официально подтверждать документ или подпись"
                            }
                        ]
                    }
                }
            ]
        }
    ]

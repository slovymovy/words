                {
                    "sense_id": "ce5dab9b-971d-33b4-9110-4bca99bae91a",
                    "sense_definition": "Zabrać kogoś własnym środkiem transportu i zawieźć go do określonego miejsca, często przy okazji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy mógłbyś mnie <w>podwieźć</w> do centrum miasta?",
                            "target_lang_translations": {
                                "ru": "Ты не мог бы <w>подвезти</w> меня до центра города?"
                            }
                        },
                        {
                            "text": "Sąsiad <w>podwiózł</w> nas rano na stację kolejową, żebyśmy zdążyli na pociąg.",
                            "target_lang_translations": {
                                "ru": "Сосед утром <w>подвез</w> нас на вокзал, чтобы мы успели на поезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "podrzucić",
                        "podrzucić"
                    ],
                    "common_phrases": [
                        "podwieźć do domu",
                        "podwieźć do pracy",
                        "podwieźć na stację",
                        "podwieźć samochodem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взять кого-то на своем транспортном средстве и довезти до определенного места, часто по пути."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвезти",
                                "target_lang_sense_clarification": "довезти кого-то"
                            },
                            {
                                "target_lang_word": "подкинуть",
                                "target_lang_sense_clarification": "разг. подвезти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f58e56ca-3864-3773-b540-33314f8af4de",
                    "sense_definition": "Dostarczyć coś, zazwyczaj ciężkiego lub w większej ilości, w określone miejsce za pomocą pojazdu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca musi <w>podwieźć</w> cegły na plac budowy przed południem.",
                            "target_lang_translations": {
                                "ru": "Водитель должен <w>подвезти</w> кирпичи на стройплощадку до полудня."
                            }
                        },
                        {
                            "text": "Proszę <w>podwieźć</w> te ciężkie paczki pod same drzwi magazynu.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>подвезите</w> эти тяжелые посылки прямо к дверям склада."
                            }
                        }
                    ],
                    "synonyms": [
                        "podrzucić"
                    ],
                    "common_phrases": [
                        "podwieźć towar",
                        "podwieźć materiały",
                        "podwieźć pod drzwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доставить что-то, обычно тяжелое или в большом количестве, в определенное место с помощью транспортного средства."
                    },
                    "translations": {

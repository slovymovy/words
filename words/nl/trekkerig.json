            "senses": [
                {
                    "sense_id": "3f53d1d9-a4c0-31bc-837f-5392ba1fcd59",
                    "sense_definition": "gekenmerkt door korte, onregelmatige bewegingen of rukjes; niet vloeiend",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vlucht van de vleermuis is snel en <w>trekkerig</w> terwijl hij jaagt.",
                            "target_lang_translations": {
                                "en": "The bat's flight is fast and <w>jerky</w> while it hunts.",
                                "ru": "Полет летучей мыши быстрый и <w>дерганый</w> во время охоты."
                            }
                        },
                        {
                            "text": "De oude motor liep niet meer soepel en voelde wat <w>trekkerig</w> aan tijdens het rijden.",
                            "target_lang_translations": {
                                "en": "The old engine no longer ran smoothly and felt a bit <w>jerky</w> while driving.",
                                "ru": "Старый двигатель работал неровно, и во время езды ход казался немного <w>прерывистым</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "vloeiend",
                        "gelijkmatig",
                        "glijdend"
                    ],
                    "common_phrases": [
                        "een trekkerige beweging",
                        "trekkerig lopen"
                    ],
                    "target_lang_definitions": {
                        "en": "Characterized by short, irregular movements or jerks; not smooth",
                        "ru": "характеризующийся короткими, нерегулярными движениями или рывками; неплавный"

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "48733741-96da-3c8d-b7fa-b174f92df62b",
                    "sense_definition": "Dzienna placówka opiekuńczo-wychowawcza dla dzieci w wieku od trzech lat do rozpoczęcia nauki w szkole.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza córka pójdzie w tym roku do <w>przedszkola</w>.",
                            "target_lang_translations": {
                                "ru": "Наша дочь в этом году пойдет в <w>детский сад</w>."
                            }
                        },
                        {
                            "text": "Musimy znaleźć dobre <w>przedszkole</w> blisko naszego miejsca pracy.",
                            "target_lang_translations": {
                                "ru": "Нам нужно найти хороший <w>детский сад</w> недалеко от нашей работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "ochronka"
                    ],
                    "common_phrases": [
                        "państwowe przedszkole",
                        "prywatne przedszkole",
                        "chodzić do przedszkola",
                        "zapisać do przedszkola"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждение для дневного пребывания и воспитания детей дошкольного возраста (обычно с 3 до 7 лет)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детский сад",
                                "target_lang_sense_clarification": "учреждение"
                            },
                            {
                                "target_lang_word": "садик",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b060942-4ee7-3cee-8a19-4a4b5ef21f21",
                    "sense_definition": "Czas lub etap edukacji spędzony w placówce dla małych dzieci.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z moim najlepszym przyjacielem znamy się od <w>przedszkola</w>.",
                            "target_lang_translations": {
                                "ru": "Мы с моим лучшим другом знакомы с <w>детского сада</w>."
                            }
                        },
                        {
                            "text": "To była moja ulubiona zabawa jeszcze w <w>przedszkolu</w>.",
                            "target_lang_translations": {
                                "ru": "Это была моя любимая игра еще в <w>детском саду</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "od przedszkola",
                        "w przedszkolu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время или этап жизни, проведенный в дошкольном учреждении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детский сад",
                                "target_lang_sense_clarification": "период времени"

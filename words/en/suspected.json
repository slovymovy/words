            "senses": [
                {
                    "sense_id": "2c85b293-fba3-352b-86f4-5d69e0ad0b3b",
                    "sense_definition": "Believed to be guilty, false, or the cause of something bad, without certain proof.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>suspected</w> thief was caught trying to leave the shop with stolen goods.",
                            "target_lang_translations": {
                                "ru": "<w>Подозреваемый</w> вор был пойман при попытке выйти из магазина с украденными товарами."
                            }
                        },
                        {
                            "text": "Doctors are monitoring the patient closely for a <w>suspected</w> infection.",
                            "target_lang_translations": {
                                "ru": "Врачи внимательно наблюдают за пациентом из-за <w>предполагаемой</w> инфекции."
                            }
                        }
                    ],
                    "synonyms": [
                        "alleged",
                        "presumed",
                        "assumed"
                    ],
                    "antonyms": [
                        "confirmed",
                        "proven",
                        "known"
                    ],
                    "common_phrases": [
                        "suspected criminal",
                        "suspected case",
                        "suspected terrorist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считающийся виновным или являющийся предполагаемой причиной чего-либо плохого без явных доказательств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подозреваемый",
                                "target_lang_sense_clarification": "о человеке, которого подозревают в преступлении"
                            },
                            {
                                "target_lang_word": "предполагаемый",
                                "target_lang_sense_clarification": "о причине или событии"
                            }
                        ]
                    }
                }
            ]

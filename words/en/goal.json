                },
                {
                    "sense_id": "7d1bfff0-bd5b-3ed0-9b5e-95a21ed6359f",
                    "sense_definition": "In many sports, the area or structure into which players attempt to put a ball or object to score.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sports_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The goalkeeper stood ready in the <w>goal</w>.",
                            "target_lang_translations": {
                                "ru": "Вратарь стоял наготове в <w>воротах</w>."
                            }
                        },
                        {
                            "text": "The ball rolled slowly past the defender and into the <w>goal</w>.",
                            "target_lang_translations": {
                                "ru": "Мяч медленно прокатился мимо защитника и попал в <w>ворота</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in goal",
                        "open goal"
                    ],
                    "target_lang_definitions": {
                        "ru": "В спорте: конструкция или зона, куда игроки пытаются забить мяч или предмет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ворота",
                                "target_lang_sense_clarification": "спортивная конструкция"

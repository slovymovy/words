    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7f8e3166-b337-378b-a841-8152f1de3dc0",
                    "sense_definition": "Having relatively little weight; not heavy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need a <w>lightweight</w> jacket for the cool summer evenings.",
                            "target_lang_translations": {
                                "ru": "Мне нужна <w>лёгкая</w> куртка для прохладных летних вечеров."
                            }
                        },
                        {
                            "text": "The laptop is <w>lightweight</w> and easy to carry in a backpack.",
                            "target_lang_translations": {
                                "ru": "Этот ноутбук <w>лёгкий</w>, и его удобно носить в рюкзаке."
                            }
                        }
                    ],
                    "synonyms": [
                        "light",
                        "portable"
                    ],
                    "antonyms": [
                        "heavy",
                        "heavyweight"
                    ],
                    "common_phrases": [
                        "lightweight material",
                        "lightweight design",
                        "lightweight fabric"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий сравнительно небольшой вес; не тяжёлый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лёгкий",
                                "target_lang_sense_clarification": "о физическом весе"
                            },
                            {
                                "target_lang_word": "облегчённый",
                                "target_lang_sense_clarification": "сделанный из лёгкого материала"
                            }

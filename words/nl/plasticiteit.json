            "pos": "noun",
            "senses": [
                {
                    "sense_id": "66c9d99d-c6a2-350f-ab24-1ccc7c405efa",
                    "sense_definition": "De eigenschap van een materiaal om van vorm te veranderen zonder te breken; de mate waarin iets kneedbaar of vervormbaar is.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klei heeft een grote <w>plasticiteit</w> zolang het vochtig is.",
                            "target_lang_translations": {
                                "en": "Clay has great <w>plasticity</w> as long as it is moist.",
                                "ru": "Глина обладает высокой <w>пластичностью</w>, пока она влажная."
                            }
                        },
                        {
                            "text": "De <w>plasticiteit</w> van de hersenen stelt ons in staat om nieuwe dingen te leren.",
                            "target_lang_translations": {
                                "en": "The <w>plasticity</w> of the brain enables us to learn new things.",
                                "ru": "<w>Пластичность</w> мозга позволяет нам учиться новому."
                            }
                        }
                    ],
                    "synonyms": [
                        "kneedbaarheid",
                        "vervormbaarheid"
                    ],
                    "antonyms": [
                        "rigiditeit",
                        "starheid"
                    ],
                    "common_phrases": [
                        "grote plasticiteit",
                        "neurale plasticiteit"
                    ],
                    "target_lang_definitions": {
                        "en": "The property of a material to change shape without breaking; the degree to which something is malleable or deformable.",
                        "ru": "Свойство материала изменять форму без разрушения; степень податливости или деформируемости."
                    },
                    "translations": {

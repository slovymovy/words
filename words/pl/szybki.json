                {
                    "sense_id": "684d6694-bb71-30e3-8fdb-4c3204005395",
                    "sense_definition": "Poruszający się z dużą prędkością lub zdolny do osiągania dużej prędkości.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <szybki> samochód wygrał wyścig bez żadnych problemów technicznych.",
                            "target_lang_translations": {
                                "ru": "Этот <быстрый> автомобиль выиграл гонку без каких-либо технических проблем."
                            }
                        },
                        {
                            "text": "Gepard to niezwykle <szybki> drapieżnik, który poluje na sawannie.",
                            "target_lang_translations": {
                                "ru": "Гепард — это невероятно <быстрый> хищник, который охотится в саванне."
                            }
                        }
                    ],
                    "synonyms": [
                        "prędki",
                        "wartki"
                    ],
                    "antonyms": [
                        "wolny",
                        "powolny"
                    ],
                    "common_phrases": [
                        "szybki samochód",
                        "szybki bieg",
                        "szybkie tempo",
                        "szybki pociąg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Движущийся с большой скоростью или способный развивать большую скорость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быстрый",
                                "target_lang_sense_clarification": "о скорости движения"
                            },
                            {
                                "target_lang_word": "скоростной",
                                "target_lang_sense_clarification": "развивающий высокую скорость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85718f77-be1e-3276-968f-2e67a1362ee4",
                    "sense_definition": "Trwający krótko lub wykonany w krótkim czasie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę wziąć <szybki> prysznic, zanim wyjdziemy na wieczorne spotkanie.",
                            "target_lang_translations": {
                                "ru": "Мне нужно принять <быстрый> душ, прежде чем мы выйдем на вечернюю встречу."
                            }
                        },
                        {
                            "text": "To była bardzo <szybka> decyzja, której teraz trochę żałuję.",
                            "target_lang_translations": {
                                "ru": "Это было очень <быстрое> решение, о котором я теперь немного жалею."
                            }
                        }
                    ],
                    "synonyms": [
                        "prędki",
                        "ekspresowy",
                        "prędki",
                        "chyży"
                    ],
                    "antonyms": [
                        "powolny",
                        "wolny",
                        "wolny"
                    ],
                    "common_phrases": [

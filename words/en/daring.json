    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5fd365f1-9201-33c5-b340-59e884185431",
                    "sense_definition": "Willing to do dangerous or difficult things; adventurous and bold.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "boldness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The firefighters made a <w>daring</w> rescue to save the family from the burning building.",
                            "target_lang_translations": {
                                "ru": "Пожарные совершили <w>отважную</w> спасательную операцию, чтобы вытащить семью из горящего здания."
                            }
                        },
                        {
                            "text": "It was a <w>daring</w> plan, but it was the only way to succeed.",
                            "target_lang_translations": {
                                "ru": "Это был <w>дерзкий</w> план, но это был единственный способ преуспеть."
                            }
                        }
                    ],
                    "synonyms": [
                        "adventurous",
                        "bold",
                        "courageous",
                        "fearless",
                        "intrepid",
                        "audacious",
                        "venturesome"
                    ],
                    "antonyms": [
                        "cautious",
                        "timid",
                        "cowardly"
                    ],
                    "common_phrases": [
                        "daring escape",
                        "daring rescue",
                        "daring plan",
                        "daring feat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовый совершать опасные или трудные поступки; смелый и рискованный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смелый",
                                "target_lang_sense_clarification": "не боящийся опасности"
                            },
                            {
                                "target_lang_word": "отважный",
                                "target_lang_sense_clarification": "полный отваги, очень храбрый"
                            },
                            {
                                "target_lang_word": "дерзкий",
                                "target_lang_sense_clarification": "рискованный, смелый до безрассудства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbd4f7be-6470-3cc2-9bd4-fd5149ca2218",
                    "sense_definition": "Involving something shocking or provocative, especially regarding clothing or social conventions.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "provocative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a <w>daring</w> dress that attracted everyone's attention at the party.",
                            "target_lang_translations": {
                                "ru": "Она надела <w>смелое</w> платье, которое привлекло всеобщее внимание на вечеринке."
                            }
                        },
                        {
                            "text": "The artist is known for his <w>daring</w> and controversial style.",
                            "target_lang_translations": {
                                "ru": "Художник известен своим <w>дерзким</w> и противоречивым стилем."
                            }
                        }
                    ],
                    "synonyms": [
                        "bold",
                        "audacious"
                    ],
                    "antonyms": [
                        "modest",
                        "conservative"
                    ],
                    "common_phrases": [
                        "daring dress",
                        "daring outfit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий в себе что-то шокирующее или провокационное, особенно в одежде или социальных нормах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смелый",
                                "target_lang_sense_clarification": "оригинальный, выходящий за рамки обычного"
                            },
                            {
                                "target_lang_word": "вызывающий",
                                "target_lang_sense_clarification": "провокационный, привлекающий внимание"
                            },
                            {
                                "target_lang_word": "дерзкий",
                                "target_lang_sense_clarification": "бросающий вызов общественному вкусу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3ca712fb-4fde-3f74-b28a-2aef614bb709",

            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "7f837721-e64c-3936-bd43-c647dc8abebb",
                    "sense_definition": "A non-standard, often dialectal or informal pronunciation or spelling of <w>yes</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Yus</w>, I reckon that is exactly what happened.",
                            "target_lang_translations": {
                                "ru": "<w>Ага</w>, я полагаю, именно это и произошло."
                            }
                        },
                        {
                            "text": "Are you going to the market? <w>Yus</w>, I am leaving now.",
                            "target_lang_translations": {
                                "ru": "Ты идешь на рынок? <w>Ага</w>, я уже выхожу."
                            }
                        }
                    ],
                    "synonyms": [
                        "yes",
                        "yeah",
                        "aye"
                    ],
                    "antonyms": [
                        "no",
                        "nah"
                    ],
                    "common_phrases": [
                        "oh yus",
                        "yus mate"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нестандартное, часто диалектное или просторечное произношение или написание слова \"yes\" (да)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ага",
                                "target_lang_sense_clarification": "разговорное, утверждение"
                            },
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "значение слова"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "14ca30a7-9362-356e-9538-706860e6fdda",
                    "sense_definition": "Either of two letters, little yus (Ѧ) or big yus (Ѫ), used in the early Cyrillic alphabet to represent nasal vowel sounds.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "cyrillic_letters",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scholar identified a big <w>yus</w> in the Old Church Slavonic manuscript.",
                            "target_lang_translations": {
                                "ru": "Ученый обнаружил большой <w>юс</w> в старославянской рукописи."
                            }
                        },
                        {
                            "text": "Most modern Slavic languages have lost the sounds originally represented by the <w>yus</w>.",
                            "target_lang_translations": {
                                "ru": "Большинство современных славянских языков утратили звуки, которые изначально обозначал <w>юс</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "big yus",
                        "little yus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Название одной из двух букв (юс малый или юс большой) в ранней кириллице, обозначавших носовые гласные звуки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юс",
                                "target_lang_sense_clarification": "название буквы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cae219dd-9d12-3628-921c-3507dde33882",
                    "sense_definition": "The plural form of the noun <w>yu</w> (referring to the Cyrillic letter Ю).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "cyrillic_letters",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The student practiced writing his <w>yus</w> until the curves were perfect.",
                            "target_lang_translations": {
                                "ru": "Студент тренировался писать свои <w>буквы ю</w>, пока изгибы не стали идеальными."
                            }
                        },
                        {
                            "text": "There are three <w>yus</w> in this short paragraph.",
                            "target_lang_translations": {
                                "ru": "В этом коротком абзаце три <w>буквы ю</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число существительного \"yu\" (обозначающего кириллическую букву Ю)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "буквы ю",
                                "target_lang_sense_clarification": "множественное число"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
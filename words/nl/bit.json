            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1917f0de-67ec-3c12-867b-d72b083d976a",
                    "sense_definition": "de kleinste eenheid van informatie in een computer, die de waarde 0 of 1 kan hebben",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een byte bestaat uit een reeks van acht <w>bits</w>.",
                            "target_lang_translations": {
                                "en": "A byte consists of a sequence of eight <w>bits</w>.",
                                "ru": "Байт состоит из последовательности восьми <w>битов</w>."
                            }
                        },
                        {
                            "text": "De snelheid van de internetverbinding wordt gemeten in <w>bits</w> per seconde.",
                            "target_lang_translations": {
                                "en": "The speed of the internet connection is measured in <w>bits</w> per second.",
                                "ru": "Скорость интернет-соединения измеряется в <w>битах</w> в секунду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bits en bytes"
                    ],
                    "target_lang_definitions": {
                        "en": "the smallest unit of information in a computer, which can have the value 0 or 1",
                        "ru": "наименьшая единица информации в компьютере, которая может принимать значение 0 или 1"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bit",
                                "target_lang_sense_clarification": "computing unit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бит",
                                "target_lang_sense_clarification": "единица информации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d86c42bd-02d8-31c9-a7a2-5d93964b310b",
                    "sense_definition": "een beschermhoesje voor de tanden dat tijdens het sporten wordt gedragen om letsel te voorkomen",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_gear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hockeyspeler deed zijn <w>bitje</w> in voordat de wedstrijd begon.",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d86775e8-af85-3f97-979b-4aef2efa32be",
                    "sense_definition": "Wyrazić żal z powodu wyrządzonej komuś przykrości, popełnionego błędu lub nietaktu i poprosić o wybaczenie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym cię serdecznie <przeprosić> za moje wczorajsze zachowanie.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы искренне <w>извиниться</w> перед тобой за мое вчерашнее поведение."
                            }
                        },
                        {
                            "text": "Uczeń <przeprosił> nauczycielkę za spóźnienie na lekcję.",
                            "target_lang_translations": {
                                "ru": "Ученик <w>извинился</w> перед учительницей за опоздание на урок."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyrazić skruchę"
                    ],
                    "antonyms": [
                        "obrazić"
                    ],
                    "common_phrases": [
                        "przeprosić za spóźnienie",
                        "przeprosić za kłopot",
                        "chcieć przeprosić",
                        "oficjalnie przeprosić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выразить сожаление по поводу причиненной неприятности, ошибки или бестактности и попросить прощения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извиниться",
                                "target_lang_sense_clarification": "попросить прощения за поступок"
                            },
                            {
                                "target_lang_word": "попросить прощения",
                                "target_lang_sense_clarification": "выразить сожаление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dff8851-b851-3f4b-b79a-fbf3ce0d9b62",
                    "sense_definition": "Pogodzić się z kimś po kłótni, wybaczając sobie wzajemnie urazy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długiej kłótni sąsiedzi w końcu się <przeprosili>.",
                            "target_lang_translations": {
                                "ru": "После долгой ссоры соседи наконец <w>помирились</w>."
                            }
                        },
                        {
                            "text": "Musisz się z nią <przeprosić>, jeśli chcesz, żeby w domu panował spokój.",
                            "target_lang_translations": {
                                "ru": "Ты должен с ней <w>помириться</w>, если хочешь, чтобы в доме царил покой."
                            }
                        }
                    ],
                    "synonyms": [
                        "pogodzić się"
                    ],
                    "antonyms": [
                        "pokłócić się"
                    ],
                    "common_phrases": [
                        "przeprosić się z kimś",
                        "przeprosić się nawzajem"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Помириться с кем-то после ссоры, простив друг другу обиды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помириться",
                                "target_lang_sense_clarification": "восстановить отношения после ссоры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c8e6cd5-033a-3479-9776-ae46f73e76c4",
                    "sense_definition": "Zacząć ponownie używać czegoś lub robić coś, czego się wcześniej zaniechało lub unikało.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy samochód się zepsuł, musiałem się <przeprosić> z rowerem.",
                            "target_lang_translations": {
                                "ru": "Когда машина сломалась, мне пришлось <w>вернуться</w> к велосипеду."
                            }
                        },
                        {
                            "text": "Zimą wielu kierowców musi się <przeprosić> ze skrobaczką do szyb.",
                            "target_lang_translations": {
                                "ru": "Зимой многим водителям приходится <w>вернуться</w> к скребку для стекол."
                            }

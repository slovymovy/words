                            },
                            {
                                "target_lang_word": "рассеивать",
                                "target_lang_sense_clarification": "разбрасывать в разные стороны"
                            },
                            {
                                "target_lang_word": "разносить",
                                "target_lang_sense_clarification": "перемещать ветром или передавать новости"
                            }
                        ]

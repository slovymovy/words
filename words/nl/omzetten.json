                    "sense_id": "43241bcf-1d21-329e-902e-95c54d471167",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>omzet</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>omzetten</w> van de verschillende filialen werden met elkaar vergeleken.",
                            "target_lang_translations": {
                                "ru": "<w>Обороты</w> различных филиалов сравнивались друг с другом.",
                                "en": "The <w>turnovers</w> of the various branches were compared with each other."
                            }
                        },
                        {
                            "text": "Na de crisis stegen de <w>omzetten</w> in de horeca weer.",
                            "target_lang_translations": {
                                "ru": "После кризиса <w>обороты</w> в сфере общепита снова выросли.",
                                "en": "After the crisis, <w>revenues</w> in the hospitality industry rose again."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dalende omzetten",
                        "stijgende omzetten"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "meervoud"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от существительного 'omzet' (оборот).",
                        "en": "Plural of the noun 'omzet' (turnover/revenue)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обороты",
                                "target_lang_sense_clarification": "объемы продаж (мн. ч.)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "turnovers",
                                "target_lang_sense_clarification": "total sales figures"
                            },
                            {
                                "target_lang_word": "revenues",
                                "target_lang_sense_clarification": "income from business"
                            }
                        ]
                    }
                }
            ]
        }
    ]

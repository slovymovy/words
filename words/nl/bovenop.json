            "senses": [
                {
                    "sense_id": "a9cbc235-aef5-3faf-b50d-18742b3e5e94",
                    "sense_definition": "Op de bovenkant van iets.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kat sprong <w>bovenop</w> de hoge kast.",
                            "target_lang_translations": {
                                "en": "The cat jumped <w>on top of</w> the high cupboard.",
                                "ru": "Кошка запрыгнула <w>на</w> высокий шкаф."
                            }
                        },
                        {
                            "text": "Leg de sleutels maar <w>bovenop</w> de stapel papieren.",
                            "target_lang_translations": {
                                "en": "Just put the keys <w>on top of</w> the stack of papers.",
                                "ru": "Положи ключи <w>поверх</w> стопки бумаг."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bovenop de kast",
                        "bovenop de stapel"
                    ],
                    "target_lang_definitions": {
                        "en": "On the upper surface of something.",
                        "ru": "На верхней поверхности чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on top of",
                                "target_lang_sense_clarification": "location"
                            },
                            {
                                "target_lang_word": "atop",
                                "target_lang_sense_clarification": "formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "поверх",
                                "target_lang_sense_clarification": "на поверхности другого предмета"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "указывает на нахождение на поверхности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4f4dd8eb-a135-331c-8c97-27616d5c6420",
                    "sense_definition": "Helemaal aan de bovenkant; op het hoogste punt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het boek lag <w>bovenop</w> en was makkelijk te vinden.",
                            "target_lang_translations": {
                                "en": "The book lay <w>on top</w> and was easy to find.",
                                "ru": "Книга лежала <w>сверху</w>, и её было легко найти."
                            }
                        },
                        {
                            "text": "Ik klom naar het dak en ging <w>bovenop</w> zitten om van het uitzicht te genieten.",
                            "target_lang_translations": {
                                "en": "I climbed to the roof and sat <w>on top</w> to enjoy the view.",
                                "ru": "Я залез на крышу и сел <w>наверху</w>, чтобы насладиться видом."
                            }
                        }
                    ],
                    "antonyms": [
                        "onderop"
                    ],
                    "common_phrases": [
                        "er bovenop",
                        "bovenop liggen"
                    ],
                    "target_lang_definitions": {
                        "en": "At the very top; at the highest point.",
                        "ru": "На самом верху; в верхней части."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on top",
                                "target_lang_sense_clarification": "position"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сверху",
                                "target_lang_sense_clarification": "на поверхности"
                            },
                            {
                                "target_lang_word": "наверху",
                                "target_lang_sense_clarification": "в верхней части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34cee4e0-d9ca-3aeb-9478-1b2bf8cc2945",
                    "sense_definition": "Als een extra toevoeging; bovendien.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat probleem kwam er nog eens <w>bovenop</w>.",
                            "target_lang_translations": {
                                "en": "That problem came <w>on top of</w> that.",
                                "ru": "Эта проблема возникла <w>вдобавок</w> ко всему."
                            }
                        },
                        {
                            "text": "<w>Bovenop</w> de directe gevolgen van de ziekte had hij ook financiële zorgen.",
                            "target_lang_translations": {
                                "en": "<w>On top of</w> the direct consequences of the illness, he also had financial worries.",
                                "ru": "<w>Вдобавок к</w> прямым последствиям болезни, у него были и финансовые заботы."
                            }
                        }
                    ],
                    "synonyms": [
                        "bovendien",
                        "daarnaast"
                    ],
                    "common_phrases": [
                        "er bovenop komen"
                    ],
                    "target_lang_definitions": {
                        "en": "In addition to something; moreover.",
                        "ru": "В качестве дополнения; сверх того."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on top of",
                                "target_lang_sense_clarification": "addition"
                            },
                            {
                                "target_lang_word": "in addition to",
                                "target_lang_sense_clarification": "formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вдобавок",
                                "target_lang_sense_clarification": "в дополнение"
                            },
                            {
                                "target_lang_word": "сверх",
                                "target_lang_sense_clarification": "помимо, в дополнение к"
                            }
                        ]
                    }
                }
            ]
        }
    ]

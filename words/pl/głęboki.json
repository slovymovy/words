    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1995b48c-4253-36c5-9f98-034388e6d705",
                    "sense_definition": "Mający dużą odległość od powierzchni do dna lub od brzegu do wnętrza; przeciwieństwo płytkiego.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_dimension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta rzeka jest zbyt <w>głęboka</w>, żebyśmy mogli przejść na drugą stronę.",
                            "target_lang_translations": {
                                "ru": "Эта река слишком <w>глубокая</w>, чтобы мы могли перейти на другую сторону."
                            }
                        },
                        {
                            "text": "Dziecko nie powinno bawić się samo w tak <w>głębokiej</w> wodzie.",
                            "target_lang_translations": {
                                "ru": "Ребёнок не должен играть один в такой <w>глубокой</w> воде."
                            }
                        }
                    ],
                    "antonyms": [
                        "płytki"
                    ],
                    "common_phrases": [
                        "głęboka woda",
                        "głęboka rzeka",
                        "głęboki rów",
                        "głęboka rana",
                        "głęboki talerz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий большое расстояние от поверхности до дна или от края внутрь; противоположность мелкому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глубокий",
                                "target_lang_sense_clarification": "о пространстве, воде, ранах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b36fc96-4124-3aed-bd17-225fb338bfa7",
                    "sense_definition": "Bardzo silny, intensywny lub osiągający wysoki stopień nasilenia (o uczuciach, stanach lub kolorach).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensity_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długim spacerze dziewczynka zapadła w <w>głęboki</w> sen.",
                            "target_lang_translations": {
                                "ru": "После долгой прогулки девочка погрузилась в <w>глубокий</w> сон."
                            }
                        },
                        {
                            "text": "Malarz użył farby o barwie <w>głębokiej</w> czerwieni, aby podkreślić emocje.",
                            "target_lang_translations": {
                                "ru": "Художник использовал краску <w>глубокого</w> красного цвета, чтобы подчеркнуть эмоции."
                            }
                        },
                        {
                            "text": "Czuł <w>głęboki</w> żal po stracie swojego najlepszego przyjaciela.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал <w>глубокую</w> скорбь после потери своего лучшего друга."
                            }
                        }
                    ],
                    "synonyms": [
                        "mocny",
                        "silny",
                        "intensywny"
                    ],
                    "common_phrases": [
                        "głęboki sen",
                        "głęboki oddech",
                        "głęboki żal",
                        "głęboki smutek",
                        "głęboka czerwień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень сильный, интенсивный или достигший высокой степени (о чувствах, состояниях или цветах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глубокий",
                                "target_lang_sense_clarification": "о чувствах, сне, цвете"
                            },
                            {
                                "target_lang_word": "сильный",
                                "target_lang_sense_clarification": "об эмоциях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "824821f4-c163-387c-bb2b-0c703b003074",
                    "sense_definition": "O dźwięku lub głosie: mający niski ton, brzmiący nisko.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna miał bardzo <w>głęboki</w> głos, który od razu przyciągał uwagę.",
                            "target_lang_translations": {
                                "ru": "У мужчины был очень <w>глубокий</w> голос, который сразу привлекал внимание."
                            }
                        },
                        {
                            "text": "Z oddali dobiegał <w>głęboki</w> dźwięk dzwonu kościelnego.",
                            "target_lang_translations": {
                                "ru": "Издалека доносился <w>глубокий</w> звук церковного колокола."
                            }
                        }
                    ],
                    "synonyms": [
                        "mocny",
                        "silny"
                    ],
                    "antonyms": [
                        "płytki",
                        "płytki"
                    ],
                    "common_phrases": [
                        "głęboki głos",
                        "głęboki ton",
                        "głębokie brzmienie"
                    ],
                    "target_lang_definitions": {
                        "ru": "О звуке или голосе: низкий по тону, густой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глубокий",
                                "target_lang_sense_clarification": "о голосе, звуке"
                            },
                            {
                                "target_lang_word": "низкий",
                                "target_lang_sense_clarification": "о тоне голоса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c76685fd-4b1c-3f14-8752-689df434d202",
                    "sense_definition": "Trudny do zrozumienia, mądry, dotyczący istoty rzeczy; niepowierzchowny.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intellectual_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest bardzo <w>głęboka</w> myśl, nad którą muszę się dłużej zastanowić.",
                            "target_lang_translations": {
                                "ru": "Это очень <w>глубокая</w> мысль, над которой мне нужно подольше подумать."
                            }
                        },
                        {
                            "text": "Książka zawiera <w>głęboką</w> analizę problemów społecznych w naszym kraju.",
                            "target_lang_translations": {
                                "ru": "Книга содержит <w>глубокий</w> анализ социальных проблем в нашей стране."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyraźny",
                        "intensywny"
                    ],
                    "antonyms": [
                        "płytki",
                        "banalny",
                        "prosty"
                    ],
                    "common_phrases": [
                        "głęboka myśl",
                        "głęboki sens",
                        "głęboka wiedza",
                        "głęboka analiza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сложный для понимания, мудрый, касающийся сути вещей; не поверхностный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глубокий",
                                "target_lang_sense_clarification": "о мыслях, знаниях, анализе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0154d8e3-2355-304f-9e69-d429eeca8021",
                    "sense_definition": "Znajdujący się w zaawansowanej fazie czasu; późny.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wróciliśmy do domu, gdy była już <w>głęboka</w> noc.",
                            "target_lang_translations": {
                                "ru": "Мы вернулись домой, когда была уже <w>глубокая</w> ночь."
                            }
                        },
                        {
                            "text": "Nawet w <w>głębokiej</w> starości dziadek zachował doskonałą pamięć.",
                            "target_lang_translations": {
                                "ru": "Даже в <w>глубокой</w> старости дедушка сохранил отличную память."
                            }
                        }
                    ],
                    "common_phrases": [
                        "głęboka noc",
                        "głęboka zima",
                        "głębokie średniowiecze",
                        "głęboka starość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в поздней стадии времени; поздний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глубокий",
                                "target_lang_sense_clarification": "о ночи, зиме, старости"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "dc812188-9e07-31bd-afd0-1f04c15adb35",
                    "sense_definition": "Uitsluitend; enkel en alleen; zonder dat er iets anders bij komt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "exclusive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was <w>louter</w> toeval dat we elkaar in de stad tegenkwamen.",
                            "target_lang_translations": {
                                "en": "It was <w>pure</w> coincidence that we ran into each other in the city.",
                                "ru": "То, что мы встретились в городе, было <w>чистой</w> случайностью."
                            }
                        },
                        {
                            "text": "Zij drinkt <w>louter</w> water en vermijdt suikerhoudende dranken.",
                            "target_lang_translations": {
                                "en": "She drinks <w>only</w> water and avoids sugary drinks.",
                                "ru": "Она пьет <w>исключительно</w> воду и избегает сладких напитков."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitsluitend",
                        "enkel",
                        "alleen"
                    ],
                    "antonyms": [
                        "deels",
                        "gedeeltelijk"
                    ],
                    "common_phrases": [
                        "louter en alleen",
                        "louter toeval"
                    ],
                    "target_lang_definitions": {
                        "en": "Exclusively; only and solely; without anything else added.",
                        "ru": "Исключительно; только и всего; без добавления чего-либо другого."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "purely",
                                "target_lang_sense_clarification": "restricted to a specific aspect or cause"
                            },
                            {
                                "target_lang_word": "solely",
                                "target_lang_sense_clarification": "only; not involving anything else"
                            },
                            {
                                "target_lang_word": "merely",
                                "target_lang_sense_clarification": "nothing more than; simply"
                            },
                            {
                                "target_lang_word": "only",
                                "target_lang_sense_clarification": "general restriction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "исключительно",
                                "target_lang_sense_clarification": "только, единственно"
                            },
                            {
                                "target_lang_word": "лишь",
                                "target_lang_sense_clarification": "только, всего лишь"
                            },
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "единственно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "11d82a74-1070-3f95-82e0-fa6b1ec56eb8",
                    "sense_definition": "Niets anders dan; volslagen; puur (gebruikt om aan te geven dat iets uitsluitend uit het genoemde bestaat).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "sheer_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat hij vertelde waren <w>louter</w> leugens om zichzelf te beschermen.",
                            "target_lang_translations": {
                                "en": "What he told were <w>nothing but</w> lies to protect himself.",
                                "ru": "То, что он рассказал, было <w>сплошной</w> ложью, чтобы защитить себя."
                            }
                        },
                        {
                            "text": "Ze deed het uit <w>louter</w> nieuwsgierigheid en niet om kwaad te doen.",
                            "target_lang_translations": {
                                "en": "She did it out of <w>sheer</w> curiosity and not to do harm.",
                                "ru": "Она сделала это из <w>чистого</w> любопытства, а не со зла."
                            }
                        }
                    ],
                    "synonyms": [
                        "puur",
                        "enkel",
                        "zuiver"
                    ],
                    "common_phrases": [
                        "uit louter",
                        "louter onzin"
                    ],
                    "target_lang_definitions": {
                        "en": "Nothing but; complete; pure (used to indicate that something consists exclusively of the mentioned thing).",
                        "ru": "Ничто иное как; сущий; чистый (используется для указания на то, что нечто состоит исключительно из названного)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sheer",
                                "target_lang_sense_clarification": "used to emphasize the size, degree, or amount (e.g., sheer curiosity)"
                            },
                            {
                                "target_lang_word": "pure",
                                "target_lang_sense_clarification": "absolute; complete (e.g., pure nonsense)"
                            },
                            {
                                "target_lang_word": "nothing but",
                                "target_lang_sense_clarification": "phrase indicating exclusivity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сплошной",
                                "target_lang_sense_clarification": "состоящий только из чего-либо (часто с негативным оттенком, напр. ложь)"
                            },
                            {
                                "target_lang_word": "чистый",
                                "target_lang_sense_clarification": "настоящий, истинный (напр. любопытство, случайность)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "488a9232-f264-38dc-b633-d99765cfabb9",
                    "sense_definition": "Zuiver; onvermengd; niet vermengd met andere stoffen of elementen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "pure_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De antieke ring is gemaakt van het <w>louterste</w> goud.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "76b5047a-9f76-3930-a98f-304ebdda552a",
                    "sense_definition": "The way in which someone stands, especially when playing a sport or fighting.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boxer adopted a defensive <w>stance</w> to protect his face.",
                            "target_lang_translations": {
                                "ru": "Боксер принял защитную <w>стойку</w>, чтобы защитить лицо."
                            }
                        },
                        {
                            "text": "You need to widen your <w>stance</w> to keep your balance on the board.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно сделать <w>стойку</w> шире, чтобы удержать равновесие на доске."
                            }
                        }
                    ],
                    "synonyms": [
                        "posture",
                        "position",
                        "stand"
                    ],
                    "common_phrases": [
                        "wide stance",
                        "fighting stance",
                        "batting stance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положение тела, которое принимает человек, особенно во время занятий спортом или борьбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сто́йка",
                                "target_lang_sense_clarification": "в спорте, боевая"
                            },
                            {
                                "target_lang_word": "по́за",
                                "target_lang_sense_clarification": "положение тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98855bd5-d043-3b03-b972-15d6bfabb158",
                    "sense_definition": "A public statement of opinion or a particular attitude towards a specific subject.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "mental_attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The politician is known for his tough <w>stance</w> on crime.",
                            "target_lang_translations": {
                                "ru": "Этот политик известен своей жесткой <w>позицией</w> в отношении преступности."
                            }
                        },
                        {
                            "text": "She refused to change her <w>stance</w> despite the criticism.",
                            "target_lang_translations": {
                                "ru": "Она отказалась изменить свою <w>позицию</w>, несмотря на критику."
                            }
                        }
                    ],
                    "synonyms": [
                        "position",
                        "stand",
                        "posture"

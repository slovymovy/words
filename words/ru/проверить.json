            "pos": "verb",
            "senses": [
                {
                    "sense_id": "58417b3c-1084-381c-b1e8-468f5cce0678",
                    "sense_definition": "Удостовериться в правильности, точности или истинности чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель обещал <w>проверить</w> наши домашние задания к завтрашнему дню.",
                            "target_lang_translations": {
                                "en": "The teacher promised to <w>check</w> our homework by tomorrow."
                            }
                        },
                        {
                            "text": "Перед выходом из дома нужно обязательно <w>проверить</w> документы и билеты.",
                            "target_lang_translations": {
                                "en": "Before leaving the house, you must <w>check</w> your documents and tickets."
                            }

                            {
                                "target_lang_word": "небылица",
                                "target_lang_sense_clarification": "выдумка"
                            },
                            {
                                "target_lang_word": "байка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "враки",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "panna",
        "panieński",
        "panieństwo"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "490cbcf8-7e67-36e3-bd1f-7dcd55488870",
                    "sense_definition": "Młoda dziewczyna lub niezamężna kobieta; często używane jako forma zwrotu do dziewczynki lub młodej kobiety.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta młoda <w>panienka</w> bardzo ładnie śpiewa w szkolnym chórze.",
                            "target_lang_translations": {
                                "ru": "Эта молодая <w>барышня</w> очень красиво поет в школьном хоре."
                            }
                        },
                        {
                            "text": "Czy ta <w>panienka</w> może mi pomóc w znalezieniu drogi?",
                            "target_lang_translations": {
                                "ru": "Может ли эта <w>девушка</w> помочь мне найти дорогу?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "młoda panienka",
                        "śliczna panienka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Молодая девушка или незамужняя женщина; часто используется как форма обращения к девочке или молодой женщине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девушка",
                                "target_lang_sense_clarification": "молодая женщина"
                            },
                            {
                                "target_lang_word": "барышня",
                                "target_lang_sense_clarification": "обращение к молодой девушке (часто ироничное или уважительное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cad9ca44-46b1-3b6d-b56a-17ad1fd3c1de",
                    "sense_definition": "Kobieta świadcząca usługi seksualne za pieniądze (określenie potoczne lub eufemistyczne).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej dzielnicy wieczorami można spotkać <w>panienki</w> stojące przy drodze.",
                            "target_lang_translations": {
                                "ru": "В этом районе по вечерам можно встретить <w>девиц</w>, стоящих у дороги."
                            }
                        },
                        {
                            "text": "Film opowiada o trudnym losie <w>panienki</w> lekkich obyczajów.",
                            "target_lang_translations": {
                                "ru": "Фильм рассказывает о тяжелой судьбе <w>девицы</w> лёгкого поведения."
                            }
                        }
                    ],
                    "synonyms": [
                        "zob. prostytutka"
                    ],
                    "common_phrases": [
                        "panienka lekkich obyczajów",
                        "panienka do towarzystwa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, оказывающая сексуальные услуги за деньги (разговорное или эвфемизм)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девица",
                                "target_lang_sense_clarification": "разг., часто в контексте «девица лёгкого поведения»"
                            },
                            {
                                "target_lang_word": "проститутка",
                                "target_lang_sense_clarification": "прямое значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2306e04-1d35-331a-8497-947897484fe9",
                    "sense_definition": "Córka pracodawców (określenie używane dawniej przez służbę domową).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Służąca przyniosła świeże kwiaty do pokoju <w>panienki</w>.",
                            "target_lang_translations": {
                                "ru": "Служанка принесла свежие цветы в комнату <w>барышни</w>."
                            }
                        },
                        {
                            "text": "Nasza <w>panienka</w> wyjeżdża jutro na pensję do miasta.",
                            "target_lang_translations": {
                                "ru": "Наша <w>барышня</w> завтра уезжает в пансион в город."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nasza panienka",
                        "pokój panienki"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historyczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дочь хозяев (обращение, использовавшееся прислугой в прошлом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "барышня",
                                "target_lang_sense_clarification": "дочь господ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e873e82-ae0a-3f5c-9338-85f1b1c87f4f",
                    "sense_definition": "Dziewica; kobieta, która nie miała jeszcze kontaktów seksualnych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych czasach oczekiwano, że panna młoda będzie <w>panienką</w>.",
                            "target_lang_translations": {
                                "ru": "В прежние времена ожидалось, что невеста будет <w>девственницей</w>."
                            }
                        },
                        {
                            "text": "Mimo swojego wieku, ona wciąż jest <w>panienką</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на свой возраст, она всё ещё <w>девочка</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "być panienką",
                        "zostać panienką"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Девственница; женщина, которая еще не имела сексуальных контактов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девственница",
                                "target_lang_sense_clarification": "медицинский/формальный термин"
                            },
                            {
                                "target_lang_word": "девочка",
                                "target_lang_sense_clarification": "разг., невинная девушка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "633595db-1d77-3744-b881-63111d8a2139",
                    "sense_definition": "Źrenica oka (znaczenie przestarzałe lub regionalne).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy spojrzał w ostre światło, jego <w>panienki</w> gwałtownie się zwęziły.",
                            "target_lang_translations": {
                                "ru": "Когда он посмотрел на яркий свет, его <w>зеницы</w> резко сузились."
                            }
                        },
                        {
                            "text": "Lekarz dokładnie obejrzał <w>panienki</w> pacjenta podczas badania.",
                            "target_lang_translations": {
                                "ru": "Врач внимательно осмотрел <w>зрачки</w> пациента во время осмотра."
                            }
                        }
                    ],
                    "synonyms": [
                        "źrenica"
                    ],
                    "common_phrases": [
                        "panienki w oczach"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "przestarzałe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Зрачок глаза (устаревшее или региональное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зеница",
                                "target_lang_sense_clarification": "устар./поэт."
                            },
                            {
                                "target_lang_word": "зрачок",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
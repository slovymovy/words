{
    "word_family": [
        "разъединение",
        "разъединитель"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d369bcba-aa83-34f4-9b04-e5756d92caab",
                    "sense_definition": "Отделять одно от другого, размыкать соединённые части или предметы",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочий начал осторожно <разъединять> электрические провода.",
                            "target_lang_translations": {
                                "en": "The worker started to carefully <disconnect> the electric wires."
                            }
                        },
                        {
                            "text": "Чтобы разобрать механизм, нужно <разъединять> детали в правильном порядке.",
                            "target_lang_translations": {
                                "en": "To disassemble the mechanism, one needs to <separate> the parts in the correct order."
                            }
                        }
                    ],
                    "synonyms": [
                        "разделять"
                    ],
                    "antonyms": [
                        "соединять"
                    ],
                    "common_phrases": [
                        "разъединять провода",
                        "разъединять руки",
                        "разъединять вагоны"
                    ],
                    "target_lang_definitions": {
                        "en": "To separate one thing from another, or to break the connection between joined parts or objects"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "disconnect",
                                "target_lang_sense_clarification": "to sever or interrupt a physical connection (e.g., wires, mechanisms)"
                            },
                            {
                                "target_lang_word": "separate",
                                "target_lang_sense_clarification": "to divide into parts or move apart"
                            },
                            {
                                "target_lang_word": "disjoin",
                                "target_lang_sense_clarification": "to separate joined parts (formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50c6aace-53e4-3940-8899-69a2e4c7ada2",
                    "sense_definition": "Нарушать связь, единство или дружеские отношения между людьми",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Политические споры не должны <разъединять> нашу семью.",
                            "target_lang_translations": {
                                "en": "Political disputes should not <divide> our family."
                            }
                        },
                        {
                            "text": "Расстояние может <разъединять> друзей, но не разрушает дружбу.",
                            "target_lang_translations": {
                                "en": "Distance can <separate> friends, but it does not destroy friendship."
                            }
                        }
                    ],
                    "synonyms": [
                        "разобщать",
                        "разлучать"
                    ],
                    "antonyms": [
                        "объединять",
                        "сводить",
                        "соединять"
                    ],
                    "common_phrases": [
                        "разъединять людей",
                        "разъединять сердца",
                        "время разъединяет"
                    ],
                    "target_lang_definitions": {
                        "en": "To disrupt the connection, unity, or friendly relations between people"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "separate",
                                "target_lang_sense_clarification": "to cause people to be apart or estranged"
                            },
                            {
                                "target_lang_word": "disunite",
                                "target_lang_sense_clarification": "to cause disagreement or destroy unity"
                            },
                            {
                                "target_lang_word": "part",
                                "target_lang_sense_clarification": "to separate or divide people"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "577327b3-e441-3a88-a521-3a8c0c768548",
                    "sense_definition": "Прерывать телефонный разговор или отключать линию связи",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "technical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Оператор будет вынужден <разъединять> абонентов при отсутствии оплаты.",
                            "target_lang_translations": {
                                "en": "The operator will be forced to <disconnect> subscribers in the absence of payment."
                            }
                        },
                        {
                            "text": "Система может автоматически <разъединять> звонок через час.",
                            "target_lang_translations": {
                                "en": "The system can automatically <disconnect> the call after an hour."
                            }
                        }
                    ],
                    "synonyms": [
                        "отключать"
                    ],
                    "antonyms": [
                        "подключать",
                        "соединять"
                    ],
                    "common_phrases": [
                        "разъединять абонентов",
                        "разъединять линию",
                        "разъединять звонок"
                    ],
                    "target_lang_definitions": {
                        "en": "To interrupt a telephone conversation or cut off a communication line"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "disconnect",
                                "target_lang_sense_clarification": "to terminate a phone call or communication line"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
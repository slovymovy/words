            "senses": [
                {
                    "sense_id": "c36ee309-39f1-3dcb-8fc7-e28561ee8333",
                    "sense_definition": "Переставать делать что-либо; прерывать совершаемое действие.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "stop_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, <w>прекращай</w> шуметь, так как дети уже спят.",
                            "target_lang_translations": {
                                "en": "Please <w>stop</w> making noise, as the children are already sleeping."
                            }
                        },
                        {
                            "text": "Мы <w>прекращаем</w> работу в офисе ровно в шесть часов вечера.",
                            "target_lang_translations": {
                                "en": "We <w>stop</w> work at the office at exactly six o'clock in the evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "переставать",
                        "кончать"
                    ],
                    "antonyms": [
                        "начинать"
                    ],
                    "common_phrases": [
                        "прекращать работу",
                        "прекращать огонь",
                        "прекращать разговоры",
                        "прекращать общение"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop doing something; to interrupt an action in progress."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "general cessation of an action"
                            },
                            {
                                "target_lang_word": "cease",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "quit",
                                "target_lang_sense_clarification": "informal, often regarding habits"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff4e61f0-3534-3a30-a6ea-9d8a44ae1621",
                    "sense_definition": "Завершать что-либо; заставлять что-либо закончиться или остановиться (о процессах, состояниях или отношениях).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stop_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Завод постепенно <w>прекращает</w> производство устаревших моделей автомобилей.",
                            "target_lang_translations": {
                                "en": "The factory is gradually <w>stopping</w> the production of obsolete car models."
                            }
                        },
                        {
                            "text": "Стороны конфликта договорились <w>прекращать</w> войну и начать переговоры.",
                            "target_lang_translations": {
                                "en": "The parties to the conflict agreed to <w>end</w> the war and start negotiations."
                            }
                        }
                    ],
                    "synonyms": [
                        "завершать",
                        "заканчивать",
                        "кончать"
                    ],
                    "antonyms": [
                        "начинать"
                    ],
                    "common_phrases": [
                        "прекращать отношения",
                        "прекращать сотрудничество",
                        "прекращать поставки",
                        "прекращать дело"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring something to an end; to cause a process, state, or relationship to finish."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "end",
                                "target_lang_sense_clarification": "to bring to a conclusion"
                            },
                            {
                                "target_lang_word": "terminate",
                                "target_lang_sense_clarification": "formal, often for contracts or processes"
                            },
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "to cause to halt"
                            }
                        ]
                    }
                }

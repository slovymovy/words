                        {
                            "text": "That is my story and I am <w>sticking</w> to it.",
                            "target_lang_translations": {
                                "ru": "Это моя версия, и я <w>придерживаюсь</w> её."
                            }
                        }
                    ],
                    "synonyms": [
                        "persist",
                        "persevere",
                        "stand by"
                    ],
                    "antonyms": [
                        "waver",
                        "quit"
                    ],
                    "common_phrases": [
                        "stick to",
                        "stick with",
                        "stick by"

            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4aa5d50d-7103-36f2-9f3c-607fdd743729",
                    "sense_definition": "Done, sent, or supplied before a particular time or event.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_preceding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must give two weeks' <w>advance</w> notice before leaving the job.",
                            "target_lang_translations": {
                                "ru": "Вы должны подать <w>предварительное</w> уведомление за две недели до увольнения."
                            }
                        },
                        {
                            "text": "We made an <w>advance</w> booking for the hotel room to ensure availability.",
                            "target_lang_translations": {
                                "ru": "Мы сделали <w>предварительное</w> бронирование номера в отеле, чтобы гарантировать наличие мест."
                            }
                        }
                    ],
                    "common_phrases": [

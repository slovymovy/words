{
    "word_family": [
        "percentage",
        "percentile"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4355ac34-5149-3f08-8db3-0ff4b7412ec1",
                    "sense_definition": "For every hundred; used with a number to express a proportion or ratio of a total.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ratio_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fifty <w>percent</w> of the students in the class are girls.",
                            "target_lang_translations": {
                                "ru": "Пятьдесят <w>процентов</w> учеников в классе — девочки."
                            }
                        },
                        {
                            "text": "I agree with your decision one hundred <w>percent</w>.",
                            "target_lang_translations": {
                                "ru": "Я согласен с вашим решением на сто <w>процентов</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "per cent"
                    ],
                    "common_phrases": [
                        "one hundred percent",
                        "fifty percent",
                        "ten percent",
                        "zero percent"
                    ],
                    "target_lang_definitions": {
                        "ru": "На каждые сто; используется с числом для выражения пропорции или соотношения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "процент",
                                "target_lang_sense_clarification": "одна сотая часть"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "922318d1-e759-36de-90bb-80e0066b0520",
                    "sense_definition": "A part, share, or proportion of a whole.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ratio_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Only a small <w>percent</w> of the population voted in the election.",
                            "target_lang_translations": {
                                "ru": "Лишь небольшой <w>процент</w> населения проголосовал на выборах."
                            }
                        },
                        {
                            "text": "A large <w>percent</w> of our budget is spent on food.",
                            "target_lang_translations": {
                                "ru": "Большой <w>процент</w> нашего бюджета тратится на еду."
                            }
                        }
                    ],
                    "synonyms": [
                        "percentage",
                        "proportion",
                        "share"
                    ],
                    "common_phrases": [
                        "small percent",
                        "large percent",
                        "high percent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть, доля или пропорция целого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "процент",
                                "target_lang_sense_clarification": "доля, часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f55a913b-a867-3480-9375-9bba03748247",
                    "sense_definition": "The symbol % used to indicate a percentage.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "symbol_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should place the <w>percent</w> sign immediately after the number.",
                            "target_lang_translations": {
                                "ru": "Вам следует ставить знак <w>процента</w> сразу после числа."
                            }
                        },
                        {
                            "text": "The <w>percent</w> looks like a fraction with two zeros.",
                            "target_lang_translations": {
                                "ru": "Знак <w>процента</w> выглядит как дробь с двумя нулями."
                            }
                        }
                    ],
                    "synonyms": [
                        "percent sign"
                    ],
                    "target_lang_definitions": {
                        "ru": "Символ %, используемый для обозначения процентов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знак процента",
                                "target_lang_sense_clarification": "символ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ff8fa0d-9b4e-3203-b364-ee68e66f4b72",
                    "sense_definition": "A government bond or security that pays a fixed rate of interest.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "financial_instrument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The investor put all his savings into the three <w>percents</w>.",
                            "target_lang_translations": {
                                "ru": "Инвестор вложил все свои сбережения в трех<w>процентные</w> бумаги."
                            }
                        },
                        {
                            "text": "Historically, the three <w>percents</w> were considered a very safe investment.",
                            "target_lang_translations": {
                                "ru": "Исторически трех<w>процентные</w> бумаги считались очень надежной инвестицией."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "financial"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Государственная облигация или ценная бумага, по которой выплачивается фиксированный процент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "процентная бумага",
                                "target_lang_sense_clarification": "облигация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
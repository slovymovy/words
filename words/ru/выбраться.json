            "senses": [
                {
                    "sense_id": "c5ef61ae-0d0f-385e-87b1-72b4a2d12463",
                    "sense_definition": "Выйти или выехать откуда-либо, преодолевая трудности или препятствия.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_out",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы с большим трудом <w>выбрались</w> из густого леса до наступления ночи.",
                            "target_lang_translations": {
                                "en": "The tourists managed to <w>get out</w> of the dense forest with great difficulty before nightfall."
                            }
                        },
                        {
                            "text": "Маленький щенок упал в яму и не мог самостоятельно <w>выбраться</w> обратно.",
                            "target_lang_translations": {
                                "en": "The little puppy fell into a pit and couldn't <w>get out</w> on its own."
                            }
                        }
                    ],
                    "synonyms": [
                        "вылезти",
                        "выпутаться",
                        "спастись"
                    ],
                    "antonyms": [
                        "застрять",
                        "увязнуть"
                    ],
                    "common_phrases": [
                        "выбраться из дома",
                        "выбраться на берег",
                        "выбраться из долгов",
                        "выбраться на свободу"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave a place or situation, often by overcoming difficulties or obstacles."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get out",
                                "target_lang_sense_clarification": "general term for leaving a place, especially with difficulty"
                            },
                            {
                                "target_lang_word": "escape",
                                "target_lang_sense_clarification": "to get free from confinement or a difficult situation"
                            },
                            {
                                "target_lang_word": "make one's way out",
                                "target_lang_sense_clarification": "emphasizes the effort or process of exiting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f7f4435-653d-3d2f-9851-a3eb1207627c",
                    "sense_definition": "Найти свободное время, чтобы пойти или поехать куда-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам наконец-то удалось <w>выбраться</w> в театр в эти выходные.",
                            "target_lang_translations": {
                                "en": "We finally managed to <w>get out</w> to the theater this weekend."
                            }
                        },
                        {
                            "text": "Я очень надеюсь <w>выбраться</w> к вам в гости на следующей неделе.",
                            "target_lang_translations": {
                                "en": "I really hope to <w>make it</w> over to visit you next week."
                            }
                        }
                    ],
                    "synonyms": [
                        "вырваться",
                        "найти время"
                    ],
                    "common_phrases": [
                        "выбраться в город",
                        "выбраться на природу",
                        "выбраться в гости",
                        "выбраться в отпуск"
                    ],
                    "target_lang_definitions": {

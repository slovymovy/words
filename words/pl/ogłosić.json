            "senses": [
                {
                    "sense_id": "eee5f02b-17fa-3577-9e59-2106e2824335",
                    "sense_definition": "Podać wiadomość do publicznej wiadomości, oficjalnie o czymś poinformować.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor szkoły <w>ogłosił</w> przerwę świąteczną podczas porannego apelu.",
                            "target_lang_translations": {
                                "ru": "Директор школы <w>объявил</w> о рождественских каникулах во время утренней линейки."
                            }
                        },
                        {
                            "text": "Rząd planuje <w>ogłosić</w> nowe przepisy podatkowe w przyszłym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Правительство планирует <w>объявить</w> о новых налоговых правилах в следующем месяце."
                            }
                        },
                        {
                            "text": "Sędzia uderzył młotkiem i uroczyście <w>ogłosił</w> wyrok w sprawie karnej.",
                            "target_lang_translations": {
                                "ru": "Судья ударил молотком и торжественно <w>огласил</w> приговор по уголовному делу."
                            }
                        }
                    ],
                    "synonyms": [
                        "obwieścić",
                        "zakomunikować",
                        "oznajmić"
                    ],
                    "antonyms": [
                        "zataić",
                        "ukryć"
                    ],
                    "common_phrases": [
                        "ogłosić wyniki",
                        "ogłosić wyrok",
                        "ogłosić przetarg",
                        "ogłosić upadłość",
                        "ogłosić przerwę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать сообщение всеобщим достоянием, официально о чём-то проинформировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объявить",
                                "target_lang_sense_clarification": "официально сообщить"
                            },
                            {
                                "target_lang_word": "огласить",
                                "target_lang_sense_clarification": "прочитать вслух, обнародовать (например, приговор)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7595e77-2f0b-39ba-9e17-541514d86f97",
                    "sense_definition": "Uznać się oficjalnie za kogoś lub nadać sobie jakiś tytuł (używane z zaimkiem zwrotnym \"się\").",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "declaration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po zdobyciu stolicy dowódca wojsk nieoczekiwanie <w>ogłosił</w> się nowym władcą.",
                            "target_lang_translations": {
                                "ru": "После захвата столицы командующий войсками неожиданно <w>объявил</w> себя новым правителем."
                            }
                        },
                        {
                            "text": "Mężczyzna <w>ogłosił</w> się bankrutem, ponieważ stracił cały majątek na giełdzie.",
                            "target_lang_translations": {
                                "ru": "Мужчина <w>объявил</w> себя банкротом, так как потерял всё состояние на бирже."
                            }
                        }
                    ],
                    "synonyms": [
                        "mianować się",
                        "obwołać się"
                    ],
                    "common_phrases": [
                        "ogłosić się królem",
                        "ogłosić się zwycięzcą",
                        "ogłosić się bankrutem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально признать себя кем-либо или присвоить себе какой-либо титул."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объявить",
                                "target_lang_sense_clarification": "себя кем-либо (напр. банкротом)"
                            },
                            {
                                "target_lang_word": "провозгласить",
                                "target_lang_sense_clarification": "торжественно объявить (себя правителем)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97eba73b-4341-35a0-95dc-ca5a89ad93c3",
                    "sense_definition": "Opublikować tekst, utwór lub dokument drukiem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "publishing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Profesor <w>ogłosił</w> drukiem wyniki swoich wieloletnich badań nad historią miasta.",
                            "target_lang_translations": {
                                "ru": "Профессор <w>опубликовал</w> результаты своих многолетних исследований истории города."
                            }
                        },
                        {
                            "text": "Młody poeta z wielką dumą <w>ogłosił</w> swój debiutancki tomik wierszy.",
                            "target_lang_translations": {
                                "ru": "Молодой поэт с большой гордостью <w>издал</w> свой дебютный сборник стихов."
                            }
                        }
                    ],
                    "synonyms": [
                        "opublikować",
                        "wydać"
                    ],
                    "common_phrases": [
                        "ogłosić drukiem"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "książkowe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Опубликовать текст, произведение или документ в печати."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опубликовать",
                                "target_lang_sense_clarification": "разместить в печати"
                            },
                            {
                                "target_lang_word": "издать",
                                "target_lang_sense_clarification": "выпустить в свет (книгу, труд)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

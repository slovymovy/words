                            },
                            {
                                "target_lang_word": "filth",
                                "target_lang_sense_clarification": "disgusting or offensive matter"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "грязь",
                                "target_lang_sense_clarification": "общее вещество (пыль, земля)"
                            },
                            {
                                "target_lang_word": "сор",

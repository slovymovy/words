    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "41a83b3c-58a5-34b3-8461-533a4c6fcd28",
                    "sense_definition": "The scientific study of the physical structure of humans, animals, and plants.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "bio_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to study <w>anatomy</w> because she wanted to become a surgeon.",
                            "target_lang_translations": {
                                "ru": "Она решила изучать <w>анатомию</w>, потому что хотела стать хирургом."
                            }
                        },
                        {
                            "text": "The textbook provides a comprehensive guide to human <w>anatomy</w> and physiology.",
                            "target_lang_translations": {
                                "ru": "Учебник представляет собой полное руководство по <w>анатомии</w> и физиологии человека."
                            }
                        }
                    ],
                    "common_phrases": [
                        "human anatomy",
                        "comparative anatomy",
                        "study anatomy",
                        "gray's anatomy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наука о строении тела людей, животных и растений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "анатомия",
                                "target_lang_sense_clarification": "наука"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29ac79c7-efbc-3b90-89b2-ccf14a7acd43",
                    "sense_definition": "The physical structure or internal organization of a living organism.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "bio_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We learned about the complex <w>anatomy</w> of the eye in biology class.",
                            "target_lang_translations": {
                                "ru": "На уроке биологии мы узнали о сложной <w>анатомии</w> глаза."
                            }
                        },
                        {
                            "text": "The <w>anatomy</w> of this flower allows it to survive in very dry climates.",
                            "target_lang_translations": {
                                "ru": "<w>Анатомия</w> этого цветка позволяет ему выживать в очень засушливом климате."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brain anatomy",
                        "internal anatomy",
                        "complex anatomy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое строение или внутренняя организация живого организма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "анатомия",
                                "target_lang_sense_clarification": "строение"
                            },
                            {
                                "target_lang_word": "строение",
                                "target_lang_sense_clarification": "структура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "715de58b-a2b6-3ed5-b509-0288b64e8dd7",
                    "sense_definition": "A detailed examination or analysis of the separate parts of a subject, such as an event or idea.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "analysis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The journalist wrote a compelling <w>anatomy</w> of the political scandal.",
                            "target_lang_translations": {
                                "ru": "Журналист написал захватывающую <w>анатомию</w> политического скандала."
                            }
                        },
                        {
                            "text": "His book offers a detailed <w>anatomy</w> of the economic crisis and its causes.",
                            "target_lang_translations": {
                                "ru": "В его книге предлагается подробная <w>анатомия</w> экономического кризиса и его причин."
                            }
                        }
                    ],
                    "synonyms": [
                        "analysis",
                        "dissection"
                    ],
                    "common_phrases": [
                        "anatomy of a crime",
                        "anatomy of a disaster",
                        "anatomy of a murder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Детальное исследование или анализ отдельных частей предмета, события или идеи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "анатомия",
                                "target_lang_sense_clarification": "анализ"
                            },
                            {
                                "target_lang_word": "разбор",
                                "target_lang_sense_clarification": "детальное изучение"
                            },
                            {
                                "target_lang_word": "анатомирование",
                                "target_lang_sense_clarification": "тщательный анализ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa5a138c-c059-3f7a-bba0-5d46f6031b6d",
                    "sense_definition": "A person's body, often used to refer to private parts in a polite or humorous way.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "body_ref",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He slipped on the ice and landed on a painful part of his <w>anatomy</w>.",
                            "target_lang_translations": {
                                "ru": "Он поскользнулся на льду и приземлился на болезненную часть своего <w>тела</w>."
                            }
                        },
                        {
                            "text": "The doctor examined the injured part of his <w>anatomy</w> carefully.",
                            "target_lang_translations": {
                                "ru": "Врач внимательно осмотрел поврежденную часть его <w>тела</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "part of his anatomy",
                        "part of her anatomy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used humorously or euphemistically"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тело человека (часто используется шутливо или как эвфемизм для обозначения интимных частей)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тело",
                                "target_lang_sense_clarification": "организм"
                            },
                            {
                                "target_lang_word": "части тела",
                                "target_lang_sense_clarification": "эвфемизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0625e40b-a946-3d2b-afb7-baf576def2e0",
                    "sense_definition": "A skeleton or a dead body.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "body_ref",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The medical students gathered around the <w>anatomy</w> to observe the bone structure.",
                            "target_lang_translations": {
                                "ru": "Студенты-медики собрались вокруг <w>скелета</w>, чтобы изучить строение костей."
                            }
                        },
                        {
                            "text": "In the old illustration, an <w>anatomy</w> stood holding an hourglass.",
                            "target_lang_translations": {
                                "ru": "На старинной иллюстрации стоял <w>скелет</w>, держащий песочные часы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скелет или мертвое тело (устаревшее значение)."
                    },
                    "translations": {

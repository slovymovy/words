            "pos": "verb",
            "senses": [
                {
                    "sense_id": "80693918-0ea9-36f6-8757-14e9c787f16f",
                    "sense_definition": "Составить мнение или суждение о качестве, характере, значении или уровне чего-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам нужно время, чтобы правильно <оценить> сложившуюся ситуацию.",
                            "target_lang_translations": {
                                "en": "We need time to correctly <assess> the current situation."
                            }
                        },
                        {
                            "text": "Жюри должно честно <оценить> выступление каждого участника конкурса.",
                            "target_lang_translations": {
                                "en": "The jury must honestly <evaluate> the performance of each contest participant."
                            }
                        }
                    ],
                    "common_phrases": [
                        "оценить ситуацию",
                        "оценить обстановку",
                        "оценить взглядом",
                        "оценить результаты",
                        "высоко оценить"
                    ],
                    "target_lang_definitions": {
                        "en": "To form an opinion or judgment about the quality, character, significance, or level of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "assess",
                                "target_lang_sense_clarification": "analyze a situation or state"
                            },
                            {
                                "target_lang_word": "evaluate",
                                "target_lang_sense_clarification": "judge quality or performance"
                            },
                            {
                                "target_lang_word": "judge",
                                "target_lang_sense_clarification": "form an opinion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f643255a-fa9c-30ea-b88f-53a4ee4f494d",
                    "sense_definition": "Признать положительные качества, важность или значимость кого-либо или чего-либо; понять ценность.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Только настоящий друг может <оценить> твою искренность и поддержку в трудную минуту.",
                            "target_lang_translations": {
                                "en": "Only a true friend can <appreciate> your sincerity and support in a difficult moment."
                            }
                        },
                        {
                            "text": "Гости успели <оценить> вкусный ужин и приятную музыку на вечеринке.",
                            "target_lang_translations": {
                                "en": "The guests managed to <appreciate> the delicious dinner and pleasant music at the party."
                            }
                        }
                    ],
                    "antonyms": [
                        "недооценить"
                    ],
                    "common_phrases": [
                        "оценить по достоинству",
                        "оценить юмор",
                        "оценить шутку",
                        "оценить вклад"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies recognizing value or advantages"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To recognize positive qualities, importance, or significance of someone or something; to understand the value."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appreciate",
                                "target_lang_sense_clarification": "recognize worth or quality"
                            },
                            {
                                "target_lang_word": "value",
                                "target_lang_sense_clarification": "consider important"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8edcc996-ced8-3efb-9643-069dc8d8d89b",
                    "sense_definition": "Определить стоимость чего-либо в денежном выражении.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эксперт приехал, чтобы <оценить> старинную мебель и картины в доме.",
                            "target_lang_translations": {
                                "en": "The expert arrived to <appraise> the antique furniture and paintings in the house."
                            }
                        },
                        {
                            "text": "Ущерб от пожара ещё предстоит <оценить> специалистам страховой компании.",
                            "target_lang_translations": {
                                "en": "The damage from the fire has yet to be <assessed> by the insurance company specialists."
                            }
                        }
                    ],
                    "synonyms": [
                        "назначить цену"
                    ],
                    "common_phrases": [
                        "оценить ущерб",
                        "оценить стоимость",
                        "оценить имущество",
                        "оценить в сумму"
                    ],
                    "target_lang_definitions": {
                        "en": "To determine the cost of something in monetary terms."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appraise",
                                "target_lang_sense_clarification": "expert valuation of property"
                            },
                            {
                                "target_lang_word": "value",
                                "target_lang_sense_clarification": "determine monetary worth"
                            },
                            {
                                "target_lang_word": "price",

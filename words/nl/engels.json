                {
                    "sense_id": "665ebf3c-be24-358c-a26d-05bab9337a86",
                    "sense_definition": "Met betrekking tot Engeland, het Engelse volk of de Engelse taal.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "english_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij leest graag <w>Engelse</w> boeken om de taal te leren.",
                            "target_lang_translations": {
                                "en": "She likes reading <w>English</w> books to learn the language.",
                                "ru": "Она любит читать <w>английские</w> книги, чтобы учить язык."
                            }
                        },
                        {
                            "text": "Voor vertrek had ik een lijst gekregen van een <w>Engelse</w> vrouw.",
                            "target_lang_translations": {
                                "en": "Before departure, I had received a list from an <w>English</w> woman.",
                                "ru": "Перед отъездом я получил список от одной <w>английской</w> женщины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Engelse taal",
                        "Engelse les",
                        "Engels ontbijt",
                        "Engelse humor"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to England, the English people, or the English language.",
                        "ru": "Относящийся к Англии, английскому народу или английскому языку."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "English",
                                "target_lang_sense_clarification": "relating to England or its people"

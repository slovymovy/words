                },
                {
                    "sense_id": "4ec2d738-56a5-3569-a019-7568da7f09b9",
                    "sense_definition": "Een specifiek onderdeel of divisie van de strijdkrachten, zoals de infanterie of cavalerie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "military_branch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De cavalerie is een historisch <w>wapen</w> binnen het leger.",
                            "target_lang_translations": {
                                "ru": "Кавалерия — это исторический <w>род войск</w> в армии."
                            }
                        },
                        {
                            "text": "Hij dient als officier bij het <w>wapen</w> van de artillerie.",
                            "target_lang_translations": {

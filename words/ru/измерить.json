                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "measure",
                                "target_lang_sense_clarification": "evaluate abstract qualities"
                            },
                            {
                                "target_lang_word": "gauge",
                                "target_lang_sense_clarification": "assess or estimate"
                            },
                            {
                                "target_lang_word": "size up",
                                "target_lang_sense_clarification": "evaluate someone visually"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e8dc6a5-901a-3145-aeae-69faccd2b50a",
                    "sense_definition": "Пройти пешком большое расстояние или побывать во многих местах.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За этот долгий день туристы успели <измерить> ногами весь исторический центр.",
                            "target_lang_translations": {
                                "en": "During this long day, the tourists managed to <w>traverse</w> the entire historic center on foot."
                            }

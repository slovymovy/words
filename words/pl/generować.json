            "senses": [
                {
                    "sense_id": "cd44b2c2-5aa4-3494-b77e-c3aba5a7a4d0",
                    "sense_definition": "Powodować powstanie czegoś, być źródłem określonych skutków, emocji lub sytuacji (często w kontekście biznesowym lub abstrakcyjnym).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "cause_create",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety, ta zmiana w projekcie będzie <w>generować</w> dodatkowe koszty dla firmy.",
                            "target_lang_translations": {
                                "ru": "К сожалению, это изменение в проекте будет <w>создавать</w> дополнительные расходы для компании."
                            }
                        },
                        {
                            "text": "Jego kontrowersyjne wypowiedzi nieustannie <w>generują</w> konflikty w zespole.",
                            "target_lang_translations": {
                                "ru": "Его спорные высказывания постоянно <w>порождают</w> конфликты в коллективе."
                            }
                        }
                    ],
                    "synonyms": [
                        "powodować"
                    ],
                    "common_phrases": [
                        "generować koszty",
                        "generować zyski",
                        "generować problemy",
                        "generować ruch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать появление чего-либо, служить источником определенных последствий, эмоций или ситуаций (часто в деловом или абстрактном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порождать",
                                "target_lang_sense_clarification": "о проблемах, конфликтах, абстрактных явлениях"
                            },
                            {
                                "target_lang_word": "создавать",
                                "target_lang_sense_clarification": "о ситуациях, затратах, условиях"
                            },
                            {
                                "target_lang_word": "генерировать",
                                "target_lang_sense_clarification": "в бизнесе: прибыль, трафик, лиды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "077c1a02-b6a5-37ab-a79c-18ea789a3c39",
                    "sense_definition": "Wytwarzać energię, dane, dokumenty lub inne obiekty za pomocą procesów technicznych, komputerowych lub fizycznych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "produce_technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "System automatycznie <w>generuje</w> raport podsumowujący sprzedaż na koniec każdego dnia.",
                            "target_lang_translations": {
                                "ru": "Система автоматически <w>генерирует</w> отчет с итогами продаж в конце каждого дня."
                            }
                        },
                        {
                            "text": "Nowoczesne panele słoneczne potrafią <w>generować</w> energię nawet w pochmurne dni.",
                            "target_lang_translations": {
                                "ru": "Современные солнечные панели способны <w>вырабатывать</w> энергию даже в пасмурные дни."
                            }
                        }
                    ],
                    "synonyms": [
                        "wytwarzać"
                    ],
                    "common_phrases": [
                        "generować energię",
                        "generować raport",
                        "generować kod",
                        "generować hasło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Производить энергию, данные, документы или другие объекты посредством технических, компьютерных или физических процессов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "генерировать",
                                "target_lang_sense_clarification": "в IT и технике: код, отчеты, пароли"
                            },
                            {
                                "target_lang_word": "вырабатывать",
                                "target_lang_sense_clarification": "об энергии, электричестве, тепле"
                            }
                        ]
                    }
                }
            ]
        }
    ]

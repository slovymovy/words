            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e99193c0-0ea3-3504-8445-ede7896f62c3",
                    "sense_definition": "Dać coś komuś bezinteresownie jako prezent lub wsparcie materialne.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na urodziny <ofiarował> swojej żonie piękny naszyjnik z prawdziwych pereł.",
                            "target_lang_translations": {
                                "ru": "На день рождения он <подарил> своей жене красивое ожерелье из настоящего жемчуга."
                            }
                        },
                        {
                            "text": "Bogaty przedsiębiorca postanowił <ofiarować> dużą sumę pieniędzy na budowę nowego szpitala.",
                            "target_lang_translations": {
                                "ru": "Богатый предприниматель решил <пожертвовать> крупную сумму денег на строительство новой больницы."
                            }
                        }
                    ],
                    "synonyms": [
                        "podarować",
                        "sprezentować",
                        "przekazać"
                    ],
                    "common_phrases": [
                        "ofiarować prezent",
                        "ofiarować pomoc",
                        "hojnie ofiarować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать что-то кому-то бескорыстно в качестве подарка или материальной поддержки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подарить",
                                "target_lang_sense_clarification": "дать в качестве подарка"
                            },
                            {
                                "target_lang_word": "пожертвовать",
                                "target_lang_sense_clarification": "передать на благотворительность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "080c5de4-b9ed-30ce-892a-b767cc1a1373",
                    "sense_definition": "Poświęcić coś cennego, na przykład swój czas, życie lub serce, dla kogoś lub jakiejś idei.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dedication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze byli gotowi <ofiarować> swoje życie w obronie niepodległości ojczyzny.",
                            "target_lang_translations": {
                                "ru": "Солдаты были готовы <пожертвовать> своими жизнями ради защиты независимости родины."
                            }
                        },
                        {
                            "text": "Matka bez wahania <ofiarowała> cały swój wolny czas na opiekę nad chorym dzieckiem.",
                            "target_lang_translations": {
                                "ru": "Мать без колебаний <пожертвовала> всем своим свободным временем ради ухода за больным ребёнком."
                            }
                        }
                    ],
                    "synonyms": [
                        "poświęcić",
                        "oddać",
                        "przeznaczyć"
                    ],
                    "common_phrases": [
                        "ofiarować życie",
                        "ofiarować serce",
                        "ofiarować czas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пожертвовать чем-то ценным, например временем или жизнью, ради кого-то или какой-то идеи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожертвовать",
                                "target_lang_sense_clarification": "отказаться от чего-то ценного ради цели"
                            },
                            {
                                "target_lang_word": "отдать",
                                "target_lang_sense_clarification": "посвятить без остатка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a88cef4-0aad-3186-9e78-2f5444474fed",
                    "sense_definition": "Złożyć dar bóstwu w ramach obrzędu religijnego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starożytności ludzie często <ofiarowali> zwierzęta, aby przebłagać gniew bogów.",
                            "target_lang_translations": {
                                "ru": "В древности люди часто <приносили в жертву> животных, чтобы умилостивить гнев богов."
                            }
                        },
                        {
                            "text": "Kapłan uroczyście <ofiarował> chleb i wino na ołtarzu podczas mszy świętej.",
                            "target_lang_translations": {
                                "ru": "Священник торжественно <принёс в дар> хлеб и вино на алтаре во время святой мессы."
                            }
                        }
                    ],
                    "synonyms": [
                        "poświęcić"
                    ],
                    "common_phrases": [
                        "ofiarować Bogu",
                        "ofiarować dary",
                        "ofiarować na ołtarzu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принести дар божеству в рамках религиозного обряда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принести в жертву",
                                "target_lang_sense_clarification": "совершить ритуальное подношение"
                            },
                            {
                                "target_lang_word": "принести в дар",
                                "target_lang_sense_clarification": "торжественно отдать богу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e1bffe2-085e-34f3-815b-0cd46c1e7b5d",
                    "sense_definition": "Zgłosić gotowość do wykonania czegoś trudnego lub poświęcić się dla kogoś (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "volunteering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody lekarz <ofiarował> się, że pojedzie na niebezpieczną misję medyczną do Afryki.",
                            "target_lang_translations": {
                                "ru": "Молодой врач <вызвался> поехать с опасной медицинской миссией в Африку."
                            }
                        },
                        {
                            "text": "Widząc trudną sytuację sąsiadów, mężczyzna sam <ofiarował> się z pomocą przy remoncie.",
                            "target_lang_translations": {
                                "ru": "Видя трудную ситуацию соседей, мужчина сам <вызвался> помочь с ремонтом."
                            }
                        }
                    ],
                    "common_phrases": [

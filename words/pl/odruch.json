{
    "word_family": [
        "odruchowy",
        "odruchowo"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "54a87026-bc36-3387-9f4e-8dd34f12e2a0",
                    "sense_definition": "Mimowolna reakcja organizmu na bodziec zewnętrzny lub wewnętrzny, zachodząca za pośrednictwem układu nerwowego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "biological_reflex",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz uderzył młoteczkiem w kolano pacjenta, aby sprawdzić jego <w>odruch</w>.",
                            "target_lang_translations": {
                                "ru": "Врач ударил молоточком по колену пациента, чтобы проверить его <w>рефлекс</w>."
                            }
                        },
                        {
                            "text": "Silne światło powoduje naturalny <w>odruch</w> mrużenia oczu.",
                            "target_lang_translations": {
                                "ru": "Яркий свет вызывает естественный <w>рефлекс</w> прищуривания глаз."
                            }
                        }
                    ],
                    "synonyms": [
                        "reakcja"
                    ],
                    "common_phrases": [
                        "odruch warunkowy",
                        "odruch bezwarunkowy",
                        "odruch wymiotny",
                        "badać odruchy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непроизвольная реакция организма на внешний или внутренний раздражитель, происходящая при участии нервной системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рефлекс",
                                "target_lang_sense_clarification": "биологическая реакция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4f06597-832c-3897-9fe0-6ae5302774c3",
                    "sense_definition": "Spontaniczna, często nawykowa i gwałtowna reakcja na jakąś sytuację lub zdarzenie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "behavioral_reflex",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To był czysty <w>odruch</w>, kiedy złapałem spadającą ze stołu szklankę.",
                            "target_lang_translations": {
                                "ru": "Это был чистый <w>рефлекс</w>, когда я поймал падающий со стола стакан."
                            }
                        },
                        {
                            "text": "Jej pierwszym <w>odruchem</w> była chęć ucieczki z tego miejsca.",
                            "target_lang_translations": {
                                "ru": "Её первым <w>порывом</w> было желание убежать из этого места."
                            }
                        }
                    ],
                    "synonyms": [
                        "impuls",
                        "automatyzm",
                        "odzew"
                    ],
                    "common_phrases": [
                        "odruch obronny",
                        "zrobić coś z odruchu",
                        "pierwszy odruch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спонтанная, часто привычная и резкая реакция на какую-либо ситуацию или событие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рефлекс",
                                "target_lang_sense_clarification": "автоматическая реакция"
                            },
                            {
                                "target_lang_word": "инстинктивное движение",
                                "target_lang_sense_clarification": "спонтанное действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5babd43f-6b0c-3ba2-8c8f-0ad244404236",
                    "sense_definition": "Nagły, wewnętrzny impuls emocjonalny lub szlachetne poruszenie serca.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotional_impulse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pomoc starszej pani była dla niego naturalnym <w>odruchem</w> serca.",
                            "target_lang_translations": {
                                "ru": "Помощь пожилой женщине была для него естественным <w>порывом</w> сердца."
                            }
                        },
                        {
                            "text": "W <w>odruchu</w> litości postanowiła przygarnąć bezdomnego psa.",
                            "target_lang_translations": {
                                "ru": "В <w>порыве</w> жалости она решила приютить бездомную собаку."
                            }
                        }
                    ],
                    "synonyms": [
                        "poryw"
                    ],
                    "common_phrases": [
                        "odruch serca",
                        "odruch litości",
                        "szlachetny odruch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапный внутренний эмоциональный импульс или благородное душевное движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порыв",
                                "target_lang_sense_clarification": "эмоциональный импульс"
                            },
                            {
                                "target_lang_word": "побуждение",
                                "target_lang_sense_clarification": "внутреннее стремление"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
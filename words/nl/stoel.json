    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c4e4d1bd-14c0-3e56-b13e-6813eb6fadfb",
                    "sense_definition": "Een zitmeubel voor één persoon, meestal met vier poten en een rugleuning.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik op deze <w>stoel</w> zitten of is hij al bezet?",
                            "target_lang_translations": {
                                "en": "May I sit on this <w>chair</w> or is it already taken?",
                                "ru": "Можно мне сесть на этот <w>стул</w> или он уже занят?"
                            }
                        },
                        {
                            "text": "Hij schoof zijn <w>stoel</w> aan tafel om te gaan eten.",
                            "target_lang_translations": {
                                "en": "He pulled his <w>chair</w> up to the table to eat.",
                                "ru": "Он придвинул свой <w>стул</w> к столу, чтобы поесть."
                            }
                        }
                    ],
                    "synonyms": [
                        "zetel"
                    ],
                    "common_phrases": [
                        "op een stoel zitten",
                        "een stoel aanschuiven",
                        "tussen twee stoelen"
                    ],
                    "target_lang_definitions": {
                        "en": "A piece of furniture for one person to sit on, usually with four legs and a backrest.",
                        "ru": "Мебель для сидения одного человека, обычно с четырьмя ножками и спинкой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chair",
                                "target_lang_sense_clarification": "furniture"
                            },
                            {
                                "target_lang_word": "seat",
                                "target_lang_sense_clarification": "general term"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "стул",
                                "target_lang_sense_clarification": "мебель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "513a6f57-9586-3c80-8b5c-a8e2daddf8a3",
                    "sense_definition": "De ontlasting van mensen of dieren.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dokter vroeg of zijn <w>stoel</w> de laatste tijd veranderd was.",
                            "target_lang_translations": {
                                "en": "The doctor asked if his <w>stool</w> had changed lately.",
                                "ru": "Врач спросил, изменился ли его <w>стул</w> в последнее время."
                            }
                        },
                        {
                            "text": "Patiënten met buikpijn hebben vaak last van een onregelmatige <w>stoel</w>.",
                            "target_lang_translations": {
                                "en": "Patients with abdominal pain often suffer from irregular <w>bowel movements</w>.",
                                "ru": "Пациенты с болью в животе часто страдают от нерегулярного <w>стула</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stoelgang"
                    ],
                    "common_phrases": [
                        "harde stoel",
                        "geen stoel hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "The excrement of humans or animals.",
                        "ru": "Испражнения людей или животных."
                    },
                    "translations": {

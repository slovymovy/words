    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cd1d9621-84f8-3b5d-9060-c5fedebf37e2",
                    "sense_definition": "Архитектурное или скульптурное сооружение в память или в честь какого-либо лица или события.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_monument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На главной площади города стоит бронзовый <w>памятник</w> поэту.",
                            "target_lang_translations": {
                                "en": "A bronze <w>monument</w> to the poet stands in the city's main square."
                            }
                        },
                        {
                            "text": "Туристы любят фотографироваться возле этого исторического <w>памятника</w>.",
                            "target_lang_translations": {
                                "en": "Tourists love taking photos near this historical <w>monument</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "монуме́нт",
                        "мемориа́л"
                    ],
                    "common_phrases": [
                        "поставить памятник",
                        "открытие памятника",
                        "памятник героям"
                    ],
                    "target_lang_definitions": {
                        "en": "An architectural or sculptural structure built to commemorate a person or event."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monument",
                                "target_lang_sense_clarification": "general term for a commemorative structure"
                            },
                            {
                                "target_lang_word": "statue",
                                "target_lang_sense_clarification": "sculpture of a person or animal"
                            },
                            {
                                "target_lang_word": "memorial",
                                "target_lang_sense_clarification": "structure honoring the dead or a tragic event"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b3a4aff-4453-364c-b08c-335eb8fa5e7f",
                    "sense_definition": "Объект, являющийся свидетельством исторического события, частью культурного наследия страны или народа.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cultural_heritage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта старинная церковь охраняется государством как <w>памятник</w> архитектуры.",
                            "target_lang_translations": {
                                "en": "This ancient church is protected by the state as a <w>monument</w> of architecture."
                            }
                        },
                        {
                            "text": "Древние летописи — это важнейший <w>памятник</w> письменности.",
                            "target_lang_translations": {
                                "en": "Ancient chronicles are a most important <w>monument</w> of literature."
                            }
                        }
                    ],
                    "common_phrases": [
                        "памятник архитектуры",
                        "памятник культуры",
                        "памятник старины",
                        "литературный памятник"
                    ],
                    "target_lang_definitions": {
                        "en": "An object or site preserved as evidence of history or part of cultural heritage."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monument",
                                "target_lang_sense_clarification": "historical or cultural record"
                            }
                        ]
                    }
                }
            ]
        }
    ]

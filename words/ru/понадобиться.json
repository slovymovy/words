{
    "word_family": [
        "надобность",
        "надо"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "703e31fb-0551-3589-ab09-b3ca3d35bdd4",
                    "sense_definition": "Стать нужным, необходимым для кого-либо или чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "need_necessity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для этого нам <w>понадобится</w> некоторое время.",
                            "target_lang_translations": {
                                "en": "We will <w>need</w> some time for this."
                            }
                        },
                        {
                            "text": "Мне срочно <w>понадобилась</w> твоя помощь в этом деле.",
                            "target_lang_translations": {
                                "en": "I urgently <w>needed</w> your help in this matter."
                            }
                        }
                    ],
                    "synonyms": [
                        "потребоваться"
                    ],
                    "common_phrases": [
                        "может понадобиться",
                        "если понадобится",
                        "срочно понадобиться",
                        "вновь понадобиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To become necessary or required for someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "need",
                                "target_lang_sense_clarification": "to require something (subject usually changes to the person needing)"
                            },
                            {
                                "target_lang_word": "be needed",
                                "target_lang_sense_clarification": "passive state of necessity"
                            },
                            {
                                "target_lang_word": "require",
                                "target_lang_sense_clarification": "formal necessity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03a313bb-7192-3b1b-b9c9-2115e54c898f",
                    "sense_definition": "Оказаться полезным, пригодным для использования в каком-либо деле.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "utility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Возьми с собой зонт, он может тебе <w>понадобиться</w>.",
                            "target_lang_translations": {
                                "en": "Take an umbrella with you, you might <w>need</w> it."
                            }
                        },
                        {
                            "text": "Этот старый ключ ещё может нам <w>понадобиться</w>.",
                            "target_lang_translations": {
                                "en": "This old key might still <w>come in handy</w> for us."
                            }
                        }
                    ],
                    "synonyms": [
                        "пригодиться"
                    ],
                    "common_phrases": [
                        "вдруг понадобится",
                        "ещё понадобится"
                    ],
                    "target_lang_definitions": {
                        "en": "To turn out to be useful or suitable for use in a particular situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come in handy",
                                "target_lang_sense_clarification": "to prove useful"
                            },
                            {
                                "target_lang_word": "be useful",
                                "target_lang_sense_clarification": "utility"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "frequency": "Middle",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biblioteka gromadzi stare <w>druki</w> z minionych stuleci.",
                            "target_lang_translations": {
                                "ru": "Библиотека собирает старинные <w>печатные издания</w> прошлых веков."
                            }
                        },
                        {
                            "text": "Na stole leżały rozrzucone <w>druki</w> reklamowe.",
                            "target_lang_translations": {
                                "ru": "На столе были разбросаны рекламные <w>буклеты</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zbiór druków",
                        "druki ulotne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, на котором напечатан текст или изображение, например книга, газета или листовка."

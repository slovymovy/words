                    ],
                    "synonyms": [
                        "themself"
                    ],
                    "common_phrases": [
                        "by themselves"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвратная форма местоимения they (они), относящаяся к одному человеку, когда пол неизвестен или не указан."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "возвратное местоимение"
                            },

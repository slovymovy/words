            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9e80dcb5-ae2c-3853-9c93-69e525ad61b6",
                    "sense_definition": "Держаться на поверхности воды или передвигаться в ней (о людях, животных и предметах).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом мы часто ездим на море и <плаваем> там целый день."
                        },
                        {
                            "text": "В тарелке с супом <плавали> крупные куски картофеля."
                        }
                    ],
                    "synonyms": [
                        "купаться"
                    ],
                    "antonyms": [
                        "тонуть",
                        "идти ко дну"
                    ],
                    "common_phrases": [
                        "уметь плавать",
                        "быстро плавать",
                        "плавать в реке",
                        "плавать на спине"
                    ]
                },
                {
                    "sense_id": "f87c0a2f-43a0-3d68-b5ce-a57060fa3081",
                    "sense_definition": "Заниматься плаванием как видом спорта или физическим упражнением.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат профессионально <плавает> и участвует в соревнованиях."
                        },
                        {
                            "text": "Врач посоветовал мне <плавать> в бассейне два раза в неделю."
                        }
                    ],
                    "common_phrases": [
                        "любить плавать",
                        "ходить плавать",
                        "научиться плавать"
                    ]
                },
                {
                    "sense_id": "0900cf8a-8a22-33e9-81f4-81019e3f2dcd",
                    "sense_definition": "Совершать рейсы, путешествовать или передвигаться по воде (о судах и моряках).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот старый капитан <плавал> на торговых судах по всему миру."
                        },
                        {
                            "text": "Огромный белый паром <плавает> между островами каждый день."
                        }
                    ],
                    "synonyms": [
                        "ходить"
                    ],
                    "common_phrases": [
                        "плавать на корабле",
                        "плавать в море",
                        "долго плавать"
                    ]
                },
                {
                    "sense_id": "f2bcddd0-d04a-3770-a264-83b7535dfb34",
                    "sense_definition": "Плавно и свободно перемещаться в воздухе.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "movement_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В голубом небе медленно <плавали> лёгкие белые облака."
                        },
                        {
                            "text": "Густой сигаретный дым <плавал> под потолком комнаты."
                        }
                    ],
                    "synonyms": [
                        "парить"
                    ],
                    "common_phrases": [
                        "облака плавают",
                        "плавать в воздухе"
                    ]
                },
                {
                    "sense_id": "3455d0de-ab5c-311f-a90f-310e5dade1f5",
                    "sense_definition": "Постоянно меняться или колебаться (о ценах, курсе валют, графике).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fluctuation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Курс доллара постоянно <плавает>, поэтому сложно планировать бюджет."
                        },
                        {
                            "text": "У нас на работе <плавает> график, и я прихожу в разное время."
                        }
                    ],
                    "synonyms": [
                        "колебаться",
                        "меняться",
                        "изменяться"
                    ],
                    "antonyms": [
                        "сохраняться"
                    ],
                    "common_phrases": [

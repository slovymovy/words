                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our team won the <w>away</w> game yesterday.",
                            "target_lang_translations": {
                                "ru": "Наша команда выиграла <w>выездную</w> игру вчера."
                            }
                        },
                        {
                            "text": "This is the entrance for the <w>away</w> supporters.",
                            "target_lang_translations": {
                                "ru": "Это вход для <w>гостевых</w> болельщиков."
                            }
                        }
                    ],
                    "antonyms": [

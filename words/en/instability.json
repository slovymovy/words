    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ba5c4c86-ed03-31e5-bbbc-2988ee76fd8a",
                    "sense_definition": "The quality or state of being likely to change, fail, or give way.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The country is facing a period of political <w>instability</w> following the election.",
                            "target_lang_translations": {
                                "ru": "Страна переживает период политической <w>нестабильности</w> после выборов."
                            }
                        },
                        {
                            "text": "Economic <w>instability</w> often discourages foreign investment and slows growth.",
                            "target_lang_translations": {
                                "ru": "Экономическая <w>нестабильность</w> часто отпугивает иностранные инвестиции и замедляет рост."
                            }
                        }
                    ],
                    "synonyms": [
                        "volatility"
                    ],
                    "antonyms": [
                        "stability",
                        "steadiness"
                    ],
                    "common_phrases": [
                        "political instability",
                        "economic instability",
                        "emotional instability"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство или состояние, характеризующееся отсутствием стабильности, постоянства или твердости; склонность к переменам или разрушению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нестабильность",
                                "target_lang_sense_clarification": "общее значение; часто используется в политическом или экономическом контексте"
                            },
                            {
                                "target_lang_word": "неустойчивость",
                                "target_lang_sense_clarification": "шаткость, склонность к колебаниям или изменениям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07064c62-4867-3382-a15f-d12c5e1337ba",
                    "sense_definition": "A state in which a system is not in equilibrium or where a small change causes a large effect.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineer warned about the structural <w>instability</w> of the old bridge.",
                            "target_lang_translations": {
                                "ru": "Инженер предупредил о структурной <w>неустойчивости</w> старого моста."
                            }
                        },
                        {
                            "text": "Atmospheric <w>instability</w> can cause sudden and severe thunderstorms.",
                            "target_lang_translations": {
                                "ru": "Атмосферная <w>неустойчивость</w> может вызвать внезапные и сильные грозы."
                            }
                        }
                    ],
                    "antonyms": [
                        "equilibrium",
                        "balance"
                    ],
                    "common_phrases": [
                        "atmospheric instability",
                        "structural instability"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние физической системы, которая не находится в равновесии или где малые изменения вызывают значительный эффект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неустойчивость",
                                "target_lang_sense_clarification": "физическое состояние, отсутствие равновесия"
                            },
                            {
                                "target_lang_word": "нестабильность",
                                "target_lang_sense_clarification": "в технических и научных контекстах"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2371ac6e-13ec-32cd-a047-684cd5c2889d",
                    "sense_definition": "Relating to a single person or thing as opposed to more than one.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "adj_single",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher tries to give <w>individual</w> attention to every student in the class.",
                            "target_lang_translations": {
                                "ru": "Учитель старается уделить <w>индивидуальное</w> внимание каждому ученику в классе."
                            }
                        },
                        {
                            "text": "We must look at each <w>individual</w> case to understand the problem fully.",
                            "target_lang_translations": {
                                "ru": "Мы должны рассмотреть каждый <w>отдельный</w> случай, чтобы полностью понять проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "single#Adjective"
                    ],
                    "antonyms": [
                        "collective#Adjective",
                        "joint#Adjective"
                    ],
                    "common_phrases": [
                        "individual needs",
                        "individual attention",
                        "individual rights",
                        "individual case"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к одному человеку или предмету, в отличие от группы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "индивидуальный",
                                "target_lang_sense_clarification": "свойственный данному лицу"
                            },
                            {
                                "target_lang_word": "отдельный",
                                "target_lang_sense_clarification": "обособленный, единичный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35f5ca35-766b-3d44-a7f1-34600907598e",
                    "sense_definition": "Intended for or used by a single person.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_single",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chef prepared an <w>individual</w> pie for each guest at the dinner party.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар приготовил <w>отдельный</w> пирог для каждого гостя на званом ужине."
                            }
                        },
                        {
                            "text": "This software license is for <w>individual</w> use only and cannot be shared.",
                            "target_lang_translations": {
                                "ru": "Эта лицензия на программное обеспечение предназначена только для <w>личного</w> использования и не подлежит передаче."
                            }
                        }
                    ],
                    "synonyms": [
                        "personal#Adjective"
                    ],
                    "antonyms": [
                        "shared#Adjective"
                    ],
                    "common_phrases": [
                        "individual portion",
                        "individual use"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предназначенный для использования одним человеком"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "личный",
                                "target_lang_sense_clarification": "принадлежащий самому человеку"
                            },
                            {
                                "target_lang_word": "индивидуальный",
                                "target_lang_sense_clarification": "персональный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "05e141f6-e7b3-3be5-92f8-7d1d1536e381",
                    "sense_definition": "A person considered alone, rather than as belonging to a group of people.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Every <w>individual</w> has the right to express their own opinion freely.",
                            "target_lang_translations": {
                                "ru": "Каждая <w>личность</w> имеет право свободно выражать свое мнение."
                            }
                        },
                        {
                            "text": "He is a very talented <w>individual</w> who works well on his own.",
                            "target_lang_translations": {
                                "ru": "Он очень талантливая <w>личность</w>, которая хорошо работает самостоятельно."
                            }
                        }
                    ],
                    "antonyms": [
                        "group#Noun"
                    ],
                    "common_phrases": [
                        "private individual",
                        "talented individual",
                        "rugged individual"
                    ],
                    "target_lang_definitions": {

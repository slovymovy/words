    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e8d515c6-3c02-3a28-bb76-951113fcf2d9",
                    "sense_definition": "Proces zdobywania wiedzy, umiejętności i poznawania świata.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "education_knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jutro mam sprawdzian, więc czeka mnie dużo <nauki>.",
                            "target_lang_translations": {
                                "ru": "Завтра у меня контрольная, так что меня ждёт много <учёбы>."
                            }
                        },
                        {
                            "text": "<Nauka> języka polskiego sprawia mi dużą przyjemność.",
                            "target_lang_translations": {
                                "ru": "<Изучение> польского языка доставляет мне большое удовольствие."
                            }
                        }
                    ],
                    "synonyms": [
                        "uczenie się",
                        "sztuczka"
                    ],
                    "common_phrases": [
                        "nauka języka",
                        "nauka jazdy",
                        "chęć nauki",
                        "lata nauki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс получения знаний, навыков и познания мира."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "учёба",
                                "target_lang_sense_clarification": "процесс обучения, занятия"
                            },
                            {
                                "target_lang_word": "изучение",
                                "target_lang_sense_clarification": "постижение конкретного предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc683966-05a1-3fe9-a8de-12f636d3e981",
                    "sense_definition": "Uporządkowany system wiedzy o rzeczywistości, wynikający z badań i obserwacji; dyscyplina badawcza.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "education_knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Medycyna to bardzo trudna i wymagająca <nauka>.",
                            "target_lang_translations": {
                                "ru": "Медицина — это очень трудная и требовательная <наука>."
                            }
                        },
                        {
                            "text": "Współczesna <nauka> rozwija się w bardzo szybkim tempie.",
                            "target_lang_translations": {
                                "ru": "Современная <наука> развивается очень быстрыми темпами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nauki ścisłe",
                        "nauki humanistyczne",
                        "świat nauki",
                        "rozwój nauki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упорядоченная система знаний о действительности; исследовательская дисциплина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наука",
                                "target_lang_sense_clarification": "система знаний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1023a13-04d4-3b38-ba12-60008d805043",
                    "sense_definition": "Czynność przekazywania wiedzy innym osobom; praca nauczyciela.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "education_knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej pasją jest <nauka> dzieci w szkole podstawowej.",
                            "target_lang_translations": {
                                "ru": "Её страсть — <обучение> детей в начальной школе."
                            }
                        },
                        {
                            "text": "Efektywna <nauka> wymaga od nauczyciela wiele cierpliwości.",
                            "target_lang_translations": {
                                "ru": "Эффективное <преподавание> требует от учителя большого терпения."
                            }
                        }
                    ],
                    "synonyms": [
                        "uczenie"
                    ],
                    "common_phrases": [
                        "metody nauki",
                        "poświęcić się nauce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность по передаче знаний другим людям; работа учителя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обучение",
                                "target_lang_sense_clarification": "процесс передачи знаний"
                            },
                            {
                                "target_lang_word": "преподавание",
                                "target_lang_sense_clarification": "профессиональная деятельность учителя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8032ddb-e45b-3369-b03a-3fe3f330a1bf",
                    "sense_definition": "Wniosek wyciągnięty z jakiegoś wydarzenia lub doświadczenia; przestroga na przyszłość.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "moral_lesson",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To była dla mnie bolesna <nauka>, żeby nie ufać nieznajomym.",
                            "target_lang_translations": {
                                "ru": "Это был для меня болезненный <урок>, чтобы не доверять незнакомцам."
                            }
                        },
                        {
                            "text": "Mam nadzieję, że ta sytuacja będzie dla ciebie <nauką> na przyszłość.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, эта ситуация будет тебе <наукой> на будущее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nauka na przyszłość",
                        "wyciągnąć naukę",
                        "gorzka nauka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вывод, сделанный из какого-либо события или опыта; предостережение на будущее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "урок",
                                "target_lang_sense_clarification": "жизненный опыт, вывод"
                            },
                            {
                                "target_lang_word": "наука",
                                "target_lang_sense_clarification": "в значении \"урок на будущее\""
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0e8ea62-d10f-3fce-a14a-d04f83c12ec3",
                    "sense_definition": "Zbiór poglądów, zasad lub dogmatów, często religijnych lub filozoficznych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "doctrine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Nauka> tego filozofa wpłynęła na całe pokolenie myślicieli.",
                            "target_lang_translations": {
                                "ru": "<Учение> этого философа повлияло на целое поколение мыслителей."
                            }
                        },
                        {
                            "text": "Ksiądz wyjaśniał wiernym <naukę> Kościoła na temat rodziny.",
                            "target_lang_translations": {
                                "ru": "Священник объяснял верующим <учение> Церкви о семье."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nauka kościoła",
                        "nauka społeczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность взглядов, принципов или догматов, часто религиозных или философских."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "учение",
                                "target_lang_sense_clarification": "система взглядов или догматов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5f46458-5701-3a76-9fe8-a98be0187e01",
                    "sense_definition": "Przemowa religijna, kazanie lub cykl spotkań przygotowujących do sakramentu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "doctrine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Narzeczeni muszą odbyć obowiązkowe <nauki> przed ślubem.",
                            "target_lang_translations": {
                                "ru": "Жених и невеста должны пройти обязательные <курсы> перед венчанием."
                            }
                        },
                        {
                            "text": "Wierni w skupieniu słuchali <nauki> rekolekcyjnej w kościele.",
                            "target_lang_translations": {
                                "ru": "Верующие сосредоточенно слушали реколлекционную <проповедь> в церкви."
                            }
                        }
                    ],
                    "synonyms": [
                        "kazanie",
                        "katecheza"
                    ],
                    "common_phrases": [
                        "nauki przedmałżeńskie",
                        "nauki rekolekcyjne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Религиозная проповедь или цикл встреч для подготовки к таинству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проповедь",
                                "target_lang_sense_clarification": "речь священника"
                            },
                            {
                                "target_lang_word": "курсы",
                                "target_lang_sense_clarification": "подготовительные занятия (в церкви)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
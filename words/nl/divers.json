{
    "word_family": [
        "diversiteit",
        "diversificatie",
        "diversifiëren"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "05e2d9cc-927a-30a3-b4ac-e332f064716f",
                    "sense_definition": "Twee of meer; een onbepaald aantal.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb je <w>diverse</w> keren proberen te bellen maar je nam niet op.",
                            "target_lang_translations": {
                                "en": "I tried to call you <w>several</w> times but you did not pick up.",
                                "ru": "Я пытался дозвониться до тебя <w>несколько</w> раз, но ты не брал трубку."
                            }
                        },
                        {
                            "text": "Er zijn <w>diverse</w> mogelijkheden om dit probleem op te lossen.",
                            "target_lang_translations": {
                                "en": "There are <w>various</w> possibilities to solve this problem.",
                                "ru": "Существуют <w>различные</w> возможности для решения этой проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "verscheidene",
                        "meerdere",
                        "enige"
                    ],
                    "common_phrases": [
                        "diverse soorten",
                        "om diverse redenen",
                        "op diverse plaatsen"
                    ],
                    "target_lang_definitions": {
                        "en": "More than one; an indefinite number.",
                        "ru": "Два или более; неопределенное количество."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "several",
                                "target_lang_sense_clarification": "Being of a number more than two or three, but not many"
                            },
                            {
                                "target_lang_word": "various",
                                "target_lang_sense_clarification": "Several different; an indefinite number of separate items"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "различные",
                                "target_lang_sense_clarification": "разные, всевозможные"
                            },
                            {
                                "target_lang_word": "несколько",
                                "target_lang_sense_clarification": "некоторое количество"
                            },
                            {
                                "target_lang_word": "разные",
                                "target_lang_sense_clarification": "отличающиеся друг от друга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ddb3b98d-a053-30bc-90d0-f1a98595835b",
                    "sense_definition": "Onderling duidelijk verschillend; van uiteenlopende aard.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De redenen voor mijn besluit zijn erg <w>divers</w>.",
                            "target_lang_translations": {
                                "en": "The reasons for my decision are very <w>diverse</w>.",
                                "ru": "Причины моего решения весьма <w>разнообразны</w>."
                            }
                        },
                        {
                            "text": "Het restaurant biedt een <w>divers</w> menu met gerechten uit de hele wereld.",
                            "target_lang_translations": {
                                "en": "The restaurant offers a <w>diverse</w> menu with dishes from around the world.",
                                "ru": "Ресторан предлагает <w>разнообразное</w> меню с блюдами со всего мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "gevarieerd",
                        "uiteenlopend",
                        "verschillend",
                        "verscheiden"
                    ],
                    "common_phrases": [
                        "heel divers",
                        "een divers aanbod"
                    ],
                    "target_lang_definitions": {
                        "en": "Mutually clearly different; of a varied nature.",
                        "ru": "Явно отличающиеся друг от друга; разного характера."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "diverse",
                                "target_lang_sense_clarification": "Showing a great deal of variety; very different"
                            },
                            {
                                "target_lang_word": "varied",
                                "target_lang_sense_clarification": "Incorporating a number of different types or elements"
                            },
                            {
                                "target_lang_word": "various",
                                "target_lang_sense_clarification": "Having a broad range of different elements"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "разнообразный",
                                "target_lang_sense_clarification": "богатый различиями, вариативный"
                            },
                            {
                                "target_lang_word": "разнородный",
                                "target_lang_sense_clarification": "состоящий из неодинаковых частей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20c093a6-0184-350c-bbac-53e0d74861e6",
                    "sense_definition": "Gekenmerkt door de aanwezigheid van mensen met verschillende achtergronden, zoals etniciteit, geslacht of geaardheid.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We leven tegenwoordig in een zeer <w>diverse</w> samenleving.",
                            "target_lang_translations": {
                                "en": "We live in a very <w>diverse</w> society nowadays.",
                                "ru": "Сегодня мы живем в очень <w>разнообразном</w> обществе."
                            }
                        },
                        {
                            "text": "Het bedrijf streeft naar een <w>divers</w> team dat een afspiegeling is van de maatschappij.",
                            "target_lang_translations": {
                                "en": "The company strives for a <w>diverse</w> team that is a reflection of society.",
                                "ru": "Компания стремится создать <w>разнообразную</w> команду, которая является отражением общества."
                            }
                        }
                    ],
                    "common_phrases": [
                        "diverse samenleving",
                        "diverse groep"
                    ],
                    "target_lang_definitions": {
                        "en": "Characterized by the presence of people with different backgrounds, such as ethnicity, gender, or orientation.",
                        "ru": "Характеризующийся наличием людей с разным происхождением, например, этническим, половым или культурным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "diverse",
                                "target_lang_sense_clarification": "Including people from a range of different social and ethnic backgrounds"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "разнообразный",
                                "target_lang_sense_clarification": "включающий разные социальные группы"
                            },
                            {
                                "target_lang_word": "многообразный",
                                "target_lang_sense_clarification": "проявляющийся во многих видах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
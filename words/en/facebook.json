            "senses": [
                {
                    "sense_id": "07653696-ae76-300d-a369-714803d248f8",
                    "sense_definition": "A social-networking website where users can find acquaintances or friends, exchange messages, and post updates.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "platform",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "Mia was just seeing a reel on <w>Facebook</w> that her friend had posted.",
                            "target_lang_translations": {
                                "ru": "Мия как раз смотрела рилс в <w>Фейсбуке</w>, который запостила её подруга."
                            }
                        },
                        {
                            "text": "Many businesses use <w>Facebook</w> to advertise their products to a local audience.",
                            "target_lang_translations": {
                                "ru": "Многие компании используют <w>Фейсбук</w> для рекламы своих продуктов местной аудитории."
                            }
                        }
                    ],
                    "synonyms": [
                        "FB"
                    ],
                    "common_phrases": [
                        "on Facebook",
                        "Facebook account",
                        "Facebook page",
                        "Facebook group"
                    ],
                    "target_lang_definitions": {
                        "ru": "Социальная сеть, где пользователи могут находить знакомых, обмениваться сообщениями и публиковать новости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Фейсбу́к",
                                "target_lang_sense_clarification": "социальная сеть"
                            },
                            {
                                "target_lang_word": "Фе́йсбук",
                                "target_lang_sense_clarification": "вариант произношения"
                            }
                        ]

    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "69aea31b-7654-3b0c-b52f-5f735ef2d932",
                    "sense_definition": "Forma narzędnika zaimka <w>kto</w>, używana w pytaniach o tożsamość, zawód lub rolę oraz po przyimkach łączących się z tym przypadkiem (np. z, przed, za).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wiem, <w>kim</w> jest ten człowiek stojący przy bramie.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, <w>кто</w> этот человек, стоящий у ворот."
                            }
                        },
                        {
                            "text": "Z <w>kim</w> planujesz spędzić tegoroczne wakacje?",
                            "target_lang_translations": {
                                "ru": "С <w>кем</w> ты планируешь провести этот отпуск?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "z kim",
                        "przed kim",
                        "za kim",
                        "kim jesteś"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "narzędnik"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Творительный падеж местоимения «kto» (кто), используемый в вопросах о личности, профессии или роли, а также после предлогов (с, перед, за)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кем",
                                "target_lang_sense_clarification": "творительный падеж местоимения кто"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4ee33c8-ef96-32df-a2dd-f8127d6235c2",
                    "sense_definition": "Forma miejscownika zaimka <w>kto</w>, używana po przyimkach takich jak o, w, przy, na.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "O <w>kim</w> rozmawialiście podczas wczorajszego spotkania?",
                            "target_lang_translations": {
                                "ru": "О <w>ком</w> вы говорили во время вчерашней встречи?"
                            }
                        },
                        {
                            "text": "W <w>kim</w> pokładasz największe nadzieje na sukces?",
                            "target_lang_translations": {
                                "ru": "На <w>кого</w> ты возлагаешь самые большие надежды на успех?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "o kim",
                        "przy kim",
                        "w kim",
                        "na kim"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "miejscownik"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предложный падеж местоимения «kto» (кто), используемый после предлогов, таких как о, в, при, на."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ком",
                                "target_lang_sense_clarification": "предложный падеж местоимения кто"
                            }
                        ]
                    }
                }
            ]
        },
        {

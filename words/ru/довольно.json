{
    "word_family": [
        "довольный",
        "довольство",
        "удовольствие"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e16961b7-1b71-3391-b8fb-e53a5d3c9bfe",
                    "sense_definition": "В значительной степени, порядочно.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот фильм оказался <w>довольно</w> интересным и поучительным.",
                            "target_lang_translations": {
                                "en": "This movie turned out to be <w>quite</w> interesting and instructive."
                            }
                        },
                        {
                            "text": "Мы живём <w>довольно</w> далеко от центра города.",
                            "target_lang_translations": {
                                "en": "We live <w>quite</w> far from the city center."
                            }
                        }
                    ],
                    "synonyms": [
                        "удовлетворённо",
                        "радостно"
                    ],
                    "common_phrases": [
                        "довольно часто",
                        "довольно много",
                        "довольно хорошо",
                        "довольно долго",
                        "довольно поздно"
                    ],
                    "target_lang_definitions": {
                        "en": "To a significant degree; considerably."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quite",
                                "target_lang_sense_clarification": "to a significant extent"
                            },
                            {
                                "target_lang_word": "rather",
                                "target_lang_sense_clarification": "somewhat; to a certain degree"
                            },
                            {
                                "target_lang_word": "pretty",
                                "target_lang_sense_clarification": "informal; to a moderately high degree"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf1b622a-1306-3c81-b857-81129f7e8013",
                    "sense_definition": "С чувством удовлетворения, с довольным видом.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кот съел всё угощение и <w>довольно</w> замурлыкал.",
                            "target_lang_translations": {
                                "en": "The cat ate all the treat and purred <w>contentedly</w>."
                            }
                        },
                        {
                            "text": "Она <w>довольно</w> улыбнулась, услышав похвалу в свой адрес.",
                            "target_lang_translations": {
                                "en": "She smiled <w>contentedly</w> upon hearing the praise directed at her."
                            }
                        }
                    ],
                    "synonyms": [
                        "удовлетворённо",
                        "радостно"
                    ],
                    "antonyms": [
                        "недовольно",
                        "сердито"
                    ],
                    "common_phrases": [
                        "довольно улыбаться",
                        "довольно кивнуть",
                        "довольно вздохнуть"
                    ],
                    "target_lang_definitions": {
                        "en": "With a feeling of satisfaction; in a pleased manner."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "contentedly",
                                "target_lang_sense_clarification": "in a satisfied or happy way"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cfa5730-b475-321b-9fdf-b754de9fbe04",
                    "sense_definition": "В достаточном количестве; столько, сколько нужно.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этого количества еды нам будет <w>довольно</w> на целую неделю.",
                            "target_lang_translations": {
                                "en": "This amount of food will be <w>enough</w> for us for a whole week."
                            }
                        },
                        {
                            "text": "У нас <w>довольно</w> времени, чтобы всё успеть.",
                            "target_lang_translations": {
                                "en": "We have <w>enough</w> time to get everything done."
                            }
                        }
                    ],
                    "synonyms": [
                        "удовлетворённо",
                        "радостно"
                    ],
                    "common_phrases": [
                        "более чем довольно"
                    ],
                    "target_lang_definitions": {
                        "en": "In sufficient quantity; as much as is needed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enough",
                                "target_lang_sense_clarification": "sufficiently; as much as required"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "7494a388-fb84-3f36-84f4-00a44138213a",
                    "sense_definition": "Возглас, выражающий требование прекратить какое-либо действие; хватит.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Довольно</w> спорить, давайте лучше займёмся делом!",
                            "target_lang_translations": {
                                "en": "<w>Enough</w> arguing, let's get to work instead!"
                            }
                        },
                        {
                            "text": "С меня <w>довольно</w>, я ухожу домой.",
                            "target_lang_translations": {
                                "en": "I've had <w>enough</w>, I'm going home."
                            }
                        }
                    ],
                    "synonyms": [
                        "удовлетворённо",
                        "радостно"
                    ],
                    "common_phrases": [
                        "с меня довольно"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation demanding to stop an action; that is enough."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enough",
                                "target_lang_sense_clarification": "exclamation to stop something"
                            },
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "command to cease an action"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "11a0c999-aa58-3042-b11c-4987600d3317",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>довольный</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальство осталось <w>довольно</w> результатами нашей работы.",
                            "target_lang_translations": {
                                "en": "The management remained <w>pleased</w> with the results of our work."
                            }
                        },
                        {
                            "text": "Его лицо было спокойно и совершенно <w>довольно</w>.",
                            "target_lang_translations": {
                                "en": "His face was calm and completely <w>content</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short neuter form of the adjective meaning pleased or satisfied."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pleased",
                                "target_lang_sense_clarification": "feeling satisfaction"
                            },
                            {
                                "target_lang_word": "satisfied",
                                "target_lang_sense_clarification": "contented with an outcome"
                            },
                            {
                                "target_lang_word": "content",
                                "target_lang_sense_clarification": "in a state of peaceful happiness"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
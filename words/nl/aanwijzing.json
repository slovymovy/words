                            },
                            {
                                "target_lang_word": "appointment",
                                "target_lang_sense_clarification": "selection for a position"
                            },
                            {
                                "target_lang_word": "indication",
                                "target_lang_sense_clarification": "act of pointing out"
                            }
                        ]
                    }
                }

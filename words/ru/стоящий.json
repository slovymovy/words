                        "нестоящий",
                        "двигающийся"
                    ],
                    "common_phrases": [
                        "отдельно стоящий",
                        "стоящий рядом",
                        "высоко стоящий"
                    ],
                    "target_lang_definitions": {
                        "en": "Active present participle of the verb 'to stand'; being in a vertical position or located somewhere."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "standing",
                                "target_lang_sense_clarification": "participle indicating position"
                            }
                        ]
                    }
                },
                {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "16bdb540-6824-3961-949e-98731f232420",
                    "sense_definition": "Wyciągnąć rękę lub przedmiot w jakimś kierunku, aby czegoś dotknąć, chwycić to lub podać.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musiałem stanąć na palcach, żeby <w>sięgnąć</w> po słoik na górnej półce.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось встать на цыпочки, чтобы <w>достать</w> банку с верхней полки."
                            }
                        },
                        {
                            "text": "Proszę <w>sięgnąć</w> do kieszeni i pokazać, co tam masz.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>залезь</w> в карман и покажи, что там у тебя."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyciągnąć rękę",
                        "chwycić"
                    ],
                    "common_phrases": [
                        "sięgnąć po książkę",
                        "sięgnąć do kieszeni",
                        "sięgnąć ręką",
                        "sięgnąć po telefon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Протянуть руку или предмет в каком-либо направлении, чтобы коснуться чего-либо, схватить или передать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потянуться",
                                "target_lang_sense_clarification": "за чем-то"
                            },
                            {
                                "target_lang_word": "достать",
                                "target_lang_sense_clarification": "до чего-то, взять"
                            },
                            {
                                "target_lang_word": "протянуть руку",
                                "target_lang_sense_clarification": "физическое действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "474603f7-d000-35d5-813e-029c331389a1",
                    "sense_definition": "Dotrzeć do pewnego poziomu, granicy, wartości lub miejsca.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "extent_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego frustracja <w>sięgnęła</w> zenitu, gdy kolejny raz odwołano spotkanie.",
                            "target_lang_translations": {
                                "ru": "Его разочарование <w>достигло</w> предела, когда встречу снова отменили."
                            }
                        },
                        {
                            "text": "Woda w rzece może <w>sięgnąć</w> wałów przeciwpowodziowych.",
                            "target_lang_translations": {
                                "ru": "Вода в реке может <w>дойти</w> до защитных дамб."
                            }
                        }
                    ],
                    "synonyms": [
                        "osiągnąć",
                        "dojść",
                        "dotrzeć"
                    ],
                    "common_phrases": [
                        "sięgnąć zenitu",
                        "sięgnąć dna",
                        "sięgnąć bruku",
                        "sięgnąć celu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дойти до определенного уровня, границы, значения или места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достигнуть",
                                "target_lang_sense_clarification": "уровня, предела"
                            },
                            {
                                "target_lang_word": "дойти",
                                "target_lang_sense_clarification": "до границы, до места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8987912-5ed5-38b3-8217-77b5a3a88ce6",
                    "sense_definition": "Zdecydować się na wykorzystanie czegoś lub czyjejś pomocy, często w sytuacji potrzeby.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_use",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W trudnych chwilach nie warto <w>sięgać</w> po alkohol.",
                            "target_lang_translations": {
                                "ru": "В трудные моменты не стоит <w>прибегать</w> к алкоголю."
                            }
                        },
                        {
                            "text": "Policja musiała <w>sięgnąć</w> po broń, aby zatrzymać uciekającego przestępcę.",
                            "target_lang_translations": {
                                "ru": "Полиции пришлось <w>применить</w> оружие, чтобы остановить убегающего преступника."
                            }
                        }
                    ],
                    "synonyms": [
                        "skorzystać",
                        "użyć",
                        "wykorzystać"
                    ],
                    "common_phrases": [
                        "sięgnąć po środki",
                        "sięgnąć po radę",
                        "sięgnąć po broń",
                        "sięgnąć po pomoc"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решить использовать что-либо или чью-либо помощь, часто в случае необходимости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прибегнуть",
                                "target_lang_sense_clarification": "к помощи, к средствам"
                            },
                            {
                                "target_lang_word": "воспользоваться",
                                "target_lang_sense_clarification": "чем-либо"
                            },
                            {
                                "target_lang_word": "взяться",
                                "target_lang_sense_clarification": "за что-то (напр. за оружие)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ace1692c-8cf8-3728-b183-cf23cee42d40",
                    "sense_definition": "Odnieść się do wydarzeń z przeszłości lub sprawdzić coś w źródłach historycznych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "time_memory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aby zrozumieć ten konflikt, trzeba <w>sięgnąć</w> do wydarzeń sprzed wieku.",
                            "target_lang_translations": {
                                "ru": "Чтобы понять этот конфликт, нужно <w>обратиться</w> к событиям столетней давности."
                            }
                        },
                        {
                            "text": "Lubię czasem <w>sięgnąć</w> pamięcią do beztroskich czasów dzieciństwa.",
                            "target_lang_translations": {
                                "ru": "Я люблю иногда <w>вернуться</w> памятью в беззаботные времена детства."
                            }
                        }
                    ],
                    "synonyms": [
                        "odwołać się",
                        "wrócić"
                    ],
                    "common_phrases": [
                        "sięgnąć pamięcią",
                        "sięgnąć do źródeł",
                        "sięgnąć do historii",
                        "sięgnąć do korzeni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обратиться к событиям прошлого или проверить что-либо в исторических источниках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обратиться",
                                "target_lang_sense_clarification": "к источникам, к истории"
                            },
                            {
                                "target_lang_word": "вернуться",
                                "target_lang_sense_clarification": "памятью, мыслями"
                            }
                        ]
                    }
                }
            ]
        }

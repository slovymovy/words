                {
                    "sense_id": "a87d690b-11fb-340b-8342-996c12705b4a",
                    "sense_definition": "De weg of route die afgelegd wordt van het ene punt naar het andere.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_path",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De trein stopt nergens op dit <w>traject</w>.",
                            "target_lang_translations": {
                                "en": "The train does not stop anywhere on this <w>route</w>.",
                                "ru": "Поезд нигде не останавливается на этом <w>маршруте</w>."
                            }
                        },
                        {
                            "text": "Tijdens het <w>traject</w> van Amsterdam naar Parijs zagen we mooie landschappen.",
                            "target_lang_translations": {
                                "en": "During the <w>journey</w> from Amsterdam to Paris, we saw beautiful landscapes.",
                                "ru": "На <w>пути</w> из Амстердама в Париж мы видели красивые пейзажи."
                            }
                        }
                    ],
                    "synonyms": [
                        "route",
                        "weg",
                        "parcours"
                    ],
                    "common_phrases": [
                        "het hele traject",
                        "een vast traject"
                    ],
                    "target_lang_definitions": {
                        "en": "The path or route traveled from one point to another.",
                        "ru": "Путь или маршрут, пролегающий от одной точки до другой."
                    },

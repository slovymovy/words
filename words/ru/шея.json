            "senses": [
                {
                    "sense_id": "15d41fd6-79e6-35aa-8c29-44b9db899bfd",
                    "sense_definition": "Часть тела, соединяющая голову с туловищем.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У жирафа очень длинная и сильная <w>шея</w>.",
                            "target_lang_translations": {
                                "en": "The giraffe has a very long and strong <w>neck</w>."
                            }
                        },
                        {
                            "text": "В холодную погоду нужно обязательно повязывать шарф на <w>шею</w>.",
                            "target_lang_translations": {
                                "en": "In cold weather, you must tie a scarf around your <w>neck</w>."
                            }
                        },
                        {
                            "text": "Мой брат не хочет работать и продолжает сидеть на <w>шее</w> у родителей.",
                            "target_lang_translations": {
                                "en": "My brother doesn't want to work and continues to sit on his parents' <w>neck</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "выя"
                    ],
                    "common_phrases": [
                        "сидеть на шее",
                        "свернуть шею",
                        "вешаться на шею",
                        "гнать в шею",
                        "по шею"
                    ],
                    "target_lang_definitions": {
                        "en": "The part of the body connecting the head to the trunk."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "neck",
                                "target_lang_sense_clarification": "anatomy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9566ee1-5adc-3ec5-836d-2842f8109075",
                    "sense_definition": "Длинная и узкая часть предмета, соединяющая его верхнюю часть с основанием.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Над строительной площадкой возвышалась металлическая <w>шея</w> башенного крана.",
                            "target_lang_translations": {
                                "en": "The metal <w>neck</w> of a tower crane rose above the construction site."
                            }
                        },
                        {
                            "text": "Старинная ваза имела хрупкую и изящную <w>шею</w>.",
                            "target_lang_translations": {
                                "en": "The antique vase had a fragile and elegant <w>neck</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A long and narrow part of an object connecting its top to its base."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "neck",
                                "target_lang_sense_clarification": "of an object"
                            }
                        ]
                    }
                }

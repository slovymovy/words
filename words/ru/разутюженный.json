                    ],
                    "common_phrases": [
                        "разутюженный шов",
                        "разутюженный припуск"
                    ],
                    "target_lang_definitions": {
                        "en": "Regarding a seam: one where the fabric allowances are spread in different directions and fixed with an iron."
                    },
                    "translations": {
                        "en": [

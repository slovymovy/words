            "pos": "noun",
            "senses": [
                {
                    "sense_id": "31429ba4-f5ce-3296-841d-e843c8f3be67",
                    "sense_definition": "Документ, дающий право на получение товара, услуги, посещение врача или проезд.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужно взять <талон> к стоматологу на завтра.",
                            "target_lang_translations": {
                                "en": "I need to get an appointment <ticket> to the dentist for tomorrow."
                            }
                        },
                        {
                            "text": "В регистратуре мне выдали <талон> с номером очереди.",
                            "target_lang_translations": {
                                "en": "At the reception desk, they gave me a <ticket> with a queue number."
                            }
                        }
                    ],
                    "synonyms": [
                        "квиток",
                        "талончик"
                    ],
                    "common_phrases": [
                        "талон к врачу",
                        "талон на питание",
                        "посадочный талон",
                        "взять талон"
                    ],
                    "target_lang_definitions": {
                        "en": "A document that gives the right to receive goods, services, see a doctor, or travel."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ticket",
                                "target_lang_sense_clarification": "for a queue or appointment"
                            },
                            {
                                "target_lang_word": "voucher",
                                "target_lang_sense_clarification": "for goods or services"
                            },
                            {
                                "target_lang_word": "coupon",
                                "target_lang_sense_clarification": "for rationing or discounts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "087e48b2-dbf7-3fc6-a8f0-902ba8aa80c3",
                    "sense_definition": "Отрывная часть документа или специальный бланк, служащий для контроля и отчётности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Продавец заполнил гарантийный <талон> на новый телевизор.",
                            "target_lang_translations": {

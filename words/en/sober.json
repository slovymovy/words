    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a89c6877-9528-33d0-831a-03febf606abf",
                    "sense_definition": "Not affected by alcohol; not drunk at the moment.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "alcohol_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was the only <w>sober</w> driver available to take us home.",
                            "target_lang_translations": {
                                "ru": "Он был единственным <w>трезвым</w> водителем, который мог отвезти нас домой."
                            }
                        },
                        {
                            "text": "You must be completely <w>sober</w> before you operate this machinery.",
                            "target_lang_translations": {
                                "ru": "Вы должны быть абсолютно <w>трезвы</w>, прежде чем управлять этим механизмом."
                            }
                        }
                    ],
                    "synonyms": [
                        "clearheaded",
                        "unintoxicated",
                        "stone-cold sober"
                    ],
                    "antonyms": [
                        "drunk",
                        "stoned"
                    ],
                    "common_phrases": [
                        "stone-cold sober",
                        "sober as a judge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не находящийся под воздействием алкоголя; не пьяный в данный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трезвый",
                                "target_lang_sense_clarification": "не пьяный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a91027b-d956-3c39-b1eb-fc29b232a44f",
                    "sense_definition": "Not drinking alcohol or using drugs, especially as a long-term lifestyle choice after addiction.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "alcohol_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has been <w>sober</w> for ten years and attends meetings regularly.",
                            "target_lang_translations": {
                                "ru": "Она остается <w>трезвой</w> уже десять лет и регулярно посещает собрания."
                            }
                        },
                        {
                            "text": "He is trying hard to stay <w>sober</w> despite the temptation.",
                            "target_lang_translations": {
                                "ru": "Он очень старается оставаться <w>трезвым</w>, несмотря на искушение."
                            }
                        }
                    ],
                    "synonyms": [
                        "teetotal",
                        "abstinent",
                        "clean",
                        "dry"
                    ],
                    "antonyms": [
                        "drunk"
                    ],
                    "common_phrases": [
                        "stay sober",
                        "clean and sober"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не употребляющий алкоголь или наркотики, особенно как осознанный выбор после зависимости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трезвый",
                                "target_lang_sense_clarification": "ведущий трезвый образ жизни"
                            },
                            {
                                "target_lang_word": "непьющий",
                                "target_lang_sense_clarification": "воздерживающийся от алкоголя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90a08e4b-7218-3c6b-9232-b58073d86c3d",
                    "sense_definition": "Serious, sensible, and solemn in manner or attitude.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "serious_demeanor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor gave a <w>sober</w> assessment of the patient's condition.",
                            "target_lang_translations": {
                                "ru": "Врач дал <w>трезвую</w> оценку состоянию пациента."
                            }
                        },
                        {
                            "text": "It was a <w>sober</w> gathering with very little laughter or conversation.",
                            "target_lang_translations": {
                                "ru": "Это было <w>сдержанное</w> собрание, почти без смеха и разговоров."
                            }
                        }
                    ],
                    "synonyms": [
                        "serious",
                        "grave",
                        "solemn",
                        "sedate",
                        "staid"
                    ],
                    "common_phrases": [
                        "sober truth",
                        "sober reflection",
                        "sober assessment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Серьезный, рассудительный и сдержанный в манерах или отношении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здравый",
                                "target_lang_sense_clarification": "рассудительный"
                            },
                            {
                                "target_lang_word": "трезвый",
                                "target_lang_sense_clarification": "реалистичный, объективный"
                            },
                            {
                                "target_lang_word": "серьезный",
                                "target_lang_sense_clarification": "сдержанный, не легкомысленный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1acf81fc-256c-358c-bd25-41506d15e335",
                    "sense_definition": "Plain and not bright or colorful; subdued.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "visual_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He dressed in a <w>sober</w> grey suit for the funeral.",
                            "target_lang_translations": {
                                "ru": "На похороны он надел <w>строгий</w> серый костюм."
                            }
                        },
                        {
                            "text": "The room was decorated in <w>sober</w> shades of brown and beige.",
                            "target_lang_translations": {
                                "ru": "Комната была оформлена в <w>неярких</w> тонах коричневого и бежевого."
                            }
                        }
                    ],
                    "synonyms": [
                        "dull",
                        "muted",
                        "subdued",
                        "somber"
                    ],
                    "common_phrases": [
                        "sober suit",
                        "sober colors"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простой, не яркий и не красочный; приглушенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неяркий",
                                "target_lang_sense_clarification": "о цвете"
                            },
                            {
                                "target_lang_word": "строгий",
                                "target_lang_sense_clarification": "об одежде или стиле"
                            },
                            {
                                "target_lang_word": "сдержанный",
                                "target_lang_sense_clarification": "умеренный, не кричащий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "54167c4d-e815-30bb-8f4c-0e9149aede80",
                    "sense_definition": "To become less drunk or to make someone less drunk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "alcohol_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A walk in the cold air helped to <w>sober</w> him.",
                            "target_lang_translations": {
                                "ru": "Прогулка на холодном воздухе помогла <w>отрезвить</w> его."
                            }
                        },
                        {
                            "text": "It took a few hours and a lot of coffee for him to <w>sober</w> up.",
                            "target_lang_translations": {
                                "ru": "Ему потребовалось несколько часов и много кофе, чтобы <w>протрезветь</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sober up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться менее пьяным или приводить кого-то в чувство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трезветь",
                                "target_lang_sense_clarification": "становиться трезвым"
                            },
                            {
                                "target_lang_word": "отрезвлять",
                                "target_lang_sense_clarification": "делать трезвым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d939124c-67e0-35d2-b416-eae428f40954",
                    "sense_definition": "To make someone feel serious or realistic about a situation.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "serious_demeanor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bad news <w>sobered</w> everyone in the room instantly.",
                            "target_lang_translations": {
                                "ru": "Плохие новости мгновенно <w>отрезвили</w> всех присутствующих."
                            }
                        },
                        {
                            "text": "Realizing the danger of the situation <w>sobered</w> the playful group.",
                            "target_lang_translations": {
                                "ru": "Осознание опасности ситуации <w>отрезвило</w> игривую группу."
                            }
                        }
                    ],
                    "synonyms": [
                        "subdue",
                        "calm"
                    ],
                    "common_phrases": [
                        "sobering thought"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то относиться к ситуации серьезно или реалистично."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрезвлять",
                                "target_lang_sense_clarification": "возвращать к реальности"
                            },
                            {
                                "target_lang_word": "остепенять",
                                "target_lang_sense_clarification": "делать более серьезным"
                            }
                        ]
                    }
                }
            ]
        },

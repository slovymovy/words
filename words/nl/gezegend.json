    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "759d0978-e011-3000-a529-c2a0932eb97d",
                    "sense_definition": "Gelukkig, voorspoedig en bevoorrecht, alsof men een gunst van God heeft ontvangen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "blessed_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij voelt zich <w>gezegend</w> met zijn goede gezondheid en fijne familie.",
                            "target_lang_translations": {
                                "en": "He feels <w>blessed</w> with his good health and fine family.",
                                "ru": "Он чувствует себя <w>одаренным</w> хорошим здоровьем и прекрасной семьей."
                            }
                        },
                        {
                            "text": "Wij wensen jullie allemaal een <w>gezegend</w> en gelukkig nieuwjaar.",
                            "target_lang_translations": {
                                "en": "We wish you all a <w>blessed</w> and happy new year.",
                                "ru": "Мы желаем всем вам <w>благословенного</w> и счастливого Нового года."
                            }
                        }
                    ],
                    "synonyms": [
                        "bevoorrecht",
                        "gelukkig"
                    ],
                    "antonyms": [
                        "vervloekt",
                        "onfortuinlijk"
                    ],
                    "common_phrases": [
                        "gezegend met",
                        "een gezegende leeftijd"
                    ],
                    "target_lang_definitions": {
                        "en": "Happy, prosperous, and privileged, often implying divine favor.",
                        "ru": "Счастливый, процветающий и привилегированный, как будто получивший милость от Бога."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blessed",
                                "target_lang_sense_clarification": "endowed with divine favor or protection"
                            },
                            {
                                "target_lang_word": "fortunate",
                                "target_lang_sense_clarification": "lucky or privileged"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "благословенный",
                                "target_lang_sense_clarification": "счастливый, ниспосланный свыше"
                            },
                            {
                                "target_lang_word": "одаренный",
                                "target_lang_sense_clarification": "в контексте обладания чем-то (напр. здоровьем)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8998025c-0a6a-3989-a4e5-677f5aa4ea94",
                    "sense_definition": "Zwanger zijn (uitsluitend gebruikt in vaste uitdrukkingen).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "pregnancy_euphemism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jonge vrouw verkeerde in <w>gezegende</w> omstandigheden en verwachtte haar eerste kind.",
                            "target_lang_translations": {
                                "en": "The young woman was in a <w>blessed</w> condition and was expecting her first child.",
                                "ru": "Молодая женщина находилась в <w>благословенном</w> состоянии и ждала своего первенца."
                            }
                        },
                        {
                            "text": "Toen zij in <w>gezegende</w> omstandigheden was, richtte ze de kinderkamer in.",
                            "target_lang_translations": {
                                "en": "When she was in a <w>blessed</w> condition, she furnished the nursery.",
                                "ru": "Когда она была в <w>интересном</w> положении, она обустраивала детскую."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwanger",
                        "in verwachting"
                    ],
                    "common_phrases": [
                        "in gezegende omstandigheden"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "euphemism"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Being pregnant (used exclusively in fixed expressions).",
                        "ru": "Быть беременной (используется исключительно в устойчивых выражениях)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blessed",
                                "target_lang_sense_clarification": "in the phrase 'in a blessed condition' (dated euphemism)"
                            },
                            {
                                "target_lang_word": "expecting",
                                "target_lang_sense_clarification": "pregnant"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "благословенный",
                                "target_lang_sense_clarification": "в выражении 'в благословенном состоянии' (устар.)"
                            },
                            {
                                "target_lang_word": "интересный",
                                "target_lang_sense_clarification": "в выражении 'в интересном положении' (аналог идиомы)"
                            }
                        ]
                    }
                }
            ]
        },
        {

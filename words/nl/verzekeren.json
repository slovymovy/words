    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d9691486-b204-352f-9356-66e99837e940",
                    "sense_definition": "Een contract afsluiten om financiële dekking te krijgen bij schade, ziekte of verlies.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je bent wettelijk verplicht om je auto te <w>verzekeren</w>.",
                            "target_lang_translations": {
                                "en": "You are legally required to <w>insure</w> your car.",
                                "ru": "Вы по закону обязаны <w>застраховать</w> свой автомобиль."
                            }
                        },
                        {
                            "text": "Zij hebben hun huis goed <w>verzekerd</w> tegen brand en diefstal.",
                            "target_lang_translations": {
                                "en": "They have <w>insured</w> their house well against fire and theft.",
                                "ru": "Они хорошо <w>застраховали</w> свой дом от пожара и кражи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich laten verzekeren",
                        "tegen schade verzekeren",
                        "de auto verzekeren",
                        "het huis verzekeren"
                    ],
                    "target_lang_definitions": {
                        "en": "To arrange for financial compensation in the event of damage, loss, or injury.",
                        "ru": "Заключать договор для получения финансового покрытия в случае ущерба, болезни или потери."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "insure",
                                "target_lang_sense_clarification": "financial protection"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "страховать",
                                "target_lang_sense_clarification": "имущество, жизнь и т.д."
                            },
                            {
                                "target_lang_word": "застраховать",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f21916ab-fecd-3a72-a3d1-c5cdc9aafd5b",
                    "sense_definition": "Met nadruk tegen iemand zeggen dat iets waar is of zeker zal gebeuren om twijfel weg te nemen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>verzekerde</w> mij dat hij op tijd aanwezig zou zijn.",
                            "target_lang_translations": {
                                "en": "He <w>assured</w> me that he would be present on time.",
                                "ru": "Он <w>заверил</w> меня, что будет вовремя."
                            }
                        },
                        {
                            "text": "Ik kan je ervan <w>verzekeren</w> dat we ons uiterste best doen.",
                            "target_lang_translations": {
                                "en": "I can <w>assure</w> you that we are doing our utmost.",
                                "ru": "Я могу тебя <w>заверить</w>, что мы делаем всё возможное."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iemand ervan verzekeren",
                        "plechtig verzekeren",
                        "elkaar verzekeren"
                    ],
                    "target_lang_definitions": {
                        "en": "To tell someone confidently that something is true or certain, often to remove doubt.",
                        "ru": "С уверенностью утверждать что-либо, чтобы развеять сомнения собеседника."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "assure",
                                "target_lang_sense_clarification": "remove doubt"
                            },
                            {
                                "target_lang_word": "affirm",
                                "target_lang_sense_clarification": "state as true"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уверять",
                                "target_lang_sense_clarification": "убеждать в истинности"
                            },
                            {
                                "target_lang_word": "заверять",
                                "target_lang_sense_clarification": "давать обещание или гарантию"
                            },
                            {
                                "target_lang_word": "гарантировать",
                                "target_lang_sense_clarification": "обещать результат"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
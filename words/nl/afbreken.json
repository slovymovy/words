            "senses": [
                {
                    "sense_id": "ebf6d08f-08f5-3fad-aaf0-6a1cc9721818",
                    "sense_definition": "Een gebouw, constructie of object slopen of uit elkaar halen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "demolish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arbeiders moesten de oude schuur <w>afbreken</w> om plaats te maken voor nieuwbouw.",
                            "target_lang_translations": {
                                "en": "The workers had to <w>tear down</w> the old barn to make room for new construction."
                            }
                        },
                        {
                            "text": "Na het festival duurde het twee dagen om het enorme podium weer <w>af</w> te <w>breken</w>.",
                            "target_lang_translations": {
                                "en": "After the festival, it took two days to <w>dismantle</w> the huge stage again."
                            }
                        }
                    ],
                    "synonyms": [
                        "neerhalen"
                    ],
                    "antonyms": [
                        "opbouwen"
                    ],
                    "common_phrases": [
                        "een huis afbreken",
                        "de tent afbreken",
                        "een kampement afbreken"
                    ],
                    "target_lang_definitions": {
                        "en": "To pull down, destroy, or take apart a building or structure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demolish",
                                "target_lang_sense_clarification": "to destroy a building completely"
                            },
                            {
                                "target_lang_word": "tear down",
                                "target_lang_sense_clarification": "to pull down a structure"
                            },
                            {
                                "target_lang_word": "dismantle",
                                "target_lang_sense_clarification": "to take apart piece by piece (e.g., a stage or machine)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf87f7eb-d251-3b3e-9e3b-467283a978a1",
                    "sense_definition": "Een handeling, proces, gesprek of reis stoppen voordat het einde is bereikt.",

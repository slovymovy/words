    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7cedb6c8-cf35-313e-9fa7-20bd51f0e404",
                    "sense_definition": "Een grapje, plagerijtje of een moment van speels plezier.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fun_joke",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was maar een <w>dolletje</w>, je hoeft niet zo boos te worden.",
                            "target_lang_translations": {
                                "en": "It was just a <w>joke</w>, you don't have to get so angry.",
                                "ru": "Это была всего лишь <w>шутка</w>, тебе не стоит так злиться."
                            }
                        },
                        {
                            "text": "Tijdens de pauze maakten de collega's even een <w>dolletje</w> om de sfeer te verlichten.",
                            "target_lang_translations": {
                                "en": "During the break, the colleagues had a <w>bit of fun</w> to lighten the mood.",
                                "ru": "Во время перерыва коллеги устроили небольшой <w>розыгрыш</w>, чтобы разрядить обстановку."
                            }
                        }
                    ],
                    "synonyms": [
                        "grapje",
                        "geintje",
                        "grol"
                    ],
                    "common_phrases": [
                        "een dolletje maken"
                    ],
                    "target_lang_definitions": {
                        "en": "A joke, a little tease, or a moment of playful fun.",
                        "ru": "Шутка, розыгрыш или момент игривого веселья."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "joke",
                                "target_lang_sense_clarification": "something said or done for amusement"
                            },
                            {
                                "target_lang_word": "bit of fun",
                                "target_lang_sense_clarification": "playful behavior"
                            },
                            {
                                "target_lang_word": "prank",
                                "target_lang_sense_clarification": "a playful trick"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шутка",
                                "target_lang_sense_clarification": "нечто сказанное или сделанное ради смеха"
                            },
                            {
                                "target_lang_word": "розыгрыш",
                                "target_lang_sense_clarification": "шутливая проделка"
                            },
                            {
                                "target_lang_word": "потеха",
                                "target_lang_sense_clarification": "развлечение, забава"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9e30f96-b168-391c-addf-a8cefdf6adbf",
                    "sense_definition": "Verkleinwoord van <w>dol</w>: een metalen pin of U-vormige steun op de rand van een boot waarin de roeispaan draait.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nautical_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zorg dat de roeispaan goed in het <w>dolletje</w> ligt voordat je kracht zet.",
                            "target_lang_translations": {
                                "en": "Make sure the oar sits well in the <w>rowlock</w> before you apply power.",
                                "ru": "Убедись, что весло хорошо лежит в <w>уключине</w>, прежде чем налегать."
                            }
                        },
                        {
                            "text": "Het <w>dolletje</w> was verroest, waardoor de riem piepte bij elke slag.",
                            "target_lang_translations": {
                                "en": "The <w>oarlock</w> was rusted, causing the oar to squeak with every stroke.",
                                "ru": "<w>Уключина</w> была ржавой, из-за чего весло скрипело при каждом гребке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Diminutive of 'dol': a metal pin or U-shaped support on the rim of a boat in which the oar turns.",
                        "ru": "Уменьшительная форма от 'dol': уключина (деталь на борту лодки для упора весла)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rowlock",
                                "target_lang_sense_clarification": "brace for an oar"
                            },
                            {
                                "target_lang_word": "oarlock",
                                "target_lang_sense_clarification": "brace for an oar (US English)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уключина",
                                "target_lang_sense_clarification": "приспособление для весла"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7192c381-975c-396b-8ad9-8ecd2890eba5",
                    "sense_definition": "Een kleine kuil of laagte in het landschap, met name in de duinen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geography_hollow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We zochten beschutting tegen de wind in een <w>dolletje</w> tussen de duinen.",
                            "target_lang_translations": {
                                "en": "We sought shelter from the wind in a <w>hollow</w> between the dunes.",
                                "ru": "Мы искали укрытие от ветра в <w>ложбинке</w> между дюнами."
                            }
                        },
                        {
                            "text": "Het regenwater bleef staan in het <w>dolletje</w> onder aan de heuvel.",
                            "target_lang_translations": {
                                "en": "The rainwater remained standing in the <w>depression</w> at the bottom of the hill.",
                                "ru": "Дождевая вода стояла в <w>ямке</w> у подножия холма."
                            }
                        }
                    ],
                    "synonyms": [
                        "kuiltje"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "often used in coastal regions"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A small pit or hollow in the landscape, especially in the dunes.",
                        "ru": "Небольшая ямка или углубление в ландшафте, особенно в дюнах."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hollow",
                                "target_lang_sense_clarification": "small depression in the ground"
                            },
                            {
                                "target_lang_word": "depression",
                                "target_lang_sense_clarification": "sunken place"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ложбинка",
                                "target_lang_sense_clarification": "небольшое углубление"
                            },
                            {
                                "target_lang_word": "ямка",
                                "target_lang_sense_clarification": "маленькая яма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adca716e-f1ec-30d9-b18a-f5354b5b51ec",
                    "sense_definition": "De letter d of het getal vier, voornamelijk gebruikt in geheimtaal of bij geldzaken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_number",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het oude Bargoens noemde men vier cent soms een <w>dolletje</w>.",
                            "target_lang_translations": {
                                "en": "In old Bargoens, four cents was sometimes called a <w>four</w>.",
                                "ru": "В старом воровском жаргоне четыре цента иногда называли <w>четвёркой</w>."
                            }
                        },
                        {
                            "text": "Niemand begreep wat hij bedoelde toen hij om een <w>dolletje</w> vroeg.",
                            "target_lang_translations": {
                                "en": "No one understood what he meant when he asked for a <w>four</w>.",
                                "ru": "Никто не понял, что он имел в виду, когда попросил <w>четвёрку</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Bargoens (Dutch thieves' cant)"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "historical usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The letter d or the number four, mainly used in secret language or money matters.",
                        "ru": "Буква «d» или число четыре, используется в тайном языке (баргоенс) или денежных делах."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "four",
                                "target_lang_sense_clarification": "slang term for the number"
                            },
                            {
                                "target_lang_word": "letter d",
                                "target_lang_sense_clarification": "slang reference"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "четвёрка",
                                "target_lang_sense_clarification": "число 4 или монета"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e52605fe-4629-3c20-b48e-3d075eac0449",
                    "sense_definition": "Present participle of <w>live</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "no",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is currently <w>living</w> in a small apartment in the city center.",
                            "target_lang_translations": {
                                "ru": "В настоящее время она <w>живет</w> в маленькой квартире в центре города."
                            }
                        },
                        {
                            "text": "We are <w>living</w> with my parents until our new house is ready.",
                            "target_lang_translations": {
                                "ru": "Мы <w>живем</w> с моими родителями, пока наш новый дом не будет готов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "living in",
                        "living with",
                        "living on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени от глагола live (жить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живущий",
                                "target_lang_sense_clarification": "причастие"
                            },
                            {
                                "target_lang_word": "проживающий",
                                "target_lang_sense_clarification": "официально или временно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "35359f21-4b05-3f9d-82fa-8f486e5b7786",
                    "sense_definition": "Having life; not dead.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "life_existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Every <w>living</w> creature needs water to survive.",
                            "target_lang_translations": {
                                "ru": "Каждое <w>живое</w> существо нуждается в воде для выживания."
                            }
                        },
                        {
                            "text": "She is considered the greatest <w>living</w> writer of her generation.",
                            "target_lang_translations": {
                                "ru": "Она считается величайшим <w>ныне живущим</w> писателем своего поколения."
                            }
                        }
                    ],
                    "synonyms": [
                        "alive",
                        "extant"
                    ],
                    "antonyms": [
                        "dead",
                        "nonliving"
                    ],
                    "common_phrases": [
                        "living thing",
                        "living creature",
                        "living organism",
                        "living legend"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий жизнью; не мертвый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живой",
                                "target_lang_sense_clarification": "существующий физически"
                            },
                            {
                                "target_lang_word": "ныне живущий",
                                "target_lang_sense_clarification": "о современниках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bedd94b-9966-343a-b1e9-93550e0b112f",
                    "sense_definition": "Currently in use or continuing to exist.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "life_existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latin is no longer spoken as a <w>living</w> language by a whole community.",
                            "target_lang_translations": {
                                "ru": "Латынь больше не используется целым сообществом как <w>живой</w> язык."
                            }
                        },
                        {
                            "text": "The events of that day are still within <w>living</w> memory for many people.",
                            "target_lang_translations": {
                                "ru": "События того дня все еще свежи в <w>живой</w> памяти многих людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "extant"
                    ],
                    "common_phrases": [
                        "living language",
                        "living memory",
                        "living history"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используемый в настоящее время или продолжающий существовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живой",
                                "target_lang_sense_clarification": "активный, действующий"
                            },
                            {
                                "target_lang_word": "действующий",
                                "target_lang_sense_clarification": "функционирующий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86485ca6-d188-39b0-8cd3-3491bea1e602",
                    "sense_definition": "Looking exactly like a real person or thing; very realistic.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "realism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boy is the <w>living</w> image of his father at that age.",
                            "target_lang_translations": {
                                "ru": "Мальчик — <w>живая</w> копия своего отца в этом возрасте."
                            }
                        },
                        {
                            "text": "Her success is <w>living</w> proof that determination leads to results.",
                            "target_lang_translations": {
                                "ru": "Ее успех — <w>живое</w> доказательство того, что решительность приводит к результатам."
                            }
                        }
                    ],
                    "synonyms": [
                        "lifelike"
                    ],
                    "common_phrases": [
                        "living image",
                        "living proof"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выглядящий точно так же, как реальный человек или вещь; очень реалистичный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живой",
                                "target_lang_sense_clarification": "как настоящий"
                            },
                            {
                                "target_lang_word": "вылитый",
                                "target_lang_sense_clarification": "о сходстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "525cd5d6-14a9-3904-ab4d-eaf1b2d9b5b9",
                    "sense_definition": "Continually updated and changing; not static.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "no",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The constitution should be a <w>living</w> document that adapts to the times.",
                            "target_lang_translations": {
                                "ru": "Конституция должна быть <w>живым</w> документом, который адаптируется к времени."
                            }
                        },
                        {
                            "text": "Web standards are often described as a <w>living</w> standard because they evolve constantly.",
                            "target_lang_translations": {
                                "ru": "Веб-стандарты часто описывают как <w>живой</w> стандарт, потому что они постоянно развиваются."
                            }
                        }
                    ],
                    "common_phrases": [
                        "living document",
                        "living standard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянно обновляемый и меняющийся; не статичный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живой",
                                "target_lang_sense_clarification": "динамичный"
                            },
                            {
                                "target_lang_word": "развивающийся",
                                "target_lang_sense_clarification": "изменяющийся со временем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f66fc9f1-6dec-322f-8b5c-6abe327979b1",
                    "sense_definition": "Used to add force to an expression, often expressing anger or surprise.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "no",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden noise scared the <w>living</w> daylights out of me.",
                            "target_lang_translations": {
                                "ru": "Внезапный шум напугал меня до <w>смерти</w>."
                            }
                        },
                        {
                            "text": "The commute to work this morning was a <w>living</w> nightmare.",
                            "target_lang_translations": {
                                "ru": "Поездка на работу сегодня утром была <w>настоящим</w> кошмаром."
                            }
                        }
                    ],
                    "common_phrases": [
                        "living daylights",
                        "living hell",
                        "living nightmare"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used for emphasis"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления выражения, часто выражая гнев или удивление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настоящий",
                                "target_lang_sense_clarification": "полный, абсолютный"
                            },
                            {
                                "target_lang_word": "сущий",
                                "target_lang_sense_clarification": "разг. усиление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc43f769-0f9d-3752-9b85-9af30c20f5b7",
                    "sense_definition": "Of rock or stone, found in its original position and not quarried.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "no",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient temple was carved directly from the <w>living</w> rock.",
                            "target_lang_translations": {
                                "ru": "Древний храм был высечен прямо в <w>природной</w> скале."
                            }
                        },
                        {
                            "text": "They discovered a chamber hewn out of the <w>living</w> stone deep in the cave.",
                            "target_lang_translations": {
                                "ru": "Они обнаружили камеру, высеченную в <w>цельном</w> камне глубоко в пещере."
                            }
                        }
                    ],
                    "common_phrases": [
                        "living rock",
                        "living stone"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary or technical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О скале или камне: находящийся в своем первоначальном положении, не добытый в карьере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "природный",
                                "target_lang_sense_clarification": "естественный"
                            },
                            {
                                "target_lang_word": "коренной",
                                "target_lang_sense_clarification": "о породе"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7bfd4cb8-a83a-302a-bfb2-262dd49d2b83",
                    "sense_definition": "Financial means; the money earned to buy the things that are needed for life.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "livelihood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He works two jobs to earn a decent <w>living</w> for his family.",
                            "target_lang_translations": {
                                "ru": "Он работает на двух работах, чтобы заработать достойные <w>средства к существованию</w> для своей семьи."
                            }
                        },
                        {
                            "text": "What do you do for a <w>living</w>?",
                            "target_lang_translations": {
                                "ru": "Чем вы зарабатываете на <w>жизнь</w>?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "make a living",
                        "earn a living",
                        "for a living"
                    ],
                    "target_lang_definitions": {
                        "ru": "Финансовые средства; деньги, заработанные для покупки необходимого для жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "средства к существованию",
                                "target_lang_sense_clarification": "деньги на жизнь"
                            },
                            {
                                "target_lang_word": "жизнь",
                                "target_lang_sense_clarification": "в выражениях 'на жизнь'"
                            },
                            {
                                "target_lang_word": "пропитание",
                                "target_lang_sense_clarification": "еда и содержание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "372b4146-9872-36ea-a539-df9c9325f51f",
                    "sense_definition": "People who are currently alive.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "life_existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is a tragedy that shocked the land of the <w>living</w>.",
                            "target_lang_translations": {
                                "ru": "Это трагедия, потрясшая мир <w>живых</w>."
                            }
                        },
                        {
                            "text": "We must respect both the dead and the <w>living</w>.",
                            "target_lang_translations": {
                                "ru": "Мы должны уважать как мертвых, так и <w>живых</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "dead"
                    ],
                    "common_phrases": [
                        "land of the living",
                        "among the living"
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди, которые живы в настоящее время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живые",
                                "target_lang_sense_clarification": "субстантивированное прилагательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "094a4f7d-849c-3bdf-a241-cff649e4cc25",
                    "sense_definition": "A particular way or style of life.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "no",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cost of <w>living</w> has increased significantly this year.",
                            "target_lang_translations": {
                                "ru": "Стоимость <w>жизни</w> значительно выросла в этом году."
                            }
                        },
                        {
                            "text": "They prefer plain <w>living</w> and high thinking.",
                            "target_lang_translations": {
                                "ru": "Они предпочитают простой <w>образ жизни</w> и высокие помыслы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cost of living",
                        "standard of living",
                        "healthy living"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный уклад или стиль жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "образ жизни",
                                "target_lang_sense_clarification": "уклад"
                            },
                            {
                                "target_lang_word": "жизнь",
                                "target_lang_sense_clarification": "быт, существование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "023503d6-4358-34a1-9b24-4e8a7d60671a",
                    "sense_definition": "A position in a church that has a source of income attached to it.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "no",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bishop granted him the <w>living</w> of the village parish.",
                            "target_lang_translations": {
                                "ru": "Епископ пожаловал ему <w>приход</w> в деревенской церкви."
                            }
                        },
                        {
                            "text": "In the past, a wealthy patron could offer a <w>living</w> to a priest of their choice.",
                            "target_lang_translations": {
                                "ru": "В прошлом богатый покровитель мог предложить <w>содержание</w> священнику по своему выбору."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical or ecclesiastical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Должность в церкви, к которой прилагается источник дохода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приход",
                                "target_lang_sense_clarification": "церковная должность"
                            },
                            {
                                "target_lang_word": "бенефиций",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

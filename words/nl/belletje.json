            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4e4d96a0-852b-3cff-9d82-ed541826acbb",
                    "sense_definition": "Verkleinwoord van <w>bel</w>; een kleine klok of een kleine bolvormige hoeveelheid lucht of gas.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "diminutive_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kat draagt een halsbandje met een zilveren <w>belletje</w> eraan.",
                            "target_lang_translations": {
                                "ru": "На кошке ошейник с серебряным <w>бубенчиком</w>."
                            }
                        },
                        {
                            "text": "Er stegen kleine <w>belletjes</w> op uit het glas met koolzuurhoudend water.",
                            "target_lang_translations": {
                                "ru": "Из стакана с газированной водой поднимались маленькие <w>пузырьки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "doet een belletje rinkelen",
                        "belletje trekken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от \"bel\"; маленький колокольчик или маленький пузырек воздуха или газа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колокольчик",
                                "target_lang_sense_clarification": "маленький звонок"
                            },
                            {
                                "target_lang_word": "бубенчик",
                                "target_lang_sense_clarification": "глухой звонок, например, на ошейнике"
                            },
                            {
                                "target_lang_word": "пузырёк",
                                "target_lang_sense_clarification": "воздуха или газа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "635dbca8-ba18-3dfa-9496-585fb394b2ca",
                    "sense_definition": "Een kort telefoongesprek.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Geef me even een <w>belletje</w> zodra je op het station bent aangekomen.",
                            "target_lang_translations": {
                                "ru": "<w>Позвони</w> мне, как только приедешь на вокзал."
                            }
                        },
                        {
                            "text": "Na een kort <w>belletje</w> met de klantenservice was het probleem snel opgelost.",
                            "target_lang_translations": {
                                "ru": "После короткого <w>звонка</w> в службу поддержки проблема была быстро решена."
                            }
                        }
                    ],
                    "synonyms": [
                        "telefoontje",
                        "oproep"
                    ],
                    "common_phrases": [
                        "een belletje geven",
                        "een belletje plegen",
                        "een belletje doen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий телефонный разговор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонок",
                                "target_lang_sense_clarification": "телефонный разговор"
                            },
                            {
                                "target_lang_word": "звоночек",
                                "target_lang_sense_clarification": "разговорное, уменьшительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74b74d91-5695-367d-940d-eaf6e7f16035",
                    "sense_definition": "Een hydroïdpoliep uit de familie Pandeidae (Neoturris pileata).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>belletje</w> is een klein zeedier dat voornamelijk in de zomer wordt waargenomen.",
                            "target_lang_translations": {
                                "ru": "Этот <w>гидроид</w> — маленькое морское животное, которое наблюдается в основном летом."
                            }
                        },
                        {
                            "text": "Biologen onderzochten de verspreiding van het <w>belletje</w> in de Noordzee.",
                            "target_lang_translations": {
                                "ru": "Биологи изучали распространение этого <w>гидроида</w> в Северном море."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гидроидный полип из семейства Pandeidae (Neoturris pileata)."
                    },
                    "translations": {

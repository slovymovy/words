                },
                {
                    "sense_id": "e22bb91f-28ed-312d-9d9d-3be5ea2e5bb7",
                    "sense_definition": "Iemand in een toestand van hevige emotie of opwinding brengen",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het publiek werd <w>vervoerd</w> door de prachtige muziek van het orkest.",
                            "target_lang_translations": {
                                "ru": "Публика была <w>восхищена</w> прекрасной музыкой оркестра.",
                                "en": "The audience was <w>transported</w> by the orchestra's beautiful music."
                            }
                        },
                        {
                            "text": "Hij was helemaal <w>vervoerd</w> van vreugde toen hij het goede nieuws hoorde.",
                            "target_lang_translations": {
                                "ru": "Он был совершенно <w>вне себя</w> от радости, когда услышал хорошие новости.",
                                "en": "He was completely <w>transported</w> with joy when he heard the good news."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontroeren",
                        "meeslepen",
                        "overweldigen"
                    ],
                    "common_phrases": [
                        "van vreugde vervoerd",
                        "door woede vervoerd",
                        "in vervoering brengen"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bd576200-7b03-35ed-bc82-ea6baba9a35a",
                    "sense_definition": "Iemand die verantwoordelijk is voor het bouwen, ontwerpen of tot stand brengen van iets, vaak infrastructuur of landschappen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "creator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>aanlegger</w> van het nieuwe stadspark heeft veel lof gekregen voor het ontwerp.",
                            "target_lang_translations": {
                                "ru": "<w>Создатель</w> нового городского парка получил много похвалы за проект.",
                                "en": "The <w>developer</w> of the new city park received much praise for the design."
                            }
                        },
                        {
                            "text": "Als <w>aanlegger</w> van de spoorlijn moest hij rekening houden met het lastige terrein.",
                            "target_lang_translations": {
                                "ru": "Как <w>строитель</w> железнодорожной линии, он должен был учитывать сложный рельеф местности.",
                                "en": "As the <w>constructor</w> of the railway line, he had to take the difficult terrain into account."
                            }
                        }
                    ],
                    "synonyms": [
                        "bouwer",
                        "constructeur",
                        "ontwerper"
                    ],
                    "common_phrases": [
                        "aanlegger van de weg",
                        "aanlegger van de tuin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, ответственный за строительство, проектирование или создание чего-либо, часто инфраструктуры или ландшафтов.",
                        "en": "Someone responsible for building, designing, or creating something, often infrastructure or landscapes."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строитель",
                                "target_lang_sense_clarification": "инфраструктура, здания"
                            },
                            {
                                "target_lang_word": "создатель",
                                "target_lang_sense_clarification": "парки, ландшафты"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "constructor",
                                "target_lang_sense_clarification": "infrastructure, railways"
                            },
                            {
                                "target_lang_word": "developer",
                                "target_lang_sense_clarification": "parks, projects"
                            },
                            {
                                "target_lang_word": "builder",
                                "target_lang_sense_clarification": "general construction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4d95ba9-e76c-34c7-8dde-2420845c5b5e",
                    "sense_definition": "Iemand die geld investeert in een project of onderneming.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "financier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zonder een gulle <w>aanlegger</w> was het project nooit van de grond gekomen.",
                            "target_lang_translations": {
                                "ru": "Без щедрого <w>инвестора</w> проект никогда бы не был запущен.",
                                "en": "Without a generous <w>investor</w>, the project would never have gotten off the ground."
                            }
                        },
                        {
                            "text": "De <w>aanlegger</w> verwachtte binnen een jaar rendement op zijn investering.",
                            "target_lang_translations": {
                                "ru": "<w>Инвестор</w> ожидал прибыли от своих вложений в течение года.",
                                "en": "The <w>investor</w> expected a return on his investment within a year."
                            }
                        }
                    ],
                    "synonyms": [
                        "geldschieter",
                        "investeerder",
                        "financier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который инвестирует деньги в проект или предприятие.",
                        "en": "Someone who invests money in a project or enterprise."
                    },
                    "translations": {

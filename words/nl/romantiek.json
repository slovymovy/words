    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "267d408e-4ada-338f-9937-a72d4d13fd20",
                    "sense_definition": "Een sfeer die gekenmerkt wordt door liefde, gevoeligheid en dromerigheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "atmosphere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij houdt van de <w>romantiek</w> van kaarslicht en zachte muziek.",
                            "target_lang_translations": {
                                "ru": "Она любит <w>романтику</w> свечей и тихой музыки."
                            }
                        },
                        {
                            "text": "Er hing veel <w>romantiek</w> in de lucht tijdens hun eerste afspraakje.",
                            "target_lang_translations": {
                                "ru": "Во время их первого свидания в воздухе витала <w>романтика</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "nuchterheid",
                        "zakelijkheid"
                    ],
                    "common_phrases": [
                        "pure romantiek",
                        "een vleugje romantiek",
                        "romantiek en passie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Атмосфера, наполненная любовью, чувствительностью и мечтательностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "романтика",
                                "target_lang_sense_clarification": "атмосфера"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "515774dc-419d-330d-b5c2-76ad13f68120",
                    "sense_definition": "Een culturele en artistieke stroming (eind 18e, begin 19e eeuw) waarin gevoel en verbeelding centraal stonden.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "art_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De landschappen van Friedrich zijn een bekend voorbeeld van de schilderkunst uit de <w>romantiek</w>.",
                            "target_lang_translations": {
                                "ru": "Пейзажи Фридриха — известный пример живописи эпохи <w>романтизма</w>."
                            }
                        },
                        {
                            "text": "In de literatuur van de <w>romantiek</w> speelde de natuur een zeer grote rol.",
                            "target_lang_translations": {
                                "ru": "В литературе <w>романтизма</w> природа играла очень большую роль."
                            }
                        }
                    ],
                    "antonyms": [
                        "classicisme",
                        "realisme",
                        "verlichting"
                    ],
                    "common_phrases": [
                        "de Duitse romantiek",
                        "tijdens de romantiek",
                        "schrijvers uit de romantiek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Культурное и художественное течение (конец XVIII — начало XIX в.), ставившее во главу угла чувства и воображение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "романтизм",
                                "target_lang_sense_clarification": "направление в искусстве"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
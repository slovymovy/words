                        "прежде",
                        "давно",
                        "встарь"
                    ],
                    "common_phrases": [
                        "не то что нынче"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "At the present time, now (usually when comparing with the past)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nowadays",
                                "target_lang_sense_clarification": "in the current era"
                            },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8e12357a-d7dd-3a9c-adac-2fa13fd479ea",
                    "sense_definition": "Twee kinderen of dieren die tijdens dezelfde zwangerschap samen in de baarmoeder zijn gegroeid en vlak na elkaar worden geboren",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn buren hebben gisteren een gezonde <w>tweeling</w> gekregen.",
                            "target_lang_translations": {
                                "ru": "У моих соседей вчера родилась здоровая <w>двойня</w>."
                            }
                        },
                        {
                            "text": "De eeneiige <w>tweeling</w> droeg precies dezelfde kleren naar school.",
                            "target_lang_translations": {
                                "ru": "Однояйцевые <w>близнецы</w> надели в школу совершенно одинаковую одежду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "eeneiige tweeling",
                        "twee-eiige tweeling",
                        "Siamese tweeling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двое детей или животных, родившихся в результате одной беременности"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "близнецы",
                                "target_lang_sense_clarification": "множественное число"
                            },
                            {
                                "target_lang_word": "двойня",
                                "target_lang_sense_clarification": "собирательное существительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45fbd235-ddad-35ac-902f-a3ef4bd7ac88",
                    "sense_definition": "Een van de twee personen die samen een tweeling vormen",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is een <w>tweeling</w> en heeft een heel sterke band met haar zus.",
                            "target_lang_translations": {
                                "ru": "Она — <w>близнец</w>, и у нее очень сильная связь с сестрой."
                            }
                        },
                        {
                            "text": "Als <w>tweeling</w> word je vaak met je broer of zus vergeleken.",
                            "target_lang_translations": {
                                "ru": "Будучи <w>близнецом</w>, тебя часто сравнивают с братом или сестрой."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из двух человек, составляющих пару близнецов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "близнец",
                                "target_lang_sense_clarification": "один из пары"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5cf32aa-7b9d-3242-8b16-2524cad9704b",
                    "sense_definition": "Iemand die geboren is onder het sterrenbeeld Tweelingen",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vader is een <w>Tweeling</w>, dus hij is jarig in juni.",
                            "target_lang_translations": {
                                "ru": "Мой отец — <w>Близнецы</w>, поэтому его день рождения в июне."
                            }
                        },
                        {
                            "text": "Men zegt dat een <w>Tweeling</w> vaak communicatief en flexibel is.",
                            "target_lang_translations": {
                                "ru": "Говорят, что <w>Близнецы</w> часто общительны и гибки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, родившийся под знаком зодиака Близнецы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Близнецы",
                                "target_lang_sense_clarification": "знак зодиака"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "029472ea-f353-3fe1-80d1-06af34219334",
                    "sense_definition": "Twee aan elkaar gegroeide vruchten of identieke voorwerpen",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tussen de kersen in de mand zat een vreemde <w>tweeling</w>.",
                            "target_lang_translations": {
                                "ru": "Среди вишен в корзине попалась странная <w>двойчатка</w>."
                            }
                        },
                        {
                            "text": "Deze tomaat is een <w>tweeling</w> die tijdens de groei is samengesmolten.",
                            "target_lang_translations": {
                                "ru": "Этот помидор — <w>сросшийся плод</w>, который соединился во время роста."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Два сросшихся плода или идентичных предмета"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двойчатка",
                                "target_lang_sense_clarification": "о плодах или орехах"
                            },
                            {
                                "target_lang_word": "сросшийся плод",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
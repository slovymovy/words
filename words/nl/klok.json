                        "ru": [
                            {
                                "target_lang_word": "часы",
                                "target_lang_sense_clarification": "наручные"
                            },
                            {
                                "target_lang_word": "ходики",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2dea6ad-b015-313d-b00e-8e9684fbf119",
                    "sense_definition": "Een hol, metalen voorwerp dat geluid maakt als de klepel de wand raakt",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "bell_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op zondagochtend hoor je de <w>klok</w> van de kerk luiden.",
                            "target_lang_translations": {
                                "en": "On Sunday mornings you can hear the church <w>bell</w> ringing.",
                                "ru": "Воскресным утром слышно, как звонит церковный <w>колокол</w>."
                            }
                        },
                        {
                            "text": "De zware <w>klok</w> werd met moeite in de toren gehesen.",
                            "target_lang_translations": {
                                "en": "The heavy <w>bell</w> was hoisted into the tower with difficulty.",
                                "ru": "Тяжелый <w>колокол</w> с трудом подняли на башню."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de klok luiden"
                    ],
                    "target_lang_definitions": {

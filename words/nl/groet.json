                    "sense_id": "b21a4821-40ca-3bc9-87f4-2d2fdb7c807c",
                    "sense_definition": "Een uiting waarmee men iemand welkom heet of afscheid neemt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gaf mij een vriendelijke <w>groet</w> toen hij binnenkwam.",
                            "target_lang_translations": {
                                "en": "He gave me a friendly <w>greeting</w> when he entered.",
                                "ru": "Он дружелюбно <w>поприветствовал</w> меня, когда вошел."
                            }
                        },
                        {
                            "text": "Doe de <w>groeten</w> aan je ouders als je thuiskomt.",
                            "target_lang_translations": {
                                "en": "Give my <w>regards</w> to your parents when you get home.",
                                "ru": "Передавай <w>привет</w> родителям, когда придешь домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "begroeting"
                    ],
                    "common_phrases": [
                        "de groeten doen",
                        "met vriendelijke groet"
                    ],
                    "target_lang_definitions": {
                        "en": "An expression used to welcome someone or say goodbye.",
                        "ru": "Слова или жесты, используемые для приветствия или прощания с кем-либо."
                    },
                    "translations": {
                        "en": [
                            {

            "senses": [
                {
                    "sense_id": "b28d9606-2292-32ec-b9f5-dc91b01cb9a4",
                    "sense_definition": "Opłaca się coś zrobić, przynosi to korzyść lub pożądany efekt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "benefit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naprawdę <w>warto</w> uczyć się języków obcych, aby podróżować bez stresu.",
                            "target_lang_translations": {
                                "ru": "Действительно <w>стоит</w> учить иностранные языки, чтобы путешествовать без стресса."
                            }
                        },
                        {
                            "text": "Nie <w>warto</w> się martwić rzeczami, na które nie mamy wpływu.",
                            "target_lang_translations": {
                                "ru": "Не <w>стоит</w> волноваться о вещах, на которые мы не можем повлиять."
                            }
                        }
                    ],
                    "synonyms": [
                        "opłaca się"
                    ],
                    "antonyms": [
                        "nie warto"
                    ],
                    "common_phrases": [
                        "warto wiedzieć",
                        "nie warto",
                        "czy warto",
                        "warto zobaczyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеет смысл что-то сделать, это приносит пользу или желаемый эффект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоит",
                                "target_lang_sense_clarification": "о выгоде или пользе"
                            },
                            {
                                "target_lang_word": "имеет смысл",
                                "target_lang_sense_clarification": "целесообразно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e2af8cd-3235-3165-85f8-0369b406e402",
                    "sense_definition": "Dobrze byłoby coś zrobić; jest to wskazane, właściwe lub godne uwagi.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "recommendation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przy okazji wizyty w Krakowie <w>warto</w> odwiedzić Wawel.",
                            "target_lang_translations": {
                                "ru": "Во время визита в Краков <w>стоит</w> посетить Вавель."
                            }
                        },
                        {
                            "text": "Sądzę, że <w>warto</w> by pójść na ten nowy spektakl w teatrze.",
                            "target_lang_translations": {
                                "ru": "Думаю, что <w>стоило</w> бы пойти на этот новый спектакль в театре."
                            }
                        }
                    ],
                    "synonyms": [
                        "należy",
                        "powinno się",
                        "wypada"
                    ],
                    "common_phrases": [
                        "warto by było",
                        "warto pamiętać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошо было бы что-то сделать; это рекомендуется или заслуживает внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоит",
                                "target_lang_sense_clarification": "совет или рекомендация"
                            },
                            {
                                "target_lang_word": "следует",
                                "target_lang_sense_clarification": "нужно, полагается"
                            }
                        ]
                    }
                }
            ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d0391289-1123-3f58-9b37-2dc6a6bb9734",
                    "sense_definition": "O człowieku lub jego czynach: mający negatywne cechy moralne, wyrządzający krzywdę, nieżyczliwy; przeciwieństwo słowa dobry.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "moral_bad",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej bajce występuje <w>zła</w> czarownica, która rzuca uroki.",
                            "target_lang_translations": {
                                "ru": "В этой сказке есть <w>злая</w> ведьма, которая накладывает чары."
                            }
                        },
                        {
                            "text": "To nie jest <w>zły</w> człowiek, po prostu popełnił błąd.",
                            "target_lang_translations": {
                                "ru": "Это не <w>плохой</w> человек, он просто совершил ошибку."
                            }
                        }
                    ],
                    "synonyms": [
                        "niedobry",
                        "podły",
                        "niegodziwy",
                        "okrutny"
                    ],
                    "antonyms": [
                        "dobry",
                        "szlachetny",
                        "prawy",
                        "miły"
                    ],
                    "common_phrases": [
                        "zły człowiek",
                        "złe intencje",
                        "zły uczynek"
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке или его поступках: обладающий отрицательными моральными качествами, причиняющий вред, недоброжелательный; противоположность слову \"добрый\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "злой",
                                "target_lang_sense_clarification": "недобрый, злобный, желающий зла"
                            },
                            {
                                "target_lang_word": "плохой",
                                "target_lang_sense_clarification": "обладающий отрицательными качествами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84eaa795-2b0e-30e7-b08e-ce820b181019",
                    "sense_definition": "Odczuwający gniew, zdenerwowanie lub irytację na kogoś lub coś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tata jest <w>zły</w>, bo zarysowałem jego samochód.",
                            "target_lang_translations": {
                                "ru": "Папа <w>злой</w>, потому что я поцарапал его машину."
                            }
                        },
                        {
                            "text": "Dlaczego jesteś na mnie <w>zła</w>?",
                            "target_lang_translations": {
                                "ru": "Почему ты на меня <w>злая</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wściekły",
                        "zdenerwowany",
                        "rozgniewany",
                        "oburzony"
                    ],
                    "antonyms": [

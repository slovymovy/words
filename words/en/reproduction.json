    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ccd2a9be-cf33-36f7-b4f4-f7617ef449d5",
                    "sense_definition": "The act of reproducing new individuals biologically.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "biological_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sexual <w>reproduction</w> involves genetic material from two parents.",
                            "target_lang_translations": {
                                "ru": "Половое <w>размножение</w> включает генетический материал от двух родителей."
                            }
                        },
                        {
                            "text": "The biology textbook explains the <w>reproduction</w> of flowering plants.",
                            "target_lang_translations": {
                                "ru": "Учебник по биологии объясняет <w>размножение</w> цветковых растений."
                            }
                        }
                    ],
                    "synonyms": [
                        "procreation"
                    ],
                    "common_phrases": [
                        "sexual reproduction",
                        "asexual reproduction",
                        "human reproduction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Биологический процесс создания новых особей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размножение",
                                "target_lang_sense_clarification": "биологический процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00f4bd42-a2ef-38d3-95ed-121fd097e566",
                    "sense_definition": "The act of making copies.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "copying_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Unauthorized <w>reproduction</w> of this article is strictly prohibited.",
                            "target_lang_translations": {
                                "ru": "Несанкционированное <w>копирование</w> этой статьи строго запрещено."
                            }
                        },
                        {
                            "text": "The cost of <w>reproduction</w> for these documents is quite high.",
                            "target_lang_translations": {
                                "ru": "Стоимость <w>копирования</w> этих документов довольно высока."
                            }
                        }
                    ],
                    "synonyms": [
                        "repro"
                    ],
                    "common_phrases": [
                        "unauthorized reproduction",
                        "mechanical reproduction",
                        "digital reproduction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по созданию копий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копирование",
                                "target_lang_sense_clarification": "создание дубликатов"
                            },
                            {
                                "target_lang_word": "воспроизведение",
                                "target_lang_sense_clarification": "воссоздание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d4af2a3-2fc3-399f-b7d4-c141ddcda236",
                    "sense_definition": "A copy of something, as in a piece of art; a duplicate.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "copy_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jim was proud of the Rembrandt <w>reproduction</w> hanging in his office.",
                            "target_lang_translations": {
                                "ru": "Джим гордился <w>репродукцией</w> Рембрандта, висящей в его кабинете."
                            }
                        },
                        {
                            "text": "This furniture is a faithful <w>reproduction</w> of an eighteenth-century design.",
                            "target_lang_translations": {
                                "ru": "Эта мебель — точная <w>копия</w> дизайна восемнадцатого века."
                            }
                        }
                    ],
                    "synonyms": [
                        "repro"
                    ],
                    "common_phrases": [
                        "faithful reproduction",
                        "art reproduction",
                        "cheap reproduction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Копия чего-либо, например произведения искусства; дубликат."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "репродукция",
                                "target_lang_sense_clarification": "копия картины"
                            },
                            {
                                "target_lang_word": "копия",
                                "target_lang_sense_clarification": "дубликат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfe2b41f-7e79-3d27-a270-963ce4e04ef0",
                    "sense_definition": "A method for reproducing a bug or problem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing_method",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please provide a minimal <w>reproduction</w> so we can investigate the issue.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, предоставьте минимальное <w>воспроизведение</w>, чтобы мы могли изучить проблему."
                            }
                        },
                        {
                            "text": "The developer asked for a consistent <w>reproduction</w> of the crash.",
                            "target_lang_translations": {
                                "ru": "Разработчик попросил стабильное <w>воспроизведение</w> сбоя."
                            }
                        }
                    ],
                    "synonyms": [
                        "repro"
                    ],
                    "common_phrases": [
                        "steps for reproduction",
                        "minimal reproduction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Метод повторения ошибки или проблемы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспроизведение",
                                "target_lang_sense_clarification": "повторение ошибки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
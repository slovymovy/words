    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0709b216-1e96-34f1-a152-c4b91f82f0c1",
                    "sense_definition": "Een mening of conclusie die men heeft gevormd na overdenking van feiten of omstandigheden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "opinion_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik vertrouw volledig op zijn <w>oordeel</w> omdat hij veel ervaring heeft met deze zaken.",
                            "target_lang_translations": {
                                "ru": "Я полностью доверяю его <w>суждению</w>, так как у него большой опыт в этих делах."
                            }
                        },
                        {
                            "text": "De vraag is waarom we het <w>oordeel</w> van vreemden vaak belangrijker vinden dan dat van vrienden.",
                            "target_lang_translations": {
                                "ru": "Вопрос в том, почему мы часто считаем <w>мнение</w> незнакомцев важнее, чем мнение друзей."
                            }
                        }
                    ],
                    "synonyms": [
                        "mening",
                        "opinie",
                        "opvatting",
                        "zienswijze",
                        "inzicht"
                    ],
                    "common_phrases": [
                        "naar mijn oordeel",
                        "een oordeel vellen",
                        "naar eigen oordeel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мнение или заключение, сформированное после обдумывания фактов или обстоятельств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суждение",
                                "target_lang_sense_clarification": "мнение, оценка"
                            },
                            {
                                "target_lang_word": "мнение",
                                "target_lang_sense_clarification": "взгляд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea328a5f-7a59-3538-b3e2-9cade3e79ebc",
                    "sense_definition": "Een officiële en bindende uitspraak van een rechter of rechtbank in een rechtszaak.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte was het niet eens met het <w>oordeel</w> van de rechtbank en ging in hoger beroep.",
                            "target_lang_translations": {
                                "ru": "Обвиняемый был не согласен с <w>решением</w> суда и подал апелляцию."
                            }
                        },
                        {
                            "text": "Hoewel er nu een unaniem <w>oordeel</w> ligt, is de juridische strijd nog niet helemaal voorbij.",
                            "target_lang_translations": {
                                "ru": "Хотя сейчас уже есть единогласное <w>решение</w>, юридическая битва еще не совсем окончена."
                            }
                        }
                    ],
                    "synonyms": [
                        "vonnis",
                        "uitspraak",
                        "beslissing",
                        "arrest"
                    ],
                    "common_phrases": [
                        "het laatste oordeel",
                        "rechterlijk oordeel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное и обязательное решение судьи или суда по судебному делу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решение",
                                "target_lang_sense_clarification": "суда"
                            },
                            {
                                "target_lang_word": "приговор",
                                "target_lang_sense_clarification": "вердикт"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cc0406c7-c63f-3dd6-bdc0-268ccd21fe02",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>oordelen</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>oordeel</w> niet over jouw keuzes, want dat is niet aan mij.",
                            "target_lang_translations": {
                                "ru": "Я не <w>сужу</w> о твоем выборе, потому что это не мое дело."
                            }
                        },
                        {
                            "text": "Als ik de situatie bekijk, <w>oordeel</w> ik dat we beter kunnen wachten.",
                            "target_lang_translations": {
                                "ru": "Рассматривая ситуацию, я <w>считаю</w>, что нам лучше подождать."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "vervoeging"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "1-е лицо единственного числа настоящего времени от глагола oordelen (судить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сужу",
                                "target_lang_sense_clarification": "высказываю мнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c825b86-1c2b-3f6c-8350-68424a69cd8c",
                    "sense_definition": "Gebiedende wijs van <w>oordelen</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Oordeel</w> zelf maar of je dit schilderij mooi vindt of niet.",
                            "target_lang_translations": {
                                "ru": "<w>Суди</w> сам, нравится тебе эта картина или нет."
                            }
                        },
                        {
                            "text": "<w>Oordeel</w> niet te snel over mensen die je nauwelijks kent.",
                            "target_lang_translations": {
                                "ru": "Не <w>суди</w> слишком быстро о людях, которых едва знаешь."
                            }
                        }
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "b869549e-db20-386a-9887-e12cad1384b1",
                    "sense_definition": "A Dravidian language spoken in the state of Tamil Nadu, India and in Sri Lanka, Singapore, and Malaysia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "She is learning to speak <w>Tamil</w> to communicate with her relatives.",
                            "target_lang_translations": {
                                "ru": "Она учится говорить на <w>тамильском</w>, чтобы общаться со своими родственниками."
                            }
                        },
                        {
                            "text": "Many ancient literary works were written in <w>Tamil</w> centuries ago.",
                            "target_lang_translations": {
                                "ru": "Многие древние литературные произведения были написаны на <w>тамильском</w> столетия назад."
                            }
                        }
                    ],
                    "common_phrases": [
                        "speak Tamil",
                        "learn Tamil",
                        "Tamil literature",
                        "Tamil language"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дравидийский язык, на котором говорят в индийском штате Тамилнад, а также на Шри-Ланке, в Сингапуре и Малайзии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тами́льский язы́к",
                                "target_lang_sense_clarification": "полное название"
                            },
                            {
                                "target_lang_word": "тами́льский",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d2295f2-f552-311a-88be-c59551bf1632",
                    "sense_definition": "The script used to write the Tamil language.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "The sign was painted in English and <w>Tamil</w>.",
                            "target_lang_translations": {
                                "ru": "Вывеска была написана на английском и <w>тамильском</w>."
                            }
                        },
                        {
                            "text": "It takes practice to master the curves of the <w>Tamil</w> writing system.",
                            "target_lang_translations": {
                                "ru": "Требуется практика, чтобы освоить изгибы <w>тамильского</w> письма."
                            }
                        }
                    ],
                    "common_phrases": [
                        "written in Tamil",
                        "Tamil script"
                    ],
                    "target_lang_definitions": {
                        "ru": "Письменность, используемая для записи тамильского языка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тами́льское письмо́",
                                "target_lang_sense_clarification": "система письменности"
                            },
                            {
                                "target_lang_word": "тами́льский",
                                "target_lang_sense_clarification": "в значении «шрифт» или «язык»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "044d592f-5cd7-3cb7-9ab3-328695f64ea9",
                    "sense_definition": "Of or pertaining to the Tamil people, culture, or language.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We enjoyed a traditional <w>Tamil</w> meal at the new restaurant.",
                            "target_lang_translations": {
                                "ru": "Мы насладились традиционным <w>тамильским</w> обедом в новом ресторане."
                            }
                        },
                        {
                            "text": "The <w>Tamil</w> community celebrates several unique and colorful festivals.",
                            "target_lang_translations": {
                                "ru": "<w>Тамильская</w> община отмечает несколько уникальных и красочных праздников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Tamil culture",
                        "Tamil people",
                        "Tamil cuisine",
                        "Tamil tradition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к тамильскому народу, культуре или языку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тами́льский",
                                "target_lang_sense_clarification": "прилагательное"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d17bf76c-f8a0-38d3-bc9f-2dbdee090cd4",
                    "sense_definition": "A part or share of a whole number or amount.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "part_of_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A large <w>proportion</w> of the budget is spent on food.",
                            "target_lang_translations": {
                                "ru": "Большая <w>часть</w> бюджета тратится на еду."
                            }
                        },
                        {
                            "text": "Only a small <w>proportion</w> of the students passed the difficult exam.",
                            "target_lang_translations": {
                                "ru": "Лишь небольшая <w>доля</w> студентов сдала этот сложный экзамен."
                            }
                        }
                    ],
                    "synonyms": [
                        "part",
                        "share",
                        "percentage",
                        "fraction",
                        "segment"
                    ],
                    "antonyms": [
                        "whole",
                        "total"
                    ],
                    "common_phrases": [
                        "a large proportion",
                        "a significant proportion",
                        "a small proportion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть или доля от целого числа или количества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "часть",
                                "target_lang_sense_clarification": "элемент целого"
                            },
                            {
                                "target_lang_word": "доля",
                                "target_lang_sense_clarification": "участие или процент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac7c77fe-57cd-3e18-a02a-e8a7f68508e2",
                    "sense_definition": "The relationship between two things in size, amount, or degree.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The punishment should be in <w>proportion</w> to the crime.",
                            "target_lang_translations": {
                                "ru": "Наказание должно быть <w>соразмерно</w> преступлению."
                            }
                        },
                        {
                            "text": "His head looks large in <w>proportion</w> to his small body.",
                            "target_lang_translations": {
                                "ru": "Его голова выглядит большой по <w>отношению</w> к его маленькому телу."
                            }
                        }
                    ],
                    "synonyms": [
                        "ratio",
                        "relationship",
                        "correlation"
                    ],
                    "common_phrases": [
                        "in proportion to",
                        "out of proportion",
                        "direct proportion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соотношение между двумя величинами по размеру, количеству или степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соотношение",
                                "target_lang_sense_clarification": "взаимосвязь"
                            },
                            {
                                "target_lang_word": "пропорция",
                                "target_lang_sense_clarification": "количественная зависимость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d05239ec-ebd9-37a1-b7d8-fc9928318c30",
                    "sense_definition": "The dimensions or size of something, especially when large or serious.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crisis has reached alarming <w>proportions</w> in recent weeks.",
                            "target_lang_translations": {
                                "ru": "За последние недели кризис достиг пугающих <w>масштабов</w>."
                            }
                        },
                        {
                            "text": "Please do not blow this minor problem out of <w>proportion</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не раздувай эту мелкую проблему до вселенских <w>масштабов</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dimensions",
                        "size",
                        "magnitude",
                        "scale"
                    ],
                    "common_phrases": [
                        "of epic proportions",
                        "blow out of proportion",
                        "reach epidemic proportions"
                    ],
                    "target_lang_definitions": {
                        "ru": "Размеры или масштаб чего-либо, особенно когда речь идет о чем-то значительном или серьезном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "масштабы",
                                "target_lang_sense_clarification": "объем или размах явления"
                            },
                            {
                                "target_lang_word": "размеры",
                                "target_lang_sense_clarification": "величина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10877fcc-6a87-36cb-bd5f-fda3d0f7571d",
                    "sense_definition": "The correct or pleasing relationship between the size and shape of different parts.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "harmony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The architect designed the building with a perfect sense of <w>proportion</w>.",
                            "target_lang_translations": {
                                "ru": "Архитектор спроектировал здание с идеальным чувством <w>пропорций</w>."
                            }
                        },
                        {
                            "text": "Her features were fine, but the face lacked <w>proportion</w>.",
                            "target_lang_translations": {
                                "ru": "Ее черты были тонкими, но лицу не хватало <w>соразмерности</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "symmetry",
                        "balance",
                        "harmony"
                    ],
                    "antonyms": [
                        "asymmetry",
                        "imbalance"
                    ],
                    "common_phrases": [
                        "sense of proportion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Правильное или гармоничное соотношение размеров и форм различных частей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соразмерность",
                                "target_lang_sense_clarification": "гармония частей"
                            },
                            {
                                "target_lang_word": "пропорциональность",
                                "target_lang_sense_clarification": "правильное соотношение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92d06fe1-5a3f-3d53-9eed-78d47e450a5b",
                    "sense_definition": "A mathematical statement that two ratios are equal.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "math",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Students learned how to solve the <w>proportion</w> to find the unknown value.",
                            "target_lang_translations": {
                                "ru": "Ученики учились решать <w>пропорцию</w>, чтобы найти неизвестное значение."
                            }
                        },
                        {
                            "text": "The equation expresses a <w>proportion</w> between the two variables.",
                            "target_lang_translations": {
                                "ru": "Уравнение выражает <w>пропорцию</w> между двумя переменными."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Математическое равенство двух отношений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропорция",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "57b0b512-720b-3936-b45f-6e0ee3a5d55e",
                    "sense_definition": "To adjust something so that it matches or balances with something else.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>proportion</w> your expenses to your income.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>соразмерять</w> свои расходы с доходами."
                            }
                        },
                        {
                            "text": "The punishment was <w>proportioned</w> to the severity of the offense.",
                            "target_lang_translations": {
                                "ru": "Наказание было <w>соразмерено</w> с тяжестью проступка."
                            }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2afeab0f-27e0-3f22-bb9c-d77c0ed2bff6",
                    "sense_definition": "Snel en krachtig heen en weer bewegen, bijvoorbeeld om iets te mengen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet het pak sap goed <w>schudden</w> voordat je het inschenkt.",
                            "target_lang_translations": {
                                "en": "You must <w>shake</w> the carton of juice well before pouring it.",
                                "ru": "Ты должен хорошо <w>взболтать</w> пакет сока, прежде чем наливать."
                            }
                        },
                        {
                            "text": "De natte hond begon zich op het strand droog te <w>schudden</w>.",
                            "target_lang_translations": {
                                "en": "The wet dog started to <w>shake</w> itself dry on the beach.",
                                "ru": "Мокрая собака начала <w>отряхиваться</w> на пляже, чтобы обсохнуть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de hand schudden",
                        "wakker schudden",
                        "heen en weer schudden"
                    ],
                    "target_lang_definitions": {
                        "en": "To move something quickly back and forth or up and down, often to mix it.",
                        "ru": "Быстро и сильно двигать туда-сюда, например, чтобы смешать что-то."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shake",
                                "target_lang_sense_clarification": "general movement / mixing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "трясти",
                                "target_lang_sense_clarification": "двигать туда-сюда"
                            },
                            {
                                "target_lang_word": "взбалтывать",
                                "target_lang_sense_clarification": "смешивать жидкость"
                            },
                            {
                                "target_lang_word": "пожимать",
                                "target_lang_sense_clarification": "руку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c35e0c8d-f1cf-3fbf-a5b5-8b51f1d724d9",
                    "sense_definition": "Het hoofd van links naar rechts bewegen als teken van ontkenning of afkeuring.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "gesture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toen ik vroeg of hij mee wilde, begon hij meteen te <w>schudden</w>.",
                            "target_lang_translations": {
                                "en": "When I asked if he wanted to come along, he immediately started <w>shaking</w> his head.",
                                "ru": "Когда я спросил, хочет ли он пойти, он сразу начал <w>качать</w> головой."
                            }
                        },
                        {
                            "text": "Zij bleef maar met haar hoofd <w>schudden</w> omdat ze het niet geloofde.",
                            "target_lang_translations": {
                                "en": "She kept <w>shaking</w> her head because she didn't believe it.",
                                "ru": "Она продолжала <w>качать</w> головой, так как не верила этому."
                            }
                        }
                    ],
                    "synonyms": [
                        "nee schudden"
                    ],
                    "antonyms": [
                        "knikken"
                    ],
                    "common_phrases": [
                        "het hoofd schudden",
                        "nee schudden"
                    ],
                    "target_lang_definitions": {
                        "en": "To move one's head from side to side to indicate 'no', denial, or disapproval.",
                        "ru": "Двигать головой слева направо в знак отрицания или неодобрения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shake",
                                "target_lang_sense_clarification": "one's head"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "качать",
                                "target_lang_sense_clarification": "головой"
                            },
                            {
                                "target_lang_word": "мотать",
                                "target_lang_sense_clarification": "головой (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "681fcdde-7354-307f-be45-b774cc771868",
                    "sense_definition": "Speelkaarten door elkaar husselen zodat ze in een willekeurige volgorde liggen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil jij de kaarten <w>schudden</w> voor het volgende spel?",
                            "target_lang_translations": {
                                "en": "Do you want to <w>shuffle</w> the cards for the next game?",
                                "ru": "Хочешь <w>потасовать</w> карты для следующей игры?"
                            }
                        },
                        {
                            "text": "Na het <w>schudden</w> deelde hij iedereen vijf kaarten.",
                            "target_lang_translations": {
                                "en": "After <w>shuffling</w>, he dealt everyone five cards.",
                                "ru": "После <w>тасования</w> он раздал каждому по пять карт."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kaarten schudden"
                    ],
                    "target_lang_definitions": {
                        "en": "To mix playing cards together so they are in a random order.",
                        "ru": "Перемешивать игральные карты, чтобы они лежали в случайном порядке."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shuffle",
                                "target_lang_sense_clarification": "playing cards"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тасовать",
                                "target_lang_sense_clarification": "карты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6220b101-d37f-3baf-b78a-5debe1a47ab1",
                    "sense_definition": "Vervoeging van <w>schudden</w>: onvoltooid verleden tijd, meervoud.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bomen <w>schudden</w> gisteren hevig in de wind.",
                            "target_lang_translations": {
                                "en": "The trees <w>shook</w> violently in the wind yesterday.",
                                "ru": "Деревья вчера сильно <w>тряслись</w> на ветру."
                            }
                        },
                        {
                            "text": "Wij <w>schudden</w> elkaar de hand toen we afscheid namen.",
                            "target_lang_translations": {
                                "en": "We <w>shook</w> hands when we said goodbye.",
                                "ru": "Мы <w>пожали</w> друг другу руки, когда прощались."
                            }
                        }
                    ],

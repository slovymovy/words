{
    "word_family": [
        "clear",
        "clarity",
        "clarify",
        "clarification",
        "clearance",
        "clearly"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5e78eec1-fe84-3dc2-b0b0-25715f38b5bb",
                    "sense_definition": "The quality of being transparent, unclouded, or easy to see through.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>clearness</w> of the water allowed us to see the fish swimming at the bottom.",
                            "target_lang_translations": {
                                "ru": "<w>Прозрачность</w> воды позволяла нам видеть рыб, плавающих на дне."
                            }
                        },
                        {
                            "text": "We admired the <w>clearness</w> of the blue sky after the heavy storm passed.",
                            "target_lang_translations": {
                                "ru": "Мы любовались <w>чистотой</w> голубого неба после того, как прошел сильный шторм."
                            }
                        }
                    ],
                    "synonyms": [
                        "transparency",
                        "clarity",
                        "limpidity"
                    ],
                    "antonyms": [
                        "cloudiness",
                        "opacity",
                        "murkiness"
                    ],
                    "common_phrases": [
                        "clearness of the water",
                        "crystal clearness",
                        "clearness of the sky"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество быть прозрачным, безоблачным или хорошо просматриваемым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачность",
                                "target_lang_sense_clarification": "воды, воздуха, материала"
                            },
                            {
                                "target_lang_word": "чистота",
                                "target_lang_sense_clarification": "неба, цвета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "965349ae-9ddb-3ab2-b4e8-b6b43a1953f2",
                    "sense_definition": "The quality of being easy to understand, distinct, or well-defined in thought or expression.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher explained the difficult concept with remarkable <w>clearness</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель объяснил трудную концепцию с замечательной <w>ясностью</w>."
                            }
                        },
                        {
                            "text": "His <w>clearness</w> of mind helped him solve the complex problem quickly.",
                            "target_lang_translations": {
                                "ru": "Его <w>ясность</w> ума помогла ему быстро решить сложную задачу."
                            }
                        }
                    ],
                    "synonyms": [
                        "clarity",
                        "distinctness",
                        "lucidity",
                        "precision"
                    ],
                    "antonyms": [
                        "confusion",
                        "ambiguity",
                        "vagueness"
                    ],
                    "common_phrases": [
                        "clearness of speech",
                        "clearness of thought",
                        "clearness of mind",
                        "with clearness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество быть легким для понимания, отчетливым или точно выраженным в мыслях или речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ясность",
                                "target_lang_sense_clarification": "ума, мысли, речи"
                            },
                            {
                                "target_lang_word": "четкость",
                                "target_lang_sense_clarification": "изложения, объяснения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "825ce557-cdc4-3489-aed7-bfba4068689f",
                    "sense_definition": "The state of being free from obstacles, blockage, or interference.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "state_of_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>clearness</w> of the path made for an easy and enjoyable hike through the woods.",
                            "target_lang_translations": {
                                "ru": "<w>Чистота</w> тропинки сделала прогулку по лесу легкой и приятной."
                            }
                        },
                        {
                            "text": "Pilots must ensure the <w>clearness</w> of the runway before attempting to land.",
                            "target_lang_translations": {
                                "ru": "Пилоты должны убедиться в <w>чистоте</w> взлетно-посадочной полосы перед посадкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "openness",
                        "emptiness"
                    ],
                    "antonyms": [
                        "obstruction",
                        "blockage"
                    ],
                    "common_phrases": [
                        "clearness of the road",
                        "clearness of the path"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние отсутствия препятствий, заторов или помех."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чистота",
                                "target_lang_sense_clarification": "дороги, пути (от мусора или препятствий)"
                            },
                            {
                                "target_lang_word": "свобода",
                                "target_lang_sense_clarification": "пространства, прохода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62e04169-f057-3b71-ad20-86ab2fe35e77",
                    "sense_definition": "Intense brightness or brilliance of light.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden <w>clearness</w> of the sun as it broke through the clouds dazzled our eyes.",
                            "target_lang_translations": {
                                "ru": "Внезапная <w>яркость</w> солнца, пробившегося сквозь облака, ослепила нас."
                            }
                        },
                        {
                            "text": "Poets often write about the <w>clearness</w> of the stars in the desert night.",
                            "target_lang_translations": {
                                "ru": "Поэты часто пишут о <w>сиянии</w> звезд в ночной пустыне."
                            }
                        }
                    ],
                    "synonyms": [
                        "brightness",
                        "brilliance",
                        "radiance"
                    ],
                    "antonyms": [
                        "dimness",
                        "darkness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Интенсивная яркость или сияние света."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яркость",
                                "target_lang_sense_clarification": "света, солнца"
                            },
                            {
                                "target_lang_word": "сияние",
                                "target_lang_sense_clarification": "звезд, небесных тел"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
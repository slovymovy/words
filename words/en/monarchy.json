    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fb738d75-caf9-3838-a597-97251cecaafc",
                    "sense_definition": "A system of government in which a single person, usually a king or queen, is the head of state.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many European countries have transitioned from an absolute <w>monarchy</w> to a democracy.",
                            "target_lang_translations": {
                                "ru": "Многие европейские страны перешли от абсолютной <w>монархии</w> к демократии."
                            }
                        },
                        {
                            "text": "In a constitutional <w>monarchy</w>, the ruler often has limited political power.",
                            "target_lang_translations": {
                                "ru": "В конституционной <w>монархии</w> правитель часто обладает ограниченной политической властью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "constitutional monarchy",
                        "absolute monarchy",
                        "abolish the monarchy",
                        "restore the monarchy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма правления, при которой главой государства является одно лицо, обычно король или королева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монархия",
                                "target_lang_sense_clarification": "система правления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94b6c5d9-d4a7-3ccc-bdb6-d4e3152be93d",
                    "sense_definition": "A country or territory ruled by a monarch.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The small <w>monarchy</w> was known for its peaceful borders and wealthy citizens.",
                            "target_lang_translations": {
                                "ru": "Эта небольшая <w>монархия</w> была известна своими мирными границами и богатыми гражданами."
                            }
                        },
                        {
                            "text": "Diplomats from the neighboring <w>monarchy</w> arrived to discuss trade agreements.",
                            "target_lang_translations": {
                                "ru": "Дипломаты из соседней <w>монархии</w> прибыли для обсуждения торговых соглашений."
                            }
                        }
                    ],
                    "synonyms": [
                        "kingdom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Страна или территория, которой правит монарх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монархия",
                                "target_lang_sense_clarification": "государство"
                            },
                            {
                                "target_lang_word": "королевство",
                                "target_lang_sense_clarification": "тип монархического государства"
                            }
                        ]
                    }
                }
            ]
        }
    ]

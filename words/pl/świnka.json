                    "semantic_group_id": "",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "Na tarczy rycerza widniał stary herb <w>Świnka</w>.",
                            "target_lang_translations": {
                                "ru": "На щите рыцаря был изображён старинный герб <w>Свинка</w>."

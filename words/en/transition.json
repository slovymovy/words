    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a39d5b7f-2b60-3945-a815-d60e6af2d7d2",
                    "sense_definition": "The process or period of changing from one form, state, style, or place to another.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company is undergoing a major <w>transition</w> to a new management style.",
                            "target_lang_translations": {
                                "ru": "Компания переживает серьезный <w>переход</w> к новому стилю управления."
                            }
                        },
                        {
                            "text": "We hope for a smooth <w>transition</w> from the old system to the new one.",
                            "target_lang_translations": {
                                "ru": "Мы надеемся на плавный <w>переход</w> от старой системы к новой."
                            }
                        }
                    ],
                    "synonyms": [
                        "gender transition",
                        "gender transition"
                    ],
                    "antonyms": [
                        "stagnation"
                    ],
                    "common_phrases": [
                        "smooth transition",
                        "period of transition",
                        "in transition",
                        "make a transition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс или период изменения из одной формы, состояния, стиля или места в другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переход",
                                "target_lang_sense_clarification": "процесс изменения"
                            },
                            {
                                "target_lang_word": "переходный период",
                                "target_lang_sense_clarification": "период времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a0fba88-1507-36f2-8289-8ad1840774ca",
                    "sense_definition": "The process of changing one's gender presentation or physical characteristics to accord with one's internal sense of gender identity.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gender_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She documented her <w>transition</w> on social media to help others.",
                            "target_lang_translations": {
                                "ru": "Она документировала свой <w>переход</w> в социальных сетях, чтобы помочь другим."
                            }
                        },
                        {
                            "text": "Support from family is very important during a gender <w>transition</w>.",
                            "target_lang_translations": {
                                "ru": "Поддержка семьи очень важна во время гендерного <w>перехода</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gender transition"
                    ],
                    "common_phrases": [
                        "medical transition",
                        "social transition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс изменения гендерной презентации или физических характеристик в соответствии с внутренним ощущением гендерной идентичности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переход",
                                "target_lang_sense_clarification": "гендерный"
                            },
                            {
                                "target_lang_word": "смена пола",
                                "target_lang_sense_clarification": "медицинский или социальный процесс"
                            }
                        ]

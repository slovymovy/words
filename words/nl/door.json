                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_movement_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik rijd nu de stad <w>door</w> om bij jou te komen.",
                            "target_lang_translations": {
                                "en": "I am driving <w>through</w> the city now to get to you."
                            }

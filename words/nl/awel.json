{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "1ebed619-2975-320c-a28d-6c172e0dde2f",
                    "sense_definition": "Een uitroep die gebruikt wordt aan het begin van een zin om een verhaal te starten, een pauze in te lassen of een reactie in te leiden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "introductory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Awel</w>, ik zal je vertellen wat er gisteren is gebeurd.",
                            "target_lang_translations": {
                                "en": "<w>Well</w>, I'll tell you what happened yesterday.",
                                "ru": "<w>Ну</w>, я расскажу тебе, что случилось вчера."
                            }
                        },
                        {
                            "text": "<w>Awel</w> meneer, aan uw vraag zie ik dat u niet op de hoogte bent.",
                            "target_lang_translations": {
                                "en": "<w>Well</w> sir, from your question I see that you are not informed.",
                                "ru": "<w>Ну</w>, сударь, по вашему вопросу я вижу, что вы не в курсе."
                            }
                        }
                    ],
                    "synonyms": [
                        "wel",
                        "welnu"
                    ],
                    "common_phrases": [
                        "awel ja",
                        "awel nee"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België (Vlaanderen)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation used at the beginning of a sentence to start a story, pause, or introduce a reaction.",
                        "ru": "Междометие, которое используется в начале предложения, чтобы начать рассказ, сделать паузу или выразить реакцию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "well",
                                "target_lang_sense_clarification": "introductory particle"
                            },
                            {
                                "target_lang_word": "so",
                                "target_lang_sense_clarification": "starting a story"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "вводное слово"
                            },
                            {
                                "target_lang_word": "итак",
                                "target_lang_sense_clarification": "начало рассказа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea86a4c2-c275-3e33-9e1a-13a2dc37901f",
                    "sense_definition": "Een uitroep om iemand aan te sporen iets te doen, of om ongeduld of verbazing uit te drukken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "encouragement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Awel</w>, blijf daar niet staan en kom binnen.",
                            "target_lang_translations": {
                                "en": "<w>Come on</w>, don't just stand there, come inside.",
                                "ru": "<w>Ну же</w>, не стой там, заходи."
                            }
                        },
                        {
                            "text": "<w>Awel</w> zeg, dat had ik helemaal niet verwacht!",
                            "target_lang_translations": {
                                "en": "<w>Well</w> I say, I didn't expect that at all!",
                                "ru": "<w>Ну и ну</w>, я этого совсем не ожидал!"
                            }
                        }
                    ],
                    "synonyms": [
                        "komaan",
                        "welaan"
                    ],
                    "common_phrases": [
                        "awel zeg",
                        "awel kom"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België (Vlaanderen)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation to urge someone to do something, or to express impatience or surprise.",
                        "ru": "Междометие, используемое, чтобы побудить кого-то к действию, или выразить нетерпение или удивление."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come on",
                                "target_lang_sense_clarification": "urging someone"
                            },
                            {
                                "target_lang_word": "well",
                                "target_lang_sense_clarification": "expressing surprise or impatience"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ну же",
                                "target_lang_sense_clarification": "побуждение"
                            },
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "нетерпение или удивление"
                            },
                            {
                                "target_lang_word": "давай",
                                "target_lang_sense_clarification": "побуждение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
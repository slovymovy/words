    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ee292c04-5f14-34b8-bab2-6393f311417c",
                    "sense_definition": "Действие по глаголу «спускать» или «спускаться»; перемещение сверху вниз.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы начали медленный <w>спуск</w> с вершины горы.",
                            "target_lang_translations": {
                                "en": "The tourists began a slow <w>descent</w> from the mountain peak."
                            }
                        },
                        {
                            "text": "Мы наблюдали за торжественным <w>спуском</w> корабля на воду.",
                            "target_lang_translations": {
                                "en": "We watched the ceremonial <w>launching</w> of the ship."
                            }
                        }
                    ],
                    "synonyms": [
                        "снижение",
                        "схождение"
                    ],
                    "antonyms": [
                        "подъём",
                        "восхождение"
                    ],
                    "common_phrases": [
                        "спуск на воду",
                        "спуск флага",
                        "быстрый спуск"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of moving downwards or lowering something; movement from top to bottom."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "descent",
                                "target_lang_sense_clarification": "movement downwards"
                            },
                            {
                                "target_lang_word": "launching",
                                "target_lang_sense_clarification": "of a ship into water"
                            },
                            {
                                "target_lang_word": "lowering",
                                "target_lang_sense_clarification": "act of moving something down"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7d66a13-3125-395d-b723-44dd21569ad5",
                    "sense_definition": "Наклонная поверхность, дорога или путь, ведущие вниз.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "location_slope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Машина осторожно ехала по крутому <w>спуску</w> к реке.",
                            "target_lang_translations": {
                                "en": "The car drove carefully down the steep <w>slope</w> to the river."
                            }
                        },
                        {
                            "text": "Зимой этот <w>спуск</w> становится очень скользким и опасным.",
                            "target_lang_translations": {
                                "en": "In winter, this <w>descent</w> becomes very slippery and dangerous."
                            }
                        }
                    ],
                    "synonyms": [
                        "склон",
                        "скат",
                        "откос"
                    ],
                    "antonyms": [
                        "подъём"
                    ],
                    "common_phrases": [
                        "крутой спуск",
                        "опасный спуск",
                        "спуск к морю"
                    ],
                    "target_lang_definitions": {
                        "en": "An inclined surface, road, or path leading downwards."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slope",
                                "target_lang_sense_clarification": "inclined surface"
                            },
                            {
                                "target_lang_word": "descent",
                                "target_lang_sense_clarification": "path leading down"
                            },
                            {
                                "target_lang_word": "incline",
                                "target_lang_sense_clarification": "gradient"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0bfd211-9794-39da-a836-7822d06ed617",
                    "sense_definition": "Выведение жидкости или газа из ёмкости, а также устройство для этого.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "technical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В ванной комнате сломался механизм <w>спуска</w> воды.",
                            "target_lang_translations": {
                                "en": "The water <w>flush</w> mechanism in the bathroom broke."
                            }
                        },
                        {
                            "text": "Для ремонта трубы потребовался полный <w>спуск</w> воды из системы отопления.",
                            "target_lang_translations": {
                                "en": "Repairing the pipe required a complete <w>draining</w> of water from the heating system."
                            }
                        }
                    ],
                    "synonyms": [
                        "слив"
                    ],
                    "antonyms": [
                        "набор"
                    ],
                    "common_phrases": [
                        "спуск воды",
                        "аварийный спуск"
                    ],
                    "target_lang_definitions": {

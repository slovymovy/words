            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7fa50ed3-557d-30d6-a76e-6c0d6ff7d58a",
                    "sense_definition": "Een persoon die fungeert als aanspreekpunt voor informatie of overleg binnen een organisatie of project.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heeft u al een vaste <w>contactpersoon</w> binnen onze organisatie?",
                            "target_lang_translations": {
                                "en": "Do you already have a regular <w>contact person</w> within our organization?"
                            }
                        },
                        {
                            "text": "Bij dringende vragen kunt u direct bellen met uw <w>contactpersoon</w>.",
                            "target_lang_translations": {
                                "en": "For urgent questions, you can call your <w>contact person</w> directly."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanspreekpersoon"
                    ],
                    "common_phrases": [
                        "vaste contactpersoon",
                        "contactpersoon voor noodgevallen"
                    ],
                    "target_lang_definitions": {
                        "en": "A person acting as a point of contact for information or consultation within an organization or project."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "contact person",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "contact",
                                "target_lang_sense_clarification": "common short form in business contexts"
                            },
                            {
                                "target_lang_word": "point of contact",
                                "target_lang_sense_clarification": "functional role"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecce6881-0d00-3975-96f7-fdf63c5ec9be",
                    "sense_definition": "Iemand die het contact verzorgt tussen twee personen of groepen die elkaar niet fysiek kunnen of mogen treffen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>contactpersoon</w> liet niets meer van zich horen na de laatste ontmoeting.",
                            "target_lang_translations": {
                                "en": "The <w>contact person</w> was not heard from again after the last meeting."
                            }
                        },
                        {
                            "text": "Hij fungeerde als <w>contactpersoon</w> tussen de twee strijdende partijen.",
                            "target_lang_translations": {
                                "en": "He acted as a <w>liaison</w> between the two warring parties."
                            }
                        }
                    ],
                    "synonyms": [
                        "contactman"
                    ],
                    "common_phrases": [
                        "fungeren als contactpersoon"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who facilitates contact between two persons or groups who cannot or may not meet physically."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "intermediary",
                                "target_lang_sense_clarification": "go-between"
                            },
                            {
                                "target_lang_word": "liaison",
                                "target_lang_sense_clarification": "connecting parties"
                            },
                            {
                                "target_lang_word": "contact person",
                                "target_lang_sense_clarification": "general term"
                            }
                        ]
                    }
                }
            ]

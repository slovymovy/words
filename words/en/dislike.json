                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предмет нелюбви",
                                "target_lang_sense_clarification": "то, что не нравится"
                            },
                            {
                                "target_lang_word": "антипатия",
                                "target_lang_sense_clarification": "в контексте вкусов"
                            }
                        ]
                    }
                }
            ]

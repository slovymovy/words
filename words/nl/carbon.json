{
    "word_family": [
        "carbonpapier"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7337c9d9-8ffa-34f0-8576-f9409bb725f9",
                    "sense_definition": "Een zeer sterk en licht materiaal bestaande uit kunststof versterkt met koolstofvezels, veel gebruikt in de sport en techniek.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn nieuwe racefiets heeft een frame dat volledig van <w>carbon</w> is gemaakt.",
                            "target_lang_translations": {
                                "ru": "У его нового гоночного велосипеда рама полностью сделана из <w>карбона</w>."
                            }
                        },
                        {
                            "text": "Onderdelen van <w>carbon</w> zijn populair in de autosport vanwege hun lage gewicht.",
                            "target_lang_translations": {
                                "ru": "Детали из <w>карбона</w> популярны в автоспорте из-за своего малого веса."
                            }
                        }
                    ],
                    "synonyms": [
                        "koolstofvezel"
                    ],
                    "common_phrases": [
                        "frame van carbon",
                        "wielen van carbon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень прочный и легкий материал, состоящий из пластика, армированного углеродными волокнами, широко используемый в спорте и технике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карбон",
                                "target_lang_sense_clarification": "материал (углепластик)"
                            },
                            {
                                "target_lang_word": "углепластик",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adcb8abd-5ae1-34be-a193-06b4d5bb4a2c",
                    "sense_definition": "Papier met een inktlaag dat tussen twee vellen wordt gelegd om tijdens het schrijven of typen direct een kopie te maken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "paper",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vroeger gebruikte men een velletje <w>carbon</w> om een doorslag van een brief te maken.",
                            "target_lang_translations": {
                                "ru": "Раньше использовали листок <w>копирки</w>, чтобы сделать копию письма."
                            }
                        },
                        {
                            "text": "Kun je nog ergens <w>carbon</w> kopen voor deze oude typemachine?",
                            "target_lang_translations": {
                                "ru": "Можно ли еще где-нибудь купить <w>копирку</w> для этой старой пишущей машинки?"
                            }
                        }
                    ],
                    "synonyms": [
                        "carbonpapier",
                        "doorslagpapier"
                    ],
                    "common_phrases": [
                        "velletje carbon"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бумага с чернильным слоем, которую кладут между двумя листами, чтобы сразу сделать копию при письме или печати."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копирка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "копировальная бумага",
                                "target_lang_sense_clarification": "официальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bc8ebfd-7b5e-38b7-936e-d888d23f4cc2",
                    "sense_definition": "Een zwarte, onzuivere diamant die voornamelijk voor industriële toepassingen zoals boren wordt gebruikt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mineral",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boorkop was versterkt met <w>carbon</w> om door het harde gesteente te komen.",
                            "target_lang_translations": {
                                "ru": "Буровая головка была усилена <w>карбонадо</w>, чтобы пройти сквозь твердую породу."
                            }
                        },
                        {
                            "text": "In de industrie wordt <w>carbon</w> gewaardeerd om zijn hardheid en niet om zijn schoonheid.",
                            "target_lang_translations": {
                                "ru": "В промышленности <w>карбонадо</w> ценится за твердость, а не за красоту."
                            }
                        }
                    ],
                    "synonyms": [
                        "carbonado"
                    ],
                    "target_lang_definitions": {
                        "ru": "Черный, непрозрачный технический алмаз, используемый в основном для промышленных целей, например, для бурения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карбонадо",
                                "target_lang_sense_clarification": "минерал"
                            },
                            {
                                "target_lang_word": "черный алмаз",
                                "target_lang_sense_clarification": "описательное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    {
                        "sense_id": "e9a31876-f130-364b-a161-930b4c39167e",
                        "entry_id": "8c079def-3904-35d0-8eca-79da7673863f",
                        "glosses": [
                            "Alternative form of adonise."
                        ],
                        "tags": [

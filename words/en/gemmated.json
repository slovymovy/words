            "senses": [
                {
                    "sense_id": "a1af5ab6-fd5b-377d-8862-eb167b20c79a",
                    "sense_definition": "Having buds or small outgrowths, often used in a biological context.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "biology_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientist observed the <w>gemmated</w> spores under the microscope to understand their reproduction.",
                            "target_lang_translations": {
                                "ru": "Ученый рассматривал <w>почкующиеся</w> споры под микроскопом, чтобы понять процесс их размножения."
                            }
                        },
                        {
                            "text": "This species of fungus is characterized by its unique <w>gemmated</w> surface structure.",
                            "target_lang_translations": {
                                "ru": "Этот вид гриба характеризуется уникальной структурой поверхности, <w>покрытой выростами</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "budded"
                    ],
                    "common_phrases": [
                        "gemmated spores"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий почки или небольшие выросты (обычно в биологическом контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почкующийся",
                                "target_lang_sense_clarification": "о процессе размножения или роста"
                            },
                            {
                                "target_lang_word": "имеющий выросты",
                                "target_lang_sense_clarification": "о структуре поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "601d2e02-430a-302a-b29a-b024c85a8f84",
                    "sense_definition": "Adorned or decorated with gems or jewels.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "decoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum displayed a <w>gemmated</w> chalice that once belonged to a medieval king.",
                            "target_lang_translations": {
                                "ru": "В музее была выставлена <w>украшенная драгоценными камнями</w> чаша, которая когда-то принадлежала средневековому королю."
                            }
                        },
                        {
                            "text": "She wore a heavily <w>gemmated</w> necklace that sparkled brilliantly under the ballroom lights.",
                            "target_lang_translations": {
                                "ru": "На ней было богато <w>усыпанное камнями</w> ожерелье, которое ярко сверкало в огнях бального зала."
                            }
                        }
                    ],
                    "synonyms": [
                        "bejeweled",
                        "jeweled"
                    ],
                    "antonyms": [
                        "unadorned",
                        "plain"
                    ],
                    "common_phrases": [
                        "gemmated crown",
                        "gemmated hilt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшенный драгоценными камнями или самоцветами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "украшенный драгоценными камнями",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "усыпанный самоцветами",
                                "target_lang_sense_clarification": "о богатом украшении"
                            }
                        ]
                    }
                }
            ]

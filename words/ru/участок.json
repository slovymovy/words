    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c265ccf2-cf18-35b6-b994-b51583e3922a",
                    "sense_definition": "Часть территории или земли, выделенная кому-либо во владение, для строительства или хозяйственных работ.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "land_property",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили купить земельный <w>участок</w> за городом, чтобы построить там большой и уютный дом.",
                            "target_lang_translations": {
                                "en": "We decided to buy a land <w>plot</w> outside the city to build a large and cozy house there."
                            }
                        },
                        {
                            "text": "Каждые выходные родители ездят на дачный <w>участок</w>, чтобы ухаживать за садом и огородом.",
                            "target_lang_translations": {
                                "en": "Every weekend, my parents go to their dacha <w>plot</w> to tend to the garden and vegetable patch."
                            }
                        }
                    ],
                    "common_phrases": [
                        "земельный участок",
                        "дачный участок",
                        "садовый участок",
                        "купить участок"
                    ],
                    "target_lang_definitions": {
                        "en": "A piece of land allocated for ownership, construction, or farming."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "plot",
                                "target_lang_sense_clarification": "of land"
                            },
                            {
                                "target_lang_word": "lot",
                                "target_lang_sense_clarification": "for building"
                            },
                            {
                                "target_lang_word": "piece of land",
                                "target_lang_sense_clarification": "general"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8e82bef-3da2-33a2-aa17-379278f0b0fc",
                    "sense_definition": "Отдельная часть какой-либо поверхности, пути, линии или пространства.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "segment_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водителей предупредили, что впереди находится опасный <w>участок</w> дороги с очень крутым поворотом.",
                            "target_lang_translations": {
                                "en": "Drivers were warned that there is a dangerous <w>section</w> of road ahead with a very sharp turn."
                            }
                        },
                        {
                            "text": "Врач внимательно осмотрел повреждённый <w>участок</w> кожи и аккуратно наложил на него повязку.",
                            "target_lang_translations": {
                                "en": "The doctor carefully examined the damaged <w>area</w> of skin and gently applied a bandage to it."
                            }
                        }
                    ],
                    "synonyms": [
                        "отрезок",
                        "часть"
                    ],
                    "common_phrases": [
                        "участок дороги",
                        "опасный участок",
                        "участок пути",
                        "участок кожи"
                    ],
                    "target_lang_definitions": {
                        "en": "A specific part of a surface, path, line, or space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "section",
                                "target_lang_sense_clarification": "of a road or line"
                            },
                            {
                                "target_lang_word": "area",
                                "target_lang_sense_clarification": "of a surface, e.g., skin"
                            },
                            {
                                "target_lang_word": "stretch",
                                "target_lang_sense_clarification": "of a road or river"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "686d6531-504f-3fc1-b2e8-570af568c75c",
                    "sense_definition": "Административное подразделение, обслуживаемое специальным лицом или учреждением (в медицине, на выборах и т.д.).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "admin_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В день выборов я пошёл на свой избирательный <w>участок</w>, чтобы проголосовать за кандидата.",
                            "target_lang_translations": {
                                "en": "On election day, I went to my polling <w>station</w> to vote for a candidate."
                            }
                        },
                        {
                            "text": "Молодой доктор получил назначение на сложный врачебный <w>участок</w> в отдалённой сельской местности.",
                            "target_lang_translations": {
                                "en": "The young doctor received an assignment to a difficult medical <w>district</w> in a remote rural area."
                            }
                        }
                    ],
                    "common_phrases": [
                        "избирательный участок",
                        "врачебный участок",
                        "прийти на участок"
                    ],
                    "target_lang_definitions": {
                        "en": "An administrative subdivision served by a specific official or institution (medical, electoral, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "district",
                                "target_lang_sense_clarification": "medical or administrative"
                            },
                            {
                                "target_lang_word": "station",
                                "target_lang_sense_clarification": "in 'polling station'"
                            },
                            {
                                "target_lang_word": "precinct",
                                "target_lang_sense_clarification": "voting or police district"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73d3c137-45a2-35ec-88c0-527d0fe58a21",
                    "sense_definition": "Отделение полиции; здание, где располагается это отделение.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "police",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хулиганов задержали на месте преступления и сразу отвезли в ближайший полицейский <w>участок</w>.",
                            "target_lang_translations": {
                                "en": "The hooligans were detained at the crime scene and immediately taken to the nearest police <w>station</w>."
                            }
                        },
                        {
                            "text": "Если вы продолжите нарушать общественный порядок, нас всех заберут в <w>участок</w> для выяснения обстоятельств.",
                            "target_lang_translations": {
                                "en": "If you continue to disturb the public order, we will all be taken to the <w>station</w> for questioning."
                            }
                        }
                    ],
                    "common_phrases": [
                        "полицейский участок",
                        "забрать в участок",
                        "сидеть в участке"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто используется как сокращение от 'полицейский участок'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A police station or the building where it is located."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "police station",
                                "target_lang_sense_clarification": "building"
                            },
                            {
                                "target_lang_word": "station",
                                "target_lang_sense_clarification": "short for police station"
                            },
                            {
                                "target_lang_word": "precinct",
                                "target_lang_sense_clarification": "US context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65d4621a-d4eb-3355-8587-fbd4689f2d7d",
                    "sense_definition": "Отдельная область деятельности, направление в какой-либо работе.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему доверили самый ответственный <w>участок</w> работы, так как он был самым опытным сотрудником.",
                            "target_lang_translations": {
                                "en": "He was entrusted with the most responsible <w>area</w> of work, as he was the most experienced employee."
                            }
                        },
                        {
                            "text": "Этот <w>участок</w> проекта требует особого внимания и высокой квалификации всех специалистов.",
                            "target_lang_translations": {
                                "en": "This <w>section</w> of the project requires special attention and high qualification from all specialists."
                            }
                        }
                    ],
                    "synonyms": [
                        "область"
                    ],
                    "common_phrases": [
                        "участок работы",
                        "ответственный участок",
                        "важный участок"
                    ],
                    "target_lang_definitions": {
                        "en": "A separate field of activity or area of work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "area",
                                "target_lang_sense_clarification": "of work or responsibility"
                            },
                            {
                                "target_lang_word": "section",
                                "target_lang_sense_clarification": "of a project"
                            },
                            {
                                "target_lang_word": "sector",
                                "target_lang_sense_clarification": "of operations"
                            }
                        ]
                    }
                }
            ]
        }

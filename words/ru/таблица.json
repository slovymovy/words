    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0f2ee5e-dbeb-3d1d-9a81-d86da2eb3115",
                    "sense_definition": "Сведения (числа или текст), расположенные в строках и столбцах для удобства восприятия и анализа.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "data_grid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый школьник должен знать <w>таблицу</w> умножения наизусть.",
                            "target_lang_translations": {
                                "en": "Every student must know the multiplication <w>table</w> by heart."
                            }
                        },
                        {
                            "text": "Мы аккуратно занесли все полученные результаты в сводную <w>таблицу</w>.",
                            "target_lang_translations": {
                                "en": "We carefully entered all the obtained results into a summary <w>table</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "таблица умножения",
                        "периодическая таблица",
                        "заполнить таблицу",
                        "электронная таблица"
                    ],
                    "target_lang_definitions": {
                        "en": "Information arranged in rows and columns for easy reading and analysis."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "table",
                                "target_lang_sense_clarification": "data arrangement"
                            },
                            {
                                "target_lang_word": "spreadsheet",
                                "target_lang_sense_clarification": "electronic document"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3809fa1e-6295-3b5b-b77a-ff8d6a87c465",
                    "sense_definition": "Учебное наглядное пособие, обычно в виде плаката с систематизированными данными, схемами или рисунками.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "educational_aid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На стенах кабинета биологии висели красочные <w>таблицы</w> с растениями.",
                            "target_lang_translations": {
                                "en": "Colorful <w>charts</w> with plants hung on the walls of the biology classroom."
                            }
                        },
                        {
                            "text": "Учитель использовал наглядную <w>таблицу</w> для объяснения грамматических правил.",
                            "target_lang_translations": {
                                "en": "The teacher used a visual <w>chart</w> to explain grammar rules."
                            }
                        }
                    ],
                    "common_phrases": [
                        "наглядная таблица",
                        "учебная таблица"
                    ],
                    "target_lang_definitions": {
                        "en": "An educational visual aid or poster displaying systematic information."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chart",
                                "target_lang_sense_clarification": "visual aid/poster"
                            },
                            {
                                "target_lang_word": "table",
                                "target_lang_sense_clarification": "systematic display"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c03409f7-e110-3436-988e-7f4c074451cc",
                    "sense_definition": "Структура данных в программировании, позволяющая хранить информацию и находить её по ключу.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Программист создал новую <w>таблицу</w> в базе данных для хранения заказов.",
                            "target_lang_translations": {
                                "en": "The programmer created a new <w>table</w> in the database to store orders."
                            }
                        },
                        {
                            "text": "Система использует хеш-<w>таблицы</w> для быстрого поиска нужных значений.",
                            "target_lang_translations": {
                                "en": "The system uses hash <w>tables</w> to quickly find the necessary values."
                            }
                        }
                    ],
                    "synonyms": [

                {
                    "sense_id": "a28392db-3683-3b2c-8e61-78d3233f29df",
                    "sense_definition": "To stop the supply of power to a device or system; to turn off or deactivate.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>unpower</w> the machine completely before opening the maintenance panel.",
                            "target_lang_translations": {
                                "ru": "Вы должны полностью <w>обесточить</w> станок, прежде чем открывать панель технического обслуживания."
                            }
                        },
                        {
                            "text": "The technician rushed to <w>unpower</w> the system to prevent a catastrophic surge.",
                            "target_lang_translations": {
                                "ru": "Техник поспешил <w>отключить питание</w> системы, чтобы предотвратить катастрофический скачок напряжения."
                            }
                        }
                    ],
                    "synonyms": [
                        "deactivate",
                        "disconnect",
                        "disable",
                        "depower"
                    ],
                    "antonyms": [

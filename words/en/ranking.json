{
    "word_family": [
        "rank",
        "ranker"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "598ee409-4d5a-3be3-87ce-a09f961a29c8",
                    "sense_definition": "One’s relative placement or position in a list, scale, or hierarchy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tennis player improved her <w>ranking</w> significantly after winning the major tournament.",
                            "target_lang_translations": {
                                "ru": "Теннисистка значительно улучшила свой <w>рейтинг</w> после победы в крупном турнире."
                            }
                        },
                        {
                            "text": "The university dropped several places in the national <w>rankings</w> this year.",
                            "target_lang_translations": {
                                "ru": "В этом году университет опустился на несколько строчек в национальном <w>рейтинге</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "standing",
                        "position",
                        "placement",
                        "rating"
                    ],
                    "common_phrases": [
                        "world ranking",
                        "ranking system",
                        "high ranking",
                        "ranking list"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относительное положение или место в списке, шкале или иерархии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рейтинг",
                                "target_lang_sense_clarification": "положение в списке оценки (спорт, университеты)"
                            },
                            {
                                "target_lang_word": "место",
                                "target_lang_sense_clarification": "позиция в списке или иерархии"
                            },
                            {
                                "target_lang_word": "ранжирование",
                                "target_lang_sense_clarification": "процесс или результат расстановки по порядку"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f72bc7e8-6d7f-3d66-8d23-8b7a1a1a3aab",
                    "sense_definition": "The present participle and gerund of <w>rank</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee is currently <w>ranking</w> the candidates based on their qualifications.",
                            "target_lang_translations": {
                                "ru": "Комитет в данный момент <w>ранжирует</w> кандидатов на основе их квалификации."
                            }
                        },
                        {
                            "text": "He is <w>ranking</w> high in the polls despite the recent controversy.",
                            "target_lang_translations": {
                                "ru": "Он <w>занимает</w> высокие позиции в опросах, несмотря на недавний скандал."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола rank (классифицировать, занимать место)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ранжирование",
                                "target_lang_sense_clarification": "процесс упорядочивания (герундий)"
                            },
                            {
                                "target_lang_word": "классификация",
                                "target_lang_sense_clarification": "распределение по категориям"
                            },
                            {
                                "target_lang_word": "занимающий место",
                                "target_lang_sense_clarification": "находящийся на определенной позиции (причастие)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1e86234c-46de-3d5a-86a2-8dbcf18dc0d1",
                    "sense_definition": "Superior in rank, position, or authority; holding the highest status in a group.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As the <w>ranking</w> officer at the scene, she took immediate command of the operation.",
                            "target_lang_translations": {
                                "ru": "Будучи <w>старшим</w> офицером на месте происшествия, она немедленно взяла на себя руководство операцией."
                            }
                        },
                        {
                            "text": "He is the <w>ranking</w> member of the committee and leads all the discussions.",
                            "target_lang_translations": {
                                "ru": "Он является <w>высокопоставленным</w> членом комитета и ведет все дискуссии."
                            }
                        }
                    ],
                    "synonyms": [
                        "senior",
                        "leading",
                        "top",
                        "chief",
                        "superior"
                    ],
                    "antonyms": [
                        "junior",
                        "subordinate"
                    ],
                    "common_phrases": [
                        "ranking officer",
                        "ranking member",
                        "ranking official"
                    ],
                    "target_lang_definitions": {
                        "ru": "Старший по званию, положению или полномочиям; занимающий высший статус в группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "старший",
                                "target_lang_sense_clarification": "по званию или должности"
                            },
                            {
                                "target_lang_word": "высокопоставленный",
                                "target_lang_sense_clarification": "имеющий высокий чин или статус"
                            },
                            {
                                "target_lang_word": "вышестоящий",
                                "target_lang_sense_clarification": "обладающий большей властью"
                            }
                        ]
                    }
                }
            ]
        }
    ]

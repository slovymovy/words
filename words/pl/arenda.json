    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5d2a2a72-d5a3-30f4-8523-b18211661d25",
                    "sense_definition": "Dawne określenie na dzierżawę majątku, gruntów lub praw do czerpania dochodów (np. z młyna czy karczmy).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "lease_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szlachcic oddał swoje dobra w <w>arendę</w>, aby zapewnić sobie stały dochód bez konieczności zarządzania nimi.",
                            "target_lang_translations": {
                                "ru": "Дворянин сдал свои имения в <w>аренду</w>, чтобы обеспечить себе постоянный доход без необходимости управлять ими."
                            }
                        },
                        {
                            "text": "Umowa <w>arendy</w> została zawarta na okres trzech lat z możliwością przedłużenia.",
                            "target_lang_translations": {
                                "ru": "Договор <w>аренды</w> был заключен сроком на три года с возможностью продления."
                            }
                        }
                    ],
                    "synonyms": [
                        "dzierżawa",
                        "najem",
                        "wynajem"
                    ],
                    "common_phrases": [
                        "wziąć w arendę",
                        "dać w arendę",
                        "trzymać w arendzie"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo używane głównie w kontekście historycznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название аренды имущества, земель или прав на получение дохода (например, с мельницы или корчмы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аренда",
                                "target_lang_sense_clarification": "сдача имущества внаем"
                            },
                            {
                                "target_lang_word": "откуп",
                                "target_lang_sense_clarification": "право на сбор доходов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9968924f-50db-3503-8800-2274d55199f8",
                    "sense_definition": "Opłata wnoszona właścicielowi za dzierżawę majątku; czynsz dzierżawny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "lease_payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzierżawca spóźnił się z zapłatą rocznej <w>arendy</w> z powodu słabych zbiorów.",
                            "target_lang_translations": {
                                "ru": "Арендатор опоздал с уплатой годовой <w>аренды</w> из-за плохого урожая."
                            }
                        },
                        {
                            "text": "Wysoka <w>arenda</w> stanowiła duże obciążenie dla prowadzącego karczmę.",
                            "target_lang_translations": {
                                "ru": "Высокая <w>арендная плата</w> была большим бременем для содержателя корчмы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "płacić arendę"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo używane głównie w kontekście historycznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Плата, вносимая владельцу за аренду имущества; арендная плата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "арендная плата",
                                "target_lang_sense_clarification": "деньги за аренду"
                            },
                            {
                                "target_lang_word": "чинш",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92aac34e-f610-3a19-9a48-e197687012fd",
                    "sense_definition": "Karczma lub gospoda, często prowadzona przez dzierżawcę.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopi zebrali się w wiejskiej <w>arendzie</w>, aby odpocząć po ciężkiej pracy.",
                            "target_lang_translations": {
                                "ru": "Крестьяне собрались в деревенской <w>корчме</w>, чтобы отдохнуть после тяжелой работы."
                            }
                        },
                        {
                            "text": "Stara <w>arenda</w> stała przy głównym trakcie i przyciągała wielu podróżnych.",
                            "target_lang_translations": {
                                "ru": "Старая <w>корчма</w> стояла у главного тракта и привлекала множество путников."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo używane głównie w kontekście historycznym"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "występuje w gwarach i literaturze regionalnej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Корчма или постоялый двор, часто управляемые арендатором."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корчма",
                                "target_lang_sense_clarification": "питейное заведение"
                            },
                            {
                                "target_lang_word": "шинок",
                                "target_lang_sense_clarification": "кабак"
                            }
                        ]
                    }
                }
            ]
        }
    ]

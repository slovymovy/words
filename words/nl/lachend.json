                    "sense_id": "aa4b048d-a4a3-3b58-9148-1ebb81f520eb",
                    "sense_definition": "Uiting gevend aan vrolijkheid of plezier door te lachen; vrolijk kijkend.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>lachende</w> meisjes hadden veel plezier tijdens het feestje.",
                            "target_lang_translations": {
                                "en": "The <w>laughing</w> girls had a lot of fun during the party.",
                                "ru": "<w>Смеющиеся</w> девочки очень веселились во время вечеринки."
                            }
                        },
                        {
                            "text": "Ik kijk in haar <w>lachende</w> gezicht en voel me meteen beter.",
                            "target_lang_translations": {
                                "en": "I look into her <w>smiling</w> face and feel better immediately.",
                                "ru": "Я смотрю на ее <w>улыбающееся</w> лицо и сразу чувствую себя лучше."
                            }
                        }
                    ],
                    "synonyms": [
                        "vrolijk"
                    ],
                    "common_phrases": [
                        "lachend gezicht",
                        "lachende ogen",
                        "lachende derde"
                    ],
                    "target_lang_definitions": {
                        "en": "Expressing cheerfulness or pleasure by laughing; looking cheerful.",
                        "ru": "Выражающий веселье или удовольствие смехом; имеющий веселый вид."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "laughing",
                                "target_lang_sense_clarification": "expressing joy through laughter"
                            },
                            {
                                "target_lang_word": "smiling",
                                "target_lang_sense_clarification": "looking cheerful (e.g., a face or eyes)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "смеющийся",
                                "target_lang_sense_clarification": "выражающий радость смехом"
                            },
                            {
                                "target_lang_word": "улыбающийся",
                                "target_lang_sense_clarification": "имеющий веселый вид (например, о лице или глазах)"
                            }
                        ]
                    }
                }
            ]

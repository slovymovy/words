                        "trafficker"
                    ],
                    "common_phrases": [
                        "notorious owler",
                        "gang of owlers"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Контрабандист, занимающийся незаконным вывозом овец или шерсти (исторический термин)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контрабандист",
                                "target_lang_sense_clarification": "шерсти или овец"
                            },
                            {
                                "target_lang_word": "контрабандист шерсти",
                                "target_lang_sense_clarification": "историческое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e266e8d2-c6b7-377e-bbad-27e9868d2074",
                    "sense_definition": "The alder tree.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He sat by the riverbank in the shade of a large <w>owler</w>.",

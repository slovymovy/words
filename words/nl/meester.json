                {
                    "sense_id": "b6a8e2bf-5d0c-3d69-bfd3-821149750a62",
                    "sense_definition": "Een onderwijzer, met name in het basisonderwijs.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>meester</w> vertelde een spannend verhaal aan de klas.",
                            "target_lang_translations": {
                                "ru": "<w>Учитель</w> рассказал классу захватывающую историю."
                            }
                        },
                        {
                            "text": "Als de bel gaat, mogen we van de <w>meester</w> naar buiten.",
                            "target_lang_translations": {
                                "ru": "Когда прозвенит звонок, <w>учитель</w> разрешает нам выйти."
                            }
                        }
                    ],
                    "synonyms": [
                        "onderwijzer",
                        "leraar",
                        "schoolmeester"
                    ],
                    "antonyms": [
                        "leerling"
                    ],
                    "common_phrases": [
                        "meester en juf",
                        "dag meester"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учитель, особенно в начальной школе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "учитель",
                                "target_lang_sense_clarification": "преподаватель в школе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46e22aa8-8868-3fac-bbcf-51cfaa834b4b",
                    "sense_definition": "Iemand die macht en gezag heeft over anderen, zoals een baas of eigenaar.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hond luistert heel goed naar zijn <w>meester</w>.",
                            "target_lang_translations": {
                                "ru": "Собака очень хорошо слушается своего <w>хозяина</w>."
                            }
                        },
                        {
                            "text": "Tijdens de wedstrijd was hij heer en <w>meester</w> op het veld.",
                            "target_lang_translations": {
                                "ru": "Во время матча он был полновластным <w>хозяином</w> на поле."
                            }
                        }
                    ],
                    "synonyms": [
                        "schoolmeester"
                    ],
                    "antonyms": [
                        "ondergeschikte",
                        "slaaf"
                    ],
                    "common_phrases": [
                        "heer en meester",
                        "zijn meester erkennen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто имеет власть и авторитет над другими, например, начальник или владелец."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хозяин",
                                "target_lang_sense_clarification": "владелец животного или раба"
                            },
                            {
                                "target_lang_word": "господин",
                                "target_lang_sense_clarification": "властитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38b6581d-657a-3238-811c-8bc91a9d3b75",
                    "sense_definition": "Iemand die uitblinkt in een bepaalde vaardigheid, kunst of vak.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rembrandt was een groot <w>meester</w> in het schilderen met licht.",
                            "target_lang_translations": {
                                "ru": "Рембрандт был великим <w>мастером</w> светотени."
                            }
                        },
                        {
                            "text": "Met die prachtige beweging toonde hij zich een waar <w>meester</w>.",
                            "target_lang_translations": {
                                "ru": "Этим прекрасным движением он показал себя настоящим <w>мастером</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "instructeur"
                    ],
                    "antonyms": [
                        "leerling"
                    ],
                    "common_phrases": [
                        "oude meesters",
                        "een waar meester"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто преуспевает в определенном навыке, искусстве или ремесле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мастер",
                                "target_lang_sense_clarification": "знаток своего дела"
                            },
                            {
                                "target_lang_word": "виртуоз",
                                "target_lang_sense_clarification": "человек, достигший совершенства в деле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18f20a94-def7-3476-9308-4e567e4d4c2a",
                    "sense_definition": "Een titel voor iemand die een universitaire studie in de rechten heeft afgerond.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Meester</w> De Vries pleitte voor vrijspraak van zijn cliënt.",
                            "target_lang_translations": {
                                "ru": "<w>Адвокат</w> Де Врис выступал за оправдание своего клиента."
                            }
                        },
                        {
                            "text": "Zij mag zich nu <w>meester</w> in de rechten noemen.",
                            "target_lang_translations": {
                                "ru": "Теперь она может называть себя <w>магистром</w> права."
                            }
                        }
                    ],
                    "common_phrases": [
                        "meester in de rechten"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "titel"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул для того, кто закончил университетский курс права (магистр права)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магистр права",
                                "target_lang_sense_clarification": "ученая степень"
                            },
                            {
                                "target_lang_word": "адвокат",
                                "target_lang_sense_clarification": "обращение к юристу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2c79751-557a-3780-814a-b06175a95e1b",
                    "sense_definition": "Een ambachtsman die zijn vak volledig beheerst en bevoegd is om leerlingen op te leiden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>meester</w> leerde zijn gezel de fijne kneepjes van het vak.",
                            "target_lang_translations": {
                                "ru": "<w>Мастер</w> обучал своего подмастерья тонкостям ремесла."
                            }
                        },
                        {
                            "text": "Om <w>meester</w> te worden, moest hij eerst een meesterproef afleggen.",
                            "target_lang_translations": {
                                "ru": "Чтобы стать <w>мастером</w>, он должен был сначала пройти испытание на звание мастера."
                            }
                        }
                    ],
                    "antonyms": [
                        "gezel",
                        "leerling"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "gildenstelsel"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ремесленник, который в совершенстве владеет своим делом и имеет право обучать учеников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мастер",
                                "target_lang_sense_clarification": "глава ремесленной мастерской"
                            }
                        ]
                    }
                }
            ]

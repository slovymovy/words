{
    "word_family": [
        "while",
        "awhile"
    ],
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "92d07d3b-3599-37a8-8f5a-9e234a03bd75",
                    "sense_definition": "During the time that something is happening; at the same time as.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drivers must switch off their engines <w>whilst</w> waiting.",
                            "target_lang_translations": {
                                "ru": "Водители должны выключать двигатели, <w>пока</w> ждут."
                            }
                        },
                        {
                            "text": "She read a book <w>whilst</w> the dinner was cooking.",
                            "target_lang_translations": {
                                "ru": "Она читала книгу, <w>в то время как</w> готовился ужин."
                            }
                        }
                    ],
                    "synonyms": [
                        "while",
                        "when",
                        "as"
                    ],
                    "common_phrases": [
                        "whilst waiting",
                        "whilst doing",
                        "whilst away"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly British"
                        },
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В то время как что-то происходит; одновременно с чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в то время как",
                                "target_lang_sense_clarification": "формальный союз для одновременных действий"
                            },
                            {
                                "target_lang_word": "пока",
                                "target_lang_sense_clarification": "обозначение длительности или одновременности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e39e909-32a5-3d39-af2e-c336fa699507",
                    "sense_definition": "Used to show a contrast or difference between two things; whereas.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "contrastive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I prefer tea, <w>whilst</w> my brother loves coffee.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю чай, <w>в то время как</w> мой брат любит кофе."
                            }
                        },
                        {
                            "text": "Some people find the task easy, <w>whilst</w> others struggle with it.",
                            "target_lang_translations": {
                                "ru": "Одни находят эту задачу легкой, <w>тогда как</w> другие испытывают с ней трудности."
                            }
                        }
                    ],
                    "synonyms": [
                        "while"
                    ],
                    "common_phrases": [
                        "whilst others"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly British"
                        },
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для противопоставления или указания на различие между двумя фактами; тогда как."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тогда как",
                                "target_lang_sense_clarification": "для явного противопоставления"
                            },
                            {
                                "target_lang_word": "в то время как",
                                "target_lang_sense_clarification": "для сравнения двух ситуаций"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "0197b455-23ce-3be3-8169-8b3a8af1ce22",
                    "sense_definition": "stanowisko pracy ze stałym wynagrodzeniem i w pełnym wymiarze godzin",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "praca_stanowisko",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mąż Oli pracuje na <w>etacie</w>, więc nie musi się przejmować opłacaniem składek.",
                            "target_lang_translations": {
                                "ru": "Муж Оли работает в <w>штате</w>, поэтому ему не нужно беспокоиться об уплате взносов."
                            }
                        },
                        {
                            "text": "Szukam stabilnej pracy na pełny <w>etat</w> w biurze rachunkowym.",
                            "target_lang_translations": {
                                "ru": "Я ищу стабильную работу на полную <w>ставку</w> в бухгалтерской фирме."
                            }
                        }
                    ],
                    "synonyms": [
                        "posada",
                        "stanowisko"
                    ],
                    "antonyms": [
                        "praca dorywcza"
                    ],
                    "common_phrases": [
                        "na etacie",
                        "pełny etat",
                        "pół etatu",
                        "pracować na etacie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Должность с фиксированной зарплатой и полной занятостью"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ставка",
                                "target_lang_sense_clarification": "должность"
                            },
                            {
                                "target_lang_word": "штат",
                                "target_lang_sense_clarification": "постоянный состав сотрудников"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6c2b5a1-8c27-377a-940f-ab89fefcf0fc",
                    "sense_definition": "liczba godzin pracy przewidziana jako pełny wymiar dla osoby zatrudnionej na podstawie umowy o pracę",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "praca_wymiar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej firmie <w>etat</w> wynosi 35 godzin tygodniowo.",
                            "target_lang_translations": {
                                "ru": "В этой фирме полная <w>ставка</w> составляет 35 часов в неделю."
                            }
                        },
                        {
                            "text": "Po urodzeniu dziecka wróciła do pracy na pół <w>etatu</w>.",
                            "target_lang_translations": {
                                "ru": "После рождения ребенка она вернулась на работу на половину <w>ставки</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wymiar czasu pracy"
                    ],
                    "common_phrases": [
                        "wymiar etatu",
                        "część etatu",
                        "ułamek etatu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество рабочих часов, предусмотренное как полная занятость"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ставка",
                                "target_lang_sense_clarification": "объем занятости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "533b0a18-e055-3760-b90f-c995a3d41439",
                    "sense_definition": "określona liczba żołnierzy i środków materiałowych w jednostce wojskowej",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "wojsko",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dowódca jednostki zatwierdził nowy <w>etat</w> dla plutonu łączności.",
                            "target_lang_translations": {
                                "ru": "Командир части утвердил новый <w>штат</w> для взвода связи."
                            }
                        },
                        {
                            "text": "Oficer został oficjalnie skreślony z <w>etatu</w> po przeniesieniu do rezerwy.",
                            "target_lang_translations": {
                                "ru": "Офицер был официально исключен из <w>штата</w> после перевода в запас."
                            }
                        }
                    ],
                    "synonyms": [
                        "stan osobowy"
                    ],
                    "common_phrases": [
                        "zgodnie z etatem",
                        "skreślić z etatu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Установленная численность личного состава и материальных средств в воинской части"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штат",
                                "target_lang_sense_clarification": "организационная структура"
                            }
                        ]
                    }
                }
            ]
        }
    ]

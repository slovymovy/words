            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f1ef457b-1ab1-345d-a866-d825b2a7555a",
                    "sense_definition": "Przykrycie twarzy lub jej części z otworami na oczy, używane w celach ochronnych, obrzędowych lub dla kamuflażu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz w szpitalu zawsze nosi ochronną <maskę> podczas operacji.",
                            "target_lang_translations": {
                                "ru": "Врач в больнице всегда носит защитную <маску> во время операции."
                            }
                        },
                        {
                            "text": "Na balu przebierańców każdy uczestnik miał na twarzy kolorową <maskę>.",
                            "target_lang_translations": {
                                "ru": "На бал-маскараде у каждого участника была на лице цветная <маска>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "maska gazowa",
                        "maska ochronna",
                        "nosić maskę",
                        "maska tlenowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрытие для лица или его части с отверстиями для глаз, используемое в защитных, ритуальных целях или для маскировки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маска",
                                "target_lang_sense_clarification": "предмет на лицо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebf35b36-949a-3e68-b4e9-e5d7737f673b",
                    "sense_definition": "Otwierana pokrywa w przedniej części nadwozia samochodu, osłaniająca silnik.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca musiał podnieść <maskę>, aby sprawdzić poziom oleju w silniku.",
                            "target_lang_translations": {
                                "ru": "Водитель должен был поднять <капот>, чтобы проверить уровень масла в двигателе."
                            }
                        },
                        {
                            "text": "Mechanik długo zaglądał pod <maskę>, szukając przyczyny awarii.",
                            "target_lang_translations": {
                                "ru": "Механик долго заглядывал под <капот> в поисках причины поломки."
                            }
                        }
                    ],
                    "synonyms": [
                        "kapot"
                    ],
                    "common_phrases": [
                        "podnieść maskę",
                        "zajrzeć pod maskę",
                        "pod maską"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открывающаяся крышка в передней части кузова автомобиля, закрывающая двигатель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капот",
                                "target_lang_sense_clarification": "автомобиля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cefbeca7-8c1d-3732-af78-9744b9bfaf10",
                    "sense_definition": "Potoczne określenie na twarz.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas gry w siatkówkę dostał piłką prosto w <maskę>.",
                            "target_lang_translations": {
                                "ru": "Во время игры в волейбол ему попали мячом прямо в <физиономию>."
                            }
                        },
                        {
                            "text": "Zmyj ten makijaż, bo masz strasznie nienaturalną <maskę>.",
                            "target_lang_translations": {
                                "ru": "Смой этот макияж, а то у тебя жутко неестественная <маска>."
                            }
                        }
                    ],

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e6e57254-d155-312f-84d2-c56df1ed0d78",
                    "sense_definition": "Een mechanisme of stand van de tandwielen in een voertuig die bepaalt hoe zwaar men trapt of hoeveel toeren de motor maakt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "gear_mechanism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij reed in de tweede <w>versnelling</w> de steile helling op.",
                            "target_lang_translations": {
                                "en": "He drove up the steep slope in second <w>gear</w>.",
                                "ru": "Он въехал на крутой склон на второй <w>передаче</w>."
                            }
                        },
                        {
                            "text": "Mijn nieuwe fiets heeft wel eenentwintig <w>versnellingen</w>.",
                            "target_lang_translations": {
                                "en": "My new bike has twenty-one <w>gears</w>.",
                                "ru": "У моего нового велосипеда целая двадцать одна <w>скорость</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "overbrenging"
                    ],
                    "common_phrases": [
                        "in de versnelling",
                        "automatische versnelling",
                        "naar een lagere versnelling"
                    ],
                    "target_lang_definitions": {
                        "en": "A specific setting of the gears in a vehicle or bicycle that controls the relationship between engine/pedal speed and wheel speed.",
                        "ru": "Механизм или положение шестерен в транспортном средстве, определяющее соотношение оборотов двигателя и колес."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gear",
                                "target_lang_sense_clarification": "setting in a transmission"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "передача",
                                "target_lang_sense_clarification": "ступень в коробке передач"
                            },
                            {
                                "target_lang_word": "скорость",
                                "target_lang_sense_clarification": "разговорное название передачи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f02d9137-1c3a-331d-82b5-75ae9d53cf55",
                    "sense_definition": "Het proces waarbij de snelheid toeneemt of het sneller gaan van een beweging.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "acceleration_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er volgde een plotselinge <w>versnelling</w> van het tempo in de wedstrijd.",
                            "target_lang_translations": {
                                "en": "There followed a sudden <w>acceleration</w> of the pace in the match.",
                                "ru": "В матче последовало внезапное <w>ускорение</w> темпа."
                            }
                        },
                        {
                            "text": "De <w>versnelling</w> van deze sportwagen is indrukwekkend.",
                            "target_lang_translations": {
                                "en": "The <w>acceleration</w> of this sports car is impressive.",
                                "ru": "<w>Разгон</w> этого спорткара впечатляет."
                            }
                        }
                    ],
                    "synonyms": [
                        "acceleratie"
                    ],
                    "antonyms": [
                        "vertraging"
                    ],
                    "common_phrases": [
                        "in een stroomversnelling"
                    ],
                    "target_lang_definitions": {
                        "en": "The process or act of increasing speed.",
                        "ru": "Процесс увеличения скорости или темпа движения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "acceleration",
                                "target_lang_sense_clarification": "increase in speed"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ускорение",
                                "target_lang_sense_clarification": "увеличение темпа"
                            },
                            {
                                "target_lang_word": "разгон",
                                "target_lang_sense_clarification": "набор скорости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ffb1d3f-b33d-3172-9b9e-2124cbbade1b",
                    "sense_definition": "De mate waarin de snelheid verandert per tijdseenheid.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "acceleration_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zwaartekracht zorgt voor een constante <w>versnelling</w> van vallende voorwerpen.",
                            "target_lang_translations": {
                                "en": "Gravity causes a constant <w>acceleration</w> of falling objects.",
                                "ru": "Гравитация обеспечивает постоянное <w>ускорение</w> падающих предметов."
                            }
                        },
                        {
                            "text": "We moeten de gemiddelde <w>versnelling</w> berekenen voor dit natuurkundeprobleem.",
                            "target_lang_translations": {
                                "en": "We need to calculate the average <w>acceleration</w> for this physics problem.",
                                "ru": "Нам нужно вычислить среднее <w>ускорение</w> для этой задачи по физике."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "physics"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The rate at which velocity changes per unit of time.",
                        "ru": "Физическая величина, характеризующая быстроту изменения скорости за единицу времени."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "acceleration",
                                "target_lang_sense_clarification": "physics quantity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ускорение",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

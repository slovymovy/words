    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1eb23f64-1389-326d-917c-19b39daf194a",
                    "sense_definition": "W dniu następującym bezpośrednio po dniu dzisiejszym",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_relative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę iść spać wcześnie, ponieważ <w>jutro</w> mam bardzo ważny egzamin.",
                            "target_lang_translations": {
                                "ru": "Мне нужно лечь спать пораньше, так как <w>завтра</w> у меня очень важный экзамен."
                            }
                        },
                        {
                            "text": "Jeśli pogoda dopisze, to <w>jutro</w> pojedziemy na wycieczkę rowerową.",
                            "target_lang_translations": {
                                "ru": "Если погода будет хорошей, то <w>завтра</w> мы поедем на велосипедную прогулку."
                            }
                        }
                    ],
                    "antonyms": [
                        "wczoraj"
                    ],
                    "common_phrases": [
                        "do jutra",
                        "na jutro",
                        "jutro rano",
                        "jutro wieczorem"
                    ],
                    "target_lang_definitions": {
                        "ru": "В день, следующий непосредственно за сегодняшним"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завтра",
                                "target_lang_sense_clarification": "на следующий день"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "31e85e56-1f95-3fb0-ae9a-bd95729db17c",
                    "sense_definition": "Dzień, który nastąpi po dniu dzisiejszym",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_relative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie odkładaj pracy na <w>jutro</w>, jeśli możesz ją wykonać dzisiaj.",
                            "target_lang_translations": {
                                "ru": "Не откладывай работу на <w>завтра</w>, если можешь выполнить её сегодня."
                            }
                        },
                        {
                            "text": "Od <w>jutra</w> wchodzi w życie nowy rozkład jazdy pociągów.",
                            "target_lang_translations": {
                                "ru": "С <w>завтрашнего дня</w> вступает в силу новое расписание поездов."
                            }
                        }
                    ],
                    "synonyms": [
                        "zajutrze"
                    ],
                    "antonyms": [
                        "wczoraj"
                    ],
                    "common_phrases": [
                        "od jutra",
                        "do jutra"
                    ],
                    "target_lang_definitions": {
                        "ru": "День, который наступит после сегодняшнего"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завтра",
                                "target_lang_sense_clarification": "следующий день"
                            },
                            {
                                "target_lang_word": "завтрашний день",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb6443aa-65c1-3e02-9e23-c2ccac76103b",
                    "sense_definition": "Przyszłość; czasy, które dopiero nadejdą",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "future_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy musimy pracować ciężko, aby zapewnić naszym dzieciom lepsze <w>jutro</w>.",
                            "target_lang_translations": {
                                "ru": "Мы все должны усердно работать, чтобы обеспечить нашим детям лучшее <w>будущее</w>."
                            }
                        },
                        {
                            "text": "Niepewne <w>jutro</w> sprawia, że wielu ludzi boi się inwestować swoje oszczędności.",
                            "target_lang_translations": {
                                "ru": "Неопределённое <w>будущее</w> заставляет многих людей бояться инвестировать свои сбережения."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyszłość"
                    ],
                    "common_phrases": [
                        "lepsze jutro",
                        "niepewne jutro",
                        "świetlane jutro"
                    ],
                    "target_lang_definitions": {
                        "ru": "Будущее; времена, которые ещё предстоят"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "будущее",
                                "target_lang_sense_clarification": "грядущее время"
                            },
                            {
                                "target_lang_word": "завтра",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99c82979-99dc-3af3-894d-640ca59704ef",
                    "sense_definition": "Pora dnia od wschodu słońca do południa",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "time_of_day",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy nastało jasne <w>jutro</w>, rycerze byli już gotowi do dalszej drogi.",
                            "target_lang_translations": {
                                "ru": "Когда настало ясное <w>утро</w>, рыцари были уже готовы к дальнейшей дороге."
                            }
                        },
                        {
                            "text": "Chłodne <w>jutro</w> przywitało wędrowców gęstą mgłą nad rzeką.",
                            "target_lang_translations": {
                                "ru": "Холодное <w>утро</w> встретило путников густым туманом над рекой."
                            }
                        }
                    ],
                    "synonyms": [
                        "rano"
                    ],
                    "traits": [
                        {

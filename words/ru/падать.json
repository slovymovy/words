{
    "word_family": [
        "падение",
        "упасть",
        "выпасть",
        "спад",
        "попадать",
        "нападать",
        "распадаться",
        "запад",
        "водопад",
        "листопад",
        "падший"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "538c1334-6e76-35b2-aa46-fc029713b813",
                    "sense_definition": "Быстро перемещаться вниз под действием силы тяжести; терять равновесие и валиться на землю.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленький ребёнок учится ходить и часто <w>падает</w>.",
                            "target_lang_translations": {
                                "en": "A small child is learning to walk and often <w>falls</w>."
                            }
                        },
                        {
                            "text": "Осенью с деревьев медленно <w>падают</w> жёлтые листья.",
                            "target_lang_translations": {
                                "en": "In autumn, yellow leaves slowly <w>fall</w> from the trees."
                            }
                        }
                    ],
                    "synonyms": [
                        "валиться",
                        "рушиться"
                    ],
                    "antonyms": [
                        "подниматься",
                        "вставать",
                        "взлетать"
                    ],
                    "common_phrases": [
                        "падать вниз",
                        "падать на колени",
                        "падать со стула",
                        "падать в обморок"
                    ],
                    "target_lang_definitions": {
                        "en": "To move downwards quickly due to gravity; to lose balance and hit the ground."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "general movement downwards"
                            },
                            {
                                "target_lang_word": "fall down",
                                "target_lang_sense_clarification": "to lose balance and collapse"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c82e68aa-1102-300a-9db0-ebfe8f460881",
                    "sense_definition": "Опускаться на землю (об атмосферных осадках, обычно о снеге).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За окном тихо <w>падал</w> крупный пушистый снег.",
                            "target_lang_translations": {
                                "en": "Large fluffy snow was quietly <w>falling</w> outside the window."
                            }
                        },
                        {
                            "text": "Весь день с неба <w>падали</w> редкие снежинки.",
                            "target_lang_translations": {
                                "en": "Rare snowflakes were <w>falling</w> from the sky all day."
                            }
                        }
                    ],
                    "synonyms": [
                        "идти",
                        "сыпаться"
                    ],
                    "common_phrases": [
                        "снег падает",
                        "хлопья падают"
                    ],
                    "target_lang_definitions": {
                        "en": "To come down to the ground (referring to precipitation, usually snow)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "of snow or rain"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b960356-11a2-31d1-9fba-7f811a9729ff",
                    "sense_definition": "Становиться меньше по уровню, силе, значению или стоимости.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "decrease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К вечеру температура воздуха обычно <w>падает</w>.",
                            "target_lang_translations": {
                                "en": "By evening, the air temperature usually <w>drops</w>."
                            }
                        },
                        {
                            "text": "Интерес к чтению книг, к сожалению, <w>падает</w> среди молодёжи.",
                            "target_lang_translations": {
                                "en": "Interest in reading books is unfortunately <w>declining</w> among young people."
                            }
                        }
                    ],
                    "synonyms": [
                        "снижаться",
                        "уменьшаться",
                        "понижаться"
                    ],
                    "antonyms": [
                        "расти",
                        "повышаться",
                        "увеличиваться"
                    ],
                    "common_phrases": [
                        "цены падают",
                        "давление падает",
                        "зрение падает",
                        "курс валюты падает"
                    ],
                    "target_lang_definitions": {
                        "en": "To become lower in level, amount, value, or strength."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "general decrease"
                            },
                            {
                                "target_lang_word": "drop",
                                "target_lang_sense_clarification": "often used for temperature or prices"
                            },
                            {
                                "target_lang_word": "decline",
                                "target_lang_sense_clarification": "formal; to decrease in quality or quantity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b9e5e47-01ee-3eeb-bddf-8e5e939f3be0",
                    "sense_definition": "Ложиться на поверхность, освещая или затемняя её (о свете, тени).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "light_shadow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Яркий луч солнца <w>падал</w> прямо на её лицо.",
                            "target_lang_translations": {
                                "en": "A bright ray of sun <w>fell</w> right on her face."
                            }
                        },
                        {
                            "text": "Длинная тень от дерева <w>падала</w> на дорогу.",
                            "target_lang_translations": {
                                "en": "A long shadow from the tree <w>fell</w> on the road."
                            }
                        }
                    ],
                    "synonyms": [
                        "ложиться"
                    ],
                    "common_phrases": [
                        "свет падает",
                        "тень падает",
                        "луч падает"
                    ],
                    "target_lang_definitions": {
                        "en": "To land on a surface, illuminating or darkening it (referring to light or shadow)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "of light, shadows, or gaze"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fbd0759-444c-34df-aa1f-b75de9595a79",
                    "sense_definition": "Приходиться на определённую дату или время (о праздниках, событиях).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "time_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом году мой день рождения <w>падает</w> на субботу.",
                            "target_lang_translations": {
                                "en": "This year my birthday <w>falls</w> on a Saturday."
                            }
                        },
                        {
                            "text": "Праздник <w>падает</w> на середину рабочей недели.",
                            "target_lang_translations": {
                                "en": "The holiday <w>falls</w> in the middle of the work week."
                            }
                        }
                    ],
                    "synonyms": [
                        "приходиться",
                        "выпадать"
                    ],
                    "common_phrases": [
                        "падать на выходной",
                        "падать на будни"
                    ],
                    "target_lang_definitions": {
                        "en": "To happen on a particular date or time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "occur on a specific day"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "553b826c-5f98-3a86-b158-2dec685e8890",
                    "sense_definition": "Временно переставать работать, отключаться из-за сбоя (о компьютерных программах, серверах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш рабочий сервер <w>падает</w> каждый раз при большой нагрузке.",
                            "target_lang_translations": {
                                "en": "Our work server <w>crashes</w> every time there is a heavy load."
                            }
                        },
                        {
                            "text": "После обновления приложение постоянно <w>падает</w> и закрывается.",
                            "target_lang_translations": {
                                "en": "After the update, the app constantly <w>crashes</w> and closes."
                            }
                        }
                    ],
                    "synonyms": [
                        "ломаться",
                        "зависать",
                        "отключаться"
                    ],
                    "common_phrases": [
                        "сайт падает",
                        "сервер падает",
                        "база данных падает"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "computer jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To stop working suddenly due to an error (referring to software or servers)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crash",
                                "target_lang_sense_clarification": "software or application failure"
                            },
                            {
                                "target_lang_word": "go down",
                                "target_lang_sense_clarification": "server or network outage"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
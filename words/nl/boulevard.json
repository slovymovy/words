            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d4326942-68d6-3f4d-9796-2df7d6aaa986",
                    "sense_definition": "Een lange, brede straat in een stad, meestal met bomen beplant",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "straat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Parijs staat bekend om zijn vele brede <w>boulevards</w> en historische gebouwen.",
                            "target_lang_translations": {
                                "ru": "Париж известен своими многочисленными широкими <w>бульварами</w> и историческими зданиями."
                            }
                        },
                        {
                            "text": "De auto's reden langzaam over de drukke <w>boulevard</w> in het centrum.",
                            "target_lang_translations": {
                                "ru": "Машины медленно ехали по оживленному <w>бульвару</w> в центре."
                            }
                        }
                    ],
                    "synonyms": [
                        "laan",
                        "dreef"
                    ],
                    "antonyms": [
                        "steeg"
                    ],
                    "common_phrases": [
                        "brede boulevard",
                        "drukke boulevard",
                        "over de boulevard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная широкая улица в городе, обычно обсаженная деревьями"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бульвар",
                                "target_lang_sense_clarification": "широкая улица с аллеей"
                            },
                            {
                                "target_lang_word": "проспект",
                                "target_lang_sense_clarification": "широкая прямая улица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49d11902-775b-3c52-9da0-362e62b53fae",
                    "sense_definition": "Een brede wandelweg langs de kust of het strand",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "kustweg",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer wandelen veel toeristen over de zonnige <w>boulevard</w>.",
                            "target_lang_translations": {
                                "ru": "Летом многие туристы гуляют по солнечной <w>набережной</w>."
                            }
                        },
                        {
                            "text": "We aten een lekker ijsje op een bankje langs de <w>boulevard</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ели вкусное мороженое на скамейке вдоль <w>набережной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "strandpromenade",
                        "wandelpromenade"
                    ],
                    "common_phrases": [
                        "op de boulevard",
                        "langs de boulevard",
                        "boulevard van Scheveningen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Широкая прогулочная дорога вдоль побережья или пляжа"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набережная",
                                "target_lang_sense_clarification": "улица вдоль берега"
                            },
                            {
                                "target_lang_word": "приморский бульвар",
                                "target_lang_sense_clarification": "бульвар у моря"
                            }
                        ]
                    }
                }
            ]
        }
    ]

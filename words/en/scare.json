{
    "word_family": [
        "scary",
        "scarecrow",
        "scaremonger"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dd1d8512-7970-369e-b736-ac9213f07d25",
                    "sense_definition": "To cause fear, terror, or alarm in someone or something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cause_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Loud noises often <w>scare</w> the young children.",
                            "target_lang_translations": {
                                "ru": "Громкие звуки часто <w>пугают</w> маленьких детей."
                            }
                        },
                        {
                            "text": "Do not let the dog <w>scare</w> away the birds in the garden.",
                            "target_lang_translations": {
                                "ru": "Не позволяй собаке <w>распугать</w> птиц в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "frighten",
                        "terrify"
                    ],
                    "antonyms": [
                        "calm",
                        "reassure"
                    ],
                    "common_phrases": [
                        "scare away",
                        "scare off",
                        "scare to death"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать страх, ужас или тревогу у кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пугать",
                                "target_lang_sense_clarification": "вызывать чувство страха (несовершенный вид)"
                            },
                            {
                                "target_lang_word": "напугать",
                                "target_lang_sense_clarification": "вызвать страх (совершенный вид)"
                            },
                            {
                                "target_lang_word": "испугать",
                                "target_lang_sense_clarification": "внезапно вызвать испуг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f5a3714-b05f-3c81-8de1-1507a7793a18",
                    "sense_definition": "To become afraid or frightened; to have the capacity to feel fear.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "feel_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is very brave and does not <w>scare</w> easily.",
                            "target_lang_translations": {
                                "ru": "Она очень смелая, и её нелегко <w>напугать</w>."
                            }
                        },
                        {
                            "text": "I do not <w>scare</w> just because of a horror movie.",
                            "target_lang_translations": {
                                "ru": "Я не <w>пугаюсь</w> просто из-за фильма ужасов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "scare easily"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пугаться, испытывать страх; быть способным чувствовать страх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пугаться",
                                "target_lang_sense_clarification": "испытывать страх"
                            },
                            {
                                "target_lang_word": "бояться",
                                "target_lang_sense_clarification": "чувствовать страх перед чем-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "78d3ae6f-6c24-336c-a7ae-825314198afc",
                    "sense_definition": "A sudden feeling of fear, shock, or alarm, usually not lasting long.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "fright_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You gave me a big <w>scare</w> when you jumped out from behind the door.",
                            "target_lang_translations": {
                                "ru": "Ты меня сильно <w>напугал</w>, когда выпрыгнул из-за двери."
                            }
                        },
                        {
                            "text": "The medical results were just a <w>scare</w> and everything is fine now.",
                            "target_lang_translations": {
                                "ru": "Результаты анализов оказались лишь ложной <w>тревогой</w>, и теперь всё в порядке."
                            }
                        }
                    ],
                    "synonyms": [
                        "fright"
                    ],
                    "common_phrases": [
                        "give a scare",
                        "get a scare",
                        "bit of a scare"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное чувство страха, испуг или тревога, обычно кратковременные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испуг",
                                "target_lang_sense_clarification": "внезапное чувство страха"
                            },
                            {
                                "target_lang_word": "страх",
                                "target_lang_sense_clarification": "чувство боязни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04152328-d71f-34a5-b835-ef01187eeca4",
                    "sense_definition": "A period of general alarm or panic caused by a specific perceived danger.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "public_alarm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The airport went through a security <w>scare</w> yesterday afternoon.",
                            "target_lang_translations": {
                                "ru": "Вчера днем в аэропорту был <w>переполох</w> из-за угрозы безопасности."
                            }
                        },
                        {
                            "text": "There was a major health <w>scare</w> regarding the contaminated water.",
                            "target_lang_translations": {
                                "ru": "Возникла серьезная <w>паника</w> по поводу здоровья из-за загрязненной воды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bomb scare",
                        "health scare",
                        "security scare"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период общей тревоги или паники, вызванный предполагаемой опасностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паника",
                                "target_lang_sense_clarification": "всеобщий страх и смятение"
                            },
                            {
                                "target_lang_word": "переполох",
                                "target_lang_sense_clarification": "суматоха, вызванная тревогой"
                            },
                            {
                                "target_lang_word": "тревога",
                                "target_lang_sense_clarification": "беспокойство из-за опасности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51cda877-b0fd-34df-a82a-e20f5fb2cafe",
                    "sense_definition": "An object or device used to frighten birds or animals away.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer set up a <w>scare</w> in the middle of the cornfield.",
                            "target_lang_translations": {
                                "ru": "Фермер поставил <w>пугало</w> посреди кукурузного поля."
                            }
                        },
                        {
                            "text": "Birds eventually ignored the old <w>scare</w> and ate the seeds.",
                            "target_lang_translations": {
                                "ru": "Птицы в конце концов перестали обращать внимание на старое <w>пугало</w> и склевали семена."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет или устройство, используемое для отпугивания птиц или животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пугало",
                                "target_lang_sense_clarification": "чучело для отпугивания птиц"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ded6356b-f334-3d1a-bad1-8031fa5da9f2",
                    "sense_definition": "Lean or scanty in amount; scarce.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "scarce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Food was <w>scare</w> during the harsh winter months.",
                            "target_lang_translations": {
                                "ru": "Зимой еда была <w>скудной</w>."
                            }
                        },
                        {
                            "text": "Resources have become <w>scare</w> in this region.",
                            "target_lang_translations": {
                                "ru": "Ресурсы в этом регионе стали <w>дефицитными</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "variant of scarce"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скудный или недостаточный по количеству (диалектный вариант слова scarce)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скудный",
                                "target_lang_sense_clarification": "недостаточный"
                            },
                            {
                                "target_lang_word": "дефицитный",
                                "target_lang_sense_clarification": "редкий, недостающий"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
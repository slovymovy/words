            "senses": [
                {
                    "sense_id": "921ab934-2e6f-3144-a3ea-741222f164f3",
                    "sense_definition": "Совокупность руководителей учреждения, организации или страны; начальство.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

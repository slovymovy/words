{
    "word_family": [
        "versterken",
        "sterk",
        "sterkte"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "703e59c6-e547-3dbf-aef5-e45db4e66d65",
                    "sense_definition": "Het proces van iets sterker, steviger of krachtiger maken",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>versterking</w> van de oude dijken is noodzakelijk voor de veiligheid.",
                            "target_lang_translations": {
                                "ru": "<w>Укрепление</w> старых дамб необходимо для безопасности."
                            }
                        },
                        {
                            "text": "Het bedrijf werkt hard aan de <w>versterking</w> van zijn positie op de markt.",
                            "target_lang_translations": {
                                "ru": "Компания усердно работает над <w>укреплением</w> своей позиции на рынке."
                            }
                        }
                    ],
                    "antonyms": [
                        "verzwakking"
                    ],
                    "common_phrases": [
                        "versterking van de dijken",
                        "versterking van de positie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс придания чему-либо большей силы, прочности или устойчивости"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрепление",
                                "target_lang_sense_clarification": "сделать прочнее, надежнее (напр. дамбы, здоровье)"
                            },
                            {
                                "target_lang_word": "усиление",
                                "target_lang_sense_clarification": "сделать мощнее, интенсивнее (напр. контроль, ветер)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67aaa110-5355-311a-8c8b-56ac3da8a6ad",
                    "sense_definition": "Extra manschappen, hulp of middelen die worden gestuurd om steun te bieden in een moeilijke situatie",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie vroeg direct om <w>versterking</w> toen de situatie uit de hand liep.",
                            "target_lang_translations": {
                                "ru": "Полиция немедленно запросила <w>подкрепление</w>, когда ситуация вышла из-под контроля."
                            }
                        },
                        {
                            "text": "Gelukkig is de <w>versterking</w> onderweg om ons te helpen.",
                            "target_lang_translations": {
                                "ru": "К счастью, <w>подкрепление</w> уже в пути, чтобы помочь нам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "om versterking vragen",
                        "wachten op versterking",
                        "versterking sturen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дополнительные силы, люди или ресурсы, отправляемые для поддержки в сложной ситуации"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подкрепление",
                                "target_lang_sense_clarification": "дополнительные силы, помощь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "002dbeb3-af9e-3bd6-8d89-1b3f86fca269",
                    "sense_definition": "Het vergroten van de kracht of amplitude van een elektrisch of geluidssignaal",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>versterking</w> van het geluid was niet voldoende voor de grote zaal.",
                            "target_lang_translations": {
                                "ru": "<w>Усиление</w> звука было недостаточным для большого зала."
                            }
                        },
                        {
                            "text": "Zonder elektronische <w>versterking</w> is de zanger achterin niet te horen.",
                            "target_lang_translations": {
                                "ru": "Без электронного <w>усиления</w> певца не слышно в задних рядах."
                            }
                        }
                    ],
                    "synonyms": [
                        "amplificatie"
                    ],
                    "antonyms": [
                        "demping",
                        "verzwakking"
                    ],
                    "common_phrases": [
                        "versterking van het signaal",
                        "elektronische versterking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличение силы или амплитуды электрического или звукового сигнала"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усиление",
                                "target_lang_sense_clarification": "звука или сигнала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3deaf7c-6814-35a7-aedc-0e92ab84ac04",
                    "sense_definition": "Een gebouw, wal of constructie die dient ter verdediging tegen vijandelijke aanvallen",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten trokken zich terug in de veilige <w>versterking</w>.",
                            "target_lang_translations": {
                                "ru": "Солдаты отступили в безопасное <w>укрепление</w>."
                            }
                        },
                        {
                            "text": "Rondom de oude stad ligt nog steeds een middeleeuwse <w>versterking</w>.",
                            "target_lang_translations": {
                                "ru": "Вокруг старого города до сих пор находится средневековое <w>укрепление</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een middeleeuwse versterking",
                        "de versterking bouwen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сооружение или конструкция, служащая для защиты от вражеских нападений"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрепление",
                                "target_lang_sense_clarification": "оборонительное сооружение"
                            },
                            {
                                "target_lang_word": "фортификация",
                                "target_lang_sense_clarification": "военный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
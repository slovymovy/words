{
    "word_family": [
        "ondergaan",
        "zonsondergang"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bc052074-3c0c-3dca-a355-0f48ca8f2b96",
                    "sense_definition": "Het verdwijnen van een hemellichaam, zoals de zon of de maan, onder de horizon.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We keken stil naar de prachtige <w>ondergang</w> van de zon in de zee.",
                            "target_lang_translations": {
                                "en": "We watched silently the beautiful <w>setting</w> of the sun into the sea.",
                                "ru": "Мы молча смотрели на прекрасный <w>закат</w> солнца в море."
                            }
                        },
                        {
                            "text": "Na de <w>ondergang</w> van de maan werd het bos volledig donker.",
                            "target_lang_translations": {
                                "en": "After the <w>setting</w> of the moon, the forest became completely dark.",
                                "ru": "После <w>захода</w> луны лес погрузился в полную темноту."
                            }
                        }
                    ],
                    "antonyms": [
                        "opkomst"
                    ],
                    "common_phrases": [
                        "ondergang van de zon",
                        "ondergang van de maan"
                    ],
                    "target_lang_definitions": {
                        "en": "The disappearance of a celestial body, such as the sun or the moon, below the horizon.",
                        "ru": "Исчезновение небесного тела, такого как солнце или луна, за горизонтом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "setting",
                                "target_lang_sense_clarification": "general term for the descent of a celestial body"
                            },
                            {
                                "target_lang_word": "sunset",
                                "target_lang_sense_clarification": "specifically of the sun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "закат",
                                "target_lang_sense_clarification": "о солнце, вечерняя заря"
                            },
                            {
                                "target_lang_word": "заход",
                                "target_lang_sense_clarification": "астрономическое явление, исчезновение за горизонтом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90c5a749-f1dd-3928-942d-c532af16812c",
                    "sense_definition": "Het proces van vernietiging, verval of het definitieve einde van iets of iemand.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De corruptie binnen de regering leidde tot de <w>ondergang</w> van het land.",
                            "target_lang_translations": {
                                "en": "The corruption within the government led to the <w>downfall</w> of the country.",
                                "ru": "Коррупция в правительстве привела к <w>гибели</w> страны."
                            }
                        },
                        {
                            "text": "Zijn gokverslaving werd uiteindelijk zijn persoonlijke <w>ondergang</w>.",
                            "target_lang_translations": {
                                "en": "His gambling addiction eventually became his personal <w>undoing</w>.",
                                "ru": "Его пристрастие к азартным играм в конечном итоге стало его личным <w>крахом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "val",
                        "ineenstorting",
                        "vernietiging"
                    ],
                    "antonyms": [
                        "opkomst"
                    ],
                    "common_phrases": [
                        "zijn eigen ondergang",
                        "de ondergang van"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of destruction, decay, or the definitive end of something or someone.",
                        "ru": "Процесс разрушения, упадка или окончательный конец чего-либо или кого-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "downfall",
                                "target_lang_sense_clarification": "loss of power, prosperity, or status"
                            },
                            {
                                "target_lang_word": "ruin",
                                "target_lang_sense_clarification": "state of complete destruction or collapse"
                            },
                            {
                                "target_lang_word": "undoing",
                                "target_lang_sense_clarification": "cause of someone's failure or ruin"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гибель",
                                "target_lang_sense_clarification": "полное уничтожение, смерть"
                            },
                            {
                                "target_lang_word": "крах",
                                "target_lang_sense_clarification": "разорение, полный провал"
                            },
                            {
                                "target_lang_word": "закат",
                                "target_lang_sense_clarification": "перен. конец эпохи, угасание"
                            },
                            {
                                "target_lang_word": "падение",
                                "target_lang_sense_clarification": "утрата власти или величия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
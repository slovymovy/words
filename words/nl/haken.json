                },
                {
                    "sense_id": "73a29f2c-e4e9-3a89-baaa-c74642b0e1f5",
                    "sense_definition": "Zeer sterk naar iets verlangen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "desire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na jaren van oorlog <haakte> de bevolking naar vrede en rust.",

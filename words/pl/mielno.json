            "pos": "name",
            "senses": [
                {
                    "sense_id": "0b8d833a-5899-34b7-96f0-b63b380cfdda",
                    "sense_definition": "miasto w północno-zachodniej Polsce, w województwie zachodniopomorskim, popularna miejscowość wypoczynkowa nad Morzem Bałtyckim",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "settlements",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "W te wakacje jedziemy do <w>Mielna</w>, żeby odpocząć nad morzem.",
                            "target_lang_translations": {
                                "ru": "В эти каникулы мы едем в <w>Мельно</w>, чтобы отдохнуть на море."
                            }
                        },
                        {
                            "text": "Nadmorskie <w>Mielno</w> słynie z szerokich plaż i licznych imprez w sezonie letnim.",
                            "target_lang_translations": {
                                "ru": "Приморское <w>Мельно</w> славится широкими пляжами и многочисленными вечеринками в летний сезон."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wakacje w Mielnie",
                        "plaża w Mielnie",
                        "gmina Mielno"
                    ],
                    "target_lang_definitions": {
                        "ru": "город на северо-западе Польши, в Западно-Поморском воеводстве, популярный курорт на Балтийском море"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мельно",
                                "target_lang_sense_clarification": "город/курорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e418236-477f-3ca0-a20a-db7a19a0cab6",
                    "sense_definition": "nazwa wielu mniejszych miejscowości (wsi) w różnych regionach Polski",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "settlements",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mój dziadek pochodzi z małej wsi <w>Mielno</w> w województwie wielkopolskim.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка родом из маленькой деревни <w>Мельно</w> в Великопольском воеводстве."
                            }
                        },
                        {
                            "text": "Na mapie Polski można znaleźć niejedno <w>Mielno</w>, nie tylko to nad morzem.",
                            "target_lang_translations": {
                                "ru": "На карте Польши можно найти не одно <w>Мельно</w>, не только то, что у моря."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "название многих небольших населенных пунктов (деревень) в различных регионах Польши"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мельно",
                                "target_lang_sense_clarification": "деревня/село"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d71a1881-b6c4-385f-9179-65e07d0f27a3",
                    "sense_definition": "nazwa kilkunastu jezior w Polsce",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hydronyms",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Wędkarze często odwiedzają jezioro <w>Mielno</w> w poszukiwaniu szczupaków.",
                            "target_lang_translations": {
                                "ru": "Рыбаки часто посещают озеро <w>Мельно</w> в поисках щук."
                            }
                        },
                        {
                            "text": "Nad jeziorem <w>Mielno</w> panuje cisza i spokój, idealna do relaksu.",
                            "target_lang_translations": {
                                "ru": "На озере <w>Мельно</w> царит тишина и покой, идеально подходящие для отдыха."
                            }
                        }
                    ],
                    "common_phrases": [

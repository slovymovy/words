    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2afff6bd-7165-37d5-9a79-af5366f03fe4",
                    "sense_definition": "Podstawowy mebel w sypialni służący do spania i odpoczynku, zazwyczaj składający się z materaca na stelażu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "furniture_bed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem bardzo zmęczony i chcę już iść do <w>łóżka</w>.",
                            "target_lang_translations": {
                                "ru": "Я очень устал и хочу уже пойти в <w>кровать</w>."
                            }
                        },
                        {
                            "text": "Mój dziadek pali papierosy w <w>łóżku</w> przed snem.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка курит сигареты в <w>постели</w> перед сном."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyro",
                        "łoże",
                        "kojo"
                    ],
                    "common_phrases": [
                        "iść do łóżka",
                        "ścielić łóżko",
                        "wstawać z łóżka",
                        "leżeć w łóżku",
                        "łóżko polowe",
                        "łóżko piętrowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основной предмет мебели в спальне, предназначенный для сна и отдыха, обычно состоящий из матраса на каркасе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кровать",
                                "target_lang_sense_clarification": "мебель"
                            },
                            {
                                "target_lang_word": "постель",
                                "target_lang_sense_clarification": "место для сна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4fdc9c2-23da-3780-bc2e-6e2cabac244c",
                    "sense_definition": "Miejsce przeznaczone dla jednego pacjenta w szpitalu lub innej placówce opiekuńczej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "furniture_bed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na onkologii zwolniło się <w>łóżko</w>, więc Staszek może iść na leczenie.",
                            "target_lang_translations": {
                                "ru": "В онкологии освободилась <w>койка</w>, так что Сташек может ложиться на лечение."
                            }
                        },
                        {
                            "text": "Szpital ma tylko jedno wolne <w>łóżko</w> na tym oddziale.",
                            "target_lang_translations": {
                                "ru": "В больнице есть только одна свободная <w>койка</w> в этом отделении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wolne łóżko",
                        "łóżko szpitalne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, предназначенное для одного пациента в больнице или другом медицинском учреждении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "койка",
                                "target_lang_sense_clarification": "в больнице"
                            },
                            {
                                "target_lang_word": "место",
                                "target_lang_sense_clarification": "для пациента"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aaaf8c28-9db6-3399-9bc3-4ceb46db85ea",
                    "sense_definition": "Potoczne określenie na aktywność seksualną lub pożycie intymne dwojga ludzi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jemu chodziło tylko o <w>łóżko</w>, a nie o poważny związek.",
                            "target_lang_translations": {
                                "ru": "Его интересовала только <w>постель</w>, а не серьезные отношения."
                            }
                        },
                        {
                            "text": "Problemy w <w>łóżku</w> często prowadzą do kryzysu w małżeństwie.",
                            "target_lang_translations": {
                                "ru": "Проблемы в <w>постели</w> часто приводят к кризису в браке."
                            }
                        }
                    ],
                    "synonyms": [
                        "seks",
                        "pożycie",
                        "stosunek"
                    ],
                    "common_phrases": [
                        "być dobrym w łóżku",
                        "zaciągnąć do łóżka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w kontekście relacji intymnych"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное обозначение сексуальной активности или интимной жизни двух людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постель",
                                "target_lang_sense_clarification": "секс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b973399-8021-3b88-aec1-99df81818669",
                    "sense_definition": "Specjalistyczny mebel lub urządzenie, na którym klient kładzie się w celu wykonania zabiegu kosmetycznego lub medycznego.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "furniture_bed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W salonie kosmetycznym stało wygodne <w>łóżko</w> do masażu.",
                            "target_lang_translations": {
                                "ru": "В косметическом салоне стояла удобная <w>кушетка</w> для массажа."
                            }
                        },
                        {
                            "text": "To nowoczesne <w>łóżko</w> opalające jest bardzo bezpieczne dla skóry.",
                            "target_lang_translations": {
                                "ru": "Этот современный <w>солярий</w> очень безопасен для кожи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "łóżko do masażu",
                        "łóżko opalające",
                        "łóżko rehabilitacyjne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специализированная мебель или устройство, на которое клиент ложится для проведения косметической или медицинской процедуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кушетка",
                                "target_lang_sense_clarification": "косметическая"
                            },
                            {
                                "target_lang_word": "стол",
                                "target_lang_sense_clarification": "массажный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
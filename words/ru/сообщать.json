    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "27b71bf5-29d2-305c-a27d-379662ec385d",
                    "sense_definition": "Передавать кому-либо информацию, извещать о чём-либо или делиться сведениями.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "info_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Газеты каждое утро <w>сообщают</w> последние новости о событиях в мире.",
                            "target_lang_translations": {
                                "en": "Newspapers <w>report</w> the latest news about world events every morning."
                            }
                        },
                        {
                            "text": "Ты должен немедленно <w>сообщать</w> нам о любых изменениях в расписании.",
                            "target_lang_translations": {
                                "en": "You must immediately <w>inform</w> us of any changes in the schedule."
                            }
                        }
                    ],
                    "synonyms": [
                        "информировать",
                        "извещать",
                        "уведомлять",
                        "докладывать",
                        "оповещать",
                        "передавать"
                    ],
                    "antonyms": [
                        "скрывать",
                        "умалчивать",
                        "утаивать"
                    ],
                    "common_phrases": [
                        "сообщать новости",
                        "сообщать подробности",
                        "сообщать о решении",
                        "сообщать сведения"
                    ],
                    "target_lang_definitions": {
                        "en": "To convey information to someone, to notify about something, or to share details."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inform",
                                "target_lang_sense_clarification": "to give information or facts to someone"
                            },
                            {
                                "target_lang_word": "report",
                                "target_lang_sense_clarification": "to give a spoken or written account of something (often news or official)"
                            },
                            {
                                "target_lang_word": "communicate",
                                "target_lang_sense_clarification": "to share or exchange information or ideas"
                            },
                            {
                                "target_lang_word": "tell",
                                "target_lang_sense_clarification": "to communicate information to someone verbally"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7af5dbdf-8445-3848-9cfd-e72e91a2c8d0",
                    "sense_definition": "Придавать чему-либо определённое свойство, качество или состояние (обычно в техническом или научном контексте).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "impart_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мощный двигатель <w>сообщает</w> автомобилю высокую скорость за несколько секунд.",
                            "target_lang_translations": {
                                "en": "A powerful engine <w>imparts</w> high speed to the car in a few seconds."
                            }
                        },
                        {
                            "text": "Специальная добавка <w>сообщает</w> сплаву дополнительную прочность и устойчивость.",
                            "target_lang_translations": {
                                "en": "A special additive <w>imparts</w> additional strength and stability to the alloy."
                            }
                        }
                    ],
                    "synonyms": [
                        "придавать"
                    ],
                    "common_phrases": [
                        "сообщать движение",
                        "сообщать скорость",
                        "сообщать ускорение",
                        "сообщать вид"
                    ],
                    "target_lang_definitions": {
                        "en": "To give a particular quality, property, or state to something (usually in technical or scientific contexts)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "impart",
                                "target_lang_sense_clarification": "to bestow a quality or characteristic (technical/formal)"
                            },
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "general term for providing a quality or state"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "932dba31-e52d-3267-af03-7f70987a732b",
                    "sense_definition": "A person whose job is to cut down trees.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "occupation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>lumberjack</w> used a large chainsaw to fell the pine tree.",
                            "target_lang_translations": {
                                "ru": "<w>Лесоруб</w> использовал большую бензопилу, чтобы свалить сосну."
                            }
                        },
                        {
                            "text": "He decided to work as a <w>lumberjack</w> in the Canadian forest.",
                            "target_lang_translations": {
                                "ru": "Он решил работать <w>лесорубом</w> в канадском лесу."
                            }
                        }
                    ],
                    "synonyms": [

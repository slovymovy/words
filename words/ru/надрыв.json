    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "144f0d57-7e01-3256-8000-3cbc69eb6a96",
                    "sense_definition": "Чрезмерное, болезненное напряжение сил или чувств; проявление сильного страдания, неестественности или истеричности в голосе и поведении.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotional_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Актёр читал монолог с таким <w>надрывом</w>, что зрители в зале замерли.",
                            "target_lang_translations": {
                                "en": "The actor read the monologue with such <w>anguish</w> that the audience froze."
                            }
                        },
                        {
                            "text": "В её голосе слышался болезненный <w>надрыв</w>, выдававший глубокое отчаяние.",
                            "target_lang_translations": {
                                "en": "A painful <w>strain</w> was audible in her voice, betraying deep despair."
                            }
                        }
                    ],
                    "synonyms": [
                        "надлом",
                        "напряжение",
                        "истеричность"
                    ],
                    "antonyms": [
                        "спокойствие",
                        "равновесие",
                        "умиротворение"
                    ],
                    "common_phrases": [
                        "с надрывом",
                        "душевный надрыв",
                        "работать на надрыве",
                        "кричать с надрывом"
                    ],
                    "target_lang_definitions": {
                        "en": "Excessive, painful strain of strength or feelings; a manifestation of intense suffering, unnaturalness, or hysteria in voice and behavior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "anguish",
                                "target_lang_sense_clarification": "intense emotional suffering"
                            },
                            {
                                "target_lang_word": "strain",
                                "target_lang_sense_clarification": "tension in voice or manner"
                            },
                            {
                                "target_lang_word": "hysteria",
                                "target_lang_sense_clarification": "excessive emotional display"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d53fc934-1c73-33f0-af99-0e8b6549d371",
                    "sense_definition": "Небольшое повреждение, при котором целостность материала или ткани нарушена не полностью; надорванное место.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач осмотрел ногу и диагностировал <w>надрыв</w> связок.",
                            "target_lang_translations": {
                                "en": "The doctor examined the leg and diagnosed a ligament <w>tear</w>."
                            }
                        },
                        {
                            "text": "На старой фотографии был виден небольшой <w>надрыв</w> в левом углу.",
                            "target_lang_translations": {
                                "en": "A small <w>tear</w> was visible in the left corner of the old photograph."
                            }
                        }
                    ],
                    "synonyms": [
                        "разрыв",
                        "трещина",
                        "повреждение"
                    ],
                    "antonyms": [
                        "целостность"
                    ],
                    "common_phrases": [
                        "надрыв связок",
                        "надрыв мышцы",
                        "надрыв ткани"
                    ],
                    "target_lang_definitions": {
                        "en": "Minor damage where the integrity of material or tissue is not completely breached; a torn place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tear",
                                "target_lang_sense_clarification": "physical damage to material or tissue"
                            },
                            {
                                "target_lang_word": "rupture",
                                "target_lang_sense_clarification": "medical term for tissue damage"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "military jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный род войск или подразделение вооруженных сил, например, пехота или кавалерия."

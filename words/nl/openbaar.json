        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bfda53c6-79b8-347f-827e-ce131041781a",
                    "sense_definition": "Vervoeging van <w>openbaren</w>: eerste persoon enkelvoud tegenwoordige tijd, gebiedende wijs of bij inversie tweede persoon enkelvoud.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>openbaar</w> de resultaten van het onderzoek pas volgende week.",
                            "target_lang_translations": {
                                "en": "I will only <w>reveal</w> the results of the investigation next week.",
                                "ru": "Я <w>обнародую</w> результаты расследования только на следующей неделе."
                            }
                        },
                        {
                            "text": "<w>Openbaar</w> je gedachten en wees eerlijk tegen ons.",
                            "target_lang_translations": {
                                "en": "<w>Reveal</w> your thoughts and be honest with us.",
                                "ru": "<w>Раскрой</w> свои мысли и будь с нами честен."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "of openbaren"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Verb form of 'openbaren': to make known or reveal.",
                        "ru": "Спряжение глагола openbaren: первое лицо единственного числа, повелительное наклонение."
                    },
                    "translations": {
                        "en": [
                            {

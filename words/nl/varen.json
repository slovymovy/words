    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dbb4b51e-68ad-31a3-90d1-171cf0ef96fd",
                    "sense_definition": "Zich met een schip of boot over het water verplaatsen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <varen> morgen met de veerboot naar het eiland.",
                            "target_lang_translations": {
                                "ru": "Мы <плывем> завтра на пароме на остров."
                            }
                        },
                        {
                            "text": "De kapitein <voer> het grote schip veilig de haven binnen.",
                            "target_lang_translations": {
                                "ru": "Капитан благополучно <завел> большой корабль в гавань."
                            }
                        },
                        {
                            "text": "In de zomer <varen> er veel plezierjachten op de meren.",
                            "target_lang_translations": {
                                "ru": "Летом по озерам <ходит> много прогулочных яхт."
                            }
                        }
                    ],
                    "synonyms": [
                        "navigeren",
                        "zeilen"
                    ],
                    "common_phrases": [
                        "laten varen",
                        "koers varen",
                        "uit varen gaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться по воде на корабле или лодке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плыть",
                                "target_lang_sense_clarification": "общее значение перемещения по воде"
                            },
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "морской термин для судов"
                            },
                            {
                                "target_lang_word": "плавать",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d00df631-e297-3475-93be-fb7168b8317c",
                    "sense_definition": "Het maken of vergaan; in een bepaalde toestand verkeren (vooral in vaste uitdrukkingen).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "state_of_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <voer> wel bij de beslissing om te investeren in vastgoed.",
                            "target_lang_translations": {
                                "ru": "Он <выиграл> от решения инвестировать в недвижимость."
                            }
                        },
                        {
                            "text": "De organisatie <vaart> een nieuwe koers om meer leden te trekken.",
                            "target_lang_translations": {
                                "ru": "Организация <берет> новый курс, чтобы привлечь больше членов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wel varen bij"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в определенном состоянии или действовать (в устойчивых выражениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преуспевать",
                                "target_lang_sense_clarification": "в выражении 'wel varen' (процветать)"
                            },
                            {
                                "target_lang_word": "следовать",
                                "target_lang_sense_clarification": "в выражении 'koers varen' (следовать курсу)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b292f12-6693-34a9-bfbc-65ce9ca82795",
                    "sense_definition": "Rijden met een voertuig over land, zoals een auto of bus.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "movement_land",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <vaart> elke dag met de auto naar zijn werk in de stad.",
                            "target_lang_translations": {
                                "ru": "Он каждый день <ездит> на машине на работу в город."
                            }
                        },
                        {
                            "text": "Mijn oom <vaart> graag in zijn nieuwe wagen.",
                            "target_lang_translations": {
                                "ru": "Мой дядя любит <ездить> на своей новой машине."
                            }
                        }
                    ],
                    "synonyms": [
                        "rijden",
                        "besturen"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Suriname"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ехать на наземном транспортном средстве (суринамский диалект)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ехать",
                                "target_lang_sense_clarification": "передвигаться на транспорте"
                            },
                            {
                                "target_lang_word": "водить",
                                "target_lang_sense_clarification": "управлять автомобилем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9588bda-43a5-37f4-8a36-641c4983f0ba",
                    "sense_definition": "Zich plechtig verplaatsen of opstijgen (vaak in religieuze of literaire context).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "movement_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Volgens de oude legende <voer> de held ten hemel.",
                            "target_lang_translations": {
                                "ru": "Согласно древней легенде, герой <вознесся> на небеса."
                            }
                        },
                        {
                            "text": "De geest <voer> uit het lichaam weg.",
                            "target_lang_translations": {
                                "ru": "Дух <вышел> из тела."
                            }
                        }
                    ],
                    "synonyms": [
                        "opstijgen",
                        "gaan"
                    ],
                    "common_phrases": [
                        "ten hemel varen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественно перемещаться или возноситься (архаичное или религиозное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возноситься",
                                "target_lang_sense_clarification": "подниматься ввысь (о душе, святых)"
                            },
                            {
                                "target_lang_word": "исходить",
                                "target_lang_sense_clarification": "покидать тело (о духе)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bcc04d43-64a6-3074-9dbd-fb83de353bcd",
                    "sense_definition": "Een plant met grote, vaak ingesneden bladeren die zich voortplant door middel van sporen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de schaduw van de grote bomen groeien prachtige <varens>.",
                            "target_lang_translations": {
                                "ru": "В тени больших деревьев растут красивые <папоротники>."
                            }
                        },
                        {
                            "text": "Zij kocht een groene <varen> om de woonkamer op te fleuren.",
                            "target_lang_translations": {
                                "ru": "Она купила зеленый <папоротник>, чтобы украсить гостиную."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Растение с крупными рассеченными листьями, размножающееся спорами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "папоротник",
                                "target_lang_sense_clarification": "ботаническое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]

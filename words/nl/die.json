{
    "word_family": [
        "diegene",
        "diezelfde",
        "diens"
    ],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "e063fcc7-3eed-366f-ab06-19ec2c852abd",
                    "sense_definition": "Aanwijzend woord dat verwijst naar een specifiek persoon of voorwerp (de-woord) dat zich verder weg bevindt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "demonstrative_det",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kijk eens naar <w>die</w> grote boom in de tuin.",
                            "target_lang_translations": {
                                "en": "Take a look at <w>that</w> big tree in the garden.",
                                "ru": "Посмотри-ка на <w>то</w> большое дерево в саду."
                            }
                        },
                        {
                            "text": "Ik vind <w>die</w> jas veel mooier dan deze.",
                            "target_lang_translations": {
                                "en": "I think <w>that</w> coat is much nicer than this one.",
                                "ru": "Мне <w>та</w> куртка нравится гораздо больше, чем эта."
                            }
                        }
                    ],
                    "antonyms": [
                        "deze"
                    ],
                    "common_phrases": [
                        "op die manier",
                        "in die tijd",
                        "die kant op"
                    ],
                    "target_lang_definitions": {
                        "en": "Demonstrative word referring to a specific person or object (de-word) located further away.",
                        "ru": "Указательное местоимение, которое относится к конкретному лицу или предмету (мужского или женского рода), находящемуся на расстоянии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "demonstrative determiner"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тот",
                                "target_lang_sense_clarification": "мужской род"
                            },
                            {
                                "target_lang_word": "та",
                                "target_lang_sense_clarification": "женский род"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06045f82-9170-3739-92d6-1a9d4f27f6cf",
                    "sense_definition": "Aanwijzend woord dat verwijst naar meerdere personen of voorwerpen die zich verder weg bevinden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "demonstrative_det",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>die</w> schoenen van jou of van mij?",
                            "target_lang_translations": {
                                "en": "Are <w>those</w> shoes yours or mine?",
                                "ru": "<w>Те</w> туфли твои или мои?"
                            }
                        },
                        {
                            "text": "Ik heb <w>die</w> mensen nog nooit eerder gezien.",
                            "target_lang_translations": {
                                "en": "I have never seen <w>those</w> people before.",
                                "ru": "Я никогда раньше не видел <w>тех</w> людей."
                            }
                        }
                    ],
                    "antonyms": [
                        "deze"
                    ],
                    "common_phrases": [
                        "al die tijd",
                        "al die mensen"
                    ],
                    "target_lang_definitions": {
                        "en": "Demonstrative word referring to multiple persons or objects located further away.",
                        "ru": "Указательное местоимение, которое относится к нескольким лицам или предметам, находящимся на расстоянии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "those",
                                "target_lang_sense_clarification": "demonstrative determiner"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "те",
                                "target_lang_sense_clarification": "множественное число"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "72bbe436-c95c-3db4-949e-42ca412f91a7",
                    "sense_definition": "Verwijst zelfstandig naar een persoon of zaak die zich niet in de directe nabijheid bevindt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "demonstrative_pron",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze fiets is kapot, maar <w>die</w> werkt nog goed.",
                            "target_lang_translations": {
                                "en": "This bike is broken, but <w>that one</w> still works well.",
                                "ru": "Этот велосипед сломан, но <w>тот</w> еще работает хорошо."
                            }
                        },
                        {
                            "text": "Ik wil niet deze taart, ik wil <w>die</w> daar.",
                            "target_lang_translations": {
                                "en": "I don't want this cake, I want <w>that one</w> over there.",
                                "ru": "Я не хочу этот торт, я хочу <w>вон тот</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "deze"
                    ],
                    "common_phrases": [
                        "die en die",
                        "dat is die"
                    ],
                    "target_lang_definitions": {
                        "en": "Refers independently to a person or thing not in the immediate vicinity.",
                        "ru": "Самостоятельно употребляемое местоимение, указывающее на лицо или предмет, находящийся не в непосредственной близости."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that one",
                                "target_lang_sense_clarification": "demonstrative pronoun"
                            },
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "demonstrative pronoun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тот",
                                "target_lang_sense_clarification": "указание на удаленный объект (м.р.)"
                            },
                            {
                                "target_lang_word": "та",
                                "target_lang_sense_clarification": "указание на удаленный объект (ж.р.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a73dce6-9089-3bf8-8a16-7d570c79e71f",
                    "sense_definition": "Verwijst zelfstandig naar meerdere personen of zaken die zich niet in de directe nabijheid bevinden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "demonstrative_pron",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Welke appels zijn lekker? <w>Die</w> uit Spanje zijn goed.",
                            "target_lang_translations": {
                                "en": "Which apples are tasty? <w>Those</w> from Spain are good.",
                                "ru": "Какие яблоки вкусные? <w>Те</w>, что из Испании, хорошие."
                            }
                        },
                        {
                            "text": "<w>Die</w> zijn veel duurder dan ik had verwacht.",
                            "target_lang_translations": {
                                "en": "<w>Those</w> are much more expensive than I expected.",
                                "ru": "<w>Те</w> намного дороже, чем я ожидал."
                            }
                        }
                    ],
                    "antonyms": [
                        "deze"
                    ],
                    "target_lang_definitions": {
                        "en": "Refers independently to multiple persons or things not in the immediate vicinity.",
                        "ru": "Самостоятельно употребляемое местоимение, указывающее на несколько лиц или предметов, находящихся не в непосредственной близости."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "those",
                                "target_lang_sense_clarification": "demonstrative pronoun"
                            },
                            {
                                "target_lang_word": "they",
                                "target_lang_sense_clarification": "personal pronoun used demonstratively"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "те",
                                "target_lang_sense_clarification": "указание на удаленные объекты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c97cc57-ef2b-387a-bc21-ccaba1fb1d98",
                    "sense_definition": "Betrekkelijk voornaamwoord dat terugverwijst naar een eerder genoemd de-woord of meervoud.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "relative_pron",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De man <w>die</w> daar loopt, is mijn buurman.",
                            "target_lang_translations": {
                                "en": "The man <w>who</w> is walking there is my neighbor.",
                                "ru": "Мужчина, <w>который</w> там идет, — мой сосед."
                            }
                        },
                        {
                            "text": "Ik ken iemand <w>die</w> je hiermee kan helpen.",
                            "target_lang_translations": {
                                "en": "I know someone <w>who</w> can help you with this.",
                                "ru": "Я знаю кое-кого, <w>кто</w> может тебе с этим помочь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "degene die",
                        "iemand die"
                    ],
                    "target_lang_definitions": {
                        "en": "Relative pronoun referring back to a previously mentioned de-word or plural noun.",
                        "ru": "Относительное местоимение, которое ссылается на ранее упомянутое существительное (de-слово или множественное число)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "who",
                                "target_lang_sense_clarification": "relative pronoun for people"
                            },
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "relative pronoun for people or things"
                            },
                            {
                                "target_lang_word": "which",
                                "target_lang_sense_clarification": "relative pronoun for things"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "который",
                                "target_lang_sense_clarification": "союзное слово"
                            },
                            {
                                "target_lang_word": "кто",
                                "target_lang_sense_clarification": "относится к одушевленным лицам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63748294-947d-3ed9-9776-b3ceeadd5e30",
                    "sense_definition": "Verwijst naar een mannelijk persoon als onbenadrukte vorm van 'hij'.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "personal_pron",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik vroeg of <w>die</w> nog mee wilde gaan.",
                            "target_lang_translations": {
                                "en": "I asked if <w>he</w> still wanted to come along.",
                                "ru": "Я спросил, хочет ли <w>он</w> пойти с нами."
                            }
                        },
                        {
                            "text": "Wat heeft <w>die</w> nou weer gedaan?",
                            "target_lang_translations": {
                                "en": "What has <w>he</w> done now?",
                                "ru": "Что <w>он</w> опять натворил?"
                            }
                        }
                    ],
                    "synonyms": [
                        "ie",
                        "tie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Spreektaal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Refers to a male person as an unstressed form of 'he'.",
                        "ru": "Разговорная форма личного местоимения, указывающая на лицо мужского пола (вместо 'hij')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "he",
                                "target_lang_sense_clarification": "personal pronoun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "он",
                                "target_lang_sense_clarification": "разговорное употребление"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
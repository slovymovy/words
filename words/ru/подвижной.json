            "senses": [
                {
                    "sense_id": "d07b2c30-58eb-3cf3-965d-266853578904",
                    "sense_definition": "Относящийся к транспортным средствам (вагонам, локомотивам); используется почти исключительно в термине «подвижной состав».",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_terms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Железнодорожный <подвижной> состав требует регулярного технического обслуживания.",
                            "target_lang_translations": {
                                "en": "The railway <w>rolling</w> stock requires regular maintenance."
                            }
                        },
                        {
                            "text": "Компания планирует закупить новый <подвижной> состав для улучшения грузоперевозок.",
                            "target_lang_translations": {
                                "en": "The company plans to purchase new <w>rolling</w> stock to improve freight transport."
                            }
                        }
                    ],
                    "common_phrases": [
                        "подвижной состав"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "used primarily in fixed phrases"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to transport vehicles (wagons, locomotives); used almost exclusively in the term \"rolling stock\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rolling",
                                "target_lang_sense_clarification": "used in 'rolling stock'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c60e810-01b9-36d5-9cbf-e2c0b9bae56a",
                    "sense_definition": "Способный перемещаться или быть перемещённым; то же, что «подвижный», но с ударением на окончании (устаревающая форма).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "movement_properties",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старинном описании упоминался <подвижной> лагерь, который быстро сворачивался.",
                            "target_lang_translations": {
                                "en": "The ancient description mentioned a <w>mobile</w> camp that could be quickly packed up."
                            }
                        },
                        {
                            "text": "Этот <подвижной> механизм позволял менять положение деталей.",
                            "target_lang_translations": {
                                "en": "This <w>movable</w> mechanism allowed changing the position of the parts."
                            }
                        }
                    ],
                    "synonyms": [
                        "подвижный",
                        "передвижной",
                        "мобильный"
                    ],
                    "antonyms": [
                        "неподвижный",
                        "стационарный"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "modern standard is подви́жный"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Capable of moving or being moved; an older or variant form of \"mobile\" or \"movable\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mobile",
                                "target_lang_sense_clarification": "capable of moving"
                            },
                            {
                                "target_lang_word": "movable",
                                "target_lang_sense_clarification": "able to be moved"
                            }
                        ]
                    }
                }

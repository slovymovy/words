    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b8569224-e955-3137-bdb1-f0081ffcf475",
                    "sense_definition": "De eerstvolgende dag na vandaag.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zie je <w>morgen</w> op school.",
                            "target_lang_translations": {
                                "en": "I'll see you <w>tomorrow</w> at school.",
                                "ru": "Увидимся <w>завтра</w> в школе."
                            }
                        },
                        {
                            "text": "We gaan <w>morgen</w> samen naar de film.",
                            "target_lang_translations": {
                                "en": "We are going to the movies together <w>tomorrow</w>.",
                                "ru": "Мы идем <w>завтра</w> вместе в кино."
                            }
                        }
                    ],
                    "synonyms": [
                        "ochtend"
                    ],
                    "antonyms": [
                        "gisteren",
                        "vandaag"
                    ],
                    "common_phrases": [
                        "tot morgen",
                        "morgen vroeg",
                        "morgen avond"
                    ],
                    "target_lang_definitions": {
                        "en": "The day after today.",
                        "ru": "День, следующий за сегодняшним."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tomorrow",
                                "target_lang_sense_clarification": "the day after today"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "завтра",
                                "target_lang_sense_clarification": "на следующий день"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "56a1d063-9655-3d15-a557-50e92164b982",
                    "sense_definition": "Het eerste deel van de dag, vanaf zonsopgang tot de middag.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de <w>morgen</w> drink ik altijd een kopje koffie.",
                            "target_lang_translations": {
                                "en": "In the <w>morning</w> I always drink a cup of coffee.",
                                "ru": "<w>Утром</w> я всегда пью чашку кофе."
                            }
                        },
                        {
                            "text": "Het was een koude <w>morgen</w> in december.",
                            "target_lang_translations": {
                                "en": "It was a cold <w>morning</w> in December.",
                                "ru": "Было холодное декабрьское <w>утро</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ochtend"
                    ],
                    "antonyms": [
                        "avond",
                        "middag",
                        "nacht"
                    ],
                    "common_phrases": [
                        "goede morgen",
                        "vroege morgen",
                        "elke morgen"
                    ],
                    "target_lang_definitions": {
                        "en": "The first part of the day, from sunrise until noon.",
                        "ru": "Первая половина дня, от восхода солнца до полудня."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "morning",
                                "target_lang_sense_clarification": "early part of the day"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "утро",
                                "target_lang_sense_clarification": "время суток"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "98d15906-1908-3d06-8cea-98dd6e6e6ddf",
                    "sense_definition": "Een begroeting die men gebruikt in de ochtend; verkorting van goedemorgen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Morgen</w>! Heb je lekker geslapen?",
                            "target_lang_translations": {
                                "en": "<w>Morning</w>! Did you sleep well?",
                                "ru": "<w>Доброе утро</w>! Ты хорошо спал?"
                            }
                        },
                        {
                            "text": "<w>Morgen</w> allemaal, laten we beginnen.",
                            "target_lang_translations": {
                                "en": "<w>Morning</w> everyone, let's start.",
                                "ru": "<w>Доброе утро</w> всем, давайте начнем."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for goedemorgen"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A greeting used in the morning; short for 'good morning'.",
                        "ru": "Приветствие, используемое утром; сокращение от «доброе утро»."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "morning",
                                "target_lang_sense_clarification": "greeting"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "доброе утро",
                                "target_lang_sense_clarification": "приветствие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

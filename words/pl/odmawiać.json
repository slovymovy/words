{
    "word_family": [
        "odmówić",
        "odmowa",
        "odmowny"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f14c07c1-2906-3c2d-93d1-747e41c05a98",
                    "sense_definition": "Nie zgadzać się na spełnienie czyjejś prośby, żądania lub propozycji; nie chcieć czegoś zrobić lub dać.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "rejection_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad jest bardzo uczynny i nigdy nie <w>odmawia</w> pomocy w trudnych sytuacjach.",
                            "target_lang_translations": {
                                "ru": "Мой сосед очень отзывчивый и никогда не <w>отказывает</w> в помощи в трудных ситуациях."
                            }
                        },
                        {
                            "text": "Dyrektor konsekwentnie <w>odmawia</w> udzielenia komentarza w sprawie ostatnich zwolnień.",
                            "target_lang_translations": {
                                "ru": "Директор последовательно <w>отказывается</w> давать комментарии по поводу последних увольнений."
                            }
                        }
                    ],
                    "synonyms": [
                        "odrzucać",
                        "odtrącać",
                        "negować"
                    ],
                    "antonyms": [
                        "przyznawać"
                    ],
                    "common_phrases": [
                        "odmawiać pomocy",
                        "odmawiać współpracy",
                        "grzecznie odmawiać",
                        "stanowczo odmawiać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не соглашаться на выполнение чьей-либо просьбы, требования или предложения; не хотеть чего-то сделать или дать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отказывать",
                                "target_lang_sense_clarification": "не давать согласия на просьбу"
                            },
                            {
                                "target_lang_word": "отказываться",
                                "target_lang_sense_clarification": "не соглашаться делать что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d0b2049-afad-398d-a618-95620dd56dc8",
                    "sense_definition": "Wypowiadać słowa modlitwy, często w sposób rytmiczny lub powtarzalny.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "recitation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdego wieczoru babcia siada w fotelu i cicho <w>odmawia</w> różaniec.",
                            "target_lang_translations": {
                                "ru": "Каждый вечер бабушка садится в кресло и тихо <w>читает</w> молитвы по четкам."
                            }
                        },
                        {
                            "text": "Wierni zgromadzili się w kościele, aby wspólnie <w>odmawiać</w> modlitwy za pomyślność ojczyzny.",
                            "target_lang_translations": {
                                "ru": "Верующие собрались в костеле, чтобы совместно <w>читать</w> молитвы за благополучие родины."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmawiać"
                    ],
                    "common_phrases": [
                        "odmawiać pacierz",
                        "odmawiać różaniec",
                        "odmawiać modlitwę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произносить слова молитвы, часто ритмично или повторяя их."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "читать",
                                "target_lang_sense_clarification": "произносить молитву"
                            },
                            {
                                "target_lang_word": "произносить",
                                "target_lang_sense_clarification": "говорить слова молитвы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b772a1d3-da46-3e81-9926-f7e447e19c93",
                    "sense_definition": "Przekonywać kogoś, aby zrezygnował z powziętego zamiaru lub planowanego działania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "persuasion_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice długo próbowali <w>odmawiać</w> synowi pomysł samotnej podróży dookoła świata.",
                            "target_lang_translations": {
                                "ru": "Родители долго пытались <w>отговаривать</w> сына от идеи одиночного кругосветного путешествия."
                            }
                        },
                        {
                            "text": "Przyjaciele starali się <w>odmawiać</w> go od ryzykownej inwestycji, ale on był uparty.",
                            "target_lang_translations": {
                                "ru": "Друзья старались <w>отговаривать</w> его от рискованной инвестиции, но он был упрям."
                            }
                        }
                    ],
                    "synonyms": [
                        "odradzać"
                    ],
                    "common_phrases": [
                        "odmawiać od zamiaru",
                        "skutecznie odmawiać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убеждать кого-либо отказаться от задуманного или планируемого действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отговаривать",
                                "target_lang_sense_clarification": "убеждать не делать чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af32a843-8c15-32ba-92ba-f50319406557",
                    "sense_definition": "Wycofywać złożone wcześniej zamówienie lub rezygnować z regularnego otrzymywania czegoś (np. prasy).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rejection_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu czytelników zaczęło <w>odmawiać</w> prenumeratę gazety po zmianie redaktora naczelnego.",
                            "target_lang_translations": {
                                "ru": "Многие читатели начали <w>отказываться</w> от подписки на газету после смены главного редактора."
                            }
                        },
                        {
                            "text": "Zdecydowaliśmy się <w>odmawiać</w> codzienne dostawy mleka, ponieważ wyjeżdżamy na urlop.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>отказаться</w> от ежедневной доставки молока, так как уезжаем в отпуск."
                            }
                        }
                    ],
                    "synonyms": [
                        "rezygnować"
                    ],
                    "common_phrases": [
                        "odmawiać prenumeratę",
                        "odmawiać gazetę",
                        "odmawiać zamówienie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отменять ранее сделанный заказ или отказываться от регулярного получения чего-либо (например, прессы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отказываться",
                                "target_lang_sense_clarification": "от подписки или услуги"
                            },
                            {
                                "target_lang_word": "аннулировать",
                                "target_lang_sense_clarification": "отменять заказ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9cb6dd1-08b0-389b-ab50-c57ed8b0478a",
                    "sense_definition": "Usuwać rzucony urok lub czar za pomocą specjalnych formuł słownych i gestów.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "recitation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miejscowa szeptucha potrafiła skutecznie <w>odmawiać</w> uroki rzucone na małe dzieci.",
                            "target_lang_translations": {
                                "ru": "Местная знахарка умела эффективно <w>снимать</w> сглаз, наложенный на маленьких детей."
                            }
                        },
                        {
                            "text": "W dawnych wierzeniach ludowych wiedziano, jak <w>odmawiać</w> choroby przy użyciu ziół i zaklęć.",
                            "target_lang_translations": {
                                "ru": "В древних народных верованиях знали, как <w>заговаривать</w> болезни при помощи трав и заклинаний."
                            }
                        }
                    ],
                    "synonyms": [
                        "odczarowywać"
                    ],
                    "common_phrases": [
                        "odmawiać urok",
                        "odmawiać czary"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "związane z kulturą ludową i folklorem"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Снимать наложенное заклятие или чары с помощью специальных словесных формул и жестов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снимать",
                                "target_lang_sense_clarification": "сглаз или порчу"
                            },
                            {
                                "target_lang_word": "отчитывать",
                                "target_lang_sense_clarification": "избавлять от чар молитвами"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
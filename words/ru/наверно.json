            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "48dca366-1e6d-3b25-bf45-1d8bee72d765",
                    "sense_definition": "Вероятно, по всей видимости; вводное слово, выражающее неуверенность или предположение.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "probability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы, <наверно>, пойдём завтра в кино, если будет хорошая погода.",
                            "target_lang_translations": {
                                "en": "We will <probably> go to the cinema tomorrow if the weather is good."
                            }
                        },
                        {
                            "text": "Ты, <наверно>, очень устал после долгой работы.",
                            "target_lang_translations": {
                                "en": "You are <probably> very tired after working for so long."
                            }
                        }
                    ],
                    "synonyms": [
                        "вероятно",
                        "возможно",
                        "по-видимому",
                        "по всей видимости",
                        "предположительно"
                    ],
                    "antonyms": [
                        "точно",
                        "определённо",
                        "вероятно"
                    ],
                    "common_phrases": [
                        "наверно, да",
                        "наверно, нет",
                        "это наверно"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to indicate that something is very likely to happen or be true, though not completely certain."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "probably",
                                "target_lang_sense_clarification": "in all likelihood"
                            },
                            {
                                "target_lang_word": "most likely",
                                "target_lang_sense_clarification": "very probable"
                            },
                            {
                                "target_lang_word": "I guess",
                                "target_lang_sense_clarification": "informal assumption"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae362d3b-91c9-3f64-a056-fb1909d2dcab",
                    "sense_definition": "Точно, несомненно, наверняка; с полной уверенностью.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хотел узнать это <наверно>, чтобы больше не сомневаться.",
                            "target_lang_translations": {
                                "en": "I wanted to know this <for sure> so as not to doubt anymore."
                            }
                        },
                        {
                            "text": "Он <наверно> знал, что победа будет за нами.",
                            "target_lang_translations": {
                                "en": "He knew <for certain> that victory would be ours."
                            }
                        }
                    ],
                    "synonyms": [
                        "точно",
                        "несомненно",
                        "наверняка",
                        "определённо"
                    ],
                    "antonyms": [

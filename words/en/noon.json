    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fadb21cd-7c52-31e3-9d26-4fec957f062c",
                    "sense_definition": "The time of day corresponding to 12 o'clock in the daytime; the middle of the day.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_of_day",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We agreed to meet at <w>noon</w> for lunch.",
                            "target_lang_translations": {
                                "ru": "Мы договорились встретиться в <w>полдень</w> за обедом."
                            }
                        },
                        {
                            "text": "The sun was high in the sky, for it was nearly <w>noon</w>.",
                            "target_lang_translations": {
                                "ru": "Солнце стояло высоко в небе, так как был почти <w>полдень</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "midday",
                        "twelve",
                        "noontide"
                    ],
                    "antonyms": [
                        "midnight"
                    ],
                    "common_phrases": [
                        "at noon",
                        "high noon",
                        "before noon",
                        "after noon",
                        "noon break"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время дня, соответствующее 12 часам; середина дня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полдень",
                                "target_lang_sense_clarification": "время суток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6c59f0b-6a4c-3653-8858-01e2ee1a0e87",
                    "sense_definition": "The highest point or culmination of something, such as a career or life.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_peak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The empire was at the <w>noon</w> of its glory when the war began.",
                            "target_lang_translations": {
                                "ru": "Империя была в <w>зените</w> своей славы, когда началась война."
                            }
                        },
                        {
                            "text": "He died tragically in the <w>noon</w> of his life.",
                            "target_lang_translations": {
                                "ru": "Он трагически погиб в <w>расцвете</w> лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "apex",
                        "capstone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высшая точка или кульминация чего-либо, например, карьеры или жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зенит",
                                "target_lang_sense_clarification": "высшая степень"
                            },
                            {
                                "target_lang_word": "расцвет",
                                "target_lang_sense_clarification": "лучшая пора"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e55403db-67a8-3ab6-913a-f4a78d4aba6a",
                    "sense_definition": "To stop, rest, or eat a meal in the middle of the day.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>nooned</w> beside a small stream to water the horses.",
                            "target_lang_translations": {
                                "ru": "Они <w>остановились на полдень</w> у ручья, чтобы напоить лошадей."
                            }
                        },
                        {
                            "text": "We <w>nooned</w> under the trees to avoid the midday sun.",
                            "target_lang_translations": {
                                "ru": "Мы <w>переждали полдень</w> под деревьями, чтобы укрыться от полуденного солнца."
                            }
                        }
                    ],
                    "synonyms": [
                        "sleep",
                        "midday",
                        "nones"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often used in historical or regional contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Останавливаться, отдыхать или принимать пищу в середине дня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "останавливаться на полдень",
                                "target_lang_sense_clarification": "делать привал"
                            },
                            {
                                "target_lang_word": "полудничать",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                }
            ]
        },
        {

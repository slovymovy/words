{
    "word_family": [
        "kade",
        "kauw"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e495675c-bafe-3312-b797-6bf2c2a583a1",
                    "sense_definition": "Een versterkte oever of wal langs het water waar schepen kunnen aanleggen; verkorte vorm van <w>kade</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We wandelden 's avonds gezellig langs de <w>ka</w>.",
                            "target_lang_translations": {
                                "ru": "Мы приятно прогуливались вечером по <w>набережной</w>."
                            }
                        },
                        {
                            "text": "Het oude schip lag stevig vastgebonden aan de <w>ka</w>.",
                            "target_lang_translations": {
                                "ru": "Старый корабль был крепко привязан к <w>причалу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan de ka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "shortening of kade"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Укрепленный берег или стена вдоль воды, где могут швартоваться корабли; сокращенная форма от \"kade\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набережная",
                                "target_lang_sense_clarification": "укрепленная часть берега"
                            },
                            {
                                "target_lang_word": "причал",
                                "target_lang_sense_clarification": "место швартовки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "379c44ef-ebab-3315-b589-ec068d26c96e",
                    "sense_definition": "Een felle, bazige vrouw die duidelijk en soms onaardig haar mening laat horen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn tante staat bekend als een echte <w>ka</w> die iedereen de les leest.",
                            "target_lang_translations": {
                                "ru": "Его тетя известна как настоящая <w>мегера</w>, которая всех поучает."
                            }
                        },
                        {
                            "text": "Pas op voor die <w>ka</w>, want ze heeft een hele scherpe tong.",
                            "target_lang_translations": {
                                "ru": "Остерегайся этой <w>злюки</w>, у нее очень острый язык."
                            }
                        }
                    ],
                    "synonyms": [
                        "kenau"
                    ],
                    "common_phrases": [
                        "een echte ka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сварливая, властная женщина, которая громко и иногда грубо высказывает свое мнение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мегера",
                                "target_lang_sense_clarification": "злая женщина"
                            },
                            {
                                "target_lang_word": "злюка",
                                "target_lang_sense_clarification": "разг."
                            },
                            {
                                "target_lang_word": "сварливая баба",
                                "target_lang_sense_clarification": "груб."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3c8bf7e-a322-3567-885f-36ba3f2f3d7a",
                    "sense_definition": "Een zwarte vogel uit de familie van de kraaien; andere naam voor <w>kauw</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zat een eenzame <w>ka</w> op de schoorsteen te roepen.",
                            "target_lang_translations": {
                                "ru": "Одинокая <w>галка</w> сидела на трубе и кричала."
                            }
                        },
                        {
                            "text": "De <w>ka</w> vloog snel weg toen de kat dichterbij kwam.",
                            "target_lang_translations": {
                                "ru": "<w>Галка</w> быстро улетела, когда кошка подошла ближе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "or archaic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Черная птица из семейства врановых; другое название для \"kauw\" (галка)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "галка",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72892e44-87b4-3e05-81f8-2a8a63cd1dbf",
                    "sense_definition": "Een spiritueel concept uit het oude Egypte dat de levenskracht of ziel van een persoon of god vertegenwoordigt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude Egyptenaren geloofden dat de <w>ka</w> na de dood bleef bestaan.",
                            "target_lang_translations": {
                                "ru": "Древние египтяне верили, что <w>Ка</w> продолжает существовать после смерти."
                            }
                        },
                        {
                            "text": "In het graf werden offers gebracht om de <w>ka</w> van de farao te voeden.",
                            "target_lang_translations": {
                                "ru": "В гробницу приносили подношения, чтобы накормить <w>Ка</w> фараона."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Духовное понятие из Древнего Египта, олицетворяющее жизненную силу или душу человека или бога."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ка",
                                "target_lang_sense_clarification": "египетская мифология"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "7fbfb3e3-2f0f-3eda-8c57-c454e4531810",
                    "sense_definition": "Nabootsing van het krassende geluid dat een kraai of kauw maakt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "\"<w>Ka</w>, <w>ka</w>!\" klonk het luid vanuit de hoge bomen.",
                            "target_lang_translations": {
                                "ru": "\"<w>Кар</w>, <w>кар</w>!\" — громко раздалось с высоких деревьев."
                            }
                        },
                        {
                            "text": "De vogel riep een keer <w>ka</w> en spreidde toen zijn vleugels.",
                            "target_lang_translations": {
                                "ru": "Птица крикнула один раз \"<w>кар</w>\" и расправила крылья."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подражание каркающему звуку, который издает ворона или галка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кар",
                                "target_lang_sense_clarification": "звукоподражание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
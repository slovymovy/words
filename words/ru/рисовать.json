                    "sense_id": "bdc3935a-6500-3c01-b5e5-a65219246b3b",
                    "sense_definition": "Создавать изображения на плоскости с помощью карандаша, красок или других инструментов.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дети очень любят <w>рисовать</w> цветными карандашами.",
                            "target_lang_translations": {
                                "en": "Children really love <w>drawing</w> with colored pencils."
                            }
                        },
                        {
                            "text": "Художник начал <w>рисовать</w> красивый пейзаж.",
                            "target_lang_translations": {
                                "en": "The artist began to <w>paint</w> a beautiful landscape."
                            }
                        }
                    ],
                    "synonyms": [
                        "писать",
                        "зарисовывать",
                        "чертить",
                        "малевать"
                    ],
                    "antonyms": [
                        "стирать"
                    ],
                    "common_phrases": [
                        "рисовать карандашом",
                        "рисовать красками",
                        "рисовать портрет",
                        "уметь рисовать"
                    ],
                    "target_lang_definitions": {
                        "en": "To create images on a surface using a pencil, paints, or other tools."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "to produce a picture with lines, e.g. with a pencil"
                            },
                            {
                                "target_lang_word": "paint",
                                "target_lang_sense_clarification": "to create a picture using paints"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44e45dc4-7fec-304c-b2ad-5640ac863704",
                    "sense_definition": "Воссоздавать образы в воображении или описывать что-либо словами.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_depiction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя память часто <w>рисовала</w> картины счастливого детства.",
                            "target_lang_translations": {
                                "en": "My memory often <w>painted</w> pictures of a happy childhood."
                            }
                        },
                        {
                            "text": "Воображение <w>рисует</w> нам сказочное будущее.",
                            "target_lang_translations": {
                                "en": "Imagination <w>paints</w> us a fairytale future."
                            }
                        }
                    ],
                    "synonyms": [
                        "воображать"
                    ],
                    "antonyms": [
                        "забывать"
                    ],
                    "common_phrases": [
                        "воображение рисует",
                        "память рисует"
                    ],
                    "target_lang_definitions": {
                        "en": "To recreate images in the imagination or describe something in words."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "picture",
                                "target_lang_sense_clarification": "to imagine or envision"
                            },
                            {
                                "target_lang_word": "imagine",
                                "target_lang_sense_clarification": "to form a mental image"
                            },
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "to depict or describe in words"
                            }
                        ]
                    }

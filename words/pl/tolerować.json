            "pos": "verb",
            "senses": [
                {
                    "sense_id": "43d9c428-551a-3b8a-b98a-52454f68780d",
                    "sense_definition": "Godzić się na czyjeś zachowanie, poglądy lub obecność, mimo braku pełnej akceptacji lub sympatii.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_acceptance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel ostrzegł, że nie będzie dłużej <w>tolerować</w> spóźniania się na lekcje.",
                            "target_lang_translations": {
                                "ru": "Учитель предупредил, что больше не будет <w>терпеть</w> опозданий на уроки."
                            }
                        },
                        {
                            "text": "Musimy nauczyć się <w>tolerować</w> odmienne zdanie innych ludzi, aby żyć w zgodzie.",
                            "target_lang_translations": {
                                "ru": "Мы должны научиться <w>терпеть</w> иное мнение других людей, чтобы жить в согласии."
                            }
                        }
                    ],
                    "synonyms": [
                        "znosić",
                        "wytrzymywać",
                        "dopuszczać",
                        "przyzwalać"
                    ],
                    "antonyms": [
                        "nie godzić się",
                        "sprzeciwiać się",
                        "zakazywać",
                        "oburzać się"
                    ],
                    "common_phrases": [
                        "tolerować czyjeś zachowanie",
                        "nie będę tego tolerować",
                        "tolerować odmienne poglądy",
                        "trudno to tolerować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мириться с чьим-то поведением, взглядами или присутствием, несмотря на отсутствие полного одобрения или симпатии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терпеть",
                                "target_lang_sense_clarification": "стоически переносить что-то неприятное"
                            },
                            {
                                "target_lang_word": "мириться",
                                "target_lang_sense_clarification": "принимать ситуацию, не пытаясь её изменить"
                            },
                            {
                                "target_lang_word": "допускать",
                                "target_lang_sense_clarification": "позволять чему-то происходить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7008fa1b-79dd-3cc1-b5a5-f78cd98c2ce4",
                    "sense_definition": "O organizmie: przyjmować leki, pokarmy lub inne czynniki bez występowania szkodliwych reakcji zdrowotnych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "biological_response",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent bardzo dobrze <w>toleruje</w> nowy lek i szybko wraca do zdrowia.",
                            "target_lang_translations": {
                                "ru": "Пациент очень хорошо <w>переносит</w> новое лекарство и быстро выздоравливает."
                            }
                        },
                        {
                            "text": "Mój organizm przestał <w>tolerować</w> produkty mleczne, więc musiałem zmienić dietę.",
                            "target_lang_translations": {
                                "ru": "Мой организм перестал <w>переносить</w> молочные продукты, поэтому мне пришлось изменить диету."
                            }
                        }
                    ],
                    "synonyms": [
                        "znosić"
                    ],
                    "common_phrases": [
                        "dobrze tolerować lek",
                        "nie tolerować laktozy",
                        "źle tolerować leczenie",
                        "organizm toleruje przeszczep"
                    ],
                    "target_lang_definitions": {
                        "ru": "Об организме: принимать лекарства, пищу или другие факторы без возникновения вредных реакций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переносить",
                                "target_lang_sense_clarification": "об организме: реагировать на воздействие"
                            },
                            {
                                "target_lang_word": "усваивать",
                                "target_lang_sense_clarification": "о пище: переваривать без проблем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07238717-e60f-3c8c-9e74-36b7fd97ddfe",
                    "sense_definition": "Znosić wzajemnie swoje towarzystwo lub obecność, mimo braku przyjaźni.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "social_acceptance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo różnicy charakterów, bracia potrafią się <w>tolerować</w> podczas rodzinnych spotkań.",
                            "target_lang_translations": {
                                "ru": "Несмотря на разницу характеров, братья могут <w>терпеть</w> друг друга во время семейных встреч."
                            }
                        },
                        {
                            "text": "Sąsiedzi nie przepadają za sobą, ale starają się jakoś <w>tolerować</w>.",
                            "target_lang_translations": {
                                "ru": "Соседи не очень любят друг друга, но стараются как-то <w>терпеть</w> друг друга."
                            }
                        }
                    ],
                    "synonyms": [
                        "znosić"
                    ],
                    "common_phrases": [
                        "ledwo się tolerować",
                        "nawzajem się tolerować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выносить общество друг друга или чье-то присутствие, несмотря на отсутствие дружеских чувств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терпеть",
                                "target_lang_sense_clarification": "выносить присутствие друг друга"
                            },
                            {
                                "target_lang_word": "уживаться",
                                "target_lang_sense_clarification": "ладить, несмотря на различия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e455e278-e268-37d5-8d12-8685da3b11e2",
                    "sense_definition": "A document, card, or official paper that proves a person's identity.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "identity_docs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must show a valid photo <w>ID</w> to enter the government building.",
                            "target_lang_translations": {
                                "ru": "Вы должны предъявить действительное <w>удостоверение личности</w> с фотографией, чтобы войти в правительственное здание."
                            }
                        },
                        {
                            "text": "I left my wallet at home, so I do not have my <w>ID</w> with me.",
                            "target_lang_translations": {
                                "ru": "Я оставил кошелек дома, поэтому у меня нет с собой <w>документов</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "photo ID",
                        "valid ID",
                        "student ID",
                        "government ID"
                    ],
                    "target_lang_definitions": {
                        "ru": "Документ, карта или официальная бумага, подтверждающая личность человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удостоверение личности",
                                "target_lang_sense_clarification": "официальный документ"
                            },
                            {
                                "target_lang_word": "документы",
                                "target_lang_sense_clarification": "разговорное, во множественном числе"
                            },
                            {
                                "target_lang_word": "паспорт",
                                "target_lang_sense_clarification": "часто используется как синоним удостоверения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f6ecd68-a92a-3eed-ae16-47d0bf07386c",
                    "sense_definition": "A unique sequence of numbers or characters used to identify a user, account, or item in a system.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "identity_docs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please enter your user <w>ID</w> and password to log in to the website.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, введите свой <w>идентификатор</w> пользователя и пароль для входа на сайт."
                            }
                        },
                        {
                            "text": "Each product in the warehouse is assigned a unique tracking <w>ID</w>.",
                            "target_lang_translations": {
                                "ru": "Каждому товару на складе присваивается уникальный <w>идентификационный номер</w> для отслеживания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "user ID",
                        "login ID",
                        "caller ID"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уникальная последовательность цифр или символов для идентификации пользователя или элемента в системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идентификатор",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "ID",
                                "target_lang_sense_clarification": "заимствование, часто используется в интернете"
                            },
                            {
                                "target_lang_word": "айди",
                                "target_lang_sense_clarification": "разговорное, сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b887141-2194-310a-8e24-40ea89c5b18b",
                    "sense_definition": "The part of the unconscious mind that is the source of basic impulses, needs, and desires.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "psychoanalysis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "According to Freud, the <w>id</w> operates on the pleasure principle, seeking immediate gratification.",
                            "target_lang_translations": {
                                "ru": "Согласно Фрейду, <w>Ид</w> действует по принципу удовольствия, стремясь к немедленному удовлетворению."
                            }
                        },
                        {
                            "text": "He felt a conflict between his moral conscience and the primal urges of his <w>id</w>.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал конфликт между своей моральной совестью и первобытными побуждениями своего <w>Ид</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lizard brain",
                        "reptilian brain"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical psychoanalysis context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть бессознательного, являющаяся источником основных инстинктов и желаний (в психоанализе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ид",
                                "target_lang_sense_clarification": "термин психоанализа"
                            },
                            {
                                "target_lang_word": "Оно",
                                "target_lang_sense_clarification": "классический перевод термина Фрейда"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07cc6e15-fb8f-3602-bed6-ba5722379abc",
                    "sense_definition": "To request to see someone's identification document to prove their age or identity.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "check_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bouncer at the club will <w>ID</w> everyone who looks under thirty.",
                            "target_lang_translations": {
                                "ru": "Вышибала в клубе будет <w>проверять документы</w> у всех, кто выглядит моложе тридцати."
                            }
                        },
                        {
                            "text": "I was surprised when the cashier decided to <w>ID</w> me for buying lottery tickets.",
                            "target_lang_translations": {
                                "ru": "Я удивился, когда кассир решил <w>спросить у меня паспорт</w> при покупке лотерейных билетов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get ID'd"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Требовать предъявить документ, удостоверяющий личность, для подтверждения возраста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверять документы",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "спрашивать паспорт",
                                "target_lang_sense_clarification": "разговорное, при покупке алкоголя и т.д."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63994ed4-e321-3879-8e22-d7cf5c608db9",
                    "sense_definition": "To identify or recognize a person or object.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "check_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police were unable to <w>ID</w> the suspect from the blurry security footage.",
                            "target_lang_translations": {
                                "ru": "Полиция не смогла <w>опознать</w> подозреваемого по размытым кадрам с камеры наблюдения."
                            }
                        },
                        {
                            "text": "Can you <w>ID</w> this strange bird sitting on the fence?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>определить</w>, что это за странная птица сидит на заборе?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Опознать или распознать человека или объект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опознать",
                                "target_lang_sense_clarification": "установить личность (часто в полиции)"
                            },
                            {
                                "target_lang_word": "идентифицировать",
                                "target_lang_sense_clarification": "формальное"
                            },
                            {
                                "target_lang_word": "определить",
                                "target_lang_sense_clarification": "понять, что это за объект"
                            }
                        ]
                    }
                }
            ]
        },

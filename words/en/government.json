    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1af638f7-02e2-3681-be25-b18be74da387",
                    "sense_definition": "The group of people with the authority to govern a country, state, or organization.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "political_authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>government</w> has announced new laws to protect the environment.",
                            "target_lang_translations": {
                                "ru": "<w>Правительство</w> объявило о новых законах для защиты окружающей среды."
                            }
                        },
                        {
                            "text": "She works for the local <w>government</w> in the housing department.",
                            "target_lang_translations": {
                                "ru": "Она работает в местной <w>администрации</w> в жилищном отделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "administration"
                    ],
                    "antonyms": [
                        "anarchy"
                    ],
                    "common_phrases": [
                        "federal government",
                        "local government",
                        "national government",
                        "government official"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, обладающих полномочиями управлять страной, государством или организацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правительство",
                                "target_lang_sense_clarification": "высший орган исполнительной власти"
                            },
                            {
                                "target_lang_word": "администрация",
                                "target_lang_sense_clarification": "органы управления (часто местные)"
                            },
                            {
                                "target_lang_word": "власть",
                                "target_lang_sense_clarification": "политическое руководство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "254bcfe7-58ac-3899-9fe4-d3f8b26b3264",
                    "sense_definition": "A specific ministry or administration led by a particular leader or party during a specific period.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "political_authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new <w>government</w> promised to reduce unemployment rates significantly.",
                            "target_lang_translations": {
                                "ru": "Новое <w>правительство</w> пообещало значительно снизить уровень безработицы."
                            }
                        },
                        {
                            "text": "Policies changed drastically under the previous <w>government</w>.",
                            "target_lang_translations": {
                                "ru": "Политика радикально изменилась при предыдущем <w>правительстве</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "administration"
                    ],
                    "common_phrases": [
                        "current government",
                        "previous government",
                        "new government"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный состав кабинета министров или администрация под руководством определенного лидера в определенный период."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правительство",
                                "target_lang_sense_clarification": "кабинет министров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32393519-3581-386c-94ac-ac501c35c943",
                    "sense_definition": "The political party or coalition currently holding power in a parliamentary system.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "political_authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>government</w> faced a vote of no confidence from the opposition.",
                            "target_lang_translations": {
                                "ru": "<w>Правительство</w> столкнулось с вотумом недоверия со стороны оппозиции."
                            }
                        },
                        {
                            "text": "It is often difficult for a minority <w>government</w> to pass legislation.",
                            "target_lang_translations": {
                                "ru": "<w>Правительству</w> меньшинства часто бывает трудно принимать законы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "minority government",
                        "coalition government"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in UK and Commonwealth English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Политическая партия или коалиция, удерживающая власть в парламентской системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правительство",
                                "target_lang_sense_clarification": "правящая партия или коалиция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "909c1986-31af-3ce3-bf03-b3270a114ee7",
                    "sense_definition": "The action or manner of controlling or regulating a system or organization.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "act_of_governing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>government</w> of the church is organized in a strict hierarchy.",
                            "target_lang_translations": {

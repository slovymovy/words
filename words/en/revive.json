    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8bb27104-ebbe-34a2-acf7-f7e0047b3ec8",
                    "sense_definition": "To make something active, successful, or popular again after a period of decline or disuse.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "restore_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government introduced new policies to <w>revive</w> the struggling economy."
                        },
                        {
                            "text": "Visiting the art gallery helped to <w>revive</w> my interest in painting."
                        }
                    ],
                    "synonyms": [
                        "renew",
                        "refresh",
                        "reinvigorate",
                        "reestablish",
                        "revivify"
                    ],
                    "antonyms": [
                        "devitalize"
                    ],
                    "common_phrases": [
                        "revive the economy",
                        "revive interest",
                        "revive a custom",
                        "revive a tradition"
                    ]
                },
                {
                    "sense_id": "c88b8ae4-4e1d-3def-a6e4-f1211007ec4c",
                    "sense_definition": "To bring someone back to consciousness, life, or health, or to return to such a state.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "restore_life",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The paramedics worked quickly to <w>revive</w> the man after his heart stopped."
                        },
                        {
                            "text": "She fainted in the heat but soon <w>revived</w> after drinking some cold water."
                        }
                    ],
                    "synonyms": [
                        "reanimate",
                        "resurrect"
                    ],
                    "common_phrases": [
                        "revive a patient",
                        "revive someone"
                    ]
                },
                {
                    "sense_id": "7e516fe6-68a1-31bf-a621-8b4803e6adcc",
                    "sense_definition": "To bring a feeling, memory, or relationship back into existence or activity.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "restore_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old photograph <w>revived</w> memories of their childhood holidays by the sea."
                        },
                        {
                            "text": "They tried to <w>revive</w> their romance with a trip to Paris, but it was too late."
                        }
                    ],
                    "synonyms": [
                        "rekindle",
                        "renew"
                    ],
                    "common_phrases": [
                        "revive memories",
                        "revive hope"
                    ]
                },
                {
                    "sense_id": "a56823ef-31f2-38f2-a142-713906e124aa",
                    "sense_definition": "To produce a play, opera, or show again that has not been performed for some time.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "theater",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The theater company plans to <w>revive</w> a classic comedy from the last century."
                        },
                        {
                            "text": "Several popular musicals have been <w>revived</w> on Broadway this season."
                        }
                    ],
                    "common_phrases": [
                        "revive a play",
                        "revive a production"
                    ]
                }
            ]
        }
    ]

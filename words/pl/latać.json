    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "eaee8b91-424d-3941-846d-102f3108c8a0",
                    "sense_definition": "Poruszać się w powietrzu za pomocą skrzydeł lub statku powietrznego.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ptaki <latają> wysoko na niebie każdego ranka.",
                            "target_lang_translations": {
                                "ru": "Птицы <летают> высоко в небе каждое утро."
                            }
                        },
                        {
                            "text": "Mój brat boi się <latać> samolotami pasażerskimi.",
                            "target_lang_translations": {
                                "ru": "Мой брат боится <летать> на пассажирских самолетах."
                            }
                        }
                    ],
                    "synonyms": [
                        "fruwać",
                        "być w locie"
                    ],
                    "antonyms": [
                        "chodzić"
                    ],
                    "common_phrases": [
                        "latać samolotem",
                        "uczyć się latać",
                        "latać wysoko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передвигаться по воздуху с помощью крыльев или на летательном аппарате."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "летать",
                                "target_lang_sense_clarification": "перемещаться по воздуху"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07a79575-73cf-3baa-8c08-cd2d6bc32ea2",
                    "sense_definition": "Szybko przemieszczać się z miejsca na miejsce, często w pośpiechu, załatwiając różne sprawy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_ground",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cały dzień <latam> po mieście, żeby załatwić ten wyjazd.",
                            "target_lang_translations": {
                                "ru": "Я весь день <бегаю> по городу, чтобы организовать эту поездку."
                            }
                        },
                        {
                            "text": "Dzieci <latają> po podwórku i bawią się w berka.",
                            "target_lang_translations": {
                                "ru": "Дети <носятся> по двору и играют в салки."
                            }
                        }
                    ],
                    "synonyms": [
                        "biegać",
                        "gw. (Górny Śląsk) lotać"
                    ],
                    "common_phrases": [
                        "latać po sklepach",
                        "latać w kółko",
                        "latać z językiem na brodzie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро перемещаться с места на место, часто в спешке, занимаясь делами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бегать",
                                "target_lang_sense_clarification": "суетливо перемещаться, хлопотать"
                            },
                            {
                                "target_lang_word": "носиться",
                                "target_lang_sense_clarification": "очень быстро передвигаться (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ee59b91-dfa5-3181-a20f-a697a59f4516",
                    "sense_definition": "Usilnie zabiegać o czyjeś względy lub starać się coś zdobyć.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek ciągle <lata> za tą nową koleżanką z pracy.",
                            "target_lang_translations": {
                                "ru": "Марек постоянно <бегает> за этой новой коллегой с работы."
                            }
                        },
                        {
                            "text": "Nie warto całe życie <latać> za pieniędzmi i sławą.",
                            "target_lang_translations": {
                                "ru": "Не стоит всю жизнь <гоняться> за деньгами и славой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "latać za dziewczynami",
                        "latać za chłopakami"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво добиваться чьего-либо внимания или стараться что-то получить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бегать",
                                "target_lang_sense_clarification": "преследовать, ухаживать (обычно с предлогом «за»)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43f6473b-f33c-3c5c-ab2c-8fe63e11bc37",
                    "sense_definition": "Być niestabilnym, chwiać się lub poruszać luźno z powodu słabego przymocowania.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klamka w drzwiach jest zepsuta i <lata> na wszystkie strony.",
                            "target_lang_translations": {
                                "ru": "Дверная ручка сломана и <болтается> во все стороны."
                            }
                        },
                        {
                            "text": "Umocuj to pudełko, bo będzie ci <latać> w bagażniku.",
                            "target_lang_translations": {
                                "ru": "Закрепи эту коробку, а то она будет <болтаться> у тебя в багажнике."
                            }
                        }
                    ],
                    "common_phrases": [
                        "coś lata luzem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть неустойчивым, шататься или свободно двигаться из-за плохого крепления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болтаться",
                                "target_lang_sense_clarification": "быть плохо закрепленным (разг.)"
                            },
                            {
                                "target_lang_word": "шататься",
                                "target_lang_sense_clarification": "быть неустойчивым"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b22d3991-6c21-3581-a007-a7fd4e766b72",
                    "sense_definition": "(z zaimkiem „się”) Zadeklarować swoje przybycie, gotowość do udziału w czymś lub chęć wykonania zadania.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent musi najpierw <w>zgłosić</w> się do rejestracji w przychodni.",
                            "target_lang_translations": {
                                "ru": "Пациент должен сначала <w>обратиться</w> в регистратуру в поликлинике."
                            }
                        },
                        {
                            "text": "Marek <w>zgłosił</w> się na ochotnika do pomocy przy sprzątaniu sali.",
                            "target_lang_translations": {
                                "ru": "Марек <w>вызвался</w> добровольцем помочь в уборке зала."
                            }
                        }
                    ],
                    "synonyms": [
                        "stawić się",
                        "zapisać się"
                    ],
                    "antonyms": [
                        "wycofać się",
                        "zrezygnować"
                    ],
                    "common_phrases": [
                        "zgłosić się na ochotnika",
                        "zgłosić się do lekarza",
                        "zgłosić się do odpowiedzi",
                        "zgłosić się po odbiór"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заявить о своем прибытии, готовности участвовать или желании выполнить задачу (обычно с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "явиться",
                                "target_lang_sense_clarification": "официально прийти куда-либо"
                            },
                            {
                                "target_lang_word": "обратиться",
                                "target_lang_sense_clarification": "подойти к кому-либо по делу"
                            },
                            {
                                "target_lang_word": "вызваться",
                                "target_lang_sense_clarification": "предложить свою помощь, стать добровольцем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "310dc844-d231-3960-ad88-4d54da263da8",
                    "sense_definition": "Poinformować odpowiednie służby, instytucje lub osoby o jakimś zdarzeniu, problemie, fakcie lub wykroczeniu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transitive_report",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Świadek natychmiast <w>zgłosił</w> wypadek drogowy na numer alarmowy.",
                            "target_lang_translations": {
                                "ru": "Свидетель немедленно <w>сообщил</w> о ДТП по номеру экстренной службы."
                            }
                        },
                        {
                            "text": "Klient <w>zgłosił</w> reklamację z powodu wadliwego działania sprzętu.",
                            "target_lang_translations": {
                                "ru": "Клиент <w>подал</w> рекламацию из-за неисправности оборудования."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawiadomić",
                        "poinformować",
                        "zameldować"
                    ],
                    "antonyms": [
                        "zataić",
                        "ukryć"
                    ],
                    "common_phrases": [
                        "zgłosić przestępstwo",
                        "zgłosić awarię",
                        "zgłosić reklamację",
                        "zgłosić zaginięcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщить соответствующим службам или лицам о событии, проблеме или правонарушении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сообщить",
                                "target_lang_sense_clarification": "проинформировать о факте"
                            },
                            {
                                "target_lang_word": "заявить",
                                "target_lang_sense_clarification": "официально доложить (напр. в полицию)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3caf9f97-43dd-3d60-bdce-cc85dfb3f718",
                    "sense_definition": "Oficjalnie przedstawić propozycję, wniosek, projekt lub kandydata do zarejestrowania i rozpatrzenia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transitive_submit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poseł opozycji <w>zgłosił</w> wniosek o przerwę w obradach sejmu.",
                            "target_lang_translations": {
                                "ru": "Депутат от оппозиции <w>внес</w> предложение о перерыве в заседании сейма."
                            }
                        },
                        {
                            "text": "Firma <w>zgłosiła</w> swój nowy wynalazek do urzędu patentowego.",
                            "target_lang_translations": {
                                "ru": "Компания <w>подала заявку</w> на свое новое изобретение в патентное бюро."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedłożyć",
                        "wnieść",
                        "zarejestrować"
                    ],
                    "antonyms": [
                        "wycofać"
                    ],
                    "common_phrases": [
                        "zgłosić kandydaturę",
                        "zgłosić wniosek",
                        "zgłosić udział",
                        "zgłosić projekt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально представить предложение, заявку или кандидата для регистрации и рассмотрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подать",
                                "target_lang_sense_clarification": "документ, жалобу или заявку"
                            },
                            {
                                "target_lang_word": "выдвинуть",
                                "target_lang_sense_clarification": "кандидатуру"
                            },
                            {
                                "target_lang_word": "внести",
                                "target_lang_sense_clarification": "предложение или проект"
                            }
                        ]
                    }
                }
            ]
        }
    ]

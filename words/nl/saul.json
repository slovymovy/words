                        "ru": [
                            {
                                "target_lang_word": "Саул",
                                "target_lang_sense_clarification": "библейский царь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db459916-b513-310d-8516-38d58d4f87ca",
                    "sense_definition": "De oorspronkelijke Joodse naam van de apostel Paulus in het Nieuwe Testament.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biblical_figures",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Op weg naar Damascus kreeg <w>Saul</w> een visioen dat zijn leven veranderde.",
                            "target_lang_translations": {
                                "en": "On the way to Damascus, <w>Saul</w> had a vision that changed his life.",
                                "ru": "По дороге в Дамаск <w>Савл</w> увидел видение, которое изменило его жизнь."
                            }
                        },
                        {
                            "text": "Voordat hij bekend werd als Paulus, vervolgde <w>Saul</w> de eerste christenen.",
                            "target_lang_translations": {
                                "en": "Before he became known as Paul, <w>Saul</w> persecuted the early Christians.",
                                "ru": "До того как стать известным как Павел, <w>Савл</w> преследовал первых христиан."
                            }
                        }
                    ],
                    "synonyms": [
                        "Paulus"
                    ],
                    "common_phrases": [
                        "Saul van Tarsus"
                    ],
                    "target_lang_definitions": {
                        "en": "The original Jewish name of the Apostle Paul in the New Testament.",
                        "ru": "Первоначальное еврейское имя апостола Павла в Новом Завете."
                    },
                    "translations": {

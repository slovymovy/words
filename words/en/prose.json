{
    "word_family": [
        "prosaic",
        "prosy",
        "proser"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4efc92fe-228e-36b4-a14e-ab2b221ab35f",
                    "sense_definition": "Language, particularly written language, not intended as poetry.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author is famous for his clear and elegant <w>prose</w>.",
                            "target_lang_translations": {
                                "ru": "Автор известен своей ясной и элегантной <w>прозой</w>."
                            }
                        },
                        {
                            "text": "She prefers reading non-fiction <w>prose</w> rather than poetry.",
                            "target_lang_translations": {
                                "ru": "Она предпочитает читать документальную <w>прозу</w>, а не поэзию."
                            }
                        }
                    ],
                    "antonyms": [
                        "poetry",
                        "verse"
                    ],
                    "common_phrases": [
                        "purple prose",
                        "prose fiction",
                        "write prose",
                        "prose style"
                    ],
                    "target_lang_definitions": {
                        "ru": "Язык, особенно письменный, не предназначенный для поэзии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проза",
                                "target_lang_sense_clarification": "нестихотворная речь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c947b1c5-f119-33e4-af0e-3906dd4cfdb9",
                    "sense_definition": "Language which evinces little imagination or animation; dull and commonplace discourse.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The report was written in dull, academic <w>prose</w> that bored everyone.",
                            "target_lang_translations": {
                                "ru": "Отчет был написан скучной, академической <w>прозой</w>, которая всем наскучила."
                            }
                        },
                        {
                            "text": "His speech was mere <w>prose</w>, lacking any real passion or creativity.",
                            "target_lang_translations": {
                                "ru": "Его речь была сплошной <w>прозой</w>, лишенной настоящей страсти или креативности."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Язык, лишенный воображения или живости; скучная и банальная речь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проза",
                                "target_lang_sense_clarification": "перен. обыденность, скука"
                            },
                            {
                                "target_lang_word": "сухость",
                                "target_lang_sense_clarification": "отсутствие эмоциональности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e4d2835-ba29-3b26-861f-03f36a1e9063",
                    "sense_definition": "A hymn with no regular meter, sometimes introduced into the Mass.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The choir sang a beautiful <w>prose</w> during the service.",
                            "target_lang_translations": {
                                "ru": "Хор пел красивую <w>прозу</w> во время службы."
                            }
                        },
                        {
                            "text": "This ancient <w>prose</w> is traditionally performed at Easter.",
                            "target_lang_translations": {
                                "ru": "Эта древняя <w>проза</w> традиционно исполняется на Пасху."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гимн без регулярного метра, иногда включаемый в мессу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проза",
                                "target_lang_sense_clarification": "церковное песнопение"
                            },
                            {
                                "target_lang_word": "секвенция",
                                "target_lang_sense_clarification": "вид католического песнопения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0df91e54-5c97-3ab2-b29c-100bfadec9f1",
                    "sense_definition": "To write or repeat in a dull, tedious, or prosy way.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tends to <w>prose</w> on about his collection for hours.",
                            "target_lang_translations": {
                                "ru": "Он имеет обыкновение часами <w>скучно рассуждать</w> о своей коллекции."
                            }
                        },
                        {
                            "text": "She sat politely while he <w>prosed</w> about the weather.",
                            "target_lang_translations": {
                                "ru": "Она вежливо сидела, пока он <w>нудно говорил</w> о погоде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prose on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Писать или говорить скучно, утомительно или прозаично."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скучно рассуждать",
                                "target_lang_sense_clarification": "говорить долго и нудно"
                            },
                            {
                                "target_lang_word": "нудить",
                                "target_lang_sense_clarification": "разг. говорить монотонно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "af3bab32-6ea9-3cb3-bd69-34d8e256759e",
                    "sense_definition": "A surname from German.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Prose</w> has lived in this neighborhood for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Проуз</w> живет в этом районе уже двадцать лет."
                            }
                        },
                        {
                            "text": "I was introduced to a woman named Sarah <w>Prose</w>.",
                            "target_lang_translations": {
                                "ru": "Меня представили женщине по имени Сара <w>Проуз</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "from German"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия немецкого происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Проуз",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
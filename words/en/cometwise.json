                            {
                                "target_lang_word": "подобно комете",
                                "target_lang_sense_clarification": "указывает на сходство движения или появления с кометой"
                            },
                            {
                                "target_lang_word": "как комета",
                                "target_lang_sense_clarification": "сравнение действия с движением кометы"
                            }
                        ]
                    }
                }
            ]
        },
        {

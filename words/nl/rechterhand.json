            "pos": "noun",
            "senses": [
                {
                    "sense_id": "62ee5b25-3c5a-3945-8bb8-3bf95f308956",
                    "sense_definition": "De hand die zich aan de rechterkant van het lichaam bevindt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De meeste mensen schrijven met hun <w>rechterhand</w>.",
                            "target_lang_translations": {
                                "en": "Most people write with their <w>right hand</w>.",
                                "ru": "Большинство людей пишут <w>правой рукой</w>."
                            }
                        },
                        {
                            "text": "Hij hield de zware tas stevig vast in zijn <w>rechterhand</w>.",
                            "target_lang_translations": {
                                "en": "He held the heavy bag firmly in his <w>right hand</w>.",
                                "ru": "Он крепко держал тяжелую сумку в <w>правой руке</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "linkerhand"
                    ],
                    "common_phrases": [
                        "aan de rechterhand"
                    ],
                    "target_lang_definitions": {
                        "en": "The hand located on the right side of the body.",
                        "ru": "Рука, расположенная с правой стороны тела."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "right hand",
                                "target_lang_sense_clarification": "body part"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "правая рука",
                                "target_lang_sense_clarification": "часть тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74610971-5bf6-322c-b7f1-462ee091e3bc",
                    "sense_definition": "Een onmisbare assistent of vertrouwenspersoon die iemand ondersteunt bij belangrijke taken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is al jaren de trouwe <w>rechterhand</w> van de directeur.",
                            "target_lang_translations": {
                                "en": "She has been the director's loyal <w>right hand</w> for years.",
                                "ru": "Она уже много лет верная <w>правая рука</w> директора."
                            }
                        },
                        {
                            "text": "Zonder zijn <w>rechterhand</w> zou hij het bedrijf niet kunnen leiden.",
                            "target_lang_translations": {
                                "en": "Without his <w>right hand</w>, he wouldn't be able to lead the company.",
                                "ru": "Без своей <w>правой руки</w> он не смог бы управлять компанией."
                            }
                        }
                    ],
                    "synonyms": [
                        "assistent",
                        "toeverlaat",
                        "helper"
                    ],
                    "common_phrases": [
                        "de rechterhand zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "An indispensable assistant or confidant who supports someone with important tasks.",
                        "ru": "Незаменимый помощник или доверенное лицо, помогающее в важных делах."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "right-hand man",
                                "target_lang_sense_clarification": "assistant"
                            },
                            {
                                "target_lang_word": "right hand",
                                "target_lang_sense_clarification": "assistant"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "правая рука",
                                "target_lang_sense_clarification": "помощник"
                            }
                        ]
                    }
                }
            ]
        }
    ]

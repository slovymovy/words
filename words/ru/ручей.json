    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7546ce00-46b5-3ff7-8fb1-241cd858bf3e",
                    "sense_definition": "Небольшой постоянный или временный водный поток, обычно впадающий в реку или озеро.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "water_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В лесу мы нашли чистый <w>ручей</w> с холодной водой и остановились отдохнуть.",
                            "target_lang_translations": {
                                "en": "In the forest, we found a clean <w>brook</w> with cold water and stopped to rest."
                            }
                        },
                        {
                            "text": "Весной снег тает, и вдоль каждой дороги бежит весёлый <w>ручей</w>.",
                            "target_lang_translations": {
                                "en": "In spring, the snow melts, and a cheerful <w>stream</w> runs along every road."
                            }
                        }
                    ],
                    "synonyms": [
                        "поток",
                        "речушка",
                        "ключ"
                    ],
                    "common_phrases": [
                        "горный ручей",
                        "лесной ручей",
                        "журчащий ручей",
                        "бежит ручей"
                    ],
                    "target_lang_definitions": {
                        "en": "A small natural stream of fresh water, usually flowing into a river or lake."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brook",
                                "target_lang_sense_clarification": "small natural stream"
                            },
                            {
                                "target_lang_word": "stream",
                                "target_lang_sense_clarification": "general flowing water"
                            },
                            {
                                "target_lang_word": "creek",
                                "target_lang_sense_clarification": "small stream, often US"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69c74cf2-55da-30b2-8aa5-494f396c51b5",
                    "sense_definition": "Обильная, непрерывно текущая струя жидкости (обычно слёз, пота или крови).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "water_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От невыносимой жары пот лил с рабочих <w>ручьём</w> во время стройки.",
                            "target_lang_translations": {
                                "en": "Due to the unbearable heat, sweat poured off the workers in a <w>stream</w> during the construction."
                            }
                        },
                        {
                            "text": "Она так сильно расстроилась, что слёзы потекли у неё <w>ручьём</w>.",
                            "target_lang_translations": {
                                "en": "She was so upset that her tears flowed in a <w>stream</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "поток",
                        "струя"
                    ],
                    "antonyms": [
                        "капля",
                        "струйка"
                    ],
                    "common_phrases": [
                        "литься ручьём",
                        "течь ручьём",
                        "в три ручья"
                    ],
                    "target_lang_definitions": {
                        "en": "An abundant, continuously flowing jet or current of liquid (usually tears, sweat, or blood)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stream",
                                "target_lang_sense_clarification": "continuous flow of liquid"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c41cd3b-f3cd-3186-9d21-3239e1ccc691",
                    "sense_definition": "Продольное углубление, канавка или жёлоб на поверхности детали машины или инструмента.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Механик внимательно проверил, как стальной трос ложится в <w>ручей</w> блока.",
                            "target_lang_translations": {
                                "en": "The mechanic carefully checked how the steel cable fit into the <w>groove</w> of the pulley block."
                            }
                        },
                        {
                            "text": "При штамповке металла необходимо следить, чтобы <w>ручей</w> формы был чистым.",
                            "target_lang_translations": {
                                "en": "When stamping metal, it is necessary to ensure that the <w>groove</w> of the mold is clean."
                            }
                        }
                    ],
                    "synonyms": [
                        "жёлоб",
                        "канавка",
                        "паз"
                    ],
                    "target_lang_definitions": {
                        "en": "A longitudinal depression, channel, or gutter on the surface of a machine part or tool."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "groove",
                                "target_lang_sense_clarification": "technical depression or channel"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
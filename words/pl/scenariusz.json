            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aac33487-8274-3f91-b91b-10e6d600340c",
                    "sense_definition": "Materiał literacki będący podstawą fabuły filmu, przedstawienia teatralnego lub programu telewizyjnego, zawierający dialogi i opis akcji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Reżyser przeczytał <w>scenariusz</w> i od razu zdecydował się nakręcić ten film.",
                            "target_lang_translations": {
                                "ru": "Режиссёр прочитал <w>сценарий</w> и сразу решил снять этот фильм."
                            }
                        },
                        {
                            "text": "Aktorzy muszą dokładnie znać <w>scenariusz</w> filmowy przed wejściem na plan.",
                            "target_lang_translations": {
                                "ru": "Актёры должны точно знать <w>сценарий</w> фильма перед выходом на съёмочную площадку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "scenariusz filmowy",
                        "napisać scenariusz",
                        "autor scenariusza",
                        "adaptacja scenariusza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Литературная основа сюжета фильма, спектакля или телепередачи, содержащая диалоги и описание действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сценарий",
                                "target_lang_sense_clarification": "фильма, спектакля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2798c044-06b1-3aee-bf4b-f081026ef6ec",
                    "sense_definition": "Szczegółowy plan lub harmonogram przebiegu jakiegoś wydarzenia, uroczystości lub spotkania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczycielka przygotowała dokładny <w>scenariusz</w> lekcji dla swoich uczniów.",
                            "target_lang_translations": {
                                "ru": "Учительница подготовила подробный <w>сценарий</w> урока для своих учеников."
                            }
                        },
                        {
                            "text": "Cała uroczystość przebiegła dokładnie tak, jak przewidywał to <w>scenariusz</w>.",
                            "target_lang_translations": {
                                "ru": "Всё торжество прошло именно так, как предусматривал <w>сценарий</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "harmonogram",
                        "program",
                        "plan",
                        "agenda"
                    ],
                    "common_phrases": [
                        "scenariusz lekcji",
                        "scenariusz imprezy",
                        "według scenariusza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подробный план или расписание хода какого-либо события, торжества или встречи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сценарий",
                                "target_lang_sense_clarification": "мероприятия"
                            },
                            {
                                "target_lang_word": "план",
                                "target_lang_sense_clarification": "расписание действий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47c3629a-d13e-34f8-89fb-f4053b4cce67",
                    "sense_definition": "Przewidywany lub planowany rozwój przyszłych wydarzeń lub sytuacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ekonomiści biorą pod uwagę czarny <w>scenariusz</w> dotyczący wzrostu inflacji.",
                            "target_lang_translations": {
                                "ru": "Экономисты принимают во внимание чёрный <w>сценарий</w> роста инфляции."
                            }
                        },
                        {
                            "text": "Najbardziej prawdopodobny <w>scenariusz</w> zakłada szybkie zakończenie tego konfliktu.",
                            "target_lang_translations": {
                                "ru": "Наиболее вероятный <w>сценарий</w> предполагает быстрое завершение этого конфликта."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aba47057-364c-3c8f-a8a7-36ced113aea8",
                    "sense_definition": "A machine or device that converts other energy forms into mechanical energy, or imparts motion.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mechanical_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The electric <w>motor</w> powers the fan quietly.",
                            "target_lang_translations": {
                                "ru": "Электрический <w>мотор</w> тихо вращает вентилятор."
                            }
                        },
                        {
                            "text": "He repaired the small <w>motor</w> inside the toy car.",
                            "target_lang_translations": {
                                "ru": "Он починил маленький <w>моторчик</w> внутри игрушечной машинки."
                            }
                        }
                    ],
                    "synonyms": [
                        "engine"
                    ],
                    "common_phrases": [
                        "electric motor",
                        "outboard motor",
                        "starter motor",
                        "motor vehicle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Машина или устройство, которое преобразует другие формы энергии в механическую энергию или создает движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двигатель",
                                "target_lang_sense_clarification": "техническое устройство"
                            },
                            {
                                "target_lang_word": "мотор",
                                "target_lang_sense_clarification": "разговорное или для небольших устройств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28b29c29-9667-3326-8e4c-a004c60aa8cd",
                    "sense_definition": "A motor car or automobile.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He bought a used <w>motor</w> for a very low price.",
                            "target_lang_translations": {
                                "ru": "Он купил подержанную <w>тачку</w> по очень низкой цене."
                            }
                        },
                        {
                            "text": "That is a very nice <w>motor</w> you are driving.",
                            "target_lang_translations": {
                                "ru": "Ты водишь очень классную <w>тачку</w>."
                            }

{
    "word_family": [
        "неистовство",
        "неистовый",
        "неистово"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8519fb96-4bd3-3818-9074-c90e7b34047e",
                    "sense_definition": "Находиться в состоянии крайнего возбуждения, ярости; вести себя буйно и безудержно.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rage_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Разъярённый зверь <w>неистовствовал</w> в клетке, пытаясь вырваться на волю.",
                            "target_lang_translations": {
                                "en": "The enraged beast <w>raged</w> in its cage, trying to break free."
                            }
                        },
                        {
                            "text": "Публика <w>неистовствовала</w> от восторга, вызывая артистов на бис.",
                            "target_lang_translations": {
                                "en": "The audience <w>went wild</w> with delight, calling the artists for an encore."
                            }
                        }
                    ],
                    "synonyms": [
                        "бесноваться",
                        "буйствовать"
                    ],
                    "common_phrases": [
                        "толпа неистовствовала",
                        "публика неистовствовала"
                    ],
                    "target_lang_definitions": {
                        "en": "To be in a state of extreme agitation or fury; to behave violently or uncontrollably."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rage",
                                "target_lang_sense_clarification": "to act with violent anger"
                            },
                            {
                                "target_lang_word": "rampage",
                                "target_lang_sense_clarification": "to move about violently or destructively"
                            },
                            {
                                "target_lang_word": "go wild",
                                "target_lang_sense_clarification": "to be overcome with excitement or enthusiasm"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "084aeeeb-9a45-34e1-91a0-a73070121d27",
                    "sense_definition": "Проявлять бурную, разрушительную силу (о природных стихиях: ветре, море, буре).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rage_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За окнами всю ночь <w>неистовствовала</w> снежная буря.",
                            "target_lang_translations": {
                                "en": "A snowstorm <w>raged</w> outside the windows all night."
                            }
                        },
                        {
                            "text": "Море <w>неистовствовало</w>, обрушивая на берег огромные пенные валы.",
                            "target_lang_translations": {
                                "en": "The sea <w>raged</w>, crashing huge foamy waves onto the shore."
                            }
                        }
                    ],
                    "synonyms": [
                        "буйствовать"
                    ],
                    "common_phrases": [
                        "буря неистовствовала",
                        "ветер неистовствовал"
                    ],
                    "target_lang_definitions": {
                        "en": "To manifest violent, destructive force (regarding natural elements like wind, sea, or storms)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rage",
                                "target_lang_sense_clarification": "of storms, wind, or sea"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bdaafbff-480c-39c5-9bff-bde0942067e2",
                    "sense_definition": "(van wonden) dichtgaan en genezen; weer gezond worden",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "health_recovery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De diepe schaafwond op haar knie <w>heelt</w> gelukkig erg snel.",
                            "target_lang_translations": {
                                "en": "The deep scrape on her knee is fortunately <w>healing</w> very quickly.",
                                "ru": "Глубокая ссадина на её колене, к счастью, очень быстро <w>заживает</w>."
                            }
                        },
                        {
                            "text": "Hij moest enkele weken rust houden om zijn gebroken been te laten <w>helen</w>.",
                            "target_lang_translations": {
                                "en": "He had to rest for a few weeks to let his broken leg <w>heal</w>.",
                                "ru": "Ему пришлось соблюдать покой несколько недель, чтобы дать сломанной ноге <w>зажить</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "genezen",
                        "healen"
                    ],
                    "target_lang_definitions": {
                        "en": "(of wounds) to close up and recover; to become healthy again",
                        "ru": "(о ранах) заживать, затягиваться; выздоравливать"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heal",
                                "target_lang_sense_clarification": "to become healthy or whole again (of wounds or injuries)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заживать",
                                "target_lang_sense_clarification": "о ранах: закрываться и восстанавливаться"
                            },
                            {
                                "target_lang_word": "затягиваться",
                                "target_lang_sense_clarification": "о ране: покрываться новой тканью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f1edc0b-2c5d-374f-94da-3680b824d10e",
                    "sense_definition": "Weer heel of gezond maken; genezen",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "health_recovery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Men zegt vaak dat de tijd alle wonden <w>heelt</w>.",
                            "target_lang_translations": {
                                "en": "It is often said that time <w>heals</w> all wounds.",
                                "ru": "Часто говорят, что время <w>лечит</w> все раны."
                            }
                        },
                        {
                            "text": "De arts deed zijn uiterste best om de zieke patiënt te <w>helen</w>.",
                            "target_lang_translations": {
                                "en": "The doctor did his utmost to <w>heal</w> the sick patient.",
                                "ru": "Врач делал всё возможное, чтобы <w>исцелить</w> больного пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "genezen"
                    ],
                    "common_phrases": [
                        "de tijd heelt alle wonden"
                    ],
                    "target_lang_definitions": {
                        "en": "To make whole or healthy again; to cure",
                        "ru": "Делать снова целым или здоровым; лечить"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heal",
                                "target_lang_sense_clarification": "to treat and cause to recover"
                            },
                            {
                                "target_lang_word": "cure",
                                "target_lang_sense_clarification": "to relieve of symptoms or a disease"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "исцелять",
                                "target_lang_sense_clarification": "полностью излечивать, возвращать здоровье"
                            },
                            {
                                "target_lang_word": "лечить",
                                "target_lang_sense_clarification": "применять медицинские меры для выздоровления"
                            }
                        ]
                    }
                },
                {

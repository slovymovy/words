    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8c7d649e-c292-36f6-8b11-afe6f33f8347",
                    "sense_definition": "The process of being mentally stimulated to do or feel something, especially to do something creative.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creative_stimulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nature is often a great source of <w>inspiration</w> for artists and writers."
                        },
                        {
                            "text": "She looked for <w>inspiration</w> in the works of the old masters."
                        }
                    ],
                    "synonyms": [
                        "motivation",
                        "spark"
                    ],
                    "common_phrases": [
                        "source of inspiration",
                        "find inspiration",
                        "look for inspiration"
                    ]
                },
                {
                    "sense_id": "417201bc-ff21-3920-b0be-247bdcb2eedb",
                    "sense_definition": "A person, object, or situation that motivates someone to create something or to be better.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "creative_stimulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His courage and determination are an <w>inspiration</w> to us all."
                        },
                        {
                            "text": "The beautiful landscape was the main <w>inspiration</w> for this painting."
                        }
                    ],
                    "synonyms": [
                        "motivation"
                    ],
                    "common_phrases": [
                        "an inspiration to",
                        "be an inspiration"
                    ]
                },
                {
                    "sense_id": "23d77321-b3da-356e-aaf1-55791ecc22d2",
                    "sense_definition": "A sudden good idea, especially one that solves a problem or is creative.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "creative_stimulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He suddenly had an <w>inspiration</w> about how to fix the broken engine."
                        },
                        {
                            "text": "The solution came to her in a sudden flash of <w>inspiration</w>."
                        }
                    ],
                    "synonyms": [
                        "flash",
                        "eureka"
                    ],
                    "common_phrases": [
                        "sudden inspiration",
                        "have an inspiration",
                        "flash of inspiration"
                    ]
                },
                {

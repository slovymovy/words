    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "ea1c4846-d08e-339f-9cc8-8c229d2fae62",
                    "sense_definition": "Following in time; later than.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_seq",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We can go to the park <w>after</w> lunch.",
                            "target_lang_translations": {
                                "ru": "Мы можем пойти в парк <w>после</w> обеда."
                            }
                        },
                        {
                            "text": "The time is now ten minutes <w>after</w> five.",
                            "target_lang_translations": {
                                "ru": "Сейчас десять минут <w>после</w> пяти."
                            }
                        },
                        {
                            "text": "She felt much better <w>after</w> a good night's sleep.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала себя намного лучше <w>после</w> хорошего ночного сна."
                            }
                        }
                    ],
                    "antonyms": [
                        "before"
                    ],
                    "common_phrases": [
                        "after school",
                        "after work",
                        "day after tomorrow",
                        "shortly after"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следующий во времени; позже, чем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "после",
                                "target_lang_sense_clarification": "предлог времени"
                            },
                            {
                                "target_lang_word": "спустя",
                                "target_lang_sense_clarification": "по прошествии времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3b0e534-c5a6-318f-aafb-7722ad46ca02",
                    "sense_definition": "Behind someone or something in place or order.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "place_seq",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please close the door <w>after</w> you when you leave.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, закрой <w>за</w> собой дверь, когда будешь уходить."
                            }
                        },
                        {
                            "text": "The letter B comes <w>after</w> the letter A in the alphabet.",
                            "target_lang_translations": {
                                "ru": "Буква Б идет <w>после</w> буквы А в алфавите."
                            }
                        }
                    ],
                    "synonyms": [
                        "post"
                    ],
                    "antonyms": [
                        "before"
                    ],
                    "common_phrases": [
                        "one after another"
                    ],
                    "target_lang_definitions": {
                        "ru": "Позади кого-то или чего-то в пространстве или по порядку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "позади кого-то"
                            },
                            {
                                "target_lang_word": "после",
                                "target_lang_sense_clarification": "следующий по порядку"
                            },
                            {
                                "target_lang_word": "вслед за",
                                "target_lang_sense_clarification": "двигаясь следом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cd7242b-06b2-333f-8e01-b2b4954682f7",
                    "sense_definition": "In honor of; using the name of someone else.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "naming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They named the baby <w>after</w> his grandfather.",
                            "target_lang_translations": {
                                "ru": "Они назвали ребенка <w>в честь</w> его дедушки."
                            }
                        },
                        {
                            "text": "The city was named <w>after</w> a famous explorer.",
                            "target_lang_translations": {
                                "ru": "Город был назван <w>в честь</w> известного исследователя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "named after",
                        "called after"
                    ],
                    "target_lang_definitions": {
                        "ru": "В честь; используя имя кого-то другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в честь",
                                "target_lang_sense_clarification": "названный именем кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa05242f-2aee-3e30-b0b9-09b8b3cf56b3",
                    "sense_definition": "In pursuit of; trying to find or catch.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "pursuit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police officer ran <w>after</w> the thief.",
                            "target_lang_translations": {
                                "ru": "Полицейский побежал <w>за</w> вором."
                            }
                        },
                        {
                            "text": "He is <w>after</w> a new job in the marketing department.",
                            "target_lang_translations": {
                                "ru": "Он <w>в поисках</w> новой работы в отделе маркетинга."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go after",
                        "run after",
                        "be after"
                    ],
                    "target_lang_definitions": {
                        "ru": "В погоне за; пытаясь найти или поймать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "преследуя кого-то"
                            },
                            {
                                "target_lang_word": "в поисках",
                                "target_lang_sense_clarification": "пытаясь найти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31bfb36a-4409-3be7-b044-c32b93978de3",
                    "sense_definition": "Repeatedly; one following another in a continuous series.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "repetition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They walked for mile <w>after</w> mile without seeing anyone.",
                            "target_lang_translations": {
                                "ru": "Они шли милю <w>за</w> милей, не видя никого."
                            }
                        },
                        {
                            "text": "She solved problem <w>after</w> problem until the work was done.",
                            "target_lang_translations": {
                                "ru": "Она решала задачу <w>за</w> задачей, пока работа не была сделана."
                            }
                        }
                    ],
                    "common_phrases": [
                        "day after day",
                        "time after time",
                        "year after year"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повторяясь; один за другим в непрерывной последовательности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "в конструкциях типа «день за днём»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56d6b11a-d283-3e7b-bcbc-f5668d20ed29",
                    "sense_definition": "In spite of; considering what has happened or been done.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "concession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>After</w> all his hard work, he still failed the exam.",
                            "target_lang_translations": {
                                "ru": "<w>После</w> всей его тяжелой работы он все же провалил экзамен."
                            }
                        },
                        {
                            "text": "I can't believe you are still hungry <w>after</w> that huge meal.",
                            "target_lang_translations": {
                                "ru": "Не могу поверить, что ты все еще голоден <w>после</w> такой огромной порции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "after all"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несмотря на; учитывая то, что произошло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "после",
                                "target_lang_sense_clarification": "несмотря на произошедшее"
                            },
                            {
                                "target_lang_word": "несмотря на",
                                "target_lang_sense_clarification": "вопреки обстоятельствам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b149ce19-40ed-3e0a-b5fb-0de4473d9193",
                    "sense_definition": "Used to indicate the recent completion of an activity (used with 'be' and a verb ending in -ing).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "dialect_perfect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>after</w> finishing my dinner, so I am quite full.",
                            "target_lang_translations": {
                                "ru": "Я <w>только что</w> закончил ужинать, так что я вполне сыт."
                            }
                        },
                        {
                            "text": "She was <w>after</w> walking all the way from the village.",
                            "target_lang_translations": {
                                "ru": "Она <w>только что</w> пришла пешком из самой деревни."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Irish English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на недавнее завершение действия (диалектная форма)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "только что",
                                "target_lang_sense_clarification": "указание на недавнее прошлое"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "3b712aef-a2b9-388e-b0d6-bdde334d4446",
                    "sense_definition": "At a time later than the time when; following the event that.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "conj_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Call me <w>after</w> you arrive at the station.",
                            "target_lang_translations": {
                                "ru": "Позвони мне <w>после того, как</w> приедешь на станцию."
                            }
                        },
                        {
                            "text": "He went to bed <w>after</w> he finished his homework.",
                            "target_lang_translations": {
                                "ru": "Он пошел спать <w>после того, как</w> закончил домашнюю работу."
                            }
                        }
                    ],
                    "antonyms": [
                        "before"
                    ],
                    "target_lang_definitions": {
                        "ru": "В то время, которое наступило позже другого события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "после того как",
                                "target_lang_sense_clarification": "союз времени"
                            },
                            {
                                "target_lang_word": "когда",
                                "target_lang_sense_clarification": "в значении «после того как»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a6b66ebf-4cfc-3706-a4d4-adaf2185b7ba",
                    "sense_definition": "At a later time; following something else.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "adv_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They lived happily ever <w>after</w>.",
                            "target_lang_translations": {
                                "ru": "Они жили долго и счастливо <w>после</w> этого."
                            }
                        },
                        {
                            "text": "I left the room, and the dog followed <w>after</w>.",
                            "target_lang_translations": {
                                "ru": "Я вышел из комнаты, и собака последовала <w>за</w> мной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ever after"
                    ],
                    "target_lang_definitions": {
                        "ru": "В более позднее время; следуя за чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потом",
                                "target_lang_sense_clarification": "наречие времени"
                            },
                            {
                                "target_lang_word": "позже",
                                "target_lang_sense_clarification": "спустя время"
                            },
                            {
                                "target_lang_word": "следом",
                                "target_lang_sense_clarification": "двигаясь позади"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b068e173-48a4-3e3e-ba3b-76bccb5cea7b",
                    "sense_definition": "Later; subsequent; following in time.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "adj_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In <w>after</w> years, he would regret his decision.",
                            "target_lang_translations": {
                                "ru": "В <w>последующие</w> годы он пожалеет о своем решении."
                            }
                        },
                        {
                            "text": "The <w>after</w> experience convinced me that I was wrong.",
                            "target_lang_translations": {
                                "ru": "<w>Полученный впоследствии</w> опыт убедил меня, что я был неправ."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often used in literary contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Более поздний; последующий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последующий",
                                "target_lang_sense_clarification": "происходящий позже"
                            },
                            {
                                "target_lang_word": "будущий",
                                "target_lang_sense_clarification": "относящийся к будущему"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc16c451-beb8-3ad5-839a-8d432c17b5f4",
                    "sense_definition": "Located at or towards the rear of a ship or aircraft.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adj_nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crew slept in the <w>after</w> cabin.",
                            "target_lang_translations": {
                                "ru": "Экипаж спал в <w>кормовой</w> каюте."
                            }
                        },
                        {
                            "text": "They loaded the cargo into the <w>after</w> hatch.",
                            "target_lang_translations": {
                                "ru": "Они погрузили груз в <w>кормовой</w> люк."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположенный на корме или ближе к корме судна или самолета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кормовой",
                                "target_lang_sense_clarification": "морской термин"
                            },
                            {
                                "target_lang_word": "задний",
                                "target_lang_sense_clarification": "расположенный сзади"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8c9ebf48-1398-3c1e-bcd5-b140593c1b04",
                    "sense_definition": "An image showing the condition or appearance following a change or treatment.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_image",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>afters</w> show a dramatic improvement in the kitchen renovation.",
                            "target_lang_translations": {
                                "ru": "<w>Снимки «после»</w> показывают значительное улучшение в ремонте кухни."
                            }
                        },
                        {
                            "text": "You should compare the befores and <w>afters</w> to see the difference.",
                            "target_lang_translations": {
                                "ru": "Тебе стоит сравнить «до» и <w>«после»</w>, чтобы увидеть разницу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Изображение, показывающее состояние или внешний вид после изменений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снимок «после»",
                                "target_lang_sense_clarification": "результат изменений"
                            },
                            {
                                "target_lang_word": "результат",
                                "target_lang_sense_clarification": "итог работы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "overdracht",
        "overdraagbaar",
        "overdrager"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "727aed50-6f0e-364d-ade8-275abf21f82e",
                    "sense_definition": "Iets, zoals een taak, bezit, macht of verantwoordelijkheid, officieel aan iemand anders geven.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transfer_ownership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verantwoordelijkheid werd gisteren aan de nieuwe manager <w>overgedragen</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера ответственность была <w>передана</w> новому менеджеру.",
                                "en": "The responsibility was <w>transferred</w> to the new manager yesterday."
                            }
                        },
                        {
                            "text": "Hij gaat zijn taken volgende week aan zijn collega <w>overdragen</w>.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе он собирается <w>передать</w> свои задачи коллеге.",
                                "en": "He is going to <w>hand over</w> his tasks to his colleague next week."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorgeven",
                        "afstaan",
                        "toevertrouwen"
                    ],
                    "antonyms": [
                        "aannemen",
                        "ontvangen",
                        "behouden"
                    ],
                    "common_phrases": [
                        "taken overdragen",
                        "kennis overdragen",
                        "het stokje overdragen",
                        "macht overdragen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально передавать что-либо, например задачу, владение, власть или ответственность, другому лицу.",
                        "en": "To officially give something, such as a task, possession, power, or responsibility, to someone else."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "полномочия, права, обязанности"
                            },
                            {
                                "target_lang_word": "перепоручать",
                                "target_lang_sense_clarification": "задачи, дела"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "transfer",
                                "target_lang_sense_clarification": "ownership, power, responsibility"
                            },
                            {
                                "target_lang_word": "hand over",
                                "target_lang_sense_clarification": "tasks, duties"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3d802b7-5e84-3cce-b9a1-d41a7050fc16",
                    "sense_definition": "Een ziekte of infectie op iemand anders of een dier doorgeven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transmit_disease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Teken kunnen de ziekte van Lyme op mensen <w>overdragen</w>.",
                            "target_lang_translations": {
                                "ru": "Клещи могут <w>передавать</w> болезнь Лайма людям.",
                                "en": "Ticks can <w>transmit</w> Lyme disease to humans."
                            }
                        },
                        {
                            "text": "Het is belangrijk om te weten hoe een virus wordt <w>overgedragen</w>.",
                            "target_lang_translations": {
                                "ru": "Важно знать, как <w>передается</w> вирус.",
                                "en": "It is important to know how a virus is <w>transmitted</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorgeven",
                        "verspreiden"
                    ],
                    "common_phrases": [
                        "een virus overdragen",
                        "een ziekte overdragen",
                        "seksueel overdraagbaar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать болезнь или инфекцию другому человеку или животному.",
                        "en": "To pass a disease or infection to another person or animal."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "инфекцию, вирус"
                            },
                            {
                                "target_lang_word": "переносить",
                                "target_lang_sense_clarification": "болезнь (о переносчиках)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "transmit",
                                "target_lang_sense_clarification": "diseases, viruses"
                            },
                            {
                                "target_lang_word": "pass on",
                                "target_lang_sense_clarification": "infections"
                            }
                        ]
                    }
                }
            ]
        }
    ]

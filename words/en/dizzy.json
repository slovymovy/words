    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "94734891-e8f5-3e19-8f98-2d55c3613996",
                    "sense_definition": "Feeling as if everything is spinning around you, often causing a loss of balance.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I stood up too fast and felt <w>dizzy</w>.",
                            "target_lang_translations": {
                                "ru": "Я встал слишком быстро, и у меня <w>закружилась голова</w>."
                            }
                        },
                        {
                            "text": "The heat in the crowded room made me feel a bit <w>dizzy</w>.",
                            "target_lang_translations": {
                                "ru": "От жары в переполненной комнате у меня немного <w>закружилась голова</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dizzying",
                        "dizzying"
                    ],
                    "common_phrases": [
                        "feel dizzy",
                        "get dizzy",
                        "dizzy spell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий, будто всё вокруг вращается, часто с потерей равновесия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствующий головокружение",
                                "target_lang_sense_clarification": "физическое состояние"
                            },
                            {
                                "target_lang_word": "испытывающий головокружение",
                                "target_lang_sense_clarification": "формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "806f8d78-fc03-3783-ad07-68dbd6a41b9d",
                    "sense_definition": "Causing a feeling of giddiness or confusion, often due to great height or speed.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We looked down from the <w>dizzy</w> heights of the mountain.",
                            "target_lang_translations": {
                                "ru": "Мы смотрели вниз с <w>головокружительной</w> высоты горы."
                            }
                        },
                        {
                            "text": "The technology is evolving at a <w>dizzy</w> speed.",
                            "target_lang_translations": {
                                "ru": "Технологии развиваются с <w>головокружительной</w> скоростью."
                            }
                        }
                    ],
                    "synonyms": [
                        "dizzying"
                    ],
                    "common_phrases": [
                        "dizzy heights",
                        "dizzy speed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий чувство головокружения или замешательства, часто из-за большой высоты или скорости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "головокружительный",
                                "target_lang_sense_clarification": "о высоте, скорости или успехе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "135bc3e5-95a0-310c-ac3c-876d95c9c7cb",
                    "sense_definition": "Silly, forgetful, or not serious.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She can be a bit <w>dizzy</w> sometimes, but she is actually very smart.",
                            "target_lang_translations": {
                                "ru": "Иногда она бывает немного <w>легкомысленной</w>, но на самом деле она очень умна."
                            }
                        },
                        {
                            "text": "He acts <w>dizzy</w> to make people laugh, but he knows exactly what he is doing.",
                            "target_lang_translations": {
                                "ru": "Он притворяется <w>рассеянным</w>, чтобы рассмешить людей, но точно знает, что делает."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Глуповатый, забывчивый или несерьезный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "легкомысленный",
                                "target_lang_sense_clarification": "о характере"
                            },
                            {
                                "target_lang_word": "рассеянный",
                                "target_lang_sense_clarification": "забывчивый"
                            },
                            {
                                "target_lang_word": "ветреный",
                                "target_lang_sense_clarification": "несерьезный"
                            }
                        ]
                    }
                }
            ]
        },
        {

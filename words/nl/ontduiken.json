{
    "word_family": [
        "ontduiking",
        "ontduiker",
        "duiken"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1b1fa02c-940b-331f-a22f-330d5821b47e",
                    "sense_definition": "Zich opzettelijk onttrekken aan een verplichting, regel of wet.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "evade_obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf probeerde jarenlang belasting te <w>ontduiken</w> door geld naar het buitenland te sluizen.",
                            "target_lang_translations": {
                                "ru": "Компания годами пыталась <w>уклоняться</w> от уплаты налогов, выводя деньги за границу.",
                                "en": "The company tried to <w>evade</w> taxes for years by funneling money abroad."
                            }
                        },
                        {
                            "text": "Je kunt de strenge regels niet blijven <w>ontduiken</w> zonder uiteindelijk in de problemen te komen.",
                            "target_lang_translations": {
                                "ru": "Нельзя вечно <w>обходить</w> строгие правила, не попав в конечном итоге в беду.",
                                "en": "You cannot keep <w>evading</w> the strict rules without eventually getting into trouble."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontlopen"
                    ],
                    "common_phrases": [
                        "belasting ontduiken",
                        "de wet ontduiken",
                        "regels ontduiken",
                        "plichten ontduiken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеренно уклоняться от выполнения обязательства, правила или закона.",
                        "en": "To intentionally withdraw from an obligation, rule, or law."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уклоняться",
                                "target_lang_sense_clarification": "от налогов, обязанностей"
                            },
                            {
                                "target_lang_word": "обходить",
                                "target_lang_sense_clarification": "закон, правила"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "evade",
                                "target_lang_sense_clarification": "taxes, duty, law"
                            },
                            {
                                "target_lang_word": "dodge",
                                "target_lang_sense_clarification": "fare, draft, payment"
                            },
                            {
                                "target_lang_word": "circumvent",
                                "target_lang_sense_clarification": "rules, regulations"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d177de16-43a7-3ec2-b726-7c76c1f2e34d",
                    "sense_definition": "Iets vervelends, gevaarlijks of een directe vraag slim weten te ontwijken.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "evade_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politicus wist de lastige vraag van de journalist handig te <w>ontduiken</w>.",
                            "target_lang_translations": {
                                "ru": "Политик сумел ловко <w>уйти</w> от каверзного вопроса журналиста.",
                                "en": "The politician managed to cleverly <w>dodge</w> the journalist's tricky question."
                            }
                        },
                        {
                            "text": "Hij kon de vliegende bal nog net op tijd <w>ontduiken</w> door snel te bukken.",
                            "target_lang_translations": {
                                "ru": "Он смог вовремя <w>увернуться</w> от летящего мяча, быстро пригнувшись.",
                                "en": "He was able to <w>dodge</w> the flying ball just in time by ducking quickly."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontwijken",
                        "ontkomen"
                    ],
                    "common_phrases": [
                        "een vraag ontduiken",
                        "gevaar ontduiken",
                        "een klap ontduiken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ловко избегать чего-то неприятного, опасного или прямого вопроса.",
                        "en": "To cleverly avoid something unpleasant, dangerous, or a direct question."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "от ответа, вопроса"
                            },
                            {
                                "target_lang_word": "уворачиваться",
                                "target_lang_sense_clarification": "физически, от удара"
                            },
                            {
                                "target_lang_word": "избегать",
                                "target_lang_sense_clarification": "опасности, столкновения"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "dodge",
                                "target_lang_sense_clarification": "a blow, a question"
                            },
                            {
                                "target_lang_word": "evade",
                                "target_lang_sense_clarification": "pursuers, questions"
                            },
                            {
                                "target_lang_word": "sidestep",
                                "target_lang_sense_clarification": "an issue, a question"
                            }
                        ]
                    }
                }
            ]
        }
    ]

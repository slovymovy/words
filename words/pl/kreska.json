                {
                    "sense_id": "a4c5ddab-ccb3-348e-a352-214a42931167",
                    "sense_definition": "Cienka, prosta linia narysowana na jakiejś powierzchni.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "graphic_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko narysowało na kartce dwie proste <kreski>.",
                            "target_lang_translations": {
                                "ru": "Ребёнок нарисовал на листе бумаги две прямые <w>линии</w>."
                            }
                        },
                        {
                            "text": "Oddzieliłem temat od reszty tekstu grubą <kreską>.",
                            "target_lang_translations": {
                                "ru": "Я отделил тему от остального текста жирной <w>чертой</w>."
                            }
                        }

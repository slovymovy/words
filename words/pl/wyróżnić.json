            "senses": [
                {
                    "sense_id": "d5927919-0fe4-30ba-8fdd-d2d7df5946a9",
                    "sense_definition": "Zwrócić na kogoś lub na coś szczególną uwagę, uznając za lepsze lub ważniejsze od innych; często wiąże się z przyznaniem nagrody lub pochwały.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "award_distinguish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor postanowił <wyróżnić> pracownika premią za jego zaangażowanie.",
                            "target_lang_translations": {
                                "ru": "Директор решил <w>отметить</w> сотрудника премией за его усердие."
                            }
                        },
                        {
                            "text": "Jury chciało szczególnie <wyróżnić> ten obraz spośród wszystkich zgłoszonych prac.",
                            "target_lang_translations": {
                                "ru": "Жюри хотело особо <w>выделить</w> эту картину среди всех представленных работ."
                            }
                        }
                    ],
                    "synonyms": [
                        "nagrodzić",
                        "uhonorować",
                        "odznaczyć",
                        "wyodrębnić"
                    ],
                    "antonyms": [
                        "pominąć",
                        "zignorować",
                        "zlekceważyć"
                    ],
                    "common_phrases": [
                        "wyróżnić nagrodą",
                        "wyróżnić za zasługi",
                        "wyróżnić z tłumu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обратить особое внимание на кого-то или что-то, признав лучшим или более важным; часто связано с награждением или похвалой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отметить",
                                "target_lang_sense_clarification": "наградой или похвалой"
                            },
                            {
                                "target_lang_word": "выделить",
                                "target_lang_sense_clarification": "из группы, подчеркнуть значимость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83b25681-c45d-38d5-81fc-c3c1301119ee",
                    "sense_definition": "Stać się widocznym, zauważalnym lub rozpoznawalnym na tle innych dzięki specyficznym cechom, zachowaniu lub osiągnięciom (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stand_out",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz mieć oryginalny pomysł, aby <wyróżnić> się na rynku pracy.",
                            "target_lang_translations": {
                                "ru": "Тебе нужна оригинальная идея, чтобы <w>выделиться</w> на рынке труда."
                            }
                        },
                        {
                            "text": "Jej niezwykły talent pozwolił jej <wyróżnić> się w grupie rówieśników.",
                            "target_lang_translations": {
                                "ru": "Её необычайный талант позволил ей <w>выделиться</w> в группе сверстников."
                            }
                        }
                    ],
                    "synonyms": [
                        "odznaczyć się",
                        "wybić się",
                        "zabłysnąć"
                    ],
                    "antonyms": [
                        "wtopić się",
                        "zlać się"
                    ],
                    "common_phrases": [
                        "wyróżnić się wiedzą",
                        "wyróżnić się strojem",
                        "wyróżnić się na tle innych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать заметным, узнаваемым на фоне других благодаря особым качествам или достижениям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выделиться",
                                "target_lang_sense_clarification": "стать заметным на фоне других"
                            },
                            {
                                "target_lang_word": "отличиться",
                                "target_lang_sense_clarification": "своими действиями или качествами"
                            }
                        ]
                    }
                }
            ]
        }
    ]

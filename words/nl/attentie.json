                {
                    "sense_id": "af3cbc44-142a-3b53-af1a-db240702b29d",
                    "sense_definition": "Het bewust richten van de gedachten of zintuigen op iets; aandacht.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik even uw <w>attentie</w> voor een belangrijke mededeling?",
                            "target_lang_translations": {
                                "en": "May I have your <w>attention</w> for an important announcement?",
                                "ru": "Могу я попросить вашего <w>внимания</w> для важного объявления?"
                            }
                        },
                        {
                            "text": "Deze brief is ter <w>attentie</w> van de afdeling klantenservice.",
                            "target_lang_translations": {
                                "en": "This letter is for the <w>attention</w> of the customer service department.",
                                "ru": "Это письмо предназначено <w>вниманию</w> отдела обслуживания клиентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "aandacht"
                    ],
                    "common_phrases": [
                        "ter attentie van",
                        "attentie vragen"
                    ],
                    "target_lang_definitions": {
                        "en": "The conscious focusing of thoughts or senses on something; attention.",
                        "ru": "Сознательное направление мыслей или чувств на что-либо; внимание."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attention",
                                "target_lang_sense_clarification": "mental focus"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "внимание",
                                "target_lang_sense_clarification": "сосредоточенность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4eb7e6d-e497-33c7-b200-92f0733eae28",
                    "sense_definition": "Een klein cadeau of gebaar om iemand te bedanken of te feliciteren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als dank voor het oppassen gaven we de buurvrouw een kleine <w>attentie</w>.",
                            "target_lang_translations": {
                                "en": "As thanks for babysitting, we gave the neighbor a small <w>token of appreciation</w>.",
                                "ru": "В благодарность за присмотр мы подарили соседке небольшой <w>знак внимания</w>."
                            }
                        },
                        {
                            "text": "Het bedrijf geeft elk jaar een leuke <w>attentie</w> aan het personeel met kerst.",
                            "target_lang_translations": {
                                "en": "The company gives the staff a nice <w>small gift</w> every year at Christmas.",
                                "ru": "Компания каждый год дарит сотрудникам приятный <w>подарок</w> на Рождество."
                            }
                        }
                    ],
                    "synonyms": [
                        "aardigheidje",
                        "presentje",
                        "geschenkje"
                    ],
                    "common_phrases": [
                        "een kleine attentie"
                    ],
                    "target_lang_definitions": {
                        "en": "A small gift or gesture to thank or congratulate someone.",
                        "ru": "Небольшой подарок или жест, чтобы поблагодарить или поздравить кого-то."

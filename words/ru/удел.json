    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0ad4522e-e860-38f2-9ad0-77469a4cb1e4",
                    "sense_definition": "Судьба, участь, жизненные обстоятельства, которые суждено пережить кому-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "destiny",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <w>удел</w> — помогать людям в трудных ситуациях.",
                            "target_lang_translations": {
                                "en": "His <w>lot</w> is to help people in difficult situations."
                            }
                        },
                        {
                            "text": "Нам достался нелёгкий <w>удел</w>, но мы справимся.",
                            "target_lang_translations": {
                                "en": "A difficult <w>lot</w> has fallen to us, but we will cope."
                            }
                        },
                        {
                            "text": "Одиночество — это часто <w>удел</w> гениев.",
                            "target_lang_translations": {
                                "en": "Loneliness is often the <w>lot</w> of geniuses."
                            }
                        }
                    ],
                    "synonyms": [
                        "судьба",
                        "участь",
                        "доля",
                        "жребий"
                    ],
                    "common_phrases": [
                        "достаться в удел",
                        "печальный удел",
                        "счастливый удел",
                        "горький удел"
                    ],
                    "target_lang_definitions": {
                        "en": "Fate, destiny, or the specific lot in life that one is destined to experience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lot",
                                "target_lang_sense_clarification": "one's situation in life or destiny"
                            },
                            {
                                "target_lang_word": "destiny",
                                "target_lang_sense_clarification": "predetermined course of events"
                            },
                            {
                                "target_lang_word": "fate",
                                "target_lang_sense_clarification": "inevitable outcome"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "406914cc-b5a9-35a7-87c9-6d81d6c06f14",
                    "sense_definition": "В Древней Руси: область или территория, которой управлял князь на правах феодального владения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "territory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый князь стремился расширить и укрепить свой <w>удел</w>.",
                            "target_lang_translations": {
                                "en": "Every prince sought to expand and strengthen his <w>appanage</w>."
                            }
                        },
                        {
                            "text": "Москва когда-то была лишь небольшим <w>уделом</w>.",
                            "target_lang_translations": {
                                "en": "Moscow was once merely a small <w>appanage</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "судьба",
                        "доля"
                    ],
                    "common_phrases": [
                        "княжеский удел",
                        "система уделов"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in historical context of Russia"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "historical term"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "In Ancient Rus: a specific territory or district ruled by a prince as a feudal possession."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appanage",
                                "target_lang_sense_clarification": "historical term for a grant of land or revenue to a royal family member"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "846e9841-6a59-3c61-90a4-0379a0bc8951",
                    "sense_definition": "Часть, доля чего-либо, выделенная кому-либо (обычно земли или наследства).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "share",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Младший сын получил свой <w>удел</w> наследства деньгами.",
                            "target_lang_translations": {
                                "en": "The younger son received his <w>portion</w> of the inheritance in money."
                            }
                        },
                        {
                            "text": "Каждому племени был назначен свой <w>удел</w> земли.",
                            "target_lang_translations": {
                                "en": "Each tribe was assigned its own <w>portion</w> of land."
                            }
                        }
                    ],
                    "synonyms": [
                        "доля",
                        "судьба"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A part or share of something allocated to someone (usually land or inheritance)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "portion",
                                "target_lang_sense_clarification": "an allocated share of inheritance or goods"
                            },
                            {
                                "target_lang_word": "share",
                                "target_lang_sense_clarification": "a part distributed to someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2d29005-5211-3fb2-a0af-1c2389712b9a",
                    "sense_definition": "Земельное имущество, принадлежавшее императорской семье в дореволюционной России.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "territory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Доходы от <w>уделов</w> шли на содержание императорского двора.",
                            "target_lang_translations": {
                                "en": "Revenues from the <w>appanages</w> went to the maintenance of the imperial court."
                            }
                        },
                        {
                            "text": "Министерство <w>уделов</w> управляло огромными территориями.",
                            "target_lang_translations": {
                                "en": "The Ministry of <w>Appanages</w> managed vast territories."
                            }
                        }
                    ],
                    "common_phrases": [
                        "департамент уделов"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical term"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Landed property belonging to the imperial family in pre-revolutionary Russia."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appanage",
                                "target_lang_sense_clarification": "specific to imperial estates and the ministry managing them"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "18251e1d-a6a3-3c59-8bd9-e69e26e1939d",
                    "sense_definition": "Человек очень высокого роста.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "huge_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В дверях появился <w>гигант</w> под два метра ростом.",
                            "target_lang_translations": {
                                "en": "A <w>giant</w> nearly two meters tall appeared in the doorway."
                            }
                        },
                        {
                            "text": "Этот добродушный <w>гигант</w> помог нам поднять тяжелую коробку.",
                            "target_lang_translations": {
                                "en": "This good-natured <w>giant</w> helped us lift the heavy box."
                            }
                        }
                    ],
                    "synonyms": [
                        "велика́н",
                        "исполи́н"
                    ],
                    "antonyms": [
                        "ка́рлик",
                        "лилипу́т"
                    ],
                    "common_phrases": [
                        "настоящий гигант",
                        "добрый гигант"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person of very tall stature."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "giant",
                                "target_lang_sense_clarification": "person of great height"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b557261-e2a1-31e6-98c7-7022b89f1851",
                    "sense_definition": "Объект, имеющий чрезвычайно большие размеры и значительно превосходящий другие подобные объекты.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "huge_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новый завод — это настоящий промышленный <w>гигант</w>.",
                            "target_lang_translations": {
                                "en": "The new factory is a real industrial <w>giant</w>."
                            }
                        },
                        {
                            "text": "В порту стоял морской <w>гигант</w>, готовый к отплытию.",
                            "target_lang_translations": {
                                "en": "A maritime <w>giant</w> stood in the port, ready for departure."
                            }
                        }
                    ],
                    "synonyms": [
                        "громада",
                        "коло́сс",
                        "исполи́н"
                    ],
                    "antonyms": [
                        "ка́рлик"
                    ],
                    "common_phrases": [
                        "промышленный гигант",
                        "газовый гигант"
                    ],
                    "target_lang_definitions": {
                        "en": "An object of extremely large size that significantly surpasses other similar objects."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "giant",
                                "target_lang_sense_clarification": "huge entity or organization"
                            },
                            {
                                "target_lang_word": "colossus",
                                "target_lang_sense_clarification": "huge object or structure"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d88dd451-cba4-3638-9221-5f722319753e",
                    "sense_definition": "Человек, выдающийся в какой-либо области, превосходящий остальных своими способностями и достижениями.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "huge_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лев Толстой — признанный литературный <w>гигант</w> своего времени.",
                            "target_lang_translations": {
                                "en": "Leo Tolstoy is a recognized literary <w>giant</w> of his time."
                            }
                        },
                        {
                            "text": "Мы должны помнить, что являемся наследниками <w>гигантов</w> науки.",
                            "target_lang_translations": {
                                "en": "We must remember that we are the heirs of the <w>giants</w> of science."
                            }
                        }
                    ],
                    "synonyms": [
                        "титан",
                        "корифей"
                    ],
                    "antonyms": [
                        "пигмей"
                    ],
                    "common_phrases": [
                        "гигант мысли",
                        "научный гигант"
                    ],
                    "target_lang_definitions": {
                        "en": "A person outstanding in a particular field, surpassing others in abilities and achievements."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "giant",
                                "target_lang_sense_clarification": "intellectual or professional"
                            },
                            {
                                "target_lang_word": "titan",
                                "target_lang_sense_clarification": "person of great achievement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d10ad06c-4f2d-3572-81ab-6693b18105d2",
                    "sense_definition": "В древнегреческой мифологии: сверхъестественное существо огромного размера и силы, вступившее в борьбу с богами.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "huge_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Древние мифы рассказывают, как <w>гигант</w> пытался разрушить гору.",
                            "target_lang_translations": {
                                "en": "Ancient myths tell how a <w>giant</w> tried to destroy the mountain."
                            }
                        },
                        {
                            "text": "Зевс победил всех <w>гигантов</w> и заточил их под землёй.",
                            "target_lang_translations": {
                                "en": "Zeus defeated all the <w>giants</w> and imprisoned them underground."
                            }
                        }
                    ],
                    "synonyms": [
                        "титан",
                        "исполин"
                    ],
                    "common_phrases": [
                        "битва гигантов"
                    ],
                    "target_lang_definitions": {
                        "en": "In ancient Greek mythology: a supernatural being of huge size and strength that fought against the gods."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "giant",
                                "target_lang_sense_clarification": "mythological creature"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d873f85-b2b5-3ae0-8b7d-afa0b1b5eac1",
                    "sense_definition": "Граммофонная пластинка самого большого размера.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В коллекции отца нашёлся редкий диск-<w>гигант</w>.",
                            "target_lang_translations": {
                                "en": "A rare <w>LP</w> was found in my father's collection."
                            }
                        },
                        {
                            "text": "Мы слушали старый <w>гигант</w> с записями классической музыки.",
                            "target_lang_translations": {
                                "en": "We listened to an old <w>LP</w> with recordings of classical music."
                            }
                        }
                    ],
                    "common_phrases": [
                        "диск-гигант"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        },
                        {
                            "trait_type": "dated",
                            "comment": "technology"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A gramophone record of the largest size."
                    },

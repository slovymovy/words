    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d9f88b52-19cc-3a8f-992f-893e6c2249ef",
                    "sense_definition": "Большой коллектив музыкантов, совместно исполняющий инструментальные музыкальные произведения.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "music_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На городской площади играл духовой <w>оркестр</w>.",
                            "target_lang_translations": {
                                "en": "A brass <w>band</w> was playing in the town square."
                            }
                        },
                        {
                            "text": "Дирижёр взмахнул палочкой, и <w>оркестр</w> начал исполнять симфонию.",
                            "target_lang_translations": {
                                "en": "The conductor waved his baton, and the <w>orchestra</w> began to play the symphony."
                            }
                        }
                    ],
                    "common_phrases": [
                        "симфонический оркестр",
                        "духовой оркестр",
                        "камерный оркестр",
                        "дирижёр оркестра"
                    ],
                    "target_lang_definitions": {
                        "en": "A large group of musicians performing instrumental musical works together."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "orchestra",
                                "target_lang_sense_clarification": "group of musicians"
                            },
                            {
                                "target_lang_word": "band",
                                "target_lang_sense_clarification": "e.g. brass band"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf9fba57-ab63-33fa-b050-8477ef7c11f6",
                    "sense_definition": "Специальное помещение или углубление перед сценой в театре, предназначенное для размещения музыкантов.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "theater_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Музыканты заняли свои места в <w>оркестре</w> и начали настраивать инструменты.",
                            "target_lang_translations": {
                                "en": "The musicians took their seats in the <w>orchestra pit</w> and began tuning their instruments."
                            }
                        },
                        {
                            "text": "Из тёмного <w>оркестра</w> доносилась тихая музыка увертюры.",
                            "target_lang_translations": {
                                "en": "Quiet overture music drifted up from the dark <w>orchestra pit</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "оркестровая яма"
                    ],
                    "common_phrases": [
                        "сидеть в оркестре",
                        "место в оркестре"
                    ],
                    "target_lang_definitions": {
                        "en": "A special area or sunken space in front of a stage in a theater, designed for musicians."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "orchestra pit",
                                "target_lang_sense_clarification": "sunken area for musicians"
                            },
                            {
                                "target_lang_word": "orchestra",
                                "target_lang_sense_clarification": "area in a theater"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c73fd26a-f560-3f94-89b5-e2280cbce2a1",
                    "sense_definition": "Неофициальное ироничное название частной военной компании «Вагнер».",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "military_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В новостях часто упоминали действия так называемого <w>оркестра</w>.",
                            "target_lang_translations": {
                                "en": "The news often mentioned the actions of the so-called <w>Orchestra</w>."
                            }
                        },
                        {
                            "text": "Бойцы этого <w>оркестра</w> участвовали во многих сложных операциях.",
                            "target_lang_translations": {
                                "en": "Fighters of this <w>Orchestra</w> participated in many complex operations."
                            }
                        }
                    ],
                    "synonyms": [
                        "вагнер"
                    ],
                    "common_phrases": [
                        "музыканты оркестра"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "military"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An unofficial, ironic nickname for the private military company \"Wagner\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Orchestra",
                                "target_lang_sense_clarification": "slang for Wagner PMC"
                            }
                        ]
                    }
                }
            ]
        }
    ]

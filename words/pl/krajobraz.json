            "senses": [
                {
                    "sense_id": "04cc80d5-a0c7-3c51-92ce-a411f414f3eb",
                    "sense_definition": "Wygląd ziemi w określonym miejscu, widok okolicy, ukształtowanie terenu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nieodłącznym elementem tego <w>krajobrazu</w> są liczne przydrożne krzyże i kapliczki.",
                            "target_lang_translations": {
                                "ru": "Неотъемлемым элементом этого <w>пейзажа</w> являются многочисленные придорожные кресты и часовни."
                            }
                        },
                        {
                            "text": "Z okna naszego hotelu roztaczał się wspaniały górski <w>krajobraz</w>.",
                            "target_lang_translations": {
                                "ru": "Из окна нашего отеля открывался великолепный горный <w>пейзаж</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pejzaż"
                    ],
                    "common_phrases": [
                        "piękny krajobraz",
                        "krajobraz górski",
                        "podziwiać krajobraz",
                        "element krajobrazu",
                        "urozmaicony krajobraz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешний вид местности, характер земной поверхности, рельеф."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пейзаж",
                                "target_lang_sense_clarification": "общий вид местности"
                            },
                            {
                                "target_lang_word": "ландшафт",
                                "target_lang_sense_clarification": "рельеф, географическая характеристика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5325f246-2cbf-3aa5-8621-0bf80c4d6b77",
                    "sense_definition": "Dzieło malarskie, rysunek lub fotografia przedstawiające widok natury.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum narodowym oglądaliśmy słynny <w>krajobraz</w> namalowany przez holenderskiego mistrza.",
                            "target_lang_translations": {
                                "ru": "В национальном музее мы рассматривали знаменитый <w>пейзаж</w>, написанный голландским мастером."
                            }
                        },
                        {
                            "text": "Ten młody artysta najchętniej tworzy olejne <w>krajobrazy</w> przedstawiające morze.",
                            "target_lang_translations": {
                                "ru": "Этот молодой художник охотнее всего создает масляные <w>пейзажи</w>, изображающие море."
                            }
                        }
                    ],
                    "synonyms": [
                        "pejzaż",
                        "obraz",
                        "landszaft"
                    ],
                    "common_phrases": [
                        "malować krajobraz",
                        "krajobraz olejny",
                        "wystawa krajobrazów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Живописное произведение, рисунок или фотография, изображающие природу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пейзаж",
                                "target_lang_sense_clarification": "картина или жанр искусства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e6ec09f-bc3d-353a-a063-676d7dde2d63",
                    "sense_definition": "Ogół faktów, zjawisk i warunków tworzących charakterystyczny układ w jakiejś dziedzinie życia (np. polityce, gospodarce).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ostatnie wybory parlamentarne całkowicie zmieniły <w>krajobraz</w> polityczny w naszym kraju.",
                            "target_lang_translations": {
                                "ru": "Прошедшие парламентские выборы полностью изменили политический <w>ландшафт</w> в нашей стране."
                            }
                        },
                        {
                            "text": "Nowe technologie szybko przekształcają <w>krajobraz</w> współczesnych mediów i komunikacji.",
                            "target_lang_translations": {
                                "ru": "Новые технологии быстро преобразуют <w>ландшафт</w> современных медиа и коммуникаций."
                            }
                        }
                    ],
                    "synonyms": [
                        "obraz"
                    ],
                    "common_phrases": [
                        "krajobraz polityczny",
                        "krajobraz po bitwie",
                        "krajobraz gospodarczy",
                        "zmieniać krajobraz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность фактов и условий, формирующих характерную ситуацию в какой-либо сфере (например, в политике)."
                    },
                    "translations": {
                        "ru": [
                            {

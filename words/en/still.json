                },
                {
                    "sense_id": "51287d6e-1163-339b-869e-e79df5e6dbc9",
                    "sense_definition": "Making no sound; quiet.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "calmness_silence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The forest was <w>still</w> and silent in the early morning.",
                            "target_lang_translations": {
                                "ru": "Ранним утром лес был <w>тих</w> и безмолвен."
                            }
                        },
                        {
                            "text": "Be <w>still</w> and listen to the music.",
                            "target_lang_translations": {
                                "ru": "Веди себя <w>тихо</w> и слушай музыку."
                            }
                        }
                    ],
                    "synonyms": [
                        "silent",
                        "quiet",
                        "noiseless",
                        "hushed"
                    ],
                    "antonyms": [
                        "noisy",
                        "loud"
                    ],
                    "common_phrases": [
                        "be still",
                        "still small voice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не издающий звуков; тихий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тихий",
                                "target_lang_sense_clarification": "без звуков"
                            },
                            {
                                "target_lang_word": "безмолвный",
                                "target_lang_sense_clarification": "полная тишина"
                            }
                        ]
                    }
                }
            ]
        },
        {

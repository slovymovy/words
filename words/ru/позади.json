    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "5eb6e755-8f3a-3e99-92c4-76a15a8d8fb0",
                    "sense_definition": "Указывает на нахождение с задней стороны кого-либо или чего-либо; за кем-чем-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы посадили большое дерево <w>позади</w> нашего дома.",
                            "target_lang_translations": {
                                "en": "We planted a big tree <w>behind</w> our house."
                            }
                        },
                        {
                            "text": "Он шёл <w>позади</w> меня и тихо напевал песню.",
                            "target_lang_translations": {
                                "en": "He walked <w>behind</w> me and quietly hummed a song."
                            }
                        }
                    ],
                    "synonyms": [
                        "за"
                    ],
                    "antonyms": [
                        "спереди",
                        "впереди"
                    ],
                    "common_phrases": [
                        "позади дома",
                        "позади нас",
                        "стоять позади"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates location at the back of someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "behind",
                                "target_lang_sense_clarification": "preposition"
                            },
                            {
                                "target_lang_word": "at the back of",
                                "target_lang_sense_clarification": "location"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4ccf6bd9-8ec6-3e2b-92fd-8134ab2c7f0a",
                    "sense_definition": "В каком-либо месте, расположенном сзади; с задней стороны.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Голоса друзей раздавались где-то <w>позади</w>.",
                            "target_lang_translations": {
                                "en": "The voices of friends could be heard somewhere <w>behind</w>."
                            }
                        },
                        {
                            "text": "Я обернулся и увидел, что город остался <w>позади</w>.",
                            "target_lang_translations": {
                                "en": "I turned around and saw that the city was left <w>behind</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "сзади"
                    ],
                    "antonyms": [
                        "спереди",
                        "впереди"
                    ],
                    "common_phrases": [
                        "остаться позади",
                        "слышать позади",
                        "идти позади"
                    ],
                    "target_lang_definitions": {
                        "en": "In a place located at the back or in the rear."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "behind",
                                "target_lang_sense_clarification": "adverb"
                            },
                            {
                                "target_lang_word": "at the back",
                                "target_lang_sense_clarification": "location"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "596284d0-5f75-3f2d-bf60-d177cb5b78a7",
                    "sense_definition": "О событиях или времени: в прошлом, то, что уже миновало.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Все наши трудности и проблемы теперь <w>позади</w>.",
                            "target_lang_translations": {
                                "en": "All our difficulties and problems are now <w>behind</w> us."
                            }
                        },
                        {
                            "text": "Холодная зима уже <w>позади</w>, и наступает тёплая весна.",
                            "target_lang_translations": {
                                "en": "The cold winter is already <w>over</w>, and warm spring is coming."
                            }
                        }
                    ],
                    "synonyms": [
                        "в прошлом"
                    ],
                    "antonyms": [
                        "в будущем",
                        "впереди"
                    ],
                    "common_phrases": [
                        "всё позади",
                        "самое страшное позади",
                        "оставить позади"
                    ],
                    "target_lang_definitions": {
                        "en": "Referring to events or time that are in the past or finished."
                    },

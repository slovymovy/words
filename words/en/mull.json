                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We took a ferry to visit the Isle of <w>Mull</w> last summer.",
                            "target_lang_translations": {
                                "ru": "Прошлым летом мы сели на паром, чтобы посетить остров <w>Малл</w>."

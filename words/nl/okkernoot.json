    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "85927835-db34-3efd-b7d2-4ecc1913f904",
                    "sense_definition": "Een eetbare vrucht met een harde, houtachtige schaal en een gerimpelde kern, afkomstig van de okkernotenboom.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fruit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik gebruik een notenkraker om de harde <w>okkernoot</w> open te maken.",
                            "target_lang_translations": {
                                "ru": "Я использую щелкунчик, чтобы расколоть твердый <w>грецкий орех</w>."
                            }
                        },
                        {
                            "text": "Voor de garnering van de taart heb ik een halve <w>okkernoot</w> gebruikt.",
                            "target_lang_translations": {
                                "ru": "Для украшения торта я использовал половинку <w>грецкого ореха</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "walnoot"
                    ],
                    "common_phrases": [
                        "een okkernoot kraken",
                        "gedroogde okkernoot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобный плод с твердой деревянистой скорлупой и морщинистым ядром, растущий на ореховом дереве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грецкий орех",
                                "target_lang_sense_clarification": "плод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ed9015e-8b6e-3d30-ab82-ef83d211f628",
                    "sense_definition": "De loofboom Juglans regia waaraan deze noten groeien.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In onze achtertuin staat een oude <w>okkernoot</w> die veel schaduw geeft.",
                            "target_lang_translations": {
                                "ru": "На нашем заднем дворе растет старый <w>грецкий орех</w>, который дает много тени."
                            }
                        },
                        {
                            "text": "De bladeren van de <w>okkernoot</w> verkleuren prachtig in de herfst.",
                            "target_lang_translations": {
                                "ru": "Листья <w>грецкого ореха</w> красиво меняют цвет осенью."
                            }
                        }
                    ],
                    "synonyms": [
                        "okkernotenboom",
                        "walnotenboom",
                        "gewone walnoot"
                    ],
                    "common_phrases": [
                        "onder de okkernoot",
                        "een grote okkernoot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лиственное дерево Juglans regia, на котором растут эти орехи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грецкий орех",
                                "target_lang_sense_clarification": "дерево"
                            },
                            {
                                "target_lang_word": "ореховое дерево",
                                "target_lang_sense_clarification": "дерево"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a49ec926-4855-33eb-9bc0-091019f9605a",
                    "sense_definition": "Wstępny, uproszczony rysunek wykonany odręcznie, zaznaczający ogólne kontury i kompozycję, często służący jako podstawa do dalszej pracy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Artysta wykonał szybki <w>szkic</w> portretu ołówkiem na kartce papieru.",
                            "target_lang_translations": {
                                "ru": "Художник сделал быстрый <w>набросок</w> портрета карандашом на листе бумаги."
                            }
                        },
                        {
                            "text": "Ten <w>szkic</w> pomoże nam zrozumieć układ pomieszczeń w nowym domu.",
                            "target_lang_translations": {
                                "ru": "Этот <w>эскиз</w> поможет нам понять планировку комнат в новом доме."
                            }
                        }
                    ],
                    "synonyms": [
                        "rysunek"
                    ],
                    "common_phrases": [
                        "szkic ołówkiem",
                        "szkic sytuacyjny",
                        "robić szkic",
                        "wstępny szkic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предварительный, упрощенный рисунок, выполненный от руки, намечающий общие контуры и композицию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набросок",
                                "target_lang_sense_clarification": "быстрый рисунок"
                            },
                            {
                                "target_lang_word": "эскиз",
                                "target_lang_sense_clarification": "предварительный рисунок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7565f2d5-80e5-30d9-869c-44095cbff353",
                    "sense_definition": "Zarys, wstępny plan lub brudnopis projektu, tekstu czy koncepcji, zawierający główne elementy bez szczegółów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest dopiero <w>szkic</w> raportu, więc brakuje w nim jeszcze wielu danych.",
                            "target_lang_translations": {
                                "ru": "Это пока только <w>набросок</w> отчета, поэтому в нем еще не хватает многих данных."
                            }
                        },
                        {
                            "text": "Mam w głowie ogólny <w>szkic</w> tego, co chcę powiedzieć na zebraniu.",
                            "target_lang_translations": {
                                "ru": "У меня в голове есть общий <w>набросок</w> того, что я хочу сказать на собрании."
                            }
                        }
                    ],
                    "synonyms": [
                        "plan"
                    ],
                    "common_phrases": [
                        "szkic projektu",
                        "szkic powieści",
                        "szkic scenariusza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предварительный план, черновик проекта или текста, содержащий основные элементы без деталей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набросок",
                                "target_lang_sense_clarification": "предварительный вариант"
                            },
                            {
                                "target_lang_word": "черновик",
                                "target_lang_sense_clarification": "предварительная версия текста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d89f658-89ce-3712-8c96-0cac366cb5a8",
                    "sense_definition": "Krótki utwór literacki lub publicystyczny o swobodnej kompozycji, opisujący zjawiska, osoby lub zdarzenia, często bez wyraźnej fabuły.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autor opublikował zbiór zawierający <w>szkice</w> historyczne z okresu wojennego.",
                            "target_lang_translations": {
                                "ru": "Автор опубликовал сборник, содержащий исторические <w>очерки</w> военного времени."
                            }
                        },
                        {
                            "text": "Ten <w>szkic</w> literacki doskonale oddaje atmosferę małego miasteczka.",
                            "target_lang_translations": {
                                "ru": "Этот литературный <w>очерк</w> прекрасно передает атмосферу маленького городка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szkic literacki",
                        "szkic historyczny",
                        "szkic krytyczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое литературное или публицистическое произведение, описывающее реальные события или людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очерк",
                                "target_lang_sense_clarification": "литературный жанр"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "060589e7-1ec5-34e4-a92a-ac6ee002a5e4",
                    "sense_definition": "Een groep van gelijksoortige bedrijven, zoals winkels, hotels of restaurants, die onder dezelfde naam en leiding opereren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "business_network",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze supermarkt is onderdeel van een grote landelijke <w>keten</w>.",
                            "target_lang_translations": {
                                "ru": "Этот супермаркет является частью крупной национальной <w>сети</w>."
                            }
                        },
                        {
                            "text": "Zij werkt als manager bij een bekende internationale <w>keten</w> van kledingwinkels.",
                            "target_lang_translations": {
                                "ru": "Она работает менеджером в известной международной <w>сети</w> магазинов одежды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een internationale keten",
                        "een keten van winkels",
                        "een grote keten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа однотипных предприятий, таких как магазины, отели или рестораны, работающих под одним названием и руководством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сеть",
                                "target_lang_sense_clarification": "магазинов, отелей и т.д."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb6dde49-8b23-3d6d-b9b6-11aec9e4d43c",
                    "sense_definition": "Een aaneengeschakelde reeks van gebeurtenissen, voorwerpen of verschijnselen die met elkaar verbonden zijn.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een <w>keten</w> van misverstanden leidde uiteindelijk tot een grote ruzie.",
                            "target_lang_translations": {
                                "ru": "<w>Цепь</w> недоразумений в конечном итоге привела к крупной ссоре."
                            }
                        },
                        {
                            "text": "De eilanden vormen een lange <w>keten</w> in de oceaan.",
                            "target_lang_translations": {
                                "ru": "Острова образуют длинную <w>цепь</w> в океане."
                            }
                        }
                    ],
                    "synonyms": [
                        "ketting",
                        "ketting",
                        "ketting"
                    ],
                    "common_phrases": [
                        "een keten van gebeurtenissen",
                        "een keten van reacties"
                    ],
                    "target_lang_definitions": {
                        "ru": "Последовательный ряд событий, предметов или явлений, связанных друг с другом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цепь",
                                "target_lang_sense_clarification": "событий, горная"
                            },
                            {
                                "target_lang_word": "цепочка",
                                "target_lang_sense_clarification": "последовательность"
                            },
                            {
                                "target_lang_word": "череда",
                                "target_lang_sense_clarification": "событий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cbaae52-f5d3-3016-b49f-64d516e8dd2b",
                    "sense_definition": "Een zwaar snoer van metalen schakels, vaak gebruikt om iets of iemand vast te leggen of de vrijheid te benemen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_chain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gevangene werd in zware <w>ketenen</w> afgevoerd naar de cel.",
                            "target_lang_translations": {
                                "ru": "Заключенного увели в камеру в тяжелых <w>оковах</w>."
                            }
                        },
                        {
                            "text": "De waakhond lag aan een dikke <w>keten</w> en kon niet ver komen.",
                            "target_lang_translations": {
                                "ru": "Сторожевая собака сидела на толстой <w>цепи</w> и не могла далеко уйти."
                            }
                        }
                    ],
                    "synonyms": [
                        "ketting"
                    ],
                    "common_phrases": [
                        "in ketenen slaan",
                        "de ketenen verbreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжелая цепь из металлических звеньев, часто используемая для ограничения свободы; во множественном числе — оковы."
                    },
                    "translations": {
                        "ru": [

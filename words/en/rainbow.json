                {
                    "sense_id": "5bfb1183-f0d7-3f34-9123-188331857871",
                    "sense_definition": "A multicolored arch in the sky, produced by the refraction of sunlight through raindrops.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "colors_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After the heavy storm passed, a beautiful <w>rainbow</w> appeared over the hills.",
                            "target_lang_translations": {
                                "ru": "После сильной грозы над холмами появилась красивая <w>радуга</w>."
                            }
                        },
                        {
                            "text": "The children drew a bright <w>rainbow</w> with crayons on the paper.",
                            "target_lang_translations": {
                                "ru": "Дети нарисовали на бумаге яркую <w>радугу</w> цветными мелками."
                            }
                        }
                    ],
                    "common_phrases": [
                        "over the rainbow",
                        "chase a rainbow",
                        "colors of the rainbow"
                    ],

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d3f737ff-b253-332c-aaeb-7484328d7ef9",
                    "sense_definition": "Agreement, harmony, or conformity with a rule or request.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee made its decision in <w>accordance</w> with the established guidelines.",
                            "target_lang_translations": {
                                "ru": "Комитет принял решение в <w>соответствии</w> с установленными правилами."
                            }
                        },
                        {
                            "text": "We must ensure that our actions are in strict <w>accordance</w> with the law.",
                            "target_lang_translations": {
                                "ru": "Мы должны гарантировать, что наши действия находятся в строгом <w>соответствии</w> с законом."
                            }
                        }
                    ],
                    "synonyms": [
                        "harmony"
                    ],
                    "common_phrases": [
                        "in accordance with",
                        "strict accordance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соответствие правилу, требованию или просьбе; согласованность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соответствие",
                                "target_lang_sense_clarification": "соотношение, выражающее согласованность или равенство"
                            },
                            {
                                "target_lang_word": "согласие",
                                "target_lang_sense_clarification": "гармония, отсутствие противоречий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e1ab67e-a7eb-3005-9525-ae9ad97488a3",
                    "sense_definition": "The act of granting or giving something to someone.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>accordance</w> of special privileges to the guests was appreciated by everyone.",
                            "target_lang_translations": {
                                "ru": "<w>Предоставление</w> особых привилегий гостям было оценено всеми."
                            }
                        },
                        {
                            "text": "He thanked the board for the <w>accordance</w> of this unique opportunity.",
                            "target_lang_translations": {
                                "ru": "Он поблагодарил совет за <w>предоставление</w> этой уникальной возможности."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по предоставлению или дарованию чего-либо кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предоставление",
                                "target_lang_sense_clarification": "выдача прав, привилегий или возможностей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
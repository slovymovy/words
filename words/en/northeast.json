                {
                    "sense_id": "4b4e631c-fdb8-3603-916c-59fb927f0363",
                    "sense_definition": "The direction or compass point halfway between north and east.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "compass_point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wind is blowing strongly from the <w>northeast</w> today.",
                            "target_lang_translations": {
                                "ru": "Сегодня дует сильный ветер с <w>северо-востока</w>."
                            }
                        },
                        {
                            "text": "Look to the <w>northeast</w> to see the sunrise over the mountains.",
                            "target_lang_translations": {
                                "ru": "Посмотрите на <w>северо-восток</w>, чтобы увидеть восход солнца над горами."
                            }
                        }
                    ],
                    "antonyms": [

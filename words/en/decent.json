        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "516d0d62-5e7d-3594-9064-d1d15920bfef",
                    "sense_definition": "Of an acceptable standard or quality; satisfactory.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quality_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hotel room was small but <w>decent</w> for the price.",
                            "target_lang_translations": {
                                "ru": "Номер в отеле был маленьким, но <w>прили́чным</w> для такой цены."
                            }
                        },
                        {
                            "text": "He finally found a <w>decent</w> job with a good salary.",
                            "target_lang_translations": {
                                "ru": "Он наконец нашел <w>досто́йную</w> работу с хорошей зарплатой."
                            }
                        }
                    ],
                    "synonyms": [
                        "satisfactory",
                        "good"
                    ],
                    "antonyms": [
                        "poor",
                        "unsatisfactory",
                        "inadequate"
                    ],
                    "common_phrases": [
                        "decent job",
                        "decent meal",
                        "decent quality",
                        "pretty decent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приемлемого уровня или качества; удовлетворительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прили́чный",
                                "target_lang_sense_clarification": "хорошего качества"

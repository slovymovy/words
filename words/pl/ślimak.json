        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "93d031c1-4c32-3dc3-9c62-d4ea32ae48f6",
                    "sense_definition": "mięczak lądowy lub wodny, zazwyczaj posiadający spiralną muszlę na grzbiecie, poruszający się powoli",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ogrodzie na liściu sałaty siedział mały <w>ślimak</w>.",
                            "target_lang_translations": {
                                "ru": "В саду на листе салата сидела маленькая <w>улитка</w>."
                            }
                        },
                        {
                            "text": "Po deszczu na chodniku pojawiło się mnóstwo <w>ślimaków</w>.",
                            "target_lang_translations": {
                                "ru": "После дождя на тротуаре появилось множество <w>улиток</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) ślimik"
                    ],
                    "common_phrases": [
                        "powolny jak ślimak",
                        "ślimak winniczek",
                        "muszla ślimaka",
                        "pokazać rogi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наземный или водный моллюск, обычно имеющий спиральную раковину и передвигающийся медленно"
                    },
                    "translations": {
                        "ru": [
                            {

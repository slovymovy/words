            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fc7db3d2-545f-3b14-85c7-fb5481db3155",
                    "sense_definition": "De leer die zich bezighoudt met de voortbrenging en verdeling van schaarse goederen en diensten",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "science",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik had een onvoldoende voor <w>economie</w>, maar mocht toch naar de volgende klas.",
                            "target_lang_translations": {
                                "ru": "У меня была неудовлетворительная оценка по <w>экономике</w>, но меня всё равно перевели в следующий класс."
                            }
                        },
                        {
                            "text": "Mijn zus wil later <w>economie</w> gaan studeren aan de universiteit.",
                            "target_lang_translations": {
                                "ru": "Моя сестра хочет в будущем изучать <w>экономику</w> в университете."
                            }
                        }
                    ],
                    "synonyms": [
                        "staathuiskunde"
                    ],
                    "common_phrases": [
                        "studie economie",
                        "leraar economie",
                        "les in economie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наука, изучающая производство и распределение товаров и услуг"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экономика",
                                "target_lang_sense_clarification": "наука / учебный предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6792ba4c-d73c-308c-bb39-3cc173e35d27",
                    "sense_definition": "De economische praktijk ofwel het geheel van productie, handelsverkeer en diensten binnen een bepaalde regio",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>economie</w> loopt in dat land al jaren slecht.",
                            "target_lang_translations": {
                                "ru": "<w>Экономика</w> этой страны уже много лет находится в плохом состоянии."
                            }
                        },
                        {
                            "text": "Door de crisis kreeg de wereldwijde <w>economie</w> een harde klap te verwerken.",
                            "target_lang_translations": {
                                "ru": "Из-за кризиса мировая <w>экономика</w> получила сильный удар."
                            }
                        }
                    ],
                    "synonyms": [
                        "nationale huishouding",
                        "staatshuishouding",
                        "marktsamenleving",
                        "bedrijvigheid"
                    ],
                    "common_phrases": [
                        "de Nederlandse economie",
                        "slecht voor de economie",
                        "een groeiende economie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хозяйственная деятельность, совокупность производства и торговли в определенном регионе"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экономика",
                                "target_lang_sense_clarification": "хозяйственная система страны или региона"
                            },
                            {
                                "target_lang_word": "хозяйство",
                                "target_lang_sense_clarification": "экономическая деятельность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fa2692a-1a20-31b2-9e19-3dadcd0a669c",
                    "sense_definition": "Zuinigheid of het efficiënt omgaan met middelen",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "thrift",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij voeren hun huishouden met grote <w>economie</w> en verspillen helemaal niets.",
                            "target_lang_translations": {
                                "ru": "Они ведут домашнее хозяйство с большой <w>экономией</w> и ничего не тратят зря."
                            }
                        },
                        {
                            "text": "De <w>economie</w> van brandstof is een belangrijk verkoopargument voor deze nieuwe auto.",
                            "target_lang_translations": {
                                "ru": "<w>Экономия</w> топлива — важный аргумент при продаже этого нового автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezuiniging",
                        "doelmatigheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бережливость или эффективное использование ресурсов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экономия",
                                "target_lang_sense_clarification": "бережливость / снижение расходов"
                            },
                            {
                                "target_lang_word": "экономичность",
                                "target_lang_sense_clarification": "эффективность использования"
                            }
                        ]
                    }
                }
            ]
        }
    ]

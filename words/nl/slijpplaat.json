    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d5e19c30-89cf-339e-b422-f3c574ad8909",
                    "sense_definition": "Een platte, vaak ronde schijf van hard materiaal die gebruikt wordt in een machine om voorwerpen te slijpen of glad te maken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de <w>slijpplaat</w> vervangen als deze te ver versleten is.",
                            "target_lang_translations": {
                                "ru": "Нужно заменить <w>шлифовальный диск</w>, если он слишком изношен."
                            }
                        },
                        {
                            "text": "De vonken vlogen in het rond toen de <w>slijpplaat</w> het metaal raakte.",
                            "target_lang_translations": {
                                "ru": "Искры полетели во все стороны, когда <w>шлифовальный диск</w> коснулся металла."
                            }
                        }
                    ],
                    "synonyms": [
                        "slijpschijf"
                    ],
                    "common_phrases": [
                        "slijpplaat vervangen",
                        "nieuwe slijpplaat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоский, часто круглый диск из твердого материала, используемый в станке для заточки или шлифовки предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шлифовальный диск",
                                "target_lang_sense_clarification": "диск для шлифовки"
                            },
                            {
                                "target_lang_word": "точильный круг",
                                "target_lang_sense_clarification": "круг для заточки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6daa7a6d-5695-3882-939d-0c9b7512e72e",
                    "sense_definition": "Een specifiek onderdeel van een ploeg dat dient om de grond te snijden of te keren.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer controleerde of de <w>slijpplaat</w> van de ploeg nog scherp genoeg was.",
                            "target_lang_translations": {
                                "ru": "Фермер проверил, достаточно ли остра <w>режущая пластина</w> плуга."
                            }
                        },
                        {
                            "text": "Voor dit type grond is een speciale <w>slijpplaat</w> op de machine nodig.",
                            "target_lang_translations": {
                                "ru": "Для этого типа почвы на машине требуется специальная <w>режущая пластина</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфическая деталь плуга, служащая для подрезания или переворачивания грунта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "режущая пластина",
                                "target_lang_sense_clarification": "деталь плуга"
                            },
                            {
                                "target_lang_word": "лемех",
                                "target_lang_sense_clarification": "функциональный аналог"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "265909f5-96d0-3d2d-86b3-21a375ec3f81",
                    "sense_definition": "Czynność orania ziemi; praca na roli przy użyciu pługa.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_plowing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik zakończył jesienną <w>oraczkę</w> tuż przed pierwszymi przymrozkami.",
                            "target_lang_translations": {
                                "ru": "Фермер закончил осеннюю <w>пахоту</w> как раз перед первыми заморозками."
                            }
                        },
                        {
                            "text": "Ciężka <w>oraczka</w> wymagała od niego wiele siły i wytrwałości.",
                            "target_lang_translations": {
                                "ru": "Тяжелая <w>пахота</w> требовала от него много сил и выносливости."
                            }
                        }
                    ],
                    "synonyms": [
                        "orka",
                        "oranie"
                    ],
                    "common_phrases": [
                        "wiosenna oraczka",
                        "jesienna oraczka",
                        "ciężka oraczka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по вспашке земли; работа в поле с использованием плуга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пахота",
                                "target_lang_sense_clarification": "процесс вспашки"
                            },
                            {
                                "target_lang_word": "вспашка",
                                "target_lang_sense_clarification": "агротехническое действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f275f234-44e2-3850-a28e-ae0e67954247",
                    "sense_definition": "Kobieta, która wykonuje pracę orania pola.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person_female",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta dzielna <w>oraczka</w> pracowała w polu na równi z mężczyznami.",
                            "target_lang_translations": {
                                "ru": "Эта отважная <w>пахарка</w> работала в поле наравне с мужчинами."
                            }
                        },
                        {
                            "text": "Na horyzoncie widać było sylwetkę samotnej <w>oraczki</w> prowadzącej pług.",
                            "target_lang_translations": {
                                "ru": "На горизонте был виден силуэт одинокой <w>пахарки</w>, ведущей плуг."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, выполняющая работу по вспашке поля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пахарка",
                                "target_lang_sense_clarification": "женщина-пахарь"
                            },
                            {
                                "target_lang_word": "женщина-пахарь",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fc3f502-4221-30cc-8847-2c2f7f8abcfc",
                    "sense_definition": "Żona oracza (mężczyzny orzącego pole).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person_female",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młoda <w>oraczka</w> przyniosła mężowi posiłek na pole w południe.",
                            "target_lang_translations": {
                                "ru": "Молодая <w>жена пахаря</w> принесла мужу обед в поле в полдень."
                            }
                        },
                        {
                            "text": "Stara <w>oraczka</w> opowiadała wnukom o trudach dawnego życia na wsi.",
                            "target_lang_translations": {
                                "ru": "Старая <w>жена пахаря</w> рассказывала внукам о тяготах прежней деревенской жизни."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "określenie historyczne lub środowiskowe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жена пахаря (мужчины, пашущего поле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жена пахаря",
                                "target_lang_sense_clarification": "супруга землепашца"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b24eec5c-c226-3669-a821-6a37fffd7206",
                    "sense_definition": "Zaorane pole, ziemia przygotowana pod uprawę.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place_field",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ptaki szukały pożywienia na świeżej <w>oraczce</w> za domem.",
                            "target_lang_translations": {
                                "ru": "Птицы искали корм на свежей <w>пашне</w> за домом."
                            }
                        },
                        {
                            "text": "Gospodarz z dumą spoglądał na swoją równą <w>oraczkę</w>.",
                            "target_lang_translations": {
                                "ru": "Хозяин с гордостью смотрел на свою ровную <w>пашню</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "świeża oraczka"
                    ],

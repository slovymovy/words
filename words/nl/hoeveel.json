                    ],
                    "target_lang_definitions": {
                        "en": "What number or quantity; used independently without a directly following noun.",
                        "ru": "Какое количество; используется самостоятельно, без следующего за ним существительного."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "how many",
                                "target_lang_sense_clarification": "referring to countable items"
                            },
                            {
                                "target_lang_word": "how much",
                                "target_lang_sense_clarification": "referring to uncountable amounts"
                            }
                        ],
                        "ru": [

{
    "word_family": [
        "filtration",
        "filtrate",
        "unfiltered",
        "infiltrate",
        "infiltration"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "40451500-d5f6-3bce-a00d-0e495a160907",
                    "sense_definition": "A device or material that removes solid particles or impurities from a liquid or gas passing through it.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to buy a new coffee <w>filter</w> for the machine.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новый кофейный <w>фильтр</w> для кофемашины."
                            }
                        },
                        {
                            "text": "The air <w>filter</w> in the car prevents dust from entering the engine.",
                            "target_lang_translations": {
                                "ru": "Воздушный <w>фильтр</w> в машине предотвращает попадание пыли в двигатель."
                            }
                        }
                    ],
                    "common_phrases": [
                        "coffee filter",
                        "water filter",
                        "air filter",
                        "oil filter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство или материал, который удаляет твердые частицы или примеси из жидкости или газа, проходящего через него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фильтр",
                                "target_lang_sense_clarification": "устройство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "349ba056-1b11-3ecc-ac18-d50de407ad90",
                    "sense_definition": "A software program or function that processes data to find or exclude specific items, such as unwanted emails or search results.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "software_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He runs an email <w>filter</w> to catch the junk mail.",
                            "target_lang_translations": {
                                "ru": "Он использует <w>фильтр</w> электронной почты, чтобы отлавливать спам."
                            }
                        },
                        {
                            "text": "You can use a search <w>filter</w> to find the cheapest hotels.",
                            "target_lang_translations": {
                                "ru": "Вы можете использовать <w>фильтр</w> поиска, чтобы найти самые дешевые отели."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spam filter",
                        "internet filter",
                        "search filter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Программное обеспечение или функция, которая обрабатывает данные для поиска или исключения определенных элементов, таких как нежелательные письма или результаты поиска."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фильтр",
                                "target_lang_sense_clarification": "программа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27a8407d-1ca4-3dff-b8a4-1aca610e87cd",
                    "sense_definition": "An effect applied to a digital image to alter its appearance, often used on social media.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "image_effect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She posted a photo with a vintage <w>filter</w>.",
                            "target_lang_translations": {
                                "ru": "Она опубликовала фото с винтажным <w>фильтром</w>."
                            }
                        },
                        {
                            "text": "He don't need no <w>filter</w> posting pictures!",
                            "target_lang_translations": {
                                "ru": "Ему не нужен <w>фильтр</w>, когда он постит фотки!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "no filter",
                        "beauty filter",
                        "apply a filter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Эффект, применяемый к цифровому изображению для изменения его внешнего вида, часто используемый в социальных сетях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фильтр",
                                "target_lang_sense_clarification": "эффект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b04adf23-126a-3b90-b5fb-a999f34343bc",
                    "sense_definition": "The ability to control what one says to avoid being rude or offensive.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He's got no <w>filter</w>, and he's always offending people as a result.",
                            "target_lang_translations": {
                                "ru": "У него нет внутреннего <w>фильтра</w>, и в результате он постоянно обижает людей."
                            }
                        },
                        {
                            "text": "Sometimes it is better to have a <w>filter</w> than to say exactly what you think.",
                            "target_lang_translations": {
                                "ru": "Иногда лучше иметь <w>фильтр</w>, чем говорить именно то, что думаешь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "have no filter"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in the phrase 'no filter'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность контролировать свою речь, чтобы избежать грубости или оскорблений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фильтр",
                                "target_lang_sense_clarification": "внутренний контроль"
                            },
                            {
                                "target_lang_word": "внутренний цензор",
                                "target_lang_sense_clarification": "метафора"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f267c9ea-6c36-30b6-9dbf-c74bcb86ccd6",
                    "sense_definition": "To remove unwanted substances or select specific elements by passing them through a barrier or system.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "action_remove",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This strainer should <w>filter</w> out the large particles.",
                            "target_lang_translations": {
                                "ru": "Это сито должно <w>отфильтровать</w> крупные частицы."
                            }
                        },
                        {
                            "text": "The software <w>filters</w> out spam messages automatically.",
                            "target_lang_translations": {
                                "ru": "Программа автоматически <w>отфильтровывает</w> спам-сообщения."
                            }
                        }
                    ],
                    "synonyms": [
                        "to filter out"
                    ],
                    "common_phrases": [
                        "filter out",
                        "filter water"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалять нежелательные вещества или выбирать определенные элементы, пропуская их через барьер или систему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фильтровать",
                                "target_lang_sense_clarification": "процесс очистки"
                            },
                            {
                                "target_lang_word": "отфильтровывать",
                                "target_lang_sense_clarification": "удалять примеси"
                            },
                            {
                                "target_lang_word": "отсеивать",
                                "target_lang_sense_clarification": "отбирать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "347533ff-e320-3590-9e1a-28b92737923b",
                    "sense_definition": "To pass through something, such as a liquid through a porous material or light through a barrier.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_pass_through",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The water <w>filtered</w> through the rock and soil.",
                            "target_lang_translations": {
                                "ru": "Вода <w>просачивалась</w> сквозь камни и почву."
                            }
                        },
                        {
                            "text": "Sunlight <w>filtered</w> through the leaves of the trees.",
                            "target_lang_translations": {
                                "ru": "Солнечный свет <w>просачивался</w> сквозь листву деревьев."
                            }
                        }
                    ],
                    "common_phrases": [
                        "filter through",
                        "filter down"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить сквозь что-либо, например, жидкость через пористый материал или свет через преграду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просачиваться",
                                "target_lang_sense_clarification": "проходить сквозь"
                            },
                            {
                                "target_lang_word": "проникать",
                                "target_lang_sense_clarification": "попадать внутрь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "881817b0-ba09-3dcb-b6e9-c3a6ce3a24b0",
                    "sense_definition": "To move slowly or gradually in a specific direction, often in small numbers.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_move_gradually",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crowd <w>filtered</w> into the theater.",
                            "target_lang_translations": {
                                "ru": "Толпа <w>просачивалась</w> в театр."
                            }
                        },
                        {
                            "text": "News of the event began to <w>filter</w> back to the main office.",
                            "target_lang_translations": {
                                "ru": "Новости о событии начали <w>просачиваться</w> обратно в главный офис."
                            }
                        }
                    ],
                    "common_phrases": [
                        "filter in",
                        "filter back"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться медленно или постепенно в определенном направлении, часто небольшими группами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просачиваться",
                                "target_lang_sense_clarification": "постепенно проникать"
                            },
                            {
                                "target_lang_word": "проникать",
                                "target_lang_sense_clarification": "распространяться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "cbeb555c-8177-381b-8ae5-366f40b38e81",
                    "sense_definition": "A surname of German origin.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Filter</w> was known for his punctuality.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Фильтер</w> был известен своей пунктуальностью."
                            }
                        },
                        {
                            "text": "We met a family named <w>Filter</w> during our trip.",
                            "target_lang_translations": {
                                "ru": "Мы встретили семью по фамилии <w>Фильтер</w> во время нашей поездки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия немецкого происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Фильтер",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "db972db4-d4b2-39fa-aabd-8138b7f1047f",
                    "sense_definition": "Het vrijwillig neerleggen van een functie of baan door de werknemer zelf",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "employment_termination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij heeft <w>ontslag</w> genomen omdat ze een betere baan heeft gevonden.",
                            "target_lang_translations": {
                                "en": "She handed in her <w>resignation</w> because she found a better job.",
                                "ru": "Она подала на <w>увольнение</w>, так как нашла работу лучше."
                            }
                        },
                        {
                            "text": "De minister bood zijn <w>ontslag</w> aan na het politieke schandaal.",
                            "target_lang_translations": {
                                "en": "The minister tendered his <w>resignation</w> after the political scandal.",
                                "ru": "Министр подал в <w>отставку</w> после политического скандала."
                            }
                        }
                    ],
                    "synonyms": [
                        "aftreden"
                    ],
                    "antonyms": [
                        "aanstelling"
                    ],
                    "common_phrases": [
                        "ontslag nemen",
                        "ontslag indienen",
                        "ontslag aanbieden"
                    ],
                    "target_lang_definitions": {
                        "en": "The voluntary resignation from a position or job by the employee",
                        "ru": "Добровольный уход сотрудника с занимаемой должности"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "resignation",
                                "target_lang_sense_clarification": "the act of voluntarily leaving a job"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "увольнение",
                                "target_lang_sense_clarification": "по собственному желанию"
                            },
                            {
                                "target_lang_word": "отставка",
                                "target_lang_sense_clarification": "официальный уход с высокого поста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3af924c-655f-337b-a9b7-81c0e325d99d",
                    "sense_definition": "Het moment waarop een patiënt het ziekenhuis mag verlaten na een behandeling",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na haar <w>ontslag</w> uit het ziekenhuis moest ze thuis nog weken rusten.",
                            "target_lang_translations": {

                    ],
                    "common_phrases": [
                        "frost hair",
                        "frosted tips"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обесцвечивать отдельные пряди волос, чтобы сделать их светлее остальных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мели́ровать",
                                "target_lang_sense_clarification": "осветлять пряди"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d2a7cdb-60ee-3868-83fe-29a072271bed",
                    "sense_definition": "To make someone angry or annoyed.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotion_coldness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It really <w>frosts</w> me when people cut in line at the store.",
                            "target_lang_translations": {
                                "ru": "Меня реально <w>вымораживает</w>, когда люди лезут без очереди в магазине."
                            }
                        },
                        {
                            "text": "His arrogant attitude tends to <w>frost</w> his coworkers.",
                            "target_lang_translations": {
                                "ru": "Его высокомерное отношение имеет тенденцию <w>бесить</w> коллег."
                            }
                        }
                    ],
                    "synonyms": [
                        "annoy",
                        "irritate",
                        "bug"
                    ],
                    "antonyms": [
                        "please",
                        "soothe"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "primarily US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Злить или раздражать кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вымора́живать",
                                "target_lang_sense_clarification": "сленг: сильно раздражать"
                            },
                            {
                                "target_lang_word": "беси́ть",
                                "target_lang_sense_clarification": "разг: выводить из себя"
                            }
                        ]
                    }

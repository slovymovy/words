    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "79c6db7d-6399-383e-bf5f-13d66223ae00",
                    "sense_definition": "The past participle of <w>do</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_form_standard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have <w>done</w> my homework already.",
                            "target_lang_translations": {
                                "ru": "Я уже <w>сделал</w> домашнее задание."
                            }
                        },
                        {
                            "text": "Nothing can be <w>done</w> to change the situation now.",
                            "target_lang_translations": {
                                "ru": "Ничего нельзя <w>сделать</w>, чтобы изменить ситуацию сейчас."
                            }
                        }
                    ],
                    "common_phrases": [
                        "have done",
                        "be done"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие прошедшего времени от глагола <w>do</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сделанный",
                                "target_lang_sense_clarification": "причастие"
                            },
                            {
                                "target_lang_word": "сделано",
                                "target_lang_sense_clarification": "краткая форма причастия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "454f8fba-4cdc-3dc9-8779-36096c22ab2e",
                    "sense_definition": "The simple past tense of <w>do</w> (did).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form_nonstandard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>done</w> it yesterday even though he was told not to.",
                            "target_lang_translations": {
                                "ru": "Он <w>сделал</w> это вчера, хотя ему говорили не делать."
                            }
                        },
                        {
                            "text": "I <w>done</w> my best, but it was not enough.",
                            "target_lang_translations": {
                                "ru": "Я <w>сделал</w> всё, что мог, но этого было недостаточно."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "non-standard"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Простое прошедшее время от <w>do</w> (did)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сделал",
                                "target_lang_sense_clarification": "просторечное употребление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa4c638e-c060-3baa-9251-d1ae039779f1",
                    "sense_definition": "Used as an auxiliary to emphasize that an action has been completely finished.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form_aave",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>done</w> left the house an hour ago.",
                            "target_lang_translations": {
                                "ru": "Она <w>уже</w> ушла из дома час назад."
                            }
                        },
                        {
                            "text": "They <w>done</w> told you the truth multiple times.",
                            "target_lang_translations": {
                                "ru": "Они <w>уже</w> говорили тебе правду много раз."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Southern US or AAVE"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как вспомогательный глагол, чтобы подчеркнуть, что действие полностью завершено."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уже",
                                "target_lang_sense_clarification": "для усиления завершенности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e67fe659-75d4-35f0-8bfe-bf2c08d25d65",
                    "sense_definition": "Fully finished or completed.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "adj_finished",
                    "name_type": "no",
                    "examples": [
                        {

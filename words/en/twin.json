                },
                {
                    "sense_id": "ae716668-fa13-3c42-88a9-4f5345dc20c8",
                    "sense_definition": "An aircraft that has two engines.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "aviation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot is experienced in flying a light <w>twin</w>.",
                            "target_lang_translations": {
                                "ru": "Пилот имеет опыт управления легким <w>двухмоторным самолетом</w>."
                            }
                        },
                        {
                            "text": "They took a small <w>twin</w> to reach the island.",
                            "target_lang_translations": {
                                "ru": "Они сели на небольшой <w>двухмоторный самолет</w>, чтобы добраться до острова."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Самолет, имеющий два двигателя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двухмоторный самолет",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {

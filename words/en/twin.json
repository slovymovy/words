                    ],
                    "common_phrases": [
                        "twin room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Номер в отеле с двумя отдельными односпальными кроватями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "номер с двумя кроватями",
                                "target_lang_sense_clarification": "описательный перевод"

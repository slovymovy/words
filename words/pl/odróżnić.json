    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e679557c-e129-3f4d-8d87-d0dff38098b5",
                    "sense_definition": "Zauważyć lub wskazać różnicę między dwiema osobami, rzeczami lub zjawiskami; rozpoznać coś jako odrębne.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czarnogłówkę z wyglądu bardzo trudno <w>odróżnić</w> od szarytki.",
                            "target_lang_translations": {
                                "ru": "Черноголовую гаичку по внешнему виду очень трудно <w>отличить</w> от буроголовой."
                            }
                        },
                        {
                            "text": "Musisz nauczyć się <w>odróżnić</w> prawdę od kłamstwa, zanim podejmiesz decyzję.",
                            "target_lang_translations": {
                                "ru": "Ты должен научиться <w>отличать</w> правду от лжи, прежде чем примешь решение."
                            }
                        },
                        {
                            "text": "Lekarz potrafił bezbłędnie <w>odróżnić</w> objawy grypy od zwykłego przeziębienia.",
                            "target_lang_translations": {
                                "ru": "Врач мог безошибочно <w>отличить</w> симптомы гриппа от обычной простуды."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozróżnić",
                        "oddzielić",
                        "zidentyfikować"
                    ],
                    "antonyms": [
                        "pomylić",
                        "utożsamić",
                        "zrównać"
                    ],
                    "common_phrases": [
                        "odróżnić prawdę od fałszu",
                        "odróżnić dobro od zła",
                        "trudno odróżnić",
                        "dać się odróżnić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заметить или указать разницу между двумя людьми, предметами или явлениями; распознать что-то как отдельное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отличить",
                                "target_lang_sense_clarification": "установить различие"
                            },
                            {
                                "target_lang_word": "различить",
                                "target_lang_sense_clarification": "распознать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b273266-ab50-3fb4-816e-887dd3754a87",
                    "sense_definition": "Sprawić, że ktoś lub coś staje się inne od otoczenia; wykazać cechy, które pozwalają na bycie zauważonym jako inny (często używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "distinction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy model samochodu <w>odróżnił się</w> od konkurencji nowoczesnym designem.",
                            "target_lang_translations": {
                                "ru": "Новая модель автомобиля <w>выделилась</w> на фоне конкурентов современным дизайном."
                            }
                        },
                        {
                            "text": "Chciał <w>odróżnić się</w> od reszty kandydatów swoją kreatywnością.",
                            "target_lang_translations": {
                                "ru": "Он хотел <w>выделиться</w> среди остальных кандидатов своей креативностью."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyróżnić się",
                        "wybić się"
                    ],
                    "antonyms": [
                        "upodobnić się",
                        "zlać się"
                    ],
                    "common_phrases": [
                        "odróżnić się od tła",
                        "odróżnić się od tłumu",
                        "odróżnić się stylem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать так, чтобы кто-то или что-то стало отличным от окружения; проявить черты, позволяющие быть замеченным (часто используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выделиться",
                                "target_lang_sense_clarification": "стать заметным на фоне других"
                            },
                            {
                                "target_lang_word": "отличиться",
                                "target_lang_sense_clarification": "проявить особенные черты"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
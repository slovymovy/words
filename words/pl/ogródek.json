            "senses": [
                {
                    "sense_id": "d9a67ef2-948b-3e7a-b6de-7633fdd3d3ae",
                    "sense_definition": "Mały <w>ogród</w>, zazwyczaj przydomowy, służący do uprawy roślin lub wypoczynku.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi dziadkowie mają piękny <w>ogródek</w> pełen kolorowych kwiatów.",
                            "target_lang_translations": {
                                "ru": "У моих дедушки и бабушки есть красивый <w>садик</w>, полный разноцветных цветов."
                            }
                        },
                        {
                            "text": "Wiosną spędzamy dużo czasu, porządkując nasz przydomowy <w>ogródek</w>.",
                            "target_lang_translations": {
                                "ru": "Весной мы проводим много времени, приводя в порядок наш приусадебный <w>садик</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogródeczek"
                    ],
                    "common_phrases": [
                        "ogródek działkowy",
                        "ogródek warzywny",
                        "prace w ogródku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький сад, обычно при доме, предназначенный для выращивания растений или отдыха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "садик",
                                "target_lang_sense_clarification": "уменьш. от сад; небольшой сад с цветами или деревьями"
                            },
                            {
                                "target_lang_word": "огородик",
                                "target_lang_sense_clarification": "уменьш. от огород; небольшое место для выращивания овощей"
                            },
                            {
                                "target_lang_word": "палисадник",
                                "target_lang_sense_clarification": "небольшой огороженный участок перед домом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc4a9014-a1a5-3503-8887-0c287e3de04a",
                    "sense_definition": "Wydzielona część na zewnątrz restauracji, kawiarni lub baru, gdzie klienci mogą spożywać posiłki i napoje.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ciepłe letnie wieczory lubimy siedzieć w <w>ogródku</w> naszej ulubionej kawiarni.",
                            "target_lang_translations": {
                                "ru": "Теплыми летними вечерами мы любим сидеть на <w>летней веранде</w> нашего любимого кафе."
                            }
                        },
                        {
                            "text": "Restauracja otworzyła <w>ogródek</w> piwny dla kibiców oglądających mecz.",
                            "target_lang_translations": {
                                "ru": "Ресторан открыл пивную <w>террасу</w> для болельщиков, смотрящих матч."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ogródek piwny",
                        "ogródek letni",
                        "siedzieć w ogródku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открытая площадка при ресторане, кафе или баре, где посетители могут есть и пить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "летняя веранда",
                                "target_lang_sense_clarification": "открытая пристройка к заведению"
                            },
                            {
                                "target_lang_word": "летняя терраса",
                                "target_lang_sense_clarification": "зона со столиками на свежем воздухе"
                            },
                            {
                                "target_lang_word": "дворик",
                                "target_lang_sense_clarification": "внутренний двор ресторана, используемый как зал"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
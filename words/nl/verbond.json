                {
                    "sense_id": "a9446f39-15a7-3f85-94fd-35555da9fe6a",
                    "sense_definition": "Een officieel verdrag of overeenkomst tussen staten, groepen of personen om samen te werken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "alliance_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De twee landen sloten een <w>verbond</w> om elkaar te verdedigen in tijden van oorlog.",
                            "target_lang_translations": {
                                "en": "The two countries concluded an <w>alliance</w> to defend each other in times of war.",
                                "ru": "Две страны заключили <w>союз</w>, чтобы защищать друг друга во время войны."
                            }
                        },
                        {
                            "text": "Door dit <w>verbond</w> konden de bedrijven hun krachten bundelen op de internationale markt.",
                            "target_lang_translations": {
                                "en": "Through this <w>alliance</w>, the companies were able to join forces on the international market.",
                                "ru": "Благодаря этому <w>альянсу</w> компании смогли объединить свои силы на международном рынке."
                            }
                        }
                    ],
                    "synonyms": [
                        "bondgenootschap",
                        "alliantie",
                        "coalitie"
                    ],
                    "common_phrases": [
                        "een verbond sluiten",
                        "een verbond verbreken"
                    ],
                    "target_lang_definitions": {
                        "en": "An official treaty or agreement between states, groups, or persons to work together.",
                        "ru": "Официальный договор или соглашение между государствами, группами или лицами о сотрудничестве."

                    "sense_id": "e708a1aa-df43-3884-9376-15c1751ff280",
                    "sense_definition": "De leiding, het bezit of de verantwoordelijkheid van iemand anders op zich nemen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn collega is ziek, dus ik moet vandaag zijn dienst <w>overnemen</w>.",
                            "target_lang_translations": {
                                "en": "My colleague is sick, so I have to <w>take over</w> his shift today.",
                                "ru": "Мой коллега заболел, поэтому сегодня я должен <w>взять на себя</w> его смену."
                            }
                        },
                        {
                            "text": "Na jaren van onderhandelingen heeft de multinational het kleine bedrijf <w>overgenomen</w>.",
                            "target_lang_translations": {
                                "en": "After years of negotiations, the multinational has <w>taken over</w> the small company.",
                                "ru": "После многих лет переговоров транснациональная корпорация <w>поглотила</w> эту маленькую компанию."
                            }
                        }
                    ],
                    "antonyms": [
                        "overdragen",
                        "afstaan"
                    ],
                    "common_phrases": [
                        "het roer overnemen",
                        "de leiding overnemen",
                        "een bedrijf overnemen",
                        "een dienst overnemen"
                    ],
                    "target_lang_definitions": {
                        "en": "To take on the leadership, possession, or responsibility of someone else.",
                        "ru": "Взять на себя руководство, владение или ответственность вместо кого-то другого."
                    },
                    "translations": {

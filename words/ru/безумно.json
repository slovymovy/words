                            {
                                "target_lang_word": "insane",
                                "target_lang_sense_clarification": "state of being mentally ill or extremely foolish"
                            },
                            {
                                "target_lang_word": "mad",
                                "target_lang_sense_clarification": "crazy or deranged"
                            }
                        ]
                    }
                }
            ]
        }
    ]

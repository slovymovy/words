                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкрутить",
                                "target_lang_sense_clarification": "лампочку, шуруп"
                            },
                            {
                                "target_lang_word": "ввинтить",
                                "target_lang_sense_clarification": "винт, деталь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b835bf86-3fa6-3295-9a3e-4e16b9a0e367",
                    "sense_definition": "Oszukać kogoś w żartobliwy sposób lub sprawić, że ktoś uwierzy w nieprawdę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wierzę ci, na pewno znowu chcesz mnie <w>wkręcić</w>.",

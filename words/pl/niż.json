    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "fe8fa704-bd3e-3296-a18c-7a1c32976f26",
                    "sense_definition": "Słowo łączące dwa elementy w porównaniach, wskazujące na różnicę między nimi; używane najczęściej po przymiotnikach lub przysłówkach w stopniu wyższym.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest znacznie wyższy <w>niż</w> ja.",
                            "target_lang_translations": {
                                "ru": "Мой брат значительно выше, <w>чем</w> я."
                            }
                        },
                        {
                            "text": "To zadanie okazało się łatwiejsze, <w>niż</w> myślałem na początku.",
                            "target_lang_translations": {
                                "ru": "Это задание оказалось легче, <w>чем</w> я думал вначале."
                            }
                        },
                        {
                            "text": "Wolę pić kawę <w>niż</w> herbatę.",
                            "target_lang_translations": {
                                "ru": "Я больше люблю пить кофе, <w>чем</w> чай."
                            }
                        }
                    ],
                    "synonyms": [
                        "aniżeli",
                        "od"
                    ],
                    "common_phrases": [
                        "więcej niż",
                        "mniej niż",
                        "raczej niż",
                        "lepszy niż",
                        "nic innego niż"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, соединяющий два элемента при сравнении, указывающий на различие между ними; чаще всего используется после прилагательных или наречий в сравнительной степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чем",
                                "target_lang_sense_clarification": "основной союз для сравнения"
                            },
                            {
                                "target_lang_word": "нежели",
                                "target_lang_sense_clarification": "книжный или устаревший вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55a00b80-b67b-3ac4-97c0-f2237359afd2",
                    "sense_definition": "Dawne określenie oznaczające: zanim; używane w literaturze lub stylizacjach językowych.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "time_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uciekł w las, <w>niż</w> nadeszła królewska pogoń.",
                            "target_lang_translations": {
                                "ru": "Он убежал в лес, <w>прежде чем</w> подоспела королевская погоня."
                            }
                        },
                        {
                            "text": "Muszę skończyć pracę, <w>niż</w> słońce zajdzie.",
                            "target_lang_translations": {
                                "ru": "Я должен закончить работу, <w>пока</w> солнце <w>не</w> зашло."
                            }
                        }
                    ],
                    "synonyms": [
                        "zanim"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie zastąpione przez 'zanim'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее слово, означающее «прежде чем» или «пока не»; используется в литературе или стилизациях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прежде чем",
                                "target_lang_sense_clarification": "временной союз"
                            },
                            {
                                "target_lang_word": "пока не",
                                "target_lang_sense_clarification": "в значении ожидания события"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9a25aa40-d9bd-39ce-966e-02f2a4fc0106",
                    "sense_definition": "Układ baryczny w atmosferze, charakteryzujący się niższym ciśnieniem w centrum niż na peryferiach, często przynoszący zachmurzenie i opady.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "meteorology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jutro nad Polskę nadciągnie deszczowy <w>niż</w> znad Atlantyku.",
                            "target_lang_translations": {
                                "ru": "Завтра на Польшу надвинется дождливый <w>циклон</w> с Атлантики."
                            }
                        },
                        {
                            "text": "Barometry wskazują głęboki <w>niż</w>, więc pogoda się pogorszy.",
                            "target_lang_translations": {
                                "ru": "Барометры показывают глубокий <w>циклон</w>, поэтому погода ухудшится."
                            }
                        }
                    ],
                    "synonyms": [
                        "cyklon",
                        "depresja"
                    ],
                    "antonyms": [
                        "wyż"
                    ],
                    "common_phrases": [
                        "niż atmosferyczny",
                        "niż baryczny",
                        "głęboki niż",
                        "centrum niżu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Барическая система в атмосфере, характеризующаяся более низким давлением в центре, чем на периферии; часто приносит облачность и осадки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "циклон",
                                "target_lang_sense_clarification": "метеорологический термин"
                            },
                            {
                                "target_lang_word": "область низкого давления",
                                "target_lang_sense_clarification": "описательный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b44f656b-aeef-3129-9463-26d0ae981f03",
                    "sense_definition": "Czasowy spadek liczebności jakiejś grupy lub wartości zjawiska (często w kontekście demografii).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "statistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele szkół jest zamykanych, ponieważ wchodzimy w <w>niż</w> demograficzny.",
                            "target_lang_translations": {
                                "ru": "Многие школы закрываются, так как мы входим в демографический <w>спад</w>."
                            }
                        },
                        {
                            "text": "Obecny <w>niż</w> na rynku pracy martwi ekonomistów.",
                            "target_lang_translations": {
                                "ru": "Нынешний <w>спад</w> на рынке труда беспокоит экономистов."
                            }
                        }
                    ],
                    "synonyms": [
                        "minimum",
                        "zanim",
                        "depresja"
                    ],
                    "antonyms": [
                        "wyż"
                    ],
                    "common_phrases": [
                        "niż demograficzny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временное снижение численности какой-либо группы или значения явления (часто в контексте демографии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спад",
                                "target_lang_sense_clarification": "снижение показателей"
                            },
                            {
                                "target_lang_word": "яма",
                                "target_lang_sense_clarification": "в контексте демографии (демографическая яма)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e5aea56-a189-3006-b9a0-ac60a2e3a52d",
                    "sense_definition": "Rozległy obszar nizinny, położony nisko względem poziomu morza.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Grab pospolity występuje naturalnie na całym <w>niżu</w>.",
                            "target_lang_translations": {
                                "ru": "Граб обыкновенный встречается в природе на всей <w>низменности</w>."
                            }
                        },
                        {
                            "text": "Krajobraz <w>niżu</w> jest zazwyczaj płaski i monotonny.",
                            "target_lang_translations": {
                                "ru": "Ландшафт <w>низменности</w> обычно плоский и однообразный."
                            }
                        }
                    ],
                    "synonyms": [
                        "nizina"
                    ],
                    "antonyms": [
                        "wyżyna",
                        "wyż"
                    ],
                    "common_phrases": [
                        "na niżu",
                        "niż środkowoeuropejski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обширная равнинная территория, расположенная низко относительно уровня моря."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "низменность",
                                "target_lang_sense_clarification": "географический термин"
                            },
                            {
                                "target_lang_word": "низина",
                                "target_lang_sense_clarification": "низкое место"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b089cdae-80e2-315f-aeab-42dc2b9b0202",
                    "sense_definition": "Forma czasownika <w>nizać</w> (tryb rozkazujący, 2. osoba liczby pojedynczej); polecenie nawlekania czegoś (np. koralików) na nitkę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_imperative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Niż</w> te koraliki ostrożnie, żeby ich nie pogubić.",
                            "target_lang_translations": {
                                "ru": "<w>Нанизывай</w> эти бусины осторожно, чтобы их не растерять."
                            }
                        },
                        {
                            "text": "Siedź spokojnie i <w>niż</w> grzyby na sznurek do suszenia.",
                            "target_lang_translations": {
                                "ru": "Сиди спокойно и <w>нанизывай</w> грибы на веревку для сушки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "tryb rozkazujący od: nizać"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола «nizać» (повелительное наклонение, 2-е лицо ед. ч.); команда нанизывать что-либо (например, бусины) на нитку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нанизывай",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

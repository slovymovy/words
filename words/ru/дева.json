    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "18e62e70-ef1e-3201-b368-907ba1d5b187",
                    "sense_definition": "Лицо женского пола, достигшее половой зрелости, но не состоящее в браке; девушка (обычно в поэтической речи или устаревшее).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "maiden_girl",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В высокой башне жила прекрасная <w>дева</w>, ожидающая своего спасителя.",
                            "target_lang_translations": {
                                "en": "A beautiful <w>maiden</w> lived in a high tower, awaiting her savior."
                            }
                        },
                        {
                            "text": "Её называли старой <w>девой</w>, потому что она так и не вышла замуж.",
                            "target_lang_translations": {
                                "en": "She was called an old <w>maid</w> because she never got married."
                            }
                        }
                    ],
                    "synonyms": [
                        "девушка",
                        "девица"
                    ],
                    "antonyms": [
                        "юноша",
                        "женщина"
                    ],
                    "common_phrases": [
                        "старая дева",
                        "прекрасная дева",
                        "юная дева"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "In modern Russian, mostly used in poetry, fantasy contexts, or the idiom 'старая дева'."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An unmarried female who has reached puberty; a girl (usually in poetic speech or archaic)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "maiden",
                                "target_lang_sense_clarification": "literary or archaic term for a young unmarried woman"
                            },
                            {
                                "target_lang_word": "damsel",
                                "target_lang_sense_clarification": "archaic term for a young unmarried woman"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1555105-478b-354b-8f1f-24e6eb03758a",
                    "sense_definition": "Женщина, не имевшая половых сношений; девственница.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "virgin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она решила посвятить себя служению Богу и остаться <w>девой</w>.",
                            "target_lang_translations": {
                                "en": "She decided to dedicate herself to serving God and remain a <w>virgin</w>."
                            }
                        },
                        {
                            "text": "В древних легендах только чистая <w>дева</w> могла приручить единорога.",
                            "target_lang_translations": {
                                "en": "In ancient legends, only a pure <w>virgin</w> could tame a unicorn."
                            }
                        }
                    ],
                    "synonyms": [
                        "девственница"
                    ],
                    "antonyms": [
                        "женщина"
                    ],
                    "common_phrases": [
                        "непорочная дева",
                        "чистая дева"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Modern term is 'девственница'."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A woman who has not had sexual intercourse; a virgin."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "virgin",
                                "target_lang_sense_clarification": "a person who has never had sexual intercourse"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "24f7a336-cfc4-34ee-8550-0a48e258cf36",
                    "sense_definition": "Шестой знак зодиака (с 23 августа по 22 сентября), изображаемый в виде девушки; человек, родившийся под этим знаком.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "zodiac",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Моя сестра по гороскопу — <w>Дева</w>, она очень любит порядок.",
                            "target_lang_translations": {
                                "en": "My sister is a <w>Virgo</w> according to the horoscope; she loves order very much."
                            }
                        },
                        {
                            "text": "Астрологи говорят, что <w>Дева</w> — это земной знак зодиака.",
                            "target_lang_translations": {
                                "en": "Astrologers say that <w>Virgo</w> is an earth sign of the zodiac."
                            }
                        }
                    ],
                    "synonyms": [
                        "♍"
                    ],
                    "antonyms": [
                        "Ры́бы"
                    ],
                    "common_phrases": [
                        "знак Девы",
                        "под знаком Девы"
                    ],
                    "target_lang_definitions": {
                        "en": "The sixth sign of the zodiac (from August 23 to September 22), depicted as a maiden; a person born under this sign."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Virgo",
                                "target_lang_sense_clarification": "zodiac sign"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5de7d4d4-8a31-3de6-85fb-eb0e20daf3ce",
                    "sense_definition": "В христианстве: Богородица, мать Иисуса Христа (обычно употребляется как Дева Мария).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "religious",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "В церкви находится старинная икона, на которой изображена Пресвятая <w>Дева</w>.",
                            "target_lang_translations": {
                                "en": "In the church, there is an ancient icon depicting the Blessed <w>Virgin</w>."
                            }
                        },
                        {
                            "text": "<w>Дева</w> Мария почитается христианами по всему миру.",
                            "target_lang_translations": {
                                "en": "The <w>Virgin</w> Mary is venerated by Christians all over the world."
                            }
                        }
                    ],
                    "synonyms": [
                        "девушка",
                        "девственница"
                    ],
                    "common_phrases": [
                        "Дева Мария",
                        "Пресвятая Дева"
                    ],
                    "target_lang_definitions": {
                        "en": "In Christianity: the Mother of God, mother of Jesus Christ (usually used as Virgin Mary)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Virgin",
                                "target_lang_sense_clarification": "referring to the Virgin Mary"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db012409-aea8-3f40-ae24-61f9aef4264a",
                    "sense_definition": "Экваториальное зодиакальное созвездие, лежащее между Львом и Весами.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "constellation",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Весной на ночном небе хорошо видно созвездие <w>Девы</w>.",
                            "target_lang_translations": {
                                "en": "In spring, the constellation of <w>Virgo</w> is clearly visible in the night sky."
                            }
                        },
                        {
                            "text": "Самая яркая звезда в созвездии <w>Девы</w> называется Спика.",
                            "target_lang_translations": {
                                "en": "The brightest star in the constellation of <w>Virgo</w> is called Spica."
                            }
                        }
                    ],
                    "synonyms": [
                        "Vir"
                    ],
                    "common_phrases": [
                        "созвездие Девы"
                    ],
                    "target_lang_definitions": {
                        "en": "An equatorial zodiacal constellation lying between Leo and Libra."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Virgo",
                                "target_lang_sense_clarification": "constellation"
                            }
                        ]
                    }
                }
            ]
        }
    ]

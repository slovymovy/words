                            },
                            {
                                "target_lang_word": "all the same",
                                "target_lang_sense_clarification": "in the final analysis"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "в конечном счёте",
                                "target_lang_sense_clarification": "подводя итог"
                            },
                            {
                                "target_lang_word": "в общем",
                                "target_lang_sense_clarification": "рассматривая ситуацию в целом"
                            }
                        ]
                    }
                }
            ]
        }
    ]

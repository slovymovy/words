{
    "word_family": [
        "jak",
        "jakoś",
        "jakość",
        "jakikolwiek",
        "jako"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "cfd87f86-bda5-37a5-96b6-d12af6a43e23",
                    "sense_definition": "Słowo wzmacniające, używane w zdaniach wykrzyknikowych do podkreślenia wysokiego stopnia jakiejś cechy lub intensywności zjawiska; wyraża podziw, zdziwienie lub żal.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emphatic_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Jakże</w> głośno potem kichali!",
                            "target_lang_translations": {
                                "ru": "<w>Как же</w> громко они потом чихали!"
                            }
                        },
                        {
                            "text": "<w>Jakże</w> byłem głupi, wierząc w te kłamstwa!",
                            "target_lang_translations": {
                                "ru": "<w>Как же</w> я был глуп, поверив в эту ложь!"
                            }
                        },
                        {
                            "text": "<w>Jakże</w> piękny jest ten widok o poranku.",
                            "target_lang_translations": {
                                "ru": "<w>Как же</w> прекрасен этот вид по утрам."
                            }
                        }
                    ],
                    "synonyms": [
                        "jak",
                        "jakżeż"
                    ],
                    "common_phrases": [
                        "a jakże",
                        "jakże bardzo"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Усилительное слово, используемое в восклицательных предложениях для подчеркивания высокой степени признака или интенсивности явления; выражает восхищение, удивление или сожаление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как же",
                                "target_lang_sense_clarification": "усиление эмоции"
                            },
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "в восклицаниях"
                            },
                            {
                                "target_lang_word": "сколь",
                                "target_lang_sense_clarification": "книжное, высокая степень"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e435fc06-098b-3c72-b286-89b8ce9f1e20",
                    "sense_definition": "Słowo służące do wyrażenia oburzenia, gwałtownego sprzeciwu, niedowierzania lub zdziwienia wobec zaistniałej sytuacji.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emphatic_indignation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Jakże</w> mogłeś to zrobić swojemu przyjacielowi?",
                            "target_lang_translations": {
                                "ru": "<w>Как же</w> ты мог сделать это своему другу?"
                            }
                        },
                        {
                            "text": "<w>Jakże</w> to, nie wiedziałeś o tym spotkaniu?",
                            "target_lang_translations": {
                                "ru": "<w>Как же</w> так, ты не знал об этой встрече?"
                            }
                        }
                    ],
                    "synonyms": [
                        "jak to",
                        "gdzież"
                    ],
                    "common_phrases": [
                        "jakże by inaczej",
                        "jakże to"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, выражающее возмущение, резкое возражение, недоверие или удивление по поводу сложившейся ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как же",
                                "target_lang_sense_clarification": "возмущение или недоумение"
                            },
                            {
                                "target_lang_word": "да как",
                                "target_lang_sense_clarification": "сильное возмущение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "57cdd18e-adfe-3fca-aebd-3e243f718c58",
                    "sense_definition": "Słowo pytające o sposób wykonania czynności lub stan rzeczy, nadające wypowiedzi charakter podniosły lub archaiczny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "interrogative_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Jakże</w> ty się tutaj dostałeś bez klucza?",
                            "target_lang_translations": {
                                "ru": "<w>Как же</w> ты сюда попал без ключа?"
                            }
                        },
                        {
                            "text": "<w>Jakże</w> go do tego namówić, skoro jest taki uparty?",
                            "target_lang_translations": {
                                "ru": "<w>Как же</w> его уговорить, если он такой упрямый?"
                            }
                        },
                        {
                            "text": "<w>Jakże</w> tam pański pokój, czy jest wygodny?",
                            "target_lang_translations": {
                                "ru": "<w>Как</w> там ваша комната, удобна ли?"
                            }
                        }
                    ],
                    "synonyms": [
                        "jak",
                        "jakżeż"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "in standard speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вопросительное слово, спрашивающее о способе действия или состоянии, придающее высказыванию возвышенный или архаичный характер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как же",
                                "target_lang_sense_clarification": "вопрос с оттенком раздумья"
                            },
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "вопрос о способе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "218a0e17-ea7b-3a0b-999d-c2c2f8385066",
                    "sense_definition": "Tłuste mięso wieprzowe pochodzące z boku tuszy, często wędzone lub parzone, używane jako składnik potraw.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na śniadanie często jemy jajecznicę ze smażonym <w>boczkiem</w> i cebulą.",
                            "target_lang_translations": {
                                "ru": "На завтрак мы часто едим яичницу с жареным <w>беконом</w> и луком."
                            }
                        },
                        {
                            "text": "Do przygotowania tego dania potrzebny będzie chudy wędzony <w>boczek</w>.",
                            "target_lang_translations": {
                                "ru": "Для приготовления этого блюда понадобится нежирная копченая <w>грудинка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bekon"
                    ],
                    "common_phrases": [
                        "wędzony boczek",
                        "jajecznica z boczkiem",
                        "plastry boczku",
                        "boczek parzony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жирная свинина с бока туши, часто копченая или вареная, используемая как ингредиент блюд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бекон",
                                "target_lang_sense_clarification": "копченое мясо"
                            },
                            {
                                "target_lang_word": "грудинка",
                                "target_lang_sense_clarification": "часть туши"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49e0d771-698e-3768-8444-88009aa38265",
                    "sense_definition": "Zdrobnienie od słowa <w>bok</w>; określenie używane często w kontekście pozycji ciała podczas snu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe dziecko smacznie spało na prawym <w>boczku</w> w swoim łóżeczku.",
                            "target_lang_translations": {
                                "ru": "Маленький ребенок сладко спал на правом <w>бочке</w> в своей кроватке."
                            }
                        },
                        {
                            "text": "Kot przewrócił się na <w>boczek</w>, domagając się głaskania po brzuchu.",
                            "target_lang_translations": {
                                "ru": "Кот перевернулся на <w>бочок</w>, требуя, чтобы ему погладили живот."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spać na boczku",
                        "przewrócić się na boczek",
                        "leżeć na boczku"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова «бок»; часто используется в контексте положения тела во время сна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бочок",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "242b56f6-c193-359c-908e-bd18bd6bb904",
                    "sense_definition": "Nadmiar tkanki tłuszczowej odkłądający się po bokach tułowia nad biodrami.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Regularne ćwiczenia na siłowni pomogą ci szybko zrzucić niechciane <w>boczki</w>.",
                            "target_lang_translations": {
                                "ru": "Регулярные упражнения в спортзале помогут тебе быстро убрать нежелательные <w>бока</w>."
                            }
                        },
                        {
                            "text": "Moje <w>boczki</w> stały się bardziej widoczne, odkąd przestałem dbać o dietę.",
                            "target_lang_translations": {
                                "ru": "Мои <w>бока</w> стали заметнее, с тех пор как я перестал следить за диетой."
                            }
                        }
                    ],
                    "synonyms": [
                        "oponka"
                    ],
                    "common_phrases": [
                        "zrzucić boczki",
                        "ćwiczenia na boczki",
                        "mieć boczki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Избыток жировой ткани, откладывающийся по бокам туловища над бедрами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бока",
                                "target_lang_sense_clarification": "жировые складки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba9a09f3-809c-3bf1-8d1b-420d4f04bc02",
                    "sense_definition": "Boczna, wydzielona część tabeli zawierająca opisy lub nagłówki wierszy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W <w>boczku</w> tabeli należy wpisać nazwiska wszystkich pracowników działu.",
                            "target_lang_translations": {
                                "ru": "В <w>боковик</w> таблицы следует вписать фамилии всех сотрудников отдела."
                            }
                        },
                        {
                            "text": "Sprawdź, czy tekst w <w>boczku</w> jest poprawnie sformatowany i czytelny.",
                            "target_lang_translations": {
                                "ru": "Проверь, правильно ли отформатирован и читаем текст в <w>боковике</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "boczek tabeli",
                        "tekst w boczku"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical/printing context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Боковая, выделенная часть таблицы, содержащая описания или заголовки строк."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "боковик",
                                "target_lang_sense_clarification": "элемент таблицы"
                            }
                        ]
                    }
                }
            ]
        },
        {

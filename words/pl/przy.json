{
    "word_family": [
        "przyimek"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "e300d97a-3fcc-3151-a757-32611218bfc8",
                    "sense_definition": "Wskazuje na miejsce znajdujące się bardzo blisko czegoś lub kogoś; określa bezpośrednie sąsiedztwo.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj wieczorem cała nasza rodzina siedziała <w>przy</w> stole.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером вся наша семья сидела <w>за</w> столом."
                            }
                        },
                        {
                            "text": "Mój nowy dom stoi <w>przy</w> głównej ulicy w mieście.",
                            "target_lang_translations": {
                                "ru": "Мой новый дом стоит <w>на</w> главной улице города."
                            }
                        }
                    ],
                    "synonyms": [
                        "obok",
                        "blisko",
                        "niedaleko"
                    ],
                    "common_phrases": [
                        "przy stole",
                        "przy oknie",
                        "przy drodze",
                        "przy biurku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на место, находящееся очень близко к чему-либо или кому-либо; обозначает непосредственную близость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "у",
                                "target_lang_sense_clarification": "возле, рядом с"
                            },
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "столом, партой"
                            },
                            {
                                "target_lang_word": "возле",
                                "target_lang_sense_clarification": "в непосредственной близости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15730f27-22e6-3668-85b6-31b1a00cfe3c",
                    "sense_definition": "Wskazuje na czas trwania jakiejś czynności lub okoliczności towarzyszące innemu wydarzeniu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_circumstance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Często rozmawiamy o naszych planach na wakacje <w>przy</w> obiedzie.",
                            "target_lang_translations": {
                                "ru": "Мы часто говорим о наших планах на отпуск <w>за</w> обедом."
                            }
                        },
                        {
                            "text": "Musimy kiedyś spokojnie porozmawiać o tym problemie <w>przy</w> kawie.",
                            "target_lang_translations": {
                                "ru": "Нам нужно когда-нибудь спокойно поговорить об этой проблеме <w>за</w> чашкой кофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "podczas"
                    ],
                    "common_phrases": [
                        "przy jedzeniu",
                        "przy kawie",
                        "przy okazji",
                        "przy pracy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на время действия или обстоятельства, сопровождающие другое событие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "едой, напитками"
                            },
                            {
                                "target_lang_word": "во время",
                                "target_lang_sense_clarification": "в ходе чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ebffa9d-aadf-317c-bb00-4cbe6778019f",
                    "sense_definition": "Wskazuje na obecność osoby, w której towarzystwie coś się dzieje lub która jest świadkiem zdarzenia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "presence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę, nie używaj takich brzydkich słów <w>przy</w> małych dzieciach.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не употребляй таких плохих слов <w>при</w> маленьких детях."
                            }
                        },
                        {
                            "text": "On zawsze zachowuje się bardzo kulturalnie <w>przy</w> obcych ludziach.",
                            "target_lang_translations": {
                                "ru": "Он всегда ведёт себя очень культурно <w>при</w> посторонних."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przy dzieciach",
                        "przy świadkach",
                        "przy ludziach",
                        "przy mnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на присутствие лица, при котором что-либо происходит или которое является свидетелем события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "при",
                                "target_lang_sense_clarification": "в присутствии кого-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c11a635-fba9-358e-9dbd-8393a648753a",
                    "sense_definition": "Wskazuje na czynność, pracę lub zadanie, które ktoś aktualnie wykonuje.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat pracuje latem <w>przy</w> zbiorach truskawek za granicą.",
                            "target_lang_translations": {
                                "ru": "Мой брат работает летом <w>на</w> сборе клубники за границей."
                            }
                        },
                        {
                            "text": "Spędziłem wiele godzin <w>przy</w> pisaniu tej trudnej pracy dyplomowej.",
                            "target_lang_translations": {
                                "ru": "Я провёл много часов <w>за</w> написанием этой сложной дипломной работы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przy budowie",
                        "przy projekcie",
                        "przy zbiorach",
                        "przy komputerze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на действие, работу или задачу, которую кто-то выполняет в данный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "работе, сборе урожая"
                            },
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "занятием, работой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93246d6f-f4f8-38d9-b33a-9d2ed87956d5",
                    "sense_definition": "Określa warunki, parametry lub stan, w jakich coś zachodzi (np. prędkość, oświetlenie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten samochód wpadł w poślizg <w>przy</w> bardzo dużej prędkości.",
                            "target_lang_translations": {
                                "ru": "Этот автомобиль занесло <w>на</w> очень большой скорости."
                            }
                        },
                        {
                            "text": "Trudno się czyta książkę wieczorem <w>przy</w> tak słabym świetle.",
                            "target_lang_translations": {
                                "ru": "Трудно читать книгу вечером <w>при</w> таком слабом свете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przy prędkości",
                        "przy świetle",
                        "przy założeniu",
                        "przy użyciu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определяет условия, параметры или состояние, в которых что-либо происходит (например, скорость, освещение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "при",
                                "target_lang_sense_clarification": "условиях, свете"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "скорости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0be3465e-ef9b-3a85-a3e0-a96e3d4b6fb0",
                    "sense_definition": "Wskazuje na element będący częścią czegoś większego lub znajdujący się bezpośrednio na czymś.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "attachment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj oderwał mi się duży guzik <w>przy</w> moim ulubionym swetrze.",
                            "target_lang_translations": {
                                "ru": "Вчера у меня оторвалась большая пуговица <w>на</w> моём любимом свитере."
                            }
                        },
                        {
                            "text": "Ten mały budynek wygląda jak dobudówka <w>przy</w> wielkim pałacu.",
                            "target_lang_translations": {
                                "ru": "Это маленькое здание выглядит как пристройка <w>при</w> огромном дворце."
                            }
                        }
                    ],
                    "common_phrases": [
                        "guzik przy płaszczu",
                        "kieszeń przy spodniach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на элемент, являющийся частью чего-то большего или находящийся непосредственно на чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "одежде, поверхности"
                            },
                            {
                                "target_lang_word": "при",
                                "target_lang_sense_clarification": "как часть строения или организации"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4aa5c540-d725-30f5-a525-417cb7dbcba0",
                    "sense_definition": "Wettelijk een kind (of dier) aannemen en verzorgen alsof het het eigen kind is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij hebben besloten om een kind uit het buitenland te <w>adopteren</w>.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>усыновить</w> ребенка из-за границы.",
                                "en": "They have decided to <w>adopt</w> a child from abroad."
                            }
                        },
                        {
                            "text": "Mijn buren gaan een hond uit het asiel <w>adopteren</w>.",
                            "target_lang_translations": {
                                "ru": "Мои соседи собираются <w>взять</w> собаку из приюта.",
                                "en": "My neighbors are going to <w>adopt</w> a dog from the shelter."
                            }
                        }
                    ],
                    "synonyms": [
                        "aannemen"
                    ],
                    "common_phrases": [
                        "een kind adopteren",
                        "een hond adopteren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Законно принять ребенка (или животное) и заботиться о нем как о своем собственном.",
                        "en": "To legally take a child (or animal) and care for it as one's own."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усыновлять",
                                "target_lang_sense_clarification": "о ребенке"
                            },
                            {
                                "target_lang_word": "брать",
                                "target_lang_sense_clarification": "о животном (из приюта)"
                            },
                            {
                                "target_lang_word": "удочерять",
                                "target_lang_sense_clarification": "о девочке"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "adopt",
                                "target_lang_sense_clarification": "a child"
                            },
                            {
                                "target_lang_word": "adopt",
                                "target_lang_sense_clarification": "a pet or animal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa70371f-ffa6-3c1f-80b8-54a81f0957ea",
                    "sense_definition": "Een idee, methode of object (zoals een monument) overnemen of onder de eigen verantwoordelijkheid nemen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De lokale school heeft het oorlogsmonument in het park <w>geadopteerd</w>.",
                            "target_lang_translations": {
                                "ru": "Местная школа <w>взяла шефство</w> над военным памятником в парке.",
                                "en": "The local school has <w>adopted</w> the war memorial in the park."
                            }
                        },
                        {
                            "text": "Het bedrijf wil deze nieuwe strategie snel <w>adopteren</w>.",
                            "target_lang_translations": {
                                "ru": "Компания хочет быстро <w>внедрить</w> эту новую стратегию.",
                                "en": "The company wants to <w>adopt</w> this new strategy quickly."
                            }
                        }
                    ],
                    "synonyms": [
                        "overnemen",
                        "omarmen",
                        "aannemen"
                    ],
                    "antonyms": [
                        "verwerpen"
                    ],
                    "common_phrases": [
                        "een monument adopteren",
                        "een werkwijze adopteren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перенять идею, метод или взять объект (например, памятник) под свою ответственность.",
                        "en": "To accept an idea, method, or object (like a monument) or take it under one's responsibility."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перенимать",
                                "target_lang_sense_clarification": "идею или метод"
                            },
                            {
                                "target_lang_word": "брать шефство",
                                "target_lang_sense_clarification": "над памятником или объектом"
                            },
                            {
                                "target_lang_word": "внедрять",
                                "target_lang_sense_clarification": "технологию или стратегию"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "adopt",
                                "target_lang_sense_clarification": "an idea, policy, or strategy"
                            },
                            {
                                "target_lang_word": "adopt",
                                "target_lang_sense_clarification": "responsibility for an object or monument"
                            }
                        ]
                    }
                }
            ]
        }

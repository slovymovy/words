    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9bf349ab-ace3-3c00-b469-cba54c0a6875",
                    "sense_definition": "Неприятное, нежелательное событие или происшествие, которое вызывает огорчение или создает трудности.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за опоздания на работу у меня случилась крупная <w>неприятность</w>.",
                            "target_lang_translations": {
                                "en": "Because of being late for work, I got into serious <w>trouble</w>."
                            }
                        },
                        {
                            "text": "Мы стараемся избегать лишних <w>неприятностей</w> с соседями.",
                            "target_lang_translations": {
                                "en": "We try to avoid unnecessary <w>trouble</w> with the neighbors."
                            }
                        }
                    ],
                    "synonyms": [
                        "огорчение",
                        "затруднение"
                    ],
                    "antonyms": [
                        "радость"
                    ],
                    "common_phrases": [
                        "мелкие неприятности",
                        "нажить неприятностей",
                        "попасть в неприятность",
                        "доставлять неприятности"
                    ],
                    "target_lang_definitions": {
                        "en": "An unpleasant, undesirable event or incident that causes distress or creates difficulties."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trouble",
                                "target_lang_sense_clarification": "difficult or distressing situation"
                            },
                            {
                                "target_lang_word": "problem",
                                "target_lang_sense_clarification": "source of difficulty or worry"
                            },
                            {
                                "target_lang_word": "unpleasantness",
                                "target_lang_sense_clarification": "unpleasant occurrence or quarrel"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c44c8eb5-7667-3afa-b6d7-5a17983229b9",
                    "sense_definition": "Свойство или качество чего-либо, вызывающее отрицательные чувства или неприязнь.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В его голосе чувствовалась какая-то скрытая <w>неприятность</w>.",
                            "target_lang_translations": {
                                "en": "Some hidden <w>unpleasantness</w> was felt in his voice."
                            }
                        },
                        {
                            "text": "Общая <w>неприятность</w> этой ситуации угнетала всех присутствующих.",
                            "target_lang_translations": {
                                "en": "The general <w>unpleasantness</w> of this situation depressed everyone present."
                            }
                        }
                    ],
                    "synonyms": [
                        "отвратительность"
                    ],
                    "antonyms": [
                        "приятность"
                    ],
                    "common_phrases": [
                        "неприятность тона",
                        "неприятность лица"
                    ],
                    "target_lang_definitions": {
                        "en": "The property or quality of something that causes negative feelings or dislike."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unpleasantness",
                                "target_lang_sense_clarification": "quality of being disagreeable"
                            },
                            {
                                "target_lang_word": "disagreeableness",
                                "target_lang_sense_clarification": "quality of not being enjoyable"
                            }
                        ]
                    }
                }

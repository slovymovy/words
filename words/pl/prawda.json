    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c7685e82-54a9-3e41-ba9e-64ccd7306cf6",
                    "sense_definition": "To, co jest zgodne z rzeczywistością; faktyczny stan rzeczy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "reality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To, co usłyszałeś od niego, to <w>szczera prawda</w>.",
                            "target_lang_translations": {
                                "ru": "То, что ты услышал от него, — <w>чистая правда</w>."
                            }
                        },
                        {
                            "text": "Musisz mi wreszcie wyznać całą <w>prawdę</w> o tym zdarzeniu.",
                            "target_lang_translations": {
                                "ru": "Ты должен наконец рассказать мне всю <w>правду</w> об этом происшествии."
                            }
                        }
                    ],
                    "synonyms": [
                        "fakt"
                    ],
                    "antonyms": [
                        "kłamstwo",
                        "nieprawda",
                        "fałsz",
                        "bujda"
                    ],
                    "common_phrases": [
                        "szczera prawda",
                        "mówić prawdę",
                        "gorzka prawda",
                        "poznać prawdę",
                        "co prawda"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что соответствует действительности; фактическое положение дел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правда",
                                "target_lang_sense_clarification": "то, что соответствует истине"
                            },
                            {
                                "target_lang_word": "истина",
                                "target_lang_sense_clarification": "объективная реальность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e0ace4f-6ae8-3320-8884-b0ddc1ecc376",
                    "sense_definition": "Słuszność jakiegoś twierdzenia, opinii lub postawy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "rightness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym sporze to ona ma <w>prawdę</w>, a nie ty.",
                            "target_lang_translations": {
                                "ru": "В этом споре <w>правда</w> на её стороне, а не на твоей."
                            }
                        },
                        {
                            "text": "Trudno odmówić mu <w>prawdy</w>, gdy mówi o problemach firmy.",
                            "target_lang_translations": {
                                "ru": "Трудно отказать ему в <w>правоте</w>, когда он говорит о проблемах фирмы."
                            }
                        }
                    ],
                    "synonyms": [
                        "racja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Правильность какого-либо утверждения, мнения или позиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правота",
                                "target_lang_sense_clarification": "справедливость, правильность действий"
                            },
                            {
                                "target_lang_word": "правда",
                                "target_lang_sense_clarification": "разг. справедливость, напр. «твоя правда»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fe433a4-38cc-3974-9155-1b1e8c7c59e6",
                    "sense_definition": "W logice: jedna z dwóch wartości logicznych, przeciwieństwo fałszu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli zdanie nie jest fałszem, to w logice klasycznej musi być <w>prawdą</w>.",
                            "target_lang_translations": {
                                "ru": "Если высказывание не является ложью, то в классической логике оно должно быть <w>истиной</w>."
                            }
                        },
                        {
                            "text": "Funkcja zwraca <w>prawdę</w>, gdy podany warunek jest spełniony.",
                            "target_lang_translations": {
                                "ru": "Функция возвращает <w>истину</w>, когда заданное условие выполнено."
                            }
                        }
                    ],
                    "antonyms": [
                        "fałsz"
                    ],
                    "target_lang_definitions": {
                        "ru": "В логике: одно из двух логических значений, противоположность лжи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "истина",
                                "target_lang_sense_clarification": "логическое значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "6001bdd1-787a-3483-a72c-118118b650d2",
                    "sense_definition": "Słowo używane na końcu zdania w celu uzyskania potwierdzenia od rozmówcy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "tag_question",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jutro jest dzień wolny od pracy, <w>prawda</w>?",
                            "target_lang_translations": {
                                "ru": "Завтра выходной, <w>правда</w>?"
                            }
                        },
                        {
                            "text": "Wyglądasz dzisiaj bardzo ładnie, <w>prawda</w>?",
                            "target_lang_translations": {
                                "ru": "Ты сегодня очень красиво выглядишь, <w>правда</w>?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое в конце предложения для получения подтверждения от собеседника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правда",
                                "target_lang_sense_clarification": "вопросительная частица"
                            },
                            {
                                "target_lang_word": "не так ли",
                                "target_lang_sense_clarification": "частица для подтверждения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37a05bd3-727a-3af1-97e5-ff94d770535d",
                    "sense_definition": "Słowo potwierdzające, że to, co ktoś powiedział, jest zgodne z rzeczywistością.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Ależ tu dzisiaj gorąco! — <w>Prawda</w>, nie da się wytrzymać.",
                            "target_lang_translations": {
                                "ru": "— Ну и жара сегодня! — <w>Правда</w>, невыносимо."
                            }
                        },
                        {
                            "text": "— On zawsze się spóźnia na spotkania. — <w>Prawda</w>, to bardzo denerwujące.",
                            "target_lang_translations": {
                                "ru": "— Он вечно опаздывает на встречи. — <w>Правда</w>, это очень раздражает."
                            }
                        }
                    ],
                    "synonyms": [
                        "fakt"
                    ],
                    "antonyms": [
                        "nieprawda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, подтверждающее, что сказанное кем-то соответствует действительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правда",
                                "target_lang_sense_clarification": "подтверждение согласия"
                            },
                            {
                                "target_lang_word": "верно",
                                "target_lang_sense_clarification": "согласие"
                            },
                            {
                                "target_lang_word": "действительно",
                                "target_lang_sense_clarification": "подтверждение факта"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    ],
                    "common_phrases": [
                        "paarsblauwe kleur",
                        "paarsblauwe tint",
                        "paarsblauwe bloemen"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a color that is intermediate between purple and blue.",
                        "ru": "Имеющий цвет, промежуточный между фиолетовым и синим."
                    },
                    "translations": {
                        "en": [
                            {

{
    "word_family": [
        "gebrek",
        "ontbrekend"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bcd3d2e3-ee66-3f69-bb46-76e82fbe13ec",
                    "sense_definition": "Niet aanwezig zijn op een plek waar men of iets wel verwacht wordt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er <w>ontbrak</w> een belangrijk bestand op de harde schijf van de computer.",
                            "target_lang_translations": {
                                "en": "An important file <w>was missing</w> from the computer's hard drive.",
                                "ru": "На жестком диске компьютера <w>отсутствовал</w> важный файл."
                            }
                        },
                        {
                            "text": "Tijdens onze luxe vakantie zal het ons aan niets <w>ontbreken</w>.",
                            "target_lang_translations": {
                                "en": "During our luxury vacation, we will <w>lack</w> nothing.",
                                "ru": "Во время нашего роскошного отпуска нам ничего не будет <w>недоставать</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "missen"
                    ],
                    "antonyms": [
                        "aanwezig zijn"
                    ],
                    "common_phrases": [
                        "aan niets ontbreken",
                        "niet mogen ontbreken"
                    ],
                    "target_lang_definitions": {
                        "en": "To not be present in a place where one or something is expected.",
                        "ru": "Не присутствовать там, где кого-то или что-то ожидают увидеть; быть в недостатке."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be missing",
                                "target_lang_sense_clarification": "not present"
                            },
                            {
                                "target_lang_word": "lack",
                                "target_lang_sense_clarification": "to be without something needed"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отсутствовать",
                                "target_lang_sense_clarification": "не быть в наличии"
                            },
                            {
                                "target_lang_word": "не хватать",
                                "target_lang_sense_clarification": "быть в недостаточном количестве"
                            },
                            {
                                "target_lang_word": "недоставать",
                                "target_lang_sense_clarification": "ощущаться как отсутствие чего-то нужного"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f9cfc534-67b6-3b5d-90a2-673c7e5daaf9",
                    "sense_definition": "Het feit dat iets of iemand er niet is; de afwezigheid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>ontbreken</w> van voldoende bewijs maakte de rechtszaak erg lastig.",
                            "target_lang_translations": {
                                "en": "The <w>lack</w> of sufficient evidence made the lawsuit very difficult.",
                                "ru": "<w>Отсутствие</w> достаточных доказательств сильно осложнило судебный процесс."
                            }
                        },
                        {
                            "text": "Door het <w>ontbreken</w> van een handtekening is dit contract helaas niet geldig.",
                            "target_lang_translations": {
                                "en": "Due to the <w>absence</w> of a signature, this contract is unfortunately not valid.",
                                "ru": "Из-за <w>отсутствия</w> подписи этот контракт, к сожалению, недействителен."
                            }
                        }
                    ],
                    "synonyms": [
                        "missen"
                    ],
                    "target_lang_definitions": {
                        "en": "The fact that something or someone is not there; absence.",
                        "ru": "Факт того, что кто-то или что-то отсутствует; небытие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "absence",
                                "target_lang_sense_clarification": "state of not being present"
                            },
                            {
                                "target_lang_word": "lack",
                                "target_lang_sense_clarification": "shortage or deficiency"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отсутствие",
                                "target_lang_sense_clarification": "состояние, когда чего-то нет"
                            },
                            {
                                "target_lang_word": "нехватка",
                                "target_lang_sense_clarification": "недостаточное количество, дефицит"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cdc966f2-85ab-3a75-8653-015acc3af744",
                    "sense_definition": "Сдавливать, стискивать что-либо или наваливаться весом; крепко обхватывать рукой (при приветствии).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_pressure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При встрече он крепко <w>жал</w> мне руку.",
                            "target_lang_translations": {
                                "en": "When we met, he firmly <w>shook</w> my hand."
                            }
                        },
                        {
                            "text": "Не нужно так сильно <w>жать</w> на дверную ручку, она может сломаться.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>press</w> on the door handle so hard, it might break."
                            }
                        }
                    ],
                    "synonyms": [
                        "давить",
                        "сжимать",
                        "стискивать",
                        "сдавливать"
                    ],
                    "antonyms": [
                        "отпускать"
                    ],
                    "common_phrases": [
                        "жать руку",
                        "жать кнопку",
                        "жать плечами"
                    ],
                    "target_lang_definitions": {
                        "en": "To squeeze, compress, or press something; to clasp a hand firmly (in greeting)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "squeeze",
                                "target_lang_sense_clarification": "apply pressure"
                            },
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "apply force"
                            },
                            {
                                "target_lang_word": "shake",
                                "target_lang_sense_clarification": "a hand"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c43a8a5-74d9-3ad8-b671-db24067f53a0",
                    "sense_definition": "Быть тесным, причинять боль или неудобство своей теснотой (об обуви или одежде).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_pressure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новые ботинки немного <w>жмут</w>, но скоро разносятся.",
                            "target_lang_translations": {
                                "en": "The new boots <w>pinch</w> a little, but they will stretch soon."
                            }
                        },
                        {
                            "text": "Это пальто мне <w>жмёт</w> в плечах, нужен размер побольше.",
                            "target_lang_translations": {
                                "en": "This coat <w>is tight</w> in the shoulders, I need a bigger size."
                            }
                        }
                    ],
                    "synonyms": [
                        "давить"
                    ],
                    "common_phrases": [
                        "туфли жмут",
                        "сапоги жмут",
                        "в плечах жмёт"
                    ],
                    "target_lang_definitions": {
                        "en": "To be tight, to cause pain or discomfort due to tightness (of shoes or clothes)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pinch",
                                "target_lang_sense_clarification": "cause pain"
                            },
                            {
                                "target_lang_word": "be tight",
                                "target_lang_sense_clarification": "fit poorly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f66016c7-953b-3cc3-b0de-daea3053ddec",
                    "sense_definition": "Выдавливать жидкость из чего-либо (фруктов, ягод, семян).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "extraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы будем <w>жать</w> сок из свежих яблок для завтрака.",
                            "target_lang_translations": {
                                "en": "We will <w>squeeze</w> juice from fresh apples for breakfast."
                            }
                        },
                        {
                            "text": "На заводе из семян подсолнечника <w>жмут</w> масло.",
                            "target_lang_translations": {
                                "en": "At the factory, they <w>press</w> oil from sunflower seeds."
                            }
                        }
                    ],
                    "synonyms": [
                        "выжимать",
                        "выдавливать",
                        "давить"
                    ],
                    "common_phrases": [
                        "жать сок",
                        "жать масло"
                    ],
                    "target_lang_definitions": {
                        "en": "To extract liquid from something (fruits, berries, seeds)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "squeeze",
                                "target_lang_sense_clarification": "juice"
                            },
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "oil"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e1ebb47-f08d-336a-87b5-f03253131cb3",
                    "sense_definition": "Быстро ехать, увеличивать скорость или энергично действовать.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "speed_pressure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Давай, <w>жми</w> на газ, а то мы опоздаем на поезд!",
                            "target_lang_translations": {
                                "en": "Come on, <w>step</w> on the gas, or we'll miss the train!"
                            }
                        },
                        {
                            "text": "Шофёр начал <w>жать</w> на педали, чтобы машина поехала быстрее.",
                            "target_lang_translations": {
                                "en": "The driver started to <w>press</w> the pedals to make the car go faster."
                            }
                        }
                    ],
                    "synonyms": [
                        "гнать"
                    ],
                    "common_phrases": [
                        "жми на газ",
                        "жми на тормоз"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To drive fast, increase speed, or act energetically."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "on the gas/pedals"
                            },
                            {
                                "target_lang_word": "step on it",
                                "target_lang_sense_clarification": "accelerate (colloquial)"
                            },
                            {
                                "target_lang_word": "rush",
                                "target_lang_sense_clarification": "move quickly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73268590-2b93-3937-9b98-c6c6f387e1de",
                    "sense_definition": "Подгонять, не давать времени, вызывать спешку (о сроках или времени).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "speed_pressure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нас очень <w>жмут</w> сроки сдачи проекта, нужно работать быстрее.",
                            "target_lang_translations": {
                                "en": "The project deadlines are really <w>pressing</w> us, we need to work faster."
                            }
                        },
                        {
                            "text": "Время <w>жмёт</w>, поэтому мы должны принять решение прямо сейчас.",
                            "target_lang_translations": {
                                "en": "Time <w>is pressing</w>, so we must make a decision right now."
                            }
                        }
                    ],
                    "synonyms": [
                        "поджимать"
                    ],
                    "common_phrases": [
                        "сроки жмут",
                        "время жмёт"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To pressure, to limit time (regarding deadlines)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "time/deadlines"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b2bade7-f287-34a8-b5bd-d3126bc0e30b",
                    "sense_definition": "Поднимать вес от груди вверх (в тяжёлой атлетике).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спортсмен готовится <w>жать</w> рекордный вес на соревнованиях.",
                            "target_lang_translations": {
                                "en": "The athlete is preparing to <w>bench press</w> a record weight at the competition."
                            }
                        },
                        {
                            "text": "Он может <w>жать</w> штангу весом сто килограммов.",
                            "target_lang_translations": {
                                "en": "He can <w>bench press</w> a barbell weighing one hundred kilograms."
                            }
                        }
                    ],
                    "common_phrases": [
                        "жать штангу",
                        "жать от груди",
                        "жать вес"
                    ],
                    "target_lang_definitions": {
                        "en": "To lift a weight upwards from the chest (in weightlifting)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bench press",
                                "target_lang_sense_clarification": "weightlifting"
                            },
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "lift weight"
                            }
                        ]
                    }
                }
            ]
        },
        {

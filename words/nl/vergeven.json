            "pos": "verb",
            "senses": [
                {
                    "sense_id": "851be8b7-d399-341c-9966-e31d281818bd",
                    "sense_definition": "Iemand een kwade of verkeerde daad niet langer toerekenen; vergiffenis schenken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal je nooit kunnen <w>vergeven</w> voor wat je hebt gedaan.",
                            "target_lang_translations": {
                                "en": "I will never be able to <w>forgive</w> you for what you have done.",
                                "ru": "Я никогда не смогу тебя <w>простить</w> за то, что ты сделал."
                            }
                        },
                        {
                            "text": "Het is beter om te <w>vergeven</w> en vergeten dan wrok te koesteren.",
                            "target_lang_translations": {
                                "en": "It is better to <w>forgive</w> and forget than to hold a grudge.",
                                "ru": "Лучше <w>простить</w> и забыть, чем таить обиду."
                            }
                        }
                    ],
                    "synonyms": [
                        "vergiftigen"
                    ],
                    "antonyms": [
                        "aanrekenen",
                        "kwalijk nemen"
                    ],
                    "common_phrases": [
                        "vergeven en vergeten"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop feeling angry or resentful towards someone for an offense, flaw, or mistake.",
                        "ru": "Перестать винить кого-либо за проступок или обиду; даровать прощение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forgive",
                                "target_lang_sense_clarification": "To cease to feel resentment against"
                            },
                            {
                                "target_lang_word": "pardon",
                                "target_lang_sense_clarification": "To release from the penalty of an offense, often formally"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прощать",
                                "target_lang_sense_clarification": "несовершенный вид"
                            },
                            {
                                "target_lang_word": "простить",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49ad6976-7a32-381b-85b2-505d08d72042",
                    "sense_definition": "Iets weggeven of toewijzen, vaak gezegd van banen, posities of kaarten die niet meer beschikbaar zijn.",

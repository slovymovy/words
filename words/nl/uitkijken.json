    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8761854b-34f8-33bd-b3b5-3724ba1c4bc3",
                    "sense_definition": "Voorzichtig zijn en alert zijn op mogelijk gevaar of dreiging.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet goed <w>uitkijken</w> als je deze drukke straat oversteekt.",
                            "target_lang_translations": {
                                "en": "You have to <w>watch out</w> carefully when you cross this busy street.",
                                "ru": "Тебе нужно быть очень <w>осторожным</w>, когда переходишь эту оживленную улицу."
                            }
                        },
                        {
                            "text": "<w>Kijk uit</w> voor die losliggende tegels op de stoep!",
                            "target_lang_translations": {
                                "en": "<w>Watch out</w> for those loose tiles on the sidewalk!",
                                "ru": "<w>Остерегайся</w> этих шатающихся плиток на тротуаре!"
                            }
                        }
                    ],
                    "synonyms": [
                        "oppassen",
                        "opletten"
                    ],
                    "common_phrases": [
                        "kijk goed uit",
                        "uitkijken voor gevaar",
                        "kijk uit"
                    ],
                    "target_lang_definitions": {
                        "en": "To be careful and alert to potential danger or threat.",
                        "ru": "Быть осторожным и внимательным к возможной опасности или угрозе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "watch out",
                                "target_lang_sense_clarification": "to be vigilant for danger"
                            },
                            {
                                "target_lang_word": "look out",
                                "target_lang_sense_clarification": "to be alert to one's surroundings"
                            },
                            {
                                "target_lang_word": "be careful",
                                "target_lang_sense_clarification": "to proceed with caution"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть осторожным",
                                "target_lang_sense_clarification": "проявлять внимательность во избежание опасности"
                            },
                            {
                                "target_lang_word": "остерегаться",
                                "target_lang_sense_clarification": "беречься чего-либо конкретного"
                            },
                            {
                                "target_lang_word": "смотреть в оба",
                                "target_lang_sense_clarification": "быть предельно внимательным (фразеологизм)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7cf22ce-4a73-3ab2-b23c-96bad1d5b6e8",
                    "sense_definition": "Met plezier of verlangen wachten op iets dat in de toekomst gaat gebeuren (meestal in combinatie met 'naar').",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen <w>kijken</w> al weken <w>uit</w> naar de komst van Sinterklaas.",
                            "target_lang_translations": {
                                "en": "The children have been <w>looking forward</w> to the arrival of Sinterklaas for weeks.",
                                "ru": "Дети уже несколько недель <w>ждут с нетерпением</w> прихода Синтерклааса."
                            }
                        },
                        {
                            "text": "Ik <w>kijk</w> er echt naar <w>uit</w> om je weer te zien.",
                            "target_lang_translations": {
                                "en": "I am really <w>looking forward</w> to seeing you again.",
                                "ru": "Я действительно <w>жду с нетерпением</w> возможности снова увидеть тебя."
                            }
                        }
                    ],
                    "synonyms": [
                        "verheugen"
                    ],
                    "common_phrases": [
                        "ergens naar uitkijken",
                        "reikhalzend uitkijken naar"
                    ],
                    "target_lang_definitions": {
                        "en": "To wait with pleasure or desire for something that will happen in the future.",
                        "ru": "С удовольствием или желанием ждать чего-то, что произойдет в будущем (обычно в сочетании с 'naar')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look forward to",
                                "target_lang_sense_clarification": "to anticipate with pleasure"
                            },
                            {
                                "target_lang_word": "await",
                                "target_lang_sense_clarification": "formal; to wait for something expected"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ждать с нетерпением",
                                "target_lang_sense_clarification": "сильно желать наступления события"
                            },
                            {
                                "target_lang_word": "предвкушать",
                                "target_lang_sense_clarification": "заранее испытывать удовольствие от будущего"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6906fe5-198e-3306-b1e0-6c1d46b68df0",
                    "sense_definition": "Zicht hebben op een bepaalde plek vanuit een positie (meestal in combinatie met 'op').",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Onze hotelkamer <w>kijkt uit</w> op het prachtige blauwe meer.",
                            "target_lang_translations": {
                                "en": "Our hotel room <w>looks out</w> on the beautiful blue lake.",
                                "ru": "Окна нашего номера <w>выходят</w> на прекрасное голубое озеро."
                            }
                        },
                        {
                            "text": "Vanuit de hoge toren <w>kijk</w> je <w>uit</w> over de hele oude stad.",
                            "target_lang_translations": {
                                "en": "From the high tower you <w>look out</w> over the entire old city.",
                                "ru": "С высокой башни ты <w>смотришь</w> на весь старый город."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uitkijken op zee",
                        "op de tuin uitkijken"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a view of a specific place from a position.",
                        "ru": "Иметь вид на определенное место с какой-либо позиции (обычно в сочетании с 'op')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look out over",
                                "target_lang_sense_clarification": "to have a view across an area"
                            },
                            {
                                "target_lang_word": "overlook",
                                "target_lang_sense_clarification": "to have a view from above"
                            },
                            {
                                "target_lang_word": "look out on",
                                "target_lang_sense_clarification": "to face a specific direction or view"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выходить на",
                                "target_lang_sense_clarification": "быть обращенным в сторону чего-либо (об окнах, зданиях)"
                            },
                            {
                                "target_lang_word": "иметь вид на",
                                "target_lang_sense_clarification": "открывать обзор на что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c37d681-d707-3874-ab76-f264cdba54ff",
                    "sense_definition": "Iets volledig bekijken tot het afgelopen is, zoals een film of programma.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zullen we deze saaie film <w>uitkijken</w> of iets anders opzetten?",
                            "target_lang_translations": {
                                "en": "Shall we <w>finish watching</w> this boring movie or put something else on?",
                                "ru": "Мы <w>досмотрим</w> этот скучный фильм или включим что-то другое?"
                            }
                        },
                        {
                            "text": "Hij heeft de hele serie in één weekend helemaal <w>uitgekeken</w>.",
                            "target_lang_translations": {
                                "en": "He <w>finished watching</w> the whole series completely in one weekend.",
                                "ru": "Он полностью <w>досмотрел</w> весь сериал за одни выходные."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de film uitkijken",
                        "niet helemaal uitkijken"
                    ],
                    "target_lang_definitions": {
                        "en": "To watch something completely until it ends, such as a movie or program.",
                        "ru": "Посмотреть что-либо полностью до конца, например фильм или программу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "finish watching",
                                "target_lang_sense_clarification": "to watch until the end"
                            },
                            {
                                "target_lang_word": "watch to the end",
                                "target_lang_sense_clarification": "to view the entirety of something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "досмотреть",
                                "target_lang_sense_clarification": "закончить просмотр"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [

            "senses": [
                {
                    "sense_id": "4227f7d0-f473-3418-8be1-a69c57f6347d",
                    "sense_definition": "Mieszkaniec miasta <w>Lidzbark</w> lub <w>Lidzbark Warmiński</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "residents",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy <w>lidzbarczanin</w> powinien znać historię swojego regionu.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>житель Лидзбарка</w> должен знать историю своего региона."
                            }
                        },
                        {
                            "text": "Mój sąsiad to rodowity <w>lidzbarczanin</w>, który mieszka tu od urodzenia.",
                            "target_lang_translations": {
                                "ru": "Мой сосед — коренной <w>житель Лидзбарка</w>, который живет здесь с рождения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rodowity lidzbarczanin",
                        "dumny lidzbarczanin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Житель города Лидзбарк или Лидзбарк-Варминьски."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "житель Лидзбарка",
                                "target_lang_sense_clarification": "житель города"
                            },
                            {
                                "target_lang_word": "лидзбаркец",
                                "target_lang_sense_clarification": "этнохороним (редко)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52b85809-72e9-3717-b131-c2c033a075d6",
                    "sense_definition": "Osoba pochodząca z miasta <w>Lidzbark</w> lub <w>Lidzbark Warmiński</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "residents",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten znany pisarz to <w>lidzbarczanin</w>, który często wraca w rodzinne strony.",
                            "target_lang_translations": {
                                "ru": "Этот известный писатель — <w>уроженец Лидзбарка</w>, который часто возвращается в родные края."
                            }
                        },
                        {
                            "text": "Mimo przeprowadzki do stolicy, w sercu na zawsze pozostał <w>lidzbarczaninem</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на переезд в столицу, в душе он навсегда остался <w>жителем Лидзбарка</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "znany lidzbarczanin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, родом из города Лидзбарк или Лидзбарк-Варминьски."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уроженец Лидзбарка",
                                "target_lang_sense_clarification": "человек по происхождению"
                            }
                        ]
                    }
                }
            ]
        }
    ]

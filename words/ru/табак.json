                    "sense_id": "b16fdde6-0bd9-30cb-bcbc-7b1d3be360ae",
                    "sense_definition": "Высушенные и измельчённые листья растения, используемые для курения, нюханья или жевания.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я полагаю даже, что курить трубку гораздо здоровее, нежели нюхать <w>табак</w>.",
                            "target_lang_translations": {
                                "en": "I even believe that smoking a pipe is much healthier than sniffing <w>tobacco</w>."
                            }
                        },
                        {
                            "text": "В комнате стоял густой запах дешёвого <w>табака</w>.",
                            "target_lang_translations": {
                                "en": "There was a thick smell of cheap <w>tobacco</w> in the room."
                            }
                        }
                    ],
                    "synonyms": [
                        "тютюн"
                    ],
                    "common_phrases": [
                        "нюхательный табак",
                        "курить табак",
                        "запах табака",
                        "дело табак"
                    ],
                    "target_lang_definitions": {
                        "en": "Dried and crushed leaves of the tobacco plant, prepared for smoking, chewing, or sniffing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tobacco",
                                "target_lang_sense_clarification": "substance"
                            }
                        ]

{
    "word_family": [
        "looker",
        "lookout",
        "outlook",
        "overlook",
        "onlooker",
        "good-looking"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "12ef55ae-deda-3f55-9d45-2dd5aef78268",
                    "sense_definition": "To try to see; to pay attention to something with one's eyes.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>look</w> at the board carefully.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, внимательно <w>посмотрите</w> на доску."
                            }
                        },
                        {
                            "text": "She <w>looked</w> out the window to see the rain.",
                            "target_lang_translations": {
                                "ru": "Она <w>выглянула</w> в окно, чтобы увидеть дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "view",
                        "regard"
                    ],
                    "common_phrases": [
                        "look at",
                        "look around",
                        "look back"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пытаться увидеть; обращать внимание на что-либо глазами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотре́ть",
                                "target_lang_sense_clarification": "направлять взгляд"
                            },
                            {
                                "target_lang_word": "гляде́ть",
                                "target_lang_sense_clarification": "разговорный синоним"
                            },
                            {
                                "target_lang_word": "посмотре́ть",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e5136ca-1103-3b87-9bc8-52c0e3b78886",
                    "sense_definition": "To appear or seem to be a certain way.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It <w>looks</w> like it is going to rain soon.",
                            "target_lang_translations": {
                                "ru": "<w>Похоже</w>, скоро пойдет дождь."
                            }
                        },
                        {
                            "text": "You <w>look</w> very tired after work.",
                            "target_lang_translations": {
                                "ru": "Ты <w>выглядишь</w> очень усталым после работы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "look like",
                        "look good",
                        "look as if"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь определенный вид; казаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вы́глядеть",
                                "target_lang_sense_clarification": "иметь вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c16962a3-4625-3478-971a-89e8e9737721",
                    "sense_definition": "To search for; to try to find something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "search",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>looking</w> for my lost keys.",
                            "target_lang_translations": {
                                "ru": "Я <w>ищу</w> свои потерянные ключи."
                            }
                        },
                        {
                            "text": "Can you help me <w>look</w> for a new apartment?",
                            "target_lang_translations": {
                                "ru": "Можешь помочь мне <w>поискать</w> новую квартиру?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "look for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искать что-либо; пытаться найти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искать",
                                "target_lang_sense_clarification": "пытаться найти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "698a465b-11c6-3de9-9fa1-644ad5a94e2f",
                    "sense_definition": "To face a certain direction or present a view.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "orientation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hotel room <w>looks</w> over the beautiful valley.",
                            "target_lang_translations": {
                                "ru": "Окна номера <w>выходят</w> на прекрасную долину."
                            }
                        },
                        {
                            "text": "Our balcony <w>looks</w> onto the busy street.",
                            "target_lang_translations": {
                                "ru": "Наш балкон <w>выходит</w> на оживленную улицу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "look over",
                        "look onto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть обращенным в определенную сторону; открывать вид на что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выходить",
                                "target_lang_sense_clarification": "об окнах, балконе"
                            },
                            {
                                "target_lang_word": "смотре́ть",
                                "target_lang_sense_clarification": "быть обращенным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d2db974-8c18-3d85-ad78-03496e60e2b6",
                    "sense_definition": "To watch a pitch go by without swinging at it (in baseball).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The batter struck out <w>looking</w> at the final pitch.",
                            "target_lang_translations": {
                                "ru": "Бэттер получил страйк-аут, <w>пропустив</w> последнюю подачу."
                            }
                        },
                        {
                            "text": "He let the ball pass and got caught <w>looking</w>.",
                            "target_lang_translations": {
                                "ru": "Он пропустил мяч и получил страйк на <w>просмотре</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "baseball"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пропускать подачу, наблюдая за мячом без попытки ударить (в бейсболе)."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "45b8b058-f343-3ff7-999a-4aea64cd3b25",
                    "sense_definition": "The act of looking or attempting to see something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Let's have a <w>look</w> at your new car.",
                            "target_lang_translations": {
                                "ru": "Давай <w>взглянем</w> на твою новую машину."
                            }
                        },
                        {
                            "text": "Take a close <w>look</w> at this map.",
                            "target_lang_translations": {
                                "ru": "Внимательно <w>посмотри</w> на эту карту."
                            }
                        }
                    ],
                    "synonyms": [
                        "view"
                    ],
                    "common_phrases": [
                        "have a look",
                        "take a look"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие смотрения или попытка увидеть что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взгляд",
                                "target_lang_sense_clarification": "направленность глаз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0803f44c-df96-36e3-9f1c-0cee6ecbfb83",
                    "sense_definition": "Physical appearance or visual impression.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I really like the <w>look</w> of the new design.",
                            "target_lang_translations": {
                                "ru": "Мне очень нравится <w>вид</w> нового дизайна."
                            }
                        },
                        {
                            "text": "She has the <w>looks</w> of a movie star.",
                            "target_lang_translations": {
                                "ru": "У нее <w>внешность</w> кинозвезды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "good looks",
                        "new look"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешний вид или визуальное впечатление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "внешний облик"
                            },
                            {
                                "target_lang_word": "вне́шность",
                                "target_lang_sense_clarification": "наружность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84d4e828-da14-3131-942e-e41990ea2489",
                    "sense_definition": "A facial expression that conveys a feeling.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He gave me a dirty <w>look</w> when I interrupted him.",
                            "target_lang_translations": {
                                "ru": "Он бросил на меня злобный <w>взгляд</w>, когда я его перебил."
                            }
                        },
                        {
                            "text": "She had a worried <w>look</w> on her face.",
                            "target_lang_translations": {
                                "ru": "У нее был обеспокоенный <w>вид</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dirty look",
                        "blank look"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение лица, передающее чувство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взгляд",
                                "target_lang_sense_clarification": "выражение глаз"
                            },
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "выражение лица"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "f821bfe2-46e7-35d2-a192-cadfaa7cf780",
                    "sense_definition": "Used to call for attention or introduce a statement.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Look</w>, I am going to explain this one more time.",
                            "target_lang_translations": {
                                "ru": "<w>Послушай</w>, я объясню это еще раз."
                            }
                        },
                        {
                            "text": "<w>Look</w>! There is a bird in the tree.",
                            "target_lang_translations": {
                                "ru": "<w>Смотри</w>! На дереве птица."
                            }
                        }
                    ],
                    "synonyms": [
                        "listen",
                        "listen up"
                    ],
                    "common_phrases": [
                        "look here"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для привлечения внимания или введения утверждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотри́",
                                "target_lang_sense_clarification": "обращение на ты"
                            },
                            {
                                "target_lang_word": "послу́шай",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "bc35f69a-8d8e-386b-8972-8cac6b2dbc42",
                    "sense_definition": "A surname.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Look</w> will see you now.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Лук</w> примет вас сейчас."
                            }
                        },
                        {
                            "text": "Is your last name <w>Look</w>?",
                            "target_lang_translations": {
                                "ru": "Ваша фамилия <w>Лук</w>?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Лук",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
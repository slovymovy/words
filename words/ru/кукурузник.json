    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b676c14a-f164-389a-b6f2-8a81ef222aee",
                    "sense_definition": "Разговорное название лёгкого самолёта (обычно биплана), используемого в сельском хозяйстве, а также в качестве учебного или связного (исторически относилось к По-2 и Ан-2).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "aircraft",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Над полем низко пролетел <w>кукурузник</w>, распыляя удобрения на посевы.",
                            "target_lang_translations": {
                                "en": "A <w>crop duster</w> flew low over the field, spraying fertilizer on the crops."
                            }
                        },
                        {
                            "text": "В годы войны этот <w>кукурузник</w> часто использовали для ночных вылетов.",
                            "target_lang_translations": {
                                "en": "During the war years, this <w>biplane</w> was often used for night sorties."
                            }
                        }
                    ],
                    "synonyms": [
                        "Ан-2",
                        "По-2",
                        "У-2",
                        "небесный тихоход"
                    ],
                    "common_phrases": [
                        "летать на кукурузнике",
                        "старый кукурузник"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial name for a light aircraft (usually a biplane) used in agriculture, as well as for training or liaison purposes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crop duster",
                                "target_lang_sense_clarification": "agricultural aircraft"
                            },
                            {
                                "target_lang_word": "biplane",
                                "target_lang_sense_clarification": "type of aircraft"
                            },
                            {
                                "target_lang_word": "puddle jumper",
                                "target_lang_sense_clarification": "slang for a small plane"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1966442-763c-30bd-879e-f20ba364efcc",
                    "sense_definition": "Ироничное или пренебрежительное прозвище Никиты Сергеевича Хрущёва, связанное с его политикой массового внедрения кукурузы в сельское хозяйство СССР.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nickname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "В народе Хрущёва часто называли <w>кукурузником</w> из-за его аграрных реформ.",
                            "target_lang_translations": {
                                "en": "People often called Khrushchev <w>Corn Man</w> because of his agrarian reforms."
                            }
                        },
                        {
                            "text": "Это обидное прозвище, <w>кукурузник</w>, закрепилось за ним после неудачной кампании.",
                            "target_lang_translations": {
                                "en": "This offensive nickname, <w>Corn Man</w>, stuck to him after the failed campaign."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "political"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An ironic or derogatory nickname for Nikita Khrushchev, associated with his policy of mass corn introduction in the USSR."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Corn Man",
                                "target_lang_sense_clarification": "nickname"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09053637-355f-32c3-bd24-11ea201eb4a6",
                    "sense_definition": "Человек, который занимается выращиванием кукурузы или профессионально связан с ней.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Местный <w>кукурузник</w> с гордостью показывал нам свой богатый урожай.",
                            "target_lang_translations": {
                                "en": "The local <w>corn grower</w> proudly showed us his rich harvest."
                            }
                        },
                        {
                            "text": "Все знали, что он заядлый <w>кукурузник</w> и знает об этом растении всё.",
                            "target_lang_translations": {
                                "en": "Everyone knew he was an avid <w>corn farmer</w> and knew everything about this plant."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person who grows corn or is professionally connected with it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "corn grower",
                                "target_lang_sense_clarification": "profession"
                            },
                            {
                                "target_lang_word": "corn farmer",
                                "target_lang_sense_clarification": "profession"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "821c850a-df72-37ea-89f4-5bd7621c2e47",
                    "sense_definition": "Поле, засеянное кукурузой; заросли кукурузы.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "field",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы долго шли через густой <w>кукурузник</w>, раздвигая высокие стебли руками.",
                            "target_lang_translations": {
                                "en": "We walked for a long time through the dense <w>cornfield</w>, pushing apart the tall stalks with our hands."
                            }
                        },
                        {
                            "text": "За деревней начинался огромный <w>кукурузник</w>, уходящий к самому горизонту.",
                            "target_lang_translations": {
                                "en": "Beyond the village began a huge <w>cornfield</w> stretching to the very horizon."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A field sown with corn; corn thickets."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cornfield",
                                "target_lang_sense_clarification": "place"
                            }
                        ]
                    }
                }
            ]

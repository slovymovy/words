            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3de52b20-0cfd-3511-9185-105b872be62e",
                    "sense_definition": "Pojedyncze przestawienie nogi w celu przemieszczenia się; ruch wykonany jedną nogą.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zrób jeden <w>krok</w> do przodu, a potem się zatrzymaj.",
                            "target_lang_translations": {
                                "ru": "Сделай один <w>шаг</w> вперед, а затем остановись."
                            }
                        },
                        {
                            "text": "Małe dziecko stawia swoje pierwsze niepewne <w>kroki</w>.",
                            "target_lang_translations": {
                                "ru": "Маленький ребенок делает свои первые неуверенные <w>шаги</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zrobić krok",
                        "pierwszy krok",
                        "krok w tył",
                        "stawiać kroki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Однократное перемещение ноги при ходьбе или беге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шаг",
                                "target_lang_sense_clarification": "движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5ec601e-8779-3243-ba12-f6e81bf08711",
                    "sense_definition": "Bardzo mała odległość od jakiegoś miejsca; bliskość.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "distance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkamy zaledwie dwa <w>kroki</w> od parku miejskiego.",
                            "target_lang_translations": {
                                "ru": "Мы живем всего в двух <w>шагах</w> от городского парка."
                            }
                        },
                        {
                            "text": "Byliśmy o <w>krok</w> od wielkiego nieszczęścia, ale udało się go uniknąć.",
                            "target_lang_translations": {
                                "ru": "Мы были в <w>шаге</w> от большого несчастья, но его удалось избежать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "o krok",
                        "dwa kroki stąd",
                        "na każdym kroku",
                        "krok od czegoś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень маленькое расстояние; непосредственная близость к какому-либо месту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шаг",
                                "target_lang_sense_clarification": "расстояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f02831a2-5b26-350e-b66d-2905306ca8cb",
                    "sense_definition": "Konkretne działanie podjęte w celu osiągnięcia zamierzonego skutku lub rozwiązania problemu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy podjąć odpowiednie <w>kroki</w>, aby naprawić tę sytuację.",
                            "target_lang_translations": {
                                "ru": "Мы должны предпринять соответствующие <w>шаги</w>, чтобы исправить эту ситуацию."
                            }
                        },
                        {
                            "text": "To był bardzo ryzykowny <w>krok</w>, ale ostatecznie przyniósł zysk.",
                            "target_lang_translations": {
                                "ru": "Это был очень рискованный <w>шаг</w>, но в конечном итоге он принес прибыль."
                            }
                        }
                    ],
                    "synonyms": [
                        "działanie",
                        "czyn"
                    ],
                    "common_phrases": [
                        "podjąć kroki",
                        "zdecydowany krok",
                        "kroki prawne",
                        "pierwszy krok do"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное действие или мера, предпринятая для достижения цели или решения проблемы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шаг",
                                "target_lang_sense_clarification": "поступок, действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9a06009-c749-39c6-a0c7-4687acc23650",
                    "sense_definition": "Etap, faza lub stadium w rozwoju czegoś lub w trakcie trwania procesu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podpisanie umowy to kolejny ważny <w>krok</w> w mojej karierze.",
                            "target_lang_translations": {
                                "ru": "Подписание договора — это очередной важный <w>шаг</w> в моей карьере."
                            }
                        },
                        {
                            "text": "Musisz mi wytłumaczyć ten przepis <w>krok</w> po kroku.",
                            "target_lang_translations": {
                                "ru": "Ты должен объяснить мне этот рецепт <w>шаг</w> за шагом."
                            }
                        }
                    ],
                    "synonyms": [
                        "etap",
                        "faza",
                        "stadium",
                        "postęp"
                    ],
                    "common_phrases": [
                        "krok po kroku",
                        "krok milowy",
                        "kolejny krok",
                        "wielki krok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Этап, фаза или стадия в развитии чего-либо или в ходе процесса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шаг",
                                "target_lang_sense_clarification": "этап, стадия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32cc75e8-5662-364d-b802-f2f2b053c988",
                    "sense_definition": "Sposób poruszania się, tempo chodu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musieliśmy przyspieszyć <w>kroku</w>, żeby zdążyć na ostatni autobus.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось прибавить <w>шагу</w>, чтобы успеть на последний автобус."
                            }
                        },
                        {
                            "text": "Mój brat szedł tak szybko, że trudno było mi dotrzymać mu <w>kroku</w>.",
                            "target_lang_translations": {
                                "ru": "Мой брат шел так быстро, что мне было трудно подстроиться под его <w>шаг</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "chód"
                    ],
                    "common_phrases": [
                        "przyspieszyć kroku",
                        "dotrzymać kroku",
                        "zwolnić kroku",
                        "szybkim krokiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Манера передвижения, темп ходьбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шаг",
                                "target_lang_sense_clarification": "темп, походка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e65067e-1231-34ab-b6e3-644b4f02a764",
                    "sense_definition": "Ustalony sposób stawiania nóg i wykonywania ruchów w tańcu.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "movement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Instruktor pokazał nam podstawowe <w>kroki</w> walca angielskiego.",
                            "target_lang_translations": {
                                "ru": "Инструктор показал нам основные <w>шаги</w> английского вальса."
                            }
                        },
                        {
                            "text": "Ciągle mylę <w>kroki</w>, kiedy próbujemy zatańczyć ten układ.",
                            "target_lang_translations": {
                                "ru": "Я постоянно путаю <w>шаги</w>, когда мы пытаемся станцевать эту связку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "krok podstawowy",
                        "mylić kroki",
                        "znać kroki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенное движение ног и перемещение тела в танце."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шаг",
                                "target_lang_sense_clarification": "танцевальное движение"
                            },
                            {
                                "target_lang_word": "па",
                                "target_lang_sense_clarification": "танцевальное движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4cd5f76-b353-36fd-a9af-41bfe0735043",
                    "sense_definition": "Część ubrania (zwykle spodni) lub ciała w miejscu, gdzie łączą się nogi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy_clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te spodnie są na mnie trochę za ciasne w <w>kroku</w>.",
                            "target_lang_translations": {
                                "ru": "Эти брюки мне немного жмут в <w>шагу</w>."
                            }
                        },
                        {
                            "text": "Podczas wchodzenia na drabinę pękły mu spodnie w <w>kroku</w>.",
                            "target_lang_translations": {
                                "ru": "Когда он поднимался по лестнице, у него порвались брюки в <w>шагу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "krocze"
                    ],
                    "common_phrases": [
                        "w kroku",
                        "szew w kroku",
                        "luźny w kroku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть одежды (обычно брюк) или тела в месте соединения ног."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шаг",
                                "target_lang_sense_clarification": "часть брюк"
                            },
                            {
                                "target_lang_word": "промежность",

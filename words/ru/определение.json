    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d131e6de-41de-3d9d-be5d-b85ab6bc4c83",
                    "sense_definition": "Формулировка, которая кратко и точно раскрывает содержание и основной смысл какого-либо понятия или слова.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "meaning_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В начале урока учитель попросил нас записать <w>определение</w> нового термина в тетрадь.",
                            "target_lang_translations": {
                                "en": "At the beginning of the lesson, the teacher asked us to write down the <w>definition</w> of the new term in our notebooks."
                            }
                        },
                        {
                            "text": "Студент не смог дать точное <w>определение</w> понятию «культура» на экзамене.",
                            "target_lang_translations": {
                                "en": "The student could not give a precise <w>definition</w> of the concept of \"culture\" at the exam."
                            }
                        }
                    ],
                    "synonyms": [
                        "объясне́ние"
                    ],
                    "common_phrases": [
                        "дать определение",
                        "точное определение",
                        "научное определение"
                    ],
                    "target_lang_definitions": {
                        "en": "A statement that briefly and precisely reveals the content and main meaning of a concept or word."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "definition",
                                "target_lang_sense_clarification": "explanation of meaning"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4290f9cd-f4e0-3803-8f09-83851789d499",
                    "sense_definition": "Второстепенный член предложения, который обозначает признак предмета и отвечает на вопросы «какой?», «чей?».",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "grammar_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом предложении прилагательное «красивый» выступает как <w>определение</w>.",
                            "target_lang_translations": {
                                "en": "In this sentence, the adjective \"beautiful\" acts as an <w>attribute</w>."
                            }
                        },
                        {
                            "text": "Ученики учились находить и подчёркивать волнистой линией <w>определение</w> в тексте.",
                            "target_lang_translations": {
                                "en": "The students learned to find and underline the <w>attribute</w> in the text with a wavy line."
                            }
                        }
                    ],
                    "synonyms": [
                        "атрибути́в"
                    ],
                    "common_phrases": [
                        "согласованное определение",
                        "несогласованное определение"
                    ],
                    "target_lang_definitions": {
                        "en": "A secondary part of a sentence that modifies a noun and answers questions like \"what kind?\" or \"whose?\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attribute",
                                "target_lang_sense_clarification": "grammar term"
                            },
                            {
                                "target_lang_word": "modifier",
                                "target_lang_sense_clarification": "linguistics"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c14a4c3-8c08-3570-81c0-8bae8d469355",
                    "sense_definition": "Процесс вычисления, установления или выяснения каких-либо данных, свойств или параметров.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "process_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для <w>определения</w> точного местоположения корабля капитан использовал навигационные приборы.",
                            "target_lang_translations": {
                                "en": "For the <w>determination</w> of the ship's exact location, the captain used navigation instruments."
                            }
                        },
                        {
                            "text": "Лаборатория занимается <w>определением</w> качества питьевой воды в нашем районе.",
                            "target_lang_translations": {
                                "en": "The laboratory deals with the <w>determination</w> of drinking water quality in our district."
                            }
                        }
                    ],
                    "synonyms": [
                        "выяснение"
                    ],
                    "common_phrases": [
                        "определение координат",
                        "методы определения",
                        "определение времени"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of calculating, establishing, or ascertaining specific data, properties, or parameters."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "determination",
                                "target_lang_sense_clarification": "act of determining"
                            },
                            {
                                "target_lang_word": "identification",
                                "target_lang_sense_clarification": "process of finding out"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac3ee317-d5ba-3483-bd18-1e784a3905a7",
                    "sense_definition": "Письменное постановление суда по вопросу, возникающему в ходе судебного разбирательства, но не решающее дело по существу.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_ruling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Судья удалился в совещательную комнату, чтобы вынести <w>определение</w> по ходатайству защиты.",
                            "target_lang_translations": {
                                "en": "The judge retired to the deliberation room to issue a <w>ruling</w> on the defense's motion."
                            }
                        },
                        {
                            "text": "Адвокат получил копию судебного <w>определения</w> о назначении экспертизы.",
                            "target_lang_translations": {
                                "en": "The lawyer received a copy of the court <w>ruling</w> on the appointment of an expert examination."
                            }
                        }
                    ],
                    "common_phrases": [
                        "судебное определение",
                        "вынести определение",
                        "обжаловать определение"
                    ],
                    "target_lang_definitions": {
                        "en": "A written judicial ruling on an issue arising during a trial that does not resolve the case on its merits."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ruling",
                                "target_lang_sense_clarification": "court decision"
                            },
                            {
                                "target_lang_word": "court order",
                                "target_lang_sense_clarification": "legal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "426826f9-6206-360d-9e37-25e7a50bf226",
                    "sense_definition": "Назначение кого-либо на должность или место службы (преимущественно в официальной или устаревшей речи).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "appointment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В газете было напечатано объявление об <w>определении</w> нового директора департамента.",
                            "target_lang_translations": {
                                "en": "An announcement about the <w>appointment</w> of the new department director was printed in the newspaper."
                            }
                        },
                        {
                            "text": "Молодой офицер с нетерпением ждал приказа о своём <w>определении</w> в полк.",
                            "target_lang_translations": {
                                "en": "The young officer waited impatiently for the order regarding his <w>assignment</w> to the regiment."
                            }
                        }
                    ],
                    "synonyms": [
                        "назначение",
                        "устраивание",
                        "пристраивание"
                    ],
                    "antonyms": [
                        "увольнение"
                    ],
                    "common_phrases": [
                        "определение на службу",
                        "определение в должность"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "используется в историческом или официально-деловом контексте"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The act of appointing someone to a position or place of service (archaic or official context)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appointment",
                                "target_lang_sense_clarification": "to a post"
                            },
                            {
                                "target_lang_word": "assignment",
                                "target_lang_sense_clarification": "to a duty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1dae6259-2456-3cbe-8c6e-72e2af0b80cf",
                    "sense_definition": "То, что предопределено высшей силой; судьба, рок или божественная воля.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "destiny",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Герой романа смиренно принял жестокое <w>определение</w> судьбы и не стал роптать.",
                            "target_lang_translations": {
                                "en": "The hero of the novel humbly accepted the cruel <w>decree</w> of fate and did not complain."
                            }
                        },
                        {
                            "text": "По божественному <w>определению</w> им суждено было встретиться через много лет.",
                            "target_lang_translations": {
                                "en": "By divine <w>predestination</w>, they were destined to meet many years later."
                            }
                        }
                    ],
                    "synonyms": [
                        "предопределение",
                        "предначертание",
                        "предписание"
                    ],
                    "common_phrases": [
                        "определение судьбы",
                        "божественное определение"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "высокий стиль"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Something predetermined by a higher power; fate, doom, or divine will."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "predestination",
                                "target_lang_sense_clarification": "fate"
                            },
                            {
                                "target_lang_word": "decree",
                                "target_lang_sense_clarification": "divine will"
                            }
                        ]
                    }
                }
            ]
        }
    ]

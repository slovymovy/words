                    "antonyms": [
                        "против"
                    ],
                    "common_phrases": [
                        "идти на врага",
                        "напасть на след"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the object of an attack or aggressive action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "against",
                                "target_lang_sense_clarification": "opposition or attack"
                            },
                            {
                                "target_lang_word": "on",
                                "target_lang_sense_clarification": "direction of attack"
                            }
                        ]
                    }
                }
            ]
        },
        {

                {
                    "sense_id": "f1d00fba-da49-3e01-8cb8-cd32f5be09ae",
                    "sense_definition": "Een blik werpen in achterwaartse richting; achterom kijken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toen hij <w>terugkeek</w> naar de oever, zag hij een groep vogels.",
                            "target_lang_translations": {
                                "en": "When he <w>looked back</w> at the bank, he saw a flock of birds.",
                                "ru": "Когда он <w>оглянулся</w> на берег, то увидел стаю птиц."
                            }
                        },
                        {
                            "text": "Je moet goed <w>terugkijken</w> voordat je met de fiets afslaat.",
                            "target_lang_translations": {
                                "en": "You must <w>look back</w> carefully before turning on your bike.",
                                "ru": "Нужно внимательно <w>посмотреть назад</w>, прежде чем поворачивать на велосипеде."
                            }
                        }
                    ],
                    "synonyms": [
                        "omkijken",
                        "achteromkijken"
                    ],
                    "antonyms": [
                        "vooruitkijken"
                    ],
                    "common_phrases": [
                        "even terugkijken",
                        "niet terugkijken"
                    ],
                    "target_lang_definitions": {
                        "en": "To cast a glance in a backward direction; to look behind oneself.",
                        "ru": "Бросать взгляд назад; смотреть себе за спину."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look back",
                                "target_lang_sense_clarification": "physical action"
                            },
                            {
                                "target_lang_word": "look behind",
                                "target_lang_sense_clarification": "physical action"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "оглядываться",
                                "target_lang_sense_clarification": "физическое действие"
                            },
                            {
                                "target_lang_word": "смотреть назад",
                                "target_lang_sense_clarification": "направление взгляда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8d26d50-0554-3393-b9e3-2a0d621e9de6",
                    "sense_definition": "In gedachten bezig zijn met het verleden; reflecteren op eerdere gebeurtenissen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zangeres <w>kijkt terug</w> op een zeer succesvolle carrière.",
                            "target_lang_translations": {
                                "en": "The singer <w>looks back</w> on a very successful career.",
                                "ru": "Певица <w>оглядывается</w> на свою очень успешную карьеру."
                            }
                        },
                        {
                            "text": "We kunnen tevreden <w>terugkijken</w> op het afgelopen jaar.",
                            "target_lang_translations": {
                                "en": "We can <w>look back</w> with satisfaction on the past year.",
                                "ru": "Мы можем с удовлетворением <w>оглянуться</w> на прошедший год."
                            }
                        }
                    ],
                    "synonyms": [
                        "terugblikken",
                        "reflecteren"
                    ],
                    "antonyms": [
                        "vooruitkijken",
                        "plannen"
                    ],
                    "common_phrases": [
                        "terugkijken op",
                        "met plezier terugkijken",
                        "tevreden terugkijken"
                    ],
                    "target_lang_definitions": {
                        "en": "To think about the past; to reflect on earlier events.",
                        "ru": "Мысленно обращаться к прошлому; размышлять о прошедших событиях."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look back",
                                "target_lang_sense_clarification": "on the past"
                            },
                            {
                                "target_lang_word": "reflect",
                                "target_lang_sense_clarification": "on events"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "оглядываться",
                                "target_lang_sense_clarification": "на прошлое"
                            },
                            {
                                "target_lang_word": "вспоминать",
                                "target_lang_sense_clarification": "обращаться к памяти"
                            }
                        ]

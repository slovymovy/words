    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "12eb127b-3598-311f-8943-21454e17a372",
                    "sense_definition": "Documents that explain how to operate a machine, software, or system.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The software comes with extensive <w>documentation</w> to help new users.",
                            "target_lang_translations": {
                                "ru": "К программному обеспечению прилагается обширная <w>документация</w> в помощь новым пользователям."
                            }
                        },
                        {
                            "text": "You should check the technical <w>documentation</w> if you encounter any errors.",
                            "target_lang_translations": {
                                "ru": "Вам следует проверить техническую <w>документацию</w>, если возникнут какие-либо ошибки."
                            }
                        }
                    ],
                    "synonyms": [
                        "manual",
                        "guide",
                        "instructions"
                    ],
                    "common_phrases": [
                        "technical documentation",
                        "software documentation",
                        "read the documentation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Документы, объясняющие, как работать с машиной, программным обеспечением или системой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "документация",
                                "target_lang_sense_clarification": "техническая или программная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc30aa7b-2cc7-35dd-9763-4e7c2352af9a",
                    "sense_definition": "Official documents provided as proof or evidence of something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Applicants must provide <w>documentation</w> of their citizenship.",
                            "target_lang_translations": {
                                "ru": "Заявители должны предоставить <w>документы</w>, подтверждающие их гражданство."
                            }
                        },
                        {
                            "text": "Without the proper <w>documentation</w>, the bank cannot approve the loan.",
                            "target_lang_translations": {
                                "ru": "Без надлежащих <w>документов</w> банк не может одобрить кредит."
                            }
                        }
                    ],
                    "synonyms": [
                        "evidence",
                        "proof",
                        "paperwork",
                        "records"
                    ],
                    "common_phrases": [
                        "provide documentation",
                        "supporting documentation",
                        "proper documentation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальные документы, предоставляемые в качестве доказательства или подтверждения чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "документы",
                                "target_lang_sense_clarification": "бумаги, удостоверения"
                            },
                            {
                                "target_lang_word": "документация",
                                "target_lang_sense_clarification": "совокупность документов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71642125-ac8b-32de-a5ea-7fc7eb60e9bd",
                    "sense_definition": "The act or process of recording information in written or other forms.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>documentation</w> of historical events requires careful research.",
                            "target_lang_translations": {
                                "ru": "<w>Документирование</w> исторических событий требует тщательного исследования."
                            }
                        },
                        {
                            "text": "Her job involves the <w>documentation</w> of rare plant species in the forest.",
                            "target_lang_translations": {
                                "ru": "Ее работа включает в себя <w>документирование</w> редких видов растений в лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "recording",
                        "logging"
                    ],
                    "common_phrases": [
                        "careful documentation",
                        "process of documentation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс записи информации в письменной или иной форме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "документирование",
                                "target_lang_sense_clarification": "процесс фиксации информации"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
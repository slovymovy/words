            "senses": [
                {
                    "sense_id": "f81b8d6f-4e9a-3d73-b00e-07132342ab05",
                    "sense_definition": "The quality of being extremely good or outstanding.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "quality_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The university is known worldwide for its academic <w>excellence</w>.",
                            "target_lang_translations": {
                                "ru": "Университет известен во всем мире своим академическим <w>превосходством</w>."
                            }
                        },
                        {
                            "text": "Our team strives for <w>excellence</w> in every project we undertake.",
                            "target_lang_translations": {
                                "ru": "Наша команда стремится к <w>совершенству</w> в каждом проекте, за который мы беремся."
                            }
                        }
                    ],
                    "synonyms": [
                        "greatness",
                        "superiority",
                        "perfection",
                        "pre-eminence"
                    ],
                    "antonyms": [
                        "mediocrity",
                        "inferiority"
                    ],
                    "common_phrases": [
                        "academic excellence",
                        "strive for excellence",
                        "center of excellence",
                        "par excellence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество быть исключительно хорошим или выдающимся; высшая степень качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совершенство",
                                "target_lang_sense_clarification": "высшая степень качества, идеал"
                            },
                            {
                                "target_lang_word": "превосходство",
                                "target_lang_sense_clarification": "обладание более высокими достоинствами по сравнению с другими"
                            },
                            {
                                "target_lang_word": "мастерство",
                                "target_lang_sense_clarification": "высокое искусство в какой-либо области"
                            }
                        ]
                    }
                },
                {

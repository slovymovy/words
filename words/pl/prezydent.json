            "senses": [
                {
                    "sense_id": "0adaa1a2-238d-3fd8-b6c2-a931ed6291d4",
                    "sense_definition": "Głowa państwa i najwyższy przedstawiciel władz w kraju o ustroju republikańskim.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "government_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obecny <w>prezydent</w> podpisał nową ustawę dotyczącą podatków.",
                            "target_lang_translations": {
                                "ru": "Нынешний <w>президент</w> подписал новый закон о налогах."
                            }
                        },
                        {
                            "text": "Wybory na <w>prezydenta</w> odbywają się w tym kraju co pięć lat.",
                            "target_lang_translations": {
                                "ru": "Выборы <w>президента</w> проходят в этой стране каждые пять лет."
                            }
                        }
                    ],
                    "antonyms": [
                        "król",
                        "monarcha"
                    ],
                    "common_phrases": [
                        "urząd prezydenta",
                        "wybory na prezydenta",
                        "pan prezydent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глава государства и высший представитель власти в стране с республиканским строем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "президент",
                                "target_lang_sense_clarification": "глава государства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b116a5d0-4b9c-3f3c-8488-6077abcf05cb",
                    "sense_definition": "Urzędnik pełniący funkcję najwyższego organu wykonawczego w dużych miastach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "local_government",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Prezydent</w> miasta ogłosił plan budowy nowej linii tramwajowej.",
                            "target_lang_translations": {
                                "ru": "<w>Мэр</w> города объявил о плане строительства новой трамвайной линии."
                            }
                        },
                        {
                            "text": "Mieszkańcy są zadowoleni z decyzji, które podejmuje ich <w>prezydent</w>.",
                            "target_lang_translations": {
                                "ru": "Жители довольны решениями, которые принимает их <w>мэр</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prezydent miasta",
                        "prezydent Warszawy"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Polska"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чиновник, выполняющий функции высшего исполнительного органа в крупных городах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мэр",
                                "target_lang_sense_clarification": "глава города"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31da1655-fd27-339b-bd44-8a83fbcb6fbf",
                    "sense_definition": "Przewodniczący zarządu dużej organizacji lub międzynarodowego koncernu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "business_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Został mianowany na stanowisko <w>prezydenta</w> wielkiej korporacji finansowej.",
                            "target_lang_translations": {
                                "ru": "Он был назначен на должность <w>президента</w> крупной финансовой корпорации."
                            }
                        },
                        {
                            "text": "Jako <w>prezydent</w> stowarzyszenia, dba o jego rozwój na arenie międzynarodowej.",
                            "target_lang_translations": {
                                "ru": "Как <w>президент</w> ассоциации, он заботится о её развитии на международной арене."
                            }
                        }
                    ],
                    "synonyms": [
                        "prezes"
                    ],
                    "common_phrases": [
                        "prezydent koncernu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Председатель правления крупной организации или международного концерна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "президент",

                            {
                                "target_lang_word": "горошек",
                                "target_lang_sense_clarification": "узор на ткани"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86e49f35-be7e-38c9-801c-bced542cef80",
                    "sense_definition": "A punctuation mark used to indicate the end of a sentence; a full stop or period.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should put a <w>dot</w> at the end of every sentence.",
                            "target_lang_translations": {
                                "ru": "В конце каждого предложения нужно ставить <w>точку</w>."
                            }
                        },
                        {
                            "text": "The website address ends with <w>dot</w> com.",
                            "target_lang_translations": {
                                "ru": "Адрес веб-сайта заканчивается на <w>точка</w> ком."
                            }
                        }
                    ],
                    "synonyms": [
                        "full stop",
                        "period",
                        "point"
                    ],
                    "common_phrases": [
                        "dot com"

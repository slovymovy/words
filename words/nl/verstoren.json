{
    "word_family": [
        "verstoring",
        "verstoorder",
        "onverstoord"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "72cb75f3-05fa-3cf8-9d0b-4cee1df5ee40",
                    "sense_definition": "Iemand uit de concentratie brengen of een activiteit onderbreken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het examen werden de kandidaten <w>verstoord</w> door het brandalarm.",
                            "target_lang_translations": {
                                "ru": "Во время экзамена кандидатам <w>помешала</w> пожарная тревога.",
                                "en": "During the exam, the candidates were <w>disturbed</w> by the fire alarm."
                            }
                        },
                        {
                            "text": "Je mag de spreker niet <w>verstoren</w> met onnodige vragen.",
                            "target_lang_translations": {
                                "ru": "Нельзя <w>мешать</w> докладчику ненужными вопросами.",
                                "en": "You must not <w>interrupt</w> the speaker with unnecessary questions."
                            }
                        }
                    ],
                    "synonyms": [
                        "onderbreken",
                        "storen"
                    ],
                    "common_phrases": [
                        "een vergadering verstoren",
                        "de les verstoren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбивать кого-то с мысли, отвлекать или прерывать чью-то деятельность.",
                        "en": "To break someone's concentration or interrupt an activity."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мешать",
                                "target_lang_sense_clarification": "создавать препятствия или неудобства"
                            },
                            {
                                "target_lang_word": "прерывать",
                                "target_lang_sense_clarification": "останавливать действие или речь"
                            },
                            {
                                "target_lang_word": "сбивать",
                                "target_lang_sense_clarification": "отвлекать от мысли или дела"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "disturb",
                                "target_lang_sense_clarification": "someone's concentration or focus"
                            },
                            {
                                "target_lang_word": "interrupt",
                                "target_lang_sense_clarification": "a speaker or an action"
                            },
                            {
                                "target_lang_word": "disrupt",
                                "target_lang_sense_clarification": "a process or event"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e55fa478-d5b7-3df2-8adf-d93bb17a15ab",
                    "sense_definition": "De heersende kalmte, rust of orde verbreken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Harde muziek kan de nachtrust van de buren ernstig <w>verstoren</w>.",
                            "target_lang_translations": {
                                "ru": "Громкая музыка может серьезно <w>нарушить</w> ночной покой соседей.",
                                "en": "Loud music can seriously <w>disturb</w> the neighbors' night's rest."
                            }
                        },
                        {
                            "text": "De demonstranten probeerden de openbare orde te <w>verstoren</w>.",
                            "target_lang_translations": {
                                "ru": "Демонстранты пытались <w>нарушить</w> общественный порядок.",
                                "en": "The demonstrators tried to <w>disrupt</w> public order."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbreken"
                    ],
                    "antonyms": [
                        "bewaren",
                        "handhaven"
                    ],
                    "common_phrases": [
                        "de openbare orde verstoren",
                        "de rust verstoren",
                        "het evenwicht verstoren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарушать царящее спокойствие, тишину или установленный порядок.",
                        "en": "To break the prevailing calm, peace, or order."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нарушать",
                                "target_lang_sense_clarification": "порядок, покой или равновесие"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "disturb",
                                "target_lang_sense_clarification": "peace, sleep, or balance"
                            },
                            {
                                "target_lang_word": "disrupt",
                                "target_lang_sense_clarification": "order or stability"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
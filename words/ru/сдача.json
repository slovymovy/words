    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "029ca36b-2912-3ccf-a8c8-bc8fedd8eb35",
                    "sense_definition": "Деньги, которые возвращают покупателю, если он дал сумму больше стоимости покупки.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Возьмите, пожалуйста, вашу <w>сдачу</w>.",
                            "target_lang_translations": {
                                "en": "Please take your <w>change</w>."
                            }
                        },
                        {
                            "text": "У продавца не нашлось <w>сдачи</w> с крупной купюры.",
                            "target_lang_translations": {
                                "en": "The seller didn't have <w>change</w> for a large bill."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сдача с тысячи",
                        "получить сдачу",
                        "мелочь на сдачу",
                        "без сдачи"
                    ],
                    "target_lang_definitions": {
                        "en": "Money returned to the buyer if they gave an amount greater than the purchase cost."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "change",
                                "target_lang_sense_clarification": "money returned"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b8b2886-1dc0-3cab-a86d-479306d4e2b6",
                    "sense_definition": "Процесс передачи выполненной работы, экзаменов, документов или вещей по назначению.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action_give",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Успешная <w>сдача</w> экзаменов необходима для поступления в университет.",
                            "target_lang_translations": {
                                "en": "Successful <w>passing</w> of exams is necessary for university admission."
                            }
                        },
                        {
                            "text": "Регистрация и <w>сдача</w> багажа заканчиваются за сорок минут до вылета.",
                            "target_lang_translations": {
                                "en": "Check-in and baggage <w>drop-off</w> close forty minutes before departure."
                            }
                        }
                    ],
                    "synonyms": [
                        "передача",
                        "вручение"
                    ],
                    "antonyms": [
                        "приём",
                        "получение"
                    ],
                    "common_phrases": [
                        "сдача экзамена",
                        "сдача багажа",
                        "сдача крови",
                        "сдача отчёта",
                        "сдача объекта"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of handing over completed work, exams, documents, or items to their destination."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "passing",
                                "target_lang_sense_clarification": "exams"
                            },
                            {
                                "target_lang_word": "handing in",
                                "target_lang_sense_clarification": "documents, work"
                            },
                            {
                                "target_lang_word": "checking in",
                                "target_lang_sense_clarification": "luggage"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6298e5c9-1f61-31fa-a080-6ac5dac8ac40",
                    "sense_definition": "Ответный удар или действие в ответ на обиду или нападение.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если тебя ударили, нужно уметь дать <w>сдачи</w>.",
                            "target_lang_translations": {
                                "en": "If you are hit, you need to know how to <w>hit back</w>."
                            }
                        },
                        {
                            "text": "Хулиган испугался, что может получить <w>сдачи</w>, и убежал.",
                            "target_lang_translations": {
                                "en": "The bully was afraid he might <w>get hit back</w>, and ran away."
                            }
                        }
                    ],
                    "synonyms": [
                        "отпор"
                    ],
                    "common_phrases": [
                        "дать сдачи",
                        "получить сдачи"
                    ],
                    "target_lang_definitions": {
                        "en": "A counter-blow or action in response to an insult or attack."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hitting back",
                                "target_lang_sense_clarification": "action"
                            },
                            {
                                "target_lang_word": "retaliation",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2867681d-7656-3032-944e-3b83c8dde9b3",
                    "sense_definition": "Предоставление имущества во временное пользование за плату.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_give",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Сдача</w> жилья в аренду приносит семье дополнительный доход.",
                            "target_lang_translations": {
                                "en": "<w>Renting out</w> housing brings the family additional income."
                            }
                        },
                        {
                            "text": "Мы подготовили все документы для <w>сдачи</w> помещения под офис.",
                            "target_lang_translations": {
                                "en": "We prepared all documents for <w>renting out</w> the premises as an office."
                            }
                        }
                    ],
                    "synonyms": [
                        "аренда"
                    ],
                    "antonyms": [
                        "съём",
                        "наём"
                    ],
                    "common_phrases": [
                        "сдача квартиры",
                        "сдача внаём",
                        "сдача в аренду"
                    ],
                    "target_lang_definitions": {
                        "en": "Providing property for temporary use for a fee."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "renting out",
                                "target_lang_sense_clarification": "property"
                            },
                            {
                                "target_lang_word": "letting",
                                "target_lang_sense_clarification": "property"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1366e1d4-1bc6-3867-9204-08f955350b27",
                    "sense_definition": "Прекращение сопротивления врагу и признание своего поражения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Генерал подписал приказ о <w>сдаче</w> крепости, чтобы сохранить жизни солдат.",
                            "target_lang_translations": {
                                "en": "The general signed the order for the <w>surrender</w> of the fortress to save the soldiers' lives."
                            }
                        },
                        {
                            "text": "После долгой осады произошла <w>сдача</w> города вражеским войскам.",
                            "target_lang_translations": {
                                "en": "After a long siege, the <w>surrender</w> of the city to enemy troops took place."
                            }
                        }
                    ],
                    "synonyms": [
                        "капитуляция"
                    ],
                    "antonyms": [
                        "сопротивление"
                    ],
                    "common_phrases": [
                        "сдача в плен",
                        "сдача города",
                        "сдача оружия",
                        "безоговорочная сдача"
                    ],
                    "target_lang_definitions": {
                        "en": "Cessation of resistance to an enemy and acknowledgement of defeat."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "surrender",
                                "target_lang_sense_clarification": "military"
                            },
                            {
                                "target_lang_word": "capitulation",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88630a34-1cd9-30d2-9e44-79223e264c7f",
                    "sense_definition": "Раздача игральных карт участникам игры.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этой игре <w>сдача</w> переходит от игрока к игроку по часовой стрелке.",
                            "target_lang_translations": {
                                "en": "In this game, the <w>deal</w> passes from player to player clockwise."
                            }
                        },
                        {
                            "text": "При первой <w>сдаче</w> мне достались очень хорошие карты.",
                            "target_lang_translations": {
                                "en": "On the first <w>deal</w>, I got very good cards."
                            }
                        }
                    ],
                    "synonyms": [
                        "раздача"
                    ],
                    "common_phrases": [
                        "твоя сдача",
                        "новая сдача"
                    ],
                    "target_lang_definitions": {
                        "en": "Distributing playing cards to game participants."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deal",
                                "target_lang_sense_clarification": "cards"
                            },
                            {
                                "target_lang_word": "dealing",
                                "target_lang_sense_clarification": "process"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
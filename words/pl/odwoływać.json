                            {
                                "target_lang_word": "обращаться",
                                "target_lang_sense_clarification": "к опыту, знаниям или традиции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ae1006d-a8c9-38b2-88b8-40eea6251b9b",
                    "sense_definition": "Pozbawiać kogoś zajmowanego stanowiska lub pełnionej funkcji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dismiss_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rada nadzorcza <odwołała> prezesa spółki z powodu słabych wyników finansowych.",
                            "target_lang_translations": {
                                "ru": "Наблюдательный совет <w>снял</w> председателя компании с должности из-за слабых финансовых результатов."
                            }

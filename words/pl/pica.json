    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bcdfba4c-8c99-3154-bc62-1b126cf48a6f",
                    "sense_definition": "Zaburzenie psychiczne polegające na odczuwaniu przymusu spożywania substancji niejadalnych, takich jak ziemia, kreda czy węgiel.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "medical_disorder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "U kobiet w ciąży czasem występuje <w>pica</w>, objawiająca się chęcią jedzenia kredy.",
                            "target_lang_translations": {
                                "ru": "У беременных женщин иногда встречается <w>пика</w>, проявляющаяся в желании есть мел."
                            }
                        },
                        {
                            "text": "Nieleczona <w>pica</w> może prowadzić do poważnych problemów zdrowotnych u dzieci.",
                            "target_lang_translations": {
                                "ru": "Нелеченая <w>пика</w> может привести к серьезным проблемам со здоровьем у детей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Психическое расстройство, характеризующееся непреодолимым желанием употреблять в пищу несъедобные вещества, такие как земля, мел или уголь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пика",
                                "target_lang_sense_clarification": "медицинский термин"
                            },
                            {
                                "target_lang_word": "аллотриофагия",
                                "target_lang_sense_clarification": "научный термин"
                            },
                            {
                                "target_lang_word": "извращение вкуса",
                                "target_lang_sense_clarification": "симптом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8afe72fa-840c-3a55-a5bf-73f36f3ddc4b",
                    "sense_definition": "Wulgarne określenie żeńskich zewnętrznych narządów płciowych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "body_vulgar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Używanie słowa <w>pica</w> w miejscu publicznym jest uważane za wysoce niestosowne.",
                            "target_lang_translations": {
                                "ru": "Использование слова <w>пизда</w> в общественном месте считается крайне неуместным."
                            }
                        },
                        {
                            "text": "To wulgarne określenie, jakim jest <w>pica</w>, obraziło wielu słuchaczy.",
                            "target_lang_translations": {
                                "ru": "Это вульгарное слово, <w>пизда</w>, оскорбило многих слушателей."
                            }
                        }
                    ],
                    "synonyms": [
                        "cipa",
                        "pizda",
                        "psiocha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное название женских наружных половых органов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пизда",
                                "target_lang_sense_clarification": "вульгарное"
                            },
                            {
                                "target_lang_word": "писька",
                                "target_lang_sense_clarification": "разговорное, грубое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0408332-1063-30b6-a48b-4732cd8a33cc",
                    "sense_definition": "Dawne lub regionalne określenie na paszę dla zwierząt gospodarskich lub prowiant wojskowy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fodder_provisions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz przygotował świeżą <w>picę</w> dla koni na długą drogę.",
                            "target_lang_translations": {
                                "ru": "Хозяин приготовил свежий <w>фураж</w> для лошадей на долгую дорогу."
                            }
                        },
                        {
                            "text": "W dawnych czasach wojsko często narzekało na marną <w>picę</w> podczas kampanii.",
                            "target_lang_translations": {
                                "ru": "В былые времена войско часто жаловалось на скудный <w>провиант</w> во время походов."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasza",
                        "furaż"
                    ],
                    "common_phrases": [
                        "sucha pica"
                    ],

                {
                    "sense_id": "110e161a-b70b-367c-81c4-8af35b2940bc",
                    "sense_definition": "De structuur of indeling van een gebouw veranderen; renoveren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij gaan onze keuken volgend jaar volledig <w>verbouwen</w>.",
                            "target_lang_translations": {
                                "en": "We are going to completely <w>renovate</w> our kitchen next year.",
                                "ru": "В следующем году мы собираемся полностью <w>перестроить</w> нашу кухню."
                            }
                        },
                        {
                            "text": "Het oude station wordt <w>verbouwd</w> tot een moderne bibliotheek.",
                            "target_lang_translations": {
                                "en": "The old station is being <w>converted</w> into a modern library.",
                                "ru": "Старый вокзал <w>перестраивается</w> в современную библиотеку."
                            }
                        }
                    ],
                    "synonyms": [
                        "renoveren",
                        "ombouwen",
                        "vernieuwen"
                    ],
                    "antonyms": [
                        "slopen",
                        "afbreken"
                    ],
                    "common_phrases": [
                        "huis verbouwen",
                        "zolder verbouwen",
                        "garage verbouwen"
                    ],
                    "target_lang_definitions": {
                        "en": "To change the structure or layout of a building; to renovate.",
                        "ru": "Изменять структуру или планировку здания; перестраивать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "renovate",
                                "target_lang_sense_clarification": "to restore or improve a building"
                            },
                            {
                                "target_lang_word": "remodel",
                                "target_lang_sense_clarification": "to change the structure or form of a room or building"
                            },
                            {
                                "target_lang_word": "convert",
                                "target_lang_sense_clarification": "to change the function or purpose of a building"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "перестраивать",
                                "target_lang_sense_clarification": "менять строение"
                            },
                            {
                                "target_lang_word": "реконструировать",
                                "target_lang_sense_clarification": "обновлять здание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a0aafec-3859-3bcd-8251-3d68a9766a15",
                    "sense_definition": "Gewassen op het land laten groeien voor de oogst; telen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Boeren <w>verbouwen</w> hier veel aardappelen en uien op het land.",
                            "target_lang_translations": {
                                "en": "Farmers <w>grow</w> many potatoes and onions on the land here.",
                                "ru": "Фермеры <w>выращивают</w> здесь на полях много картофеля и лука."
                            }
                        },
                        {
                            "text": "In dit koude klimaat kun je geen druiven <w>verbouwen</w>.",
                            "target_lang_translations": {
                                "en": "In this cold climate you cannot <w>grow</w> grapes.",
                                "ru": "В этом холодном климате нельзя <w>выращивать</w> виноград."
                            }
                        }
                    ],
                    "synonyms": [

{
    "word_family": [
        "slate",
        "slater",
        "slating"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c59490e5-2599-34b8-b246-19a858099f79",
                    "sense_definition": "Scheduled or designated to happen at a specific time or to be used for a specific purpose.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "planning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new museum is <w>slated</w> to open to the public next month.",
                            "target_lang_translations": {
                                "ru": "Открытие нового музея для публики <w>запланировано</w> на следующий месяц."
                            }
                        },
                        {
                            "text": "Several old buildings are <w>slated</w> for demolition to make way for the park.",
                            "target_lang_translations": {
                                "ru": "Несколько старых зданий <w>намечены</w> под снос, чтобы освободить место для парка."
                            }
                        }
                    ],
                    "synonyms": [
                        "scheduled",
                        "planned",
                        "set"
                    ],
                    "common_phrases": [
                        "slated for",
                        "slated to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запланированный или назначенный на определенное время или для определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запланированный",
                                "target_lang_sense_clarification": "включенный в план"
                            },
                            {
                                "target_lang_word": "намеченный",
                                "target_lang_sense_clarification": "назначенный на время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b6adc7a-7c75-3472-a95a-4c7ac18ea396",
                    "sense_definition": "Covered with slate tiles, typically referring to a roof.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cottage has a steeply <w>slated</w> roof that withstands heavy rain.",
                            "target_lang_translations": {
                                "ru": "У коттеджа крутая <w>крытая сланцем</w> крыша, которая выдерживает сильный дождь."
                            }
                        },
                        {
                            "text": "We admired the grey <w>slated</w> turrets of the ancient castle.",
                            "target_lang_translations": {
                                "ru": "Мы любовались серыми <w>крытыми сланцем</w> башенками древнего замка."
                            }
                        }
                    ],
                    "synonyms": [
                        "tiled"
                    ],
                    "common_phrases": [
                        "slated roof"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрытый сланцевой черепицей или шифером, обычно о крыше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крытый сланцем",
                                "target_lang_sense_clarification": "о крыше"
                            },
                            {
                                "target_lang_word": "сланцевый",
                                "target_lang_sense_clarification": "сделанный из сланца"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "813869b5-8c8b-3850-ab5c-b21271ac7bf9",
                    "sense_definition": "Severely criticized or reviewed unfavorably.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "criticism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The widely <w>slated</w> proposal was eventually withdrawn by the council.",
                            "target_lang_translations": {
                                "ru": "Широко <w>раскритикованное</w> предложение было в итоге отозвано советом."
                            }
                        },
                        {
                            "text": "Despite being <w>slated</w> by critics, the movie became a commercial success.",
                            "target_lang_translations": {
                                "ru": "Несмотря на то, что фильм был <w>раскритикован</w> критиками, он имел коммерческий успех."
                            }
                        }
                    ],
                    "synonyms": [
                        "criticized",
                        "panned"
                    ],
                    "antonyms": [
                        "praised",
                        "acclaimed"
                    ],
                    "common_phrases": [
                        "heavily slated"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергшийся резкой критике или получивший негативные отзывы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскритикованный",
                                "target_lang_sense_clarification": "получивший плохие отзывы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "41fe1e3f-298e-322d-99be-e6f4996ea45f",
                    "sense_definition": "Simple past and past participle of <w>slate</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee <w>slated</w> the conference for the first week of June.",
                            "target_lang_translations": {
                                "ru": "Комитет <w>наметил</w> конференцию на первую неделю июня."
                            }
                        },
                        {
                            "text": "Newspapers <w>slated</w> the politician for his controversial remarks.",
                            "target_lang_translations": {
                                "ru": "Газеты <w>раскритиковали</w> политика за его спорные высказывания."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола slate."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наметил",
                                "target_lang_sense_clarification": "запланировал (прош. вр.)"
                            },
                            {
                                "target_lang_word": "раскритиковал",
                                "target_lang_sense_clarification": "подверг критике (прош. вр.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
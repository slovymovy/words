    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ecf3d77e-40f9-3ae1-aa33-7d5cf8ef8693",
                    "sense_definition": "The hard outer covering that protects certain animals, such as snails, oysters, turtles, and crabs.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We walked along the beach looking for a pretty <w>shell</w> to take home.",
                            "target_lang_translations": {
                                "ru": "Мы гуляли по пляжу в поисках красивой <w>ракушки</w>, чтобы забрать её домой."
                            }
                        },
                        {
                            "text": "The turtle pulled its head back into its <w>shell</w> when it saw the dog.",
                            "target_lang_translations": {
                                "ru": "Черепаха втянула голову в <w>панцирь</w>, когда увидела собаку."
                            }
                        }
                    ],
                    "synonyms": [
                        "shell-like",
                        "shell-like"
                    ],
                    "common_phrases": [
                        "sea shell",
                        "snail shell",
                        "turtle shell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердое внешнее покрытие, защищающее определенных животных, таких как улитки, устрицы, черепахи и крабы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раковина",
                                "target_lang_sense_clarification": "моллюска"
                            },
                            {
                                "target_lang_word": "панцирь",
                                "target_lang_sense_clarification": "черепахи, краба"
                            },
                            {
                                "target_lang_word": "ракушка",
                                "target_lang_sense_clarification": "найденная на пляже"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ffdf79f-b4fc-3cfc-8f5a-5dc2c22bb790",
                    "sense_definition": "The hard outer layer of an egg, nut, or seed that protects what is inside.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful not to get any pieces of <w>shell</w> in the cake batter.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, чтобы кусочки <w>скорлупы</w> не попали в тесто для торта."
                            }

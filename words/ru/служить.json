    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fb80fb05-a934-30ff-9b2f-b967f153821c",
                    "sense_definition": "Состоять на военной службе, исполнять воинский долг.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой старший брат сейчас <w>служит</w> в армии.",
                            "target_lang_translations": {
                                "en": "My older brother is currently <w>serving</w> in the army."
                            }
                        },
                        {
                            "text": "Офицер долго <w>служил</w> на флоте и много путешествовал.",
                            "target_lang_translations": {
                                "en": "The officer <w>served</w> in the navy for a long time and traveled a lot."
                            }
                        }
                    ],
                    "synonyms": [
                        "состоять"
                    ],
                    "common_phrases": [
                        "служить в армии",
                        "служить на флоте",
                        "служить на границе"
                    ],
                    "target_lang_definitions": {
                        "en": "To be a member of the armed forces; to perform military duty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serve",
                                "target_lang_sense_clarification": "in the military"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e12aef5-444a-34da-a558-f9cb6b0c58b3",
                    "sense_definition": "Выполнять роль чего-либо, являться чем-либо, использоваться для какой-либо цели.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его слова <w>послужили</w> сигналом к началу действий.",
                            "target_lang_translations": {
                                "en": "His words <w>served</w> as a signal to start the action."
                            }
                        },
                        {
                            "text": "Этот старый ящик <w>служит</w> нам столом во время пикника.",
                            "target_lang_translations": {
                                "en": "This old box <w>serves</w> as a table for us during the picnic."
                            }
                        }
                    ],
                    "synonyms": [
                        "являться",
                        "использоваться",
                        "предназначаться"
                    ],
                    "common_phrases": [
                        "служить примером",
                        "служить доказательством",
                        "служить причиной",
                        "служить основой"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform a specific function or role; to be used for a specific purpose."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serve",
                                "target_lang_sense_clarification": "function as"
                            },
                            {
                                "target_lang_word": "act",
                                "target_lang_sense_clarification": "function as"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c67434aa-ee72-3b17-bc61-90f2184ec06d",
                    "sense_definition": "Работать, занимать должность (обычно в государственных учреждениях, церкви или театре).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Известная актриса <w>служит</w> в этом театре уже двадцать лет.",
                            "target_lang_translations": {
                                "en": "The famous actress has been <w>working</w> in this theater for twenty years."
                            }
                        },
                        {
                            "text": "Его отец всю жизнь <w>служил</w> чиновником в министерстве.",
                            "target_lang_translations": {
                                "en": "His father <w>served</w> as an official in the ministry all his life."
                            }
                        }
                    ],
                    "synonyms": [
                        "работать",
                        "состоять"
                    ],
                    "common_phrases": [
                        "служить в театре",
                        "служить в министерстве",
                        "поступить служить"
                    ],
                    "target_lang_definitions": {
                        "en": "To hold an official position or work, typically in government, church, or theater."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serve",
                                "target_lang_sense_clarification": "hold a position"
                            },
                            {
                                "target_lang_word": "work",
                                "target_lang_sense_clarification": "be employed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab7e2d25-387c-3409-8618-0491c6076149",
                    "sense_definition": "Делать что-то для пользы и блага кого-либо, выполнять чью-то волю, посвящать свои силы делу.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "devotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы готовы честно <w>служить</w> народу и своей стране.",
                            "target_lang_translations": {
                                "en": "We are ready to honestly <w>serve</w> the people and our country."
                            }
                        },
                        {
                            "text": "Он хотел <w>служить</w> науке, но обстоятельства сложились иначе.",
                            "target_lang_translations": {
                                "en": "He wanted to <w>serve</w> science, but circumstances turned out otherwise."
                            }
                        }
                    ],
                    "synonyms": [
                        "помогать",
                        "угождать",
                        "содействовать"
                    ],
                    "antonyms": [
                        "приказывать"
                    ],
                    "common_phrases": [
                        "служить родине",
                        "служить верой и правдой",
                        "служить людям",
                        "служить искусству"
                    ],
                    "target_lang_definitions": {
                        "en": "To work for the benefit of someone or something; to devote oneself to a cause."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serve",
                                "target_lang_sense_clarification": "be of service to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "765916bd-3a63-35c4-a3c0-6fdf3f713971",
                    "sense_definition": "Быть пригодным к употреблению, работать исправно (о вещах, механизмах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "items",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это зимнее пальто <w>служило</w> мне много лет.",
                            "target_lang_translations": {
                                "en": "This winter coat <w>served</w> me well for many years."
                            }
                        },
                        {
                            "text": "Старый автомобиль всё ещё <w>служит</w> и редко ломается.",
                            "target_lang_translations": {
                                "en": "The old car still <w>runs</w> and rarely breaks down."
                            }
                        }
                    ],
                    "synonyms": [
                        "работать",
                        "действовать",
                        "годиться"
                    ],
                    "antonyms": [
                        "ломаться"
                    ],
                    "common_phrases": [
                        "долго служить",
                        "верно служить"
                    ],
                    "target_lang_definitions": {
                        "en": "To remain in usable condition; to function properly over time (of objects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "last",
                                "target_lang_sense_clarification": "endure"
                            },
                            {
                                "target_lang_word": "serve",
                                "target_lang_sense_clarification": "be useful"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8138814c-e7be-320a-a9b0-dc5cc2cfcaee",
                    "sense_definition": "Совершать церковные обряды, проводить богослужение.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В воскресенье священник <w>служит</w> праздничную литургию.",
                            "target_lang_translations": {
                                "en": "On Sunday, the priest <w>celebrates</w> the festive liturgy."
                            }
                        },
                        {
                            "text": "Отец Михаил <w>служит</w> в небольшом деревенском храме.",
                            "target_lang_translations": {
                                "en": "Father Mikhail <w>serves</w> in a small village church."
                            }
                        }
                    ],
                    "common_phrases": [
                        "служить в храме",
                        "служить мессу",
                        "служить молебен"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform religious rites or conduct a religious service."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "celebrate",
                                "target_lang_sense_clarification": "mass or liturgy"
                            },
                            {
                                "target_lang_word": "officiate",
                                "target_lang_sense_clarification": "religious ceremony"
                            },
                            {
                                "target_lang_word": "serve",
                                "target_lang_sense_clarification": "in a church"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71aca046-c063-3308-9542-48373c4fdcb0",
                    "sense_definition": "Стоять на задних лапах (о собаке).",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "dog",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленькая собачка умеет <w>служить</w>, выпрашивая еду.",
                            "target_lang_translations": {
                                "en": "The little dog knows how to <w>beg</w>, asking for food."
                            }
                        },
                        {
                            "text": "Хозяин скомандовал псу: «<w>Служи</w>!»",
                            "target_lang_translations": {
                                "en": "The owner commanded the dog: \"<w>Beg</w>!\""
                            }
                        }
                    ],
                    "common_phrases": [
                        "собака служит",
                        "учить служить"
                    ],
                    "target_lang_definitions": {
                        "en": "To sit up on hind legs (of a dog)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beg",
                                "target_lang_sense_clarification": "dog trick"
                            },
                            {
                                "target_lang_word": "sit up",
                                "target_lang_sense_clarification": "dog trick"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
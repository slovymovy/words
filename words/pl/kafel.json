    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1bd6fc30-5b65-3571-aefb-b24045335299",
                    "sense_definition": "Płytka ceramiczna, często ozdobna, używana do pokrywania ścian, podłóg lub do budowy pieców grzewczych.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ceramika_budowlana",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszej kuchni na podłodze położyliśmy duże, białe <w>kafle</w>.",
                            "target_lang_translations": {
                                "ru": "Мы положили на пол в нашей кухне большую белую <w>плитку</w>."
                            }
                        },
                        {
                            "text": "Stary piec <w>kaflowy</w> w salonie nadal świetnie ogrzewa całe mieszkanie.",
                            "target_lang_translations": {
                                "ru": "Старая <w>изразцовая</w> печь в гостиной до сих пор отлично обогревает всю квартиру."
                            }
                        }
                    ],
                    "synonyms": [
                        "płytka",
                        "płyta"
                    ],
                    "common_phrases": [
                        "piec kaflowy",
                        "układać kafle",
                        "myć kafle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Керамическая плитка, часто декоративная, используемая для облицовки стен, полов или для строительства отопительных печей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кафель",
                                "target_lang_sense_clarification": "материал, глазурованная плитка"
                            },
                            {
                                "target_lang_word": "плитка",
                                "target_lang_sense_clarification": "отдельный элемент покрытия"
                            },
                            {
                                "target_lang_word": "изразец",
                                "target_lang_sense_clarification": "специальная плитка для облицовки печей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2127f5c2-5efd-326a-9f01-6feb4777d773",
                    "sense_definition": "Potoczne określenie kwoty tysiąca jednostek walutowych, zazwyczaj złotych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "pieniądze_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naprawa samochodu kosztowała mnie aż trzy <w>kafle</w>, co było sporym wydatkiem.",
                            "target_lang_translations": {
                                "ru": "Ремонт машины обошелся мне аж в три <w>штуки</w>, что было немалой тратой."
                            }
                        },
                        {
                            "text": "Mój znajomy zarabia miesięcznie pięć <w>kafli</w> na rękę.",
                            "target_lang_translations": {
                                "ru": "Мой знакомый зарабатывает пять <w>штук</w> чистыми в месяц."
                            }
                        }
                    ],
                    "synonyms": [
                        "tysiak",
                        "patyk",
                        "koło"
                    ],
                    "common_phrases": [
                        "dwa kafle",
                        "kilka kafli"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "określenie pieniędzy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное обозначение суммы в тысячу денежных единиц, обычно злотых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штука",
                                "target_lang_sense_clarification": "разг. тысяча"
                            },
                            {
                                "target_lang_word": "косарь",
                                "target_lang_sense_clarification": "сленг. тысяча"
                            }
                        ]
                    }
                }
            ]
        }
    ]

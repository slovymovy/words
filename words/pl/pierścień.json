            "pos": "noun",
            "senses": [
                {
                    "sense_id": "626684e5-03cc-3c7a-b2fa-8936062aca52",
                    "sense_definition": "Ozdoba w kształcie obręczy, często wykonana z metalu szlachetnego i posiadająca kamień, noszona na palcu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "jewelry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na jej serdecznym palcu błyszczał piękny, złoty <w>pierścień</w> z rubinem.",
                            "target_lang_translations": {
                                "ru": "На её безымянном пальце сверкал красивый золотой <w>перстень</w> с рубином."
                            }
                        },
                        {
                            "text": "Podczas uroczystej ceremonii król przekazał swój <w>pierścień</w> następcy tronu.",
                            "target_lang_translations": {
                                "ru": "Во время торжественной церемонии король передал свой <w>перстень</w> наследнику трона."
                            }
                        }
                    ],
                    "synonyms": [
                        "sygnet"
                    ],
                    "common_phrases": [
                        "złoty pierścień",
                        "pierścień z diamentem",
                        "pierścień zaręczynowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшение в форме обода, часто из драгоценного металла с камнем, которое носят на пальце."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кольцо",
                                "target_lang_sense_clarification": "общее название украшения"
                            },
                            {
                                "target_lang_word": "перстень",
                                "target_lang_sense_clarification": "массивное кольцо, обычно с драгоценным камнем или печаткой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10164838-ad40-3c7a-8b83-742f584e8959",
                    "sense_definition": "Zamknięty układ ludzi, wojsk lub przedmiotów otaczających kogoś lub coś ze wszystkich stron.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "shape_arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja utworzyła szczelny <w>pierścień</w> wokół budynku, aby nikt nie mógł uciec.",
                            "target_lang_translations": {
                                "ru": "Полиция создала плотное <w>кольцо</w> вокруг здания, чтобы никто не мог сбежать."
                            }
                        },
                        {
                            "text": "Wrogie wojska zamknęły <w>pierścień</w> okrążenia wokół broniącego się miasta.",
                            "target_lang_translations": {
                                "ru": "Вражеские войска сомкнули <w>кольцо</w> окружения вокруг обороняющегося города."
                            }

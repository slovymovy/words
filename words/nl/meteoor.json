    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "df3b4a00-c4ab-38b5-8ca6-e3c26e933123",
                    "sense_definition": "Een kortstondig lichtspoor aan de hemel dat ontstaat wanneer een klein stukje materie uit de ruimte in de dampkring verbrandt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "astronomy_phenomenon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We zagen gisteravond een prachtige <w>meteoor</w> door de lucht schieten.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером мы видели, как красивый <w>метеор</w> пронесся по небу."
                            }
                        },
                        {
                            "text": "Als je geluk hebt, kun je vannacht een heldere <w>meteoor</w> waarnemen.",
                            "target_lang_translations": {
                                "ru": "Если повезет, сегодня ночью можно будет наблюдать яркий <w>метеор</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "vallende ster"
                    ],
                    "common_phrases": [
                        "een meteoor waarnemen",
                        "heldere meteoor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кратковременный световой след в небе, возникающий при сгорании в атмосфере мелких частиц космического вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "метеор",
                                "target_lang_sense_clarification": "астрономическое явление"
                            },
                            {
                                "target_lang_word": "падающая звезда",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4da1ddc9-f486-36de-8789-5bab4905df4b",
                    "sense_definition": "Een hemellichaam dat licht uitstraalt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude poëzie wordt de zon soms beschreven als een gouden <w>meteoor</w>.",
                            "target_lang_translations": {
                                "ru": "В старой поэзии солнце иногда описывается как золотой <w>метеор</w>."
                            }
                        },
                        {
                            "text": "Elke <w>meteoor</w> aan het firmament draagt bij aan de pracht van de nachtelijke hemel.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>метеор</w> на небосводе вносит свой вклад в великолепие ночного неба."
                            }
                        }
                    ],
                    "synonyms": [
                        "hemellicht"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небесное тело, излучающее свет (часто используется в поэтическом или устаревшем контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "метеор",
                                "target_lang_sense_clarification": "небесное тело, поэтич."
                            },
                            {
                                "target_lang_word": "светило",
                                "target_lang_sense_clarification": "источник света на небе"
                            }
                        ]
                    }
                }
            ]
        }
    ]

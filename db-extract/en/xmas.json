                "senses": [
                    {
                        "sense_id": "ea673b0d-0eb2-3159-a151-dbfbe25b9d46",
                        "entry_id": "c11cf9cf-fcb0-3fd9-907e-ce7ee8aec702",
                        "glosses": [
                            "Abkürzung für Christmas; Weihnachten"
                        ],
                        "tags": [
                            "colloquial"
                        ],
                        "examples": [
                            {
                                "example_id": "6396b213-a116-431b-b61e-b9c33fb23330",
                                "sense_id": "ea673b0d-0eb2-3159-a151-dbfbe25b9d46",
                                "text": "I wish you merry Xmas! - Ich wünsche Dir frohe Weihnachten!"
                            }
                        ],
                        "sense_index_json": "\"1\""
                    }
                ],
                "translations": [
                    {
                        "translation_id": "7e76bdf3-181b-463c-ab3b-323707c25bf1",
                        "source_entry_id": "c11cf9cf-fcb0-3fd9-907e-ce7ee8aec702",
                        "source_sense_id": "ea673b0d-0eb2-3159-a151-dbfbe25b9d46",
                        "source_sense_description": "umgangssprachlich: Abkürzung für das Wort Christmas, also Weihnachten",
                        "target_lang_word": "Weihnachten",
                        "target_lang_code": "de",
                        "target_sense_ids": [],
                        "cross_references": [
                            {
                                "target_word": "Weihnachten",
                                "target_lang_code": "de",
                                "target_senses": [
                                    {
                                        "sense_id": "4546f9f3-8298-3355-83c1-38a4642e05c6",
                                        "entry_id": "c7a22a8e-be8f-303f-abdc-8c8fd16d6508",
                                        "glosses": [
                                            "christlicher Feiertag am 25. Dezember, an dem die Geburt von Jesus Christus gefeiert wird; in vielen Staaten ein gesetzlicher Feiertag und Auftakt der Weihnachtsferien; in Deutschland, Österreich und Teilen der Schweiz kommt der 26. Dezember als zweiter staatlicher Feiertag (zweiter Weihnachtstag, zweiter Weihnachtsfeiertag) dazu"
                                        ],
                                        "tags": [],
                                        "examples": [
                                            {
                                                "example_id": "09604b7f-9ed2-4001-b5e7-d019a7b24ca0",
                                                "sense_id": "4546f9f3-8298-3355-83c1-38a4642e05c6",
                                                "text": "In drei Tagen ist Weihnachten."
                                            },
                                            {
                                                "example_id": "b3a15443-c6ab-4b96-b1c6-c5fe3dfdf370",
                                                "sense_id": "4546f9f3-8298-3355-83c1-38a4642e05c6",
                                                "text": "„Suppenküche der Johanniter. Bedürftigen ein schönes Weihnachten bescheren.“"
                                            },
                                            {
                                                "example_id": "d33f36d9-80a0-4735-9f50-d5bd4d661b95",
                                                "sense_id": "4546f9f3-8298-3355-83c1-38a4642e05c6",
                                                "text": "„Die gemütlichen Weihnachten sind eine bürgerliche Erfindung des 19. Jahrhunderts.“"
                                            },
                                            {
                                                "example_id": "ccd6398f-5f9b-4a86-8bf2-acad2081fd16",
                                                "sense_id": "4546f9f3-8298-3355-83c1-38a4642e05c6",
                                                "text": "Frohe Weihnachten und ein glückliches neues Jahr. ( Frohe Weihnachten und ein glückliches neues Jahr.⁽ᴵⁿᶠᵒ⁾)"
                                            },
                                            {
                                                "example_id": "cdac979d-61e2-4bf9-b2ee-a611c6d27644",
                                                "sense_id": "4546f9f3-8298-3355-83c1-38a4642e05c6",
                                                "text": "Zu Weihnachten soll es schneien."
                                            },
                                            {
                                                "example_id": "b291e594-e56a-4aca-9b21-f1c8cc5a9c04",
                                                "sense_id": "4546f9f3-8298-3355-83c1-38a4642e05c6",
                                                "text": "An Weihnachten soll es nicht schneien."
                                            },
                                            {
                                                "example_id": "2c91b82d-9eb4-4f91-9be2-b85a26bef470",
                                                "sense_id": "4546f9f3-8298-3355-83c1-38a4642e05c6",
                                                "text": "Wir fahren über Weihnachten in Urlaub."
                                            },
                                            {
                                                "example_id": "abb908d9-3d7b-4db6-b9d8-a422f1fa20de",
                                                "sense_id": "4546f9f3-8298-3355-83c1-38a4642e05c6",
                                                "text": "Letzte Weihnachten hat es geregnet."
                                            },
                                            {
                                                "example_id": "6e3b385f-d379-49c8-8d84-ffc7b4e37b86",
                                                "sense_id": "4546f9f3-8298-3355-83c1-38a4642e05c6",
                                                "text": "„Nach Weihnachten fuhren Mama und Papa zum Klassentreffen nach Jever.“"
                                            },
                                            {
                                                "example_id": "dd0ee12f-392b-4602-b744-1baf714c25bb",
                                                "sense_id": "4546f9f3-8298-3355-83c1-38a4642e05c6",
                                                "text": "„In Deutschland waren wir Weihnachten immer bei meiner Großmutter, das war sehr schön.“"
                                            }
                                        ],
                                        "sense_index_json": "\"1\""
                                    }
                                ]
                            }
                        ]
                    }
                ],

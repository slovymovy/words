            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e6d0c35-a798-319e-b175-827d5a20d93c",
                    "sense_definition": "Możliwość porozumiewania się, wymiany informacji lub utrzymywanie relacji z drugą osobą.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jesteśmy w stałym <w>kontakcie</w> telefonicznym i mailowym.",
                            "target_lang_translations": {
                                "ru": "Мы поддерживаем постоянный телефонный и почтовый <w>контакт</w>."
                            }
                        },
                        {
                            "text": "Po przeprowadzce do innego miasta straciłem <w>kontakt</w> z dawnymi kolegami.",
                            "target_lang_translations": {
                                "ru": "После переезда в другой город я потерял <w>связь</w> со старыми приятелями."
                            }
                        }
                    ],
                    "synonyms": [
                        "łączność",
                        "połączenie",
                        "styczność"
                    ],
                    "antonyms": [
                        "izolacja"
                    ],
                    "common_phrases": [
                        "być w kontakcie",
                        "nawiązać kontakt",
                        "utrzymywać kontakt",
                        "zerwać kontakt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возможность общения, обмена информацией или поддержания отношений с другим человеком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контакт",
                                "target_lang_sense_clarification": "общение, взаимодействие"
                            },
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "соединение, коммуникация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "783e28f8-5f3c-3de0-9ed3-4504bd46820e",
                    "sense_definition": "Element instalacji elektrycznej w ścianie, do którego wkłada się wtyczkę, aby podłączyć urządzenie do prądu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "electricity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sprawdź, czy wtyczka jest dobrze włożona do <w>kontaktu</w>.",
                            "target_lang_translations": {
                                "ru": "Проверь, хорошо ли вилка вставлена в <w>розетку</w>."
                            }
                        },
                        {
                            "text": "W tym pokoju jest tylko jeden <w>kontakt</w>, więc potrzebujemy przedłużacza.",
                            "target_lang_translations": {
                                "ru": "В этой комнате только одна <w>розетка</w>, поэтому нам нужен удлинитель."
                            }
                        }
                    ],
                    "synonyms": [
                        "gniazdko",
                        "gniazdo"
                    ],
                    "common_phrases": [
                        "włożyć do kontaktu",
                        "wyjąć z kontaktu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "standardowo nazywane gniazdem lub gniazdkiem"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент электропроводки в стене, в который вставляется вилка для подключения прибора к сети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "розетка",
                                "target_lang_sense_clarification": "электрическая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4af91a2-cc84-35af-bfe0-bed6e64fc5ed",
                    "sense_definition": "Bezpośrednie zetknięcie się dwóch ciał, przedmiotów lub substancji; fizyczna bliskość.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_touch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Unikaj bezpośredniego <w>kontaktu</w> skóry z tą substancją chemiczną.",
                            "target_lang_translations": {
                                "ru": "Избегай прямого <w>контакта</w> кожи с этим химическим веществом."
                            }
                        },
                        {
                            "text": "Piłka wyszła na aut po <w>kontakcie</w> z nogą obrońcy.",
                            "target_lang_translations": {
                                "ru": "Мяч ушел в аут после <w>соприкосновения</w> с ногой защитника."
                            }
                        }
                    ],
                    "synonyms": [
                        "dotyk",
                        "styczność",
                        "stykanie się"
                    ],
                    "antonyms": [
                        "izolacja"
                    ],
                    "common_phrases": [
                        "kontakt fizyczny",
                        "kontakt wzrokowy",
                        "bezpośredni kontakt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непосредственное соприкосновение двух тел, предметов или веществ; физическая близость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контакт",

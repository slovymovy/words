    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "43293827-216e-302a-bd5e-fe5a05765317",
                    "sense_definition": "Een stuk gereedschap met een scherp, getand metalen blad om materialen zoals hout in stukken te verdelen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De timmerman pakte zijn <w>zaag</w> om de plank in tweeën te delen.",
                            "target_lang_translations": {
                                "ru": "Плотник взял свою <w>пилу</w>, чтобы распилить доску пополам."
                            }
                        },
                        {
                            "text": "Pas goed op je vingers als je met een scherpe <w>zaag</w> werkt.",
                            "target_lang_translations": {
                                "ru": "Береги пальцы, когда работаешь острой <w>пилой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "scherpe zaag",
                        "met de zaag",
                        "elektrische zaag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструмент с острым зубчатым металлическим лезвием для разрезания материалов, например дерева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пила",
                                "target_lang_sense_clarification": "инструмент"
                            },
                            {
                                "target_lang_word": "ножовка",
                                "target_lang_sense_clarification": "ручная пила"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44fb5945-3e58-3fb5-a7ae-0a7ffa0763b0",
                    "sense_definition": "Een persoon, vaak een vrouw, die voortdurend zeurt of klaagt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn buurvrouw is een echte <w>zaag</w> die over alles klaagt.",
                            "target_lang_translations": {
                                "ru": "Моя соседка — настоящая <w>зануда</w>, которая жалуется на всё."
                            }
                        },
                        {
                            "text": "Luister niet naar die oude <w>zaag</w>, ze is nooit tevreden.",
                            "target_lang_translations": {
                                "ru": "Не слушай эту старую <w>ворчунью</w>, она никогда не бывает довольна."
                            }
                        }
                    ],
                    "synonyms": [
                        "zeur",
                        "zanikster"
                    ],
                    "common_phrases": [
                        "oude zaag",
                        "wat een zaag"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, часто женщина, который постоянно ноет или жалуется."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зануда",
                                "target_lang_sense_clarification": "надоедливый человек"
                            },
                            {
                                "target_lang_word": "пила",
                                "target_lang_sense_clarification": "перен. ворчливая женщина"
                            },
                            {
                                "target_lang_word": "ворчунья",
                                "target_lang_sense_clarification": "женщина, которая ворчит"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "516e2498-469c-33a4-9f64-62af8f1d8376",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>zagen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>zaag</w> het hout in kleine blokken voor de open haard.",
                            "target_lang_translations": {
                                "ru": "Я <w>пилю</w> дрова на мелкие чурбаки для камина."
                            }
                        },
                        {
                            "text": "Vandaag <w>zaag</w> ik alle planken op de juiste lengte.",
                            "target_lang_translations": {
                                "ru": "Сегодня я <w>пилю</w> все доски нужной длины."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола <w>zagen</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пилю",
                                "target_lang_sense_clarification": "форма глагола \"пилить\""
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21893d33-19a0-3468-bbd4-3e8ba5d4f4ee",
                    "sense_definition": "Gebiedende wijs van <w>zagen</w>.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Zaag</w> die tak voorzichtig door zodat hij niet op het dak valt.",
                            "target_lang_translations": {
                                "ru": "<w>Пили</w> эту ветку осторожно, чтобы она не упала на крышу."
                            }
                        },
                        {
                            "text": "<w>Zaag</w> recht, anders past het stuk niet in de kast.",
                            "target_lang_translations": {
                                "ru": "<w>Пили</w> ровно, иначе деталь не войдет в шкаф."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола <w>zagen</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пили",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
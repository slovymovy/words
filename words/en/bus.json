{
    "word_family": [
        "minibus",
        "omnibus",
        "busboy",
        "busload",
        "busman"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bd79897a-b6c0-382d-bb8c-e8f367a8cef9",
                    "sense_definition": "A motor vehicle for transporting large numbers of people along roads.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transportation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I take the <w>bus</w> to school every morning because it is cheaper than driving.",
                            "target_lang_translations": {
                                "ru": "Я езжу в школу на <w>автобусе</w> каждое утро, потому что это дешевле, чем на машине."
                            }
                        },
                        {
                            "text": "We missed the last <w>bus</w> home and had to call a taxi.",
                            "target_lang_translations": {
                                "ru": "Мы опоздали на последний <w>автобус</w> домой, и нам пришлось вызвать такси."
                            }
                        }
                    ],
                    "synonyms": [
                        "coach",
                        "omnibus",
                        "motorbus",
                        "autobus"
                    ],
                    "common_phrases": [
                        "take the bus",
                        "miss the bus",
                        "bus stop",
                        "school bus",
                        "bus driver"
                    ],
                    "target_lang_definitions": {
                        "ru": "Моторное транспортное средство для перевозки большого количества людей по дорогам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "автобус",
                                "target_lang_sense_clarification": "транспортное средство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f49dead-9bfc-3042-b45f-372b2b0dc1e3",
                    "sense_definition": "An electrical conductor or interface serving as a common connection for two or more circuits or components.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "electronics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The data <w>bus</w> allows the processor to communicate with the memory units.",
                            "target_lang_translations": {
                                "ru": "<w>Шина</w> данных позволяет процессору обмениваться информацией с модулями памяти."
                            }
                        },
                        {
                            "text": "A fault in the main system <w>bus</w> caused the entire computer to crash.",
                            "target_lang_translations": {
                                "ru": "Сбой в главной системной <w>шине</w> привел к полному отказу компьютера."
                            }
                        }
                    ],
                    "synonyms": [
                        "busbar",
                        "electrical bus"
                    ],
                    "common_phrases": [
                        "data bus",
                        "system bus",
                        "universal serial bus"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Электрический проводник или интерфейс, служащий общим соединением для двух или более цепей или компонентов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шина",
                                "target_lang_sense_clarification": "в электронике и вычислительной технике"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e88c01b3-537a-301b-bb9a-beee329ad526",
                    "sense_definition": "To transport people using a bus.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transportation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The school decided to <w>bus</w> the students to the museum for the field trip.",
                            "target_lang_translations": {
                                "ru": "Школа решила <w>отвезти на автобусе</w> учеников в музей на экскурсию."
                            }
                        },
                        {
                            "text": "Protesters were <w>bussed</w> in from neighboring cities to join the demonstration.",
                            "target_lang_translations": {
                                "ru": "Протестующих <w>привезли на автобусах</w> из соседних городов для участия в демонстрации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bus in",
                        "bus out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перевозить людей на автобусе."
                    },
                    "translations": {
                        "ru": []
                    }
                },
                {
                    "sense_id": "05fde0f7-2d3d-3963-9b8f-2f5d9e8d54d2",
                    "sense_definition": "To clear dirty dishes and remains of meals from tables in a restaurant.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hospitality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He started working at the diner to <w>bus</w> tables during the summer break.",
                            "target_lang_translations": {
                                "ru": "Он начал работать в закусочной и <w>убирать</w> со столов во время летних каникул."
                            }
                        },
                        {
                            "text": "Please <w>bus</w> your own tray to the rack when you are finished eating.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>отнесите</w> свой поднос на стойку, когда закончите есть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bus tables"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Убирать грязную посуду и остатки еды со столов в ресторане."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }
    ]
}
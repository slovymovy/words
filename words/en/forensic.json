    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7749fe12-0d20-3996-82c7-76c323f93dac",
                    "sense_definition": "Relating to the use of scientific methods to investigate crimes or establish facts in a court of law.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "science_law",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police found no <w>forensic</w> evidence at the crime scene to link him to the theft.",
                            "target_lang_translations": {
                                "ru": "Полиция не нашла на месте преступления никаких <w>криминалистических</w> улик, связывающих его с кражей."
                            }
                        },
                        {
                            "text": "She decided to study <w>forensic</w> science because she wanted to help solve complex cases.",
                            "target_lang_translations": {
                                "ru": "Она решила изучать <w>судебную</w> экспертизу, потому что хотела помогать раскрывать сложные дела."
                            }
                        }
                    ],
                    "common_phrases": [
                        "forensic evidence",
                        "forensic science",
                        "forensic expert",
                        "forensic analysis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к использованию научных методов для расследования преступлений или установления фактов в суде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судебный",
                                "target_lang_sense_clarification": "общий термин, напр. судебная экспертиза"
                            },
                            {
                                "target_lang_word": "криминалистический",
                                "target_lang_sense_clarification": "относящийся к расследованию преступлений"
                            },
                            {
                                "target_lang_word": "судебно-медицинский",
                                "target_lang_sense_clarification": "относящийся к медицине в рамках закона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32c37d0c-d78c-3820-a425-be673d940378",
                    "sense_definition": "Extremely detailed, precise, and thorough, often used to describe an examination or analysis.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "meticulous",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accountant conducted a <w>forensic</w> examination of the company's financial records.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер провел <w>тщательную</w> проверку финансовых отчетов компании."
                            }
                        },
                        {
                            "text": "He described the events of that night with <w>forensic</w> precision.",
                            "target_lang_translations": {
                                "ru": "Он описал события той ночи со <w>скрупулёзной</w> точностью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "forensic examination",
                        "forensic detail",
                        "forensic precision"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезвычайно подробный, точный и тщательный (часто об анализе или проверке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тщательный",
                                "target_lang_sense_clarification": "очень подробный"
                            },
                            {
                                "target_lang_word": "скрупулёзный",
                                "target_lang_sense_clarification": "предельно точный и дотошный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "361c371a-99c9-3346-aacf-209905f24a3f",
                    "sense_definition": "Relating to courts of law or suitable for legal argumentation.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "science_law",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer displayed great <w>forensic</w> skill during the cross-examination.",
                            "target_lang_translations": {
                                "ru": "Адвокат продемонстрировал великолепное <w>судебное</w> мастерство во время перекрестного допроса."
                            }
                        },
                        {
                            "text": "His <w>forensic</w> eloquence was enough to persuade the jury of his client's innocence.",
                            "target_lang_translations": {
                                "ru": "Его <w>судебного</w> красноречия было достаточно, чтобы убедить присяжных в невиновности клиента."
                            }
                        }
                    ],
                    "synonyms": [
                        "appropriate for a court of law"
                    ],
                    "common_phrases": [
                        "forensic eloquence",
                        "forensic skill"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к судам или подходящий для юридической аргументации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судебный",
                                "target_lang_sense_clarification": "связанный с судопроизводством"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "246ad1c8-96fc-3f4a-aa1a-5f6883db80d2",
                    "sense_definition": "Relating to the art of formal debate or public argumentation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "debate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She joined the university's <w>forensic</w> team to improve her public speaking abilities.",
                            "target_lang_translations": {
                                "ru": "Она присоединилась к университетской <w>дискуссионной</w> команде, чтобы улучшить свои навыки публичных выступлений."
                            }
                        },
                        {
                            "text": "The politician used <w>forensic</w> techniques to dismantle his opponent's argument.",
                            "target_lang_translations": {
                                "ru": "Политик использовал <w>риторические</w> приемы, чтобы разбить аргументы оппонента."
                            }

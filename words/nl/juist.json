                        "ru": [
                            {
                                "target_lang_word": "только что",
                                "target_lang_sense_clarification": "о времени: момент назад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "542d7816-930d-3ad9-ab77-f72dae35424a",
                    "sense_definition": "Om een tegenstelling aan te geven of iets te benadrukken dat anders is dan verwacht.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "contrast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben niet boos op je, ik ben <w>juist</w> heel trots.",
                            "target_lang_translations": {
                                "en": "I am not angry with you; I am <w>actually</w> very proud.",
                                "ru": "Я не сержусь на тебя, я <w>наоборот</w> очень горжусь."
                            }
                        },
                        {
                            "text": "Katten houden niet van water, terwijl honden er <w>juist</w> gek op zijn.",
                            "target_lang_translations": {
                                "en": "Cats do not like water, whereas dogs <w>actually</w> love it.",
                                "ru": "Кошки не любят воду, тогда как собаки <w>наоборот</w> от неё без ума."
                            }
                        }
                    ],
                    "synonyms": [
                        "daarentegen"
                    ],
                    "common_phrases": [
                        "juist niet",
                        "juist andersom"
                    ],
                    "target_lang_definitions": {
                        "en": "To indicate a contrast or emphasize something different than expected.",
                        "ru": "Наоборот, напротив; для выражения противопоставления или подчеркивания отличия от ожидаемого."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "actually",
                                "target_lang_sense_clarification": "as a matter of fact; used to contrast"
                            },
                            {
                                "target_lang_word": "in fact",
                                "target_lang_sense_clarification": "in reality"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "наоборот",
                                "target_lang_sense_clarification": "противопоставление"
                            },
                            {
                                "target_lang_word": "напротив",
                                "target_lang_sense_clarification": "вводное слово для контраста"
                            }
                        ]
                    }
                }

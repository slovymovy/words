{
    "word_family": [
        "косвенный",
        "косвенность"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e3524f55-1a35-3a46-a0b2-8a0c4db992c2",
                    "sense_definition": "Не прямо, не непосредственно; через промежуточные звенья, действия или обстоятельства.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "indirect_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это событие лишь <w>косвенно</w> повлияло на принятие окончательного решения.",
                            "target_lang_translations": {
                                "en": "This event only <w>indirectly</w> influenced the final decision."
                            }
                        },
                        {
                            "text": "Автор книги <w>косвенно</w> намекает на реальные исторические факты.",
                            "target_lang_translations": {
                                "en": "The book's author <w>indirectly</w> hints at real historical facts."
                            }
                        }
                    ],
                    "synonyms": [
                        "опосредованно"
                    ],
                    "antonyms": [
                        "напрямую",
                        "непосредственно",
                        "прямо"
                    ],
                    "common_phrases": [
                        "косвенно влиять",
                        "косвенно подтверждать",
                        "косвенно связан",
                        "косвенно указывать"
                    ],
                    "target_lang_definitions": {
                        "en": "Not directly; through intermediate steps, actions, or circumstances."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "indirectly",
                                "target_lang_sense_clarification": "not in a direct manner"
                            },
                            {
                                "target_lang_word": "implicitly",
                                "target_lang_sense_clarification": "suggested but not directly expressed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2f0cf53-7324-3aa6-826c-e626e560a316",
                    "sense_definition": "В наклонном направлении, под углом; наискось.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солнечные лучи падали <w>косвенно</w>, создавая длинные тени на стене.",
                            "target_lang_translations": {
                                "en": "The sun's rays fell <w>obliquely</w>, creating long shadows on the wall."
                            }
                        },
                        {
                            "text": "Дым от костра поднимался <w>косвенно</w>, уносимый легким ветром.",
                            "target_lang_translations": {
                                "en": "Smoke from the bonfire rose <w>slantwise</w>, carried away by a light wind."
                            }
                        }
                    ],
                    "antonyms": [
                        "прямо"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "устаревшее значение"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "In a slanting direction or at an angle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "obliquely",
                                "target_lang_sense_clarification": "at a slant or angle"
                            },
                            {
                                "target_lang_word": "slantwise",
                                "target_lang_sense_clarification": "in a slanting direction"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
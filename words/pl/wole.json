    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "20657d81-c955-35fa-bdf1-4d878831f501",
                    "sense_definition": "Forma fleksyjna rzeczownika <w>wół</w> (miejscownik lub wołacz liczby pojedynczej).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz często opowiadał o swoim ulubionym <w>wole</w>.",
                            "target_lang_translations": {
                                "ru": "Хозяин часто рассказывал о своем любимом <w>воле</w>."
                            }
                        },
                        {
                            "text": "W tej bajce jest mowa o pracowitym <w>wole</w> i leniwym koniu.",
                            "target_lang_translations": {
                                "ru": "В этой сказке говорится о трудолюбивом <w>воле</w> и ленивом коне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "o wole"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "miejscownik / wołacz lp"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Падежная форма существительного «wół» (вол) — предложный или звательный падеж единственного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вол",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8254c4c-99e4-3a9a-bda5-50baadcc6648",
                    "sense_definition": "Chorobowe powiększenie tarczycy, widoczne jako zgrubienie na przedniej części szyi.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "pathology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz stwierdził u pacjenta <w>wole</w> spowodowane niedoborem jodu.",
                            "target_lang_translations": {
                                "ru": "Врач обнаружил у пациента <w>зоб</w>, вызванный дефицитом йода."
                            }
                        },
                        {
                            "text": "Duże <w>wole</w> może utrudniać oddychanie i przełykanie.",
                            "target_lang_translations": {
                                "ru": "Большой <w>зоб</w> может затруднять дыхание и глотание."
                            }
                        }
                    ],
                    "synonyms": [
                        "wole tarczycy"
                    ],
                    "common_phrases": [
                        "wole tarczycy",
                        "wole guzowate"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "medycyna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Патологическое увеличение щитовидной железы, видимое как утолщение на передней части шеи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зоб",
                                "target_lang_sense_clarification": "медицина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50e90fa1-e84a-336a-b2c2-b93722c9f61e",
                    "sense_definition": "Rozszerzona część przełyku u ptaków, służąca do gromadzenia i wstępnego zmiękczania pokarmu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gołąb wypełnił swoje <w>wole</w> ziarnem zebranym na rynku.",
                            "target_lang_translations": {
                                "ru": "Голубь наполнил свой <w>зоб</w> зерном, собранным на рынке."
                            }
                        },
                        {
                            "text": "Niektóre ptaki karmią młode wydzieliną produkowaną przez <w>wole</w>.",
                            "target_lang_translations": {
                                "ru": "Некоторые птицы кормят птенцов секретом, вырабатываемым <w>зобом</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "zoologia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Расширенная часть пищевода у птиц, служащая для накопления и предварительного размягчения пищи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зоб",
                                "target_lang_sense_clarification": "анатомия птиц"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea0e97fb-27f0-3734-b6a2-6b28e856a023",
                    "sense_definition": "Forma fleksyjna rzeczownika <w>wola</w> (mianownik, biernik lub wołacz liczby mnogiej).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dwie sprzeczne <w>wole</w> walczyły o dominację w grupie.",
                            "target_lang_translations": {
                                "ru": "Две противоречивые <w>воли</w> боролись за доминирование в группе."
                            }
                        },
                        {
                            "text": "Nasze <w>wole</w> muszą się zjednoczyć, abyśmy osiągnęli sukces.",
                            "target_lang_translations": {
                                "ru": "Наши <w>воли</w> должны объединиться, чтобы мы достигли успеха."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "liczba mnoga"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Падежная форма существительного «wola» (воля) — именительный, винительный или звательный падеж множественного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воля",
                                "target_lang_sense_clarification": "желание, решимость"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "766bf637-b91c-3105-92eb-834aea0ca8dd",
                    "sense_definition": "Przymiotnik dzierżawczy oznaczający przynależność do wołu; występuje głównie w stałych zwrotach.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dachu zamontowano małe <w>wole</w> oko, które doświetla strych.",
                            "target_lang_translations": {
                                "ru": "В крыше установили маленькое окно типа «<w>бычий</w> глаз», которое освещает чердак."
                            }
                        },
                        {
                            "text": "Patrzył na nas przez wielkie, <w>wole</w> oczy.",
                            "target_lang_translations": {
                                "ru": "Он смотрел на нас большими <w>воловьими</w> глазами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wole oko"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "lub specjalistyczne (architektura)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Притяжательное прилагательное, означающее принадлежность волу; встречается в основном в устойчивых выражениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воловий",
                                "target_lang_sense_clarification": "прилагательное"
                            },
                            {
                                "target_lang_word": "бычий",
                                "target_lang_sense_clarification": "в архитектурном термине"
                            }
                        ]
                    }
                }
            ]
        }
    ]

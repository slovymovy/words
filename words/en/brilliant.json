                    "target_lang_definitions": {
                        "ru": "Обладающий резким, чистым тоном (о звуке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хрустальный",
                                "target_lang_sense_clarification": "о чистом, звонком звуке"
                            },
                            {
                                "target_lang_word": "блестящий",
                                "target_lang_sense_clarification": "виртуозный (о звуке или исполнении)"
                            }
                        ]
                    }
                }
            ]
        },
        {

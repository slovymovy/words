{
    "word_family": [
        "good",
        "well",
        "better"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "60765ed6-5155-3e77-86e6-36edf24b9363",
                    "sense_definition": "The superlative form of <w>good</w>: having the highest quality, excellence, or standing.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "adj_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is my <w>best</w> friend in the whole world.",
                            "target_lang_translations": {
                                "ru": "Она моя <w>лучшая</w> подруга во всем мире."
                            }
                        },
                        {
                            "text": "This is the <w>best</w> meal I have ever eaten.",
                            "target_lang_translations": {
                                "ru": "Это <w>лучшая</w> еда, которую я когда-либо ел."
                            }
                        }
                    ],
                    "synonyms": [
                        "optimal"
                    ],
                    "antonyms": [
                        "worst"
                    ],
                    "common_phrases": [
                        "best friend",
                        "all the best",
                        "best practice",
                        "best wishes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Превосходная степень от \"хороший\": обладающий высшим качеством, превосходством или положением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лу́чший",
                                "target_lang_sense_clarification": "самый хороший"
                            },
                            {
                                "target_lang_word": "наилу́чший",
                                "target_lang_sense_clarification": "оптимальный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e39d7a76-d7f2-3880-b87e-06f0336bcc3d",
                    "sense_definition": "The largest or most significant part of something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adj_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It took the <w>best</w> part of the week to finish the project.",
                            "target_lang_translations": {
                                "ru": "На завершение проекта ушла <w>бо́льшая</w> часть недели."
                            }
                        },
                        {
                            "text": "He spent the <w>best</w> part of his life traveling abroad.",
                            "target_lang_translations": {
                                "ru": "Он провел <w>бо́льшую</w> часть своей жизни, путешествуя за границей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "best part of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самая большая или значительная часть чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бо́льшая",
                                "target_lang_sense_clarification": "значительная часть"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c6262feb-506e-389c-b609-dac417c31bff",
                    "sense_definition": "The superlative form of <w>well</w>: in the most excellent, effective, or suitable manner.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "adv_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He knows <w>best</w> how to fix the engine.",
                            "target_lang_translations": {
                                "ru": "Он <w>лучше всего</w> знает, как починить двигатель."
                            }
                        },
                        {
                            "text": "I like this color <w>best</w> out of all the options.",
                            "target_lang_translations": {
                                "ru": "Мне этот цвет нравится <w>больше всего</w> из всех вариантов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "know best",
                        "do best",
                        "like best"
                    ],
                    "target_lang_definitions": {
                        "ru": "Превосходная степень от \"хорошо\": самым отличным, эффективным или подходящим образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лу́чше всего",
                                "target_lang_sense_clarification": "наилучшим образом"
                            },
                            {
                                "target_lang_word": "бо́льше всего",
                                "target_lang_sense_clarification": "о предпочтении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b45ca758-8074-3a95-b280-84ef68247fd8",
                    "sense_definition": "Used to indicate that it is advisable, wise, or most advantageous to do something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adv_advice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You had <w>best</w> leave before the storm starts.",
                            "target_lang_translations": {
                                "ru": "Тебе <w>лучше</w> уйти, пока не началась буря."
                            }
                        },
                        {
                            "text": "We'd <w>best</w> call ahead to make a reservation.",
                            "target_lang_translations": {
                                "ru": "Нам <w>лучше</w> позвонить заранее, чтобы забронировать столик."
                            }
                        }
                    ],
                    "common_phrases": [
                        "had best"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на то, что сделать что-то целесообразно, разумно или наиболее выгодно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лу́чше",
                                "target_lang_sense_clarification": "стоит, следует"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6874aaf4-0482-35cc-b50d-ac3084cba232",
                    "sense_definition": "The supreme effort or maximum capability one can produce.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_effort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will do my <w>best</w> to help you with your homework.",
                            "target_lang_translations": {
                                "ru": "Я сделаю <w>всё возможное</w>, чтобы помочь тебе с домашним заданием."
                            }
                        },
                        {
                            "text": "She achieved a personal <w>best</w> in the marathon yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера она установила личный <w>рекорд</w> в марафоне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "do one's best",
                        "try one's best",
                        "personal best"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высшее усилие или максимальная способность, которую кто-либо может проявить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всё возмо́жное",
                                "target_lang_sense_clarification": "максимальные усилия"
                            },
                            {
                                "target_lang_word": "реко́рд",
                                "target_lang_sense_clarification": "личное достижение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd293f29-b86c-31f4-8313-f03d1c8cdb4d",
                    "sense_definition": "The person or thing that is the most excellent or outstanding.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Only the <w>best</w> is good enough for this project.",
                            "target_lang_translations": {
                                "ru": "Только <w>лучшее</w> достаточно хорошо для этого проекта."
                            }
                        },
                        {
                            "text": "Even the <w>best</w> of us make mistakes sometimes.",
                            "target_lang_translations": {
                                "ru": "Даже <w>лучшие</w> из нас иногда совершают ошибки."
                            }
                        }
                    ],
                    "synonyms": [
                        "cream of the crop",
                        "gold standard"
                    ],
                    "common_phrases": [
                        "the best of",
                        "simply the best"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или вещь, являющиеся самыми превосходными или выдающимися."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лу́чшее",
                                "target_lang_sense_clarification": "существительное"
                            },
                            {
                                "target_lang_word": "лу́чший",
                                "target_lang_sense_clarification": "о человеке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9d68132f-b196-36be-a1d3-15a1c85a3478",
                    "sense_definition": "To surpass someone in skill or achievement; to defeat.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_defeat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>bested</w> his opponent in the final round of the tournament.",
                            "target_lang_translations": {
                                "ru": "Он <w>победил</w> своего соперника в финальном раунде турнира."
                            }
                        },
                        {
                            "text": "The champion was finally <w>bested</w> by a young newcomer.",
                            "target_lang_translations": {
                                "ru": "Чемпиона наконец <w>превзошел</w> молодой новичок."
                            }
                        }
                    ],
                    "antonyms": [
                        "worst"
                    ],
                    "target_lang_definitions": {
                        "ru": "Превзойти кого-либо в мастерстве или достижениях; победить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "победи́ть",
                                "target_lang_sense_clarification": "одержать верх"
                            },
                            {
                                "target_lang_word": "превзойти́",
                                "target_lang_sense_clarification": "быть лучше кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c34fc565-5568-3e56-86d3-92b8ac459487",
                    "sense_definition": "To be advisable or necessary (a colloquial shortening of 'had best').",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_advice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You <w>best</w> get going if you want to catch the bus.",
                            "target_lang_translations": {
                                "ru": "Тебе <w>лучше</w> идти, если хочешь успеть на автобус."
                            }
                        },
                        {
                            "text": "We <w>best</w> be careful not to wake the baby.",
                            "target_lang_translations": {
                                "ru": "Нам <w>лучше</w> быть осторожными, чтобы не разбудить ребенка."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Shortened form of 'had best'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть целесообразным или необходимым (разговорное сокращение от 'had best')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лу́чше",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {

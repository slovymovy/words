    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8bf096fd-3436-3954-9da0-1c9797d805f8",
                    "sense_definition": "To give food to a person or an animal.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_eating_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please remember to <w>feed</w> the dog before you go to bed.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не забудь <w>покормить</w> собаку перед сном."
                            }
                        },
                        {
                            "text": "The mother <w>feeds</w> her baby every three to four hours.",
                            "target_lang_translations": {
                                "ru": "Мать <w>кормит</w> своего ребенка каждые три-четыре часа."
                            }
                        }
                    ],
                    "synonyms": [
                        "nourish"
                    ],
                    "antonyms": [
                        "starve"
                    ],
                    "common_phrases": [
                        "feed the dog",
                        "feed the baby",
                        "feed a family"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать еду человеку или животному."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кормить",
                                "target_lang_sense_clarification": "давать пищу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3d77d93-18b6-39a3-93b2-744a8c8a19a4",
                    "sense_definition": "To eat food; primarily used to describe animals eating.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "food_eating_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cows <w>feed</w> on grass in the open fields during the summer.",
                            "target_lang_translations": {
                                "ru": "Коровы <w>питаются</w> травой на открытых полях летом."
                            }
                        },
                        {
                            "text": "Many birds <w>feed</w> on seeds and small insects found in the garden.",
                            "target_lang_translations": {
                                "ru": "Многие птицы <w>питаются</w> семенами и мелкими насекомыми, которых находят в саду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "feed on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Есть, питаться; обычно используется для описания животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "питаться",
                                "target_lang_sense_clarification": "чем-либо (травой, семенами)"
                            },
                            {
                                "target_lang_word": "есть",
                                "target_lang_sense_clarification": "принимать пищу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb960695-9308-3511-b3d3-6c144b62d5a1",
                    "sense_definition": "To supply a machine, system, or process with necessary materials, power, or data.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "supply_flow_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>feed</w> the paper gently into the printer to avoid a jam.",
                            "target_lang_translations": {
                                "ru": "Нужно аккуратно <w>подавать</w> бумагу в принтер, чтобы избежать замятия."
                            }
                        },
                        {
                            "text": "The small mountain streams <w>feed</w> the river with fresh water.",
                            "target_lang_translations": {
                                "ru": "Маленькие горные ручьи <w>питают</w> реку свежей водой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "feed data",
                        "feed into",
                        "feed the meter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабжать машину, систему или процесс необходимыми материалами, энергией или данными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подавать",
                                "target_lang_sense_clarification": "материал в машину"
                            },
                            {
                                "target_lang_word": "питать",
                                "target_lang_sense_clarification": "снабжать энергией или ресурсами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3298abcc-0242-30e6-ab3e-fadc500a1d0f",
                    "sense_definition": "To satisfy, encourage, or intensify a strong desire, feeling, or habit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He bought the expensive car just to <w>feed</w> his own vanity.",
                            "target_lang_translations": {
                                "ru": "Он купил эту дорогую машину только чтобы <w>потешить</w> свое тщеславие."
                            }
                        },
                        {
                            "text": "Ignoring the problem will only <w>feed</w> the rumors spreading around the office.",
                            "target_lang_translations": {
                                "ru": "Игнорирование проблемы будет лишь <w>подпитывать</w> слухи, распространяющиеся по офису."
                            }
                        }
                    ],
                    "common_phrases": [
                        "feed an addiction",
                        "feed the ego"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удовлетворять, поощрять или усиливать сильное желание, чувство или привычку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подпитывать",
                                "target_lang_sense_clarification": "усиливать (слухи, чувства)"
                            },
                            {
                                "target_lang_word": "тешить",
                                "target_lang_sense_clarification": "удовлетворять (самолюбие)"
                            },
                            {
                                "target_lang_word": "питать",
                                "target_lang_sense_clarification": "поддерживать чувство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c6f6a8a9-101f-3c38-b87b-77cd7809e8e9",
                    "sense_definition": "A continuously updated stream of digital content, such as news, social media posts, or video.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "supply_flow_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I checked my social media <w>feed</w> to see the latest photos from my friends.",
                            "target_lang_translations": {
                                "ru": "Я проверил свою <w>ленту</w> в социальных сетях, чтобы посмотреть последние фотографии друзей."
                            }
                        },
                        {
                            "text": "The news <w>feed</w> on the website updates automatically every minute.",
                            "target_lang_translations": {
                                "ru": "<w>Лента новостей</w> на сайте обновляется автоматически каждую минуту."
                            }
                        }
                    ],
                    "common_phrases": [
                        "news feed",
                        "live feed",
                        "social media feed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянно обновляемый поток цифрового контента, например новостей или постов в социальных сетях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лента",
                                "target_lang_sense_clarification": "в соцсетях"
                            },
                            {
                                "target_lang_word": "лента новостей",
                                "target_lang_sense_clarification": "новостная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "baccb0b8-25fb-38d2-b849-bec3b6a03416",
                    "sense_definition": "Food provided for animals, especially livestock.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "food_eating_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer went to the store to buy a large sack of chicken <w>feed</w>.",
                            "target_lang_translations": {
                                "ru": "Фермер поехал в магазин, чтобы купить большой мешок куриного <w>корма</w>."
                            }
                        },
                        {
                            "text": "We need to make sure there is enough winter <w>feed</w> for the cattle.",
                            "target_lang_translations": {
                                "ru": "Нам нужно убедиться, что для скота достаточно зимнего <w>корма</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "animal feed",
                        "cattle feed",
                        "chicken feed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Еда, предназначенная для животных, особенно для скота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корм",
                                "target_lang_sense_clarification": "для животных"
                            },
                            {
                                "target_lang_word": "фураж",
                                "target_lang_sense_clarification": "для скота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce95b6ba-95b9-328e-aaba-30140eeb412d",
                    "sense_definition": "The mechanism or action of supplying material to a machine.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "supply_flow_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The paper <w>feed</w> on this old printer gets stuck very often.",
                            "target_lang_translations": {
                                "ru": "Механизм <w>подачи</w> бумаги на этом старом принтере очень часто заедает."
                            }
                        },
                        {
                            "text": "Engineers checked the fuel <w>feed</w> to ensure the engine was running smoothly.",
                            "target_lang_translations": {
                                "ru": "Инженеры проверили <w>подачу</w> топлива, чтобы убедиться, что двигатель работает плавно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "paper feed",
                        "gravity feed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Механизм или действие по подаче материала в машину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подача",
                                "target_lang_sense_clarification": "механизм или действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbad3506-0104-3ce2-b6a2-f75f0375da40",
                    "sense_definition": "A large meal or a gathering to eat.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "food_eating_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went over to my grandmother's house for a big Sunday <w>feed</w>.",
                            "target_lang_translations": {
                                "ru": "Мы пошли к бабушке на большое воскресное <w>застолье</w>."
                            }
                        },
                        {
                            "text": "The community organized a crab <w>feed</w> to raise money for the school.",
                            "target_lang_translations": {
                                "ru": "Община организовала <w>угощение</w> крабами, чтобы собрать деньги для школы."
                            }
                        }
                    ],
                    "synonyms": [
                        "feast"
                    ],
                    "common_phrases": [
                        "have a feed",
                        "good feed"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe a substantial meal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обильная еда или застолье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застолье",
                                "target_lang_sense_clarification": "праздничный обед"
                            },
                            {
                                "target_lang_word": "пирушка",
                                "target_lang_sense_clarification": "дружеское застолье"
                            },
                            {
                                "target_lang_word": "угощение",
                                "target_lang_sense_clarification": "еда на мероприятии"
                            }
                        ]
                    }
                }
            ]
        }
    ]

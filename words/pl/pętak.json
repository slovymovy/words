    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c4840c07-ce8c-3dff-ab09-7dbfba31bd54",
                    "sense_definition": "Lekceważące określenie dziecka, zwłaszcza dorastającego chłopca, którego uważa się za niedojrzałego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "child_insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten mały <w>pętak</w> znowu wybił szybę piłką i uciekł.",
                            "target_lang_translations": {
                                "ru": "Этот маленький <w>сопляк</w> снова разбил окно мячом и убежал."
                            }
                        },
                        {
                            "text": "Nie przejmuj się tym, co mówi jakiś <w>pętak</w> z sąsiedztwa.",
                            "target_lang_translations": {
                                "ru": "Не обращай внимания на то, что говорит какой-то соседский <w>шкет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "smarkacz",
                        "chłystek",
                        "gówniarz",
                        "bachor",
                        "smark"
                    ],
                    "common_phrases": [
                        "głupi pętak",
                        "mały pętak"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo o wydźwięku lekceważącym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пренебрежительное название ребенка, особенно мальчика-подростка, которого считают незрелым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопляк",
                                "target_lang_sense_clarification": "о незрелом юноше"
                            },
                            {
                                "target_lang_word": "шкет",
                                "target_lang_sense_clarification": "о мальчишке"
                            },
                            {
                                "target_lang_word": "малявка",
                                "target_lang_sense_clarification": "о маленьком ребенке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0db95b4b-411f-35b0-83b9-7a37fef56f48",
                    "sense_definition": "Pogardliwe określenie dorosłego mężczyzny, którego traktuje się jako osobę niepoważną, bez znaczenia lub godną politowania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "man_insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie będę słuchał rozkazów tego <w>pętaka</w>, który na niczym się nie zna.",
                            "target_lang_translations": {
                                "ru": "Я не буду слушать приказы этого <w>ничтожества</w>, который ни в чем не разбирается."
                            }
                        },
                        {
                            "text": "Zachowujesz się jak zwykły <w>pętak</w>, a nie jak odpowiedzialny kierownik.",
                            "target_lang_translations": {
                                "ru": "Ты ведешь себя как обычный <w>сопляк</w>, а не как ответственный руководитель."
                            }
                        }
                    ],
                    "synonyms": [
                        "chłystek",
                        "pętaczyna"
                    ],
                    "common_phrases": [
                        "zwykły pętak"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo o wydźwięku pogardliwym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Презрительное название взрослого мужчины, которого считают несерьезным, незначительным или жалким."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ничтожество",
                                "target_lang_sense_clarification": "о незначительном человеке"
                            },
                            {
                                "target_lang_word": "сопляк",
                                "target_lang_sense_clarification": "о незрелом мужчине"
                            },
                            {
                                "target_lang_word": "хлыщ",
                                "target_lang_sense_clarification": "о пустом человеке"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
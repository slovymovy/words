    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "216585e8-cf4d-3792-b078-822157c3859f",
                    "sense_definition": "Betrekking hebbend op de inhoud, de betekenis of de zaak zelf, in tegenstelling tot de vorm.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>inhoudelijke</w> behandeling van de rechtszaak begint volgende week.",
                            "target_lang_translations": {
                                "en": "The <w>substantive</w> handling of the lawsuit begins next week.",
                                "ru": "Рассмотрение судебного дела <w>по существу</w> начнется на следующей неделе."
                            }
                        },
                        {
                            "text": "Hij had geen <w>inhoudelijk</w> weerwoord op de kritische vragen van de journalist.",
                            "target_lang_translations": {
                                "en": "He had no <w>substantive</w> rebuttal to the journalist's critical questions.",
                                "ru": "У него не было <w>содержательного</w> возражения на критические вопросы журналиста."
                            }
                        }
                    ],
                    "synonyms": [
                        "wezenlijk",
                        "materieel"
                    ],
                    "antonyms": [
                        "formeel"
                    ],
                    "common_phrases": [
                        "inhoudelijke argumenten",
                        "inhoudelijke kritiek",
                        "inhoudelijke discussie"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the content, meaning, or the matter itself, as opposed to the form.",
                        "ru": "Относящийся к содержанию, смыслу или сути дела, в отличие от формы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "substantive",
                                "target_lang_sense_clarification": "relating to the essence or substance of a matter"
                            },
                            {
                                "target_lang_word": "content-related",
                                "target_lang_sense_clarification": "pertaining to the content rather than the form"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "содержательный",
                                "target_lang_sense_clarification": "относящийся к сути, наполненный смыслом"
                            },
                            {
                                "target_lang_word": "по существу",
                                "target_lang_sense_clarification": "касающийся сути дела (часто в официальном или юридическом контексте)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a667d702-e4e6-36ea-956f-2e0667b01a72",
                    "sense_definition": "Wat de inhoud betreft; op het gebied van de inhoud.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het rapport is <w>inhoudelijk</w> in orde, maar de opmaak kan nog verbeterd worden.",
                            "target_lang_translations": {
                                "en": "The report is fine <w>in terms of content</w>, but the layout can still be improved.",
                                "ru": "Отчет <w>по содержанию</w> в порядке, но оформление еще можно улучшить."
                            }
                        },
                        {
                            "text": "We zijn het <w>inhoudelijk</w> met elkaar eens, maar verschillen van mening over de aanpak.",
                            "target_lang_translations": {
                                "en": "We agree with each other <w>substantively</w>, but differ in opinion regarding the approach.",
                                "ru": "Мы <w>по сути</w> согласны друг с другом, но расходимся во мнениях относительно подхода."
                            }
                        }
                    ],
                    "antonyms": [
                        "formeel"
                    ],
                    "common_phrases": [

{
    "word_family": [
        "manifestacja",
        "manifestant",
        "manifest",
        "zamanifestować",
        "manifestacyjny"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cca8715f-7b51-3eba-b721-d0efa2c58217",
                    "sense_definition": "Brać udział w publicznym zgromadzeniu, aby wyrazić poparcie dla jakiejś idei lub sprzeciw wobec czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "public_protest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tysiące niezadowolonych obywateli wyszło na ulice, aby <w>manifestować</w> przeciwko nowym podatkom.",
                            "target_lang_translations": {
                                "ru": "Тысячи недовольных граждан вышли на улицы, чтобы <w>манифестировать</w> против новых налогов."
                            }
                        },
                        {
                            "text": "Związkowcy zapowiedzieli, że będą <w>manifestować</w> pod budynkiem parlamentu do skutku.",
                            "target_lang_translations": {
                                "ru": "Члены профсоюза заявили, что будут <w>митинговать</w> у здания парламента, пока не добьются своего."
                            }
                        }
                    ],
                    "synonyms": [
                        "demonstrować"
                    ],
                    "common_phrases": [
                        "manifestować na ulicach",
                        "manifestować poparcie",
                        "manifestować sprzeciw",
                        "tłumnie manifestować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать участие в публичном собрании, чтобы выразить поддержку какой-либо идее или протест против чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "манифестировать",
                                "target_lang_sense_clarification": "участвовать в манифестации"
                            },
                            {
                                "target_lang_word": "демонстрировать",
                                "target_lang_sense_clarification": "участвовать в демонстрации"
                            },
                            {
                                "target_lang_word": "митинговать",
                                "target_lang_sense_clarification": "проводить митинг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c29354d-a573-3bf0-99fb-3bd9742908d6",
                    "sense_definition": "Wyraźnie okazywać swoje uczucia, poglądy lub cechy; dawać czemuś wyraz poprzez zachowanie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "expression_of_feelings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna starał się <w>manifestować</w> swoją niezależność poprzez ekscentryczny ubiór.",
                            "target_lang_translations": {
                                "ru": "Мужчина старался <w>демонстрировать</w> свою независимость с помощью эксцентричной одежды."
                            }
                        },
                        {
                            "text": "Nie powinieneś tak otwarcie <w>manifestować</w> swojej niechęci do nowych współpracowników.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует так открыто <w>проявлять</w> свою неприязнь к новым коллегам."
                            }
                        }
                    ],
                    "synonyms": [
                        "okazywać",
                        "demonstrować"
                    ],
                    "common_phrases": [
                        "manifestować niezadowolenie",
                        "manifestować poglądy",
                        "manifestować obecność",
                        "manifestować uczucia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Явно показывать свои чувства, взгляды или качества; выражать что-либо своим поведением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демонстрировать",
                                "target_lang_sense_clarification": "открыто показывать"
                            },
                            {
                                "target_lang_word": "проявлять",
                                "target_lang_sense_clarification": "выражать чувства или качества"
                            },
                            {
                                "target_lang_word": "выказывать",
                                "target_lang_sense_clarification": "обнаруживать отношение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c49fbf53-1f28-3d22-9a9f-4c335648c659",
                    "sense_definition": "Stawać się widocznym, ujawniać swoją obecność lub działanie (zwykle w połączeniu z zaimkiem „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "appearance_revelation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta rzadka choroba może <w>manifestować się</w> nagłymi bólami głowy i osłabieniem.",
                            "target_lang_translations": {
                                "ru": "Это редкое заболевание может <w>проявляться</w> внезапными головными болями и слабостью."
                            }
                        },
                        {
                            "text": "Kryzys gospodarczy zaczął <w>manifestować się</w> spadkiem produkcji i wzrostem bezrobocia.",
                            "target_lang_translations": {
                                "ru": "Экономический кризис начал <w>проявляться</w> спадом производства и ростом безработицы."
                            }
                        }
                    ],
                    "synonyms": [
                        "objawiać się",
                        "ujawniać się",
                        "uzewnętrzniać się"
                    ],
                    "common_phrases": [
                        "choroba się manifestuje",
                        "problem się manifestuje",
                        "objawy manifestują się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться видимым, обнаруживать свое присутствие или действие (обычно о симптомах или явлениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проявляться",
                                "target_lang_sense_clarification": "становиться заметным (о признаках)"
                            },
                            {
                                "target_lang_word": "обнаруживаться",
                                "target_lang_sense_clarification": "выявляться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
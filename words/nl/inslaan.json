    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a29f514d-4ad2-3018-8e05-9c05d0d95db5",
                    "sense_definition": "Grote hoeveelheden van iets kopen om een voorraad aan te leggen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "buy_stock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij moeten nog veel eten en drinken <w>inslaan</w> voor het feest van morgen.",
                            "target_lang_translations": {
                                "en": "We still need to <w>stock up on</w> a lot of food and drinks for tomorrow's party.",
                                "ru": "Нам нужно <w>закупить</w> еще много еды и напитков для завтрашней вечеринки."
                            }
                        },
                        {
                            "text": "Mijn ouders hebben extra hout <w>ingeslagen</w> voor de koude wintermaanden.",
                            "target_lang_translations": {
                                "en": "My parents have <w>stocked up on</w> extra wood for the cold winter months.",
                                "ru": "Мои родители <w>запаслись</w> дровами на холодные зимние месяцы."
                            }
                        }
                    ],
                    "synonyms": [
                        "inkopen",
                        "hamsteren",
                        "aanschaffen"
                    ],
                    "antonyms": [
                        "verkopen",
                        "opmaken"
                    ],
                    "common_phrases": [
                        "voorraad inslaan",
                        "groot inslaan",
                        "boodschappen inslaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To buy large quantities of something to create a stock.",
                        "ru": "Покупать что-либо в больших количествах, чтобы создать запас."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stock up on",
                                "target_lang_sense_clarification": "to accumulate supplies"
                            },
                            {
                                "target_lang_word": "buy in",
                                "target_lang_sense_clarification": "to purchase in bulk"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "закупать",
                                "target_lang_sense_clarification": "приобретать в большом количестве"
                            },
                            {
                                "target_lang_word": "запасаться",
                                "target_lang_sense_clarification": "делать запасы чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4f93c43-1518-3306-ac82-aa509be8ced1",
                    "sense_definition": "Een zijweg nemen of een bepaalde richting kiezen om te volgen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het stoplicht moet je direct de eerste straat rechts <w>inslaan</w>.",
                            "target_lang_translations": {
                                "en": "After the traffic light, you must immediately <w>turn into</w> the first street on the right.",
                                "ru": "После светофора нужно сразу <w>свернуть</w> на первую улицу направо."
                            }
                        },
                        {
                            "text": "Het bedrijf wil met deze nieuwe strategie een andere weg <w>inslaan</w>.",
                            "target_lang_translations": {
                                "en": "With this new strategy, the company wants to <w>take</w> a different path.",
                                "ru": "С этой новой стратегией компания хочет <w>выбрать</w> другой путь."
                            }
                        }
                    ],
                    "synonyms": [
                        "afslaan"
                    ],
                    "common_phrases": [
                        "een weg inslaan",
                        "een richting inslaan",
                        "rechtsaf inslaan",
                        "linksaf inslaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To take a side road or choose a specific direction to follow.",
                        "ru": "Свернуть на боковую улицу или выбрать определенное направление движения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn into",
                                "target_lang_sense_clarification": "entering a street"
                            },
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "choosing a path or direction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "свернуть",
                                "target_lang_sense_clarification": "повернуть на другую улицу"
                            },
                            {
                                "target_lang_word": "выбрать",
                                "target_lang_sense_clarification": "направление или путь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c51ba97e-6ec4-3e49-90a0-0a817a80bb44",
                    "sense_definition": "Met grote kracht ergens op neerkomen of botsen, vaak gebruikt voor bliksem of projectielen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de zware storm zag ik de bliksem in een boom <w>inslaan</w>.",
                            "target_lang_translations": {
                                "en": "During the heavy storm, I saw lightning <w>strike</w> a tree.",
                                "ru": "Во время сильной грозы я видел, как молния <w>ударила</w> в дерево."
                            }
                        },
                        {
                            "text": "Het nieuws over zijn plotselinge vertrek <w>sloeg</w> <w>in</w> als een bom.",
                            "target_lang_translations": {
                                "en": "The news of his sudden departure <w>hit</w> like a bombshell.",
                                "ru": "Новость о его внезапном отъезде <w>разорвалась</w> словно бомба."
                            }
                        }
                    ],
                    "synonyms": [
                        "treffen",
                        "neerkomen"
                    ],
                    "antonyms": [
                        "missen"
                    ],
                    "common_phrases": [
                        "inslaan als een bom",
                        "de bliksem slaat in"
                    ],
                    "target_lang_definitions": {
                        "en": "To come down or collide with great force, often used for lightning or projectiles.",
                        "ru": "С силой ударять или попадать во что-то (часто о молнии или снарядах)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strike",
                                "target_lang_sense_clarification": "lightning or impact"
                            },
                            {
                                "target_lang_word": "hit",
                                "target_lang_sense_clarification": "impact like a bomb"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ударить",
                                "target_lang_sense_clarification": "о молнии или сильном ударе"
                            },
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "о снаряде или бомбе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a962af88-e51f-3873-8f7e-96a532c920f1",
                    "sense_definition": "Iets kapotmaken door er hard tegenaan te slaan, zodat het naar binnen breekt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "break",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De brandweer moest de voordeur <w>inslaan</w> om de bewoners te redden.",
                            "target_lang_translations": {
                                "en": "The fire department had to <w>smash in</w> the front door to save the residents.",
                                "ru": "Пожарным пришлось <w>выбить</w> входную дверь, чтобы спасти жильцов."
                            }
                        },
                        {
                            "text": "Vandalen hebben gisteravond de ruiten van de bushokjes <w>ingeslagen</w>.",
                            "target_lang_translations": {
                                "en": "Vandals <w>smashed in</w> the windows of the bus shelters last night.",
                                "ru": "Вандалы вчера вечером <w>разбили</w> стекла на автобусных остановках."
                            }
                        }
                    ],
                    "synonyms": [
                        "insmijten",
                        "verbrijzelen"
                    ],
                    "common_phrases": [
                        "een ruit inslaan",
                        "de deur inslaan",
                        "iemands hersens inslaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To destroy something by hitting it hard so that it breaks inwards.",
                        "ru": "Разбить что-либо сильным ударом снаружи внутрь."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smash in",
                                "target_lang_sense_clarification": "windows or doors"
                            },
                            {
                                "target_lang_word": "bash in",
                                "target_lang_sense_clarification": "violent breaking"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выбить",
                                "target_lang_sense_clarification": "дверь или окно"
                            },
                            {
                                "target_lang_word": "разбить",
                                "target_lang_sense_clarification": "стекло или витрину"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
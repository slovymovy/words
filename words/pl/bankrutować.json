{
    "word_family": [
        "bankrut",
        "bankructwo"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "06c72460-42a0-39f9-9f3d-26f5a3e74b14",
                    "sense_definition": "Stawać się niewypłacalnym, tracić majątek i nie być w stanie spłacić swoich zobowiązań finansowych.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "financial_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele małych przedsiębiorstw zaczęło <w>bankrutować</w> z powodu nagłego kryzysu gospodarczego.",
                            "target_lang_translations": {
                                "ru": "Многие малые предприятия начали <w>банкротиться</w> из-за внезапного экономического кризиса."
                            }
                        },
                        {
                            "text": "Jeśli firma będzie dalej tak zarządzana, wkrótce zacznie <w>bankrutować</w>.",
                            "target_lang_translations": {
                                "ru": "Если компанией и дальше будут так управлять, она вскоре начнет <w>банкротиться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "plajtować",
                        "upadać"
                    ],
                    "antonyms": [
                        "prosperować",
                        "bogacić się"
                    ],
                    "common_phrases": [
                        "firma bankrutuje",
                        "zaczynać bankrutować",
                        "bankrutować przez długi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться неплатежеспособным, терять имущество и не иметь возможности погасить свои финансовые обязательства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "банкротиться",
                                "target_lang_sense_clarification": "становиться несостоятельным должником"
                            },
                            {
                                "target_lang_word": "разоряться",
                                "target_lang_sense_clarification": "терять всё своё состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "067e158f-7e4a-378a-be56-4ddba4f1def1",
                    "sense_definition": "Tracić na wartości, znaczeniu lub autorytecie (zwykle w odniesieniu do idei, planów lub grup społecznych).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stara ideologia zaczyna <w>bankrutować</w> w oczach młodego pokolenia wyborców.",
                            "target_lang_translations": {
                                "ru": "Старая идеология начинает <w>терпеть крах</w> в глазах молодого поколения избирателей."
                            }
                        },
                        {
                            "text": "Ten system wartości zdaje się powoli <w>bankrutować</w> we współczesnym świecie.",
                            "target_lang_translations": {
                                "ru": "Эта система ценностей, кажется, медленно <w>обесценивается</w> в современном мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "upadać"
                    ],
                    "antonyms": [
                        "rozkwitać"
                    ],
                    "common_phrases": [
                        "moralnie bankrutować",
                        "politycznie bankrutować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Терять ценность, значение или авторитет (обычно в отношении идей, планов или социальных групп)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терпеть крах",
                                "target_lang_sense_clarification": "об идеях, планах или системах"
                            },
                            {
                                "target_lang_word": "обесцениваться",
                                "target_lang_sense_clarification": "терять значимость или авторитет"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
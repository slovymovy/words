    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e3059511-112f-3abc-94bb-e9e0ee136199",
                    "sense_definition": "Создавать что-либо в мыслях или воображении, сочинять, находить способ сделать что-то.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "creative_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Писатель каждый день <w>придумывает</w> новые приключения для своих героев.",
                            "target_lang_translations": {
                                "en": "The writer <w>invents</w> new adventures for his characters every day."
                            }
                        },
                        {
                            "text": "Нам нужно срочно <w>придумывать</w> план действий на завтра.",
                            "target_lang_translations": {
                                "en": "We urgently need to <w>come up with</w> a plan of action for tomorrow."
                            }
                        },
                        {
                            "text": "Мама всё время <w>придумывала</w> новые развлечения, чтобы детям не было скучно.",
                            "target_lang_translations": {
                                "en": "Mom was always <w>thinking up</w> new entertainment so the children wouldn't be bored."
                            }
                        }
                    ],
                    "synonyms": [
                        "изобретать",
                        "сочинять",
                        "создавать",
                        "разрабатывать"
                    ],
                    "antonyms": [
                        "копировать",
                        "повторять",
                        "заимствовать"
                    ],
                    "common_phrases": [
                        "придумывать историю",
                        "придумывать план",
                        "придумывать название",
                        "придумывать оправдания"
                    ],
                    "target_lang_definitions": {
                        "en": "To create something in one's thoughts or imagination, to compose, or to find a way to do something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come up with",
                                "target_lang_sense_clarification": "an idea, plan, or solution"
                            },
                            {
                                "target_lang_word": "invent",
                                "target_lang_sense_clarification": "a story, character, or device"
                            },
                            {
                                "target_lang_word": "think up",
                                "target_lang_sense_clarification": "something in one's mind"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa1367ec-e9eb-3041-b651-08a9baa84725",
                    "sense_definition": "Говорить неправду, сообщать о том, чего на самом деле не было.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "deception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не верь ему, он всё это <w>придумывает</w>, чтобы привлечь внимание.",
                            "target_lang_translations": {
                                "en": "Don't believe him, he is <w>making up</w> all this to attract attention."
                            }
                        },
                        {
                            "text": "Хватит <w>придумывать</w>, скажи мне правду.",
                            "target_lang_translations": {
                                "en": "Stop <w>making things up</w>, tell me the truth."
                            }
                        }
                    ],
                    "synonyms": [
                        "выдумывать",
                        "лгать",
                        "сочинять"
                    ],
                    "antonyms": [
                        "говорить правду"
                    ],
                    "common_phrases": [
                        "хватит придумывать",
                        "придумывать небылицы",
                        "много придумывать"
                    ],
                    "target_lang_definitions": {
                        "en": "To tell a lie or report something that did not actually happen."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make up",
                                "target_lang_sense_clarification": "a lie or story"
                            },
                            {
                                "target_lang_word": "fabricate",
                                "target_lang_sense_clarification": "information or details"
                            }
                        ]
                    }
                }
            ]
        }
    ]

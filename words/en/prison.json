    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2e5b5e68-2191-3b3e-adfd-df2967e5bf7d",
                    "sense_definition": "A place or institution where people are held against their will, especially for long-term confinement as punishment for serious crimes.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "confinement_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cold stone walls of the <w>prison</w> had stood for over a century.",
                            "target_lang_translations": {
                                "ru": "Холодные каменные стены <w>тюрьмы</w> стояли уже более века."
                            }
                        },
                        {
                            "text": "He visited his brother in <w>prison</w> every weekend to bring him supplies.",
                            "target_lang_translations": {
                                "ru": "Он навещал брата в <w>тюрьме</w> каждые выходные, чтобы передать ему вещи."
                            }
                        }
                    ],
                    "synonyms": [
                        "big house",
                        "bridewell"
                    ],
                    "common_phrases": [
                        "go to prison",
                        "state prison",
                        "federal prison",
                        "prison guard",
                        "prison cell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или учреждение, где людей содержат против их воли, особенно в качестве наказания за серьезные преступления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тюрьма",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "место заключения",
                                "target_lang_sense_clarification": "официально"
                            },
                            {
                                "target_lang_word": "зона",
                                "target_lang_sense_clarification": "разговорное, жаргон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20320e49-bf5a-3807-bb94-da687a8c02f1",
                    "sense_definition": "The state or condition of being confined in a prison.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "confinement_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Prison</w> was a harrowing experience for him that changed his outlook on life.",
                            "target_lang_translations": {
                                "ru": "<w>Тюрьма</w> стала для него мучительным опытом, изменившим его взгляд на жизнь."
                            }
                        },
                        {
                            "text": "The judge sentenced the thief to three years in <w>prison</w> for his crimes.",
                            "target_lang_translations": {
                                "ru": "Судья приговорил вора к трем годам <w>тюрьмы</w> за его преступления."
                            }
                        }
                    ],
                    "synonyms": [
                        "imprisonment"
                    ],
                    "common_phrases": [
                        "life in prison",
                        "release from prison",
                        "sent to prison"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние или условие нахождения в заключении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тюремное заключение",
                                "target_lang_sense_clarification": "процесс/состояние"
                            },
                            {
                                "target_lang_word": "лишение свободы",
                                "target_lang_sense_clarification": "юридический термин"
                            },
                            {
                                "target_lang_word": "тюрьма",
                                "target_lang_sense_clarification": "разговорное обозначение срока"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1abb490f-3af5-3cf5-ac3d-0ae9577bde19",
                    "sense_definition": "Any restrictive environment or situation that feels like confinement, such as a harsh school or a difficult job.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_confinement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The academy was a <w>prison</w> for many of its students because of its strict teachers.",
                            "target_lang_translations": {
                                "ru": "Академия была <w>тюрьмой</w> для многих студентов из-за строгих преподавателей."
                            }
                        },
                        {
                            "text": "Without any creative freedom, this office job feels like a <w>prison</w>.",
                            "target_lang_translations": {
                                "ru": "Без какой-либо творческой свободы эта офисная работа кажется <w>тюрьмой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "feel like a prison"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любая ограничивающая среда или ситуация, которая ощущается как несвобода, например, суровая школа или тяжелая работа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тюрьма",
                                "target_lang_sense_clarification": "в переносном смысле"
                            },
                            {
                                "target_lang_word": "кабала",
                                "target_lang_sense_clarification": "полная зависимость"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eaaf8db8-41e3-33b4-a5a3-fb6e068e6ebb",
                    "sense_definition": "Hair hanging over the forehead, usually cut straight across.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "hair_style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to cut a <w>fringe</w> to change her hairstyle.",
                            "target_lang_translations": {
                                "ru": "Она решила выстричь <w>чёлку</w>, чтобы изменить прическу."
                            }
                        },
                        {
                            "text": "Her long <w>fringe</w> was getting in her eyes, so she pinned it back.",
                            "target_lang_translations": {
                                "ru": "Её длинная <w>чёлка</w> лезла в глаза, поэтому она заколола её назад."
                            }
                        }
                    ],
                    "synonyms": [
                        "bangs"
                    ],
                    "common_phrases": [
                        "cut a fringe",
                        "long fringe",
                        "side fringe"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British/Commonwealth English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Волосы, свисающие на лоб, обычно подстриженные прямо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёлка",
                                "target_lang_sense_clarification": "прическа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "560dd055-7030-33f3-8a10-f6d9239593a7",
                    "sense_definition": "A decorative border of hanging threads, cords, or strips, often attached to the edge of clothing, rugs, or curtains.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "decoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old lamp had a shade with a gold <w>fringe</w> around the bottom.",
                            "target_lang_translations": {
                                "ru": "У старой лампы был абажур с золотой <w>бахромой</w> по нижнему краю."
                            }
                        },
                        {
                            "text": "She wore a leather jacket with a long <w>fringe</w> on the sleeves.",
                            "target_lang_translations": {
                                "ru": "На ней была кожаная куртка с длинной <w>бахромой</w> на рукавах."
                            }
                        }
                    ],
                    "synonyms": [
                        "edge",
                        "edge"
                    ],
                    "common_phrases": [
                        "silk fringe",
                        "gold fringe",
                        "leather fringe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративный край из свисающих нитей, шнуров или полосок, часто прикрепляемый к краю одежды, ковров или штор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бахрома",
                                "target_lang_sense_clarification": "украшение из нитей"
                            },
                            {
                                "target_lang_word": "кайма",
                                "target_lang_sense_clarification": "край, окантовка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdb13ac7-60cf-37d7-8c31-b38dd9202ea4",
                    "sense_definition": "The outer edge or periphery of an area, city, or group.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "periphery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They built a new shopping centre on the <w>fringe</w> of the town.",
                            "target_lang_translations": {
                                "ru": "Они построили новый торговый центр на <w>окраине</w> города."
                            }
                        },
                        {
                            "text": "He lives on the <w>fringe</w> of the forest, far from the busy city centre.",
                            "target_lang_translations": {
                                "ru": "Он живёт на <w>краю</w> леса, далеко от шумного центра города."
                            }
                        }
                    ],
                    "synonyms": [
                        "outskirts",
                        "periphery",
                        "edge"
                    ],
                    "antonyms": [
                        "center"
                    ],
                    "common_phrases": [
                        "on the fringe",
                        "urban fringe"
                    ],
                    "target_lang_definitions": {

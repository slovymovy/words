                    "frequency": "Low",
                    "semantic_group_id": "immoral",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They want to right a <w>wrong</w> from the past.",
                            "target_lang_translations": {
                                "ru": "Они хотят исправить <w>несправедливость</w> прошлого."
                            }
                        },
                        {
                            "text": "She forgave him for the <w>wrongs</w> he committed against her family.",
                            "target_lang_translations": {
                                "ru": "Она простила его за <w>обиды</w>, которые он причинил её семье."
                            }
                        }
                    ],
                    "synonyms": [
                        "grievance",
                        "injustice"
                    ],
                    "common_phrases": [
                        "right a wrong",
                        "past wrongs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несправедливое или вредное действие, совершенное против кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несправедливость",
                                "target_lang_sense_clarification": "отсутствие справедливости"
                            },
                            {
                                "target_lang_word": "обида",
                                "target_lang_sense_clarification": "причиненное огорчение"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a8cd344c-2d89-356c-811e-fa80cb9f3770",
                    "sense_definition": "De mogelijkheid om te zien of de afstand die men kan kijken; het blikveld.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vision_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vanaf het balkon hebben we vrij <w>zicht</w> op het haventerrein.",
                            "target_lang_translations": {
                                "en": "From the balcony we have a clear <w>view</w> of the harbour area.",
                                "ru": "С балкона у нас открывается свободный <w>вид</w> на территорию порта."
                            }
                        },
                        {
                            "text": "Door de dichte mist was het <w>zicht</w> op de snelweg erg slecht.",
                            "target_lang_translations": {
                                "en": "Due to the dense fog, the <w>visibility</w> on the highway was very poor.",
                                "ru": "Из-за густого тумана <w>видимость</w> на шоссе была очень плохой."
                            }
                        },
                        {
                            "text": "De auto verdween al snel uit het <w>zicht</w> toen hij de hoek omging.",
                            "target_lang_translations": {
                                "en": "The car quickly disappeared from <w>sight</w> when it went around the corner.",
                                "ru": "Машина быстро скрылась из <w>виду</w>, когда завернула за угол."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitzicht"
                    ],
                    "common_phrases": [
                        "in het zicht",
                        "uit het zicht",
                        "op het eerste zicht"
                    ],
                    "target_lang_definitions": {
                        "en": "The possibility to see or the distance one can look; the field of view.",
                        "ru": "Возможность видеть или расстояние, на которое можно смотреть; поле зрения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "view",
                                "target_lang_sense_clarification": "what can be seen from a particular place"
                            },
                            {
                                "target_lang_word": "visibility",
                                "target_lang_sense_clarification": "the distance one can see, often related to weather"
                            },
                            {
                                "target_lang_word": "sight",
                                "target_lang_sense_clarification": "the area within which one can see; field of vision"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "то, что открывается взору"
                            },
                            {
                                "target_lang_word": "видимость",
                                "target_lang_sense_clarification": "состояние атмосферы, дальность обзора"
                            },
                            {
                                "target_lang_word": "обзор",
                                "target_lang_sense_clarification": "возможность видеть пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1033214a-cca8-3f7f-a829-d8fd576b7aba",
                    "sense_definition": "Het vermogen om te zien met de ogen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vision_ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hopelijk kon ik nog wel wat zien, anders was ik behalve mijn spraak ook nog mijn <w>zicht</w> kwijt.",
                            "target_lang_translations": {
                                "en": "Hopefully I could still see something, otherwise I would have lost my <w>sight</w> as well as my speech.",
                                "ru": "Надеюсь, я всё ещё мог что-то видеть, иначе я потерял бы не только речь, но и <w>зрение</w>."
                            }
                        },
                        {
                            "text": "Zonder bril is mijn <w>zicht</w> niet scherp genoeg om te lezen.",
                            "target_lang_translations": {
                                "en": "Without glasses my <w>vision</w> is not sharp enough to read.",
                                "ru": "Без очков моё <w>зрение</w> недостаточно острое для чтения."
                            }
                        }
                    ],
                    "synonyms": [
                        "zeissel"
                    ],
                    "common_phrases": [
                        "het zicht verliezen"
                    ],
                    "target_lang_definitions": {
                        "en": "The ability to see with the eyes.",
                        "ru": "Способность видеть глазами."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sight",
                                "target_lang_sense_clarification": "the faculty or power of seeing"
                            },
                            {
                                "target_lang_word": "vision",
                                "target_lang_sense_clarification": "the ability to see; eyesight"
                            },
                            {
                                "target_lang_word": "eyesight",
                                "target_lang_sense_clarification": "physical ability to see"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "зрение",
                                "target_lang_sense_clarification": "физическая способность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d96cd91-8fe7-38da-b3d1-600904ca7a2a",
                    "sense_definition": "Het begrijpen van een situatie of onderwerp; de mate waarin men iets overziet.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vision_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze zijn de vaardigheid nog aan het leren, en hebben nog minder <w>zicht</w> op de gevaren van het verkeer.",
                            "target_lang_translations": {
                                "en": "They are still learning the skill, and have even less <w>insight</w> into the dangers of traffic.",
                                "ru": "Они всё ещё осваивают этот навык и пока имеют слабое <w>представление</w> об опасностях дорожного движения."
                            }
                        },
                        {
                            "text": "De manager heeft geen goed <w>zicht</w> op de werkelijke kosten van het project.",
                            "target_lang_translations": {
                                "en": "The manager does not have a good <w>grasp</w> of the actual costs of the project.",
                                "ru": "У менеджера нет чёткого <w>понимания</w> реальных затрат на проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitzicht"
                    ],
                    "common_phrases": [
                        "zicht hebben op",
                        "zicht krijgen op"
                    ],
                    "target_lang_definitions": {
                        "en": "Understanding a situation or subject; the extent to which one oversees something.",
                        "ru": "Понимание ситуации или предмета; степень осведомленности о чем-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "insight",
                                "target_lang_sense_clarification": "a clear understanding of a complex situation"
                            },
                            {
                                "target_lang_word": "grasp",
                                "target_lang_sense_clarification": "comprehension or understanding of a subject"
                            },
                            {
                                "target_lang_word": "overview",
                                "target_lang_sense_clarification": "a general review or summary of a subject"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "представление",
                                "target_lang_sense_clarification": "понимание ситуации"
                            },
                            {
                                "target_lang_word": "понимание",
                                "target_lang_sense_clarification": "осознание сути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39597476-3b67-3ed0-837b-19d97fb6fda3",
                    "sense_definition": "Een landbouwwerktuig dat lijkt op een kleine zeis, gebruikt om graan of gras te maaien.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gras maait men met de zeis, haver met een <w>zicht</w>.",
                            "target_lang_translations": {
                                "en": "Grass is mowed with a scythe, oats with a <w>sickle</w>.",
                                "ru": "Траву косят косой, а овёс — <w>серпом</w>."
                            }
                        },
                        {
                            "text": "De boer sleep zijn <w>zicht</w> voordat hij begon met de oogst.",
                            "target_lang_translations": {
                                "en": "The farmer sharpened his <w>sickle</w> before he started the harvest.",
                                "ru": "Фермер наточил свой <w>серп</w> перед началом сбора урожая."
                            }
                        }
                    ],
                    "synonyms": [
                        "sikkel",
                        "pik",
                        "zeissel"
                    ],
                    "target_lang_definitions": {
                        "en": "An agricultural tool resembling a small scythe, used to mow grain or grass.",
                        "ru": "Сельскохозяйственный инструмент, похожий на маленькую косу, используемый для скашивания зерна или травы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sickle",
                                "target_lang_sense_clarification": "a short-handled farming tool with a semicircular blade"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "серп",
                                "target_lang_sense_clarification": "инструмент для жатвы"
                            },
                            {
                                "target_lang_word": "коса",
                                "target_lang_sense_clarification": "инструмент для кошения"
                            }
                        ]
                    }
                }
            ]
        },
        {

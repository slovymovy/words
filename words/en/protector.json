{
    "word_family": [
        "protect",
        "protection",
        "protective",
        "protectively",
        "protectiveness",
        "protectorship"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "24544674-a4b0-385d-843e-9988500e6c28",
                    "sense_definition": "Someone who protects or guards people or things from danger or harm.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The loyal dog acted as a <w>protector</w> of the family home.",
                            "target_lang_translations": {
                                "ru": "Верный пес выступал в роли <w>защитника</w> семейного очага."
                            }
                        },
                        {
                            "text": "She saw herself as a fierce <w>protector</w> of animal rights.",
                            "target_lang_translations": {
                                "ru": "Она считала себя ярой <w>защитницей</w> прав животных."
                            }
                        }
                    ],
                    "synonyms": [
                        "guard",
                        "sentry"
                    ],
                    "antonyms": [
                        "attacker",
                        "assailant"
                    ],
                    "common_phrases": [
                        "fierce protector",
                        "loyal protector",
                        "family protector"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто защищает или охраняет людей или вещи от опасности или вреда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защитник",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "защитница",
                                "target_lang_sense_clarification": "женский род"
                            },
                            {
                                "target_lang_word": "охранитель",
                                "target_lang_sense_clarification": "книжное/устаревшее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd466f02-1066-3492-a9d7-862962fe1e44",
                    "sense_definition": "A device or mechanism designed to prevent damage or injury to something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to buy a new screen <w>protector</w> because my phone keeps getting scratched.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую <w>защиту</w> для экрана, потому что мой телефон постоянно царапается."
                            }
                        },
                        {
                            "text": "The hockey player wore a chest <w>protector</w> to prevent injury during the game.",
                            "target_lang_translations": {
                                "ru": "Хоккеист надел нагрудный <w>протектор</w>, чтобы предотвратить травму во время игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "guard",
                        "sentry"
                    ],
                    "common_phrases": [
                        "screen protector",
                        "surge protector",
                        "chest protector",
                        "pocket protector"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство или приспособление, предназначенное для предотвращения повреждений или травм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защита",
                                "target_lang_sense_clarification": "общее название защитных средств"
                            },
                            {
                                "target_lang_word": "протектор",
                                "target_lang_sense_clarification": "техническое или спортивное снаряжение"
                            },
                            {
                                "target_lang_word": "защитное приспособление",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6ef50a8-9476-3b54-88e5-06df03ce09a1",
                    "sense_definition": "A person who rules a kingdom during the absence, minority, or incapacity of the monarch; a regent.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The duke was appointed as <w>protector</w> of the realm until the young king came of age.",
                            "target_lang_translations": {
                                "ru": "Герцог был назначен <w>протектором</w> королевства до совершеннолетия юного короля."
                            }
                        },
                        {
                            "text": "Oliver Cromwell is a famous historical figure who held the title of Lord <w>Protector</w>.",
                            "target_lang_translations": {
                                "ru": "Оливер Кромвель — известная историческая личность, носившая титул Лорда-<w>протектора</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Lord Protector"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical title"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, управляющее королевством во время отсутствия, несовершеннолетия или недееспособности монарха; регент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протектор",
                                "target_lang_sense_clarification": "исторический титул"
                            },
                            {
                                "target_lang_word": "регент",
                                "target_lang_sense_clarification": "временный правитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d847e5cc-3438-3431-bae2-56d98cf63ab4",
                    "sense_definition": "A state or subject under international law that exercises control and protection over another state.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The empire acted as a <w>protector</w> for the smaller territories along its border.",
                            "target_lang_translations": {
                                "ru": "Империя выступала в качестве <w>протектора</w> для малых территорий вдоль своей границы."
                            }
                        },
                        {
                            "text": "Under the new treaty, the powerful nation became the official <w>protector</w> of the island's interests.",
                            "target_lang_translations": {
                                "ru": "Согласно новому договору, могущественная держава стала официальным <w>протектором</w> интересов острова."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Государство или субъект международного права, осуществляющий контроль и защиту другого государства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протектор",
                                "target_lang_sense_clarification": "в международных отношениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07913cf0-1a2a-3219-a17f-0c945cb6b9b3",
                    "sense_definition": "A cardinal appointed to look after the interests of a specific religious order, nation, or institution in Rome.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was appointed as the cardinal <w>protector</w> of the Franciscan order.",
                            "target_lang_translations": {
                                "ru": "Он был назначен кардиналом-<w>протектором</w> ордена францисканцев."
                            }
                        },
                        {
                            "text": "The role of the <w>protector</w> was to represent the college within the church hierarchy.",
                            "target_lang_translations": {
                                "ru": "Роль <w>протектора</w> заключалась в представлении интересов коллегии в церковной иерархии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cardinal protector"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кардинал, назначенный заботиться об интересах определенного религиозного ордена, нации или учреждения в Риме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протектор",
                                "target_lang_sense_clarification": "церковный титул"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
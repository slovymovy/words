                {
                    "sense_id": "10504c2f-47e1-385f-b688-1babb4053888",
                    "sense_definition": "Niet eerder dan een bepaald moment; of slechts een beperkte hoeveelheid.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_restriction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De winkel gaat <w>pas</w> om tien uur open.",

            "senses": [
                {
                    "sense_id": "51d284c4-41c9-3aa2-9640-77388d7dc1c0",
                    "sense_definition": "Długa myśliwska broń palna, zazwyczaj ładowana śrutem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myśliwy zarzucił ciężką <w>strzelbę</w> na ramię i ruszył w głąb lasu.",
                            "target_lang_translations": {
                                "ru": "Охотник закинул тяжелое <w>ружьё</w> на плечо и двинулся вглубь леса."
                            }
                        },
                        {
                            "text": "Nad kominkiem w salonie wisiała stara, zabytkowa <w>strzelba</w> dziadka.",
                            "target_lang_translations": {
                                "ru": "Над камином в гостиной висело старое, антикварное дедушкино <w>ружьё</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "fuzja"
                    ],
                    "common_phrases": [
                        "nabita strzelba",
                        "strzelba myśliwska",
                        "lufa strzelby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинноствольное охотничье огнестрельное оружие, обычно заряжаемое дробью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ружьё",
                                "target_lang_sense_clarification": "охотничье"
                            },
                            {
                                "target_lang_word": "дробовик",
                                "target_lang_sense_clarification": "гладкоствольное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "217f7b8a-1f8c-3ccd-bdcd-8f77ece56c37",
                    "sense_definition": "Ogólne, często potoczne określenie na dowolną broń palną.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierz dokładnie wyczyścił swoją <w>strzelbę</w> przed wieczorną wartą.",
                            "target_lang_translations": {
                                "ru": "Солдат тщательно вычистил свое <w>ружьё</w> перед вечерним караулом."
                            }
                        },
                        {
                            "text": "Huknęła <w>strzelba</w> i stado ptaków zerwało się gwałtownie do lotu.",
                            "target_lang_translations": {
                                "ru": "Грохнуло <w>ружьё</w>, и стая птиц резко взмыла в воздух."
                            }
                        }
                    ],
                    "synonyms": [
                        "broń palna"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used loosely for rifles or muskets"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Общее, часто разговорное название любого огнестрельного оружия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ружьё",
                                "target_lang_sense_clarification": "общее название"
                            },
                            {
                                "target_lang_word": "винтовка",
                                "target_lang_sense_clarification": "нарезное оружие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96931346-1f47-3086-8b9d-c84cdf544a02",
                    "sense_definition": "Dawne określenie na polowanie lub wyprawę myśliwską.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szlachta z całej okolicy zjechała się na wielką <w>strzelbę</w> w puszczy.",
                            "target_lang_translations": {
                                "ru": "Дворянство со всей округи съехалось на большую <w>охоту</w> в пущу."
                            }
                        },
                        {
                            "text": "Cały tydzień spędzili na <w>strzelbie</w>, ale nie udało im się nic upolować.",
                            "target_lang_translations": {
                                "ru": "Всю неделю они провели на <w>охоте</w>, но им ничего не удалось подстрелить."
                            }
                        }
                    ],
                    "synonyms": [
                        "polowanie",
                        "łów"
                    ],
                    "common_phrases": [
                        "iść na strzelbę"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название охоты или охотничьей экспедиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охота",
                                "target_lang_sense_clarification": "устаревшее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90732645-ebbd-3ed5-8d07-140cf48ce238",
                    "sense_definition": "Gwałtowna wymiana ognia lub odgłosy wystrzałów.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z oddali słychać było głośną <w>strzelbę</w> dochodzącą z pola bitwy.",
                            "target_lang_translations": {
                                "ru": "Издали была слышна громкая <w>стрельба</w>, доносившаяся с поля битвы."
                            }
                        },
                        {
                            "text": "Nagle wybuchła chaotyczna <w>strzelba</w> między walczącymi oddziałami.",
                            "target_lang_translations": {
                                "ru": "Внезапно вспыхнула хаотичная <w>перестрелка</w> между сражающимися отрядами."
                            }
                        }
                    ],
                    "synonyms": [
                        "strzelanina"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "literary or historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Интенсивная перестрелка или звуки выстрелов."
                    },

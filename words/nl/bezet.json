            "senses": [
                {
                    "sense_id": "3d1f98c4-6ceb-34ab-8590-5610523e8293",
                    "sense_definition": "Niet beschikbaar omdat het door iemand anders of iets anders in gebruik is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sorry, u kunt hier niet zitten want deze stoel is al <w>bezet</w>.",
                            "target_lang_translations": {
                                "en": "Sorry, you cannot sit here because this seat is already <w>taken</w>.",
                                "ru": "Извините, вы не можете здесь сесть, так как этот стул уже <w>занят</w>."
                            }
                        },
                        {
                            "text": "Ik probeerde mijn moeder te bellen, maar de lijn was steeds <w>bezet</w>.",
                            "target_lang_translations": {
                                "en": "I tried to call my mother, but the line was <w>busy</w> all the time.",
                                "ru": "Я пытался дозвониться до мамы, но линия была всё время <w>занята</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "in gesprek"
                    ],
                    "antonyms": [
                        "vrij",
                        "onbezet"
                    ],
                    "common_phrases": [
                        "de lijn is bezet",
                        "deze plaats is bezet",
                        "het toilet is bezet"
                    ],
                    "target_lang_definitions": {
                        "en": "Not available because it is being used by someone or something else.",
                        "ru": "Недоступный, так как используется кем-то другим или чем-то иным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "occupied",
                                "target_lang_sense_clarification": "general use, toilets, or spaces"
                            },
                            {
                                "target_lang_word": "taken",
                                "target_lang_sense_clarification": "for seats or places"
                            },
                            {
                                "target_lang_word": "busy",
                                "target_lang_sense_clarification": "for telephone lines"
                            }

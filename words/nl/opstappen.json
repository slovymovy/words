    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f97ca06c-acc1-3e0e-816a-afe6a1d3f019",
                    "sense_definition": "Op een fiets gaan zitten of een voertuig betreden om te vertrekken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <stapte> met een zwaai <op> zijn nieuwe fiets.",
                            "target_lang_translations": {
                                "en": "He <w>got on</w> his new bike with a swing.",
                                "ru": "Он с размаху <сел> на свой новый велосипед."
                            }
                        },
                        {
                            "text": "De passagiers mogen nu <opstappen> voor de trein naar Amsterdam.",
                            "target_lang_translations": {
                                "en": "The passengers may now <w>board</w> the train to Amsterdam.",
                                "ru": "Пассажиры теперь могут <садиться> в поезд до Амстердама."
                            }
                        }
                    ],
                    "synonyms": [
                        "instappen",
                        "bestijgen"
                    ],
                    "antonyms": [
                        "afstappen",
                        "uitstappen"
                    ],
                    "common_phrases": [
                        "op de fiets stappen"
                    ],
                    "target_lang_definitions": {
                        "en": "To sit on a bicycle or enter a vehicle to depart.",
                        "ru": "Садиться на велосипед или входить в транспортное средство, чтобы отправиться в путь."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get on",
                                "target_lang_sense_clarification": "bicycle or bus"
                            },
                            {
                                "target_lang_word": "board",
                                "target_lang_sense_clarification": "train, plane, or ship"
                            },
                            {
                                "target_lang_word": "mount",
                                "target_lang_sense_clarification": "bicycle or horse (formal)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "садиться",
                                "target_lang_sense_clarification": "на велосипед, в транспорт"
                            },
                            {
                                "target_lang_word": "совершать посадку",
                                "target_lang_sense_clarification": "о пассажирах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d49fdd83-c15c-3f3e-8fe4-1a7904b31484",
                    "sense_definition": "Weggaan of vertrekken van een locatie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_depart",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is al laat, dus ik <stap> maar eens <op>.",
                            "target_lang_translations": {
                                "en": "It is already late, so I'll be <w>heading off</w>.",
                                "ru": "Уже поздно, так что я, пожалуй, <пойду>."
                            }
                        },
                        {
                            "text": "Zullen we <opstappen> voordat het begint te regenen?",
                            "target_lang_translations": {
                                "en": "Shall we <w>leave</w> before it starts raining?",
                                "ru": "Может, <пойдем> до того, как начнется дождь?"
                            }
                        }
                    ],
                    "synonyms": [
                        "weggaan",
                        "vertrekken"
                    ],
                    "antonyms": [
                        "blijven"
                    ],
                    "common_phrases": [
                        "ik stap op"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To leave or depart from a location.",
                        "ru": "Уходить или покидать место пребывания."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "head off",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "be off",
                                "target_lang_sense_clarification": "informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "покидать место"
                            },
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "уходить, отправляться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "194458e1-3929-369b-804f-194065d0971c",
                    "sense_definition": "Een officiële functie of baan neerleggen, vaak onder druk of na een besluit.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "work_politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De minister moest <opstappen> na het grote schandaal.",
                            "target_lang_translations": {
                                "en": "The minister had to <w>resign</w> after the major scandal.",
                                "ru": "Министр был вынужден <уйти в отставку> после крупного скандала."
                            }
                        },
                        {
                            "text": "Hij heeft besloten vrijwillig <op> te <stappen> als voorzitter.",
                            "target_lang_translations": {
                                "en": "He has decided to voluntarily <w>step down</w> as chairman.",
                                "ru": "Он решил добровольно <покинуть пост> председателя."
                            }
                        }
                    ],
                    "synonyms": [
                        "aftreden",
                        "ontslag nemen",
                        "terugtreden"
                    ],
                    "antonyms": [
                        "aanblijven",
                        "aantreden"
                    ],
                    "common_phrases": [
                        "moeten opstappen",
                        "direct opstappen"
                    ],
                    "target_lang_definitions": {
                        "en": "To resign from an official position or job, often under pressure.",
                        "ru": "Оставлять официальную должность или увольняться, часто под давлением."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "resign",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "step down",
                                "target_lang_sense_clarification": "common phrasal verb"
                            },
                            {
                                "target_lang_word": "quit",
                                "target_lang_sense_clarification": "informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уходить в отставку",
                                "target_lang_sense_clarification": "с должности"
                            },
                            {
                                "target_lang_word": "покидать пост",
                                "target_lang_sense_clarification": "официально"
                            },
                            {
                                "target_lang_word": "увольняться",
                                "target_lang_sense_clarification": "с работы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "558d96ac-d953-36d1-840f-42cf7f467fd5",
                    "sense_definition": "De meervoudsvorm van opstap; verhogingen om ergens op te komen of beginpunten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_plural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er waren verschillende <opstappen> geplaatst om het podium bereikbaar te maken.",
                            "target_lang_translations": {
                                "en": "Several <w>steps</w> had been placed to make the stage accessible.",
                                "ru": "Были установлены различные <ступеньки>, чтобы обеспечить доступ к сцене."
                            }
                        },
                        {
                            "text": "Deze kleine successen zijn goede <opstappen> naar een grotere carrière.",
                            "target_lang_translations": {
                                "en": "These small successes are good <w>stepping stones</w> to a bigger career.",
                                "ru": "Эти маленькие успехи — хорошие <трамплины> к большой карьере."
                            }
                        }
                    ],
                    "synonyms": [
                        "afstapjes",
                        "treden"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of 'opstap'; elevations to step onto something or figurative starting points.",
                        "ru": "Ступени или возвышения для подъема куда-либо; также переносное значение (начальные этапы)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "steps",
                                "target_lang_sense_clarification": "physical object"
                            },
                            {
                                "target_lang_word": "stepping stones",
                                "target_lang_sense_clarification": "figurative"
                            },
                            {
                                "target_lang_word": "mounting blocks",
                                "target_lang_sense_clarification": "specific physical aid"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ступеньки",
                                "target_lang_sense_clarification": "для подъема"
                            },
                            {
                                "target_lang_word": "подножки",
                                "target_lang_sense_clarification": "у транспорта"
                            }
                        ]
                    }
                }
            ]
        }
    ]

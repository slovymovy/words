            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e4f0f06b-2679-3458-a980-266b1a429658",
                    "sense_definition": "В размере одной второй части; на пятьдесят процентов.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот стакан <наполовину> пуст, а тот полон.",
                            "target_lang_translations": {
                                "en": "This glass is <half> empty, and that one is full."
                            }
                        },
                        {
                            "text": "Нам пришлось сократить расходы компании <наполовину>.",
                            "target_lang_translations": {
                                "en": "We had to cut the company's expenses <by half>."
                            }
                        }
                    ],
                    "antonyms": [
                        "полностью",
                        "целиком"
                    ],
                    "common_phrases": [
                        "стакан наполовину пуст",
                        "разделить наполовину",
                        "сделать наполовину"
                    ],
                    "target_lang_definitions": {
                        "en": "In the amount of one half; by fifty percent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "half",
                                "target_lang_sense_clarification": "general meaning of 50%"
                            },
                            {
                                "target_lang_word": "in half",
                                "target_lang_sense_clarification": "when dividing or splitting something"
                            },
                            {
                                "target_lang_word": "by half",
                                "target_lang_sense_clarification": "when reducing an amount"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43a16ec2-dbba-3d03-992c-a3e24bb2f1db",
                    "sense_definition": "Не совсем, не полностью, лишь отчасти (о действиях, состояниях или качествах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он говорил <наполовину> в шутку, но я всё равно обиделся.",
                            "target_lang_translations": {
                                "en": "He was speaking <half> in jest, but I still took offense."
                            }
                        },
                        {
                            "text": "Я уже <наполовину> забыл, о чём мы спорили вчера.",
                            "target_lang_translations": {
                                "en": "I have already <half> forgotten what we were arguing about yesterday."
                            }
                        }
                    ],
                    "synonyms": [
                        "отчасти",
                        "не до конца",
                        "в некоторой степени",
                        "до некоторой степени"
                    ],
                    "antonyms": [
                        "полностью",
                        "совсем"
                    ],
                    "common_phrases": [
                        "наполовину правда",
                        "наполовину шутка"
                    ],
                    "target_lang_definitions": {
                        "en": "Not quite, not fully, only partially (regarding actions, states, or qualities)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "half",
                                "target_lang_sense_clarification": "used with adjectives or verbs (e.g., half-joking)"
                            },
                            {
                                "target_lang_word": "partly",
                                "target_lang_sense_clarification": "to some extent"
                            },
                            {
                                "target_lang_word": "partially",
                                "target_lang_sense_clarification": "incomplete degree"
                            }
                        ]
                    }
                }
            ]
        }

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "df9d466d-4a09-3a61-b423-4f90c445a3fa",
                    "sense_definition": "Not easily annoyed or angered; having a calm and patient temperament.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "temperament",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>unirritable</w> disposition makes him an excellent teacher for young children.",
                            "target_lang_translations": {
                                "ru": "Его <w>нераздражительный</w> характер делает его отличным учителем для маленьких детей."
                            }
                        },
                        {
                            "text": "She remained surprisingly <w>unirritable</w> despite the long delay at the airport.",
                            "target_lang_translations": {
                                "ru": "Она оставалась удивительно <w>невозмутимой</w> несмотря на долгую задержку в аэропорту."
                            }
                        }
                    ],
                    "synonyms": [
                        "calm",
                        "placid",
                        "good-natured",
                        "patient",
                        "easygoing"
                    ],
                    "antonyms": [
                        "irritable",
                        "short-tempered",
                        "touchy",
                        "cranky"
                    ],
                    "common_phrases": [
                        "unirritable disposition",
                        "unirritable nature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не склонный к раздражению или гневу; обладающий спокойным и терпеливым характером."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нераздражительный",
                                "target_lang_sense_clarification": "о характере или нраве"
                            },
                            {
                                "target_lang_word": "невозмутимый",
                                "target_lang_sense_clarification": "спокойный, уравновешенный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c607e70f-0837-3c0c-a33a-4a6ecca16d26",
                    "sense_definition": "Not capable of reacting or responding to a physical or biological stimulus.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biological_response",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nerve endings in the scar tissue are <w>unirritable</w> and feel numb.",
                            "target_lang_translations": {
                                "ru": "Нервные окончания в рубцовой ткани <w>невозбудимы</w> и потеряли чувствительность."
                            }
                        },
                        {
                            "text": "Experiments showed that the muscle became <w>unirritable</w> after prolonged exposure to the toxin.",
                            "target_lang_translations": {
                                "ru": "Эксперименты показали, что мышца стала <w>невозбудимой</w> после длительного воздействия токсина."
                            }
                        }
                    ],
                    "synonyms": [
                        "unresponsive",
                        "insensitive"
                    ],
                    "antonyms": [
                        "responsive",
                        "sensitive",
                        "irritable"
                    ],
                    "common_phrases": [
                        "unirritable tissue",
                        "unirritable nerve"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не способный реагировать на физические или биологические раздражители."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невозбудимый",
                                "target_lang_sense_clarification": "физиологический термин"
                            },
                            {
                                "target_lang_word": "нечувствительный",
                                "target_lang_sense_clarification": "не реагирующий на стимулы"
                            }
                        ]
                    }
                }
            ]
        }
    ]

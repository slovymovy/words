                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удовлетворять",
                                "target_lang_sense_clarification": "потребность или запрос"
                            },
                            {
                                "target_lang_word": "выполнять",
                                "target_lang_sense_clarification": "заказ или рецепт"
                            },
                            {
                                "target_lang_word": "восполнять",
                                "target_lang_sense_clarification": "нехватку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0567ab3b-36f1-3d01-85c0-5c6b152012e4",
                    "sense_definition": "To appoint a person to a vacant job or role.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "satisfy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>fill</w> this vacancy as soon as possible.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>заполнить</w> эту вакансию как можно скорее."
                            }
                        },
                        {
                            "text": "The manager position has already been <w>filled</w> by an internal candidate.",
                            "target_lang_translations": {
                                "ru": "Должность менеджера уже <w>занята</w> внутренним кандидатом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fill a vacancy",
                        "fill a position",
                        "fill a post"
                    ],
                    "target_lang_definitions": {
                        "ru": "Назначать человека на вакантную должность или роль."

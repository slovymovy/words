            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7cc2f385-34d7-34dd-b41c-c7ba9ca26a8c",
                    "sense_definition": "Заставлять перемещаться в определённом направлении (животных, людей или предметы).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пастух каждый день <гнал> стадо коров на зелёный луг."
                        },
                        {
                            "text": "Сильный ветер <гнал> по небу тяжёлые серые тучи."
                        }
                    ],
                    "antonyms": [
                        "останавливать"
                    ],
                    "common_phrases": [
                        "гнать стадо",
                        "гнать мяч",
                        "ветер гонит"
                    ]
                },
                {
                    "sense_id": "93617054-01e0-3179-9ff4-97a38bbe3d42",
                    "sense_definition": "Быстро ехать, управлять транспортным средством на высокой скорости.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "motion_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель такси <гнал> машину в аэропорт, чтобы мы не опоздали."
                        },
                        {
                            "text": "Не нужно так <гнать>, дорога очень скользкая и опасная."
                        }
                    ],
                    "antonyms": [
                        "останавливать"
                    ],
                    "common_phrases": [
                        "гнать машину",
                        "гнать лошадей",
                        "гнать во весь опор"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ]
                },
                {
                    "sense_id": "adb3eac4-5652-3976-9ed4-2240689e811c",
                    "sense_definition": "Грубо заставлять уйти, выгонять кого-либо прочь.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "pressure_expulsion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если этот человек снова придёт, смело <гони> его отсюда."
                        },
                        {
                            "text": "Хозяин дома <гнал> непрошеных гостей со своего двора."
                        }
                    ],
                    "antonyms": [
                        "приглашать",
                        "призывать"
                    ],
                    "common_phrases": [
                        "гнать прочь",
                        "гнать в шею",
                        "гнать из дома"
                    ]
                },
                {
                    "sense_id": "07b9ddeb-1a36-351b-ad66-22654facd824",
                    "sense_definition": "Говорить неправду, выдумывать, рассказывать небылицы или говорить глупости.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "speech_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не слушай его, он просто <гонит> и ничего не знает."
                        },
                        {
                            "text": "Хватит <гнать>, расскажи нам, как всё было на самом деле."
                        }
                    ],
                    "synonyms": [
                        "врать",
                        "лгать",
                        "выдумывать",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bf509c89-d5ef-30ff-a102-ef2cde2835b1",
                    "sense_definition": "Money in the form of notes, bills, and coins, as opposed to checks, credit, or electronic transactions.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "money_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I wanted to buy a coffee, but I didn't have any <w>cash</w> in my wallet.",
                            "target_lang_translations": {
                                "ru": "Я хотел купить кофе, но у меня в кошельке не было <w>наличных</w>."
                            }
                        },
                        {
                            "text": "The small shop accepts <w>cash</w> only, so you cannot use your card.",
                            "target_lang_translations": {
                                "ru": "Этот маленький магазин принимает только <w>наличные</w>, поэтому вы не можете использовать карту."
                            }
                        }
                    ],
                    "synonyms": [
                        "currency",
                        "change",
                        "bills",
                        "coins",
                        "legal tender",
                        "banknotes"
                    ],
                    "antonyms": [
                        "credit",
                        "check"
                    ],
                    "common_phrases": [
                        "pay in cash",
                        "hard cash",
                        "petty cash",
                        "cash only",
                        "spare cash"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деньги в виде банкнот и монет, в отличие от чеков, кредитов или электронных транзакций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нали́чные",
                                "target_lang_sense_clarification": "деньги"
                            },
                            {
                                "target_lang_word": "нали́чка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "нали́чные де́ньги",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c884e8ec-3e3e-3946-998a-7c19d88a22ca",
                    "sense_definition": "Liquid assets or money that can be traded or used quickly, as distinct from invested assets.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "money_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company is profitable, but it has a serious <w>cash</w> flow problem.",
                            "target_lang_translations": {
                                "ru": "Компания прибыльна, но у нее серьезные проблемы с потоком <w>денежных средств</w>."
                            }
                        },
                        {
                            "text": "Investors are holding more <w>cash</w> because the stock market is volatile.",
                            "target_lang_translations": {
                                "ru": "Инвесторы держат больше <w>наличности</w>, так как фондовый рынок нестабилен."
                            }
                        }
                    ],
                    "synonyms": [
                        "funds",
                        "capital",
                        "liquidity",
                        "resources"
                    ],
                    "antonyms": [
                        "debt",
                        "liability"
                    ],
                    "common_phrases": [
                        "cash flow",
                        "cash cow",
                        "cash strapped",
                        "cash crop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ликвидные активы или деньги, которые можно быстро использовать, в отличие от инвестированных активов."
                    },
                    "translations": {
                        "ru": [

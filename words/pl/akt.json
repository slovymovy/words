            "senses": [
                {
                    "sense_id": "94a87a71-3a4f-3562-b332-755f25d7be4a",
                    "sense_definition": "Czyn, działanie, postępek; to, co zostało zrobione przez kogoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To był <w>akt</w> wielkiej odwagi, który uratował im życie.",
                            "target_lang_translations": {
                                "ru": "Это был <w>акт</w> огромного мужества, который спас им жизнь."
                            }
                        },
                        {
                            "text": "Policja bada ten incydent jako chuligański <w>akt</w> wandalizmu.",
                            "target_lang_translations": {
                                "ru": "Полиция расследует этот инцидент как хулиганский <w>акт</w> вандализма."
                            }
                        }
                    ],
                    "synonyms": [
                        "poczynanie",
                        "posunięcie",
                        "gest",
                        "przejaw"
                    ],
                    "antonyms": [
                        "nieakt"
                    ],
                    "common_phrases": [
                        "akt wandalizmu",
                        "akt desperacji",
                        "akt łaski",
                        "akt przemocy",
                        "akt dobrej woli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поступок, действие; то, что было кем-то совершено."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акт",
                                "target_lang_sense_clarification": "действие, проявление (книжн.)"
                            },
                            {
                                "target_lang_word": "поступок",
                                "target_lang_sense_clarification": "совершенное действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b911f92-88e7-330a-9e3f-b46bfa570520",
                    "sense_definition": "Część utworu dramatycznego lub przedstawienia teatralnego, zwykle oddzielona od innych przerwą.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "theater_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W trzecim <w>akcie</w> sztuki następuje niespodziewany zwrot akcji.",
                            "target_lang_translations": {
                                "ru": "В третьем <w>акте</w> пьесы происходит неожиданный поворот сюжета."
                            }
                        },
                        {
                            "text": "Widzowie z niecierpliwością czekali na ostatni <w>akt</w> opery.",
                            "target_lang_translations": {
                                "ru": "Зрители с нетерпением ждали последнего <w>акта</w> оперы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pierwszy akt",
                        "w drugim akcie",
                        "akt dramatu",
                        "podział na akty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть драматического произведения или театрального представления, обычно отделенная от других перерывом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акт",
                                "target_lang_sense_clarification": "часть пьесы"
                            },
                            {
                                "target_lang_word": "действие",
                                "target_lang_sense_clarification": "театральный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "898504bb-32df-3c37-bd6e-e7fa43fa66ed",
                    "sense_definition": "Oficjalny dokument prawny lub urzędowy, stwierdzający jakiś fakt, stan prawny lub wolę władzy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz udać się do urzędu, aby odebrać odpis <w>aktu</w> urodzenia.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно пойти в учреждение, чтобы забрать копию <w>свидетельства</w> о рождении."
                            }
                        },
                        {
                            "text": "Wczoraj podpisaliśmy u notariusza <w>akt</w> zakupu nowego mieszkania.",
                            "target_lang_translations": {
                                "ru": "Вчера мы подписали у нотариуса <w>акт</w> о покупке новой квартиры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "akt notarialny",
                        "akt prawny",
                        "akt urodzenia",
                        "akt zgonu",
                        "akt oskarżenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный правовой или служебный документ, подтверждающий какой-либо факт, статус или волеизъявление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акт",
                                "target_lang_sense_clarification": "официальный документ"

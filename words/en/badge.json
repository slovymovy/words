{
    "word_family": [
        "badging",
        "rebadge",
        "debadge"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f1797f32-7ae0-3984-8540-2e3cc4a884ad",
                    "sense_definition": "A distinctive mark or object worn on clothing to show rank, membership, or authority.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "visual_identifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The officer showed his <w>badge</w> before entering the house.",
                            "target_lang_translations": {
                                "ru": "Офицер показал свой <w>жетон</w> перед тем, как войти в дом."
                            }
                        },
                        {
                            "text": "Every scout works hard to earn a merit <w>badge</w>.",
                            "target_lang_translations": {
                                "ru": "Каждый скаут усердно трудится, чтобы получить <w>значок</w> за заслуги."
                            }
                        }
                    ],
                    "synonyms": [
                        "emblem",
                        "pin"
                    ],
                    "common_phrases": [
                        "police badge",
                        "merit badge",
                        "wear a badge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличительный знак или предмет, который носят на одежде для обозначения звания, членства в организации или полномочий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "значок",
                                "target_lang_sense_clarification": "членства или отличия"
                            },
                            {
                                "target_lang_word": "жетон",
                                "target_lang_sense_clarification": "полицейский"
                            },
                            {
                                "target_lang_word": "нагрудный знак",
                                "target_lang_sense_clarification": "официальный"
                            },
                            {
                                "target_lang_word": "бляха",
                                "target_lang_sense_clarification": "разговорное, о полицейском значке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc50c994-7114-3125-a067-2228c7b7a4bd",
                    "sense_definition": "A small tag or plate worn on clothing that displays a person's name or identity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "visual_identifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please wear your visitor <w>badge</w> at all times in the building.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, носите ваш <w>бейдж</w> посетителя всё время пребывания в здании."
                            }
                        },
                        {
                            "text": "She forgot to pin her name <w>badge</w> to her jacket.",
                            "target_lang_translations": {
                                "ru": "Она забыла приколоть свой <w>бейдж</w> с именем к жакету."
                            }
                        }
                    ],
                    "common_phrases": [
                        "name badge",
                        "visitor badge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая карточка или табличка на одежде, на которой указано имя человека или его статус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бейдж",
                                "target_lang_sense_clarification": "именная карточка"
                            },
                            {
                                "target_lang_word": "бейджик",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "табличка с именем",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2cb9a843-ad5b-3562-9424-ffb54d0efdd6",
                    "sense_definition": "A plastic card used to unlock doors or gain entry to a secure area.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "access_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to swipe your <w>badge</w> to open the main gate.",
                            "target_lang_translations": {
                                "ru": "Вам нужно приложить свой <w>пропуск</w>, чтобы открыть главные ворота."
                            }
                        },
                        {
                            "text": "I left my security <w>badge</w> on my desk and cannot get back in.",
                            "target_lang_translations": {
                                "ru": "Я оставил свой <w>пропуск</w> на столе и не могу войти обратно."
                            }
                        }
                    ],
                    "synonyms": [
                        "keycard or ID card or access card"
                    ],
                    "common_phrases": [
                        "security badge",
                        "swipe a badge",
                        "badge reader"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пластиковая карта, используемая для открывания дверей или прохода в охраняемую зону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропуск",
                                "target_lang_sense_clarification": "карта доступа"
                            },
                            {
                                "target_lang_word": "электронный ключ",
                                "target_lang_sense_clarification": "карта-ключ"
                            },
                            {
                                "target_lang_word": "бейдж",
                                "target_lang_sense_clarification": "как карта доступа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "543a6601-7c32-3e36-b328-ad0b86e034a3",
                    "sense_definition": "An icon or symbol awarded to a user in a game or app for an achievement.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "digital_reward",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was happy to unlock a new <w>badge</w> after completing the level.",
                            "target_lang_translations": {
                                "ru": "Он был счастлив открыть новый <w>значок</w> после завершения уровня."
                            }
                        },
                        {
                            "text": "Her profile shows every <w>badge</w> she has collected over the years.",
                            "target_lang_translations": {
                                "ru": "В её профиле отображается каждый <w>значок</w>, который она собрала за эти годы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "earn a badge",
                        "unlock a badge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иконка или символ, присуждаемый пользователю в игре или приложении за какое-либо достижение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "значок",
                                "target_lang_sense_clarification": "в интерфейсе"
                            },
                            {
                                "target_lang_word": "ачивка",
                                "target_lang_sense_clarification": "сленг, достижение"
                            },
                            {
                                "target_lang_word": "награда",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfbbe616-0091-32eb-a07c-bc798270e672",
                    "sense_definition": "A police officer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>badge</w> asked to see my driver's license.",
                            "target_lang_translations": {
                                "ru": "<w>Полицейский</w> попросил показать мои водительские права."
                            }
                        },
                        {
                            "text": "He knew every <w>badge</w> in the precinct by name.",
                            "target_lang_translations": {
                                "ru": "Он знал каждого <w>копа</w> в участке по имени."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "refers to the officer by their insignia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полицейский (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полицейский",
                                "target_lang_sense_clarification": "нейтральное"
                            },
                            {
                                "target_lang_word": "коп",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "фараон",
                                "target_lang_sense_clarification": "устаревший сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5bcd6e00-46a6-32b0-82b6-8551f4419f42",
                    "sense_definition": "To enter or exit a restricted area by scanning an identification card.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Employees must <w>badge</w> in at the front desk every morning.",
                            "target_lang_translations": {
                                "ru": "Сотрудники должны <w>отмечаться</w> на стойке регистрации каждое утро."
                            }
                        },
                        {
                            "text": "He <w>badged</w> out for lunch and returned an hour later.",
                            "target_lang_translations": {
                                "ru": "Он <w>отметился</w> на выход на обед и вернулся через час."
                            }
                        }
                    ],
                    "common_phrases": [
                        "badge in",
                        "badge out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Входить или выходить из зоны ограниченного доступа, сканируя идентификационную карту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отмечаться",
                                "target_lang_sense_clarification": "фиксировать приход/уход"
                            },
                            {
                                "target_lang_word": "проходить по пропуску",
                                "target_lang_sense_clarification": "фразовый перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ac4fa78-602d-38e0-963b-5471ff80fb5f",
                    "sense_definition": "To mark or label something with a specific brand or name.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_label",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car was <w>badged</w> as a luxury model for the foreign market.",
                            "target_lang_translations": {
                                "ru": "Автомобиль был <w>маркирован</w> как люксовая модель для зарубежного рынка."
                            }
                        },
                        {
                            "text": "They <w>badged</w> the equipment with the company logo.",
                            "target_lang_translations": {
                                "ru": "Они <w>брендировали</w> оборудование логотипом компании."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Помечать или маркировать что-либо определенным брендом или названием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брендировать",
                                "target_lang_sense_clarification": "наносить логотип"
                            },
                            {
                                "target_lang_word": "маркировать",
                                "target_lang_sense_clarification": "ставить знак"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f4d40c64-95b8-37c3-b200-78519ee07dbb",
                    "sense_definition": "Zich rechtop op de voeten bevinden; in verticale positie zijn.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <stond> al een uur in de rij voor de kassa.",
                            "target_lang_translations": {
                                "ru": "Он уже час <стоял> в очереди в кассу."
                            }
                        },
                        {
                            "text": "Ik <sta> liever dan dat ik op de grond moet zitten.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю <стоять>, чем сидеть на полу."
                            }
                        }
                    ],
                    "antonyms": [
                        "zitten",
                        "liggen"
                    ],
                    "common_phrases": [
                        "in de rij staan",
                        "stil staan",
                        "op de uitkijk staan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в вертикальном положении на ногах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоять",
                                "target_lang_sense_clarification": "физическое положение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef67f6f0-b539-30e3-b66e-fe2dc64dcb0f",
                    "sense_definition": "Zich op een bepaalde plaats bevinden; ergens geplaatst zijn.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In Holland <staat> een huis.",
                            "target_lang_translations": {
                                "ru": "В Голландии <стоит> дом."
                            }
                        },
                        {
                            "text": "De vaas <staat> op de grote tafel in de woonkamer.",
                            "target_lang_translations": {
                                "ru": "Ваза <стоит> на большом столе в гостиной."
                            }
                        }
                    ],
                    "synonyms": [
                        "zijn"
                    ],
                    "common_phrases": [
                        "op tafel staan",
                        "voor de deur staan",
                        "in de weg staan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в определенном месте; быть расположенным где-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоять",
                                "target_lang_sense_clarification": "о предметах"
                            },
                            {
                                "target_lang_word": "находиться",
                                "target_lang_sense_clarification": "местоположение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36b3b9f4-60a8-38ab-8243-d34301ead667",
                    "sense_definition": "Geschreven zijn; vermeld worden in een tekst.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de krant <staat> een interessant artikel over het weer.",
                            "target_lang_translations": {
                                "ru": "В газете <написана> интересная статья о погоде."
                            }
                        },
                        {
                            "text": "Wat <staat> er eigenlijk op dat bord geschreven?",
                            "target_lang_translations": {
                                "ru": "Что, собственно, <написано> на этой доске?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de krant staan",
                        "op het menu staan",
                        "zwart op wit staan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть написанным или упомянутым в тексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть написанным",
                                "target_lang_sense_clarification": "о тексте"
                            },
                            {
                                "target_lang_word": "говориться",
                                "target_lang_sense_clarification": "содержаться в тексте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "448fb05f-27d1-3ecc-ac60-7cfb77d42a5d",
                    "sense_definition": "Iemand goed passen of flatteren (van kleding of uiterlijk).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die nieuwe jurk <staat> je echt heel goed.",
                            "target_lang_translations": {
                                "ru": "Это новое платье тебе очень <идет>."
                            }
                        },
                        {
                            "text": "Ik vind dat die kleur hem helemaal niet <staat>.",
                            "target_lang_translations": {
                                "ru": "Я считаю, что этот цвет ему совсем не <идет>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "goed staan",
                        "leuk staan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошо сидеть, подходить к лицу (об одежде или внешности)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "быть к лицу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5bcf1fa-25f3-3be2-b02f-a117d1e61721",
                    "sense_definition": "Hulpwerkwoord dat aangeeft dat een handeling aan de gang is (tijdens het staan).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "grammatical_aspect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat <sta> jij daar in de keuken te koken?",
                            "target_lang_translations": {
                                "ru": "Что ты там <стоишь> и готовишь на кухне?"
                            }
                        },
                        {
                            "text": "Hij <staat> buiten te telefoneren met zijn collega.",
                            "target_lang_translations": {
                                "ru": "Он <стоит> на улице и разговаривает по телефону с коллегой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "staan te wachten",
                        "staan te kijken",
                        "staan te praten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вспомогательный глагол, указывающий на то, что действие происходит в данный момент (обычно стоя)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоять",
                                "target_lang_sense_clarification": "в конструкции 'стоять и делать что-то'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbcd3c04-8d66-3b8e-adf4-a4d6d9607d2a",
                    "sense_definition": "Aangeven dat iets in de nabije toekomst gaat gebeuren.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "grammatical_aspect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als hij naar binnen gaat, <staat> hem een grote verrassing te wachten.",
                            "target_lang_translations": {
                                "ru": "Когда он войдет, его <ждет> большой сюрприз."
                            }
                        },
                        {
                            "text": "Er <staat> ons nog heel wat te gebeuren dit jaar.",
                            "target_lang_translations": {
                                "ru": "В этом году нам <предстоит> еще многое пережить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "te wachten staan",
                        "te gebeuren staan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывать на то, что что-то произойдет в ближайшем будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предстоять",
                                "target_lang_sense_clarification": "о будущем событии"
                            },
                            {
                                "target_lang_word": "ждать",
                                "target_lang_sense_clarification": "ожидать кого-то"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
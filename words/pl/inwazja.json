            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ffcd678b-25a4-3193-80a8-d92158a67d38",
                    "sense_definition": "Zbrojne wtargnięcie wojsk na terytorium innego państwa w celu jego podbicia lub okupacji.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "military_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wojskowa <w>inwazja</w> na sąsiedni kraj spotkała się z ostrym sprzeciwem społeczności międzynarodowej.",
                            "target_lang_translations": {
                                "ru": "Военное <w>вторжение</w> в соседнюю страну встретило резкое осуждение международного сообщества."
                            }
                        },
                        {
                            "text": "Planowana <w>inwazja</w> lądowa została wstrzymana z powodu trudnych warunków pogodowych.",
                            "target_lang_translations": {
                                "ru": "Планируемое наземное <w>вторжение</w> было приостановлено из-за сложных погодных условий."
                            }
                        }
                    ],
                    "synonyms": [
                        "agresja",
                        "napaść",
                        "najazd",
                        "interwencja"
                    ],
                    "common_phrases": [
                        "inwazja lądowa",
                        "inwazja zbrojna",
                        "inwazja wojskowa",
                        "inwazja na kraj"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вооруженное вступление войск на территорию другого государства с целью его завоевания или оккупации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вторжение",
                                "target_lang_sense_clarification": "военное действие"
                            },
                            {
                                "target_lang_word": "нападение",
                                "target_lang_sense_clarification": "атака, агрессия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9a144ab-505a-32fa-84b8-8a4b75d1df53",
                    "sense_definition": "Masowe i nagłe pojawienie się dużej liczby ludzi, zwierząt lub rzeczy w jednym miejscu, często postrzegane jako uciążliwe.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mass_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdy słoneczny weekend miasto przeżywa prawdziwą <w>inwazję</w> turystów.",
                            "target_lang_translations": {
                                "ru": "В каждые солнечные выходные город переживает настоящее <w>нашествие</w> туристов."
                            }
                        },
                        {
                            "text": "Tegoroczna <w>inwazja</w> komarów uniemożliwia wieczorne spacery nad jeziorem.",
                            "target_lang_translations": {
                                "ru": "В этом году <w>нашествие</w> комаров делает невозможными вечерние прогулки у озера."
                            }
                        }
                    ],
                    "synonyms": [
                        "najazd",
                        "zalew",
                        "natłok",
                        "nawał",
                        "napływ"
                    ],
                    "common_phrases": [
                        "inwazja turystów",
                        "inwazja owadów",
                        "inwazja fanów"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used metaphorically to describe a large crowd or mass occurrence"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Массовое и внезапное появление большого количества людей, животных или предметов в одном месте, часто доставляющее неудобства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нашествие",
                                "target_lang_sense_clarification": "большое скопление людей или вредителей"
                            },
                            {
                                "target_lang_word": "наплыв",
                                "target_lang_sense_clarification": "прибытие большого количества людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8e57475-7744-37af-869a-64b15eaabd85",
                    "sense_definition": "Gwałtowne rozprzestrzenianie się obcego gatunku na nowym terenie, zagrażające rodzimej przyrodzie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biological_spread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lokalny ekosystem cierpi przez gwałtowną <w>inwazję</w> obcych gatunków roślin.",
                            "target_lang_translations": {
                                "ru": "Местная экосистема страдает от стремительной <w>инвазии</w> чужеродных видов растений."
                            }
                        },
                        {
                            "text": "Rolnicy obawiają się kolejnej <w>inwazji</w> szarańczy, która może zniszczyć uprawy.",
                            "target_lang_translations": {
                                "ru": "Фермеры опасаются очередного <w>нашествия</w> саранчи, которое может уничтожить посевы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "inwazja gatunków",
                        "inwazja szkodników"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стремительное распространение чужеродного вида на новой территории, угрожающее местной природе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инвазия",
                                "target_lang_sense_clarification": "биологический термин"
                            },
                            {
                                "target_lang_word": "экспансия",
                                "target_lang_sense_clarification": "расширение ареала обитания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bf0b910-1137-3135-8d45-a5cbfaeaca0d",
                    "sense_definition": "Proces wniknięcia i namnażania się w organizmie pasożytów, bakterii lub innych czynników chorobotwórczych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biological_spread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Badania laboratoryjne potwierdziły masową <w>inwazję</w> pasożytów w układzie pokarmowym.",
                            "target_lang_translations": {
                                "ru": "Лабораторные исследования подтвердили массовую <w>инвазию</w> паразитов в пищеварительной системе."
                            }
                        },
                        {
                            "text": "Podany antybiotyk ma za zadanie powstrzymać dalszą <w>inwazję</w> bakterii w organizmie pacjenta.",
                            "target_lang_translations": {
                                "ru": "Назначенный антибиотик призван остановить дальнейшее <w>распространение</w> бактерий в организме пациента."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c92b9f1a-1f3a-30ee-b205-aa8dc2681f32",
                    "sense_definition": "Existing or happening before something else in time or order.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "time_order",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I had no <w>prior</w> knowledge that you were coming to visit.",
                            "target_lang_translations": {
                                "ru": "У меня не было <w>предварительной</w> информации о том, что вы собираетесь приехать."
                            }
                        },
                        {
                            "text": "The job requires <w>prior</w> experience in a similar role.",
                            "target_lang_translations": {
                                "ru": "Для этой работы требуется <w>предыдущий</w> опыт на аналогичной должности."
                            }
                        }
                    ],
                    "synonyms": [
                        "previous",
                        "earlier",
                        "former",
                        "preceding",
                        "past",
                        "advance"
                    ],
                    "common_phrases": [
                        "prior knowledge",
                        "prior agreement",
                        "prior notice",
                        "prior engagement",
                        "without prior warning"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существующий или происходящий раньше чего-либо другого во времени или порядке."

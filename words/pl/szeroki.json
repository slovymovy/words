    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7afa9c4a-0c8e-3ead-a389-5d5ba9ad3869",
                    "sense_definition": "Mający dużą odległość między przeciwległymi bokami lub krawędziami; o dużym wymiarze poprzecznym.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta rzeka jest bardzo <w>szeroka</w> i głęboka, więc trudno przez nią przepłynąć.",
                            "target_lang_translations": {
                                "ru": "Эта река очень <w>широкая</w> и глубокая, поэтому её трудно переплыть."
                            }
                        },
                        {
                            "text": "Kupiłem nowe, <w>szerokie</w> łóżko, żeby wygodniej spać.",
                            "target_lang_translations": {
                                "ru": "Я купил новую <w>широкую</w> кровать, чтобы спать удобнее."
                            }
                        }
                    ],
                    "synonyms": [
                        "obszerny"
                    ],
                    "antonyms": [
                        "wąski",
                        "nieszeroki"
                    ],
                    "common_phrases": [
                        "szeroka droga",
                        "szeroki uśmiech",
                        "szerokie ramiona"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий большое расстояние между противоположными сторонами; большой в поперечнике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широкий",
                                "target_lang_sense_clarification": "о физическом размере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bce439aa-6e94-3bd6-ade9-053895b51041",
                    "sense_definition": "O ubraniu: luźny, nieprzylegający ściśle do ciała lub za duży.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lubię nosić <w>szerokie</w> swetry w chłodne, zimowe dni.",
                            "target_lang_translations": {
                                "ru": "Я люблю носить <w>широкие</w> свитеры в прохладные зимние дни."
                            }
                        },
                        {
                            "text": "Te spodnie są na mnie za <w>szerokie</w> w pasie, muszę założyć pasek.",
                            "target_lang_translations": {
                                "ru": "Эти брюки мне слишком <w>широки</w> в поясе, мне нужно надеть ремень."
                            }
                        }
                    ],
                    "synonyms": [
                        "luźny",
                        "przyduży",
                        "obwisły"
                    ],
                    "antonyms": [
                        "obcisły",
                        "opięty"
                    ],
                    "common_phrases": [
                        "szerokie spodnie",
                        "szeroki rękaw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Об одежде: свободный, не прилегающий плотно к телу или слишком большой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широкий",
                                "target_lang_sense_clarification": "свободный, великоватый"
                            },
                            {
                                "target_lang_word": "просторный",
                                "target_lang_sense_clarification": "свободный крой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90a1a7b8-cbac-31f0-bef4-6ac6f8ab52d6",
                    "sense_definition": "Obejmujący wiele spraw, zagadnień lub osób; mający duży zasięg.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten skomplikowany problem wymaga <w>szerokiej</w> dyskusji w naszym zespole.",
                            "target_lang_translations": {
                                "ru": "Эта сложная проблема требует <w>широкого</w> обсуждения в нашей команде."
                            }
                        },
                        {
                            "text": "Nasz nowy pracownik jest człowiekiem o bardzo <w>szerokiej</w> wiedzy.",
                            "target_lang_translations": {
                                "ru": "Наш новый сотрудник — человек с очень <w>широкими</w> знаниями."
                            }
                        }
                    ],
                    "synonyms": [
                        "wszechstronny",
                        "różnorodny",
                        "wielostronny"
                    ],
                    "antonyms": [
                        "ograniczony"
                    ],
                    "common_phrases": [
                        "szeroka wiedza",
                        "szerokie horyzonty",
                        "szeroka oferta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охватывающий многие вопросы или людей; имеющий большой диапазон или охват."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широкий",
                                "target_lang_sense_clarification": "разносторонний, всеобъемлющий"
                            },
                            {
                                "target_lang_word": "обширный",
                                "target_lang_sense_clarification": "о знаниях, планах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cb2c80a-d9f0-3abe-bebc-333d579b009a",
                    "sense_definition": "Rozciągający się daleko, zajmujący dużą, otwartą przestrzeń.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Statek wypłynął z portu na <w>szerokie</w> wody oceanu.",
                            "target_lang_translations": {
                                "ru": "Корабль вышел из порта в <w>широкие</w> воды океана."
                            }
                        },
                        {
                            "text": "Przed naszym domem rozciągały się <w>szerokie</w> pola pszenicy.",
                            "target_lang_translations": {
                                "ru": "Перед нашим домом раскинулись <w>широкие</w> пшеничные поля."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozległy",
                        "daleki"
                    ],
                    "common_phrases": [
                        "szeroki świat",
                        "szerokie wody"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простирающийся далеко, занимающий большое открытое пространство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широкий",
                                "target_lang_sense_clarification": "о пространстве, просторах"
                            },
                            {
                                "target_lang_word": "бескрайний",
                                "target_lang_sense_clarification": "необъятный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

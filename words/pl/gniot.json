                {
                    "sense_id": "27ba1ed6-e163-3adf-adc9-f8eb11b3071c",
                    "sense_definition": "Bezwartościowy, nieudany utwór, na przykład film, książka lub piosenka; rzecz źle wykonana.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "bad_quality_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy film to totalny <w>gniot</w>, szkoda na niego czasu i pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Этот новый фильм — полная <w>дрянь</w>, жаль на него времени и денег."
                            }
                        },
                        {
                            "text": "Krytycy zgodnie uznali tę powieść za literacki <w>gniot</w>.",
                            "target_lang_translations": {
                                "ru": "Критики единодушно признали этот роман литературной <w>халтурой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "chała",
                        "szmira",
                        "tandeta",
                        "bubel",
                        "słabizna"
                    ],
                    "antonyms": [
                        "dzieło",
                        "majstersztyk"
                    ],
                    "common_phrases": [
                        "totalny gniot",
                        "straszny gniot",
                        "literacki gniot"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech to criticize poor quality"
                        }
                    ],
                    "target_lang_definitions": {

{
    "word_family": [
        "bolwerken"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "789b7487-548c-38bc-ba58-b18d2e550b71",
                    "sense_definition": "Een uitstekend verdedigingswerk van een vesting of muur.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "defensie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten trokken zich terug in het versterkte <w>bolwerk</w>.",
                            "target_lang_translations": {
                                "en": "The soldiers retreated into the fortified <w>stronghold</w>.",
                                "ru": "Солдаты отступили в укрепленный <w>бастион</w>."
                            }
                        },
                        {
                            "text": "Vanaf het hoge <w>bolwerk</w> hadden ze een goed uitzicht over de vijand.",
                            "target_lang_translations": {
                                "en": "From the high <w>bastion</w>, they had a good view of the enemy.",
                                "ru": "С высокого <w>бастиона</w> у них был хороший обзор на врага."
                            }
                        }
                    ],
                    "synonyms": [
                        "bastion",
                        "schans",
                        "vestingwerk"
                    ],
                    "common_phrases": [
                        "een stevig bolwerk",
                        "het oude bolwerk"
                    ],
                    "target_lang_definitions": {
                        "en": "A projecting defensive structure of a fortress or wall.",
                        "ru": "Выступающее оборонительное сооружение крепости или стены."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bulwark",
                                "target_lang_sense_clarification": "defensive wall or rampart"
                            },
                            {
                                "target_lang_word": "bastion",
                                "target_lang_sense_clarification": "projecting part of a fortification"
                            },
                            {
                                "target_lang_word": "stronghold",
                                "target_lang_sense_clarification": "fortified place"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бастион",
                                "target_lang_sense_clarification": "оборонительное сооружение"
                            },
                            {
                                "target_lang_word": "укрепление",
                                "target_lang_sense_clarification": "фортификация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2256aed7-ff8a-31ce-92af-9c52dc713541",
                    "sense_definition": "Een plaats of situatie waarin een groep, partij of overtuiging een zeer sterke positie heeft.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figuurlijk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze stad is al jaren een <w>bolwerk</w> van de socialistische partij.",
                            "target_lang_translations": {
                                "en": "This city has been a <w>stronghold</w> of the socialist party for years.",
                                "ru": "Этот город уже много лет является <w>оплотом</w> социалистической партии."
                            }
                        },
                        {
                            "text": "De universiteit werd gezien als een <w>bolwerk</w> van progressieve ideeën.",
                            "target_lang_translations": {
                                "en": "The university was seen as a <w>bastion</w> of progressive ideas.",
                                "ru": "Университет рассматривался как <w>оплот</w> прогрессивных идей."
                            }
                        }
                    ],
                    "synonyms": [
                        "bastion",
                        "vesting"
                    ],
                    "common_phrases": [
                        "een links bolwerk",
                        "het laatste bolwerk"
                    ],
                    "target_lang_definitions": {
                        "en": "A place or situation where a group, party, or conviction holds a very strong position.",
                        "ru": "Место или ситуация, где группа, партия или убеждение занимают очень сильную позицию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stronghold",
                                "target_lang_sense_clarification": "center of support or influence"
                            },
                            {
                                "target_lang_word": "bastion",
                                "target_lang_sense_clarification": "institution defending specific principles"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "оплот",
                                "target_lang_sense_clarification": "перен. твердыня, центр влияния"
                            },
                            {
                                "target_lang_word": "цитадель",
                                "target_lang_sense_clarification": "перен. средоточие чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6626d32f-bbdc-366f-aa0b-ed475b7853c7",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>bolwerken</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>bolwerk</w> het wel om alles voor de deadline af te krijgen.",
                            "target_lang_translations": {
                                "en": "I will <w>manage</w> to get everything done before the deadline.",
                                "ru": "Я <w>справлюсь</w> с тем, чтобы закончить всё до дедлайна."
                            }
                        },
                        {
                            "text": "Denk je dat ik dat in mijn eentje <w>bolwerk</w>?",
                            "target_lang_translations": {
                                "en": "Do you think I can <w>pull</w> that <w>off</w> on my own?",
                                "ru": "Ты думаешь, я <w>осилю</w> это в одиночку?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'bolwerken' (to manage, to pull off).",
                        "ru": "Форма первого лица единственного числа настоящего времени глагола bolwerken (справляться)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "manage",
                                "target_lang_sense_clarification": "to succeed in doing something"
                            },
                            {
                                "target_lang_word": "pull off",
                                "target_lang_sense_clarification": "to succeed in a difficult task"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "справляюсь",
                                "target_lang_sense_clarification": "удается выполнить"
                            },
                            {
                                "target_lang_word": "осиливаю",
                                "target_lang_sense_clarification": "могу сделать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc687aeb-33b7-3876-90ec-efead85cdff1",
                    "sense_definition": "Gebiedende wijs van <w>bolwerken</w>.",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Bolwerk</w> dit probleem eerst voordat je aan iets nieuws begint.",
                            "target_lang_translations": {
                                "en": "<w>Handle</w> this problem first before you start something new.",
                                "ru": "<w>Разберись</w> сначала с этой проблемой, прежде чем начинать что-то новое."
                            }
                        },
                        {
                            "text": "<w>Bolwerk</w> het zelf maar als je geen hulp wilt.",
                            "target_lang_translations": {
                                "en": "<w>Figure</w> it <w>out</w> yourself if you don't want help.",
                                "ru": "<w>Справляйся</w> с этим сам, если не хочешь помощи."
                            }
                        }

{
    "word_family": [],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3c284bee-b21e-3118-9907-b7c79809d6ac",
                    "sense_definition": "A narrow opening, groove, or slit, typically designed to receive something such as a coin or a letter.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_opening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please insert your coin into the <w>slot</w> to start the machine.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, опустите монету в <w>прорезь</w>, чтобы запустить автомат."
                            }
                        },
                        {
                            "text": "The letter fell through the mail <w>slot</w> and onto the floor.",
                            "target_lang_translations": {
                                "ru": "Письмо упало через почтовую <w>щель</w> на пол."
                            }
                        }
                    ],
                    "synonyms": [
                        "opening",
                        "slit",
                        "groove",
                        "aperture"
                    ],
                    "antonyms": [
                        "tab#Noun"
                    ],
                    "common_phrases": [
                        "mail slot",
                        "coin slot",
                        "card slot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкое отверстие, паз или щель, обычно предназначенные для опускания чего-либо, например монеты или письма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щель",
                                "target_lang_sense_clarification": "узкое отверстие (для писем, в стене)"
                            },
                            {
                                "target_lang_word": "прорезь",
                                "target_lang_sense_clarification": "отверстие для монет или карт"
                            },
                            {
                                "target_lang_word": "отверстие",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "980fe1a2-06a0-3206-9895-5d5b94a37694",
                    "sense_definition": "A specific period of time or a position allocated in a schedule or sequence.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_allocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have a free <w>slot</w> in my calendar at two o'clock.",
                            "target_lang_translations": {
                                "ru": "У меня есть свободное <w>окно</w> в календаре в два часа."
                            }
                        },
                        {
                            "text": "The television show was moved to a prime-time <w>slot</w>.",
                            "target_lang_translations": {
                                "ru": "Телешоу перенесли на <w>время</w> прайм-тайма."
                            }
                        }
                    ],
                    "synonyms": [
                        "opening",
                        "window",
                        "gap",
                        "place"
                    ],
                    "common_phrases": [
                        "time slot",
                        "delivery slot",
                        "landing slot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный период времени или место, выделенное в расписании или последовательности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окно",
                                "target_lang_sense_clarification": "свободное время в расписании"
                            },
                            {
                                "target_lang_word": "слот",
                                "target_lang_sense_clarification": "временной интервал (проф.)"
                            },
                            {
                                "target_lang_word": "интервал",
                                "target_lang_sense_clarification": "промежуток времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92b9907c-a851-32ab-a674-4d92c498a3a2",
                    "sense_definition": "A gambling machine that is operated by inserting coins; a slot machine.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "gambling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent the whole evening playing the <w>slots</w> at the casino.",
                            "target_lang_translations": {
                                "ru": "Он провел весь вечер, играя в <w>игровые автоматы</w> в казино."
                            }
                        },
                        {
                            "text": "We walked past the poker tables and went straight to the <w>slots</w>.",
                            "target_lang_translations": {
                                "ru": "Мы прошли мимо покерных столов и направились прямо к <w>автоматам</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "slot machine"
                    ],
                    "common_phrases": [
                        "play the slots"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for slot machine"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Игровой автомат, работающий от опускания монет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игровой автомат",
                                "target_lang_sense_clarification": "устройство для азартных игр"
                            },
                            {
                                "target_lang_word": "слот",
                                "target_lang_sense_clarification": "разговорное название автомата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce274875-27ba-3910-9e2d-eedc3963d4ef",
                    "sense_definition": "A specific place in a computer or electronic device where a component, such as a memory card, can be inserted.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_opening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My laptop has an extra <w>slot</w> for adding more memory.",
                            "target_lang_translations": {
                                "ru": "В моем ноутбуке есть дополнительный <w>слот</w> для увеличения памяти."
                            }
                        },
                        {
                            "text": "Make sure the graphics card is firmly seated in the expansion <w>slot</w>.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что видеокарта плотно сидит в <w>слоте</w> расширения."
                            }
                        }
                    ],
                    "synonyms": [
                        "port",
                        "socket"
                    ],
                    "common_phrases": [
                        "expansion slot",
                        "memory slot",
                        "USB slot"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специальное место в компьютере или электронном устройстве, куда можно вставить компонент, например карту памяти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слот",
                                "target_lang_sense_clarification": "компьютерный термин"
                            },
                            {
                                "target_lang_word": "разъём",
                                "target_lang_sense_clarification": "гнездо для подключения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "75523a12-5a59-3500-b6af-e0b1c127e389",
                    "sense_definition": "To place something into a narrow opening or to fit something exactly into a specific space.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_opening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>slotted</w> the disk into the drive and waited for it to load.",
                            "target_lang_translations": {
                                "ru": "Он <w>вставил</w> диск в дисковод и стал ждать загрузки."
                            }
                        },
                        {
                            "text": "The final piece of the puzzle <w>slotted</w> into place perfectly.",
                            "target_lang_translations": {
                                "ru": "Последний кусочек пазла идеально <w>встал</w> на свое место."
                            }
                        }
                    ],
                    "synonyms": [
                        "insert",
                        "fit",
                        "slide"
                    ],
                    "common_phrases": [
                        "slot in",
                        "slot into place"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать что-либо в узкое отверстие или точно вставлять что-либо в определенное пространство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вставлять",
                                "target_lang_sense_clarification": "помещать внутрь"
                            },
                            {
                                "target_lang_word": "вставать на место",
                                "target_lang_sense_clarification": "подходить по размеру"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f6b0292-5c5d-3828-b324-d045e9ab3ba9",
                    "sense_definition": "To assign a time or place for someone or something in a schedule or plan.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_allocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor managed to <w>slot</w> me in for a quick appointment this morning.",
                            "target_lang_translations": {
                                "ru": "Врачу удалось <w>втиснуть</w> меня на быстрый прием сегодня утром."
                            }
                        },
                        {
                            "text": "We need to <w>slot</w> this meeting between the two conference calls.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>поставить</w> эту встречу между двумя конференц-звонками."
                            }
                        }
                    ],
                    "synonyms": [
                        "schedule",
                        "fit in"
                    ],
                    "common_phrases": [
                        "slot someone in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Назначать время или место для кого-либо или чего-либо в расписании или плане."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "втиснуть",
                                "target_lang_sense_clarification": "найти время в плотном графике"
                            },
                            {
                                "target_lang_word": "включить в расписание",
                                "target_lang_sense_clarification": "запланировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28083ede-fead-362f-941a-4b14ac7e2b3f",
                    "sense_definition": "To kick or put a ball into a goal or a specific position with precision.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The striker calmly <w>slotted</w> the ball past the goalkeeper.",
                            "target_lang_translations": {
                                "ru": "Нападающий спокойно <w>отправил</w> мяч мимо вратаря."
                            }
                        },
                        {
                            "text": "She <w>slotted</w> home the penalty to win the match for her team.",
                            "target_lang_translations": {
                                "ru": "Она <w>забила</w> пенальти, выиграв матч для своей команды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "slot home",
                        "slot the ball"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Забивать мяч в ворота или отправлять его в определенную позицию с точностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забить",
                                "target_lang_sense_clarification": "отправить мяч в ворота"
                            },
                            {
                                "target_lang_word": "отправить",
                                "target_lang_sense_clarification": "послать мяч в определенную точку"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
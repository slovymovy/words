                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "pronominaal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het huis is oud, maar hij woont er al jaren <w>in</w>.",
                            "target_lang_translations": {
                                "en": "The house is old, but he has lived <w>in</w> it for years."
                            }
                        },
                        {
                            "text": "Dit is een doos waar veel spullen <w>in</w> passen.",
                            "target_lang_translations": {
                                "en": "This is a box that many things fit <w>in</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Inside or within (as part of a pronominal adverb)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in",
                                "target_lang_sense_clarification": "prepositional use"
                            },
                            {
                                "target_lang_word": "inside",
                                "target_lang_sense_clarification": "location"
                            }
                        ]
                    }
                }
            ]
        },

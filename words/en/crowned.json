            "senses": [
                {
                    "sense_id": "36a46d38-af45-3d63-9630-8eb8b3762538",
                    "sense_definition": "Wearing a crown or invested with the authority of a monarch.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "royal_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>crowned</w> heads of Europe gathered for the royal wedding.",
                            "target_lang_translations": {
                                "ru": "<w>Коронованные</w> особы Европы собрались на королевскую свадьбу."
                            }
                        },
                        {
                            "text": "A <w>crowned</w> lion is depicted on the national coat of arms.",
                            "target_lang_translations": {
                                "ru": "На государственном гербе изображен <w>коронованный</w> лев."
                            }

            "senses": [
                {
                    "sense_id": "8881c82f-d59d-3518-9742-c65a3c2ebd8d",
                    "sense_definition": "Iemand die door de staat of de publieke opinie als een gevaar voor de samenleving wordt beschouwd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_threat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dictator bestempelde de kritische journalist als een <w>volksvijand</w>.",
                            "target_lang_translations": {
                                "ru": "Диктатор объявил критически настроенного журналиста <w>врагом народа</w>."
                            }
                        },
                        {
                            "text": "In de propaganda werd hij afgeschilderd als <w>volksvijand</w> nummer één.",
                            "target_lang_translations": {
                                "ru": "В пропаганде его изображали <w>врагом народа</w> номер один."
                            }
                        }
                    ],
                    "synonyms": [
                        "staatsvijand",
                        "publieke vijand"
                    ],
                    "antonyms": [
                        "volksheld"
                    ],
                    "common_phrases": [
                        "volksvijand nummer één"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, которого государство или общественное мнение считает опасным для общества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "враг народа",
                                "target_lang_sense_clarification": "политический и исторический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57c56699-2ec2-39b1-9e84-de5e542c61d9",
                    "sense_definition": "Een ziekte of maatschappelijk probleem dat als een grote bedreiging voor de bevolking wordt gezien.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_threat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door onze ongezonde levensstijl is obesitas <w>volksvijand</w> nummer één geworden.",
                            "target_lang_translations": {
                                "ru": "Из-за нашего нездорового образа жизни ожирение стало <w>врагом общества</w> номер один."
                            }
                        },
                        {
                            "text": "Stress wordt tegenwoordig vaak gezien als een stille <w>volksvijand</w>.",
                            "target_lang_translations": {
                                "ru": "Стресс сегодня часто рассматривают как тихого <w>врага общества</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "volksvijand nummer één"
                    ],
                    "target_lang_definitions": {
                        "ru": "Болезнь или социальная проблема, которая рассматривается как серьезная угроза для населения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "враг общества",
                                "target_lang_sense_clarification": "об угрозе благополучию"
                            },
                            {
                                "target_lang_word": "общественная угроза",
                                "target_lang_sense_clarification": "о проблеме или явлении"
                            }
                        ]
                    }
                }
            ]
        }
    ]

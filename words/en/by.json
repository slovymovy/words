    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "856fd7dd-e500-3370-8501-56e3e2ba4705",
                    "sense_definition": "Indicates a means, method, or mode of transport used to achieve something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "means_method",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We usually go to work <w>by</w> bus because it is cheaper.",
                            "target_lang_translations": {
                                "ru": "Мы обычно ездим на работу <w>на</w> автобусе, потому что это дешевле."
                            }
                        },
                        {
                            "text": "She managed to open the locked door <w>by</w> using a credit card.",
                            "target_lang_translations": {
                                "ru": "Ей удалось открыть запертую дверь <w>с помощью</w> кредитной карты."
                            }
                        }
                    ],
                    "synonyms": [
                        "via",
                        "through"
                    ],
                    "common_phrases": [
                        "by bus",
                        "by car",
                        "by chance",
                        "by mistake",
                        "by hand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на средство, метод или способ передвижения, используемый для достижения чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "с транспортом (на автобусе, на машине)"
                            },
                            {
                                "target_lang_word": "с по́мощью",
                                "target_lang_sense_clarification": "указывает на инструмент или метод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7cf3870-0812-350a-871b-a30d66197721",
                    "sense_definition": "Located near or next to something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He sat <w>by</w> the fire to keep warm during the cold night.",
                            "target_lang_translations": {
                                "ru": "Он сидел <w>у</w> огня, чтобы согреться холодной ночью."
                            }
                        },
                        {
                            "text": "Our house is located right <w>by</w> the river.",
                            "target_lang_translations": {
                                "ru": "Наш дом находится прямо <w>у</w> реки."
                            }
                        }
                    ],
                    "synonyms": [
                        "beside",
                        "near",
                        "next to"
                    ],
                    "common_phrases": [
                        "by the window",
                        "by the sea",
                        "by my side",
                        "sit by me"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находится рядом или около чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "у",
                                "target_lang_sense_clarification": "в непосредственной близости"
                            },
                            {
                                "target_lang_word": "ря́дом с",
                                "target_lang_sense_clarification": "по соседству"
                            },
                            {
                                "target_lang_word": "о́коло",
                                "target_lang_sense_clarification": "возле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ab24d22-7400-34fb-a74a-5e6b476f47fe",
                    "sense_definition": "Not later than a specific time or deadline.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please finish your homework <w>by</w> dinner time.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, закончи домашнюю работу <w>к</w> ужину."
                            }
                        },
                        {
                            "text": "We need to be at the airport <w>by</w> six o'clock.",
                            "target_lang_translations": {
                                "ru": "Нам нужно быть в аэропорту <w>к</w> шести часам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "by tomorrow",
                        "by the time",
                        "by now",
                        "by then"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не позднее определенного времени или срока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "к определенному моменту времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e909e7e4-107f-38d3-82e1-71c85e910d6a",
                    "sense_definition": "Indicates the creator, author, or composer of a work.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "agent_cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are reading a famous play <w>by</w> William Shakespeare in class.",
                            "target_lang_translations": {
                                "ru": "В классе мы читаем известную пьесу Уильяма Шекспира."
                            }
                        },
                        {
                            "text": "The painting on the wall is <w>by</w> a local artist.",
                            "target_lang_translations": {
                                "ru": "Картина на стене написана местным художником."
                            }
                        }
                    ],
                    "common_phrases": [
                        "book by",
                        "music by",
                        "written by",
                        "directed by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на создателя, автора или композитора произведения."
                    },
                    "translations": {
                        "ru": []
                    }
                },
                {
                    "sense_id": "132c09ff-c03f-32cf-a5df-44c67467196b",
                    "sense_definition": "Indicates the person or thing that performs the action in a passive sentence.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "agent_cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The window was broken <w>by</w> a stray baseball yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера окно было разбито шальным бейсбольным мячом."
                            }
                        },
                        {
                            "text": "The decision was made <w>by</w> the committee after a long discussion.",
                            "target_lang_translations": {
                                "ru": "Решение было принято комитетом после долгого обсуждения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "caused by",
                        "made by",
                        "done by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на лицо или предмет, совершающий действие в пассивном залоге."
                    },
                    "translations": {
                        "ru": []
                    }
                },
                {
                    "sense_id": "aa86d36d-aa67-3806-9c3b-c7bebd3875cd",
                    "sense_definition": "Passing close to something while moving to the other side of it.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He walked right <w>by</w> me without saying hello.",
                            "target_lang_translations": {
                                "ru": "Он прошел прямо <w>мимо</w> меня, не поздоровавшись."
                            }
                        },
                        {
                            "text": "The bus drove <w>by</w> the station without stopping.",
                            "target_lang_translations": {
                                "ru": "Автобус проехал <w>мимо</w> станции без остановки."
                            }
                        }
                    ],
                    "synonyms": [
                        "past"
                    ],
                    "common_phrases": [
                        "walk by",
                        "drive by",
                        "run by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить близко к чему-то, двигаясь на другую сторону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ми́мо",
                                "target_lang_sense_clarification": "проходя вдоль или через"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5998eae-480a-316d-a810-be450c6f29bf",
                    "sense_definition": "Used to separate numbers when describing dimensions or size.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The room measures roughly ten feet <w>by</w> twelve feet.",
                            "target_lang_translations": {
                                "ru": "Размеры комнаты примерно десять <w>на</w> двенадцать футов."
                            }
                        },
                        {
                            "text": "We need a piece of wood that is two <w>by</w> four inches.",
                            "target_lang_translations": {
                                "ru": "Нам нужен кусок дерева размером два <w>на</w> четыре дюйма."
                            }
                        }
                    ],
                    "common_phrases": [
                        "two by four",
                        "ten by ten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для разделения чисел при описании размеров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "при указании размеров (длина на ширину)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27b24e1e-90eb-3261-8b05-2f98657c6186",
                    "sense_definition": "Indicates the amount of difference or change.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company profits increased <w>by</w> twenty percent this year.",
                            "target_lang_translations": {
                                "ru": "Прибыль компании в этом году выросла <w>на</w> двадцать процентов."
                            }
                        },
                        {
                            "text": "He missed the world record <w>by</w> only a few seconds.",
                            "target_lang_translations": {
                                "ru": "Он отстал от мирового рекорда всего <w>на</w> несколько секунд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "by far",
                        "increase by",
                        "win by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на величину разницы или изменения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "при сравнении величин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f807451-0490-3465-9739-7aea65894981",
                    "sense_definition": "Per; in units of a specified size, time, or measure.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The construction workers are paid <w>by</w> the hour.",
                            "target_lang_translations": {
                                "ru": "Строителям платят <w>за</w> час."
                            }
                        },
                        {
                            "text": "Little <w>by</w> little, the snail moved across the garden path.",
                            "target_lang_translations": {
                                "ru": "Мало-<w>по</w>малу улитка ползла по садовой дорожке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "day by day",
                        "step by step",
                        "one by one",
                        "bit by bit"
                    ],
                    "target_lang_definitions": {
                        "ru": "За; в единицах определенного размера, времени или меры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "при оплате за единицу времени/товара"
                            },
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "распределительное значение (по капле, по шагу)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61c2de19-313c-3857-aa07-cb30886d2ed6",
                    "sense_definition": "According to; in the opinion or judgment of.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>By</w> my watch, we have five minutes left.",
                            "target_lang_translations": {
                                "ru": "<w>По</w> моим часам у нас осталось пять минут."
                            }
                        },
                        {
                            "text": "It is five o'clock <w>by</w> the tower clock.",
                            "target_lang_translations": {
                                "ru": "Сейчас пять часов <w>по</w> башенным часам."
                            }
                        }
                    ],
                    "synonyms": [
                        "according to"
                    ],
                    "common_phrases": [
                        "by my reckoning",
                        "by all accounts",
                        "play by the rules"
                    ],
                    "target_lang_definitions": {
                        "ru": "Согласно; по мнению или суждению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "согласно чему-либо (по часам, по правилам)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbfae8f9-a5b1-3ec5-8c97-411958b31869",
                    "sense_definition": "Used to invoke an authority or something sacred in an oath or exclamation.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "oath",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>By</w> Jove, I think she has finally solved the puzzle!",
                            "target_lang_translations": {
                                "ru": "<w>Клянусь</w> Юпитером, я думаю, она наконец решила головоломку!"
                            }
                        },
                        {
                            "text": "I swear <w>by</w> the moon that I will return.",
                            "target_lang_translations": {
                                "ru": "Я клянусь луной, что вернусь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "by Jove",
                        "by God"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "Common in older literature or dramatic speech."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для призыва к авторитету или чему-то священному в клятве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кляну́сь",
                                "target_lang_sense_clarification": "вводное слово при клятве"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "da8940c1-beec-3772-9586-b58e4a0a9687",
                    "sense_definition": "Along a path past someone or something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I waved at her as she drove <w>by</w> in her new car.",
                            "target_lang_translations": {
                                "ru": "Я помахал ей, когда она проезжала <w>мимо</w> на своей новой машине."
                            }
                        },
                        {
                            "text": "Time goes <w>by</w> so quickly when you are having fun.",
                            "target_lang_translations": {
                                "ru": "Время летит так быстро, когда тебе весело."
                            }
                        }
                    ],
                    "synonyms": [
                        "past"
                    ],
                    "common_phrases": [
                        "pass by",
                        "go by",
                        "walk by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вдоль пути мимо кого-то или чего-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ми́мо",
                                "target_lang_sense_clarification": "наречие места/направления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "084f6dcd-1593-3b8d-9cc9-2b0dcb980fa7",
                    "sense_definition": "To or at a place for a brief visit.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "visit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will stop <w>by</w> later to pick up the documents.",
                            "target_lang_translations": {
                                "ru": "Я зайду позже, чтобы забрать документы."
                            }
                        },
                        {
                            "text": "Why don't you come <w>by</w> for coffee this afternoon?",
                            "target_lang_translations": {
                                "ru": "Почему бы тебе не зайти на кофе сегодня днем?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "stop by",
                        "drop by",
                        "come by",
                        "swing by"
                    ],
                    "target_lang_definitions": {
                        "ru": "В какое-то место с кратким визитом."
                    },
                    "translations": {
                        "ru": []
                    }
                },
                {
                    "sense_id": "3d2e99d8-57e4-3911-a93e-8697927df026",
                    "sense_definition": "In the vicinity; close at hand.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "location_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He stayed close <w>by</w> in case I needed help with the heavy boxes.",
                            "target_lang_translations": {
                                "ru": "Он оставался <w>рядом</w> на случай, если мне понадобится помощь с тяжелыми коробками."
                            }
                        },
                        {
                            "text": "There was a shepherd standing <w>by</w>.",
                            "target_lang_translations": {
                                "ru": "<w>Рядом</w> стоял пастух."
                            }
                        }
                    ],
                    "synonyms": [
                        "nearby",
                        "close"
                    ],
                    "common_phrases": [
                        "close by",
                        "near by",
                        "stand by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поблизости; под рукой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ря́дом",
                                "target_lang_sense_clarification": "наречие места"
                            },
                            {
                                "target_lang_word": "побли́зости",
                                "target_lang_sense_clarification": "неподалеку"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3501071c-e80d-39cf-85fc-78b092952795",
                    "sense_definition": "Subsidiary, incidental, or secondary; off to one side.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "secondary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They took a <w>by</w> path through the woods to avoid the main road.",
                            "target_lang_translations": {
                                "ru": "Они пошли <w>обходной</w> тропой через лес, чтобы избежать главной дороги."
                            }
                        },
                        {
                            "text": "The committee treated the complaint as a <w>by</w> issue rather than a priority.",
                            "target_lang_translations": {
                                "ru": "Комитет отнесся к жалобе как к <w>второстепенному</w> вопросу, а не как к приоритету."
                            }
                        }
                    ],
                    "synonyms": [
                        "incidental",
                        "secondary"
                    ],
                    "antonyms": [
                        "main",
                        "principal"
                    ],
                    "common_phrases": [
                        "by path",
                        "by room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дополнительный, случайный или вторичный; в стороне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побо́чный",
                                "target_lang_sense_clarification": "второстепенный"
                            },
                            {
                                "target_lang_word": "второстепе́нный",
                                "target_lang_sense_clarification": "не основной"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
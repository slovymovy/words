                {
                    "sense_id": "b30b2119-d27c-3594-b210-f43c3a0219ac",
                    "sense_definition": "De bijdrage, mening of ideeën die iemand toevoegt aan een gesprek, proces of gezamenlijk project.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "contribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>inbreng</w> tijdens de vergadering was erg nuttig voor het team.",
                            "target_lang_translations": {
                                "ru": "Его <w>вклад</w> во время собрания был очень полезен для команды."
                            }
                        },
                        {
                            "text": "De leraar vroeg om de actieve <w>inbreng</w> van alle studenten bij het project.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил всех студентов принять активное <w>участие</w> в проекте."
                            }
                        }

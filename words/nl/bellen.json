                    "semantic_group_id": "games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij speelde een kaart met <w>bellen</w> op tafel.",
                            "target_lang_translations": {
                                "en": "He played a card with <w>Bells</w> on the table.",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dae38fae-96dc-32b4-b053-281804eeb2af",
                    "sense_definition": "Намеренно или случайно сыпать на себя что-либо мелкое или сыпучее (песок, муку, снег и т.п.).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дети играли на пляже и весело <w>обсыпались</w> тёплым песком."
                        },
                        {
                            "text": "Чтобы случайно не <w>обсыпаться</w> мукой, пекарь всегда надевал чистый фартук."
                        }
                    ],
                    "antonyms": [
                        "отряхиваться"
                    ],
                    "common_phrases": [
                        "обсыпаться песком",
                        "обсыпаться мукой",
                        "обсыпаться снегом"
                    ]
                },
                {
                    "sense_id": "aa4e4520-6bd6-3700-9244-34534a88b558",
                    "sense_definition": "Покрываться множеством мелких частиц, пятен или высыпаний по всей поверхности (часто о болезни, пыли или осадках).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_of_being_covered",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После аллергии лицо ребёнка <w>обсыпалось</w> красной сыпью."
                        },
                        {
                            "text": "Мы шли через густой лес, и одежда <w>обсыпалась</w> хвоей и сухими листьями."
                        }
                    ],
                    "synonyms": [
                        "покрываться"
                    ],
                    "antonyms": [
                        "очищаться"
                    ],
                    "common_phrases": [
                        "обсыпаться сыпью",
                        "лицо обсыпалось",
                        "тело обсыпалось"
                    ]
                },
                {
                    "sense_id": "527e3f38-fe6a-336c-8fc2-65ec3706315e",
                    "sense_definition": "Разрушаться и падать вниз мелкими частями (о материалах) или терять листву и лепестки (о растениях).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "crumbling_shedding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старом заброшенном доме штукатурка давно <w>обсыпалась</w> со стен."
                        },
                        {
                            "text": "К концу октября деревья в саду окончательно <w>обсыпались</w>."
                        }
                    ],
                    "synonyms": [
                        "осыпаться",
                        "разрушаться",
                        "облетать"
                    ],
                    "common_phrases": [
                        "штукатурка обсыпалась",
                        "краска обсыпается",
                        "розы обсыпались"
                    ]
                }
            ]
        }
    ]

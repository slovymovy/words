                {
                    "sense_id": "65bf9630-bca8-328f-a2f3-37e96c3e04d3",
                    "sense_definition": "grube płótno konopne lub lniane, używane dawniej do wyrobu żagli lub worków",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "textile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiec zamówił belę <w>partu</w>, aby uszyć z niej wytrzymałe worki na zboże.",
                            "target_lang_translations": {
                                "ru": "Купец заказал кипу <w>грубого холста</w>, чтобы сшить из него прочные мешки для зерна."
                            }
                        },
                        {
                            "text": "Stare żagle wykonane z <w>partu</w> były ciężkie i trudne do wysuszenia.",
                            "target_lang_translations": {
                                "ru": "Старые паруса, сделанные из <w>парусины</w>, были тяжелыми, и их было трудно сушить."
                            }
                        }
                    ],
                    "synonyms": [
                        "partia"
                    ],
                    "common_phrases": [
                        "bela partu",
                        "worek z partu"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "słownictwo techniczne lub historyczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубая конопляная или льняная ткань, использовавшаяся ранее для изготовления парусов или мешков"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грубый холст",
                                "target_lang_sense_clarification": "ткань"

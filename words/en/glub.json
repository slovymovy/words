            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3448e02d-e6ab-30cb-8d24-dedd12843eb1",
                    "sense_definition": "The sound of underwater bubbles, or of water bubbling.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy stone sank into the dark pond with a distinct <w>glub</w>.",
                            "target_lang_translations": {
                                "ru": "Тяжёлый камень погрузился в тёмный пруд с отчётливым <w>бульком</w>."
                            }
                        },
                        {
                            "text": "We could hear the <w>glub</w> <w>glub</w> of the water cooler as it dispensed a drink.",
                            "target_lang_translations": {
                                "ru": "Мы слышали <w>бульканье</w> кулера, когда он наливал воду."
                            }
                        }
                    ],
                    "synonyms": [
                        "gurgle",
                        "glug",
                        "bubble"
                    ],
                    "common_phrases": [
                        "glub glub"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук лопающихся пузырей или бурления воды"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бульк",
                                "target_lang_sense_clarification": "резкий, однократный звук"
                            },
                            {
                                "target_lang_word": "бульканье",
                                "target_lang_sense_clarification": "продолжительный звук бурления"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e8a043ee-4d4d-34d2-86b1-0a52db05b64c",
                    "sense_definition": "To make a sound like underwater bubbling.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Air bubbles began to <w>glub</w> to the surface from the diver's mask.",
                            "target_lang_translations": {
                                "ru": "Пузырьки воздуха начали <w>булькать</w>, поднимаясь к поверхности из маски дайвера."
                            }
                        },
                        {
                            "text": "The thick liquid started to <w>glub</w> slowly out of the narrow bottle.",
                            "target_lang_translations": {
                                "ru": "Густая жидкость начала медленно <w>выбулькиваться</w> из узкой бутылки."
                            }
                        }
                    ],
                    "synonyms": [
                        "gurgle",
                        "glug",
                        "bubble"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать звук, похожий на бурление воды"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "булькать",
                                "target_lang_sense_clarification": "издавать характерный звук жидкости"
                            }
                        ]
                    }
                }
            ]
        }
    ]

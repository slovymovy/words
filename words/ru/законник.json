                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В музее хранится древний рукописный <w>законник</w> пятнадцатого века.",
                            "target_lang_translations": {
                                "en": "An ancient handwritten <w>law code</w> from the fifteenth century is kept in the museum."

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1a63eead-af9a-3761-ab42-93240dbec61f",
                    "sense_definition": "О чувстве страха, которое испытывает кто-либо (используется как сказуемое).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "state_of_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне <страшно> оставаться дома одному в темноте.",
                            "target_lang_translations": {
                                "en": "I am <scared> to stay home alone in the dark."
                            }
                        },
                        {
                            "text": "В лесу ночью было тихо и очень <страшно>.",
                            "target_lang_translations": {
                                "en": "In the forest at night it was quiet and very <scary>."
                            }
                        }
                    ],
                    "synonyms": [
                        "боязно",
                        "жутко"
                    ],
                    "common_phrases": [
                        "мне страшно",
                        "стало страшно",
                        "как страшно"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to describe a feeling of fear experienced by someone or a situation that causes fear."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scared",
                                "target_lang_sense_clarification": "feeling fear (used with 'to be')"
                            },
                            {
                                "target_lang_word": "scary",
                                "target_lang_sense_clarification": "causing fear (describing a situation)"
                            },
                            {
                                "target_lang_word": "afraid",
                                "target_lang_sense_clarification": "feeling fear"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe64f585-01b4-3d60-a6fe-3249a3d0f2b1",
                    "sense_definition": "Очень, в очень большой степени.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <страшно> устал после долгой работы.",
                            "target_lang_translations": {
                                "en": "I am <terribly> tired after long work."
                            }
                        },
                        {
                            "text": "Нам было <страшно> интересно слушать эту историю.",
                            "target_lang_translations": {
                                "en": "It was <awfully> interesting for us to listen to this story."
                            }
                        }
                    ],
                    "synonyms": [
                        "очень",
                        "ужасно",
                        "жутко"
                    ],
                    "antonyms": [
                        "немного"
                    ],
                    "common_phrases": [
                        "страшно интересно",
                        "страшно рад",
                        "страшно скучно"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Very; to a very great degree (used as an intensifier)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "terribly",
                                "target_lang_sense_clarification": "very much"
                            },
                            {
                                "target_lang_word": "awfully",
                                "target_lang_sense_clarification": "very much"
                            },
                            {
                                "target_lang_word": "dreadfully",
                                "target_lang_sense_clarification": "extremely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffb25352-317a-36d5-ae5c-6fa20ffe0deb",
                    "sense_definition": "Таким образом, что это вызывает страх; пугающе.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ветер <страшно> выл за окном всю ночь.",
                            "target_lang_translations": {
                                "en": "The wind howled <dreadfully> outside the window all night."
                            }
                        },
                        {
                            "text": "Зверь зарычал так <страшно>, что все разбежались.",
                            "target_lang_translations": {
                                "en": "The beast growled so <terrifyingly> that everyone scattered."
                            }
                        }
                    ],
                    "synonyms": [
                        "пугающе",
                        "ужасно",
                        "жутко"
                    ],
                    "antonyms": [
                        "мило"
                    ],
                    "common_phrases": [
                        "страшно выглядит",
                        "страшно кричать"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that causes fear or terror."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "77ca742d-2649-3ce7-949a-ab073f335787",
                    "sense_definition": "In a disorderly, chaotic, or untidy state.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_of_disorder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please clean up your <w>messy</w> room before you go out.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, прибери свою <w>неубранную</w> комнату, прежде чем уйти."
                            }
                        },
                        {
                            "text": "She tied her <w>messy</w> hair back with a simple ribbon.",
                            "target_lang_translations": {
                                "ru": "Она собрала свои <w>растрёпанные</w> волосы простой лентой."
                            }
                        }
                    ],
                    "synonyms": [
                        "untidy",
                        "disorderly",
                        "chaotic",
                        "cluttered"
                    ],
                    "antonyms": [
                        "neat",
                        "orderly"
                    ],
                    "common_phrases": [
                        "messy room",
                        "messy hair",
                        "messy desk"
                    ],
                    "target_lang_definitions": {
                        "ru": "В беспорядке, хаотичном или неопрятном состоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беспорядочный",
                                "target_lang_sense_clarification": "хаотичный, лишенный порядка"
                            },
                            {
                                "target_lang_word": "неубранный",
                                "target_lang_sense_clarification": "о комнате или помещении"
                            },
                            {
                                "target_lang_word": "растрёпанный",
                                "target_lang_sense_clarification": "о волосах или внешнем виде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a66075f8-3bb7-34e0-adf3-05985948363d",
                    "sense_definition": "Prone to causing a mess or lacking neatness.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_of_disorder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a very <w>messy</w> eater and often drops food on his shirt.",
                            "target_lang_translations": {
                                "ru": "Он очень <w>неаккуратный</w> в еде и часто роняет пищу на рубашку."
                            }
                        },
                        {
                            "text": "My roommate is so <w>messy</w> that I have to clean up after him daily.",
                            "target_lang_translations": {
                                "ru": "Мой сосед по комнате такой <w>неряшливый</w>, что мне приходится убирать за ним каждый день."
                            }
                        }
                    ],
                    "synonyms": [
                        "untidy",
                        "disorderly"
                    ],
                    "antonyms": [
                        "neat",
                        "orderly"
                    ],
                    "common_phrases": [
                        "messy eater",
                        "messy person",
                        "messy cook"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склонный создавать беспорядок или лишенный аккуратности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неряшливый",
                                "target_lang_sense_clarification": "о человеке, склонном к беспорядку"
                            },
                            {
                                "target_lang_word": "неаккуратный",
                                "target_lang_sense_clarification": "небрежный в действиях или внешнем виде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f03a377-bfe0-332b-9c99-160fb5eb42b0",
                    "sense_definition": "Complicated, difficult, or unpleasant to deal with.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "complicated_situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They are going through a very <w>messy</w> divorce right now.",
                            "target_lang_translations": {
                                "ru": "Они сейчас переживают очень <w>грязный</w> развод."
                            }
                        },
                        {
                            "text": "The political situation became <w>messy</w> after the scandal was revealed.",
                            "target_lang_translations": {
                                "ru": "Политическая ситуация стала <w>запутанной</w> после того, как скандал был раскрыт."
                            }
                        }
                    ],
                    "synonyms": [
                        "chaotic",
                        "disorderly"
                    ],
                    "antonyms": [
                        "orderly"
                    ],
                    "common_phrases": [
                        "messy divorce",
                        "messy situation",
                        "messy business"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запутанный, сложный или неприятный для разрешения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запутанный",
                                "target_lang_sense_clarification": "о сложной ситуации"
                            },
                            {
                                "target_lang_word": "грязный",
                                "target_lang_sense_clarification": "неприятный, скандальный (например, развод)"
                            },
                            {
                                "target_lang_word": "сложный",
                                "target_lang_sense_clarification": "трудный для решения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "120d6e3b-2aa2-3121-9338-e53f4af3eb04",
                    "sense_definition": "Argumentative, spreading gossip, or creating drama.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stop being so <w>messy</w> and mind your own business for once.",
                            "target_lang_translations": {
                                "ru": "Перестань быть таким <w>склочным</w> и займись хоть раз своим делом."
                            }
                        },
                        {
                            "text": "She loves to be <w>messy</w> by telling everyone secrets that are not hers to share.",
                            "target_lang_translations": {
                                "ru": "Она любит быть <w>скандальной</w>, рассказывая всем секреты, которыми не должна делиться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "acting messy",
                        "being messy"
                    ],
                    "traits": [

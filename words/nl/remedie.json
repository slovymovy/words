            "senses": [
                {
                    "sense_id": "fbe97762-1f59-376c-b394-0545e3e4b973",
                    "sense_definition": "Een geneesmiddel of behandeling die bedoeld is om een ziekte of lichamelijk ongemak te genezen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wetenschappers zoeken naarstig naar een effectieve <w>remedie</w> tegen het virus.",
                            "target_lang_translations": {
                                "ru": "Ученые усердно ищут эффективное <w>средство</w> против вируса."
                            }
                        },
                        {
                            "text": "Voor deze zeldzame aandoening is helaas nog geen <w>remedie</w> beschikbaar.",
                            "target_lang_translations": {
                                "ru": "К сожалению, для этого редкого заболевания пока нет доступного <w>лекарства</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "geneesmiddel",
                        "medicijn",
                        "medicament"
                    ],
                    "common_phrases": [
                        "remedie tegen",
                        "zoeken naar een remedie",
                        "geen remedie voor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лекарство или метод лечения, предназначенные для излечения болезни или физического недуга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лекарство",
                                "target_lang_sense_clarification": "медицинский препарат"
                            },
                            {
                                "target_lang_word": "средство",
                                "target_lang_sense_clarification": "метод лечения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38a258a7-8b28-3608-81d1-c2747f03469c",
                    "sense_definition": "Een oplossing of maatregel om een probleem, slechte toestand of ongewenste situatie te verbeteren of op te lossen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een lange wandeling in de natuur is vaak een goede <w>remedie</w> tegen stress.",
                            "target_lang_translations": {
                                "ru": "Длительная прогулка на природе часто является хорошим <w>средством</w> от стресса."
                            }
                        },
                        {
                            "text": "Humor kan een krachtige <w>remedie</w> zijn om de spanning in een groep te breken.",
                            "target_lang_translations": {
                                "ru": "Юмор может быть мощным <w>средством</w>, чтобы снять напряжение в группе."
                            }
                        }
                    ],
                    "synonyms": [
                        "oplossing",
                        "redmiddel",
                        "uitweg"
                    ],
                    "common_phrases": [
                        "remedie tegen verveling",
                        "de beste remedie",
                        "een paardenmiddel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решение или мера для исправления проблемы или улучшения нежелательной ситуации."
                    },
                    "translations": {

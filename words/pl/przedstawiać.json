{
    "word_family": [
        "przedstawienie",
        "przedstawiciel",
        "przedstawicielstwo"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "71fa9308-a721-3483-a95a-915b6e2b1834",
                    "sense_definition": "Zapoznawać kogoś z kimś innym, wymieniając ich imiona lub nazwiska w celu nawiązania znajomości.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "introduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym ci <przedstawić> moją siostrę, która właśnie przyjechała.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <представить> тебе мою сестру, которая только что приехала."
                            }
                        },
                        {
                            "text": "Gospodarz imprezy musi <przedstawić> gościom nową osobę.",
                            "target_lang_translations": {
                                "ru": "Хозяин вечеринки должен <представить> гостям нового человека."
                            }
                        }
                    ],
                    "synonyms": [
                        "prezentować"
                    ],
                    "common_phrases": [
                        "przedstawić kogoś komuś",
                        "przedstawić rodzicom",
                        "przedstawić znajomym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знакомить кого-то с кем-то другим, называя их имена для установления знакомства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представлять",
                                "target_lang_sense_clarification": "знакомить"
                            },
                            {
                                "target_lang_word": "знакомить",
                                "target_lang_sense_clarification": "сводить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ce9695c-7e4f-3465-8ebf-5794f3fc8e94",
                    "sense_definition": "Podawać swoje imię i nazwisko nowo poznanej osobie (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "introduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy uczeń wstał i <przedstawił> się całej klasie.",
                            "target_lang_translations": {
                                "ru": "Новый ученик встал и <представился> всему классу."
                            }
                        },
                        {
                            "text": "Pamiętaj, aby <przedstawić> się, gdy wchodzisz do gabinetu dyrektora.",
                            "target_lang_translations": {
                                "ru": "Помни, что нужно <представиться>, когда входишь в кабинет директора."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przedstawić się nazwiskiem",
                        "przedstawić się jako"
                    ],
                    "target_lang_definitions": {
                        "ru": "Называть свое имя и фамилию при знакомстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представляться",
                                "target_lang_sense_clarification": "называть себя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "251c3816-2761-3b6a-8612-bcaf472e2247",
                    "sense_definition": "Prezentować coś, dawać do obejrzenia, poddawać pod ocenę lub proponować (np. plan, dokumenty, dowody).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "presentation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zebraniu kierownik <przedstawił> plan działania na przyszły rok.",
                            "target_lang_translations": {
                                "ru": "На собрании руководитель <представил> план действий на следующий год."
                            }
                        },
                        {
                            "text": "Adwokat <przedstawił> w sądzie nowe dowody w sprawie.",
                            "target_lang_translations": {
                                "ru": "Адвокат <представил> в суде новые доказательства по делу."
                            }
                        }
                    ],
                    "synonyms": [
                        "prezentować",
                        "przedłożyć",
                        "proponować",
                        "pokazywać"
                    ],
                    "common_phrases": [
                        "przedstawić ofertę",
                        "przedstawić raport",
                        "przedstawić dowody",
                        "przedstawić propozycję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показывать, предъявлять или предлагать что-либо для оценки или ознакомления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представлять",
                                "target_lang_sense_clarification": "план, отчет"
                            },
                            {
                                "target_lang_word": "предъявлять",
                                "target_lang_sense_clarification": "документы, доказательства"
                            },
                            {
                                "target_lang_word": "демонстрировать",
                                "target_lang_sense_clarification": "показывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da95e7a2-7d8b-3c7f-ab24-56eb4670a366",
                    "sense_definition": "Tworzyć w umyśle obraz czegoś lub kogoś; wyobrażać sobie coś (zwykle w połączeniu z zaimkiem „sobie”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "imagination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie potrafię <przedstawić> sobie życia bez dostępu do internetu.",
                            "target_lang_translations": {
                                "ru": "Я не могу <представить> себе жизнь без доступа к интернету."
                            }
                        },
                        {
                            "text": "Czy możesz <przedstawić> sobie, jak wielki był to sukces?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <представить> себе, какой это был успех?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wyobrażać",
                        "wizualizować"
                    ],
                    "common_phrases": [
                        "przedstawiać sobie",
                        "trudno przedstawić sobie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать образ в уме; воображать (обычно с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представлять",
                                "target_lang_sense_clarification": "себе"
                            },
                            {
                                "target_lang_word": "воображать",
                                "target_lang_sense_clarification": "фантазировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b10450c-720f-3691-8964-07729d00f9b6",
                    "sense_definition": "Opisywać lub ukazywać kogoś albo coś w dziele artystycznym, literackim lub w relacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "depiction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten słynny obraz <przedstawia> bitwę pod Grunwaldem.",
                            "target_lang_translations": {
                                "ru": "Эта знаменитая картина <изображает> Грюнвальдскую битву."
                            }
                        },
                        {
                            "text": "Autor w swojej książce <przedstawia> losy polskiej emigracji.",
                            "target_lang_translations": {
                                "ru": "Автор в своей книге <описывает> судьбы польской эмиграции."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokazywać",
                        "prezentować"
                    ],
                    "common_phrases": [
                        "przedstawiać postać",
                        "przedstawiać historię",
                        "przedstawiać w świetle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывать или показывать кого-либо или что-либо в произведении искусства или рассказе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изображать",
                                "target_lang_sense_clarification": "на картине, в книге"
                            },
                            {
                                "target_lang_word": "показывать",
                                "target_lang_sense_clarification": "демонстрировать"
                            },
                            {
                                "target_lang_word": "описывать",
                                "target_lang_sense_clarification": "словесно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2f12ae9-1eaa-3c2f-a58c-cfbd1a2744d4",
                    "sense_definition": "Stanowić coś, być czymś, mieć określoną wartość lub znaczenie dla kogoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "constitution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dla kolekcjonera ten stary znaczek <przedstawia> ogromną wartość.",
                            "target_lang_translations": {
                                "ru": "Для коллекционера эта старая марка <представляет> огромную ценность."
                            }
                        },
                        {
                            "text": "To zadanie jest proste i nie <przedstawia> dla nas żadnego problemu.",
                            "target_lang_translations": {
                                "ru": "Эта задача проста и не <представляет> для нас никакой проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "stanowić",
                        "reprezentować"
                    ],
                    "common_phrases": [
                        "przedstawiać wartość",
                        "przedstawiać problem",
                        "nie przedstawiać trudności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Являться чем-то, иметь определенную ценность или значение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представлять",
                                "target_lang_sense_clarification": "собой, ценность"
                            },
                            {
                                "target_lang_word": "являться",
                                "target_lang_sense_clarification": "быть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de245770-8cc9-3678-914c-1cd7f6312681",
                    "sense_definition": "Wyglądać w określony sposób, kształtować się (o sytuacji lub stanie rzeczy; używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sytuacja gospodarcza kraju <przedstawia> się coraz lepiej.",
                            "target_lang_translations": {
                                "ru": "Экономическая ситуация в стране <выглядит> всё лучше."
                            }
                        },
                        {
                            "text": "Sprawa <przedstawia> się zupełnie inaczej, niż myśleliśmy na początku.",
                            "target_lang_translations": {
                                "ru": "Дело <обстоит> совершенно иначе, чем мы думали вначале."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokazywać się"
                    ],
                    "common_phrases": [
                        "sytuacja przedstawia się",
                        "sprawa przedstawia się",
                        "rzecz przedstawia się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выглядеть определенным образом, складываться (о ситуации или положении дел)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выглядеть",
                                "target_lang_sense_clarification": "иметь вид"
                            },
                            {
                                "target_lang_word": "обстоять",
                                "target_lang_sense_clarification": "о делах"
                            },
                            {
                                "target_lang_word": "складываться",
                                "target_lang_sense_clarification": "о ситуации"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
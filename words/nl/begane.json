                {
                    "sense_id": "40702b49-6229-37c0-87c0-9e6d14fabbdd",
                    "sense_definition": "Gelegen op het niveau van de straat of het maaiveld; vrijwel uitsluitend gebruikt om de onderste verdieping van een gebouw aan te duiden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het appartement bevindt zich op de <w>begane</w> grond en heeft een tuin.",
                            "target_lang_translations": {
                                "ru": "Квартира находится на <w>первом</w> этаже и имеет сад."
                            }
                        },
                        {
                            "text": "De lift gaat naar alle verdiepingen, behalve naar de <w>begane</w> grond.",
                            "target_lang_translations": {
                                "ru": "Лифт ходит на все этажи, кроме <w>первого</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "begane grond"
                    ],
                    "target_lang_definitions": {

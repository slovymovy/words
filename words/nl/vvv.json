        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "cd8477fa-badb-30a0-aebc-2490a4b20ecd",
                    "sense_definition": "Een organisatie die samenwerkt om het toerisme in een bepaalde regio te bevorderen (Vereniging voor Vreemdelingenverkeer).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tourism_org",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "De <w>VVV</w> organiseert deze zomer verschillende evenementen voor toeristen.",
                            "target_lang_translations": {
                                "en": "The <w>tourist board</w> is organizing various events for tourists this summer.",
                                "ru": "<w>Бюро по туризму</w> организует этим летом различные мероприятия для туристов."
                            }
                        },
                        {
                            "text": "Hij is al jarenlang de directeur van de <w>VVV</w> op het eiland.",
                            "target_lang_translations": {
                                "en": "He has been the director of the <w>tourist association</w> on the island for years.",
                                "ru": "Он уже много лет является директором <w>туристической ассоциации</w> на острове."
                            }
                        }
                    ],
                    "synonyms": [
                        "verkeersbureau"
                    ],
                    "common_phrases": [
                        "directeur van de VVV",
                        "Nederlandse VVV"
                    ],
                    "target_lang_definitions": {
                        "en": "An organization that works together to promote tourism in a certain region (Association for Tourist Traffic).",
                        "ru": "Организация, работающая над развитием туризма в определенном регионе."
                    },

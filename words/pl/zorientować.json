    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ad5a15de-a004-357a-9752-71b874550176",
                    "sense_definition": "Uświadomić sobie coś, zrozumieć istotę sytuacji lub problemu (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek szybko <zorientował> się, że zapomniał kluczyków do samochodu.",
                            "target_lang_translations": {
                                "ru": "Марек быстро <понял>, что забыл ключи от машины."
                            }
                        },
                        {
                            "text": "Musisz <zorientować> się w sytuacji, zanim podejmiesz ostateczną decyzję.",
                            "target_lang_translations": {
                                "ru": "Ты должен <разобраться> в ситуации, прежде чем принимать окончательное решение."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrozumieć",
                        "pojąć",
                        "domyślić się",
                        "połapać się"
                    ],
                    "common_phrases": [
                        "szybko się zorientować",
                        "zorientować się w sytuacji",
                        "zorientować się w porę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осознать что-то, понять суть ситуации или проблемы (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разобраться",
                                "target_lang_sense_clarification": "вникнуть в суть ситуации"
                            },
                            {
                                "target_lang_word": "понять",
                                "target_lang_sense_clarification": "осознать факт"
                            },
                            {
                                "target_lang_word": "сориентироваться",
                                "target_lang_sense_clarification": "быстро оценить обстановку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92cd9529-3f5f-3712-844f-6d3a5bda9606",
                    "sense_definition": "Przekazać komuś niezbędne informacje, aby ułatwić mu zrozumienie tematu lub odnalezienie się w nowym otoczeniu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierownik musi <zorientować> nowych pracowników w ich codziennych obowiązkach.",
                            "target_lang_translations": {
                                "ru": "Руководитель должен <сориентировать> новых сотрудников в их повседневных обязанностях."
                            }
                        },
                        {
                            "text": "Czy mógłbyś mnie <zorientować> w aktualnym stanie tego projektu?",
                            "target_lang_translations": {
                                "ru": "Не мог бы ты <сориентировать> меня по текущему состоянию этого проекта?"
                            }
                        }
                    ],
                    "synonyms": [
                        "poinformować",
                        "zaznajomić",
                        "wdrożyć",
                        "objaśnić"
                    ],
                    "antonyms": [
                        "zdezorientować"
                    ],
                    "common_phrases": [
                        "zorientować kogoś w temacie",
                        "zorientować klienta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать кому-то необходимую информацию, чтобы помочь понять тему или освоиться в новой обстановке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сориентировать",
                                "target_lang_sense_clarification": "дать информацию, направить"
                            },
                            {
                                "target_lang_word": "ввести в курс дела",
                                "target_lang_sense_clarification": "проинформировать о текущем состоянии"
                            },
                            {
                                "target_lang_word": "ознакомить",
                                "target_lang_sense_clarification": "с обязанностями или деталями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "097d17f4-026b-323a-a7d3-c0f21dc942e4",
                    "sense_definition": "Ustalić swoje położenie w przestrzeni lub określić kierunki świata (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Turysta próbował <zorientować> się w terenie za pomocą mapy i kompasu.",
                            "target_lang_translations": {
                                "ru": "Турист пытался <сориентироваться> на местности с помощью карты и компаса."
                            }
                        },
                        {
                            "text": "Po wyjściu z metra trudno było mi się <zorientować>, w którą stronę iść.",
                            "target_lang_translations": {
                                "ru": "После выхода из метро мне было трудно <сориентироваться>, в какую сторону идти."
                            }
                        }
                    ],
                    "synonyms": [
                        "odnaleźć się",
                        "rozeznać się"
                    ],
                    "antonyms": [
                        "zdezorientować"
                    ],
                    "common_phrases": [
                        "zorientować się w terenie",
                        "zorientować się na mapie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определить своё местоположение в пространстве или стороны света (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сориентироваться",
                                "target_lang_sense_clarification": "на местности"
                            },
                            {
                                "target_lang_word": "определить местоположение",
                                "target_lang_sense_clarification": "найти свое место на карте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "684e1b89-e8a7-3688-9005-4a1093b4bd5f",
                    "sense_definition": "Skierować coś w określoną stronę lub nadać czemuś konkretny cel.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma postanowiła <zorientować> swoje działania na rynki zagraniczne.",
                            "target_lang_translations": {
                                "ru": "Компания решила <сориентировать> свою деятельность на зарубежные рынки."
                            }
                        },
                        {
                            "text": "Architekt chciał tak <zorientować> budynek, aby salon był nasłoneczniony.",
                            "target_lang_translations": {
                                "ru": "Архитектор хотел так <сориентировать> здание, чтобы гостиная была солнечной."
                            }
                        }
                    ],
                    "synonyms": [
                        "ukierunkować",
                        "skierować",
                        "nastawić"
                    ],
                    "common_phrases": [
                        "zorientować na cel",
                        "zorientować na sukces",
                        "zorientować mapę"
                    ],
                    "target_lang_definitions": {

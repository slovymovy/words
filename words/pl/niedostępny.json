            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3dd8588f-b2e9-3b20-8cac-eea55879a8d3",
                    "sense_definition": "Taki, którego nie można w danej chwili kupić, zamówić lub z którego nie można skorzystać, ponieważ brakuje go w ofercie lub magazynie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety, ten model butów jest obecnie <w>niedostępny</w> w naszym sklepie internetowym.",
                            "target_lang_translations": {
                                "ru": "К сожалению, эта модель обуви в настоящее время <w>недоступна</w> в нашем интернет-магазине."
                            }
                        },
                        {
                            "text": "Produkt jest chwilowo <w>niedostępny</w>, ale spodziewamy się dostawy w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Товар временно <w>недоступен</w>, но мы ожидаем поставку на следующей неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieosiągalny"
                    ],
                    "antonyms": [
                        "dostępny"
                    ],
                    "common_phrases": [
                        "produkt niedostępny",
                        "towar niedostępny",
                        "chwilowo niedostępny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который нельзя купить, заказать или использовать в данный момент, так как его нет в предложении или на складе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недоступный",
                                "target_lang_sense_clarification": "о товаре или услуге"
                            },
                            {
                                "target_lang_word": "отсутствующий",
                                "target_lang_sense_clarification": "которого нет в наличии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db462e7a-90c8-377a-8141-c05ca9f2041a",
                    "sense_definition": "Taki, z którym nie można nawiązać połączenia telefonicznego, zwykle z powodu braku zasięgu lub wyłączonego urządzenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Próbowałem dzwonić do szefa, ale jego numer jest ciągle <w>niedostępny</w>.",
                            "target_lang_translations": {
                                "ru": "Я пытался дозвониться до начальника, но его номер постоянно <w>недоступен</w>."
                            }
                        },
                        {
                            "text": "Abonent czasowo <w>niedostępny</w>, proszę spróbować zadzwonić później.",
                            "target_lang_translations": {
                                "ru": "Абонент временно <w>недоступен</w>, пожалуйста, попробуйте позвонить позже."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieosiągalny"
                    ],
                    "antonyms": [
                        "dostępny"
                    ],
                    "common_phrases": [
                        "abonent niedostępny",
                        "numer niedostępny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, с которым невозможно установить телефонную связь, обычно из-за отсутствия сигнала или выключенного устройства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недоступный",
                                "target_lang_sense_clarification": "об абоненте или номере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9f78a03-6ed5-3bb2-9a0f-8c21e3c2da62",
                    "sense_definition": "Taki, do którego nie można wejść, dotrzeć lub uzyskać dostępu ze względu na przeszkody fizyczne lub zakazy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wysoki szczyt górski pozostaje <w>niedostępny</w> dla amatorów wspinaczki zimą.",
                            "target_lang_translations": {
                                "ru": "Высокая горная вершина остается <w>недоступной</w> для любителей скалолазания зимой."
                            }
                        },
                        {
                            "text": "Wiele tajnych dokumentów w tym archiwum jest <w>niedostępnych</w> dla zwykłych obywateli.",
                            "target_lang_translations": {
                                "ru": "Многие секретные документы в этом архиве <w>недоступны</w> для обычных граждан."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieprzystępny",
                        "nieosiągalny",
                        "niezdobyty"
                    ],
                    "antonyms": [
                        "dostępny"
                    ],
                    "common_phrases": [
                        "teren niedostępny",
                        "obszar niedostępny",
                        "szczyt niedostępny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, куда невозможно войти, добраться или получить доступ из-за физических препятствий или запретов."

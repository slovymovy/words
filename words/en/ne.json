            "senses": [
                {
                    "sense_id": "fa574b50-bbb1-3059-8f88-5eefe31215eb",
                    "sense_definition": "Abbreviation of northeast, a cardinal point of the compass halfway between north and east.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "compass_points",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The strong wind is blowing from the <w>NE</w> at thirty miles per hour.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер дует с <w>северо-востока</w> со скоростью тридцать миль в час."
                            }
                        },
                        {
                            "text": "We need to travel <w>NE</w> for about ten miles to reach the cabin.",
                            "target_lang_translations": {
                                "ru": "Нам нужно ехать на <w>северо-восток</w> около десяти миль, чтобы добраться до хижины."
                            }
                        }
                    ],
                    "antonyms": [
                        "SW"
                    ],
                    "common_phrases": [
                        "heading NE",
                        "NE wind",
                        "located in the NE"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"северо-восток\", сторона света между севером и востоком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "северо-восток",
                                "target_lang_sense_clarification": "сторона света"
                            },
                            {
                                "target_lang_word": "С-В",
                                "target_lang_sense_clarification": "обозначение на компасе"
                            }
                        ]
                    }
                }
            ]
        },
        {

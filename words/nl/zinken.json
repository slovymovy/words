                {
                    "sense_id": "b5cc2b24-f82f-30c8-b954-7916d71f5147",
                    "sense_definition": "Naar beneden zakken in een vloeistof, meestal water.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_downwards",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zware steen zal direct naar de bodem <w>zinken</w>.",
                            "target_lang_translations": {
                                "ru": "Тяжелый камень сразу же <w>утонет</w>.",
                                "en": "The heavy stone will <w>sink</w> straight to the bottom."
                            }
                        },
                        {
                            "text": "Het oude schip is tijdens de zware storm <w>gezonken</w>.",
                            "target_lang_translations": {
                                "ru": "Старый корабль <w>затонул</w> во время сильного шторма.",
                                "en": "The old ship <w>sank</w> during the heavy storm."
                            }
                        }
                    ],
                    "synonyms": [
                        "ondergaan",
                        "wegzakken"
                    ],
                    "antonyms": [
                        "drijven",
                        "bovenkomen"
                    ],
                    "common_phrases": [
                        "tot zinken brengen",
                        "de moed zinkt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опускаться на дно в жидкости, обычно в воде.",
                        "en": "To drop down in a liquid, usually water."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тонуть",
                                "target_lang_sense_clarification": "опускаться в воду"
                            },
                            {
                                "target_lang_word": "идти ко дну",
                                "target_lang_sense_clarification": "погружаться на дно"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "sink",
                                "target_lang_sense_clarification": "to descend in liquid"
                            },
                            {
                                "target_lang_word": "go down",
                                "target_lang_sense_clarification": "to submerge"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "00503c2d-ca97-3ba1-86c9-28d87d1a3629",
                    "sense_definition": "Gemaakt van het metaal zink.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel oude huizen in de stad hebben nog <w>zinken</w> dakgoten.",
                            "target_lang_translations": {
                                "ru": "У многих старых домов в городе все еще есть <w>цинковые</w> водосточные желоба.",
                                "en": "Many old houses in the city still have <w>zinc</w> gutters."
                            }
                        },
                        {
                            "text": "Mijn grootmoeder gebruikte vroeger altijd een <w>zinken</w> teil voor de was.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка раньше всегда использовала <w>цинковый</w> таз для стирки.",
                                "en": "My grandmother used to always use a <w>zinc</w> tub for the laundry."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zinken dakgoot",
                        "zinken emmer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный из металла цинк.",
                        "en": "Made of the metal zinc."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цинковый",
                                "target_lang_sense_clarification": "сделанный из цинка"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "zinc",
                                "target_lang_sense_clarification": "made of zinc"
                            }
                        ]
                    }
                }
            ]
        },

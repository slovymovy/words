    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "86bc1d2d-cfed-3fc5-9b63-ea3c7d391f08",
                    "sense_definition": "Something or someone that is completely unsuccessful, disappointing, or useless.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "failure_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new movie was a complete <w>dud</w> despite all the advertising.",
                            "target_lang_translations": {
                                "ru": "Новый фильм оказался полным <w>прова́лом</w>, несмотря на всю рекламу."
                            }
                        },
                        {
                            "text": "I bought a new battery for my watch, but it turned out to be a <w>dud</w>.",
                            "target_lang_translations": {
                                "ru": "Я купил новую батарейку для часов, но она оказалась <w>брако́ванной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "blank",
                        "blank",
                        "blank"
                    ],
                    "antonyms": [
                        "success",
                        "hit"
                    ],
                    "common_phrases": [
                        "a complete dud",
                        "turn out to be a dud"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то или кто-то совершенно неудачный, разочаровывающий или бесполезный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прова́л",
                                "target_lang_sense_clarification": "о фильме, мероприятии или событии"
                            },
                            {
                                "target_lang_word": "пусты́шка",
                                "target_lang_sense_clarification": "о чем-то, не оправдавшем ожиданий"
                            },
                            {
                                "target_lang_word": "брак",
                                "target_lang_sense_clarification": "о дефектном товаре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35814e5a-c3ab-3f07-a918-1b839b245041",
                    "sense_definition": "A device, especially a bomb or explosive, that fails to detonate or work as intended.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "failure_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers were careful not to touch the unexploded <w>dud</w> in the field.",
                            "target_lang_translations": {
                                "ru": "Солдаты были осторожны и не трогали <w>неразорва́вшийся снаря́д</w> в поле."
                            }
                        },
                        {
                            "text": "We thought the firework was a <w>dud</w> until it suddenly went off.",
                            "target_lang_translations": {
                                "ru": "Мы думали, что фейерверк — <w>пусты́шка</w>, пока он внезапно не взорвался."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство, особенно бомба или взрывчатое вещество, которое не взорвалось или не сработало как задумано."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неразорва́вшийся снаря́д",
                                "target_lang_sense_clarification": "военный термин"
                            }
                        ]

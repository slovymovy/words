                    ],
                    "common_phrases": [
                        "platinum card",
                        "platinum member",
                        "platinum service",
                        "platinum status"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень дорогой, элитный или высшего качества и статуса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платиновый",
                                "target_lang_sense_clarification": "о статусе или услуге"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ce2dde3-5ffb-372f-97ee-881fccc45131",
                    "sense_definition": "Of a musical recording that has sold over one million copies.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "music_award",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Their debut album went <w>platinum</w> within just three months.",
                            "target_lang_translations": {
                                "ru": "Их дебютный альбом стал <w>платиновым</w> всего за три месяца."
                            }
                        },
                        {
                            "text": "It is a <w>platinum</w> record that defined a generation of pop music.",
                            "target_lang_translations": {
                                "ru": "Это <w>платиновая</w> пластинка, определившая поколение поп-музыки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go platinum",
                        "platinum album",
                        "platinum record"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к музыкальной записи, проданной тиражом более миллиона копий."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bb8945d7-1278-31f0-a6ef-5ed9acfa1665",
                    "sense_definition": "An Internet site or web page where users can post messages and discuss specific topics.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I posted a question on the support <w>forum</w> to get help with my computer.",
                            "target_lang_translations": {
                                "ru": "Я опубликовал вопрос на <w>форуме</w> техподдержки, чтобы получить помощь с компьютером."
                            }
                        },
                        {
                            "text": "You must register an account to comment on this discussion <w>forum</w>.",
                            "target_lang_translations": {
                                "ru": "Вы должны зарегистрировать аккаунт, чтобы оставлять комментарии на этом дискуссионном <w>форуме</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "message board",
                        "discussion board"
                    ],
                    "common_phrases": [
                        "online forum",
                        "discussion forum",
                        "support forum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Интернет-сайт или веб-страница, где пользователи могут публиковать сообщения и обсуждать определенные темы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "форум",
                                "target_lang_sense_clarification": "веб-сайт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "941c20fc-5156-3557-ae62-e23a042642b0",
                    "sense_definition": "A meeting, situation, or medium where ideas and views on a particular issue can be exchanged.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The town hall meeting provided a public <w>forum</w> for citizens to voice their concerns.",
                            "target_lang_translations": {
                                "ru": "Собрание в мэрии предоставило гражданам общественную <w>площадку</w> (или <w>трибуну</w>), чтобы озвучить свои проблемы."
                            }
                        },
                        {
                            "text": "Universities often serve as a <w>forum</w> for intellectual debate and discovery.",
                            "target_lang_translations": {
                                "ru": "Университеты часто служат <w>форумом</w> для интеллектуальных дебатов и открытий."
                            }
                        }
                    ],
                    "synonyms": [
                        "symposium",
                        "assembly",
                        "meeting"
                    ],
                    "common_phrases": [
                        "public forum",
                        "open forum",
                        "international forum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собрание, ситуация или среда, где можно обмениваться идеями и мнениями по конкретному вопросу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "форум",
                                "target_lang_sense_clarification": "собрание, съезд"
                            },
                            {
                                "target_lang_word": "площадка",
                                "target_lang_sense_clarification": "место для дискуссий"
                            },
                            {
                                "target_lang_word": "трибуна",
                                "target_lang_sense_clarification": "возможность высказаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07b8b1cd-f7e1-3ac0-a87c-090ecf2a28c1",
                    "sense_definition": "A public square or marketplace in an ancient Roman city, used for judicial and other business.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tourists love to visit the ruins of the ancient Roman <w>forum</w>.",
                            "target_lang_translations": {
                                "ru": "Туристы любят посещать руины древнеримского <w>форума</w>."
                            }
                        },
                        {
                            "text": "Senators gathered in the <w>forum</w> to discuss the new laws of the empire.",
                            "target_lang_translations": {
                                "ru": "Сенаторы собирались на <w>форуме</w>, чтобы обсудить новые законы империи."
                            }
                        }
                    ],
                    "synonyms": [
                        "marketplace",
                        "public square"
                    ],
                    "common_phrases": [
                        "Roman forum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общественная площадь или рынок в древнеримском городе, использовавшиеся для судебных и других дел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "форум",
                                "target_lang_sense_clarification": "историческое"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
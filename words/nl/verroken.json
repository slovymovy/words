            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b851f139-f577-3d1b-877c-ad55f2f21263",
                    "sense_definition": "Tabak volledig oproken of door roken verbruiken.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft gisteren zijn laatste sigaar <w>verroken</w>.",
                            "target_lang_translations": {
                                "en": "He <w>smoked</w> his last cigar yesterday.",
                                "ru": "Вчера он <w>выкурил</w> свою последнюю сигару."
                            }
                        },
                        {
                            "text": "Het is zonde om al je geld aan sigaretten te <w>verroken</w>.",
                            "target_lang_translations": {
                                "en": "It is a waste to <w>smoke away</w> all your money on cigarettes.",
                                "ru": "Жаль <w>прокурить</w> все свои деньги на сигареты."
                            }
                        }
                    ],
                    "synonyms": [
                        "oproken"
                    ],
                    "common_phrases": [
                        "tabak verroken",
                        "een sigaar verroken"
                    ],
                    "target_lang_definitions": {
                        "en": "To smoke completely or consume resources by smoking.",
                        "ru": "Полностью выкурить табак или потратить ресурсы (деньги) на курение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smoke away",
                                "target_lang_sense_clarification": "to spend money or time on smoking"
                            },
                            {
                                "target_lang_word": "smoke up",
                                "target_lang_sense_clarification": "to finish a supply of tobacco"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выкурить",
                                "target_lang_sense_clarification": "употребить табачное изделие до конца"
                            },
                            {
                                "target_lang_word": "прокурить",
                                "target_lang_sense_clarification": "потратить деньги на табак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71283c41-baae-3e0d-a733-24bdd79b5d63",
                    "sense_definition": "Door brand of rookontwikkeling verloren gaan.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hele inboedel is tijdens de felle brand <w>verroken</w>.",
                            "target_lang_translations": {
                                "en": "The entire household contents <w>went up in smoke</w> during the fierce fire.",
                                "ru": "Всё имущество <w>сгорело</w> во время сильного пожара."
                            }
                        },
                        {
                            "text": "Vele kostbare documenten zijn in het archief <w>verroken</w>.",
                            "target_lang_translations": {
                                "en": "Many valuable documents <w>were destroyed by smoke</w> in the archive.",
                                "ru": "Многие ценные документы <w>сгорели</w> в архиве."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbranden"
                    ],
                    "target_lang_definitions": {
                        "en": "To be lost or destroyed due to fire or smoke.",
                        "ru": "Быть уничтоженным огнем или дымом; исчезнуть, превратившись в дым."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go up in smoke",
                                "target_lang_sense_clarification": "to be destroyed by fire"
                            },
                            {
                                "target_lang_word": "be consumed by smoke",
                                "target_lang_sense_clarification": "to be ruined specifically by smoke damage"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сгореть",
                                "target_lang_sense_clarification": "быть уничтоженным огнем"
                            },
                            {
                                "target_lang_word": "обратиться в дым",
                                "target_lang_sense_clarification": "исчезнуть в результате пожара"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "1aa20d93-8ff2-3872-a05d-6f1ea173a20b",
                    "sense_definition": "Południowoamerykański ssak parzystokopytny o gęstej, puszystej sierści, żyjący w Andach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Haremowe grupy rodzinne <w>wigoni</w> składają się z 6–19 osobników.",
                            "target_lang_translations": {
                                "ru": "Гаремные семейные группы <w>викуний</w> состоят из 6–19 особей."
                            }
                        },
                        {
                            "text": "W ogrodzie zoologicznym urodził się mały <w>wigoń</w>, który od razu stał się ulubieńcem zwiedzających.",
                            "target_lang_translations": {
                                "ru": "В зоопарке родилась маленькая <w>викунья</w>, которая сразу стала любимицей посетителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "wikunia",
                        "wikunia andyjska"
                    ],
                    "common_phrases": [
                        "stado wigoni",
                        "wełna z wigonia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Южноамериканское парнокопытное млекопитающее с густой пушистой шерстью, обитающее в Андах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "викунья",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b561cf0-b31b-38ea-8071-e324da76f6a6",
                    "sense_definition": "Przędza lub tkanina wykonana z wełny wikunii.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej nowy płaszcz był uszyty z miękkiego <w>wigonia</w> w kolorze beżowym.",
                            "target_lang_translations": {
                                "ru": "Её новое пальто было сшито из мягкой <w>вигони</w> бежевого цвета."
                            }
                        },
                        {
                            "text": "Ceny ubrań wykonanych z naturalnego <w>wigonia</w> są zazwyczaj bardzo wysokie.",
                            "target_lang_translations": {
                                "ru": "Цены на одежду из натуральной <w>вигони</w> обычно очень высоки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "płaszcz z wigonia",
                        "domieszka wigonia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пряжа или ткань, изготовленная из шерсти викуньи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вигонь",
                                "target_lang_sense_clarification": "ткань или пряжа"
                            },
                            {
                                "target_lang_word": "шерсть викуньи",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e3a819bc-461d-3e3a-aff6-26ddb975350a",
                    "sense_definition": "Надевать одежду, обувь или аксессуары, чтобы проверить, подходят ли они по размеру и как выглядят.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед покупкой она долго <примеривала> разные платья перед зеркалом.",
                            "target_lang_translations": {
                                "en": "Before buying, she <tried on> different dresses in front of the mirror for a long time."
                            }
                        },
                        {
                            "text": "В этом магазине можно спокойно <примеривать> обувь, сидя на удобном диване.",
                            "target_lang_translations": {
                                "en": "In this store, you can calmly <try on> shoes while sitting on a comfortable sofa."
                            }
                        }
                    ],
                    "synonyms": [
                        "примерять"
                    ],
                    "common_phrases": [
                        "примеривать костюм",
                        "примеривать обувь",
                        "примеривать перед зеркалом"
                    ],
                    "target_lang_definitions": {
                        "en": "To put on clothes, shoes, or accessories to check if they fit in size and how they look."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "try on",
                                "target_lang_sense_clarification": "clothes or shoes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54ccae97-2dd4-328f-976e-6cc3a739890f",
                    "sense_definition": "Мысленно сопоставлять или прикидывать одну ситуацию, роль или образ к другой для оценки соответствия.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Актёр вживался в роль и <примеривал> на себя характер своего персонажа.",
                            "target_lang_translations": {
                                "en": "The actor was getting into the role and <trying on> the character of his persona."
                            }
                        },
                        {
                            "text": "Не стоит <примеривать> чужие неудачи на свою собственную жизнь.",
                            "target_lang_translations": {
                                "en": "You shouldn't <apply> other people's failures to your own life."
                            }
                        }
                    ],
                    "synonyms": [
                        "примерять"
                    ],
                    "common_phrases": [
                        "примеривать на себя",
                        "примеривать роль"
                    ],
                    "target_lang_definitions": {
                        "en": "To mentally compare or apply a situation, role, or image to another to assess suitability."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "try on",
                                "target_lang_sense_clarification": "figurative: a role or image"
                            },
                            {
                                "target_lang_word": "apply",
                                "target_lang_sense_clarification": "a situation to oneself"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
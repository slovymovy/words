{
    "word_family": [
        "ratuszowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "af79a4e3-0567-33e6-9815-3a5d783785b1",
                    "sense_definition": "Reprezentacyjny budynek będący siedzibą władz miejskich (rady miasta, burmistrza lub prezydenta miasta), zazwyczaj usytuowany na głównym rynku.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Turyści chętnie fotografują zabytkowy <w>ratusz</w> stojący na środku rynku.",
                            "target_lang_translations": {
                                "ru": "Туристы охотно фотографируют старинную <w>ратушу</w>, стоящую посреди рыночной площади."
                            }
                        },
                        {
                            "text": "Umówiliśmy się na spotkanie pod wieżą <w>ratusza</w> w samo południe.",
                            "target_lang_translations": {
                                "ru": "Мы договорились о встрече у башни <w>ратуши</w> ровно в полдень."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zabytkowy ratusz",
                        "wieża ratusza",
                        "rynek i ratusz",
                        "siedziba w ratuszu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представительное здание, являющееся резиденцией городских властей (городского совета, мэра), обычно расположенное на главной рыночной площади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ратуша",
                                "target_lang_sense_clarification": "здание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0db0313-d67e-313c-93bc-50fd53422a35",
                    "sense_definition": "Ogół urzędników i władz zarządzających miastem; urząd miejski jako instytucja.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ratusz</w> ogłosił nowe plany dotyczące remontu dróg w centrum.",
                            "target_lang_translations": {
                                "ru": "<w>Мэрия</w> объявила о новых планах по ремонту дорог в центре."
                            }
                        },
                        {
                            "text": "Mieszkańcy złożyli petycję, ale rzecznik <w>ratusza</w> odmówił komentarza.",
                            "target_lang_translations": {
                                "ru": "Жители подали петицию, но представитель <w>мэрии</w> отказался от комментариев."
                            }
                        }
                    ],
                    "synonyms": [
                        "magistrat"
                    ],
                    "common_phrases": [
                        "rzecznik ratusza",
                        "decyzja ratusza",
                        "pracownik ratusza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность чиновников и властей, управляющих городом; городское управление как учреждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мэрия",
                                "target_lang_sense_clarification": "орган власти"
                            },
                            {
                                "target_lang_word": "городская администрация",
                                "target_lang_sense_clarification": "учреждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e23c36a9-912e-35d0-bd7d-6fb3718c4828",
                    "sense_definition": "Dawne pomieszczenie w budynku miejskim służące jako areszt lub więzienie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pijanych awanturników straż miejska zamykała na noc w <w>ratuszu</w>.",
                            "target_lang_translations": {
                                "ru": "Пьяных дебоширов городская стража запирала на ночь на <w>гауптвахте</w>."
                            }
                        },
                        {
                            "text": "W starych kronikach opisano ucieczkę więźnia, który wydostał się z <w>ratusza</w>.",
                            "target_lang_translations": {
                                "ru": "В старых хрониках описан побег узника, который выбрался из <w>темницы</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trafić do ratusza",
                        "siedzieć w ratuszu"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "znaczenie historyczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинное помещение в городском здании, служившее арестным домом или тюрьмой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гауптвахта",
                                "target_lang_sense_clarification": "место заключения (устар.)"
                            },
                            {
                                "target_lang_word": "арестантская",
                                "target_lang_sense_clarification": "помещение для задержанных"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
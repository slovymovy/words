    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0155f2e4-29d3-3c73-9c82-e4d0661edbe3",
                    "sense_definition": "Обратный скос броневого листа в нижней части башни танка или другой боевой машины.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Конструкторы решили убрать <w>заман</w> башни, чтобы повысить защиту танка.",
                            "target_lang_translations": {
                                "en": "The designers decided to remove the turret <w>shot trap</w> to improve the tank's protection."
                            }
                        },
                        {
                            "text": "Снаряд попал в <w>заман</w>, что привело к заклиниванию механизма поворота.",
                            "target_lang_translations": {
                                "en": "The shell hit the <w>undercut</w>, which led to the jamming of the traverse mechanism."
                            }
                        }
                    ],
                    "common_phrases": [
                        "заман башни",
                        "задний заман"
                    ],
                    "target_lang_definitions": {
                        "en": "A reverse slope or undercut in the lower part of a tank turret or other combat vehicle, often creating a vulnerability."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shot trap",
                                "target_lang_sense_clarification": "military terminology; a design flaw where projectiles are deflected into vulnerable areas"
                            },
                            {
                                "target_lang_word": "undercut",
                                "target_lang_sense_clarification": "structural description of the reverse slope geometry"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "162122ef-c366-3714-a5ca-454a4d2d20f8",
                    "sense_definition": "То, что привлекает или манит; приманка, соблазн.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Главный <w>заман</w> этой поездки заключался в полной тишине и отсутствии людей.",
                            "target_lang_translations": {
                                "en": "The main <w>attraction</w> of this trip lay in the complete silence and absence of people."
                            }
                        },
                        {
                            "text": "В этом рискованном деле был свой особый <w>заман</w>.",
                            "target_lang_translations": {
                                "en": "There was a special <w>allure</w> in this risky business."
                            }
                        }
                    ],
                    "synonyms": [
                        "приманка",
                        "соблазн"
                    ],
                    "common_phrases": [
                        "главный заман"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that attracts, beckons, or tempts; a lure or enticement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lure",
                                "target_lang_sense_clarification": "something that tempts or attracts"
                            },
                            {
                                "target_lang_word": "allure",
                                "target_lang_sense_clarification": "quality of being powerfully and mysteriously attractive"
                            },
                            {
                                "target_lang_word": "attraction",
                                "target_lang_sense_clarification": "a feature or quality that makes something interesting or desirable"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "2d42c72f-4618-304d-bb99-265171f75cec",
                    "sense_definition": "Мужское личное имя.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Наш новый сосед <w>Заман</w> оказался очень вежливым человеком.",
                            "target_lang_translations": {
                                "en": "Our new neighbor <w>Zaman</w> turned out to be a very polite person."
                            }
                        },
                        {
                            "text": "Вчера <w>Заман</w> успешно сдал вступительный экзамен.",
                            "target_lang_translations": {
                                "en": "Yesterday <w>Zaman</w> successfully passed the entrance exam."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A male personal name."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Zaman",
                                "target_lang_sense_clarification": "proper noun"
                            }
                        ]
                    }
                }
            ]
        }
    ]

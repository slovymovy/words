                {
                    "sense_id": "3090fda0-5469-3844-9f43-42746a62f3b9",
                    "sense_definition": "A wide, flat metal container with a handle, used for cooking food.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cooking_vessel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She placed the <w>pan</w> on the stove to heat the oil.",
                            "target_lang_translations": {
                                "ru": "Она поставила <w>сковороду</w> на плиту, чтобы разогреть масло."
                            }
                        },
                        {
                            "text": "I need a clean frying <w>pan</w> to cook the eggs.",
                            "target_lang_translations": {
                                "ru": "Мне нужна чистая <w>сковорода</w>, чтобы пожарить яйца."
                            }
                        }
                    ],
                    "synonyms": [
                        "frying pan",
                        "skillet"
                    ],
                    "common_phrases": [
                        "frying pan",
                        "pots and pans",
                        "non-stick pan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Широкая плоская металлическая емкость с ручкой, используемая для жарки пищи."

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "23d5af0c-7125-3549-9ed2-802462c961ea",
                    "sense_definition": "С чувством глубокого отчаяния, безнадёжности или с крайним напряжением сил; пренебрегая опасностью.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "despair_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <w>отчаянно</w> пытался найти выход из сложившейся ситуации, но времени оставалось мало.",
                            "target_lang_translations": {
                                "en": "He <w>desperately</w> tried to find a way out of the situation, but there was little time left."
                            }
                        },
                        {
                            "text": "Маленький котёнок <w>отчаянно</w> пищал, застряв на высоком дереве.",
                            "target_lang_translations": {
                                "en": "The little kitten squeaked <w>desperately</w>, stuck in a tall tree."
                            }
                        }
                    ],
                    "synonyms": [
                        "безнадёжно",
                        "неистово",
                        "безрассудно"
                    ],
                    "antonyms": [
                        "спокойно",
                        "сдержанно",
                        "осторожно"
                    ],
                    "common_phrases": [
                        "отчаянно бороться",
                        "отчаянно кричать",
                        "отчаянно сопротивляться",
                        "отчаянно смелый"
                    ],
                    "target_lang_definitions": {
                        "en": "In a manner showing deep despair, hopelessness, or extreme effort; sometimes disregarding danger."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "desperately",
                                "target_lang_sense_clarification": "with extreme urgency or hopelessness"
                            },
                            {
                                "target_lang_word": "frantically",
                                "target_lang_sense_clarification": "in a panicked or hurried manner"
                            },
                            {
                                "target_lang_word": "recklessly",
                                "target_lang_sense_clarification": "without regard for danger"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3dc7700f-f498-3869-9199-82f7eb9576c1",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>отчаянный</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его положение было <w>отчаянно</w>, и он не знал, к кому обратиться за помощью.",
                            "target_lang_translations": {
                                "en": "His situation was <w>desperate</w>, and he did not know who to turn to for help."
                            }
                        },
                        {
                            "text": "Лицо её было бледно и <w>отчаянно</w>, выражая всю глубину горя.",
                            "target_lang_translations": {
                                "en": "Her face was pale and <w>desperate</w>, expressing the full depth of her grief."
                            }
                        }
                    ],
                    "traits": [
                        {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4d8c971e-c9e1-331a-947c-9fdd2b04eac8",
                    "sense_definition": "Przywrócić czemuś świeżość, czystość lub pierwotny, nowy wygląd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <odświeżyć> ściany w salonie, bo kolor już wyblakł.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <обновить> стены в гостиной, так как цвет уже выцвел."
                            }
                        },
                        {
                            "text": "Warto otworzyć szeroko okna, aby <odświeżyć> powietrze w całym mieszkaniu.",
                            "target_lang_translations": {
                                "ru": "Стоит широко открыть окна, чтобы <освежить> воздух во всей квартире."
                            }
                        }
                    ],
                    "synonyms": [
                        "odnowić",
                        "zregenerować"
                    ],
                    "antonyms": [
                        "zaniedbać",
                        "zniszczyć"
                    ],
                    "common_phrases": [
                        "odświeżyć mieszkanie",
                        "odświeżyć wygląd",
                        "odświeżyć powietrze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вернуть чему-либо свежесть, чистоту или первоначальный вид."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обновить",
                                "target_lang_sense_clarification": "сделать новым, отремонтировать"
                            },
                            {
                                "target_lang_word": "освежить",
                                "target_lang_sense_clarification": "сделать свежим (воздух, вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f87ca95-a3a8-398f-8897-1884446a6f3c",
                    "sense_definition": "Sprawić, że ktoś odzyskuje siły witalne, energię lub czuje się rześko (często używane zwrotnie).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimny prysznic pomógł mi się <odświeżyć> po długiej podróży.",
                            "target_lang_translations": {
                                "ru": "Холодный душ помог мне <освежиться> после долгой поездки."
                            }
                        },
                        {
                            "text": "Krótki spacer po lesie znakomicie <odświeżył> moje myśli.",
                            "target_lang_translations": {
                                "ru": "Короткая прогулка по лесу отлично <освежила> мои мысли."
                            }
                        }
                    ],
                    "synonyms": [
                        "orzeźwić",
                        "ożywić"
                    ],
                    "antonyms": [
                        "zmęczyć",
                        "znużyć"
                    ],
                    "common_phrases": [
                        "odświeżyć się",
                        "odświeżyć umysł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восстановить силы, бодрость или привести себя в порядок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освежиться",
                                "target_lang_sense_clarification": "привести себя в порядок, взбодриться"
                            },
                            {
                                "target_lang_word": "взбодрить",
                                "target_lang_sense_clarification": "придать энергии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b1952c7-8628-3141-b6af-93ed6e3c3dbe",
                    "sense_definition": "Przypomnieć sobie zdobytą wcześniej wiedzę lub umiejętności, aby znów biegle się nimi posługiwać.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wyjazdem do Londynu chcę <odświeżyć> swój angielski.",
                            "target_lang_translations": {
                                "ru": "Перед поездкой в Лондон я хочу <освежить> свой английский."
                            }
                        },
                        {
                            "text": "Przeglądam stare notatki, żeby <odświeżyć> pamięć przed egzaminem.",
                            "target_lang_translations": {
                                "ru": "Я просматриваю старые заметки, чтобы <освежить> память перед экзаменом."
                            }
                        }
                    ],
                    "synonyms": [
                        "przypomnieć",
                        "powtórzyć"
                    ],
                    "antonyms": [
                        "zapomnieć"
                    ],
                    "common_phrases": [
                        "odświeżyć pamięć",
                        "odświeżyć znajomość",
                        "odświeżyć język"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вспомнить ранее полученные знания или навыки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освежить",
                                "target_lang_sense_clarification": "в памяти, знания"
                            },
                            {
                                "target_lang_word": "подтянуть",
                                "target_lang_sense_clarification": "улучшить уровень знаний"
                            }
                        ]
                    }
                }
            ]
        }
    ]

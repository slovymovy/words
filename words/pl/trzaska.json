    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ce40f807-82af-3646-9369-b1cf58e19f6b",
                    "sense_definition": "Trzecia osoba liczby pojedynczej czasu teraźniejszego czasownika <w>trzaskać</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad zawsze głośno <trzaska> drzwiami, gdy wychodzi z domu.",
                            "target_lang_translations": {
                                "ru": "Мой сосед всегда громко <хлопает> дверью, когда выходит из дома."
                            }
                        },
                        {
                            "text": "Ogień w kominku wesoło <trzaska>, dając przyjemne ciepło w salonie.",
                            "target_lang_translations": {
                                "ru": "Огонь в камине весело <трещит>, давая приятное тепло в гостиной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trzaska drzwiami",
                        "trzaska mróz",
                        "ogień trzaska"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Третье лицо единственного числа настоящего времени глагола trzaskać."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хлопает",
                                "target_lang_sense_clarification": "издавать резкий звук удара (о дверях)"
                            },
                            {
                                "target_lang_word": "трещит",
                                "target_lang_sense_clarification": "издавать сухой треск (об огне, морозе)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "28fc36c4-4e68-3ead-9e05-958110d17410",
                    "sense_definition": "Drobny, ostry kawałek drewna oderwany od większej całości, często używany jako podpałka.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wbiła mi się w palec mała <trzaska>, kiedy rąbałem drewno na opał.",
                            "target_lang_translations": {
                                "ru": "Мне в палец вонзилась маленькая <заноза>, когда я колол дрова на топливо."
                            }
                        },
                        {
                            "text": "Wrzucił kilka suchych <trzasek> do ogniska, żeby ogień szybciej zapłonął.",
                            "target_lang_translations": {
                                "ru": "Он бросил несколько сухих <щепок> в костер, чтобы огонь быстрее разгорелся."
                            }
                        }
                    ],
                    "synonyms": [
                        "drzazga"
                    ],
                    "common_phrases": [
                        "sucha trzaska",
                        "ostra trzaska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкий, острый кусок дерева, отколовшийся от большего целого, часто используемый для растопки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щепка",
                                "target_lang_sense_clarification": "кусок дерева"
                            },
                            {
                                "target_lang_word": "заноза",
                                "target_lang_sense_clarification": "острый кусочек, попавший под кожу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d075e3fe-c67b-3e92-81b8-a7576408db66",
                    "sense_definition": "Gatunek motyla nocnego z rodziny sówkowatych (błędnica trzaska).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Błędnica <trzaska> to rzadki motyl, którego można spotkać w lasach liściastych.",
                            "target_lang_translations": {
                                "ru": "Совка <птичье крыло> — это редкая бабочка, которую можно встретить в лиственных лесах."
                            }
                        },
                        {
                            "text": "W atlasie owadów znajduje się szczegółowy opis gatunku błędnica <trzaska>.",
                            "target_lang_translations": {
                                "ru": "В атласе насекомых находится подробное описание вида совка <птичье крыло>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "błędnica trzaska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид ночной бабочки из семейства совок (Dipterygia scabriuscula)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совка",
                                "target_lang_sense_clarification": "семейство бабочек"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
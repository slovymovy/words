                        "высекать"
                    ],
                    "antonyms": [
                        "вставлять"
                    ],
                    "common_phrases": [
                        "выбивать дверь",
                        "выбивать стекло",
                        "выбивать зуб",
                        "выбивать клин"
                    ],

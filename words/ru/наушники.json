    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b05f2876-c40f-3dc3-ab48-db1805c399e3",
                    "sense_definition": "Устройство для индивидуального прослушивания звука, которое надевается на голову или вставляется в уши.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "audio_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я всегда беру с собой <наушники>, чтобы слушать музыку в метро.",
                            "target_lang_translations": {
                                "en": "I always take my <headphones> with me to listen to music on the subway."
                            }
                        },
                        {
                            "text": "Пожалуйста, надень <наушники>, если хочешь смотреть фильм ночью.",
                            "target_lang_translations": {
                                "en": "Please put on <headphones> if you want to watch a movie at night."
                            }
                        }
                    ],
                    "synonyms": [
                        "уши"
                    ],
                    "common_phrases": [
                        "надеть наушники",
                        "снять наушники",
                        "беспроводные наушники",
                        "слушать в наушниках"
                    ],
                    "target_lang_definitions": {
                        "en": "A device for individual sound listening, worn on the head or inserted into the ears."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "headphones",
                                "target_lang_sense_clarification": "audio device worn over the head"
                            },
                            {
                                "target_lang_word": "earphones",
                                "target_lang_sense_clarification": "audio device inserted into the ears"
                            },
                            {
                                "target_lang_word": "earbuds",
                                "target_lang_sense_clarification": "small in-ear audio device"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12a09404-1b71-357e-a37b-0e0f88db0471",
                    "sense_definition": "Тёплый аксессуар в виде двух накладок на дужке, закрывающий уши для защиты от холода.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_accessory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой она предпочитает носить меховые <наушники> вместо шапки.",
                            "target_lang_translations": {
                                "en": "In winter, she prefers wearing fur <earmuffs> instead of a hat."
                            }
                        },
                        {
                            "text": "Эти пушистые <наушники> отлично защищают от ветра и мороза.",
                            "target_lang_translations": {
                                "en": "These fluffy <earmuffs> protect perfectly against wind and frost."
                            }
                        }
                    ],
                    "common_phrases": [
                        "меховые наушники",
                        "тёплые наушники",
                        "зимние наушники"
                    ],
                    "target_lang_definitions": {
                        "en": "A warm accessory consisting of two pads on a band, covering the ears to protect against the cold."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "earmuffs",
                                "target_lang_sense_clarification": "clothing accessory for warmth"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a4ed10a-b46c-3338-b0b3-6bc455f6c714",
                    "sense_definition": "Опускающиеся детали зимней шапки, предназначенные для защиты ушей и шеи от холода.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Когда начался сильный буран, он опустил <наушники> своей шапки.",
                            "target_lang_translations": {
                                "en": "When the heavy blizzard started, he lowered the <earflaps> of his hat."
                            }
                        },
                        {
                            "text": "У старой ушанки были длинные <наушники>, которые можно было завязать под подбородком.",
                            "target_lang_translations": {
                                "en": "The old ushanka had long <earflaps> that could be tied under the chin."
                            }
                        }
                    ],
                    "common_phrases": [
                        "опустить наушники",
                        "поднять наушники",
                        "завязать наушники"
                    ],
                    "target_lang_definitions": {
                        "en": "Flaps on a winter hat designed to protect the ears and neck from the cold."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "earflaps",
                                "target_lang_sense_clarification": "part of a hat"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb3d6de7-9652-3cc2-87ac-b0e6dec99b03",
                    "sense_definition": "Форма именительного или винительного падежа множественного числа существительного <w>наушник</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В коробке лежал только один сломанный <наушник>.",
                            "target_lang_translations": {
                                "en": "There was only one broken <earphone> in the box."
                            }
                        },
                        {
                            "text": "Этот <наушник> постоянно выпадает из уха при беге.",
                            "target_lang_translations": {
                                "en": "This <earbud> keeps falling out of my ear while running."
                            }
                        }
                    ],
                    "traits": [

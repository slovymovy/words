{
    "word_family": [
        "welnu"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "aac023be-4f4a-3002-8857-fd4d9eab314a",
                    "sense_definition": "Op het huidige ogenblik; op dit moment.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_indication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb <w>nu</w> helaas geen tijd om je te helpen met je huiswerk.",
                            "target_lang_translations": {
                                "en": "Unfortunately, I don't have time <w>now</w> to help you with your homework.",
                                "ru": "К сожалению, у меня <w>сейчас</w> нет времени помочь тебе с домашним заданием."
                            }
                        },
                        {
                            "text": "Kom je <w>nu</w> meteen naar huis of blijf je nog even daar?",
                            "target_lang_translations": {
                                "en": "Are you coming home right <w>now</w> or are you staying there a bit longer?",
                                "ru": "Ты идешь домой <w>прямо сейчас</w> или останешься там еще ненадолго?"
                            }
                        }
                    ],
                    "synonyms": [
                        "momenteel",
                        "thans",
                        "heden"
                    ],
                    "antonyms": [
                        "toen",
                        "straks",
                        "later"
                    ],
                    "common_phrases": [
                        "nu en dan",
                        "tot nu toe",
                        "vanaf nu",
                        "nu of nooit"
                    ],
                    "target_lang_definitions": {
                        "en": "At the present moment; right now.",
                        "ru": "В настоящий момент; прямо сейчас."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "now",
                                "target_lang_sense_clarification": "at this time"
                            },
                            {
                                "target_lang_word": "currently",
                                "target_lang_sense_clarification": "at present"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сейчас",
                                "target_lang_sense_clarification": "в данный момент"
                            },
                            {
                                "target_lang_word": "теперь",
                                "target_lang_sense_clarification": "в настоящее время (в отличие от прошлого)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb57c7e6-087a-35c0-9e89-fd3ac9a1337c",
                    "sense_definition": "Wordt gebruikt om ongeduld uit te drukken of om een vraag of bevel kracht bij te zetten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "modal_particle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat is er <w>nu</w> weer aan de hand met die computer?",
                            "target_lang_translations": {
                                "en": "What is wrong with that computer <w>now</w>?",
                                "ru": "Что <w>же</w> опять случилось с этим компьютером?"
                            }
                        },
                        {
                            "text": "Luister <w>nu</w> toch eens goed naar wat ik tegen je zeg.",
                            "target_lang_translations": {
                                "en": "Just listen carefully <w>now</w> to what I'm telling you.",
                                "ru": "Послушай <w>же</w> внимательно, что я тебе говорю."
                            }
                        }
                    ],
                    "synonyms": [
                        "nou"
                    ],
                    "common_phrases": [
                        "kom nu",
                        "wat nu weer"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to express impatience or to reinforce a question or command.",
                        "ru": "Используется для выражения нетерпения или для усиления вопроса или приказа."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "now",
                                "target_lang_sense_clarification": "emphasis or impatience"
                            },
                            {
                                "target_lang_word": "just",
                                "target_lang_sense_clarification": "reinforcement of command"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "же",
                                "target_lang_sense_clarification": "усилительная частица"
                            },
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "выражение нетерпения или побуждения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e95efac0-cc6a-3914-823b-39adc7acee39",
                    "sense_definition": "Wordt gebruikt om een overgang, tegenstelling of nieuw punt in een verhaal aan te geven.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "discourse_marker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Nu</w> zijn er mensen die beweren dat dit niet waar is.",
                            "target_lang_translations": {
                                "en": "<w>Now</w>, there are people who claim that this is not true.",
                                "ru": "<w>Однако</w> есть люди, которые утверждают, что это неправда."
                            }
                        },
                        {
                            "text": "<w>Nu</w> wil het toeval dat ik hem gisteren nog heb gesproken.",
                            "target_lang_translations": {
                                "en": "<w>Now</w>, it just so happens that I spoke to him yesterday.",
                                "ru": "<w>И вот</w> по совпадению я говорил с ним только вчера."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to indicate a transition, contrast, or new point in a story.",
                        "ru": "Используется для обозначения перехода, противопоставления или новой мысли в повествовании."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "now",
                                "target_lang_sense_clarification": "discourse marker"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "однако",
                                "target_lang_sense_clarification": "противопоставление"
                            },
                            {
                                "target_lang_word": "и вот",
                                "target_lang_sense_clarification": "переход к новому факту"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "d90f57c2-d3e4-3d3b-aba1-9efa7af120a7",
                    "sense_definition": "Omdat; gegeven het feit dat een bepaalde situatie is ingetreden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "conjunction_reason",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Nu</w> je het zegt, weet ik het weer precies.",
                            "target_lang_translations": {
                                "en": "<w>Now that</w> you mention it, I remember it exactly again.",
                                "ru": "<w>Раз</w> ты это сказал, я снова точно это помню."
                            }
                        },
                        {
                            "text": "We kunnen eindelijk beginnen met de vergadering <w>nu</w> iedereen aanwezig is.",
                            "target_lang_translations": {
                                "en": "We can finally start the meeting <w>now that</w> everyone is present.",
                                "ru": "Мы можем наконец начать собрание, <w>раз</w> все присутствуют."
                            }
                        }
                    ],
                    "synonyms": [
                        "welnu",
                        "thans"
                    ],
                    "common_phrases": [
                        "nu dat",
                        "nu eenmaal"
                    ],
                    "target_lang_definitions": {
                        "en": "Because; given the fact that a certain situation has occurred.",
                        "ru": "Поскольку; учитывая тот факт, что определенная ситуация наступила."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "now that",
                                "target_lang_sense_clarification": "conjunction"
                            },
                            {
                                "target_lang_word": "since",
                                "target_lang_sense_clarification": "reason"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "раз",
                                "target_lang_sense_clarification": "разговорный союз причины"
                            },
                            {
                                "target_lang_word": "теперь, когда",
                                "target_lang_sense_clarification": "временной и причинный союз"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "5d7cd375-3dce-3240-9c34-ee52a12f3d31",
                    "sense_definition": "Een uitroep om de aandacht te vestigen op wat volgt of om een conclusie in te leiden.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "interjection_intro",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Nu</w>, wat heb je uiteindelijk kunnen vinden in de bibliotheek?",
                            "target_lang_translations": {
                                "en": "<w>Well</w>, what did you eventually manage to find in the library?",
                                "ru": "<w>Ну</w>, что ты в итоге смог найти в библиотеке?"
                            }
                        },
                        {
                            "text": "<w>Nu</w>, laten we dan maar eens gaan kijken wat er gebeurd is.",
                            "target_lang_translations": {
                                "en": "<w>Well</w>, let's go and see what happened then.",
                                "ru": "<w>Итак</w>, давай посмотрим, что произошло."
                            }
                        }
                    ],
                    "synonyms": [
                        "welnu"
                    ],
                    "common_phrases": [
                        "nu ja",
                        "nu goed"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation to draw attention to what follows or to introduce a conclusion.",
                        "ru": "Восклицание для привлечения внимания к тому, что следует далее, или для введения вывода."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "well",
                                "target_lang_sense_clarification": "introductory particle"
                            },
                            {
                                "target_lang_word": "now",
                                "target_lang_sense_clarification": "introductory exclamation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "вводное слово"
                            },
                            {
                                "target_lang_word": "итак",
                                "target_lang_sense_clarification": "подведение итога"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bccb2600-cc2a-31da-a98b-7fde7b974bff",
                    "sense_definition": "De dertiende letter van het Griekse alfabet.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "greek_letter",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "In de natuurkunde wordt de Griekse letter <w>nu</w> vaak gebruikt als symbool.",
                            "target_lang_translations": {
                                "en": "In physics, the Greek letter <w>nu</w> is often used as a symbol.",
                                "ru": "В физике греческая буква <w>ню</w> часто используется как символ."
                            }
                        },
                        {
                            "text": "De vorm van de kleine letter <w>nu</w> lijkt sterk op de Latijnse v.",
                            "target_lang_translations": {
                                "en": "The shape of the lowercase letter <w>nu</w> looks very much like the Latin v.",
                                "ru": "Форма строчной буквы <w>ню</w> сильно напоминает латинскую v."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The thirteenth letter of the Greek alphabet.",
                        "ru": "Тринадцатая буква греческого алфавита."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nu",
                                "target_lang_sense_clarification": "Greek letter"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ню",
                                "target_lang_sense_clarification": "греческая буква"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "61c9b0dd-f6e5-3370-af42-a764d7beed61",
                    "sense_definition": "Zich verzetten tegen een idee, plan of persoon; weerstand bieden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "opposition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bewoners <w>kantten</w> zich fel tegen de bouw van de nieuwe snelweg.",
                            "target_lang_translations": {
                                "en": "The residents fiercely <w>opposed</w> the construction of the new highway."
                            }
                        },
                        {
                            "text": "Hij <w>kant</w> zich tegen elke vorm van discriminatie op de werkvloer.",
                            "target_lang_translations": {

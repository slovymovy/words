    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ef5f3f5-e30b-3204-9a7e-2b6b6095a906",
                    "sense_definition": "Een teken of merk dat aangeeft dat iemand schande heeft begaan of in ongenade is gevallen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In vroegere tijden kregen misdadigers soms letterlijk een <w>schandmerk</w>.",
                            "target_lang_translations": {
                                "ru": "В прежние времена преступникам иногда буквально ставили <w>клеймо</w>."
                            }
                        },
                        {
                            "text": "Hij droeg zijn ontslag als een onzichtbaar <w>schandmerk</w> met zich mee.",
                            "target_lang_translations": {
                                "ru": "Он нес свое увольнение как невидимое <w>клеймо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "brandmerk",
                        "stigma",
                        "smet"
                    ],
                    "antonyms": [
                        "ereteken"
                    ],
                    "common_phrases": [
                        "een schandmerk dragen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знак или метка, указывающая на то, что кто-то совершил постыдный поступок или впал в немилость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клеймо",
                                "target_lang_sense_clarification": "знак позора"
                            },
                            {
                                "target_lang_word": "пятно позора",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "стигма",
                                "target_lang_sense_clarification": "книжное/социальное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "16a96b45-e559-3fc6-9302-c8968cea7670",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>schandmerken</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>schandmerk</w> niemand zonder dat er harde bewijzen zijn.",
                            "target_lang_translations": {
                                "ru": "Я никого не <w>клеймлю</w> без веских доказательств."
                            }
                        },
                        {
                            "text": "Als ik hem publiekelijk beschuldig, <w>schandmerk</w> ik hem voor het leven.",
                            "target_lang_translations": {
                                "ru": "Если я публично обвиню его, я <w>заклеймлю</w> его на всю жизнь."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола 'schandmerken' (клеймить, позорить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клеймить",
                                "target_lang_sense_clarification": "подвергать позору"
                            },
                            {
                                "target_lang_word": "позорить",
                                "target_lang_sense_clarification": "выставлять на позор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "928f8bca-bbd1-3d23-9f09-c8139d8fd47f",
                    "sense_definition": "Gebiedende wijs van <w>schandmerken</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Schandmerk</w> deze mensen niet, maar probeer hen te helpen.",
                            "target_lang_translations": {
                                "ru": "Не <w>клейми</w> этих людей, а попытайся им помочь."
                            }
                        },
                        {
                            "text": "<w>Schandmerk</w> nooit iemand op basis van geruchten.",
                            "target_lang_translations": {
                                "ru": "Никогда не <w>клейми</w> кого-либо на основании слухов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола 'schandmerken' (клеймить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клеймить",
                                "target_lang_sense_clarification": "призыв не ставить клеймо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8ecbe5f-f15e-3b2a-b461-34d54011d099",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>schandmerken</w> (bij inversie).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waarom <w>schandmerk</w> je hem terwijl hij onschuldig is?",
                            "target_lang_translations": {
                                "ru": "Почему ты <w>клеймишь</w> его, хотя он невиновен?"
                            }
                        },
                        {
                            "text": "<w>Schandmerk</w> je de daders of zoek je naar een oplossing?",
                            "target_lang_translations": {
                                "ru": "Ты <w>клеймишь</w> виновных или ищешь решение?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма второго лица единственного числа настоящего времени от глагола 'schandmerken' (используется при инверсии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клеймить",
                                "target_lang_sense_clarification": "действие по отношению к другому"
                            }
                        ]
                    }
                }
            ]
        }
    ]

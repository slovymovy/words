{
    "word_family": [
        "odkładać",
        "odkład"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "946e945c-d51d-3874-a7a9-6fdc9552810a",
                    "sense_definition": "Umieścić przedmiot w określonym miejscu lub odsunąć go na bok, gdy nie jest już potrzebny.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <w>odłożyć</w> tę książkę na półkę po przeczytaniu.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>положите</w> эту книгу на полку после прочтения."
                            }
                        },
                        {
                            "text": "Muszę <w>odłożyć</w> te dokumenty na miejsce, zanim wyjdę z biura.",
                            "target_lang_translations": {
                                "ru": "Я должен <w>положить</w> эти документы на место, прежде чем уйду из офиса."
                            }
                        }
                    ],
                    "synonyms": [
                        "odwlec się",
                        "odciągnąć się"
                    ],
                    "antonyms": [
                        "wziąć",
                        "podnieść"
                    ],
                    "common_phrases": [
                        "odłożyć na miejsce",
                        "odłożyć na półkę",
                        "odłożyć na bok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить предмет в определенное место или убрать его в сторону, когда он больше не нужен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положи́ть",
                                "target_lang_sense_clarification": "поместить на место"
                            },
                            {
                                "target_lang_word": "отложи́ть",
                                "target_lang_sense_clarification": "убрать в сторону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00e33de9-cf74-3cdc-b9a3-090a64f41ae7",
                    "sense_definition": "Zakończyć rozmowę telefoniczną poprzez przerwanie połączenia.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "telephony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Byłam tak zdenerwowana, że musiałam natychmiast <w>odłożyć</w> słuchawkę.",
                            "target_lang_translations": {
                                "ru": "Я так нервничала, что мне пришлось немедленно <w>положить</w> трубку."
                            }
                        },
                        {
                            "text": "Nie zdążyłem nic powiedzieć, bo on zdążył już <w>odłożyć</w> słuchawkę.",
                            "target_lang_translations": {
                                "ru": "Я не успел ничего сказать, так как он уже <w>положил</w> трубку."
                            }
                        }
                    ],
                    "synonyms": [
                        "spóźnić się"
                    ],
                    "antonyms": [
                        "odebrać"
                    ],
                    "common_phrases": [
                        "odłożyć słuchawkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Закончить телефонный разговор, повесив трубку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положи́ть",
                                "target_lang_sense_clarification": "трубку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80304b95-95ab-3cb9-9647-bd52adffe6e9",
                    "sense_definition": "Przesunąć zaplanowane działanie lub wydarzenie na późniejszy termin.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_management",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musieliśmy <w>odłożyć</w> nasze wakacje z powodu nagłej choroby.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <w>отложить</w> наш отпуск из-за внезапной болезни."
                            }
                        },
                        {
                            "text": "Lepiej nie <w>odłożyć</w> tej ważnej decyzji na ostatnią chwilę.",
                            "target_lang_translations": {
                                "ru": "Лучше не <w>откладывать</w> это важное решение на последний момент."
                            }
                        }
                    ],
                    "synonyms": [
                        "opóźnić się",
                        "odwlec się"
                    ],
                    "antonyms": [
                        "przyspieszyć"
                    ],
                    "common_phrases": [
                        "odłożyć na później",
                        "odłożyć spotkanie",
                        "odłożyć decyzję",
                        "odłożyć wizytę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перенести запланированное действие или событие на более поздний срок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отложи́ть",
                                "target_lang_sense_clarification": "перенести на потом"
                            },
                            {
                                "target_lang_word": "перенести́",
                                "target_lang_sense_clarification": "изменить дату"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25e279a0-0cc3-35d6-88b4-62ad033a9f06",
                    "sense_definition": "Zachować część pieniędzy, aby wykorzystać je w przyszłości; zaoszczędzić.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Staram się co miesiąc <w>odłożyć</w> niewielką sumę na nowe mieszkanie.",
                            "target_lang_translations": {
                                "ru": "Я стараюсь каждый месяц <w>откладывать</w> небольшую сумму на новую квартиру."
                            }
                        },
                        {
                            "text": "Udało nam się <w>odłożyć</w> wystarczająco dużo pieniędzy na podróż.",
                            "target_lang_translations": {
                                "ru": "Нам удалось <w>отложить</w> достаточно денег на поездку."
                            }
                        }
                    ],
                    "antonyms": [
                        "wydać"
                    ],
                    "common_phrases": [
                        "odłożyć pieniądze",
                        "odłożyć na czarną godzinę",
                        "odłożyć grosz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отложить часть денег для использования в будущем; сэкономить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отложи́ть",
                                "target_lang_sense_clarification": "деньги, сбережения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51d9c52f-590e-3622-ab32-541a012e74dc",
                    "sense_definition": "Nagromadzić się w jednym miejscu, tworząc warstwę (zwykle o tłuszczu, osadzie lub substancjach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nadmiar kalorii może szybko <w>odłożyć</w> się w postaci tkanki tłuszczowej.",
                            "target_lang_translations": {
                                "ru": "Избыток калорий может быстро <w>отложиться</w> в виде жировой ткани."
                            }
                        },
                        {
                            "text": "W starych rurach może <w>odłożyć</w> się kamień i zablokować przepływ.",
                            "target_lang_translations": {
                                "ru": "В старых трубах может <w>отложиться</w> накипь и заблокировать поток."
                            }
                        }
                    ],
                    "synonyms": [
                        "odcisnąć się"
                    ],
                    "common_phrases": [
                        "odłożyć się w biodrach",
                        "odłożyć się na dnie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "used reflexively with 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Накопиться в одном месте, образуя слой (обычно о жире, осадке или веществах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отложи́ться",
                                "target_lang_sense_clarification": "накопиться"
                            },
                            {
                                "target_lang_word": "осе́сть",
                                "target_lang_sense_clarification": "об осадке"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
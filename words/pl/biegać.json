{
    "word_family": [
        "bieg",
        "biegacz",
        "biegowy",
        "wybieg",
        "przebieg",
        "zabieg",
        "obieg",
        "zbieg"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "773b80bd-f525-3c24-9ee7-9c98cf8ff2ed",
                    "sense_definition": "Poruszać się szybko, odrywając stopy od ziemi; przemieszczać się szybciej niż idąc.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci uwielbiają <biegać> boso po trawie w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Дети обожают <бегать> босиком по траве в саду."
                            }
                        },
                        {
                            "text": "Muszę szybko <biegać>, żeby zdążyć na ostatni autobus.",
                            "target_lang_translations": {
                                "ru": "Мне нужно быстро <бежать>, чтобы успеть на последний автобус."
                            }
                        },
                        {
                            "text": "Psy <biegały> po podwórku i głośno szczekały na przechodniów.",
                            "target_lang_translations": {
                                "ru": "Собаки <бегали> по двору и громко лаяли на прохожих."
                            }
                        }
                    ],
                    "synonyms": [
                        "latać",
                        "ganiać"
                    ],
                    "antonyms": [
                        "chodzić",
                        "stać"
                    ],
                    "common_phrases": [
                        "biegać boso",
                        "szybko biegać",
                        "biegać po podwórku",
                        "biegać dookoła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро передвигаться, отрывая ноги от земли; перемещаться быстрее, чем шагом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бегать",
                                "target_lang_sense_clarification": "перемещаться в разных направлениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66a582a9-385f-3c62-9f17-93c4a2fff098",
                    "sense_definition": "Uprawiać sport polegający na biegu; trenować bieganie dla zdrowia lub zawodowo.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat planuje <biegać> w maratonie w przyszłym roku.",
                            "target_lang_translations": {
                                "ru": "Мой брат планирует <бежать> марафон в следующем году."
                            }
                        },
                        {
                            "text": "Codziennie rano <biegam> przez godzinę, żeby utrzymać dobrą formę.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я <бегаю> по часу, чтобы поддерживать хорошую форму."
                            }
                        }
                    ],
                    "common_phrases": [
                        "biegać maratony",
                        "biegać rano",
                        "biegać dla zdrowia",
                        "biegać na nartach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заниматься бегом как спортом; тренироваться для здоровья или профессионально."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бегать",
                                "target_lang_sense_clarification": "заниматься спортом"
                            },
                            {
                                "target_lang_word": "заниматься бегом",
                                "target_lang_sense_clarification": "тренироваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bf27f29-1132-34b9-8cf8-96bc1382ded9",
                    "sense_definition": "Chodzić w wiele miejsc, aby załatwić różne sprawy; być bardzo zajętym i w ciągłym ruchu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cały dzień musiałem <biegać> po urzędach, żeby odebrać dokumenty.",
                            "target_lang_translations": {
                                "ru": "Весь день мне пришлось <бегать> по учреждениям, чтобы забрать документы."
                            }
                        },
                        {
                            "text": "Przed świętami wszyscy zaczynają <biegać> po sklepach w poszukiwaniu prezentów.",
                            "target_lang_translations": {
                                "ru": "Перед праздниками все начинают <бегать> по магазинам в поисках подарков."
                            }
                        }
                    ],
                    "synonyms": [
                        "latać",
                        "ganiać"
                    ],
                    "common_phrases": [
                        "biegać po sklepach",
                        "biegać po urzędach",
                        "biegać za sprawami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ходить во многие места, чтобы уладить дела; быть очень занятым и в постоянном движении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бегать",
                                "target_lang_sense_clarification": "хлопотать, суетиться"
                            },
                            {
                                "target_lang_word": "мотаться",
                                "target_lang_sense_clarification": "по делам (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "910f67a2-ebe8-33db-912f-a4b21ea6eaa0",
                    "sense_definition": "Podążać za kimś lub czymś, starając się to złapać, zdobyć lub osiągnąć.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "pursuit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie powinieneś <biegać> za dziewczynami, które cię nie szanują.",
                            "target_lang_translations": {
                                "ru": "Тебе не стоит <бегать> за девушками, которые тебя не уважают."
                            }
                        },
                        {
                            "text": "Wielu ludzi woli <biegać> za pieniędzmi niż cieszyć się życiem.",
                            "target_lang_translations": {
                                "ru": "Многие люди предпочитают <бегать> за деньгами, чем наслаждаться жизнью."
                            }
                        },
                        {
                            "text": "Kot zaczął <biegać> za myszą, która uciekła do kuchni.",
                            "target_lang_translations": {
                                "ru": "Кот начал <бегать> за мышью, которая убежала в кухню."
                            }
                        }
                    ],
                    "synonyms": [
                        "ganiać"
                    ],
                    "common_phrases": [
                        "biegać za piłką",
                        "biegać za pieniędzmi",
                        "biegać za kimś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следовать за кем-то или чем-то, пытаясь поймать, заполучить или достичь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бегать (за)",
                                "target_lang_sense_clarification": "преследовать, ухаживать"
                            },
                            {
                                "target_lang_word": "гоняться",
                                "target_lang_sense_clarification": "преследовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9c00503-7065-3dcf-81b1-bd4fe57db59a",
                    "sense_definition": "Często korzystać z toalety z powodu problemów żołądkowych (np. biegunki).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zjadłem nieświeżą rybę i całą noc musiałem <biegać> do łazienki.",
                            "target_lang_translations": {
                                "ru": "Я съел несвежую рыбу, и мне пришлось всю ночь <бегать> в ванную."
                            }
                        },
                        {
                            "text": "Boli go brzuch i ciągle <biega>, więc nie przyjdzie dziś do pracy.",
                            "target_lang_translations": {
                                "ru": "У него болит живот, и он постоянно <бегает>, так что сегодня не придет на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "latać"
                    ],
                    "common_phrases": [
                        "biegać do toalety",
                        "biegać do łazienki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "euphemism for diarrhea"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часто пользоваться туалетом из-за проблем с желудком (например, диареи)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бегать",
                                "target_lang_sense_clarification": "в туалет (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "032e31d6-2a0d-30b8-8edd-7c95899fc83b",
                    "sense_definition": "Poruszać się szybko i niespokojnie po jakiejś powierzchni (używane w odniesieniu do oczu, palców lub myśli).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej palce zaczęły szybko <biegać> po klawiaturze fortepianu.",
                            "target_lang_translations": {
                                "ru": "Её пальцы начали быстро <бегать> по клавиатуре фортепиано."
                            }
                        },
                        {
                            "text": "Jego oczy zaczęły nerwowo <biegać> po pokoju, jakby czegoś szukał.",
                            "target_lang_translations": {
                                "ru": "Его глаза начали нервно <бегать> по комнате, словно он что-то искал."
                            }
                        }
                    ],
                    "synonyms": [
                        "latać"
                    ],
                    "common_phrases": [
                        "oczy biegają",
                        "palce biegają",
                        "myśli biegają"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро и беспокойно перемещаться по поверхности (о глазах, пальцах или мыслях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бегать",
                                "target_lang_sense_clarification": "о глазах, пальцах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
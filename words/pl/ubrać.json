{
    "word_family": [
        "ubranie",
        "ubiór",
        "ubierać",
        "rozebrać",
        "przebrać",
        "dobrać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "66a72eaf-87e8-3536-9426-bbed2bc07289",
                    "sense_definition": "Założyć na siebie odzież (używane z zaimkiem zwrotnym \"się\").",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz się ciepło <ubrać>, ponieważ na zewnątrz pada śnieg.",
                            "target_lang_translations": {
                                "ru": "Ты должен тепло <одеться>, так как на улице идет снег."
                            }
                        },
                        {
                            "text": "Marek postanowił <ubrać> się w swój najlepszy garnitur na wesele.",
                            "target_lang_translations": {
                                "ru": "Марек решил <одеться> в свой лучший костюм на свадьбу."
                            }
                        }
                    ],
                    "antonyms": [
                        "rozebrać się"
                    ],
                    "common_phrases": [
                        "ubrać się ciepło",
                        "szybko się ubrać",
                        "ubrać się modnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надеть на себя одежду (используется с возвратным местоимением \"się\")."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одеться",
                                "target_lang_sense_clarification": "надеть одежду на себя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74d24532-633b-3850-861c-a36f205c4856",
                    "sense_definition": "Założyć na kogoś ubranie lub okryć kogoś czymś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mama musiała <ubrać> dziecko w kurtkę i czapkę przed wyjściem.",
                            "target_lang_translations": {
                                "ru": "Маме пришлось <одеть> ребенка в куртку и шапку перед выходом."
                            }
                        },
                        {
                            "text": "Pielęgniarka pomogła <ubrać> pacjenta po badaniu lekarskim.",
                            "target_lang_translations": {
                                "ru": "Медсестра помогла <одеть> пациента после медицинского осмотра."
                            }
                        }
                    ],
                    "synonyms": [
                        "odziać"
                    ],
                    "antonyms": [
                        "rozebrać"
                    ],
                    "common_phrases": [
                        "ubrać dziecko",
                        "ubrać lalkę",
                        "ubrać kogoś do ślubu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надеть одежду на кого-то или укрыть кого-то чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одеть",
                                "target_lang_sense_clarification": "кого-то в одежду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5547a285-bf33-348c-a911-5bbaf4232b18",
                    "sense_definition": "Ozdobić coś, nadać czemuś odświętny wygląd (najczęściej choinkę).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "decorating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W Wigilię cała rodzina zbiera się, aby wspólnie <ubrać> choinkę.",
                            "target_lang_translations": {
                                "ru": "В сочельник вся семья собирается, чтобы вместе <нарядить> елку."
                            }
                        },
                        {
                            "text": "Chcemy <ubrać> salę balonami na przyjęcie urodzinowe.",
                            "target_lang_translations": {
                                "ru": "Мы хотим <украсить> зал шарами на день рождения."
                            }
                        }
                    ],
                    "antonyms": [
                        "rozebrać"
                    ],
                    "common_phrases": [
                        "ubrać choinkę",
                        "ubrać w słowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Украсить что-то, придать чему-то праздничный вид (чаще всего елку)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нарядить",
                                "target_lang_sense_clarification": "елку"
                            },
                            {
                                "target_lang_word": "украсить",
                                "target_lang_sense_clarification": "помещение, зал"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
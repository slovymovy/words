    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "336e7072-3f77-3ff3-b186-13ac7b26155a",
                    "sense_definition": "Średniej wielkości ptak wędrowny o jaskrawym, turkusowo-niebieskim upierzeniu i rdzawym grzbiecie, żywiący się głównie owadami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "birds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Egzotycznie wyglądająca <w>kraska</w> jest w Polsce gatunkiem skrajnie nielicznym.",
                            "target_lang_translations": {
                                "ru": "Экзотически выглядящая <w>сизоворонка</w> является в Польше крайне редким видом."
                            }
                        },
                        {
                            "text": "Ornitolodzy podejmują działania, aby ochrona <w>kraski</w> była bardziej skuteczna.",
                            "target_lang_translations": {
                                "ru": "Орнитологи предпринимают действия, чтобы охрана <w>сизоворонки</w> была более эффективной."
                            }
                        }
                    ],
                    "synonyms": [
                        "kraska zwyczajna"
                    ],
                    "common_phrases": [
                        "ochrona kraski",
                        "populacja kraski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Среднего размера перелётная птица с ярким бирюзово-синим оперением и рыжей спиной, питающаяся в основном насекомыми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сизоворонка",
                                "target_lang_sense_clarification": "вид птицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40bf4662-9bfa-36ac-83fa-027822d61fe6",
                    "sense_definition": "Jajko wielkanocne pofarbowane na jeden kolor, bez wyskrobywanych wzorów.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "easter_eggs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W koszyku ze święconką leżała czerwona <w>kraska</w> przygotowana przez babcię.",
                            "target_lang_translations": {
                                "ru": "В корзинке с освящённой едой лежала красная <w>крашенка</w>, приготовленная бабушкой."
                            }
                        },
                        {
                            "text": "Tradycyjna <w>kraska</w> różni się od pisanki tym, że nie ma żadnych wzorów.",
                            "target_lang_translations": {
                                "ru": "Традиционная <w>крашенка</w> отличается от писанки тем, что на ней нет никаких узоров."
                            }
                        }
                    ],
                    "synonyms": [
                        "kraska zwyczajna"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Mazowsze"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "etnografia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пасхальное яйцо, окрашенное в один цвет, без выцарапанных узоров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крашенка",
                                "target_lang_sense_clarification": "пасхальное яйцо"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5eb89e49-1de3-336a-8b2e-715001b0a9fd",
                    "sense_definition": "A small cupboard or compartment with a lock, usually used to store personal possessions in schools, gyms, or workplaces.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "storage_compartment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She placed her books in her <w>locker</w> before the first class started.",
                            "target_lang_translations": {
                                "ru": "Она положила книги в свой <w>шкафчик</w> перед началом первого урока."
                            }
                        },
                        {
                            "text": "The players went back to the <w>locker</w> room to change after the game.",
                            "target_lang_translations": {
                                "ru": "Игроки вернулись в <w>раздевалку</w>, чтобы переодеться после игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "footlocker"
                    ],
                    "common_phrases": [
                        "locker room",
                        "gym locker",
                        "school locker",
                        "storage locker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой шкаф или отделение с замком, обычно используемое для хранения личных вещей в школах, спортзалах или на работе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шкафчик",
                                "target_lang_sense_clarification": "для хранения вещей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b438581a-4395-3de6-aae6-63e5770841bd",
                    "sense_definition": "A storage compartment or chest on a ship.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "storage_compartment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sailor stowed the ropes in the deck <w>locker</w> to keep them dry.",
                            "target_lang_translations": {
                                "ru": "Матрос убрал канаты в палубный <w>рундук</w>, чтобы они оставались сухими."
                            }
                        },
                        {
                            "text": "According to the legend, the lost treasure is in Davy Jones's <w>locker</w>.",
                            "target_lang_translations": {
                                "ru": "Согласно легенде, потерянное сокровище находится в <w>сундуке</w> Дэйви Джонса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Davy Jones's locker",
                        "chain locker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ящик или ларь для хранения вещей на судне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рундук",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2ba6f1a-6a80-339d-8c70-4acb9a54e595",
                    "sense_definition": "One who locks something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was the designated <w>locker</w> of the main gate every evening.",
                            "target_lang_translations": {
                                "ru": "Он был назначенным <w>запирающим</w> главных ворот каждый вечер."
                            }
                        },
                        {
                            "text": "The <w>locker</w> must ensure the mechanism is fully engaged before leaving.",
                            "target_lang_translations": {
                                "ru": "<w>Запирающий</w> должен убедиться, что механизм полностью задействован, прежде чем уйти."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто запирает что-либо."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {

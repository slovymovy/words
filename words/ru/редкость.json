            "senses": [
                {
                    "sense_id": "d7ea8932-5c80-3ef3-b33c-d2a732fcab63",
                    "sense_definition": "Свойство по значению прилагательного <w>редкий</w>; то, что встречается нечасто или в малом количестве.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В наши дни настоящая доброта — это большая <w>редкость</w>.",
                            "target_lang_translations": {
                                "en": "True kindness is a great <w>rarity</w> these days."
                            }
                        },
                        {
                            "text": "Погода стояла на <w>редкость</w> тёплая и солнечная для этого времени года.",
                            "target_lang_translations": {
                                "en": "The weather was <w>exceptionally</w> warm and sunny for this time of year."
                            }
                        }
                    ],
                    "antonyms": [
                        "частота",
                        "обыденность",
                        "обыкновенность"
                    ],
                    "common_phrases": [
                        "большая редкость",
                        "на редкость"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality or state of being rare; something that happens or is found very infrequently."
                    },
                    "translations": {
                        "en": [
                            {

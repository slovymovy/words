    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d5e2780f-c677-3f2b-b14b-35817bbb1842",
                    "sense_definition": "Naciśnięcie przycisku myszy komputerowej lub podobnego urządzenia wskazującego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "computer_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wystarczy jeden <klik>, aby otworzyć wybrany program.",
                            "target_lang_translations": {
                                "ru": "Достаточно одного <w>клика</w>, чтобы открыть выбранную программу."
                            }
                        },
                        {
                            "text": "Ten <klik> myszką był zupełnie przypadkowy.",
                            "target_lang_translations": {
                                "ru": "Этот <w>клик</w> мышкой был совершенно случайным."
                            }
                        }
                    ],
                    "synonyms": [
                        "kliknięcie"
                    ],
                    "common_phrases": [
                        "jeden klik",
                        "podwójny klik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нажатие кнопки компьютерной мыши или аналогичного указывающего устройства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клик",
                                "target_lang_sense_clarification": "компьютерный термин"
                            },
                            {
                                "target_lang_word": "щелчок",
                                "target_lang_sense_clarification": "действие мышью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5429aba-ad80-35d0-b2d5-f8727597f902",
                    "sense_definition": "Pojedyncze wejście na stronę internetową lub w link reklamowy, używane jako miara statystyczna.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "computer_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Reklamodawca płaci za każdy <klik> w baner na stronie.",
                            "target_lang_translations": {
                                "ru": "Рекламодатель платит за каждый <w>клик</w> по баннеру на сайте."
                            }
                        },
                        {
                            "text": "Liczba <klik>ów w ten artykuł przerosła nasze oczekiwania.",
                            "target_lang_translations": {
                                "ru": "Количество <w>кликов</w> по этой статье превзошло наши ожидания."
                            }
                        }
                    ],
                    "synonyms": [
                        "kliknięcie"
                    ],
                    "common_phrases": [
                        "koszt za klik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единичный переход на веб-страницу или по рекламной ссылке, используемый как статистическая мера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клик",
                                "target_lang_sense_clarification": "интернет-маркетинг"
                            },
                            {
                                "target_lang_word": "переход",
                                "target_lang_sense_clarification": "статистика посещений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d7de215-88af-3a04-9740-ae7abb90b4d2",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>klika</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej firmie jest wiele zamkniętych <klik>, do których trudno przeniknąć.",
                            "target_lang_translations": {
                                "ru": "В этой фирме много закрытых <w>клик</w>, в которые трудно проникнуть."
                            }
                        },
                        {
                            "text": "Unikam towarzyskich <klik>, które zajmują się głównie plotkowaniem.",
                            "target_lang_translations": {
                                "ru": "Я избегаю светских <w>клик</w>, которые занимаются в основном сплетнями."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "dopełniacz liczby mnogiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного «klika» (клика)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клик",
                                "target_lang_sense_clarification": "форма слова «клика»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "250379a8-cd1e-3083-acc3-6e9cd3557a6e",
                    "sense_definition": "Krótki, suchy dźwięk przypominający trzask lub odgłos przełącznika.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Usłyszałem cichy <klik> zamka w drzwiach wejściowych.",
                            "target_lang_translations": {
                                "ru": "Я услышал тихий <w>щелчок</w> замка во входной двери."
                            }
                        },
                        {
                            "text": "Mechanizm zadziałał i rozległ się wyraźny <klik>.",
                            "target_lang_translations": {
                                "ru": "Механизм сработал, и раздался отчетливый <w>щелчок</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий сухой звук, напоминающий треск или звук переключателя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щелчок",
                                "target_lang_sense_clarification": "звук"
                            },
                            {
                                "target_lang_word": "клацанье",
                                "target_lang_sense_clarification": "звук механизма"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "26d7a2a4-dfa3-3719-b33b-9a57f7fb8c85",
                    "sense_definition": "Słowo naśladujące krótki, suchy dźwięk, np. przełącznika, aparatu lub myszy komputerowej.",
                    "learner_level": "A1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_intj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Klik>, i zdjęcie zostało zrobione w idealnym momencie.",
                            "target_lang_translations": {
                                "ru": "<w>Щёлк</w> — и фотография была сделана в идеальный момент."
                            }
                        },
                        {
                            "text": "Zrobił <klik> pilotem i brama garażowa się otworzyła.",
                            "target_lang_translations": {
                                "ru": "Он сделал <w>щёлк</w> пультом, и гаражные ворота открылись."
                            }
                        }
                    ],
                    "common_phrases": [
                        "klik i gotowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звукоподражание короткому сухому звуку, например, переключателя, фотоаппарата или компьютерной мыши."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щёлк",
                                "target_lang_sense_clarification": "звукоподражание"
                            },
                            {
                                "target_lang_word": "клац",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8748bc33-ae65-36d4-a3d8-9da0e26c95fe",
                    "sense_definition": "Wydzielina z nosa.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko wytarło rękawem <klik> wiszący pod nosem.",
                            "target_lang_translations": {
                                "ru": "Ребенок вытер рукавом <w>соплю</w>, висящую под носом."
                            }
                        },
                        {
                            "text": "Masz <klik> na twarzy, weź chusteczkę.",
                            "target_lang_translations": {
                                "ru": "У тебя <w>сопля</w> на лице, возьми салфетку."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Radomsko"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделения из носа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопля",
                                "target_lang_sense_clarification": "разговорное/грубое"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
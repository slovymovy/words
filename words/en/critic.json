    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f26c421e-65fb-375a-b8ea-31c7ea1fd7e0",
                    "sense_definition": "A person whose job is to give an opinion about the quality of books, movies, music, or other forms of art.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new play was praised by almost every theater <w>critic</w> in the city.",
                            "target_lang_translations": {
                                "ru": "Новую пьесу похвалил почти каждый театральный <w>критик</w> в городе."
                            }
                        },
                        {
                            "text": "As a food <w>critic</w>, she visits different restaurants every week to taste their signature dishes.",
                            "target_lang_translations": {
                                "ru": "Как ресторанный <w>критик</w>, она каждую неделю посещает разные рестораны, чтобы попробовать их фирменные блюда."
                            }
                        }
                    ],
                    "synonyms": [
                        "reviewer",
                        "commentator",
                        "pundit",
                        "analyst",
                        "judge"
                    ],
                    "common_phrases": [
                        "film critic",
                        "literary critic",
                        "art critic",
                        "food critic",
                        "music critic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, чья работа заключается в оценке качества книг, фильмов, музыки или других видов искусства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "критик",
                                "target_lang_sense_clarification": "профессиональный обозреватель"
                            },
                            {
                                "target_lang_word": "рецензент",
                                "target_lang_sense_clarification": "автор рецензий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a6141c8-8569-35a0-ab53-76c5fd4293b4",
                    "sense_definition": "A person who expresses strong disapproval of someone or something and points out their faults.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has been a vocal <w>critic</w> of the government's economic policies for many years.",
                            "target_lang_translations": {
                                "ru": "Он много лет был ярым <w>критиком</w> экономической политики правительства."
                            }
                        },
                        {
                            "text": "Even her harshest <w>critic</w> had to admit that she performed well under pressure.",
                            "target_lang_translations": {
                                "ru": "Даже её самый суровый <w>критик</w> был вынужден признать, что она хорошо справилась под давлением."
                            }
                        }
                    ],
                    "synonyms": [
                        "detractor",
                        "faultfinder",
                        "attacker",
                        "accuser"
                    ],
                    "antonyms": [
                        "supporter",
                        "advocate",
                        "defender",
                        "proponent",
                        "champion"
                    ],
                    "common_phrases": [
                        "harsh critic",
                        "vocal critic",
                        "fierce critic",
                        "outspoken critic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который выражает решительное неодобрение кого-либо или чего-либо и указывает на недостатки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "критик",
                                "target_lang_sense_clarification": "тот, кто выражает неодобрение"
                            },
                            {
                                "target_lang_word": "критикан",
                                "target_lang_sense_clarification": "придирчивый человек"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
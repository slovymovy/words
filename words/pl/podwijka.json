    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f8c781d3-6178-347c-9210-0a2895e520a2",
                    "sense_definition": "Dawne nakrycie głowy mężatek, osłaniające szyję i policzki; rzadsza forma słowa <w>podwika</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "head_covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na starym portrecie szlachcianka ma na głowie białą <w>podwijkę</w>.",
                            "target_lang_translations": {
                                "ru": "На старинном портрете у дворянки на голове белый <w>повойник</w>."
                            }
                        },
                        {
                            "text": "Kobieta poprawiła lnianą <w>podwijkę</w>, która zsunęła się jej na ramię.",
                            "target_lang_translations": {
                                "ru": "Женщина поправила льняную <w>намитку</w>, которая сползла ей на плечо."
                            }
                        }
                    ],
                    "synonyms": [
                        "podwika",
                        "kwef",
                        "namitka"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo używane w kontekście historycznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинный головной убор замужних женщин, закрывающий шею и щеки; более редкая форма слова podwika."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повойник",
                                "target_lang_sense_clarification": "старинный головной убор"
                            },
                            {
                                "target_lang_word": "намитка",
                                "target_lang_sense_clarification": "головной убор из ткани"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82cedfbd-2960-3646-8b73-4a89f776d0bd",
                    "sense_definition": "Rodzaj fałdowanej spódnicy kobiecej, noszonej w dawnych czasach.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing_skirt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do tańca dziewczyna założyła szeroką, wełnianą <w>podwijkę</w>.",
                            "target_lang_translations": {
                                "ru": "Для танца девушка надела широкую шерстяную <w>юбку</w>."
                            }
                        },
                        {
                            "text": "Jej kolorowa <w>podwijka</w> szeleściła przy każdym kroku.",
                            "target_lang_translations": {
                                "ru": "Ее цветная <w>юбка</w> шелестела при каждом шаге."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "określenie historyczne lub regionalne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность женской юбки в складку, которую носили в старину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юбка",
                                "target_lang_sense_clarification": "старинная, широкая"
                            }
                        ]
                    }
                }
            ]
        }
    ]

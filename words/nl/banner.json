            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4dfb7f2d-1c41-366d-9864-444596dd2e90",
                    "sense_definition": "Een advertentie op een website, meestal in de vorm van een rechthoekige afbeelding of animatie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb een programma geïnstalleerd dat elke <w>banner</w> op websites blokkeert.",
                            "target_lang_translations": {
                                "ru": "Я установил программу, которая блокирует каждый <w>баннер</w> на веб-сайтах."
                            }
                        },
                        {
                            "text": "De kleurrijke <w>banner</w> trok direct mijn aandacht toen ik de site opende.",
                            "target_lang_translations": {
                                "ru": "Красочный <w>баннер</w> сразу привлек мое внимание, когда я открыл сайт."
                            }
                        }
                    ],
                    "synonyms": [
                        "webbanner",
                        "advertentiebanner"
                    ],
                    "common_phrases": [
                        "op een banner klikken",
                        "een banner blokkeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Реклама на веб-сайте, обычно в виде прямоугольного изображения или анимации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баннер",
                                "target_lang_sense_clarification": "интернет-реклама"
                            },
                            {
                                "target_lang_word": "веб-баннер",
                                "target_lang_sense_clarification": "реклама на сайте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d08ac8c4-1bee-32a3-92e4-5feef120fd81",
                    "sense_definition": "Een doek of strook materiaal met tekst of afbeeldingen, vaak gebruikt voor reclame of aankondigingen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij de ingang van de beurs stond een grote <w>banner</w> met het logo van het bedrijf.",
                            "target_lang_translations": {
                                "ru": "У входа на выставку стоял большой <w>баннер</w> с логотипом компании."
                            }
                        },
                        {
                            "text": "We hebben een <w>banner</w> laten drukken voor het feest van volgende week.",
                            "target_lang_translations": {
                                "ru": "Мы заказали печать <w>баннера</w> для вечеринки на следующей неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "spandoek",
                        "reclamedoek"
                    ],
                    "common_phrases": [
                        "een banner ophangen",
                        "een roll-up banner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полотно или полоса материала с текстом или изображениями, часто используемая для рекламы или объявлений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баннер",
                                "target_lang_sense_clarification": "наружная реклама, стенд"
                            },
                            {
                                "target_lang_word": "растяжка",
                                "target_lang_sense_clarification": "рекламное полотно"
                            }
                        ]
                    }
                }
            ]
        }

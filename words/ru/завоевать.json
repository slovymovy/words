    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bec7d08e-4ff1-3517-b957-205093bef33d",
                    "sense_definition": "Захватить силой оружия, подчинить своей власти территорию или население",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Император мечтал <w>завоевать</w> весь мир и создать огромную империю.",
                            "target_lang_translations": {
                                "en": "The emperor dreamed of <w>conquering</w> the whole world and creating a huge empire."
                            }
                        },
                        {
                            "text": "Армии удалось <w>завоевать</w> город после долгой и тяжёлой осады.",
                            "target_lang_translations": {
                                "en": "The army managed to <w>conquer</w> the city after a long and difficult siege."
                            }
                        }
                    ],
                    "synonyms": [
                        "захватить",
                        "покорить",
                        "оккупировать"
                    ],
                    "antonyms": [
                        "освободить",
                        "сдать",
                        "потерять"
                    ],
                    "common_phrases": [
                        "завоевать город",
                        "завоевать страну",
                        "завоевать мир",
                        "завоевать территорию"
                    ],
                    "target_lang_definitions": {
                        "en": "To seize by force of arms, to subjugate a territory or population to one's power"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conquer",
                                "target_lang_sense_clarification": "to take control of a country or city by force"
                            },
                            {
                                "target_lang_word": "capture",
                                "target_lang_sense_clarification": "to take possession of a place by force"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6be47bcc-7a78-3c7d-8d14-fab89687b5b6",
                    "sense_definition": "Добиться чего-либо, получить что-либо значимое, прилагая усилия (уважение, награду, любовь)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спортсмен упорно тренировался, чтобы <w>завоевать</w> золотую медаль на соревнованиях.",
                            "target_lang_translations": {
                                "en": "The athlete trained hard to <w>win</w> the gold medal at the competition."
                            }
                        },
                        {
                            "text": "Ему потребовалось много времени, чтобы <w>завоевать</w> доверие своих новых коллег.",
                            "target_lang_translations": {
                                "en": "It took him a long time to <w>win</w> the trust of his new colleagues."
                            }
                        }
                    ],
                    "synonyms": [
                        "добиться",
                        "заслужить",
                        "выиграть",
                        "получить"
                    ],
                    "antonyms": [
                        "потерять",
                        "утратить"
                    ],
                    "common_phrases": [
                        "завоевать доверие",
                        "завоевать авторитет",
                        "завоевать сердце",
                        "завоевать медаль",
                        "завоевать популярность"
                    ],
                    "target_lang_definitions": {
                        "en": "To achieve something, to obtain something significant through effort (respect, reward, love)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "win",
                                "target_lang_sense_clarification": "to obtain something desired (prize, affection, trust) by effort"
                            },
                            {
                                "target_lang_word": "gain",
                                "target_lang_sense_clarification": "to acquire or secure (respect, popularity)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aef8e9e6-018e-32cb-bb5d-608801da22e2",
                    "sense_definition": "Potoczne określenie osób lub grup społecznych zachowujących się w sposób zdemoralizowany, wulgarny, agresywny lub nadużywających alkoholu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod sklepem znowu zebrała się miejscowa <w>patologia</w>.",
                            "target_lang_translations": {
                                "ru": "У магазина снова собрался местный <w>сброд</w>."
                            }
                        },
                        {
                            "text": "Mieszkańcy skarżą się na hałas i awantury, które urządza osiedlowa <w>patologia</w>.",
                            "target_lang_translations": {
                                "ru": "Жители жалуются на шум и скандалы, которые устраивают местные <w>маргиналы</w>."
                            }

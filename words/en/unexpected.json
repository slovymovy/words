    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "79ca44b9-23e9-3d23-b927-fa503213c0e7",
                    "sense_definition": "Not expected, anticipated, or foreseen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The project presented <w>unexpected</w> difficulties for the team.",
                            "target_lang_translations": {
                                "ru": "Проект создал <w>непредвиденные</w> трудности для команды."
                            }
                        },
                        {
                            "text": "We faced an <w>unexpected</w> delay at the airport due to the storm.",
                            "target_lang_translations": {
                                "ru": "Мы столкнулись с <w>неожиданной</w> задержкой в аэропорту из-за шторма."
                            }
                        }
                    ],
                    "synonyms": [
                        "surprising"
                    ],
                    "antonyms": [
                        "expected"
                    ],
                    "common_phrases": [
                        "unexpected result",
                        "unexpected guest",
                        "unexpected delay",
                        "totally unexpected"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не ожидаемый, не предвиденный или внезапный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неожиданный",
                                "target_lang_sense_clarification": "случившийся внезапно, вопреки ожиданиям"
                            },
                            {
                                "target_lang_word": "непредвиденный",
                                "target_lang_sense_clarification": "не запланированный или не предсказанный заранее"
                            },
                            {
                                "target_lang_word": "нежданный",
                                "target_lang_sense_clarification": "которого не ждали (часто о гостях)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "404a5205-9280-3c7e-bb39-21d386c367f8",
                    "sense_definition": "Someone or something that is not expected.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should always try to prepare for the <w>unexpected</w>.",
                            "target_lang_translations": {
                                "ru": "Всегда следует пытаться подготовиться к <w>неожиданному</w>."
                            }
                        },
                        {
                            "text": "Life is full of the <w>unexpected</w>, so we must be adaptable.",
                            "target_lang_translations": {
                                "ru": "Жизнь полна <w>неожиданностей</w>, поэтому мы должны уметь приспосабливаться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "expect the unexpected",
                        "the unexpected"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то или кто-то, кого не ожидали; неожиданное событие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неожиданность",
                                "target_lang_sense_clarification": "внезапное событие или обстоятельство"
                            },
                            {
                                "target_lang_word": "неожиданное",
                                "target_lang_sense_clarification": "то, чего не ждали (субстантивированное прилагательное)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

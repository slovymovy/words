                    "common_phrases": [
                        "калечный вид",
                        "калечный человек",
                        "калечный солдат"
                    ],
                    "target_lang_definitions": {
                        "en": "Having physical injuries, traumatized; relating to a person with physical disabilities."
                    },
                    "translations": {
                        "en": [

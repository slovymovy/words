            "pos": "noun",
            "senses": [
                {
                    "sense_id": "73f47866-0906-3f29-9e27-58eafb787e6f",
                    "sense_definition": "Een plotselinge verandering of overgang van de ene situatie, keuze of instelling naar een andere.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "change_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij maakte een grote <w>switch</w> in zijn carrière door leraar te worden.",
                            "target_lang_translations": {
                                "en": "He made a big <w>switch</w> in his career by becoming a teacher.",
                                "ru": "Он совершил серьезный <w>переход</w> в своей карьере, став учителем."
                            }
                        },
                        {
                            "text": "De <w>switch</w> van zomer- naar wintertijd gebeurt dit weekend.",
                            "target_lang_translations": {
                                "en": "The <w>switch</w> from summer to winter time happens this weekend.",
                                "ru": "<w>Переход</w> с летнего на зимнее время происходит в эти выходные."
                            }
                        }
                    ],
                    "synonyms": [
                        "verandering",
                        "omschakeling",
                        "overgang",
                        "omzetting"
                    ],
                    "common_phrases": [
                        "een switch maken",
                        "een snelle switch"
                    ],
                    "target_lang_definitions": {
                        "en": "A sudden change or transition from one situation, choice, or setting to another.",
                        "ru": "Внезапное изменение или переход от одной ситуации, выбора или настройки к другой."
                    },
                    "translations": {

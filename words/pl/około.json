    "entries": [
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "65711396-20b9-359f-9198-49b73cdf04a5",
                    "sense_definition": "Słowo używane przed liczbą, miarą lub określeniem czasu, wskazujące, że podana wartość nie jest precyzyjna; w przybliżeniu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "approximation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po <około> czterdziestu minutach ciasto można wyjąć z piekarnika.",
                            "target_lang_translations": {
                                "ru": "<Примерно> через сорок минут пирог можно вынимать из духовки."
                            }
                        },
                        {
                            "text": "W sali zgromadziło się <około> dwustu osób czekających na koncert.",
                            "target_lang_translations": {
                                "ru": "В зале собралось <около> двухсот человек, ожидающих концерта."
                            }
                        },
                        {
                            "text": "Potrafił pracować nad <około> dwudziestoma zagadnieniami naraz.",
                            "target_lang_translations": {
                                "ru": "Он мог работать над <примерно> двадцатью задачами одновременно."
                            }
                        }
                    ],
                    "synonyms": [
                        "mniej więcej",
                        "w przybliżeniu",
                        "koło",
                        "plus minus",
                        "na oko",
                        "z grubsza",
                        "rzędu"
                    ],
                    "antonyms": [
                        "dokładnie",
                        "równo",
                        "precyzyjnie",
                        "ściśle"
                    ],
                    "common_phrases": [
                        "około godziny",
                        "około roku",
                        "około tysiąca",
                        "około połowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое перед числом, мерой или указанием времени, означающее, что значение не является точным; приблизительно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "около",
                                "target_lang_sense_clarification": "предлог, требующий родительного падежа (кого? чего?)"
                            },
                            {
                                "target_lang_word": "примерно",
                                "target_lang_sense_clarification": "наречие, часто используется для оценки количества"
                            },
                            {
                                "target_lang_word": "приблизительно",
                                "target_lang_sense_clarification": "формальный синоним слова «примерно»"
                            }
                        ]
                    }
                }
            ]
        },
        {

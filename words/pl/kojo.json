    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "15047d00-a193-350b-bc26-914bf0d42cc8",
                    "sense_definition": "Prowizoryczne miejsce do spania, twarde łóżko lub prycza, często wykonane z desek.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bed_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po ciężkim dniu w pracy marzył tylko o tym, by wskoczyć w <w>kojo</w>.",
                            "target_lang_translations": {
                                "ru": "После тяжелого рабочего дня он мечтал только о том, чтобы запрыгнуть в <w>койку</w>."
                            }
                        },
                        {
                            "text": "To stare <w>kojo</w> było niewygodne, ale lepsze niż spanie na gołej ziemi.",
                            "target_lang_translations": {
                                "ru": "Эта старая <w>койка</w> была неудобной, но лучше, чем спать на голой земле."
                            }
                        }
                    ],
                    "synonyms": [
                        "prycza"
                    ],
                    "common_phrases": [
                        "wskoczyć w kojo",
                        "twarde kojo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временное место для сна, жесткая кровать или нары, часто сделанные из досок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "койка",
                                "target_lang_sense_clarification": "разговорное, простая кровать"
                            },
                            {
                                "target_lang_word": "топчан",
                                "target_lang_sense_clarification": "жесткая кровать, лежанка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5609995b-002e-3ccc-9a5c-440740818262",
                    "sense_definition": "W gwarze więziennej: łóżko lub prycza znajdująca się w celi.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "bed_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Więzień musiał idealnie pościelić swoje <w>kojo</w> przed porannym apelem.",
                            "target_lang_translations": {
                                "ru": "Заключенный должен был идеально заправить свою <w>шконку</w> перед утренней проверкой."
                            }
                        },
                        {
                            "text": "Nowy osadzony zajął wolne <w>kojo</w> pod oknem, bo nikt inny go nie chciał.",
                            "target_lang_translations": {
                                "ru": "Новый заключенный занял свободную <w>шконку</w> у окна, так как никто другой не хотел её брать."
                            }
                        }
                    ],
                    "synonyms": [
                        "prycza"
                    ],
                    "common_phrases": [
                        "górne kojo",
                        "dolne kojo"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gwara więzienna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "На тюремном жаргоне: кровать или нары, находящиеся в камере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шконка",
                                "target_lang_sense_clarification": "тюремный жаргон"
                            },
                            {
                                "target_lang_word": "нары",
                                "target_lang_sense_clarification": "деревянный настил для сна"
                            }
                        ]
                    }
                }
            ]
        }
    ]

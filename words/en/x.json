        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "92852e6f-0ed2-3f06-90b9-754165efe673",
                    "sense_definition": "Used in place of 'and' to indicate a collaboration between artists or brands.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "collaboration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new Nike <w>x</w> Supreme sneakers sold out in minutes.",
                            "target_lang_translations": {
                                "ru": "Новые кроссовки Nike <w>совместно с</w> Supreme были распроданы за минуты."
                            }
                        },
                        {
                            "text": "I really like the track produced by Calvin Harris <w>x</w> Rihanna.",
                            "target_lang_translations": {
                                "ru": "Мне очень нравится трек, записанный Calvin Harris <w>и</w> Rihanna."
                            }
                        }
                    ],
                    "synonyms": [
                        "and",
                        "with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется вместо 'и' для обозначения сотрудничества между художниками или брендами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и",
                                "target_lang_sense_clarification": "союз"
                            },
                            {
                                "target_lang_word": "совместно с",
                                "target_lang_sense_clarification": "коллаборация"
                            }
                        ]
                    }
                }
            ]
        },

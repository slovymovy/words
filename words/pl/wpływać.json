    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a31ad54a-9a61-3262-abf3-827ba90cf30a",
                    "sense_definition": "O rzekach i strumieniach: kończyć swój bieg, wpadając do innego zbiornika wodnego, takiego jak morze, jezioro lub inna rzeka.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "water_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta duża rzeka <w>wpływa</w> bezpośrednio do Morza Bałtyckiego."
                        },
                        {
                            "text": "Widzieliśmy miejsce, gdzie górski potok <w>wpływa</w> do jeziora."
                        }
                    ],
                    "synonyms": [
                        "uchodzić"
                    ],
                    "antonyms": [
                        "wypływać"
                    ],
                    "common_phrases": [
                        "wpływać do morza",
                        "wpływać do jeziora",
                        "rzeka wpływa"
                    ]
                },
                {
                    "sense_id": "5fe6fdb0-c2c7-3693-bdcc-6e1e5fb13119",
                    "sense_definition": "Wywierać wpływ na kogoś lub na coś; sprawiać, że ktoś zmienia zachowanie lub że zachodzą pewne zjawiska.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stres w pracy negatywnie <w>wpływa</w> na jego zdrowie i samopoczucie."
                        },
                        {
                            "text": "Twoja opinia bardzo <w>wpływa</w> na moje ostateczne decyzje."
                        }
                    ],
                    "synonyms": [
                        "oddziaływać"
                    ],
                    "common_phrases": [
                        "wpływać na decyzję",
                        "korzystnie wpływać",
                        "negatywnie wpływać",
                        "wpływać na rozwój"
                    ]
                },
                {
                    "sense_id": "4d5f23b7-75ef-39d7-887f-d04b602ea55f",
                    "sense_definition": "Dostawać się do wnętrza czegoś lub na dany obszar, płynąc w wodzie lub unosząc się w powietrzu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_entry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe ryby często <w>wpływają</w> między skały, aby ukryć się przed drapieżnikami."
                        },
                        {
                            "text": "Zimne powietrze <w>wpływa</w> do pokoju przez otwarte okno."
                        }
                    ],
                    "antonyms": [
                        "wypływać"
                    ],
                    "common_phrases": [
                        "wpływać do środka",
                        "wpływać na głęboką wodę"
                    ]
                },
                {
                    "sense_id": "b3b899a3-6546-3508-86ab-beef0dfa5eab",
                    "sense_definition": "Wchodzić statkiem, łodzią lub innym obiektem pływającym do portu lub przystani.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_entry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielki statek pasażerski powoli <w>wpływa</w> do portu w Gdyni."
                        },
                        {
                            "text": "Jachty zazwyczaj <w>wpływają</w> do mariny przed zachodem słońca."
                        }
                    ],
                    "synonyms": [
                        "zawijać"
                    ],
                    "antonyms": [
                        "wypływać"
                    ],
                    "common_phrases": [
                        "wpływać do portu",
                        "wpływać do przystani"
                    ]
                },
                {
                    "sense_id": "769d104c-4c00-308c-85a4-0b5d0ad10c24",
                    "sense_definition": "O pieniądzach, dokumentach lub pismach: docierać do adresata, urzędu lub na konto bankowe.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wypłata zawsze <w>wpływa</w> na moje konto dziesiątego dnia miesiąca."
                        },
                        {
                            "text": "Do biura codziennie <w>wpływają</w> nowe podania o pracę od kandydatów."
                        }
                    ],
                    "common_phrases": [
                        "wpływać na konto",
                        "wpływać do urzędu",
                        "wniosek wpływa"
                    ]
                }
            ]
        }
    ]

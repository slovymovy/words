                {
                    "sense_id": "afe44b17-86e7-35ba-a8dc-9df70c149144",
                    "sense_definition": "To design a building, machine, or structure.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "design",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The famous architect <w>planned</w> the entire city center.",
                            "target_lang_translations": {
                                "ru": "Знаменитый архитектор <w>спроектировал</w> весь центр города."
                            }
                        },
                        {
                            "text": "Engineers <w>planned</w> the bridge to withstand strong winds.",
                            "target_lang_translations": {
                                "ru": "Инженеры <w>спроектировали</w> мост так, чтобы он выдерживал сильные ветры."
                            }
                        }
                    ],
                    "synonyms": [
                        "design",
                        "blueprint",
                        "scheme"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать проект или чертёж здания, машины или сооружения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проектировать",
                                "target_lang_sense_clarification": "создавать проект"
                            },
                            {
                                "target_lang_word": "разрабатывать",
                                "target_lang_sense_clarification": "конструировать"
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9e68c261-6d9c-389f-a180-e9233cd05244",
                    "sense_definition": "Het inzicht of bewustzijn van een situatie; het begrijpen van hoe iets in elkaar zit.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kwam plotseling tot het <w>besef</w> dat hij zijn sleutels vergeten was.",
                            "target_lang_translations": {
                                "en": "He suddenly came to the <w>realization</w> that he had forgotten his keys.",
                                "ru": "Он внезапно пришел к <w>осознанию</w>, что забыл свои ключи."
                            }
                        },
                        {
                            "text": "Door haar drukke werk raakte ze elk <w>besef</w> van tijd kwijt.",
                            "target_lang_translations": {
                                "en": "Due to her busy work, she lost all <w>sense</w> of time.",
                                "ru": "Из-за напряженной работы она потеряла всякое <w>ощущение</w> времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "bewustzijn",
                        "inzicht",
                        "notie",
                        "benul"
                    ],
                    "antonyms": [
                        "onwetendheid"
                    ],
                    "common_phrases": [
                        "besef van tijd",
                        "tot het besef komen",
                        "enig besef hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "The understanding, insight, or awareness of a situation; the realization of a fact.",
                        "ru": "Понимание или осознание ситуации; понимание того, как что-то устроено."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "realization",
                                "target_lang_sense_clarification": "the act of becoming fully aware of something"
                            },
                            {
                                "target_lang_word": "awareness",
                                "target_lang_sense_clarification": "the state of knowing about something"
                            },
                            {
                                "target_lang_word": "sense",
                                "target_lang_sense_clarification": "a feeling or perception, often used with time or reality"
                            },
                            {
                                "target_lang_word": "notion",
                                "target_lang_sense_clarification": "a vague idea or understanding"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "осознание",
                                "target_lang_sense_clarification": "полное понимание ситуации"
                            },
                            {
                                "target_lang_word": "понимание",
                                "target_lang_sense_clarification": "способность осмыслить что-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "52c9019d-6ba9-3690-9811-52d60623f98e",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>beseffen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>besef</w> nu pas hoeveel geluk ik heb gehad.",
                            "target_lang_translations": {
                                "en": "I only now <w>realize</w> how lucky I have been.",
                                "ru": "Я только сейчас <w>осознаю</w>, как мне повезло."
                            }
                        },
                        {
                            "text": "Soms <w>besef</w> ik niet hoe laat het eigenlijk is.",
                            "target_lang_translations": {
                                "en": "Sometimes I do not <w>realize</w> what time it actually is.",
                                "ru": "Иногда я не <w>осознаю</w>, сколько на самом деле времени."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of <w>to realize</w>.",
                        "ru": "Форма первого лица единственного числа настоящего времени глагола <w>beseffen</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "realize",
                                "target_lang_sense_clarification": "to understand or become aware of a fact"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "осознаю",
                                "target_lang_sense_clarification": "я понимаю/осознаю"
                            },
                            {
                                "target_lang_word": "понимаю",
                                "target_lang_sense_clarification": "я постигаю смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26fc9bbd-aab8-3bc3-913c-acedd53d26e3",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>beseffen</w> (bij inversie).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Besef</w> je wel wat voor gevolgen dit kan hebben?",
                            "target_lang_translations": {
                                "en": "Do you <w>realize</w> what kind of consequences this can have?",
                                "ru": "<w>Осознаешь</w> ли ты, какие последствия это может иметь?"
                            }
                        },
                        {
                            "text": "Waarom <w>besef</w> je niet dat dit gevaarlijk is?",
                            "target_lang_translations": {
                                "en": "Why do you not <w>realize</w> that this is dangerous?",
                                "ru": "Почему ты не <w>понимаешь</w>, что это опасно?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Second person singular present tense of <w>to realize</w> (used in questions/inversion).",
                        "ru": "Форма второго лица единственного числа настоящего времени глагола <w>beseffen</w> (при инверсии)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "realize",
                                "target_lang_sense_clarification": "to understand or become aware of a fact"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "осознаешь",
                                "target_lang_sense_clarification": "ты понимаешь/осознаешь"
                            },
                            {
                                "target_lang_word": "понимаешь",
                                "target_lang_sense_clarification": "ты постигаешь смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4415b683-0d86-3b1b-a025-46f3e1a09a0c",
                    "sense_definition": "Gebiedende wijs van <w>beseffen</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Besef</w> goed dat dit je allerlaatste kans is.",
                            "target_lang_translations": {
                                "en": "<w>Realize</w> well that this is your very last chance.",
                                "ru": "<w>Пойми</w> хорошенько, что это твой самый последний шанс."
                            }
                        },
                        {
                            "text": "<w>Besef</w> wel dat je hier zelf voor gekozen hebt.",
                            "target_lang_translations": {
                                "en": "Do <w>realize</w> that you chose this yourself.",
                                "ru": "<w>Осознай</w> же, что ты сам выбрал это."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative mood of <w>to realize</w>.",
                        "ru": "Повелительное наклонение глагола <w>beseffen</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "realize",

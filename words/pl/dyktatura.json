    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0814550b-2325-3a91-9084-b59deabba7ef",
                    "sense_definition": "Forma sprawowania absolutnej i nieograniczonej władzy w państwie przez jedną osobę lub grupę osób, często z naruszeniem praw obywatelskich",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "political_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele państw w tym regionie to brutalne <w>dyktatury</w> łamiące prawa człowieka.",
                            "target_lang_translations": {
                                "ru": "Многие страны в этом регионе — это жестокие <w>диктатуры</w>, нарушающие права человека."
                            }
                        },
                        {
                            "text": "Wojskowa <w>dyktatura</w> została obalona przez masowe protesty społeczne.",
                            "target_lang_translations": {
                                "ru": "Военная <w>диктатура</w> была свергнута массовыми общественными протестами."
                            }
                        }
                    ],
                    "synonyms": [
                        "tyrania",
                        "despotyzm"
                    ],
                    "antonyms": [
                        "demokracja"
                    ],
                    "common_phrases": [
                        "dyktatura wojskowa",
                        "dyktatura proletariatu",
                        "obalić dyktaturę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма правления, при которой неограниченная власть принадлежит одному лицу или группе лиц, часто с нарушением прав граждан"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "диктатура",
                                "target_lang_sense_clarification": "политический режим"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ea51a87-b285-3442-a3ae-6159bc08dc8b",
                    "sense_definition": "Sytuacja w grupie, instytucji lub rodzinie, w której jedna osoba narzuca swoją wolę innym w sposób bezwzględny",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ich domu panuje surowa <w>dyktatura</w> ojca, której wszyscy się podporządkowują.",
                            "target_lang_translations": {
                                "ru": "В их доме царит суровая <w>диктатура</w> отца, которой все подчиняются."
                            }
                        },
                        {
                            "text": "Nie zamierzam ulegać <w>dyktaturze</w> mody i będę ubierać się wygodnie.",
                            "target_lang_translations": {
                                "ru": "Я не собираюсь поддаваться <w>диктатуре</w> моды и буду одеваться удобно."
                            }
                        }
                    ],
                    "synonyms": [
                        "despotyzm",
                        "tyrania"
                    ],
                    "common_phrases": [
                        "dyktatura mody",
                        "dyktatura cen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация в группе или семье, когда кто-то навязывает свою волю другим; жесткое требование следовать определенным нормам"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "диктатура",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "деспотизм",
                                "target_lang_sense_clarification": "поведение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                },
                {
                    "sense_id": "94501fef-dad8-317d-a751-573910d158cb",
                    "sense_definition": "Czuć bardzo silną niechęć do kogoś lub czegoś; bardzo nie lubić (używane głównie w przeczeniach).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tolerance_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra nie <znosi> smaku gotowanej marchewki.",
                            "target_lang_translations": {
                                "ru": "Моя сестра не <выносит> вкуса вареной моркови."
                            }
                        },
                        {
                            "text": "Ten nauczyciel jest bardzo surowy i nie <znosi> sprzeciwu.",
                            "target_lang_translations": {

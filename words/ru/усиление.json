            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ec5ec226-664a-3ecd-9719-de78ed1fc40b",
                    "sense_definition": "Действие по значению глагола <w>усилить</w> (или <w>усиливать</w>); увеличение силы, интенсивности или степени проявления чего-либо.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_strengthening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В связи с праздниками произошло значительное <w>усиление</w> охраны общественного порядка.",
                            "target_lang_translations": {
                                "en": "Due to the holidays, there was a significant <w>reinforcement</w> of public order security."
                            }
                        },
                        {
                            "text": "Метеорологи прогнозируют резкое <w>усиление</w> ветра и морозов на следующей неделе.",
                            "target_lang_translations": {
                                "en": "Meteorologists predict a sharp <w>strengthening</w> of the wind and frost next week."
                            }
                        }
                    ],
                    "synonyms": [
                        "усиливание"
                    ],
                    "antonyms": [
                        "ослабление"
                    ],
                    "common_phrases": [
                        "усиление контроля",
                        "усиление ветра",
                        "усиление мер безопасности",
                        "усиление позиций"
                    ],
                    "target_lang_definitions": {
                        "en": "The action of making something stronger or more intense; an increase in force, intensity, or degree."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strengthening",
                                "target_lang_sense_clarification": "general increase in strength or force"
                            },
                            {
                                "target_lang_word": "reinforcement",
                                "target_lang_sense_clarification": "making something stronger by adding support, personnel, or material"
                            },
                            {
                                "target_lang_word": "intensification",
                                "target_lang_sense_clarification": "increase in degree, magnitude, or activity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a42b59c-a15f-31ef-bdb5-0f292d3b57b0",
                    "sense_definition": "Увеличение мощности электрических, звуковых или иных сигналов.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для передачи данных на большое расстояние необходимо <w>усиление</w> сигнала.",
                            "target_lang_translations": {
                                "en": "Signal <w>amplification</w> is necessary for transmitting data over long distances."
                            }
                        },
                        {
                            "text": "Инженер настроил прибор на максимальное <w>усиление</w> звука для лучшей слышимости.",
                            "target_lang_translations": {
                                "en": "The engineer set the device to maximum sound <w>amplification</w> for better audibility."
                            }
                        }
                    ],
                    "antonyms": [
                        "ослабление"
                    ],
                    "common_phrases": [
                        "коэффициент усиления",
                        "усиление сигнала",
                        "линейное усиление"
                    ],
                    "target_lang_definitions": {
                        "en": "An increase in the power or amplitude of electrical, sound, or other signals."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "amplification",
                                "target_lang_sense_clarification": "increasing signal power or volume"
                            },
                            {
                                "target_lang_word": "gain",
                                "target_lang_sense_clarification": "technical measure of signal increase"
                            }
                        ]
                    }
                }
            ]

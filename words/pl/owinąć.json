                {
                    "sense_id": "753c2b60-1751-3bc3-9d50-b0c8a6931571",
                    "sense_definition": "Otoczyć coś lub kogoś warstwą materiału, papieru lub innej rzeczy, zakrywając to lub oplatając dookoła.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "wrapping_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz dokładnie <owinąć> prezent papierem, żeby niespodzianka się udała.",
                            "target_lang_translations": {
                                "ru": "Ты должен тщательно <обернуть> подарок бумагой, чтобы сюрприз удался."
                            }
                        },
                        {
                            "text": "Pielęgniarka musiała <owinąć> bandażem moją zranioną rękę.",
                            "target_lang_translations": {
                                "ru": "Медсестра должна была <обмотать> бинтом мою раненую руку."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawinąć",
                        "otulić",
                        "obwiązać",
                        "opasać"
                    ],
                    "antonyms": [
                        "odwinąć",
                        "rozwinąć"
                    ],
                    "common_phrases": [
                        "owinąć prezent",
                        "owinąć bandażem",
                        "owinąć wokół szyi",
                        "owinąć taśmą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обернуть что-либо или кого-либо слоем материала, бумаги или другого предмета, закрывая или обматывая вокруг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обернуть",
                                "target_lang_sense_clarification": "покрыть поверхность чем-либо со всех сторон"
                            },
                            {
                                "target_lang_word": "обмотать",
                                "target_lang_sense_clarification": "намотать что-то вокруг (например, бинт или веревку)"
                            },
                            {
                                "target_lang_word": "завернуть",
                                "target_lang_sense_clarification": "упаковать внутрь чего-либо (например, подарок)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f9907af-229f-393c-824b-5de867ab465b",
                    "sense_definition": "Otoczyć własne ciało czymś, na przykład tkaniną lub kocem, w celu okrycia się lub ogrzania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "wrapping_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy zrobiło się zimno, postanowiła szczelnie <owinąć się> ciepłym kocem.",
                            "target_lang_translations": {
                                "ru": "Когда стало холодно, она решила плотно <закутаться> в теплое одеяло."
                            }
                        },
                        {
                            "text": "Po wyjściu z wody musisz szybko <owinąć się> ręcznikiem, żeby nie zachorować.",
                            "target_lang_translations": {
                                "ru": "После выхода из воды ты должен быстро <обернуться> полотенцем, чтобы не заболеть."
                            }
                        }
                    ],
                    "synonyms": [
                        "otulić się",
                        "zawinąć się"
                    ],
                    "antonyms": [
                        "odwinąć się",
                        "rozwinąć się"
                    ],
                    "common_phrases": [
                        "owinąć się kocem",
                        "owinąć się ręcznikiem",
                        "owinąć się szalem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обернуть собственное тело чем-либо, например тканью или одеялом, чтобы укрыться или согреться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закутаться",
                                "target_lang_sense_clarification": "плотно укрыться чем-то теплым"
                            },
                            {
                                "target_lang_word": "обернуться",
                                "target_lang_sense_clarification": "обмотать что-то вокруг себя"
                            },
                            {
                                "target_lang_word": "завернуться",
                                "target_lang_sense_clarification": "полностью укрыться в ткань"

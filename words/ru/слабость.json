            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4467f4d7-0bad-33c8-b6af-1920ebdc5a7c",
                    "sense_definition": "Состояние недостатка физических сил, энергии или здоровья.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После долгой болезни он всё ещё чувствовал сильную <w>слабость</w> во всём теле.",
                            "target_lang_translations": {
                                "en": "After a long illness, he still felt severe <w>weakness</w> in his whole body."
                            }
                        },
                        {
                            "text": "Внезапная <w>слабость</w> заставила его остановиться и присесть на скамейку в парке.",
                            "target_lang_translations": {
                                "en": "Sudden <w>weakness</w> forced him to stop and sit down on a bench in the park."
                            }
                        }
                    ],
                    "synonyms": [
                        "бессилие",
                        "немощь",
                        "вялость"
                    ],
                    "antonyms": [
                        "сила",
                        "мощь"
                    ],
                    "common_phrases": [
                        "чувствовать слабость",
                        "общая слабость",
                        "сильная слабость"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of lacking physical strength, energy, or health."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weakness",
                                "target_lang_sense_clarification": "physical condition"
                            },
                            {
                                "target_lang_word": "frailty",
                                "target_lang_sense_clarification": "delicate health"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43336a19-7b5e-389d-ae89-a9dd12ba0956",
                    "sense_definition": "Недостаток воли, решительности или твёрдости в характере и поступках.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "character_trait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он считал, что просить о помощи — это недопустимая <w>слабость</w> для мужчины.",
                            "target_lang_translations": {
                                "en": "He considered asking for help to be an unacceptable <w>weakness</w> for a man."
                            }
                        },
                        {
                            "text": "В минуту опасности он проявил <w>слабость</w> и не смог принять верное решение.",
                            "target_lang_translations": {
                                "en": "In a moment of danger, he showed <w>weakness</w> and could not make the right decision."
                            }
                        }
                    ],
                    "synonyms": [
                        "безволие",
                        "мягкотелость",
                        "нерешительность"
                    ],
                    "antonyms": [
                        "сила"
                    ],
                    "common_phrases": [
                        "проявить слабость",
                        "минутная слабость"
                    ],
                    "target_lang_definitions": {
                        "en": "A lack of willpower, determination, or firmness in character."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weakness",
                                "target_lang_sense_clarification": "lack of will"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e1fde41-c8a3-3f60-93f1-0d05aeb8c345",
                    "sense_definition": "Особая склонность, влечение или пристрастие к кому-либо или чему-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "preference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У моей бабушки всегда была особая <w>слабость</w> к сладостям и домашней выпечке.",
                            "target_lang_translations": {
                                "en": "My grandmother always had a special <w>weakness</w> for sweets and homemade pastries."
                            }
                        },
                        {
                            "text": "Все знали, что этот старый автомобиль — его главная <w>слабость</w> и гордость.",
                            "target_lang_translations": {
                                "en": "Everyone knew that this old car was his main <w>weakness</w> and pride."
                            }
                        }
                    ],
                    "synonyms": [
                        "пристрастие",
                        "склонность",
                        "влечение"
                    ],
                    "common_phrases": [
                        "питать слабость",
                        "иметь слабость"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A special liking, attraction, or partiality towards someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weakness",
                                "target_lang_sense_clarification": "fondness"
                            },
                            {
                                "target_lang_word": "soft spot",
                                "target_lang_sense_clarification": "idiomatic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c7e75e5-bcc4-33ac-814e-f6f76e70e807",
                    "sense_definition": "Уязвимое место, недостаток или несовершенство в чём-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "flaw",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный шахматист быстро нашёл <w>слабость</w> в защите своего соперника и выиграл партию.",
                            "target_lang_translations": {
                                "en": "The experienced chess player quickly found a <w>weakness</w> in his opponent's defense and won the game."
                            }
                        },
                        {
                            "text": "Главная <w>слабость</w> этого плана заключалась в отсутствии запасного варианта действий.",
                            "target_lang_translations": {
                                "en": "The main <w>weakness</w> of this plan lay in the absence of a backup option."
                            }
                        }
                    ],
                    "synonyms": [
                        "уязвимость",
                        "недостаток",
                        "изъян"
                    ],
                    "antonyms": [
                        "сила"
                    ],
                    "common_phrases": [
                        "найти слабость",
                        "использовать слабость"

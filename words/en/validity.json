                {
                    "sense_id": "173fda7a-9a4a-3d9e-b037-c448bdcade63",
                    "sense_definition": "The quality of being based on truth, facts, or reason.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to check the <w>validity</w> of his argument before making a decision.",
                            "target_lang_translations": {
                                "ru": "Нам нужно проверить <w>обоснованность</w> его аргумента, прежде чем принимать решение."
                            }
                        },
                        {
                            "text": "The <w>validity</w> of the results was questioned by the experts.",
                            "target_lang_translations": {
                                "ru": "<w>Достоверность</w> результатов была поставлена под сомнение экспертами."
                            }
                        }
                    ],
                    "synonyms": [
                        "validness"
                    ],
                    "antonyms": [
                        "invalidity"
                    ],
                    "common_phrases": [
                        "check the validity",
                        "question the validity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество быть основанным на истине, фактах или логике; убедительность."

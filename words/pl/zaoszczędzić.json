            "senses": [
                {
                    "sense_id": "bf403b7a-ea6a-319a-bd6c-a15b1d332448",
                    "sense_definition": "Zgromadzić pieniądze lub inne zasoby poprzez ograniczenie wydatków lub mniejsze zużycie; zachować część środków dla siebie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "finance_resources",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzięki wyprzedaży udało mi się <w>zaoszczędzić</w> sporo pieniędzy na nowe meble.",
                            "target_lang_translations": {
                                "ru": "Благодаря распродаже мне удалось <w>сэкономить</w> немало денег на новую мебель."
                            }
                        },
                        {
                            "text": "Jeśli pojedziemy skrótem, możemy <w>zaoszczędzić</w> przynajmniej piętnaście minut drogi.",
                            "target_lang_translations": {
                                "ru": "Если мы поедем короткой дорогой, то сможем <w>сэкономить</w> как минимум пятнадцать минут пути."
                            }
                        }
                    ],
                    "synonyms": [
                        "odłożyć",
                        "zachować",
                        "uzbierać",
                        "zgromadzić"
                    ],
                    "antonyms": [
                        "wydać",
                        "zmarnować",
                        "roztrwonić",
                        "stracić"
                    ],
                    "common_phrases": [
                        "zaoszczędzić pieniądze",
                        "zaoszczędzić czas",
                        "zaoszczędzić na czymś",
                        "sporo zaoszczędzić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Накопить деньги или другие ресурсы за счет сокращения расходов или меньшего потребления; сохранить часть средств для себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сэкономить",
                                "target_lang_sense_clarification": "деньги, время или ресурсы"
                            },
                            {
                                "target_lang_word": "сберечь",
                                "target_lang_sense_clarification": "сохранить, не потратить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36c6dd44-e0f6-39a6-977e-ec4717f1e44e",
                    "sense_definition": "Uchronić kogoś przed czymś nieprzyjemnym, trudnym lub męczącym; sprawić, że ktoś nie musi czegoś doświadczać.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałem ci <w>zaoszczędzić</w> rozczarowania, dlatego nie powiedziałem o tym wcześniej.",
                            "target_lang_translations": {
                                "ru": "Я хотел <w>избавить</w> тебя от разочарования, поэтому не сказал об этом раньше."
                            }
                        },
                        {
                            "text": "Proszę, <w>zaoszczędź</w> mi szczegółów tej nieprzyjemnej rozmowy.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>избавь</w> меня от подробностей этого неприятного разговора."
                            }
                        }
                    ],
                    "synonyms": [
                        "oszczędzić",
                        "darować"
                    ],
                    "antonyms": [
                        "przysporzyć",
                        "obarczyć"
                    ],
                    "common_phrases": [
                        "zaoszczędzić kłopotu",
                        "zaoszczędzić wstydu",
                        "zaoszczędzić nerwów",
                        "zaoszczędzić cierpień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уберечь кого-либо от чего-то неприятного, трудного или утомительного; сделать так, чтобы кому-то не пришлось переживать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избавить",
                                "target_lang_sense_clarification": "освободить от чего-то неприятного"
                            },
                            {
                                "target_lang_word": "уберечь",
                                "target_lang_sense_clarification": "защитить от переживаний"

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ee1aa986-9a69-322c-a546-f1dc3358f3a6",
                    "sense_definition": "The grounds and buildings of a university, college, school, or large organization.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most first-year students choose to live on <w>campus</w>.",
                            "target_lang_translations": {
                                "ru": "Большинство первокурсников предпочитают жить в <w>кампусе</w>."
                            }
                        },
                        {
                            "text": "The university <w>campus</w> includes a library, a gym, and several dormitories.",
                            "target_lang_translations": {
                                "ru": "Университетский <w>кампус</w> включает в себя библиотеку, спортзал и несколько общежитий."
                            }
                        }
                    ],
                    "synonyms": [
                        "grounds",
                        "premises",
                        "site"
                    ],
                    "common_phrases": [
                        "on campus",
                        "off campus",
                        "college campus",
                        "university campus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория и здания университета, колледжа или школы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ка́мпус",
                                "target_lang_sense_clarification": "территория университета"
                            },
                            {
                                "target_lang_word": "студгородо́к",
                                "target_lang_sense_clarification": "студенческий городок"
                            },
                            {

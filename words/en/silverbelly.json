    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4c7ae8a2-c801-3b15-9f07-c1c8e527183e",
                    "sense_definition": "Any fish of the genus Gerres, typically small and silvery, found in coastal waters.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fish_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fisherman was happy to find a large <w>silverbelly</w> in his net this morning.",
                            "target_lang_translations": {
                                "ru": "Рыбак был счастлив найти крупную <w>сребробрюшку</w> в своей сети этим утром."
                            }
                        },
                        {
                            "text": "We decided to fry the fresh <w>silverbelly</w> with garlic and herbs for dinner.",
                            "target_lang_translations": {
                                "ru": "Мы решили пожарить свежую <w>сребробрюшку</w> с чесноком и травами на ужин."
                            }
                        }
                    ],
                    "synonyms": [
                        "mojarra"
                    ],
                    "common_phrases": [
                        "catch a silverbelly",
                        "fresh silverbelly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любая рыба рода Gerres, обычно мелкая и серебристая, обитающая в прибрежных водах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сребробрюшка",
                                "target_lang_sense_clarification": "рыба"
                            },
                            {
                                "target_lang_word": "геррес",
                                "target_lang_sense_clarification": "род рыб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "947c5df1-1c47-3757-8fb1-6319d979dac3",
                    "sense_definition": "A grey cowboy hat, specifically a Stetson.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rancher tipped his <w>silverbelly</w> to the crowd as he rode by.",
                            "target_lang_translations": {
                                "ru": "Фермер приподнял свою <w>серебристо-серую шляпу</w> перед толпой, проезжая мимо."
                            }
                        },
                        {
                            "text": "He prefers to wear a <w>silverbelly</w> rather than a standard black hat.",
                            "target_lang_translations": {
                                "ru": "Он предпочитает носить <w>серебристо-серую шляпу</w>, а не обычную черную."
                            }
                        }
                    ],
                    "synonyms": [
                        "Stetson"
                    ],
                    "common_phrases": [
                        "wear a silverbelly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Серая ковбойская шляпа, в частности фирмы Stetson."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серебристо-серая ковбойская шляпа",
                                "target_lang_sense_clarification": "головной убор"
                            }
                        ]
                    }
                }
            ]
        }
    ]

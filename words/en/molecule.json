                {
                    "sense_id": "784296f1-6aa2-3464-8788-0a5e27072f63",
                    "sense_definition": "The smallest unit of a chemical substance, consisting of two or more atoms held together by bonds.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "chemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A water <w>molecule</w> is made up of two hydrogen atoms and one oxygen atom.",
                            "target_lang_translations": {
                                "ru": "<w>Молекула</w> воды состоит из двух атомов водорода и одного атома кислорода."
                            }
                        },
                        {
                            "text": "Scientists are testing a new <w>molecule</w> that could help treat the disease.",
                            "target_lang_translations": {
                                "ru": "Ученые тестируют новую <w>молекулу</w>, которая может помочь в лечении болезни."
                            }
                        }
                    ],
                    "synonyms": [
                        "particle"
                    ],
                    "common_phrases": [
                        "water molecule",
                        "oxygen molecule",
                        "organic molecule",
                        "single molecule"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наименьшая частица вещества, состоящая из двух или более атомов, соединенных химическими связями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "моле́кула",
                                "target_lang_sense_clarification": "химия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ddadee8b-1964-34fb-bd81-9afee6cdcaf7",
                    "sense_definition": "A very small amount or tiny particle of something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is not a <w>molecule</w> of truth in the rumors spreading around town.",
                            "target_lang_translations": {
                                "ru": "В слухах, распространяющихся по городу, нет ни <w>крупицы</w> правды."
                            }
                        },
                        {
                            "text": "He does not possess a single <w>molecule</w> of sympathy for his rivals.",
                            "target_lang_translations": {

            "senses": [
                {
                    "sense_id": "27288263-4533-38d4-84ea-de4baa4f02ed",
                    "sense_definition": "Initialism of month-over-month; used to compare financial figures to the previous month.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "business_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company reported a significant <w>mom</w> increase in sales.",
                            "target_lang_translations": {
                                "ru": "Компания сообщила о значительном <w>помесячном</w> росте продаж."
                            }

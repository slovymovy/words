            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2ae0301b-3840-3dc9-8d0d-0d83421a6c74",
                    "sense_definition": "Iets wat je in je handen hebt aan iemand anders geven, vaak nadat je het zelf hebt gekregen",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je het zout even <w>doorgeven</w>?",
                            "target_lang_translations": {
                                "en": "Can you <w>pass</w> the salt?",
                                "ru": "Ты не мог бы <w>передать</w> соль?"
                            }
                        },
                        {
                            "text": "We moesten de zware dozen aan elkaar <w>doorgeven</w>.",
                            "target_lang_translations": {
                                "en": "We had to <w>pass</w> the heavy boxes to each other.",
                                "ru": "Нам пришлось <w>передавать</w> тяжелые коробки друг другу."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanreiken",
                        "overhandigen"
                    ],
                    "antonyms": [
                        "houden"
                    ],
                    "common_phrases": [
                        "het zout doorgeven",
                        "de bal doorgeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To give something you are holding to someone else, often after receiving it yourself",
                        "ru": "Передавать предмет из рук в руки, часто после получения его от кого-то другого"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "give physically"
                            },
                            {
                                "target_lang_word": "pass on",
                                "target_lang_sense_clarification": "transfer to the next person"
                            },
                            {
                                "target_lang_word": "hand",
                                "target_lang_sense_clarification": "give with one's hand"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "предмет"
                            },
                            {
                                "target_lang_word": "вручать",
                                "target_lang_sense_clarification": "отдавать в руки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1797b3fb-85d2-3c29-8537-167e12d1430b",
                    "sense_definition": "Een bericht of informatie aan een ander persoon vertellen of sturen",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal de boodschap direct aan mijn collega <w>doorgeven</w>.",
                            "target_lang_translations": {
                                "en": "I will <w>pass on</w> the message to my colleague immediately.",
                                "ru": "Я сразу <w>передам</w> сообщение своему коллеге."
                            }
                        },
                        {
                            "text": "Heb jij je nieuwe adres al <w>doorgegeven</w>?",
                            "target_lang_translations": {
                                "en": "Have you <w>passed on</w> your new address yet?",
                                "ru": "Ты уже <w>сообщил</w> свой новый адрес?"
                            }
                        }
                    ],
                    "synonyms": [
                        "overbrengen",
                        "mededelen"
                    ],
                    "antonyms": [
                        "verzwijgen"
                    ],
                    "common_phrases": [
                        "een boodschap doorgeven",
                        "gegevens doorgeven",
                        "een wijziging doorgeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To tell or send a message or information to another person",
                        "ru": "Сообщать информацию или передавать сообщение другому лицу"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pass on",

            "senses": [
                {
                    "sense_id": "fff66fac-8105-35b6-9ea9-a8cdbd69b43b",
                    "sense_definition": "Not brought together, collected, or harvested.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer was worried about the <w>ungathered</w> crops before the storm arrived.",
                            "target_lang_translations": {
                                "ru": "Фермер беспокоился о <w>неубранном</w> урожае перед приходом шторма."
                            }
                        },
                        {
                            "text": "Many wild berries were left <w>ungathered</w> in the forest.",
                            "target_lang_translations": {
                                "ru": "Много диких ягод осталось <w>несобранными</w> в лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "uncollected",
                        "unharvested",
                        "unpicked"
                    ],
                    "antonyms": [
                        "gathered",
                        "collected",
                        "harvested"
                    ],
                    "common_phrases": [
                        "left ungathered",
                        "ungathered crops",
                        "ungathered fruit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не собранный вместе, не коллекционированный или не убранный (об урожае)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несобранный",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "неубранный",
                                "target_lang_sense_clarification": "об урожае"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24808762-b8b4-3e39-bc6c-d3e65062c351",
                    "sense_definition": "Relating to printed sheets that have not been arranged in the correct order for binding.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The printer delivered the <w>ungathered</w> signatures to the bindery for assembly.",
                            "target_lang_translations": {
                                "ru": "Типография доставила <w>неподобранные</w> тетради в переплетную мастерскую для сборки."
                            }
                        },
                        {
                            "text": "These <w>ungathered</w> sheets must be collated before they can be stitched into a book.",
                            "target_lang_translations": {
                                "ru": "Эти <w>неподобранные</w> листы необходимо скомплектовать, прежде чем их можно будет сшить в книгу."
                            }
                        }
                    ],
                    "synonyms": [

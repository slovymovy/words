    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d8b474a8-42cf-3c41-902d-eec02921600f",
                    "sense_definition": "Zupełnie sam, bez niczyjego towarzystwa (zdrobnienie podkreślające osamotnienie od słowa <w>sam</w>).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "samotność",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopiec został w domu zupełnie <w>samiutki</w>, gdy rodzice wyszli.",
                            "target_lang_translations": {
                                "ru": "Мальчик остался дома <w>один-одинешенек</w>, когда родители ушли."
                            }
                        },
                        {
                            "text": "Czuję się <w>samiutki</w> jak palec w tym wielkim mieście.",
                            "target_lang_translations": {
                                "ru": "Я чувствую себя <w>один-одинешенек</w> в этом огромном городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "samiuśki"
                    ],
                    "antonyms": [
                        "razem",
                        "wspólnie"
                    ],
                    "common_phrases": [
                        "samiutki jak palec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершенно один, без чьего-либо общества (уменьшительная форма, подчеркивающая одиночество)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "один-одинешенек",
                                "target_lang_sense_clarification": "выразительная идиома, подчеркивающая полное одиночество"
                            },
                            {
                                "target_lang_word": "совсем один",
                                "target_lang_sense_clarification": "буквальное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9965a8dd-96af-3261-94bc-bf5f0b31f208",
                    "sense_definition": "Najbardziej skrajny lub dokładny; forma wzmacniająca znaczenie słowa <w>sam</w> w kontekście miejsca i czasu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "podkreślenie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkamy na <w>samiutkim</w> końcu tej długiej ulicy.",
                            "target_lang_translations": {
                                "ru": "Мы живем в <w>самом</w> конце этой длинной улицы."
                            }
                        },
                        {
                            "text": "Wstaliśmy dzisiaj o <w>samiutkim</w> świcie, żeby zdążyć na pociąg.",
                            "target_lang_translations": {
                                "ru": "Мы встали сегодня на <w>самом</w> рассвете, чтобы успеть на поезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "samiuśki"
                    ],
                    "common_phrases": [
                        "na samiutkim końcu",
                        "od samiutkiego rana",
                        "w samiutkim środku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самый крайний или точный; форма, усиливающая значение слова \"сам\" в контексте места и времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самый",
                                "target_lang_sense_clarification": "используется для усиления значения (на самом краю, с самого утра)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "b1d9b4cf-50a0-37d7-a7c8-94a3502c864a",
                    "sense_definition": "Initialism of <w>also known as</w>; used to indicate a pseudonym, nickname, or alternative name for a person or thing.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "alias_indicator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police arrested John Smith, <w>aka</w> \"The Cat Burglar,\" late last night.",
                            "target_lang_translations": {
                                "ru": "Полиция арестовала Джона Смита, <w>он же</w> «Кот-грабитель», поздно ночью."
                            }
                        },
                        {
                            "text": "She lives in the Big Apple, <w>aka</w> New York City.",
                            "target_lang_translations": {
                                "ru": "Она живет в «Большом яблоке», <w>также известном как</w> Нью-Йорк."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от «also known as» (также известный как); используется для указания псевдонима, прозвища или альтернативного имени человека или предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "также известный как",
                                "target_lang_sense_clarification": "стандартный перевод"
                            },
                            {
                                "target_lang_word": "он же",
                                "target_lang_sense_clarification": "используется при перечислении имен или кличек"
                            },
                            {
                                "target_lang_word": "известный как",
                                "target_lang_sense_clarification": "указание на альтернативное имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0e221777-fb69-33b5-9235-70d671088dd4",
                    "sense_definition": "Used to introduce an explanation, clarification, or ironic rephrasing of the previous statement.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "clarification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My car is vintage, <w>aka</w> old and constantly breaking down.",
                            "target_lang_translations": {
                                "ru": "Моя машина винтажная, <w>то есть</w> старая и постоянно ломается."
                            }
                        },
                        {
                            "text": "He is a culinary enthusiast, <w>aka</w> he loves to eat everything in sight.",
                            "target_lang_translations": {
                                "ru": "Он кулинарный энтузиаст, <w>иначе говоря</w>, он любит есть всё, что видит."
                            }
                        }
                    ],
                    "synonyms": [
                        "in other words",
                        "that is",
                        "i.e."
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used for humor or emphasis"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения пояснения, уточнения или ироничного перефразирования предыдущего утверждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "то есть",
                                "target_lang_sense_clarification": "пояснение"
                            },
                            {
                                "target_lang_word": "иначе говоря",
                                "target_lang_sense_clarification": "перефразирование"
                            },
                            {
                                "target_lang_word": "другими словами",
                                "target_lang_sense_clarification": "уточнение смысла"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1e2072b5-3ba5-30aa-887b-330b6b32d092",
                    "sense_definition": "Warunki życia charakteryzujące się bogactwem, dużą wygodą i posiadaniem drogich rzeczy, wykraczające poza standardowe potrzeby.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu ludzi marzy o tym, aby pławić się w <w>luksusie</w> i nie martwić o pieniądze.",
                            "target_lang_translations": {
                                "ru": "Многие люди мечтают купаться в <w>роскоши</w> и не беспокоиться о деньгах."
                            }
                        },
                        {
                            "text": "Ten pięciogwiazdkowy hotel oferuje swoim gościom niespotykany <w>luksus</w>.",
                            "target_lang_translations": {
                                "ru": "Этот пятизвездочный отель предлагает своим гостям невиданную <w>роскошь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "komfort",
                        "przepych",
                        "zbytek"
                    ],
                    "antonyms": [
                        "bieda",
                        "nędza",
                        "skromność"
                    ],
                    "common_phrases": [
                        "pławić się w luksusie",
                        "żyć w luksusie",
                        "odrobina luksusu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Условия жизни, характеризующиеся богатством, большим комфортом и обладанием дорогими вещами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роскошь",
                                "target_lang_sense_clarification": "богатство, пышность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e7a36ac-bedb-3586-88e9-afd0e138cff4",
                    "sense_definition": "Rzecz, usługa lub przyjemność, która nie jest niezbędna do życia, ale zaspokaja wyszukane potrzeby lub świadczy o zamożności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W trudnych czasach nawet zwykła kawa była traktowana jak wielki <w>luksus</w>.",
                            "target_lang_translations": {
                                "ru": "В трудные времена даже обычный кофе считался большой <w>роскошью</w>."
                            }
                        },
                        {
                            "text": "Niestety nie mogę sobie pozwolić na taki <w>luksus</w> jak częste wyjazdy zagraniczne.",
                            "target_lang_translations": {
                                "ru": "К сожалению, я не могу позволить себе такую <w>роскошь</w>, как частые поездки за границу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbytek"
                    ],
                    "antonyms": [
                        "konieczność"
                    ],
                    "common_phrases": [
                        "pozwolić sobie na luksus",
                        "traktować jak luksus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещь, услуга или удовольствие, не являющиеся необходимыми для жизни, но удовлетворяющие изысканные потребности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роскошь",
                                "target_lang_sense_clarification": "дорогое удовольствие, излишество"
                            },
                            {
                                "target_lang_word": "излишество",
                                "target_lang_sense_clarification": "то, без чего можно обойтись"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "70537fee-23ad-3a17-b029-7641664e9722",
                    "sense_definition": "Przesunąć coś na bok lub zebrać rozsypane rzeczy w jedno miejsce, często wykonując ruch ręką lub narzędziem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelner podszedł do naszego stolika i szybko <w>zgarnął</w> okruchy z obrusu.",
                            "target_lang_translations": {
                                "ru": "Официант подошел к нашему столику и быстро <w>смахнул</w> крошки со скатерти."
                            }
                        },
                        {
                            "text": "Musisz <w>zgarnąć</w> te suche liście na jedną stertę, zanim zacznie padać deszcz.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>сгрести</w> эти сухие листья в одну кучу, пока не пошел дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "zsunąć"
                    ],
                    "common_phrases": [
                        "zgarnąć okruchy ze stołu",
                        "zgarnąć włosy z czoła",
                        "zgarnąć śnieg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сдвинуть что-то в сторону или собрать рассыпанные предметы в одно место, часто движением руки или инструментом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сгрести",
                                "target_lang_sense_clarification": "собрать в кучу"
                            },
                            {
                                "target_lang_word": "смахнуть",
                                "target_lang_sense_clarification": "удалить с поверхности движением руки"
                            },
                            {
                                "target_lang_word": "собрать",
                                "target_lang_sense_clarification": "соединить вместе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45609d15-02ac-396c-8ff5-280c93f924f3",
                    "sense_definition": "Zdobyć coś cennego, wygrać nagrodę lub uzyskać dużą korzyść, często w rywalizacji.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz zespół <w>zgarnął</w> główną nagrodę w prestiżowym konkursie informatycznym.",
                            "target_lang_translations": {
                                "ru": "Наша команда <w>забрала</w> главный приз в престижном конкурсе по информатике."
                            }
                        },
                        {
                            "text": "Udało mu się <w>zgarnąć</w> całą pulę pieniędzy podczas wczorajszej gry w karty.",
                            "target_lang_translations": {
                                "ru": "Ему удалось <w>сорвать</w> весь банк во время вчерашней игры в карты."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdobyć"
                    ],
                    "common_phrases": [
                        "zgarnąć nagrodę",
                        "zgarnąć całą pulę",
                        "zgarnąć wszystko"
                    ],
                    "traits": [
                        {

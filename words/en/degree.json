    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "168ccebe-8e0a-3596-9c60-e79b739c600a",
                    "sense_definition": "A unit for measuring temperature on scales such as Celsius or Fahrenheit.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "measurement_units",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Water boils at one hundred <w>degrees</w> Celsius.",
                            "target_lang_translations": {
                                "ru": "Вода кипит при ста <w>градусах</w> Цельсия."
                            }
                        },
                        {
                            "text": "The temperature outside dropped to five <w>degrees</w> below zero.",
                            "target_lang_translations": {
                                "ru": "Температура на улице опустилась до пяти <w>градусов</w> ниже нуля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "degrees Celsius",
                        "degrees Fahrenheit",
                        "thirty degrees"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица измерения температуры по шкалам, таким как Цельсий или Фаренгейт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "градус",
                                "target_lang_sense_clarification": "единица измерения температуры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0ccab37-6e59-3476-af73-2ba23c203104",
                    "sense_definition": "An official qualification awarded to a student by a university or college upon the successful completion of a course of study.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has two bachelor's <w>degrees</w> and is studying towards a master's.",
                            "target_lang_translations": {
                                "ru": "У неё две <w>степени</w> бакалавра, и она учится на магистра."
                            }
                        },
                        {
                            "text": "He earned a <w>degree</w> in biology from a prestigious university.",
                            "target_lang_translations": {
                                "ru": "Он получил <w>степень</w> в области биологии в престижном университете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bachelor's degree",
                        "master's degree",
                        "university degree",
                        "honorary degree"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная квалификация, присваиваемая студенту университетом или колледжем после успешного завершения курса обучения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "степень",
                                "target_lang_sense_clarification": "академическая квалификация"
                            },
                            {
                                "target_lang_word": "диплом",
                                "target_lang_sense_clarification": "документ об образовании"
                            },
                            {
                                "target_lang_word": "учёная степень",
                                "target_lang_sense_clarification": "научное звание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05cb2986-3a27-3311-b642-04075f3bc8a6",
                    "sense_definition": "A unit of measurement for angles, equal to one three-hundred-sixtieth of a full circle.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "measurement_units",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A right angle is a ninety-<w>degree</w> angle.",
                            "target_lang_translations": {
                                "ru": "Прямой угол — это угол в девяносто <w>градусов</w>."
                            }
                        },
                        {
                            "text": "Most humans have a field of vision of almost 180 <w>degrees</w>.",
                            "target_lang_translations": {
                                "ru": "Поле зрения большинства людей составляет почти 180 <w>градусов</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "arcdegree"
                    ],
                    "common_phrases": [
                        "ninety degrees",
                        "180 degrees",
                        "360 degrees"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица измерения углов, равная одной трёхсотшестидесятой части полного круга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "градус",
                                "target_lang_sense_clarification": "геометрическая единица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c0db822-2375-3f6d-be85-a46653619941",
                    "sense_definition": "The amount, level, or extent to which something happens, is present, or is true.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To what <w>degree</w> do the two accounts of the accident agree?",
                            "target_lang_translations": {
                                "ru": "В какой <w>степени</w> эти две версии происшествия совпадают?"
                            }
                        },
                        {
                            "text": "I agree with your plan to some <w>degree</w>, but I have a few concerns.",
                            "target_lang_translations": {
                                "ru": "Я до некоторой <w>степени</w> согласен с вашим планом, но у меня есть несколько опасений."
                            }
                        }
                    ],
                    "synonyms": [
                        "level",
                        "scale",
                        "magnitude"
                    ],
                    "common_phrases": [
                        "to some degree",
                        "to a degree",
                        "high degree of",
                        "by degrees"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество, уровень или предел, до которого что-либо происходит, присутствует или является истинным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "степень",
                                "target_lang_sense_clarification": "мера, уровень"
                            },
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "показатель величины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec66d5e8-2142-3c1f-9f87-fe0fcf474881",
                    "sense_definition": "The highest power of a variable in a mathematical equation or polynomial.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mathematics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A quadratic polynomial is a polynomial of <w>degree</w> two.",
                            "target_lang_translations": {
                                "ru": "Квадратный многочлен — это многочлен второй <w>степени</w>."
                            }
                        },
                        {
                            "text": "Students learned how to solve equations of the third <w>degree</w>.",
                            "target_lang_translations": {
                                "ru": "Студенты учились решать уравнения третьей <w>степени</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наивысшая степень переменной в математическом уравнении или многочлене."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "степень",
                                "target_lang_sense_clarification": "математический показатель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92ae56c5-faa5-3561-a431-dcc6134255e8",
                    "sense_definition": "Any of the forms of an adjective or adverb used to show comparison, such as positive, comparative, or superlative.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The word 'better' represents the comparative <w>degree</w> of the adjective 'good'.",
                            "target_lang_translations": {
                                "ru": "Слово 'better' представляет собой сравнительную <w>степень</w> прилагательного 'good'."
                            }
                        },
                        {
                            "text": "In English grammar, adjectives change form to indicate the <w>degree</w> of comparison.",
                            "target_lang_translations": {
                                "ru": "В английской грамматике прилагательные изменяют форму, чтобы указать на <w>степень</w> сравнения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "comparative degree",
                        "superlative degree"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любая из форм прилагательного или наречия, используемая для сравнения, например, положительная, сравнительная или превосходная."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "степень",
                                "target_lang_sense_clarification": "грамматическая категория"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2defd38-00ea-3ad5-87a9-15a15e50ab0c",
                    "sense_definition": "A specific social standing, rank, or station in society.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In medieval times, people of high <w>degree</w> held great power over the land.",
                            "target_lang_translations": {
                                "ru": "В средневековье люди высокого <w>звания</w> имели большую власть над землями."
                            }
                        },
                        {
                            "text": "The ceremony was attended by citizens of every <w>degree</w> and profession.",
                            "target_lang_translations": {
                                "ru": "На церемонии присутствовали граждане всех <w>сословий</w> и профессий."
                            }
                        }
                    ],
                    "synonyms": [
                        "grade"
                    ],
                    "common_phrases": [
                        "people of high degree"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly found in older literature or historical contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенное социальное положение, ранг или статус в обществе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звание",
                                "target_lang_sense_clarification": "социальный статус"
                            },
                            {
                                "target_lang_word": "положение",
                                "target_lang_sense_clarification": "статус в обществе"
                            },
                            {
                                "target_lang_word": "сословие",
                                "target_lang_sense_clarification": "социальная группа"
                            }
                        ]
                    }
                }
            ]
        },

                {
                    "sense_id": "a157bd99-7ccf-3513-bb59-42637905737e",
                    "sense_definition": "Перестать жить, прекратить биологическое существование",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "biological_death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой дедушка <умер> в прошлом году от старости.",
                            "target_lang_translations": {
                                "en": "My grandfather <died> last year of old age."
                            }
                        },
                        {
                            "text": "Раненый солдат <умер> в госпитале через два дня.",
                            "target_lang_translations": {
                                "en": "The wounded soldier <died> in the hospital two days later."
                            }
                        }
                    ],
                    "synonyms": [
                        "скончаться (skončatʹsja)",
                        "погибнуть",
                        "уйти из жизни (ujti iz žizni)",
                        "помирать (pomiratʹ)",
                        "сконча́ться"
                    ],
                    "antonyms": [
                        "родиться",
                        "жить",
                        "ожить",
                        "воскреснуть"
                    ],
                    "common_phrases": [
                        "умереть от старости",
                        "умереть молодым",
                        "умереть от болезни",
                        "умереть за родину"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop living, to cease biological existence"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "die",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "pass away",
                                "target_lang_sense_clarification": "polite/euphemistic"
                            },
                            {
                                "target_lang_word": "succumb",
                                "target_lang_sense_clarification": "formal, often with 'to'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4ccb495-e177-3810-a138-a0c4ae008cd2",
                    "sense_definition": "Перестать работать, сломаться или разрядиться (о технике)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "malfunction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой телефон внезапно <умер>, и я не смог позвонить.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b2cec8d3-2fdd-3850-af3e-01d10321ed23",
                    "sense_definition": "Op een informele en gezellige manier met elkaar praten over alledaagse dingen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "chatting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben de hele avond gezellig zitten <w>kletsen</w> over onze vakantie.",
                            "target_lang_translations": {
                                "ru": "Мы весь вечер приятно <w>болтали</w> о нашем отпуске.",
                                "en": "We sat <w>chatting</w> pleasantly all evening about our vacation."
                            }
                        },
                        {
                            "text": "Hij kwam bij me zitten op het terras en we <w>kletsten</w> wat over het weer.",
                            "target_lang_translations": {
                                "ru": "Он подсел ко мне на террасе, и мы немного <w>поболтали</w> о погоде.",
                                "en": "He came and sat with me on the terrace and we <w>chatted</w> a bit about the weather."
                            }
                        }
                    ],
                    "synonyms": [
                        "babbelen",
                        "keuvelen",
                        "klessebessen"
                    ],
                    "common_phrases": [
                        "gezellig kletsen",
                        "even kletsen",
                        "wat kletsen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговаривать друг с другом в неформальной и приятной манере о повседневных вещах.",
                        "en": "To talk to each other in an informal and pleasant way about everyday things."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болтать",
                                "target_lang_sense_clarification": "вести непринужденную беседу"
                            },
                            {
                                "target_lang_word": "беседовать",
                                "target_lang_sense_clarification": "разговаривать, общаться"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "chat",
                                "target_lang_sense_clarification": "talk informally"
                            },
                            {
                                "target_lang_word": "chatter",
                                "target_lang_sense_clarification": "talk quickly or at length"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7dba7fd-5d08-3eec-9d0f-4549fed35fb9",
                    "sense_definition": "Onzin vertellen of dingen zeggen die niet waar zijn.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "nonsense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Geloof hem niet, hij <w>kletst</w> maar wat uit zijn nek.",
                            "target_lang_translations": {
                                "ru": "Не верь ему, он просто <w>несет</w> чушь.",
                                "en": "Don't believe him, he's just <w>talking nonsense</w>."
                            }
                        },
                        {
                            "text": "Je moet niet zo <w>kletsen</w>, want dat is helemaal niet waar.",
                            "target_lang_translations": {
                                "ru": "Не надо так <w>болтать</w>, ведь это совсем неправда.",
                                "en": "You shouldn't <w>talk rubbish</w> like that, because that is not true at all."
                            }
                        }
                    ],
                    "synonyms": [
                        "leuteren",
                        "zwammen",
                        "lullen",
                        "ouwehoeren"
                    ],
                    "common_phrases": [

                },
                {
                    "sense_id": "5b66eab0-54d5-38dc-adbe-982e08246aa5",
                    "sense_definition": "To exhaust or tire someone out completely.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_erosion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The long journey <w>wore</w> the travelers out.",

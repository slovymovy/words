    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9ddb1c9c-f385-30c7-93fc-af5b9383d746",
                    "sense_definition": "Słowo pytajne służące do pytania o miejsce, w którego kierunku odbywa się ruch; w jakie miejsce.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Dokąd</w> idziesz tak wcześnie rano?",
                            "target_lang_translations": {
                                "ru": "<w>Куда</w> ты идёшь так рано утром?"
                            }
                        },
                        {
                            "text": "<w>Dokąd</w> jedzie ten pociąg pospieszny?",
                            "target_lang_translations": {
                                "ru": "<w>Куда</w> едет этот скорый поезд?"
                            }
                        }
                    ],
                    "antonyms": [
                        "skąd"
                    ],
                    "common_phrases": [
                        "dokąd idziesz",
                        "dokąd zmierzasz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вопросительное слово, используемое для вопроса о месте, в направлении которого происходит движение; в какое место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куда",
                                "target_lang_sense_clarification": "вопросительное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e4daab5-95cd-3d51-898f-8a6ebc554600",
                    "sense_definition": "Słowo łączące zdanie nadrzędne z podrzędnym, wskazujące na miejsce lub kierunek ruchu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pójdę wszędzie tam, <w>dokąd</w> ty pójdziesz.",
                            "target_lang_translations": {
                                "ru": "Я пойду везде, <w>куда</w> пойдёшь ты."
                            }
                        },
                        {
                            "text": "Nie pamiętam, <w>dokąd</w> schowałem ten ważny dokument.",
                            "target_lang_translations": {
                                "ru": "Я не помню, <w>куда</w> спрятал этот важный документ."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tam dokąd",
                        "patrzeć dokąd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союзное слово, соединяющее главное предложение с придаточным и указывающее на место или направление движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куда",
                                "target_lang_sense_clarification": "союзное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffc8f42e-f27d-31fd-86f8-77cd445facf2",
                    "sense_definition": "Słowo pytajne służące do pytania o czas trwania jakiejś czynności; do kiedy, jak długo.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Dokąd</w> będziesz na mnie czekać w tym miejscu?",
                            "target_lang_translations": {
                                "ru": "<w>До каких пор</w> ты будешь ждать меня здесь?"
                            }
                        },
                        {
                            "text": "<w>Dokąd</w> mamy znosić to zachowanie?",
                            "target_lang_translations": {
                                "ru": "<w>До каких пор</w> мы должны терпеть это поведение?"
                            }
                        }
                    ],
                    "synonyms": [
                        "jak długo",
                        "jak długo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вопросительное слово, используемое для вопроса о продолжительности действия; до какого времени, как долго."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до каких пор",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "сколько",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
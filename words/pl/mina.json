                    "sense_id": "a3c9b8cb-4962-3c24-90ce-3d49e72acd8e",
                    "sense_definition": "Wyraz twarzy odzwierciedlający emocje, nastrój lub reakcję na coś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "facial_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy usłyszał te wieści, jego <w>mina</w> natychmiast posmutniała.",
                            "target_lang_translations": {
                                "ru": "Когда он услышал эти новости, <w>выражение</w> его <w>лица</w> сразу стало грустным."
                            }
                        },
                        {
                            "text": "Mimo problemów starał się robić dobrą <w>minę</w> do złej gry.",
                            "target_lang_translations": {
                                "ru": "Несмотря на проблемы, он старался делать хорошую <w>мину</w> при плохой игре."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyraz twarzy",
                        "grymas"
                    ],
                    "common_phrases": [
                        "robić dobrą minę do złej gry",
                        "stroić miny",
                        "kwaśna mina",
                        "mieć nietęgą minę",
                        "zrzedła mu mina"
                    ],
                    "target_lang_definitions": {

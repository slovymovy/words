    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "87bfefaa-522a-3e1a-936e-6cb146b94ba8",
                    "sense_definition": "In elkaar gesponnen vezels, zoals van wol of katoen, gebruikt om te naaien of te weven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "textile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze probeerde de <w>draad</w> door het oog van de naald te steken.",
                            "target_lang_translations": {
                                "ru": "Она пыталась продеть <w>нитку</w> в игольное ушко."
                            }
                        },
                        {
                            "text": "Er zit een losse <w>draad</w> aan je trui die je beter kunt afknippen.",
                            "target_lang_translations": {
                                "ru": "На твоем свитере торчит <w>нитка</w>, которую лучше отрезать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naald en draad",
                        "aan een zijden draadje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скрученные волокна, например шерсти или хлопка, используемые для шитья или ткачества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нитка",
                                "target_lang_sense_clarification": "для шитья"
                            },
                            {
                                "target_lang_word": "нить",
                                "target_lang_sense_clarification": "материал/волокно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b36fd25f-af46-3980-9f85-98005e97beff",
                    "sense_definition": "Een lang, dun en buigbaar stuk metaal, vaak gebruikt om elektriciteit te geleiden of dingen vast te binden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De elektricien verving de oude <w>draad</w> in de muur omdat deze onveilig was.",
                            "target_lang_translations": {
                                "ru": "Электрик заменил старый <w>провод</w> в стене, так как он был небезопасен."
                            }
                        },
                        {
                            "text": "Met een stukje ijzeren <w>draad</w> maakte hij het hek weer stevig vast.",
                            "target_lang_translations": {
                                "ru": "Куском железной <w>проволоки</w> он снова прочно закрепил забор."
                            }
                        }
                    ],
                    "synonyms": [
                        "snoer"
                    ],
                    "common_phrases": [
                        "koperen draad",
                        "onder stroom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный, тонкий и гибкий кусок металла, часто используемый для проведения электричества или связывания предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проволока",
                                "target_lang_sense_clarification": "металлическая"
                            },
                            {
                                "target_lang_word": "провод",
                                "target_lang_sense_clarification": "электрический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "befe4aec-2d32-370a-87dd-d1940be9c9fc",
                    "sense_definition": "De logische lijn of samenhang in een verhaal, gesprek of betoog.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de vele onderbrekingen raakte de spreker de <w>draad</w> van zijn verhaal kwijt.",
                            "target_lang_translations": {
                                "ru": "Из-за множества прерываний оратор потерял <w>нить</w> своего рассказа."
                            }
                        },
                        {
                            "text": "Na de pauze pakten we de <w>draad</w> van de vergadering weer op.",
                            "target_lang_translations": {
                                "ru": "После перерыва мы снова подхватили <w>нить</w> собрания."
                            }
                        }
                    ],
                    "synonyms": [
                        "verhaallijn",
                        "lijn"
                    ],
                    "common_phrases": [
                        "de draad kwijtraken",
                        "de draad oppakken",
                        "de rode draad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Логическая линия или связность в рассказе, разговоре или аргументации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нить",
                                "target_lang_sense_clarification": "повествования/мысли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3979e35d-e198-32ce-95ce-14d2b857c1e3",
                    "sense_definition": "Een reeks opeenvolgende berichten over een specifiek onderwerp op een internetforum of in e-mail.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "internet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb een interessante <w>draad</w> gelezen op het forum over tuinieren.",
                            "target_lang_translations": {
                                "ru": "Я прочитал интересную <w>ветку</w> на форуме о садоводстве."
                            }
                        },
                        {
                            "text": "Reageer alsjeblieft in de juiste <w>draad</w> om het overzichtelijk te houden.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отвечай в правильной <w>ветке</w>, чтобы сохранять порядок."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Серия последовательных сообщений на определенную тему на интернет-форуме или в электронной почте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ветка",
                                "target_lang_sense_clarification": "обсуждения"
                            },
                            {
                                "target_lang_word": "тред",
                                "target_lang_sense_clarification": "интернет-сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ecd93f7-2e73-31ae-ad89-1a5c353177fb",
                    "sense_definition": "De spiraalvormige groef op een schroef, bout of moer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>draad</w> van deze bout is beschadigd, waardoor de moer er niet meer op past.",
                            "target_lang_translations": {
                                "ru": "<w>Резьба</w> этого болта повреждена, поэтому гайка больше не накручивается."
                            }
                        },
                        {
                            "text": "Je moet voorzichtig draaien om de <w>draad</w> niet kapot te maken.",
                            "target_lang_translations": {
                                "ru": "Нужно крутить осторожно, чтобы не сорвать <w>резьбу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "schroefdraad"
                    ],
                    "common_phrases": [
                        "metrische draad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спиральная канавка на винте, болте или гайке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резьба",
                                "target_lang_sense_clarification": "винтовая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "030403f2-d5de-3c79-8069-ac9039b4af9e",
                    "sense_definition": "Een oude bijnaam voor een radiotelegrafist.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>draad</w> aan boord van het schip stuurde het dringende bericht naar de wal.",
                            "target_lang_translations": {
                                "ru": "<w>Радист</w> на борту корабля отправил срочное сообщение на берег."
                            }
                        },
                        {
                            "text": "Vroeger werd een marconist door de bemanning vaak de <w>draad</w> genoemd.",
                            "target_lang_translations": {
                                "ru": "Раньше экипаж часто называл маркониста <w>радистом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "vonk",
                        "vonkenboer"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historisch"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старое прозвище радиотелеграфиста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "радист",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "43b7885b-f04b-364e-a8af-82a1da7c5415",
                    "sense_definition": "De dodelijke elektrische versperring op de grens tussen België en Nederland tijdens de Eerste Wereldoorlog.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "history",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "Veel mensen probeerden te vluchten, maar <w>De Draad</w> maakte dat levensgevaarlijk.",
                            "target_lang_translations": {
                                "ru": "Многие люди пытались бежать, но <w>Проволока смерти</w> делала это смертельно опасным."
                            }
                        },
                        {
                            "text": "In het museum leerden we over de slachtoffers die vielen bij <w>De Draad</w>.",
                            "target_lang_translations": {
                                "ru": "В музее мы узнали о жертвах, погибших на <w>Проволоке смерти</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historisch"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Смертельное электрическое заграждение на границе между Бельгией и Нидерландами во время Первой мировой войны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Проволока смерти",
                                "target_lang_sense_clarification": "историческое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
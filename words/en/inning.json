    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "694dc647-f997-3e47-aeb3-94c2138bbbfc",
                    "sense_definition": "A division of a baseball game during which both teams have a turn at batting.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "sports_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The home team managed to score three runs in the bottom of the ninth <w>inning</w>.",
                            "target_lang_translations": {
                                "ru": "Хозяевам поля удалось заработать три очка в нижней части девятого <w>иннинга</w>."
                            }
                        },
                        {
                            "text": "Fans usually stand up and sing during the seventh-inning stretch of a baseball <w>inning</w>.",
                            "target_lang_translations": {
                                "ru": "Болельщики обычно встают и поют во время традиционной паузы в середине седьмого <w>иннинга</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "period",
                        "round"
                    ],
                    "common_phrases": [
                        "seventh-inning stretch",
                        "top of the inning",
                        "bottom of the inning",
                        "extra innings"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть бейсбольного матча, в которой обе команды по очереди играют в нападении и защите"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иннинг",
                                "target_lang_sense_clarification": "период в бейсболе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "706581e8-6090-3cf2-9ee9-dc208f445f8e",
                    "sense_definition": "A specific stage or period of opportunity in a process or event, often used metaphorically.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are still in the early <w>innings</w> of developing this revolutionary technology.",
                            "target_lang_translations": {
                                "ru": "Мы все еще находимся на ранних <w>стадиях</w> разработки этой революционной технологии."
                            }
                        },
                        {
                            "text": "The company is entering the final <w>inning</w> of its negotiations with the new partner.",
                            "target_lang_translations": {
                                "ru": "Компания вступает в завершающий <w>этап</w> переговоров с новым партнером."
                            }
                        }
                    ],
                    "synonyms": [
                        "phase",
                        "stage",
                        "chapter"
                    ],
                    "common_phrases": [
                        "early innings",
                        "late innings",
                        "final inning"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный этап или период возможностей в процессе или событии (часто используется метафорически)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "этап",
                                "target_lang_sense_clarification": "стадия процесса"
                            },
                            {
                                "target_lang_word": "стадия",
                                "target_lang_sense_clarification": "фаза развития"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e39a008d-0819-3238-9d3c-9afd67c41565",
                    "sense_definition": "A player's turn to make shots in games like billiards or snooker.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He accumulated fifty points in a single <w>inning</w> before missing a difficult shot.",
                            "target_lang_translations": {
                                "ru": "Он набрал пятьдесят очков за один <w>подход</w>, прежде чем промахнулся при сложном ударе."
                            }
                        },
                        {
                            "text": "The player waited patiently for his next <w>inning</w> at the table.",
                            "target_lang_translations": {
                                "ru": "Игрок терпеливо ждал своего следующего <w>подхода</w> к столу."
                            }
                        }
                    ],
                    "synonyms": [
                        "turn",
                        "visit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очередь игрока совершать удары в таких играх, как бильярд или снукер"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подход",
                                "target_lang_sense_clarification": "очередь удара в бильярде"
                            }
                        ]
                    }
                }
            ]
        },
        {

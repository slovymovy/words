    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "34bae96d-f5d6-3651-ba5b-e4970474ddfb",
                    "sense_definition": "Перемещаться в разных направлениях или регулярно с помощью транспортного средства (в отличие от ходьбы пешком).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой отец каждый день вынужден <ездить> на работу на старой машине.",
                            "target_lang_translations": {
                                "en": "My father has to <drive> to work every day in an old car."
                            }
                        },
                        {
                            "text": "Летом мы очень любим <ездить> к бабушке в деревню на поезде.",
                            "target_lang_translations": {
                                "en": "In the summer, we really love to <go> to grandma's in the village by train."
                            }
                        }
                    ],
                    "synonyms": [
                        "кататься",
                        "путешествовать"
                    ],
                    "antonyms": [
                        "ходить"
                    ],
                    "common_phrases": [
                        "ездить на работу",
                        "ездить на машине",
                        "ездить в отпуск",
                        "часто ездить"
                    ],
                    "target_lang_definitions": {
                        "en": "To move in different directions or regularly using a vehicle (as opposed to walking)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "by vehicle"
                            },
                            {
                                "target_lang_word": "travel",
                                "target_lang_sense_clarification": "general movement"
                            },
                            {
                                "target_lang_word": "commute",
                                "target_lang_sense_clarification": "regularly to work"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9ba9962-521a-3df4-a2c5-b5ff8e83b2fa",
                    "sense_definition": "Уметь управлять транспортным средством или животным; передвигаться верхом или за рулём.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "motion_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В детстве брат научил меня <ездить> на двухколёсном велосипеде.",
                            "target_lang_translations": {
                                "en": "In childhood, my brother taught me how to <ride> a two-wheeled bicycle."
                            }
                        },
                        {
                            "text": "Она прекрасно умеет <ездить> верхом и часто участвует в соревнованиях.",
                            "target_lang_translations": {
                                "en": "She knows how to <ride> a horse perfectly and often participates in competitions."
                            }
                        }
                    ],
                    "synonyms": [
                        "управлять",
                        "водить"
                    ],
                    "common_phrases": [
                        "ездить верхом",
                        "ездить на велосипеде",
                        "уметь ездить"
                    ],
                    "target_lang_definitions": {
                        "en": "To be able to control a vehicle or animal; to move on horseback or behind the wheel."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ride",
                                "target_lang_sense_clarification": "a bike or horse"
                            },
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "a car"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "813d5724-8373-3b60-bafa-a1f0476c8a8b",
                    "sense_definition": "Скользить, сдвигаться с места, быть плохо закреплённым (о предметах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sliding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эти новые очки постоянно <ездят> по носу, когда я наклоняю голову.",
                            "target_lang_translations": {
                                "en": "These new glasses constantly <slide> down my nose when I tilt my head."
                            }
                        },
                        {
                            "text": "Тяжёлый рюкзак неудобно <ездит> по спине во время быстрой ходьбы.",
                            "target_lang_translations": {
                                "en": "The heavy backpack <slides> uncomfortably on my back during a fast walk."
                            }
                        }
                    ],
                    "synonyms": [
                        "скользить",
                        "съезжать",
                        "ерзать"
                    ],
                    "common_phrases": [
                        "ездить по столу",
                        "ездить по спине"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To slide, move out of place, or be loose (referring to objects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slide",
                                "target_lang_sense_clarification": "move smoothly against a surface"
                            },
                            {
                                "target_lang_word": "slip",
                                "target_lang_sense_clarification": "move out of position"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31d0e849-ae85-3a54-81a7-07624ab94c39",
                    "sense_definition": "Использовать кого-либо в своих интересах, заставлять выполнять чужую работу (обычно в выражении «ездить на ком-то»).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "exploitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты слишком добрый, поэтому твои коллеги начали на тебе <ездить>.",
                            "target_lang_translations": {
                                "en": "You are too kind, so your colleagues have started to <take advantage of> you."
                            }
                        },
                        {
                            "text": "Не позволяй другим людям <ездить> на себе и решать свои проблемы твоими руками.",
                            "target_lang_translations": {
                                "en": "Don't let other people <walk all over> you and solve their problems with your hands."
                            }
                        }
                    ],
                    "synonyms": [
                        "эксплуатировать",
                        "использовать"
                    ],
                    "common_phrases": [
                        "ездить на шее",
                        "на нём ездят"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "figurative usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To use someone for one's own interests, to force someone to do one's work (figurative)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take advantage of",
                                "target_lang_sense_clarification": "exploit someone"
                            },
                            {
                                "target_lang_word": "walk all over",
                                "target_lang_sense_clarification": "treat with disrespect"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
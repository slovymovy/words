                },
                {
                    "sense_id": "f7fbc54e-349f-3a0f-99d3-3dcf643a98f3",
                    "sense_definition": "Niewielka wieś w Polsce położona w województwie łódzkim.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "places",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Kiedyś <w>Rawicz</w> należał do województwa piotrkowskiego.",
                            "target_lang_translations": {
                                "ru": "Когда-то <w>Равич</w> относился к Пётрковскому воеводству."

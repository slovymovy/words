{
    "word_family": [
        "somebody",
        "someone",
        "something",
        "somehow",
        "somewhat",
        "somewhere",
        "sometime",
        "sometimes"
    ],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "18db3ab9-61ac-372d-b5e7-857a21d8ad09",
                    "sense_definition": "An unspecified number of countable people or things.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "indefinite_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I bought <w>some</w> apples from the market.",
                            "target_lang_translations": {
                                "ru": "Я купил <w>несколько</w> яблок на рынке."
                            }
                        },
                        {
                            "text": "There are <w>some</w> books on the table that you might like.",
                            "target_lang_translations": {
                                "ru": "На столе есть <w>несколько</w> книг, которые могут тебе понравиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "a few",
                        "several",
                        "a number of"
                    ],
                    "antonyms": [
                        "no",
                        "many"
                    ],
                    "common_phrases": [
                        "some people",
                        "some days",
                        "some things"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неопределенное количество исчисляемых людей или предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не́сколько",
                                "target_lang_sense_clarification": "небольшое число"
                            },
                            {
                                "target_lang_word": "немно́го",
                                "target_lang_sense_clarification": "малое количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "061ae817-f1bb-310b-a6a1-47908082d4ba",
                    "sense_definition": "An unspecified amount of something uncountable.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "indefinite_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Would you like <w>some</w> water with your meal?",
                            "target_lang_translations": {
                                "ru": "Хочешь <w>немного</w> воды за едой?"
                            }
                        },
                        {
                            "text": "I need <w>some</w> help with this heavy box.",
                            "target_lang_translations": {
                                "ru": "Мне нужна <w>кое-какая</w> помощь с этой тяжелой коробкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "a little",
                        "a bit of"
                    ],
                    "antonyms": [
                        "no",
                        "much"
                    ],
                    "common_phrases": [
                        "some water",
                        "some time",
                        "some help",
                        "some money"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неопределенное количество чего-то неисчисляемого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "немно́го",
                                "target_lang_sense_clarification": "небольшое количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74ca4932-cb50-3ed0-b091-be872b1724e0",
                    "sense_definition": "Used to refer to a person or thing that is not known or specified.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "unknown_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Some</w> guy called for you while you were out.",
                            "target_lang_translations": {
                                "ru": "<w>Какой-то</w> парень звонил тебе, пока тебя не было."
                            }
                        },
                        {
                            "text": "It must be in <w>some</w> drawer, but I cannot remember which one.",
                            "target_lang_translations": {
                                "ru": "Это должно быть в <w>каком-то</w> ящике, но я не помню, в каком."
                            }
                        }
                    ],
                    "synonyms": [
                        "a certain"
                    ],
                    "common_phrases": [
                        "some guy",
                        "some reason",
                        "some way"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения человека или предмета, который неизвестен или не уточняется."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "како́й-то",
                                "target_lang_sense_clarification": "неизвестный"
                            },
                            {
                                "target_lang_word": "не́кий",
                                "target_lang_sense_clarification": "определенный, но неизвестный"
                            },
                            {
                                "target_lang_word": "како́й-нибудь",
                                "target_lang_sense_clarification": "любой, безразлично какой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93c38bfe-9c87-39eb-b7f9-ffe062f1a96f",
                    "sense_definition": "A considerable quantity or number of something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "considerable_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He lived in that small village for <w>some</w> years.",
                            "target_lang_translations": {
                                "ru": "Он прожил в той маленькой деревне <w>несколько</w> лет."
                            }
                        },
                        {
                            "text": "It took <w>some</w> time to finish the project completely.",
                            "target_lang_translations": {
                                "ru": "Потребовалось <w>некоторое</w> время, чтобы полностью закончить проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "a fair amount of",
                        "quite a few"
                    ],
                    "antonyms": [
                        "no",
                        "zero#Determiner"
                    ],
                    "common_phrases": [
                        "for some time",
                        "some years ago"
                    ],
                    "target_lang_definitions": {
                        "ru": "Значительное количество или число чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не́сколько",
                                "target_lang_sense_clarification": "значительное число"
                            },
                            {
                                "target_lang_word": "не́который",
                                "target_lang_sense_clarification": "определенный промежуток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9778477f-56bc-3563-9b1e-b02280a4dd46",
                    "sense_definition": "Used to emphasize that someone or something is impressive or remarkable.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That was <w>some</w> party you threw last night!",
                            "target_lang_translations": {
                                "ru": "<w>Ну и</w> вечеринку ты закатил вчера вечером!"
                            }
                        },
                        {
                            "text": "He is <w>some</w> player to watch on the field.",
                            "target_lang_translations": {
                                "ru": "Он <w>выдающийся</w> игрок, за которым стоит наблюдать на поле."
                            }
                        }
                    ],
                    "synonyms": [
                        "quite a",
                        "an impressive"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для подчеркивания того, что кто-то или что-то является впечатляющим или замечательным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вот э́то",
                                "target_lang_sense_clarification": "для усиления"
                            },
                            {
                                "target_lang_word": "ну и",
                                "target_lang_sense_clarification": "разговорное усиление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "c02f1a45-56ab-352a-8b99-baf40d766063",
                    "sense_definition": "A certain number of people or things from a larger group, often used to contrast with others.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "subset_contrast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Some</w> enjoy spicy food, while others prefer it mild.",
                            "target_lang_translations": {
                                "ru": "<w>Некоторые</w> любят острую еду, в то время как другие предпочитают неострую."
                            }
                        },
                        {
                            "text": "<w>Some</w> say it is true, but I doubt it.",
                            "target_lang_translations": {
                                "ru": "<w>Некоторые</w> говорят, что это правда, но я сомневаюсь."
                            }
                        }
                    ],
                    "synonyms": [
                        "a few"
                    ],
                    "antonyms": [
                        "none",
                        "many"
                    ],
                    "common_phrases": [
                        "some say",
                        "some agree"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенное количество людей или вещей из большей группы, часто используется для противопоставления другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не́которые",
                                "target_lang_sense_clarification": "часть группы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea5730fa-84ec-37d6-a0ea-4ff0704edbff",
                    "sense_definition": "An indefinite quantity or part of something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "indefinite_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have plenty of paper, so please take <w>some</w>.",
                            "target_lang_translations": {
                                "ru": "У меня много бумаги, так что, пожалуйста, возьми <w>немного</w>."
                            }
                        },
                        {
                            "text": "If you want cake, you can have <w>some</w> of mine.",
                            "target_lang_translations": {
                                "ru": "Если хочешь торта, можешь взять <w>немного</w> моего."
                            }
                        }
                    ],
                    "synonyms": [
                        "a part"
                    ],
                    "antonyms": [
                        "none"
                    ],
                    "common_phrases": [
                        "some of",
                        "have some"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неопределенное количество или часть чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "немно́го",
                                "target_lang_sense_clarification": "небольшая часть"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c250083e-e2a8-3a43-9b8e-dbdee7c419f8",
                    "sense_definition": "Approximately or roughly; used before a number or measurement.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "approximation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He weighs <w>some</w> 90 kilos after the diet.",
                            "target_lang_translations": {
                                "ru": "Он весит <w>около</w> 90 килограммов после диеты."
                            }
                        },
                        {
                            "text": "<w>Some</w> thirty people attended the meeting yesterday.",
                            "target_lang_translations": {
                                "ru": "<w>Примерно</w> тридцать человек посетили собрание вчера."
                            }
                        }
                    ],
                    "synonyms": [
                        "approximately",
                        "roughly",
                        "about",
                        "around"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приблизительно или грубо; используется перед числом или измерением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о́коло",
                                "target_lang_sense_clarification": "приблизительно"
                            },
                            {
                                "target_lang_word": "приме́рно",
                                "target_lang_sense_clarification": "примерно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a113bf81-c599-3141-8815-5e0b527f1bef",
                    "sense_definition": "To some extent; somewhat or a little.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I feel <w>some</w> better after taking the medicine.",
                            "target_lang_translations": {
                                "ru": "Я чувствую себя <w>немного</w> лучше после приема лекарства."
                            }
                        },
                        {
                            "text": "It looks <w>some</w> like his father, but not exactly.",
                            "target_lang_translations": {
                                "ru": "Он <w>немного</w> похож на своего отца, но не совсем."
                            }
                        }
                    ],
                    "synonyms": [
                        "somewhat",
                        "a little",
                        "slightly"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often dialectal US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В некоторой степени; немного или слегка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "немно́го",
                                "target_lang_sense_clarification": "слегка"
                            },
                            {
                                "target_lang_word": "не́сколько",
                                "target_lang_sense_clarification": "в некоторой степени"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
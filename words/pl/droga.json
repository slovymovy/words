                },
                {
                    "sense_id": "8b8b1148-a092-3657-bc69-a2abd51c1af0",
                    "sense_definition": "Długość toru, jaki pokonuje ciało fizyczne w ruchu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ruchu jednostajnym <droga> jest iloczynem prędkości i czasu.",
                            "target_lang_translations": {
                                "ru": "При равномерном движении пройденный <путь> равен произведению скорости на время."
                            }
                        },
                        {
                            "text": "<Droga> hamowania samochodu wydłuża się na mokrej nawierzchni.",
                            "target_lang_translations": {
                                "ru": "Тормозной <путь> автомобиля увеличивается на мокром покрытии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "droga hamowania",
                        "prędkość i droga"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Длина траектории, которую преодолевает физическое тело в движении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путь",

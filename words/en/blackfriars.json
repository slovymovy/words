            "senses": [
                {
                    "sense_id": "b3207f3b-db4e-3e8f-9d9e-66026fb7ffc2",
                    "sense_definition": "An area within the City of London, England, named after a former monastery of Dominican friars.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "place_london",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We took the train to <w>Blackfriars</w> station to see the river.",
                            "target_lang_translations": {
                                "ru": "Мы поехали на поезде до станции <w>Блэкфрайарс</w>, чтобы посмотреть на реку."
                            }
                        },
                        {
                            "text": "The office is located just a short walk from <w>Blackfriars</w> Bridge.",
                            "target_lang_translations": {
                                "ru": "Офис расположен всего в нескольких минутах ходьбы от моста <w>Блэкфрайарс</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Район в лондонском Сити, названный в честь бывшего монастыря монахов-доминиканцев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Блэкфрайарс",
                                "target_lang_sense_clarification": "район Лондона"
                            }

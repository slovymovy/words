                        ]
                    }
                },
                {
                    "sense_id": "92f90e18-7a89-360b-96eb-cf6d737aa77e",
                    "sense_definition": "A person who works in a job that requires special education and training.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "career_standards",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many medical <w>professionals</w> work long hours in the hospital emergency room.",
                            "target_lang_translations": {
                                "ru": "Многие медицинские <w>специалисты</w> работают долгие часы в отделении скорой помощи."
                            }
                        },
                        {
                            "text": "We need to consult with a legal <w>professional</w> to understand the contract terms.",
                            "target_lang_translations": {
                                "ru": "Нам нужно проконсультироваться с <w>юристом</w> (специалистом), чтобы понять условия контракта."
                            }
                        }
                    ],
                    "synonyms": [
                        "practitioner",
                        "specialist"
                    ],
                    "antonyms": [
                        "layman"
                    ],
                    "common_phrases": [
                        "health professional",
                        "medical professional",
                        "legal professional",
                        "young professional"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, работающий на должности, требующей специального образования и подготовки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "специалист",
                                "target_lang_sense_clarification": "квалифицированный работник"
                            },
                            {
                                "target_lang_word": "профессионал",
                                "target_lang_sense_clarification": "работник определенной сферы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ce3051b-a5d4-3835-86bd-9a60a11f5d73",
                    "sense_definition": "Someone who has a lot of skill and experience in a particular activity.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "skill_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a real <w>professional</w> when it comes to organizing large corporate events.",
                            "target_lang_translations": {
                                "ru": "Она настоящий <w>профессионал</w>, когда дело доходит до организации крупных корпоративных мероприятий."
                            }
                        },
                        {
                            "text": "You can tell he is a <w>professional</w> by the way he handles the tools.",
                            "target_lang_translations": {
                                "ru": "Сразу видно, что он <w>профессионал</w>, по тому, как он обращается с инструментами."
                            }
                        }
                    ],
                    "synonyms": [
                        "expert",
                        "master",
                        "pro"
                    ],
                    "antonyms": [
                        "novice",
                        "beginner"
                    ],
                    "common_phrases": [
                        "true professional",
                        "real professional",
                        "consummate professional"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто обладает большим мастерством и опытом в определенной деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "профессионал",
                                "target_lang_sense_clarification": "знаток своего дела"
                            },
                            {
                                "target_lang_word": "мастер",
                                "target_lang_sense_clarification": "виртуоз"
                            },
                            {
                                "target_lang_word": "эксперт",
                                "target_lang_sense_clarification": "знаток"
                            }
                        ]

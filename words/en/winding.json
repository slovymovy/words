{
    "word_family": [
        "unwind",
        "rewind",
        "winder"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2f00585a-2287-3a6b-9459-23cbf87f51c7",
                    "sense_definition": "Having a curving and twisting shape or course; not straight.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "twisting_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We drove carefully along the <w>winding</w> road that led up the mountain.",
                            "target_lang_translations": {
                                "ru": "Мы осторожно ехали по <w>извилистой</w> дороге, ведущей в гору."
                            }
                        },
                        {
                            "text": "The river follows a <w>winding</w> course through the valley.",
                            "target_lang_translations": {
                                "ru": "Река течет по <w>извилистому</w> руслу через долину."
                            }
                        }
                    ],
                    "synonyms": [
                        "meandering",
                        "tortuous"
                    ],
                    "antonyms": [
                        "straight",
                        "direct"
                    ],
                    "common_phrases": [
                        "winding road",
                        "winding path",
                        "winding river",
                        "long winding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий изогнутую и извилистую форму или курс; не прямой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извилистый",
                                "target_lang_sense_clarification": "о дороге, реке или пути"
                            },
                            {
                                "target_lang_word": "петляющий",
                                "target_lang_sense_clarification": "делающий петли, меняющий направление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d2729b2-dfe6-3711-9dc9-3024a0a50135",
                    "sense_definition": "Of a staircase: arranged in a spiral or helical shape.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "twisting_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They climbed the narrow <w>winding</w> stairs to reach the top of the tower.",
                            "target_lang_translations": {
                                "ru": "Они поднялись по узкой <w>винтовой</w> лестнице, чтобы добраться до вершины башни."
                            }
                        },
                        {
                            "text": "The old castle is famous for its stone <w>winding</w> staircase.",
                            "target_lang_translations": {
                                "ru": "Старый замок славится своей каменной <w>винтовой</w> лестницей."
                            }
                        }
                    ],
                    "synonyms": [
                        "circuitous",
                        "tortuous"
                    ],
                    "antonyms": [
                        "straight"
                    ],
                    "common_phrases": [
                        "winding staircase",
                        "winding stairs"
                    ],
                    "target_lang_definitions": {
                        "ru": "О лестнице: устроенная в форме спирали или винта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "винтовой",
                                "target_lang_sense_clarification": "о лестнице"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d43eda9-f799-3d30-a364-9ab58b3d24b7",
                    "sense_definition": "Of speech or writing: not direct, simple, or to the point; complicated and rambling.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "indirect_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor gave a long, <w>winding</w> explanation that confused the students.",
                            "target_lang_translations": {
                                "ru": "Профессор дал длинное, <w>витиеватое</w> объяснение, которое сбило студентов с толку."
                            }
                        },
                        {
                            "text": "I could not follow the <w>winding</w> logic of his argument.",
                            "target_lang_translations": {
                                "ru": "Я не мог уследить за <w>путанной</w> логикой его аргументов."
                            }
                        }
                    ],
                    "synonyms": [
                        "indirect",
                        "circuitous",
                        "circumlocutionary"
                    ],
                    "antonyms": [
                        "direct",
                        "straightforward"
                    ],
                    "target_lang_definitions": {
                        "ru": "О речи или письме: непрямой, сложный и путанный; многословный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "витиеватый",
                                "target_lang_sense_clarification": "сложный, украшенный лишними деталями"
                            },
                            {
                                "target_lang_word": "путанный",
                                "target_lang_sense_clarification": "нелогичный, сбивающий с толку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40f91882-3ec5-3367-b5e7-577cd3e71ef2",
                    "sense_definition": "Causing someone to be out of breath; physically exhausting.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "breath_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boxer delivered a <w>winding</w> blow to his opponent's stomach.",
                            "target_lang_translations": {
                                "ru": "Боксер нанес противнику <w>сбивающий дыхание</w> удар в живот."
                            }
                        },
                        {
                            "text": "It was a <w>winding</w> climb to the summit, leaving us gasping for air.",
                            "target_lang_translations": {
                                "ru": "Это был <w>изнурительный</w> подъем на вершину, заставивший нас хватать ртом воздух."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий потерю дыхания; физически изнуряющий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбивающий дыхание",
                                "target_lang_sense_clarification": "об ударе или физической нагрузке"
                            },
                            {
                                "target_lang_word": "изнурительный",
                                "target_lang_sense_clarification": "утомительный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1523073b-9f54-3953-abf5-26bea1af466e",
                    "sense_definition": "A curving, sinuous, or twisting form, movement, or section of something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "twisting_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boat navigated the many <w>windings</w> of the stream.",
                            "target_lang_translations": {
                                "ru": "Лодка лавировала между многочисленными <w>изгибами</w> ручья."
                            }
                        },
                        {
                            "text": "We followed the <w>windings</w> of the trail deep into the woods.",
                            "target_lang_translations": {
                                "ru": "Мы следовали за <w>поворотами</w> тропы вглубь леса."
                            }
                        }
                    ],
                    "synonyms": [
                        "meandering",
                        "withe",
                        "withy"
                    ],
                    "common_phrases": [
                        "windings of the river",
                        "windings of the road"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изогнутая, извилистая форма, движение или участок чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изгиб",
                                "target_lang_sense_clarification": "поворот реки или дороги"
                            },
                            {
                                "target_lang_word": "поворот",
                                "target_lang_sense_clarification": "изменение направления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84228a0e-c9ef-3e02-81f8-84dda22bccea",
                    "sense_definition": "The act of twisting, coiling, or wrapping something around an object.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_coiling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>winding</w> of the wool into balls took all afternoon.",
                            "target_lang_translations": {
                                "ru": "<w>Сматывание</w> шерсти в клубки заняло весь день."
                            }
                        },
                        {
                            "text": "Proper <w>winding</w> of the bandage is important for support.",
                            "target_lang_translations": {
                                "ru": "Правильное <w>наложение</w> повязки важно для поддержки."
                            }
                        }
                    ],
                    "synonyms": [
                        "lapping"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по скручиванию, наматыванию или обертыванию чего-либо вокруг предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наматывание",
                                "target_lang_sense_clarification": "процесс накручивания"
                            },
                            {
                                "target_lang_word": "сматывание",
                                "target_lang_sense_clarification": "сбор нити в клубок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb853b0d-efdb-3f60-8d59-62e8309c7079",
                    "sense_definition": "A length of wire wrapped around a core, used in electrical components like motors or transformers.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "electrical_component",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The motor failed because the copper <w>windings</w> had overheated.",
                            "target_lang_translations": {
                                "ru": "Двигатель вышел из строя, потому что медные <w>обмотки</w> перегрелись."
                            }
                        },
                        {
                            "text": "Technicians checked the transformer <w>windings</w> for any breaks.",
                            "target_lang_translations": {
                                "ru": "Техники проверили <w>обмотки</w> трансформатора на наличие разрывов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "copper windings",
                        "motor windings"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отрезок проволоки, намотанный на сердечник, используемый в электрических компонентах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обмотка",
                                "target_lang_sense_clarification": "электротехнический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "446d8cd4-25bb-3115-83fb-d4208402ddd6",
                    "sense_definition": "The act of tightening the spring of a clockwork mechanism.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_mechanism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The daily <w>winding</w> of the grandfather clock was a family tradition.",
                            "target_lang_translations": {
                                "ru": "Ежедневный <w>завод</w> напольных часов был семейной традицией."
                            }
                        },
                        {
                            "text": "This toy requires manual <w>winding</w> to move.",
                            "target_lang_translations": {
                                "ru": "Эта игрушка требует ручного <w>завода</w>, чтобы двигаться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "winding up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по затягиванию пружины часового механизма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завод",
                                "target_lang_sense_clarification": "часов или механизма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c61c0acd-de2f-3b37-aedf-55a55fadaf60",
                    "sense_definition": "The act of blowing a horn or wind instrument to make a sound.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "breath_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We heard the <w>winding</w> of a horn in the distance.",
                            "target_lang_translations": {
                                "ru": "Мы услышали <w>трубление</w> рога вдалеке."
                            }
                        },
                        {
                            "text": "The hunt began with the loud <w>winding</w> of the bugle.",
                            "target_lang_translations": {
                                "ru": "Охота началась с громкого <w>сигнала</w> горна."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "or literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по дутью в рог или духовой инструмент для извлечения звука."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трубление",
                                "target_lang_sense_clarification": "звук рога или горна"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "9befdd82-0b82-376d-9700-8e3528b83d9e",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Winding</w> has lived in this town for many years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Виндинг</w> живет в этом городе уже много лет."
                            }
                        },
                        {
                            "text": "I saw the name <w>Winding</w> on the mailbox.",
                            "target_lang_translations": {
                                "ru": "Я увидел фамилию <w>Виндинг</w> на почтовом ящике."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "from Danish"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Виндинг",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
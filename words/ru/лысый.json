    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cdb61453-1914-36fe-aa23-e36f94d6df4a",
                    "sense_definition": "Не имеющий волос на голове.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_hair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой дедушка стал совсем <w>лысым</w> к шестидесяти годам.",
                            "target_lang_translations": {
                                "en": "My grandfather became completely <w>bald</w> by the time he was sixty."
                            }
                        },
                        {
                            "text": "<w>Лысым</w> тебе будет намного лучше, это сейчас модно.",
                            "target_lang_translations": {
                                "en": "You will look much better <w>bald</w>, it's fashionable now."
                            }
                        }
                    ],
                    "synonyms": [
                        "безволосый",
                        "плешивый"
                    ],
                    "antonyms": [
                        "волосатый"
                    ],
                    "common_phrases": [
                        "лысый мужчина",
                        "лысая голова",
                        "стать лысым"
                    ],
                    "target_lang_definitions": {
                        "en": "Having no hair on the head."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bald",
                                "target_lang_sense_clarification": "having little or no hair on the head"
                            },
                            {
                                "target_lang_word": "bald-headed",
                                "target_lang_sense_clarification": "having a bald head"
                            },
                            {
                                "target_lang_word": "hairless",
                                "target_lang_sense_clarification": "lacking hair"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb68ab34-9952-3796-b60f-7d315500b390",
                    "sense_definition": "Человек, не имеющий волос на голове (часто используется как прозвище или описание).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_hair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эй, <w>лысый</w>, ты забыл свою сумку на скамейке!",
                            "target_lang_translations": {
                                "en": "Hey, <w>baldy</w>, you left your bag on the bench!"
                            }
                        },
                        {
                            "text": "В комнату вошёл высокий <w>лысый</w> в чёрном костюме.",
                            "target_lang_translations": {
                                "en": "A tall <w>bald man</w> in a black suit entered the room."
                            }
                        }
                    ],
                    "synonyms": [
                        "плешивый"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used as a form of address or nickname"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person who has no hair on their head (often used as a nickname or description)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "baldy",
                                "target_lang_sense_clarification": "colloquial, often offensive or playful nickname"
                            },
                            {
                                "target_lang_word": "bald man",
                                "target_lang_sense_clarification": "descriptive term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78f39837-f98f-31a7-aa57-41d67cc77d0b",
                    "sense_definition": "О шинах: стёршийся, потерявший рельефный рисунок протектора.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "texture_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой очень опасно ездить на <w>лысой</w> резине.",
                            "target_lang_translations": {
                                "en": "It is very dangerous to drive on <w>bald</w> tires in winter."
                            }
                        },
                        {
                            "text": "У его старого велосипеда были совершенно <w>лысые</w> покрышки.",
                            "target_lang_translations": {
                                "en": "His old bicycle had completely <w>bald</w> tires."
                            }
                        }
                    ],
                    "synonyms": [
                        "голый"
                    ],
                    "common_phrases": [
                        "лысая резина",
                        "лысые покрышки"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common driver slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Of tires: worn smooth, having lost the tread pattern."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bald",
                                "target_lang_sense_clarification": "of tires whose surface is worn away"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a97277ca-9f55-3d56-a461-431934de5b9c",
                    "sense_definition": "Лишённый растительности (о местности) или ворса (о ткани).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "texture_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы поднялись на <w>лысую</w> гору, чтобы увидеть закат.",
                            "target_lang_translations": {
                                "en": "The tourists climbed the <w>bald</w> mountain to see the sunset."
                            }
                        },
                        {
                            "text": "В коридоре лежал старый <w>лысый</w> ковёр, который давно пора выбросить.",
                            "target_lang_translations": {
                                "en": "In the hallway lay an old <w>threadbare</w> carpet that should have been thrown out long ago."
                            }
                        }
                    ],
                    "synonyms": [
                        "голый",
                        "оголённый",
                        "облезлый"
                    ],
                    "antonyms": [
                        "заросший"
                    ],
                    "common_phrases": [
                        "лысая гора",
                        "лысый холм"
                    ],
                    "target_lang_definitions": {
                        "en": "Lacking vegetation (of terrain) or nap (of fabric)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bald",
                                "target_lang_sense_clarification": "denuded of any covering"
                            },
                            {
                                "target_lang_word": "bare",
                                "target_lang_sense_clarification": "of terrain"
                            },
                            {
                                "target_lang_word": "threadbare",
                                "target_lang_sense_clarification": "of fabric/carpet"
                            }
                        ]
                    }
                }
            ]
        }
    ]

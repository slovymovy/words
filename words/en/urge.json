    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "10fb7238-f2b4-3fab-8fec-5e1b9744c4ac",
                    "sense_definition": "To strongly advise or try to persuade someone to do something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "persuasion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My parents <w>urged</w> me to finish my degree before traveling.",
                            "target_lang_translations": {
                                "ru": "Мои родители <w>убеждали</w> меня закончить учебу перед путешествием."
                            }
                        },
                        {
                            "text": "Police <w>urge</w> caution when driving on icy roads.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>призывает</w> к осторожности при вождении на обледенелых дорогах."
                            }
                        }
                    ],
                    "synonyms": [
                        "encourage",
                        "press",
                        "push",
                        "prompt",
                        "induce"
                    ],
                    "antonyms": [
                        "discourage",
                        "deter"
                    ],
                    "common_phrases": [
                        "urge someone to",
                        "urge caution",
                        "urge restraint"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво советовать или пытаться убедить кого-то сделать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убеждать",
                                "target_lang_sense_clarification": "склонять к действию доводами"
                            },
                            {
                                "target_lang_word": "призывать",
                                "target_lang_sense_clarification": "обращаться с просьбой или требованием"
                            },
                            {
                                "target_lang_word": "настаивать",
                                "target_lang_sense_clarification": "упорно требовать исполнения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbf2a962-a216-313d-88e1-3ca15f0b8cb7",
                    "sense_definition": "To recommend something strongly or present it as necessary.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "persuasion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The report <w>urged</w> immediate action to solve the housing crisis.",
                            "target_lang_translations": {
                                "ru": "В отчете <w>настаивалось</w> на немедленных действиях для решения жилищного кризиса."
                            }
                        },
                        {
                            "text": "Scientists <w>urge</w> the importance of preserving biodiversity.",
                            "target_lang_translations": {
                                "ru": "Ученые <w>настаивают</w> на важности сохранения биоразнообразия."
                            }
                        }
                    ],
                    "synonyms": [
                        "advocate",
                        "recommend",
                        "promote"
                    ],
                    "antonyms": [
                        "oppose"
                    ],
                    "common_phrases": [
                        "urge the importance",
                        "urge action",
                        "urge reform"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Настоятельно рекомендовать что-то или представлять это как необходимость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настаивать",
                                "target_lang_sense_clarification": "утверждать необходимость чего-либо"
                            },
                            {
                                "target_lang_word": "настоятельно рекомендовать",
                                "target_lang_sense_clarification": "давать строгий совет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2742fd4-9256-35ca-a02b-d86d01fa3b3f",
                    "sense_definition": "To force or drive a person or animal to move forward or faster.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rider <w>urged</w> his horse up the steep hill.",
                            "target_lang_translations": {
                                "ru": "Всадник <w>подгонял</w> свою лошадь вверх по крутому склону."
                            }
                        },
                        {
                            "text": "She <w>urged</w> the team forward despite the heavy rain.",
                            "target_lang_translations": {
                                "ru": "Она <w>подгоняла</w> команду вперед, несмотря на сильный дождь."
                            }

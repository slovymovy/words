{
    "word_family": [
        "hejka"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "14dc965a-e6da-3cd8-a6c8-7b83cf8353d5",
                    "sense_definition": "Nieformalny zwrot używany na powitanie kogoś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hej</w>, jak się dzisiaj czujesz?",
                            "target_lang_translations": {
                                "ru": "<w>Привет</w>, как ты себя сегодня чувствуешь?"
                            }
                        },
                        {
                            "text": "<w>Hej</w>, dawno się nie widzieliśmy, co u ciebie słychać?",
                            "target_lang_translations": {
                                "ru": "<w>Привет</w>, давно не виделись, как у тебя дела?"
                            }
                        }
                    ],
                    "synonyms": [
                        "cześć",
                        "hejka"
                    ],
                    "common_phrases": [
                        "no hej",
                        "wielkie hej"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal situations"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное приветствие, используемое при встрече."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привет",
                                "target_lang_sense_clarification": "универсальное неформальное приветствие"
                            },
                            {
                                "target_lang_word": "хей",
                                "target_lang_sense_clarification": "сленговое приветствие, заимствование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "163d4cd6-e10b-39b1-84cc-a41bf67228ef",
                    "sense_definition": "Okrzyk mający na celu zwrócenie czyjejś uwagi, ostrzeżenie lub wyrażenie emocji.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "attention_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hej</w>, patrz na ten wielki samolot na niebie!",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>, посмотри на тот огромный самолет в небе!"
                            }
                        },
                        {
                            "text": "<w>Hej</w>, nie wolno tutaj parkować samochodu.",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>, здесь нельзя парковать машину."
                            }
                        }
                    ],
                    "synonyms": [
                        "ej",
                        "halo",
                        "uwaga",
                        "patrz"
                    ],
                    "common_phrases": [
                        "hej ty",
                        "hej tam"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "can be impolite if used aggressively"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, используемый для привлечения внимания, предупреждения или выражения эмоций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эй",
                                "target_lang_sense_clarification": "оклик для привлечения внимания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35e540dd-48c8-35d3-a82d-80f317026461",
                    "sense_definition": "Słowo wyrażające potwierdzenie lub zgodę, używane regionalnie lub potocznie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "affirmation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pójdziesz z nami jutro do kina? – <w>Hej</w>, chętnie pójdę.",
                            "target_lang_translations": {
                                "ru": "Пойдешь с нами завтра в кино? – <w>Да</w>, с удовольствием пойду."
                            }
                        },
                        {
                            "text": "Czy to na pewno twoja książka? – <w>Hej</w>, moja.",
                            "target_lang_translations": {
                                "ru": "Это точно твоя книга? – <w>Да</w>, моя."
                            }
                        }
                    ],
                    "synonyms": [
                        "ano",
                        "tak",
                        "jo"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "usage similar to 'jo' or 'tak' in some regions"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, выражающее подтверждение или согласие (региональное или разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "подтверждение"
                            },
                            {
                                "target_lang_word": "ага",
                                "target_lang_sense_clarification": "разговорное согласие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
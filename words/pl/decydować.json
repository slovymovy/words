{
    "word_family": [
        "decyzja",
        "decydent",
        "zdecydowany",
        "zdecydowanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2a0249c8-9d37-39dc-9195-151e75d0cd49",
                    "sense_definition": "Podejmować świadomą decyzję, dokonywać wyboru lub wydawać postanowienie w jakiejś sprawie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "volition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor finansowy samodzielnie <decydował> o wszystkich ważnych zakupach w firmie.",
                            "target_lang_translations": {
                                "ru": "Финансовый директор самостоятельно <принимал решения> обо всех важных закупках в фирме."
                            }
                        },
                        {
                            "text": "Rodzice pozwalają mi <decydować>, jak chcę spędzić tegoroczne wakacje.",
                            "target_lang_translations": {
                                "ru": "Родители позволяют мне <решать>, как я хочу провести эти каникулы."
                            }
                        }
                    ],
                    "synonyms": [
                        "postanawiać",
                        "rozstrzygać"
                    ],
                    "antonyms": [
                        "wahać się"
                    ],
                    "common_phrases": [
                        "decydować o losie",
                        "decydować o przyszłości",
                        "decydować w sprawie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать сознательное решение, делать выбор или выносить постановление по какому-либо вопросу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решать",
                                "target_lang_sense_clarification": "делать выбор, обдумывать"
                            },
                            {
                                "target_lang_word": "принимать решение",
                                "target_lang_sense_clarification": "официально или осознанно делать выбор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab660572-2edb-369a-bc87-ac770158b226",
                    "sense_definition": "Podejmować ostateczne postanowienie o zrobieniu czegoś (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "volition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat bardzo długo <decydował się>, czy do niej zatelefonować.",
                            "target_lang_translations": {
                                "ru": "Мой брат очень долго <решался>, позвонить ли ей."
                            }
                        },
                        {
                            "text": "Wielu klientów <decyduje się> na zakup samochodu w systemie ratalnym.",
                            "target_lang_translations": {
                                "ru": "Многие клиенты <решаются> на покупку автомобиля в рассрочку."
                            }
                        }
                    ],
                    "synonyms": [
                        "postanawiać"
                    ],
                    "common_phrases": [
                        "decydować się na krok",
                        "decydować się na wyjazd",
                        "decydować się na ślub"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать окончательное решение сделать что-либо (часто после колебаний)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решаться",
                                "target_lang_sense_clarification": "на какой-либо поступок или действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fa51581-cb1c-3a5d-aab6-b62196614675",
                    "sense_definition": "Mieć kluczowy wpływ na przebieg lub wynik czegoś; być czynnikiem przesądzającym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "O wyborze najlepszej oferty w przetargu najczęściej <decyduje> niska cena.",
                            "target_lang_translations": {
                                "ru": "Выбор лучшего предложения на тендере чаще всего <определяет> низкая цена."
                            }
                        },
                        {
                            "text": "Szybkość reakcji kierowcy może <decydować> o bezpieczeństwie pasażerów.",
                            "target_lang_translations": {
                                "ru": "Скорость реакции водителя может <определять> безопасность пассажиров."
                            }
                        }
                    ],
                    "synonyms": [
                        "przesądzać",
                        "rozstrzygać",
                        "stanowić"
                    ],
                    "common_phrases": [
                        "decydować o wyniku",
                        "decydować o sukcesie",
                        "o wszystkim decydować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказывать ключевое влияние на ход или результат чего-либо; быть решающим фактором."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "определять",
                                "target_lang_sense_clarification": "обусловливать результат"
                            },
                            {
                                "target_lang_word": "решать",
                                "target_lang_sense_clarification": "иметь решающее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e01ef7d-0f44-36e7-8e9c-fbfb1370c29c",
                    "sense_definition": "Rozstrzygać się; stawać się pewnym lub wiadomym (zazwyczaj w odniesieniu do losu lub wyniku).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "event_outcome",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właśnie w tej chwili <decydują> się losy naszego awansu do finału.",
                            "target_lang_translations": {
                                "ru": "Именно в этот момент <решается> судьба нашего выхода в финал."
                            }
                        },
                        {
                            "text": "Podczas ostatnich minut meczu <decydował> się ostateczny wynik spotkania.",
                            "target_lang_translations": {
                                "ru": "В последние минуты матча <решался> окончательный исход встречи."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozstrzygać się, ważyć się"
                    ],
                    "common_phrases": [
                        "losy się decydują",
                        "wszystko się decyduje"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym „się” w znaczeniu biernym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрешаться; становиться определённым или известным (обычно о судьбе или исходе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решаться",
                                "target_lang_sense_clarification": "об исходе дела или судьбе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "1f775916-2fbf-3b58-9473-a362de708ce9",
                    "sense_definition": "Wskazuje na ruch z miejsca znajdującego się za czymś lub na zewnątrz czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słońce w końcu wyłoniło się <w>spoza</w> gęstych chmur.",
                            "target_lang_translations": {
                                "ru": "Солнце наконец показалось <w>из-за</w> густых облаков."
                            }
                        },
                        {
                            "text": "Tajemniczy głos dobiegał <w>spoza</w> zamkniętych drzwi.",
                            "target_lang_translations": {
                                "ru": "Таинственный голос доносился <w>из-за</w> закрытой двери."
                            }
                        }
                    ],
                    "synonyms": [
                        "zza"
                    ],
                    "common_phrases": [
                        "spoza chmur",
                        "patrzeć spoza okularów",
                        "wyjść spoza ukrycia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на движение из места, находящегося за чем-либо или снаружи чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "из-за",
                                "target_lang_sense_clarification": "движение из места позади объекта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b6b4f9f-e649-3b98-a3a9-d36d25250492",
                    "sense_definition": "Wskazuje na pochodzenie z zewnątrz jakiejś grupy, organizacji lub terytorium, oznaczając brak przynależności do nich.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "On jest <w>spoza</w> naszej szkoły, więc go nie znam.",
                            "target_lang_translations": {
                                "ru": "Он <w>не из</w> нашей школы, поэтому я его не знаю."
                            }
                        },
                        {
                            "text": "W konkursie wzięli udział uczniowie <w>spoza</w> naszego województwa.",
                            "target_lang_translations": {
                                "ru": "В конкурсе приняли участие ученики <w>из-за пределов</w> нашего воеводства."
                            }
                        }
                    ],
                    "antonyms": [
                        "z"
                    ],
                    "common_phrases": [
                        "ludzie spoza miasta",
                        "kandydat spoza układu",
                        "goście spoza rodziny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на происхождение извне какой-либо группы, организации или территории, означая отсутствие принадлежности к ним."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "из-за пределов",
                                "target_lang_sense_clarification": "извне территории или круга"
                            },
                            {
                                "target_lang_word": "не из",
                                "target_lang_sense_clarification": "отсутствие принадлежности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
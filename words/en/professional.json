    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "56f3ba58-5e74-3005-9e4f-61753cadfdd5",
                    "sense_definition": "Done as a job for money rather than as a hobby.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "paid_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He became a <w>professional</w> football player at the age of eighteen.",
                            "target_lang_translations": {
                                "ru": "Он стал <w>профессиональным</w> футболистом в возрасте восемнадцати лет."
                            }
                        },
                        {
                            "text": "Being a <w>professional</w> photographer requires expensive equipment and a lot of dedication.",
                            "target_lang_translations": {
                                "ru": "Работа <w>профессиональным</w> фотографом требует дорогого оборудования и большой самоотдачи."
                            }
                        }
                    ],
                    "synonyms": [
                        "paid"
                    ],
                    "antonyms": [
                        "amateur",
                        "unpaid"
                    ],
                    "common_phrases": [
                        "professional athlete",
                        "professional career",
                        "professional sport"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполняемый как работа за деньги, а не как хобби."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "профессиональный",
                                "target_lang_sense_clarification": "связанный с заработком, не любительский"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abf53041-2e3e-3a00-abf7-8b32a6352c7b",
                    "sense_definition": "Relating to a job that requires special education, training, or high standards.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "career_standards",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should seek <w>professional</w> advice before making such a big financial decision.",
                            "target_lang_translations": {
                                "ru": "Вам следует обратиться за <w>профессиональным</w> советом, прежде чем принимать такое важное финансовое решение."
                            }
                        },
                        {
                            "text": "The company maintains high <w>professional</w> standards in all its business dealings.",
                            "target_lang_translations": {
                                "ru": "Компания поддерживает высокие <w>профессиональные</w> стандарты во всех своих деловых отношениях."
                            }
                        }
                    ],
                    "synonyms": [
                        "vocational",
                        "occupational"
                    ],
                    "common_phrases": [
                        "professional development",
                        "professional advice",
                        "professional experience",
                        "professional qualification"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к работе, требующей специального образования, подготовки или высоких стандартов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "профессиональный",
                                "target_lang_sense_clarification": "относящийся к профессии, деловой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88b42222-6fd4-33e9-bcac-c63440c6a0b9",
                    "sense_definition": "Showing the high level of skill, competence, or polite behavior expected of a trained person.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "skill_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She did a very <w>professional</w> job painting the house; it looks brand new.",
                            "target_lang_translations": {
                                "ru": "Она выполнила очень <w>профессиональную</w> работу по покраске дома; он выглядит как новый."
                            }
                        },
                        {
                            "text": "His behavior during the difficult meeting was calm and <w>professional</w>.",
                            "target_lang_translations": {
                                "ru": "Его поведение во время сложной встречи было спокойным и <w>профессиональным</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "expert",
                        "competent",
                        "efficient",
                        "businesslike"
                    ],
                    "antonyms": [
                        "unprofessional",
                        "amateurish",
                        "incompetent"
                    ],
                    "common_phrases": [
                        "very professional",
                        "professional manner",
                        "professional look"
                    ],
                    "target_lang_definitions": {
                        "ru": "Демонстрирующий высокий уровень мастерства, компетентности или вежливого поведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "профессиональный",
                                "target_lang_sense_clarification": "мастерский, компетентный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "416922f5-8571-336c-9aad-8785a1929054",
                    "sense_definition": "A person who plays a sport or performs an activity as a paid job.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "paid_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tennis tournament is open to both amateurs and <w>professionals</w>.",
                            "target_lang_translations": {
                                "ru": "Теннисный турнир открыт как для любителей, так и для <w>профессионалов</w>."
                            }
                        },
                        {
                            "text": "He turned <w>professional</w> after winning the gold medal in the Olympics.",
                            "target_lang_translations": {
                                "ru": "Он стал <w>профессионалом</w> после завоевания золотой медали на Олимпийских играх."
                            }
                        }
                    ],
                    "synonyms": [

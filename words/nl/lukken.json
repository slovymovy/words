                {
                    "sense_id": "fdf1c39a-c4eb-353c-82c0-ba82806eb182",
                    "sense_definition": "Goed aflopen of het gewenste resultaat opleveren",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "succes_hebben",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik hoop dat het plan zal <w>lukken</w>.",
                            "target_lang_translations": {
                                "ru": "Я надеюсь, что план <w>сработает</w>.",
                                "en": "I hope the plan will <w>work</w>."
                            }
                        },
                        {
                            "text": "De taart is deze keer heel goed <w>gelukt</w>.",
                            "target_lang_translations": {
                                "ru": "В этот раз торт очень хорошо <w>получился</w>.",
                                "en": "The cake <w>turned out</w> very well this time."
                            }
                        }
                    ],
                    "synonyms": [
                        "slagen"
                    ],
                    "antonyms": [
                        "mislukken",
                        "falen"
                    ],
                    "common_phrases": [
                        "het moet lukken",
                        "alles zal lukken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Завершиться успешно или принести желаемый результат",
                        "en": "To turn out well or yield the desired result"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получаться",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "11fb59ce-4313-32b2-bea4-5411e0186adb",
                    "sense_definition": "Член верхней палаты парламента или законодательного собрания.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "political_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Американский <w>сенатор</w> предложил новый законопроект о налогах.",
                            "target_lang_translations": {
                                "en": "The American <w>senator</w> proposed a new tax bill."
                            }
                        },
                        {
                            "text": "На пресс-конференции выступил <w>сенатор</w> от нашего региона.",
                            "target_lang_translations": {
                                "en": "A <w>senator</w> from our region spoke at the press conference."
                            }
                        }
                    ],
                    "antonyms": [
                        "депутат"
                    ],
                    "common_phrases": [
                        "американский сенатор",
                        "российский сенатор",
                        "стать сенатором",
                        "выборы сенатора"
                    ],
                    "target_lang_definitions": {
                        "en": "A member of the upper house of a parliament or legislative assembly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "senator",
                                "target_lang_sense_clarification": "member of a legislative body"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7820471f-865f-3b0f-bb8c-ac1056a38fab",
                    "sense_definition": "Член сената в Древнем Риме или в некоторых исторических государствах.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый римский <w>сенатор</w> носил белую тогу с пурпурной полосой.",
                            "target_lang_translations": {
                                "en": "Every Roman <w>senator</w> wore a white toga with a purple stripe."
                            }
                        },
                        {
                            "text": "Император созвал совет, на который пригласил самых влиятельных <w>сенаторов</w>.",
                            "target_lang_translations": {
                                "en": "The emperor convened a council to which he invited the most influential <w>senators</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "римский сенатор",
                        "древний сенатор"
                    ],
                    "target_lang_definitions": {
                        "en": "A member of the senate in Ancient Rome or in certain historical states."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "senator",
                                "target_lang_sense_clarification": "historical (Ancient Rome)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40f3dd99-0190-3370-a338-71de2a52ce4b",
                    "sense_definition": "Член Правительствующего сената в Российской империи, а также чиновник в этом звании.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За долгую и безупречную службу его пожаловали в <w>сенаторы</w>.",
                            "target_lang_translations": {
                                "en": "For long and impeccable service, he was granted the rank of <w>senator</w>."
                            }
                        },
                        {
                            "text": "На балу присутствовал старый <w>сенатор</w> со множеством орденов.",
                            "target_lang_translations": {
                                "en": "An old <w>senator</w> with many orders attended the ball."
                            }
                        }
                    ],
                    "common_phrases": [
                        "звание сенатора",
                        "чин сенатора"
                    ],
                    "target_lang_definitions": {
                        "en": "A member of the Governing Senate in the Russian Empire, or an official holding this rank."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "senator",
                                "target_lang_sense_clarification": "historical (Russian Empire)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

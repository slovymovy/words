{
    "word_family": [
        "king",
        "kingly",
        "kingship"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "44b1dee4-5861-3392-8bea-f5169360dd09",
                    "sense_definition": "A country or territory ruled by a king or queen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "realm_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prince will inherit the <w>kingdom</w> when his father dies.",
                            "target_lang_translations": {
                                "ru": "Принц унаследует <w>королевство</w>, когда умрёт его отец."
                            }
                        },
                        {
                            "text": "She traveled to a distant <w>kingdom</w> across the sea.",
                            "target_lang_translations": {
                                "ru": "Она отправилась в далёкое <w>королевство</w> за морем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "united kingdom",
                        "ancient kingdom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Страна или территория, которой правит король или королева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "королевство",
                                "target_lang_sense_clarification": "государство монарха"
                            },
                            {
                                "target_lang_word": "царство",
                                "target_lang_sense_clarification": "государство царя или монарха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9b5b041-e0e3-306e-9cc9-44378430cf85",
                    "sense_definition": "A major category in the classification of living organisms, such as plants or animals.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lions and tigers belong to the animal <w>kingdom</w>.",
                            "target_lang_translations": {
                                "ru": "Львы и тигры принадлежат к <w>царству</w> животных."
                            }
                        },
                        {
                            "text": "Mushrooms are part of the fungi <w>kingdom</w>, not plants.",
                            "target_lang_translations": {
                                "ru": "Грибы являются частью <w>царства</w> грибов, а не растений."
                            }
                        }
                    ],
                    "synonyms": [
                        "regnum"
                    ],
                    "common_phrases": [
                        "animal kingdom",
                        "plant kingdom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высшая категория в классификации живых организмов, например, растения или животные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "царство",
                                "target_lang_sense_clarification": "биологическая категория"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "745bf5be-d5a1-354f-a873-3ffbb8706b42",
                    "sense_definition": "A conceptual area or sphere where a specific thing is dominant.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "realm_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The library was her private <w>kingdom</w> of knowledge.",
                            "target_lang_translations": {
                                "ru": "Библиотека была её личным <w>царством</w> знаний."
                            }
                        },
                        {
                            "text": "In the <w>kingdom</w> of dreams, anything is possible.",
                            "target_lang_translations": {
                                "ru": "В <w>царстве</w> снов возможно всё."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kingdom of heaven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Концептуальная область или сфера, где доминирует определенное явление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "царство",
                                "target_lang_sense_clarification": "перен. сфера господства чего-либо"
                            },
                            {
                                "target_lang_word": "мир",
                                "target_lang_sense_clarification": "сфера существования"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
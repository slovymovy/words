    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "521ea8b1-95e0-3b3a-af42-803e556a5d92",
                    "sense_definition": "A small stall or structure used for the display and sale of goods, or for providing information.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "structure_small",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bought our tickets at the small <w>booth</w> near the entrance.",
                            "target_lang_translations": {
                                "ru": "Мы купили билеты в маленьком <w>киоске</w> у входа."
                            }
                        },
                        {
                            "text": "The farmer set up a <w>booth</w> at the market to sell fresh vegetables.",
                            "target_lang_translations": {
                                "ru": "Фермер поставил <w>палатку</w> на рынке, чтобы продавать свежие овощи."
                            }
                        }
                    ],
                    "synonyms": [
                        "Boothferry",
                        "East Riding of Yorkshire",
                        "England"
                    ],
                    "common_phrases": [
                        "ticket booth",
                        "information booth",
                        "market booth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой киоск, стойка или палатка, используемые для демонстрации и продажи товаров или предоставления информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "киоск",
                                "target_lang_sense_clarification": "для продажи билетов, газет или информации"
                            },
                            {
                                "target_lang_word": "стенд",
                                "target_lang_sense_clarification": "выставочный или информационный"
                            },
                            {
                                "target_lang_word": "палатка",
                                "target_lang_sense_clarification": "торговая точка на рынке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e93a49a-7659-303d-be13-7f5874a46d48",
                    "sense_definition": "A small, enclosed space designed for one person to perform a specific activity privately, such as making a phone call or voting.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "structure_enclosed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He stepped into the phone <w>booth</w> to make a private call.",
                            "target_lang_translations": {
                                "ru": "Он зашел в телефонную <w>будку</w>, чтобы сделать личный звонок."
                            }
                        },
                        {
                            "text": "Voters waited in line to enter the polling <w>booth</w> and cast their ballots.",
                            "target_lang_translations": {
                                "ru": "Избиратели ждали в очереди, чтобы войти в <w>кабинку</w> для голосования и опустить бюллетени."
                            }
                        }
                    ],
                    "synonyms": [
                        "Boothferry",
                        "East Riding of Yorkshire",
                        "England"
                    ],
                    "common_phrases": [
                        "phone booth",
                        "polling booth",
                        "photo booth",
                        "voting booth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое закрытое пространство, предназначенное для одного человека для выполнения определенных действий (звонок, голосование)."
                    },

                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Not more than; at the maximum limit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at most",
                                "target_lang_sense_clarification": "indicating an upper limit"
                            }
                        ]
                    }
                }
            ]
        }
    ]

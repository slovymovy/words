    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cbd8f704-f309-3f82-8cb1-2a915d3a8fc7",
                    "sense_definition": "A verbal dispute or a strong disagreement between people.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_dispute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They had a loud <w>argument</w> about whose turn it was to wash the dishes.",
                            "target_lang_translations": {
                                "ru": "У них вышел громкий <w>спор</w> о том, чья очередь мыть посуду."
                            }
                        },
                        {
                            "text": "I do not want to get into an <w>argument</w> with you right now.",
                            "target_lang_translations": {
                                "ru": "Я не хочу сейчас вступать с тобой в <w>спор</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "quarrel",
                        "dispute",
                        "disagreement",
                        "row",
                        "fight",
                        "altercation"
                    ],
                    "antonyms": [
                        "recantation",
                        "recantation"
                    ],
                    "common_phrases": [
                        "have an argument",
                        "get into an argument",
                        "win an argument",
                        "heated argument"
                    ],
                    "target_lang_definitions": {
                        "ru": "Словесная перепалка или сильное разногласие между людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спор",
                                "target_lang_sense_clarification": "словесное состязание, обсуждение"
                            },
                            {
                                "target_lang_word": "ссора",
                                "target_lang_sense_clarification": "состояние вражды, перебранка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69324068-8bc8-3ece-8424-877996c86264",
                    "sense_definition": "A reason or set of reasons given with the aim of persuading others that an action or idea is right or wrong.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_reasoning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her <w>argument</w> for changing the company policy was very convincing.",
                            "target_lang_translations": {
                                "ru": "Её <w>аргумент</w> в пользу изменения политики компании был очень убедительным."
                            }
                        },
                        {
                            "text": "There is a strong <w>argument</w> that we should save more money for the future.",
                            "target_lang_translations": {
                                "ru": "Существует веский <w>довод</w> в пользу того, что нам следует откладывать больше денег на будущее."
                            }
                        }
                    ],
                    "synonyms": [
                        "reason",
                        "justification",
                        "point",
                        "claim",
                        "explanation"
                    ],
                    "common_phrases": [
                        "valid argument",
                        "strong argument",
                        "argument for",
                        "argument against"
                    ],
                    "target_lang_definitions": {
                        "ru": "Довод или ряд доводов, приводимых с целью убедить других в правильности действия или идеи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аргумент",
                                "target_lang_sense_clarification": "основание, доказательство"
                            },
                            {
                                "target_lang_word": "довод",
                                "target_lang_sense_clarification": "соображение, приводимое в доказательство"
                            },
                            {
                                "target_lang_word": "обоснование",
                                "target_lang_sense_clarification": "подкрепление фактами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "253d3a5c-5dee-33d3-bf94-09ae4f922162",
                    "sense_definition": "A value that is passed to a function, procedure, or routine in computer programming.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "group_technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to define the correct data type for each <w>argument</w> in the function.",
                            "target_lang_translations": {
                                "ru": "Вам нужно определить правильный тип данных для каждого <w>аргумента</w> функции."
                            }
                        },
                        {
                            "text": "The program failed because an invalid <w>argument</w> was passed to the command.",
                            "target_lang_translations": {
                                "ru": "Программа дала сбой, потому что команде был передан неверный <w>аргумент</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "parameter"
                    ],
                    "common_phrases": [
                        "pass an argument",
                        "function argument"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Значение, передаваемое функции, процедуре или подпрограмме в программировании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аргумент",
                                "target_lang_sense_clarification": "независимое значение"
                            },
                            {
                                "target_lang_word": "параметр",
                                "target_lang_sense_clarification": "переменная функции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04cc3f53-9e18-3c84-87ff-4ab6912e8c67",
                    "sense_definition": "A series of statements where the final one is a conclusion intended to follow logically from the previous ones.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_reasoning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor presented a complex logical <w>argument</w> regarding the nature of truth.",
                            "target_lang_translations": {
                                "ru": "Профессор представил сложную логическую <w>аргументацию</w> касательно природы истины."
                            }
                        },
                        {
                            "text": "His <w>argument</w> was flawed because one of the initial premises was false.",
                            "target_lang_translations": {
                                "ru": "Его <w>довод</w> был ошибочным, так как одна из исходных посылок была ложной."
                            }
                        }
                    ],
                    "synonyms": [
                        "reasoning",
                        "deduction"
                    ],
                    "common_phrases": [
                        "logical argument",
                        "deductive argument",
                        "circular argument"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "philosophy or logic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Серия утверждений, где последнее является выводом, логически следующим из предыдущих."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аргументация",
                                "target_lang_sense_clarification": "процесс приведения доводов"
                            },
                            {
                                "target_lang_word": "логический довод",
                                "target_lang_sense_clarification": "умозаключение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aac42bec-5977-331d-a43a-ef48c03c967b",
                    "sense_definition": "The independent variable of a function.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In this equation, we need to determine the value of the <w>argument</w> to solve for y.",
                            "target_lang_translations": {
                                "ru": "В этом уравнении нам нужно определить значение <w>аргумента</w>, чтобы найти y."
                            }
                        },
                        {
                            "text": "The function returns a specific result depending on the input <w>argument</w>.",
                            "target_lang_translations": {
                                "ru": "Функция возвращает определенный результат в зависимости от входного <w>аргумента</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "variable"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Независимая переменная функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аргумент",
                                "target_lang_sense_clarification": "математическая переменная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8cce23a-e06a-3dd1-9bd4-52afb9acc0f6",
                    "sense_definition": "A summary of the subject matter of a literary work, such as a book or poem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>argument</w> at the beginning of the chapter explains the upcoming events.",
                            "target_lang_translations": {
                                "ru": "<w>Краткое содержание</w> в начале главы объясняет предстоящие события."
                            }
                        },
                        {
                            "text": "Scholars analyzed the <w>argument</w> of the epic to understand the author's intent.",
                            "target_lang_translations": {
                                "ru": "Ученые проанализировали <w>фабулу</w> эпоса, чтобы понять замысел автора."
                            }
                        }
                    ],
                    "synonyms": [
                        "summary",
                        "synopsis",
                        "abstract"
                    ],
                    "common_phrases": [
                        "argument of the poem"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Краткое изложение содержания литературного произведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "краткое содержание",
                                "target_lang_sense_clarification": "резюме произведения"
                            },
                            {
                                "target_lang_word": "фабула",
                                "target_lang_sense_clarification": "основа сюжета"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
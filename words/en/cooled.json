{
    "word_family": [
        "cooler",
        "coolant",
        "coolness",
        "coolly"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "57b35360-9f5a-3764-9e61-2a1962d83fc5",
                    "sense_definition": "Simple past and past participle of <w>cool</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rain <w>cooled</w> the hot pavement very quickly.",
                            "target_lang_translations": {
                                "ru": "Дождь очень быстро <w>остудил</w> горячий тротуар."
                            }
                        },
                        {
                            "text": "She <w>cooled</w> down after sitting in the shade for a while.",
                            "target_lang_translations": {
                                "ru": "Она <w>остыла</w>, посидев немного в тени."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cooled down",
                        "cooled off"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола cool (охлаждать, остывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остыл",
                                "target_lang_sense_clarification": "стал холодным или спокойным (о человеке или предмете)"
                            },
                            {
                                "target_lang_word": "охладил",
                                "target_lang_sense_clarification": "сделал холодным (переходный глагол)"
                            },
                            {
                                "target_lang_word": "остудил",
                                "target_lang_sense_clarification": "понизил температуру чего-либо (часто о еде или напитках)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "460f61d9-b490-3640-99dd-8132b856e0d0",
                    "sense_definition": "Brought to a lower temperature; having become cold or less hot.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "temperature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should serve the <w>cooled</w> cake with fresh fruit.",
                            "target_lang_translations": {
                                "ru": "Вам следует подавать <w>остывший</w> пирог со свежими фруктами."
                            }
                        },
                        {
                            "text": "The <w>cooled</w> air from the conditioner felt wonderful.",
                            "target_lang_translations": {
                                "ru": "<w>Охлаждённый</w> воздух из кондиционера был чудесен."
                            }
                        }
                    ],
                    "synonyms": [
                        "chilled",
                        "refrigerated"
                    ],
                    "antonyms": [
                        "heated",
                        "warmed"
                    ],
                    "common_phrases": [
                        "cooled air",
                        "cooled lava"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доведенный до более низкой температуры; ставший холодным или менее горячим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остывший",
                                "target_lang_sense_clarification": "ставший холодным естественным образом"
                            },
                            {
                                "target_lang_word": "охлаждённый",
                                "target_lang_sense_clarification": "подвергшийся специальному охлаждению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d719afaa-fd7d-30c0-9bd9-65732f606b26",
                    "sense_definition": "Reduced in temperature by a specific mechanism or substance (often used in compounds).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "temperature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic repaired the old water-<w>cooled</w> engine.",
                            "target_lang_translations": {
                                "ru": "Механик починил старый двигатель с водяным <w>охлаждением</w>."
                            }
                        },
                        {
                            "text": "This high-performance computer uses a liquid-<w>cooled</w> system.",
                            "target_lang_translations": {
                                "ru": "Этот высокопроизводительный компьютер использует систему с жидкостным <w>охлаждением</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "water-cooled",
                        "air-cooled",
                        "liquid-cooled"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сниженный в температуре с помощью определенного механизма или вещества (часто используется в составных словах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с ... охлаждением",
                                "target_lang_sense_clarification": "используется в технических терминах (напр., с водяным охлаждением)"
                            },
                            {
                                "target_lang_word": "охлаждаемый",
                                "target_lang_sense_clarification": "подвергаемый охлаждению чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c321bef-cd8c-3384-9373-2e7f50d0ce40",
                    "sense_definition": "Having become less angry, excited, or enthusiastic.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>cooled</w> anger allowed him to think more clearly about the problem.",
                            "target_lang_translations": {
                                "ru": "Его <w>остывший</w> гнев позволил ему яснее обдумать проблему."
                            }
                        },
                        {
                            "text": "Over time, their <w>cooled</w> passion turned into a steady friendship.",
                            "target_lang_translations": {
                                "ru": "Со временем их <w>угасшая</w> страсть превратилась в крепкую дружбу."
                            }
                        }
                    ],
                    "synonyms": [
                        "calmed",
                        "composed"
                    ],
                    "antonyms": [
                        "agitated",
                        "inflamed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ставший менее сердитым, взволнованным или полным энтузиазма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остывший",
                                "target_lang_sense_clarification": "успокоившийся, утративший пыл"
                            },
                            {
                                "target_lang_word": "угасший",
                                "target_lang_sense_clarification": "о чувствах или страсти"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "8fcdf73d-1c45-3e7f-8050-cfd60429fd4e",
                    "sense_definition": "The state of having no job; joblessness.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "state_of_joblessness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Long periods of <w>unemployment</w> can be financially difficult for families.",
                            "target_lang_translations": {
                                "ru": "Длительные периоды <w>безработицы</w> могут быть финансово трудными для семей."
                            }
                        },
                        {
                            "text": "The closure of the local factory caused widespread <w>unemployment</w> in the town.",
                            "target_lang_translations": {
                                "ru": "Закрытие местного завода привело к массовой <w>безработице</w> в городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "joblessness",
                        "worklessness"
                    ],
                    "antonyms": [
                        "employment"
                    ],
                    "common_phrases": [
                        "youth unemployment",
                        "mass unemployment",
                        "period of unemployment",
                        "face unemployment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние отсутствия работы; положение человека, не имеющего занятости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безработица",
                                "target_lang_sense_clarification": "состояние отсутствия работы"
                            },
                            {
                                "target_lang_word": "незанятость",
                                "target_lang_sense_clarification": "отсутствие занятости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f27fe81-c4e2-3a5d-a92c-d3f93bc481b2",
                    "sense_definition": "The level or percentage of people without jobs in an economy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "state_of_joblessness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government announced that <w>unemployment</w> had fallen to five percent.",
                            "target_lang_translations": {
                                "ru": "Правительство объявило, что <w>безработица</w> снизилась до пяти процентов."
                            }
                        },
                        {
                            "text": "Economists are worried about the sudden rise in <w>unemployment</w> this quarter.",
                            "target_lang_translations": {
                                "ru": "Экономисты обеспокоены внезапным ростом <w>безработицы</w> в этом квартале."
                            }
                        }
                    ],
                    "synonyms": [
                        "joblessness"
                    ],
                    "antonyms": [
                        "employment"
                    ],
                    "common_phrases": [
                        "unemployment rate",
                        "rise in unemployment",
                        "fall in unemployment",
                        "high unemployment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уровень или процент людей, не имеющих работы, в экономике страны или региона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безработица",
                                "target_lang_sense_clarification": "экономическое явление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "612ac011-2298-35ca-af94-ebc252c5c40c",
                    "sense_definition": "An instance, period, or specific type of joblessness.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His resume lists several short <w>unemployments</w> between his contract roles.",
                            "target_lang_translations": {
                                "ru": "В его резюме указано несколько коротких периодов <w>безработицы</w> между контрактными должностями."
                            }
                        },
                        {
                            "text": "Different <w>unemployments</w>, such as seasonal or structural, require different solutions.",
                            "target_lang_translations": {
                                "ru": "Различные виды <w>безработицы</w>, такие как сезонная или структурная, требуют разных решений."
                            }
                        }
                    ],
                    "common_phrases": [
                        "seasonal unemployment",
                        "structural unemployment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный случай, период или конкретный вид отсутствия работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безработица",
                                "target_lang_sense_clarification": "период или вид"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fee7bd47-5c44-311f-a88d-79a1cae1206f",
                    "sense_definition": "Happening or existing without stopping, pausing, or interruption over a period of time.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "unbroken_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory operates under <w>continuous</w> supervision to ensure safety.",
                            "target_lang_translations": {
                                "ru": "Завод работает под <w>непрерывным</w> наблюдением для обеспечения безопасности."
                            }
                        },
                        {
                            "text": "We heard a <w>continuous</w> low hum coming from the refrigerator.",
                            "target_lang_translations": {
                                "ru": "Мы слышали <w>непрерывное</w> низкое гудение, исходящее от холодильника."
                            }
                        }
                    ],
                    "synonyms": [
                        "nonstop",
                        "uninterrupted",
                        "constant",
                        "incessant",
                        "ceaseless",
                        "unbroken",
                        "ongoing"
                    ],
                    "antonyms": [
                        "intermittent",
                        "interrupted",
                        "broken",
                        "stopped"
                    ],
                    "common_phrases": [
                        "continuous improvement",
                        "continuous flow",
                        "continuous supply",
                        "continuous process"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий или существующий без остановок, пауз или перерывов в течение определенного периода времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непрерывный",
                                "target_lang_sense_clarification": "происходящий без пауз"
                            },
                            {
                                "target_lang_word": "постоянный",
                                "target_lang_sense_clarification": "длящийся всё время"
                            },
                            {
                                "target_lang_word": "беспрерывный",
                                "target_lang_sense_clarification": "не прекращающийся"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d148aeef-99a5-3c10-afdc-61e081361d7b",
                    "sense_definition": "Forming an unbroken line or surface; having no gaps or spaces in between.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "unbroken_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fence forms a <w>continuous</w> barrier along the entire border.",
                            "target_lang_translations": {
                                "ru": "Забор образует <w>сплошной</w> барьер вдоль всей границы."
                            }
                        },
                        {
                            "text": "Try to draw a <w>continuous</w> line without lifting your pen from the paper.",
                            "target_lang_translations": {
                                "ru": "Попробуйте нарисовать <w>непрерывную</w> линию, не отрывая ручки от бумаги."
                            }
                        }
                    ],
                    "synonyms": [
                        "unbroken",
                        "connected"
                    ],
                    "antonyms": [
                        "broken",
                        "disconnected",
                        "disjoint"
                    ],
                    "common_phrases": [
                        "continuous line",
                        "continuous surface"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образующий сплошную линию или поверхность; не имеющий промежутков или разрывов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сплошной",
                                "target_lang_sense_clarification": "без промежутков"
                            },
                            {
                                "target_lang_word": "непрерывный",
                                "target_lang_sense_clarification": "цельный, неразрывный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2fd70f1c-d6ad-3c61-861d-6031620ae9b5",
                    "sense_definition": "Relating to a verb form that expresses an action happening over a period of time; progressive.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "grammar_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Students learn the present <w>continuous</w> tense early in their studies.",
                            "target_lang_translations": {
                                "ru": "Студенты изучают настоящее <w>длительное</w> время в начале своего обучения."
                            }
                        },
                        {
                            "text": "The past <w>continuous</w> describes what was happening at a specific moment in the past.",
                            "target_lang_translations": {
                                "ru": "Прошедшее <w>длительное</w> время описывает то, что происходило в определенный момент в прошлом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "present continuous",
                        "past continuous",
                        "continuous tense",
                        "continuous aspect"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к глагольной форме, выражающей действие, происходящее в течение определенного периода времени (продолженное время)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "длительный",
                                "target_lang_sense_clarification": "грамматический термин"
                            },
                            {
                                "target_lang_word": "продолженный",
                                "target_lang_sense_clarification": "вариант названия времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edc248ae-a0df-304d-ad19-bf8e82eb9dc3",
                    "sense_definition": "Describing a mathematical function or curve that has no sudden changes, breaks, or holes.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "math_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>continuous</w> function can be graphed without lifting the pencil from the paper.",
                            "target_lang_translations": {
                                "ru": "График <w>непрерывной</w> функции можно построить, не отрывая карандаша от бумаги."
                            }
                        },
                        {
                            "text": "Calculus primarily deals with <w>continuous</w> change rather than discrete steps.",
                            "target_lang_translations": {
                                "ru": "Математический анализ в основном имеет дело с <w>непрерывными</w> изменениями, а не с дискретными шагами."
                            }
                        }
                    ],
                    "antonyms": [

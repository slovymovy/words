{
    "word_family": [
        "acquisition",
        "acquisitive",
        "acquirement",
        "acquirer"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bb612978-bc5c-3c64-9741-a0b9db68bbba",
                    "sense_definition": "To obtain or gain something, especially through one's own efforts, ability, or purchase.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She worked hard to <w>acquire</w> the necessary skills for the job.",
                            "target_lang_translations": {
                                "ru": "Она усердно работала, чтобы <w>овладеть</w> необходимыми навыками для этой работы."
                            }
                        },
                        {
                            "text": "The museum managed to <w>acquire</w> a rare painting from the 18th century.",
                            "target_lang_translations": {
                                "ru": "Музею удалось <w>приобрести</w> редкую картину XVIII века."
                            }
                        },
                        {
                            "text": "It takes time to <w>acquire</w> a taste for such strong coffee.",
                            "target_lang_translations": {
                                "ru": "Нужно время, чтобы <w>приобрести</w> вкус к такому крепкому кофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "obtain",
                        "gain",
                        "get",
                        "secure",
                        "procure",
                        "earn",
                        "attain",
                        "achieve",
                        "win"
                    ],
                    "antonyms": [
                        "lose"
                    ],
                    "common_phrases": [
                        "acquire knowledge",
                        "acquire a skill",
                        "acquire property",
                        "acquire a taste"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать или приобретать что-либо, особенно благодаря собственным усилиям, способностям или покупке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приобретать",
                                "target_lang_sense_clarification": "покупать или получать в собственность"
                            },
                            {
                                "target_lang_word": "овладевать",
                                "target_lang_sense_clarification": "усваивать знания или навыки"
                            },
                            {
                                "target_lang_word": "получать",
                                "target_lang_sense_clarification": "общий смысл: добывать, доставать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25ac3c21-7556-352d-aacf-18c94c314d15",
                    "sense_definition": "To become affected by an illness or disease.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Patients with weak immune systems can easily <w>acquire</w> infections in the hospital.",
                            "target_lang_translations": {
                                "ru": "Пациенты со слабой иммунной системой могут легко <w>подхватить</w> инфекцию в больнице."
                            }
                        },
                        {
                            "text": "He was worried he might <w>acquire</w> the virus while traveling abroad.",
                            "target_lang_translations": {
                                "ru": "Он беспокоился, что может <w>заразиться</w> вирусом во время поездки за границу."
                            }
                        }
                    ],
                    "synonyms": [
                        "contract",
                        "catch",
                        "get"
                    ],
                    "common_phrases": [
                        "acquire an infection",
                        "acquire a disease"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заразиться болезнью или заболеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подхватить",
                                "target_lang_sense_clarification": "разг. заразиться чем-либо"
                            },
                            {
                                "target_lang_word": "заразиться",
                                "target_lang_sense_clarification": "получить инфекцию или вирус"
                            },
                            {
                                "target_lang_word": "получить",
                                "target_lang_sense_clarification": "в контексте болезней или травм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dca57cb-f41a-395c-bfac-cdee48bb2489",
                    "sense_definition": "To sample signals or collect data and convert them into digital values for processing.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The software is designed to <w>acquire</w> data from multiple sensors simultaneously.",
                            "target_lang_translations": {
                                "ru": "Программное обеспечение разработано для того, чтобы <w>собирать</w> данные с нескольких датчиков одновременно."
                            }
                        },
                        {
                            "text": "It took several minutes for the satellite to <w>acquire</w> the signal from the ground station.",
                            "target_lang_translations": {
                                "ru": "Спутнику потребовалось несколько минут, чтобы <w>поймать</w> сигнал от наземной станции."
                            }
                        }
                    ],
                    "synonyms": [
                        "collect",
                        "gather",
                        "get",
                        "obtain"
                    ],
                    "common_phrases": [
                        "acquire data",
                        "acquire a signal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считывать сигналы или собирать данные для последующей обработки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "накапливать данные"
                            },
                            {
                                "target_lang_word": "получать",
                                "target_lang_sense_clarification": "принимать сигнал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fac48a60-48f9-302b-8944-ef9cca7beb29",
                    "sense_definition": "To begin tracking a mobile target with a detector, radar, or weapon sight.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot struggled to <w>acquire</w> the target due to heavy cloud cover.",
                            "target_lang_translations": {
                                "ru": "Пилот с трудом пытался <w>захватить</w> цель из-за плотной облачности."
                            }
                        },
                        {
                            "text": "Once the radar <w>acquires</w> the object, the system tracks its speed automatically.",
                            "target_lang_translations": {
                                "ru": "Как только радар <w>захватывает</w> объект, система автоматически отслеживает его скорость."
                            }
                        }
                    ],
                    "synonyms": [
                        "get",
                        "secure"
                    ],
                    "antonyms": [
                        "lose"
                    ],
                    "common_phrases": [
                        "acquire a target"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнаруживать и брать на сопровождение цель (радаром или прицелом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "захватывать",
                                "target_lang_sense_clarification": "фиксировать цель для отслеживания"
                            },
                            {
                                "target_lang_word": "обнаруживать",
                                "target_lang_sense_clarification": "находить объект"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
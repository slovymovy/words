{
    "word_family": [
        "confusion",
        "confused",
        "confusing"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c54ffce8-c823-3b9a-b8f2-e280ac49eb4b",
                    "sense_definition": "To make someone feel uncertain, puzzled, or unable to understand something clearly.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The complicated instructions completely <w>confused</w> the new students.",
                            "target_lang_translations": {
                                "ru": "Сложные инструкции окончательно <w>сбили с толку</w> новых студентов."
                            }
                        },
                        {
                            "text": "Please do not <w>confuse</w> the issue by bringing up irrelevant details.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>запутывайте</w> дело, приводя неуместные подробности."
                            }
                        }
                    ],
                    "synonyms": [
                        "puzzle",
                        "perplex",
                        "bewilder",
                        "baffle",
                        "confound"
                    ],
                    "antonyms": [
                        "clarify",
                        "enlighten"
                    ],
                    "common_phrases": [
                        "confuse the issue",
                        "confuse matters",
                        "easily confuse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то чувствовать неуверенность, озадачить или лишить ясности понимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбивать с толку",
                                "target_lang_sense_clarification": "лишать соображения, приводить в замешательство"
                            },
                            {
                                "target_lang_word": "запутывать",
                                "target_lang_sense_clarification": "делать неясным, сложным для понимания"
                            },
                            {
                                "target_lang_word": "смущать",
                                "target_lang_sense_clarification": "вызывать недоумение, растерянность или неловкость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48675c4e-1c00-36b8-9edb-1011e2947174",
                    "sense_definition": "To wrongly think that one person or thing is someone or something else.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "identification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "People often <w>confuse</w> me with my twin brother.",
                            "target_lang_translations": {
                                "ru": "Люди часто <w>путают</w> меня с моим братом-близнецом."
                            }
                        },
                        {
                            "text": "You should not <w>confuse</w> confidence with arrogance.",
                            "target_lang_translations": {
                                "ru": "Не следует <w>путать</w> уверенность с высокомерием."
                            }
                        }
                    ],
                    "synonyms": [
                        "mix up",
                        "mistake"
                    ],
                    "antonyms": [
                        "distinguish"
                    ],
                    "common_phrases": [
                        "confuse with",
                        "confuse for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ошибочно принимать одного человека или предмет за другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путать",
                                "target_lang_sense_clarification": "не различать, принимать одно за другое"
                            },
                            {
                                "target_lang_word": "перепутать",
                                "target_lang_sense_clarification": "совершить ошибку в идентификации (совершенный вид)"
                            },
                            {
                                "target_lang_word": "смешивать",
                                "target_lang_sense_clarification": "не видеть разницы между понятиями или вещами"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "101a76fe-fbb3-3842-a129-898f03587004",
                    "sense_definition": "Disordered or mixed up; an archaic form of the adjective confused.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient text was written in a <w>confuse</w> and difficult style.",
                            "target_lang_translations": {
                                "ru": "Древний текст был написан <w>запутанным</w> и трудным стилем."
                            }
                        },
                        {
                            "text": "His thoughts remained <w>confuse</w> despite his attempts to organize them.",
                            "target_lang_translations": {
                                "ru": "Его мысли оставались <w>сбивчивыми</w>, несмотря на попытки привести их в порядок."
                            }
                        }
                    ],
                    "antonyms": [
                        "organize"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "The standard modern adjective is 'confused'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Беспорядочный или смешанный; устаревшая форма прилагательного \"confused\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запутанный",
                                "target_lang_sense_clarification": "сложный, непонятный"
                            },
                            {
                                "target_lang_word": "сбивчивый",
                                "target_lang_sense_clarification": "беспорядочный, лишенный логики"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
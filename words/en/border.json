{
    "word_family": [
        "borderline",
        "borderland",
        "borderless",
        "borderer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ac04273d-9551-3203-914f-8432c36e4f23",
                    "sense_definition": "The line or frontier area separating political or geographical regions.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geographical_boundary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We showed our passports to the guards at the <w>border</w>.",
                            "target_lang_translations": {
                                "ru": "Мы показали паспорта пограничникам на <w>грани́це</w>."
                            }
                        },
                        {
                            "text": "The <w>border</w> between the two countries is heavily guarded.",
                            "target_lang_translations": {
                                "ru": "<w>Грани́ца</w> между двумя странами усиленно охраняется."
                            }
                        }
                    ],
                    "synonyms": [
                        "frontier",
                        "boundary"
                    ],
                    "antonyms": [
                        "interior",
                        "center"
                    ],
                    "common_phrases": [
                        "cross the border",
                        "national border",
                        "border control",
                        "open border"
                    ],
                    "target_lang_definitions": {
                        "ru": "Линия или пограничная зона, разделяющая политические или географические регионы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грани́ца",
                                "target_lang_sense_clarification": "политическая или географическая"
                            },
                            {
                                "target_lang_word": "рубе́ж",
                                "target_lang_sense_clarification": "военный или переносный смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d44bcd1f-b916-3dbd-846e-27746fc21cfb",
                    "sense_definition": "The outer edge or margin of something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_edge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We set up our tent on the <w>border</w> of the forest.",
                            "target_lang_translations": {
                                "ru": "Мы поставили палатку на <w>краю́</w> леса."
                            }
                        },
                        {
                            "text": "The <w>border</w> of the rug has become frayed over time.",
                            "target_lang_translations": {
                                "ru": "<w>Кайма́</w> ковра со временем истрепалась."
                            }
                        }
                    ],
                    "synonyms": [
                        "edge",
                        "margin",
                        "rim",
                        "perimeter"
                    ],
                    "antonyms": [
                        "center",
                        "middle"
                    ],
                    "common_phrases": [
                        "border of the lake",
                        "border of the page"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешний край или кромка чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "край",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "кро́мка",
                                "target_lang_sense_clarification": "узкий край материала"
                            },
                            {
                                "target_lang_word": "кайма́",
                                "target_lang_sense_clarification": "полоса по краю ткани"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce5d67ea-4b96-32a6-aa67-f330e7b508a1",
                    "sense_definition": "A decorative strip around the edge of an object.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "decoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The invitation card features an elegant gold <w>border</w>.",
                            "target_lang_translations": {
                                "ru": "На пригласительной открытке — элегантная золотая <w>оканто́вка</w>."
                            }
                        },
                        {
                            "text": "She painted a floral <w>border</w> around the window frame.",
                            "target_lang_translations": {
                                "ru": "Она нарисовала цветочный <w>бордю́р</w> вокруг оконной рамы."
                            }
                        }
                    ],
                    "synonyms": [
                        "edging",
                        "frame",
                        "trim"
                    ],
                    "common_phrases": [
                        "decorative border",
                        "lace border",
                        "gold border"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративная полоса по краю предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кайма́",
                                "target_lang_sense_clarification": "на ткани или бумаге"
                            },
                            {
                                "target_lang_word": "бордю́р",
                                "target_lang_sense_clarification": "в отделке помещений или на дороге"
                            },
                            {
                                "target_lang_word": "оканто́вка",
                                "target_lang_sense_clarification": "узкое обрамление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1768ffec-4e70-34c3-bdbb-2d18735fea15",
                    "sense_definition": "A strip of ground in a garden where ornamental plants are grown.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gardening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gardener planted colorful tulips in the herbaceous <w>border</w>.",
                            "target_lang_translations": {
                                "ru": "Садовник посадил разноцветные тюльпаны в травянистый <w>бордю́р</w>."
                            }
                        },
                        {
                            "text": "We spent the afternoon weeding the flower <w>border</w>.",
                            "target_lang_translations": {
                                "ru": "Мы провели день, пропалывая цветочный <w>бордю́р</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "flowerbed",
                        "bed"
                    ],
                    "common_phrases": [
                        "herbaceous border",
                        "flower border"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полоса земли в саду, где выращиваются декоративные растения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бордю́р",
                                "target_lang_sense_clarification": "садоводство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a4de1422-63d6-3475-9596-088e4d85a8bb",
                    "sense_definition": "To lie adjacent to or share a boundary with another area.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "geographical_boundary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "France <w>borders</w> Spain to the south along the Pyrenees.",
                            "target_lang_translations": {
                                "ru": "Франция <w>грани́чит</w> с Испанией на юге вдоль Пиренеев."
                            }
                        },
                        {
                            "text": "The small village <w>borders</w> a large national park.",
                            "target_lang_translations": {
                                "ru": "Маленькая деревня <w>грани́чит</w> с большим национальным парком."
                            }
                        }
                    ],
                    "synonyms": [
                        "adjoin",
                        "abut",
                        "touch"
                    ],
                    "common_phrases": [
                        "border the sea",
                        "border a country"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться рядом или иметь общую границу с другой территорией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грани́чить",
                                "target_lang_sense_clarification": "иметь общую границу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f5290d2-8d91-3ab0-ba45-5725317bcc23",
                    "sense_definition": "To form a border or edge around something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_edge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tall oak trees <w>border</w> the long driveway to the house.",
                            "target_lang_translations": {
                                "ru": "Высокие дубы <w>окаймля́ют</w> длинную подъездную дорогу к дому."
                            }
                        },
                        {
                            "text": "A thick hedge <w>borders</w> the garden to provide privacy.",
                            "target_lang_translations": {
                                "ru": "Густая живая изгородь <w>окаймля́ет</w> сад, обеспечивая уединение."
                            }
                        }
                    ],
                    "synonyms": [
                        "surround",
                        "frame",
                        "enclose",
                        "bound"
                    ],
                    "common_phrases": [
                        "border the path",
                        "border the road"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образовывать границу или край вокруг чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окаймля́ть",
                                "target_lang_sense_clarification": "окружать"
                            },
                            {
                                "target_lang_word": "обрамля́ть",
                                "target_lang_sense_clarification": "служить рамкой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9021cae-c7a8-3d80-b480-3d33b850e0f2",
                    "sense_definition": "To come very near to a specific quality or state, often a negative one.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "approximation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His extreme confidence often <w>borders</w> on arrogance.",
                            "target_lang_translations": {
                                "ru": "Его чрезмерная уверенность часто <w>грани́чит</w> с высокомерием."
                            }
                        },
                        {
                            "text": "The chaotic situation in the office <w>borders</w> on madness.",
                            "target_lang_translations": {
                                "ru": "Хаотичная ситуация в офисе <w>грани́чит</w> с безумием."
                            }
                        }
                    ],
                    "synonyms": [
                        "verge on",
                        "approach"
                    ],
                    "common_phrases": [
                        "border on the ridiculous",
                        "border on obsession",
                        "border on chaos"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть очень близким к определенному качеству или состоянию, часто негативному."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грани́чить",
                                "target_lang_sense_clarification": "быть близким к чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c979b759-2357-3a38-a0fb-1c02586d086e",
                    "sense_definition": "To put a decorative border on something.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "decoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>bordered</w> the tablecloth with delicate lace.",
                            "target_lang_translations": {
                                "ru": "Она <w>окайми́ла</w> скатерть тонким кружевом."
                            }
                        },
                        {
                            "text": "The artist <w>bordered</w> the painting with a dark line.",
                            "target_lang_translations": {
                                "ru": "Художник <w>окайми́л</w> картину темной линией."
                            }
                        }
                    ],
                    "synonyms": [
                        "trim",
                        "edge",
                        "frame"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать декоративную кайму на чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окаймля́ть",
                                "target_lang_sense_clarification": "делать кайму"
                            },
                            {
                                "target_lang_word": "обшива́ть",
                                "target_lang_sense_clarification": "шитье"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "6842c635-595f-35cd-918a-2474d7b59bd0",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Border</w> has lived in this town for fifty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Бордер</w> живет в этом городе пятьдесят лет."
                            }
                        },
                        {
                            "text": "We are looking for a file under the name <w>Border</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ищем файл под фамилией <w>Бордер</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "surname"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бордер",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "08af7284-45c6-3819-be37-e7ac5d3d1644",
                    "sense_definition": "Charakterystyczny ubiór o określonym kroju i kolorze, noszony przez uczniów w szkole lub członków organizacji młodzieżowych.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "uniform_clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszej szkole każdy uczeń musi codziennie nosić granatowy <w>mundurek</w>.",
                            "target_lang_translations": {
                                "ru": "В нашей школе каждый ученик обязан ежедневно носить темно-синюю <w>школьную форму</w>."
                            }
                        },
                        {
                            "text": "Harcerze założyli swoje odświętne <w>mundurki</w> na uroczysty apel.",
                            "target_lang_translations": {
                                "ru": "Харцеры надели свою праздничную <w>форму</w> на торжественную линейку."
                            }
                        }
                    ],
                    "synonyms": [
                        "fartuszek"
                    ],
                    "common_phrases": [
                        "mundurek szkolny",
                        "mundurek harcerski",
                        "nosić mundurek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характерная одежда определенного покроя и цвета, которую носят ученики в школе или члены молодежных организаций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "школьная форма",
                                "target_lang_sense_clarification": "одежда для учеников"
                            },
                            {
                                "target_lang_word": "форма",
                                "target_lang_sense_clarification": "одежда членов организации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d25cf9c-2576-3cb5-b3c9-444ea0476451",
                    "sense_definition": "Zdrobnienie od słowa <w>mundur</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "diminutive_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopiec przebrał się w mały żołnierski <w>mundurek</w> do zabawy.",

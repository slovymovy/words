                {
                    "sense_id": "960d03b4-081f-3d1d-a9c8-749d37bc440a",
                    "sense_definition": "Czas, w którym urządzenie, przedmiot lub organizm zachowuje sprawność i może prawidłowo funkcjonować.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "durability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niewłaściwe ładowanie telefonu może znacznie skrócić <w>żywotność</w> baterii.",
                            "target_lang_translations": {
                                "ru": "Неправильная зарядка телефона может значительно сократить <w>срок службы</w> аккумулятора."
                            }
                        },
                        {
                            "text": "Producent gwarantuje, że <w>żywotność</w> tego silnika wynosi co najmniej dziesięć lat.",
                            "target_lang_translations": {
                                "ru": "Производитель гарантирует, что <w>срок службы</w> этого двигателя составляет не менее десяти лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "trwałość",
                        "wytrzymałość"
                    ],
                    "antonyms": [
                        "awaryjność",
                        "nietrwałość"
                    ],
                    "common_phrases": [
                        "żywotność baterii",
                        "przedłużyć żywotność",
                        "długa żywotność",
                        "krótka żywotność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время, в течение которого устройство, предмет или организм сохраняет работоспособность и может правильно функционировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срок службы",
                                "target_lang_sense_clarification": "период эксплуатации технического устройства или предмета"
                            },
                            {
                                "target_lang_word": "долговечность",
                                "target_lang_sense_clarification": "свойство быть прочным и долго служить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3aa0eadc-6af8-359e-9066-665e125e64a9",
                    "sense_definition": "Cecha kogoś, kto jest pełen energii, siły życiowej i chęci do działania.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "vitality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo podeszłego wieku, babcia zaskakuje wszystkich swoją niezwykłą <w>żywotnością</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на преклонный возраст, бабушка удивляет всех своей необычайной <w>жизненной силой</w>."
                            }
                        },
                        {
                            "text": "Jego naturalna <w>żywotność</w> sprawia, że zawsze jest duszą towarzystwa.",
                            "target_lang_translations": {
                                "ru": "Его природная <w>живость</w> делает его душой компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "witalność",
                        "werwa",
                        "energia",
                        "dynamizm"
                    ],
                    "antonyms": [
                        "apatia",
                        "bierność"
                    ],
                    "common_phrases": [
                        "niezwykła żywotność",
                        "zachować żywotność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Черта человека, полного энергии, жизненных сил и желания действовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жизненная сила",
                                "target_lang_sense_clarification": "внутренняя энергия и мощь организма"
                            },
                            {
                                "target_lang_word": "живость",
                                "target_lang_sense_clarification": "подвижность, энергичность характера"
                            },
                            {
                                "target_lang_word": "витальность",
                                "target_lang_sense_clarification": "книжное; жизненная энергия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9520675-609d-349a-9065-5bf962eb58d2",
                    "sense_definition": "Trwałość i aktualność pewnych idei, tradycji lub zjawisk mimo upływu czasu.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "relevance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zaskakująca <w>żywotność</w> tych przesądów utrudnia wprowadzenie koniecznych zmian w społeczeństwie.",
                            "target_lang_translations": {
                                "ru": "Удивительная <w>живучесть</w> этих предрассудков затрудняет внедрение необходимых изменений в обществе."
                            }
                        },
                        {
                            "text": "Dyskusja potwierdziła <w>żywotność</w> poruszanych przez autora problemów moralnych.",
                            "target_lang_translations": {
                                "ru": "Дискуссия подтвердила <w>актуальность</w> поднятых автором моральных проблем."
                            }
                        }
                    ],
                    "synonyms": [
                        "aktualność"
                    ],
                    "antonyms": [
                        "przestarzałość",
                        "nieaktualność"
                    ],
                    "common_phrases": [
                        "żywotność mitu",
                        "żywotność tradycji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устойчивость и актуальность определенных идей, традиций или явлений, несмотря на течение времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живучесть",
                                "target_lang_sense_clarification": "способность сохраняться и существовать долгое время (об идеях, мифах)"
                            },
                            {
                                "target_lang_word": "актуальность",
                                "target_lang_sense_clarification": "важность и значимость в настоящий момент"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "забывать",
        "забывчивый",
        "забывчивость",
        "незабываемый",
        "забвение",
        "забытье",
        "полузабытый"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5a11fe3f-1c17-3543-a48b-209d9c9da9cf",
                    "sense_definition": "Перестать помнить информацию, события или людей; утратить воспоминание.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "memory_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Прости, я совсем <забыл> про твой день рождения.",
                            "target_lang_translations": {
                                "en": "I'm sorry, I completely <forgot> about your birthday."
                            }
                        },
                        {
                            "text": "Я <забыл> название этой улицы, но помню, где она находится.",
                            "target_lang_translations": {
                                "en": "I <forgot> the name of this street, but I remember where it is."
                            }
                        }
                    ],
                    "synonyms": [
                        "запамятовать"
                    ],
                    "antonyms": [
                        "взять"
                    ],
                    "common_phrases": [
                        "совсем забыть",
                        "забыть имя",
                        "забыть навсегда",
                        "быстро забыть"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop remembering information, events, or people; to lose a memory."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forget",
                                "target_lang_sense_clarification": "to cease remembering"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07b1f92c-ec51-322a-af3b-0bd0e47a8a43",
                    "sense_definition": "Не сделать чего-либо нужного или запланированного из-за того, что это вылетело из головы.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "negligence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, не <забудь> выключить свет перед уходом.",
                            "target_lang_translations": {
                                "en": "Please don't <forget> to turn off the light before leaving."
                            }
                        },
                        {
                            "text": "Он так спешил на работу, что <забыл> позавтракать.",
                            "target_lang_translations": {
                                "en": "He was in such a hurry to get to work that he <forgot> to eat breakfast."
                            }
                        }
                    ],
                    "synonyms": [
                        "упустить"
                    ],
                    "antonyms": [
                        "сделать"
                    ],
                    "common_phrases": [
                        "не забудь",
                        "забыть позвонить",
                        "забыть купить",
                        "забыть сделать"
                    ],
                    "target_lang_definitions": {
                        "en": "To fail to do something necessary or planned because it slipped one's mind."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forget",
                                "target_lang_sense_clarification": "to fail to do something"
                            },
                            {
                                "target_lang_word": "neglect",
                                "target_lang_sense_clarification": "to fail to attend to something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d555af47-5abb-3368-8dde-a0b3d5038635",
                    "sense_definition": "Оставить вещь где-либо по рассеянности, не взять её с собой.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "leaving_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ученик <забыл> дома тетрадь с домашним заданием.",
                            "target_lang_translations": {
                                "en": "The student <left> his homework notebook at home."
                            }
                        },
                        {
                            "text": "Мы приехали в аэропорт и поняли, что <забыли> билеты.",
                            "target_lang_translations": {
                                "en": "We arrived at the airport and realized that we had <forgotten> the tickets."
                            }
                        }
                    ],
                    "antonyms": [
                        "взять"
                    ],
                    "common_phrases": [
                        "забыть дома",
                        "забыть ключи",
                        "забыть вещи",
                        "забыть в машине"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave an item somewhere due to absent-mindedness; not to take it with oneself."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "to leave something behind (usually with a location)"
                            },
                            {
                                "target_lang_word": "forget",
                                "target_lang_sense_clarification": "to unintentionally not bring"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "812f2083-1e21-3790-af9c-6b629cd883e7",
                    "sense_definition": "Перестать сердиться, простить обиду или намеренно перестать думать о чём-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "forgiveness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Давай <забудем> старые обиды и начнём всё сначала.",
                            "target_lang_translations": {
                                "en": "Let's <forget> old grievances and start over."
                            }
                        },
                        {
                            "text": "Ему пришлось <забыть> о гордости, чтобы попросить помощи.",
                            "target_lang_translations": {
                                "en": "He had to <forget> about his pride to ask for help."
                            }
                        }
                    ],
                    "common_phrases": [
                        "давай забудем",
                        "забыть обиду",
                        "забыть о прошлом",
                        "забыть ссору"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies moving on or forgiving"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To stop being angry, forgive an offense, or intentionally stop thinking about something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forget",
                                "target_lang_sense_clarification": "to dismiss from mind"
                            },
                            {
                                "target_lang_word": "let go",
                                "target_lang_sense_clarification": "to stop holding a grudge"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ba41337d-da57-3e7f-b5cd-5611b14d5496",
                    "sense_definition": "Состояние полной утраты памяти о чём-либо; то же, что забвение.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "oblivion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его славное имя несправедливо ушло в <забыть>.",
                            "target_lang_translations": {
                                "en": "His glorious name unfairly faded into <oblivion>."
                            }
                        },
                        {
                            "text": "Старинные обычаи канули в <забыть> и больше не соблюдаются.",
                            "target_lang_translations": {
                                "en": "Ancient customs have sunk into <oblivion> and are no longer observed."
                            }
                        }
                    ],
                    "synonyms": [
                        "забвение"
                    ],
                    "antonyms": [
                        "известность"
                    ],
                    "common_phrases": [
                        "уйти в забыть",
                        "кануть в забыть"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "used in older texts or poetic style"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "simple speech style"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A state of complete memory loss regarding something; synonymous with oblivion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oblivion",
                                "target_lang_sense_clarification": "state of being forgotten"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
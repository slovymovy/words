    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c5b3e84-52d0-3879-9397-03567001b1d3",
                    "sense_definition": "Zespół budynków, urządzeń lub obiektów znajdujących się w jednym miejscu i tworzących funkcjonalną całość.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_physical_set",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W centrum miasta powstaje nowoczesny <w>kompleks</w> handlowy z kinem i restauracjami.",
                            "target_lang_translations": {
                                "ru": "В центре города строится современный торговый <w>комплекс</w> с кинотеатром и ресторанами."
                            }
                        },
                        {
                            "text": "Ten <w>kompleks</w> sportowy oferuje basen, siłownię oraz korty tenisowe.",
                            "target_lang_translations": {
                                "ru": "Этот спортивный <w>комплекс</w> предлагает бассейн, тренажерный зал и теннисные корты."
                            }
                        }
                    ],
                    "synonyms": [
                        "ośrodek",
                        "centrum",
                        "zespół"
                    ],
                    "common_phrases": [
                        "kompleks sportowy",
                        "kompleks wypoczynkowy",
                        "kompleks hotelowy",
                        "kompleks budynków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа зданий, сооружений или объектов, расположенных в одном месте и образующих функциональное целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комплекс",
                                "target_lang_sense_clarification": "зданий, сооружений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86576a07-a424-3ecb-9858-ba62d7e7efbc",
                    "sense_definition": "Zespół myśli, uczuć i wspomnień, często nieuświadomionych, które wpływają na zachowanie człowieka i wywołują lęk lub poczucie niższości.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu nastolatków ma <w>kompleksy</w> na punkcie swojego wyglądu.",
                            "target_lang_translations": {
                                "ru": "У многих подростков есть <w>комплексы</w> по поводу своей внешности."
                            }
                        },
                        {
                            "text": "Musisz uwierzyć w siebie i przestać leczyć swoje <w>kompleksy</w>.",
                            "target_lang_translations": {
                                "ru": "Ты должен поверить в себя и перестать зацикливаться на своих <w>комплексах</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "uraz",
                        "zahamowanie"
                    ],
                    "common_phrases": [
                        "mieć kompleksy",
                        "kompleks niższości",
                        "leczyć kompleksy",
                        "bez kompleksów",
                        "kompleks Edypa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность мыслей, чувств и воспоминаний, часто неосознанных, влияющих на поведение человека и вызывающих страх или чувство неполноценности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комплекс",
                                "target_lang_sense_clarification": "психологический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41f9b1ef-503e-3a6d-8d3b-8fe5a1185cce",
                    "sense_definition": "Zbiór różnych elementów, zjawisk lub cech, które są ze sobą powiązane i stanowią pewną całość.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_physical_set",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rząd przygotował cały <w>kompleks</w> działań mających na celu poprawę gospodarki.",
                            "target_lang_translations": {
                                "ru": "Правительство подготовило целый <w>комплекс</w> мер, направленных на улучшение экономики."
                            }
                        },
                        {
                            "text": "Lekarz zalecił pacjentowi przyjmowanie preparatu zawierającego <w>kompleks</w> witamin.",
                            "target_lang_translations": {
                                "ru": "Врач рекомендовал пациенту принимать препарат, содержащий <w>комплекс</w> витаминов."
                            }
                        }
                    ],
                    "synonyms": [
                        "całość",
                        "zbiór",
                        "ogół",
                        "system"
                    ],
                    "common_phrases": [
                        "kompleks zagadnień",
                        "kompleks witamin",
                        "kompleks leśny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность различных элементов, явлений или свойств, связанных между собой и составляющих единое целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комплекс",
                                "target_lang_sense_clarification": "совокупность мер, витаминов"
                            },
                            {
                                "target_lang_word": "совокупность",
                                "target_lang_sense_clarification": "общая сумма, набор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a49f35eb-5ad9-30e6-903c-7b64cea469f2",
                    "sense_definition": "Związek chemiczny składający się z atomu centralnego otoczonego przez inne atomy lub grupy atomów zwane ligandami.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wyniku reakcji powstał trwały <w>kompleks</w> miedzi o niebieskiej barwie.",
                            "target_lang_translations": {
                                "ru": "В результате реакции образовался устойчивый <w>комплекс</w> меди синего цвета."
                            }
                        },
                        {
                            "text": "Studenci badali właściwości tego <w>kompleksu</w> w laboratorium chemicznym.",
                            "target_lang_translations": {
                                "ru": "Студенты изучали свойства этого <w>комплекса</w> в химической лаборатории."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2614fe9a-cc8d-3074-8aae-37a19282ae20",
                    "sense_definition": "De handeling of het proces waarbij een deel zich losmaakt van een groter geheel, vaak gebruikt in een politieke, religieuze of organisatorische context.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vooral in de protestantse kerken kent men een rijke geschiedenis van <w>afsplitsingen</w>.",
                            "target_lang_translations": {
                                "en": "Especially in Protestant churches, there is a rich history of <w>schisms</w>.",
                                "ru": "Особенно в протестантских церквях известна богатая история <w>расколов</w>."
                            }
                        },
                        {
                            "text": "De <w>afsplitsing</w> van de politieke partij zorgde voor veel onrust in de regering.",
                            "target_lang_translations": {
                                "en": "The <w>split</w> of the political party caused a lot of unrest in the government.",
                                "ru": "<w>Раскол</w> политической партии вызвал много беспокойства в правительстве."
                            }
                        }
                    ],
                    "synonyms": [
                        "fork"
                    ],
                    "common_phrases": [
                        "afsplitsing van",
                        "politieke afsplitsing"
                    ],
                    "target_lang_definitions": {
                        "en": "The act or process of separating from a larger whole, often used in political, religious, or organizational contexts.",
                        "ru": "Действие или процесс, при котором часть отделяется от большего целого, часто используется в политическом, религиозном или организационном контексте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "split",
                                "target_lang_sense_clarification": "general separation"
                            },
                            {
                                "target_lang_word": "secession",
                                "target_lang_sense_clarification": "formal withdrawal from an organization or state"
                            },
                            {
                                "target_lang_word": "schism",
                                "target_lang_sense_clarification": "religious or ideological division"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "раскол",
                                "target_lang_sense_clarification": "в организации, партии или церкви"
                            },
                            {
                                "target_lang_word": "отделение",
                                "target_lang_sense_clarification": "процесс обособления"
                            },
                            {
                                "target_lang_word": "отпочкование",
                                "target_lang_sense_clarification": "образно об организациях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db110178-31ba-3efe-b8a6-ca09a7ef4267",
                    "sense_definition": "Een groep, deel of entiteit die zich heeft afgescheiden van een groter geheel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze nieuwe fractie is een <w>afsplitsing</w> van de liberalen.",
                            "target_lang_translations": {
                                "en": "This new faction is a <w>splinter group</w> of the liberals.",
                                "ru": "Эта новая фракция — <w>отколовшаяся группа</w> либералов."
                            }
                        },
                        {
                            "text": "Het bedrijf is een succesvolle <w>afsplitsing</w> van het grote moederconcern.",
                            "target_lang_translations": {
                                "en": "The company is a successful <w>spin-off</w> from the large parent concern.",
                                "ru": "Эта компания — успешный <w>спин-офф</w> крупного материнского концерна."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kleine afsplitsing"
                    ],
                    "target_lang_definitions": {
                        "en": "A group, part, or entity that has separated from a larger whole.",
                        "ru": "Группа, часть или организация, которая отделилась от большего целого."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "splinter group",
                                "target_lang_sense_clarification": "political or social group"
                            },
                            {
                                "target_lang_word": "spin-off",
                                "target_lang_sense_clarification": "business or corporate entity"
                            },
                            {
                                "target_lang_word": "offshoot",
                                "target_lang_sense_clarification": "general derivative entity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отколовшаяся группа",
                                "target_lang_sense_clarification": "о людях или партиях"
                            },
                            {
                                "target_lang_word": "ответвление",
                                "target_lang_sense_clarification": "о структуре или направлении"
                            },
                            {
                                "target_lang_word": "спин-офф",
                                "target_lang_sense_clarification": "о бизнесе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27711194-2b41-36e5-b670-25c48b35149d",
                    "sense_definition": "Een nieuw softwareproject dat is gestart op basis van de broncode van een bestaand project, waarbij de ontwikkeling een eigen richting opgaat.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ontwikkelaars maakten een <w>afsplitsing</w> van de code om nieuwe functies te testen.",
                            "target_lang_translations": {
                                "en": "The developers made a <w>fork</w> of the code to test new features.",
                                "ru": "Разработчики сделали <w>форк</w> кода, чтобы протестировать новые функции."
                            }
                        },
                        {
                            "text": "Deze cryptomunt is ontstaan als een <w>afsplitsing</w> van Bitcoin.",
                            "target_lang_translations": {
                                "en": "This cryptocurrency originated as a <w>fork</w> of Bitcoin.",
                                "ru": "Эта криптовалюта возникла как <w>форк</w> Биткойна."
                            }
                        }
                    ],
                    "synonyms": [
                        "fork"
                    ],
                    "target_lang_definitions": {
                        "en": "A new software project started based on the source code of an existing project.",
                        "ru": "Новый программный проект, запущенный на основе исходного кода существующего проекта, где разработка идет в своем направлении."
                    },
                    "translations": {
                        "en": [
                            {

{
    "word_family": [
        "marcher"
    ],
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c79239a3-405d-35ff-a37f-15043e853066",
                    "sense_definition": "The third month of the Gregorian calendar, following February and preceding April.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "month_name",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "The weather usually starts to get warmer <w>in March</w>.",
                            "target_lang_translations": {
                                "ru": "Погода обычно начинает теплеть <w>в марте</w>."
                            }
                        },
                        {
                            "text": "My sister was born on the first of <w>March</w>.",
                            "target_lang_translations": {
                                "ru": "Моя сестра родилась первого <w>марта</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in March",
                        "early March",
                        "late March",
                        "mid-March"
                    ],
                    "target_lang_definitions": {
                        "ru": "Третий месяц григорианского календаря, следующий за февралем и предшествующий апрелю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "март",
                                "target_lang_sense_clarification": "месяц года"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a44e8ab6-ce79-3a89-98cc-57e491297150",
                    "sense_definition": "To walk with regular, determined steps, often in a group or military formation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "walking_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers <w>marched forward</w> into battle.",
                            "target_lang_translations": {
                                "ru": "Солдаты <w>маршировали вперед</w> в бой."
                            }
                        },
                        {
                            "text": "They <w>marched</w> all day in the hot sun without stopping.",
                            "target_lang_translations": {
                                "ru": "Они <w>шагали</w> весь день под палящим солнцем без остановки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "march in step",
                        "march forward",
                        "march past",
                        "on the march"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ходить ритмичным, решительным шагом, часто в группе или военном строю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маршировать",
                                "target_lang_sense_clarification": "идти строевым шагом"
                            },
                            {
                                "target_lang_word": "шагать",
                                "target_lang_sense_clarification": "идти ритмично или решительно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c982edb-59fd-3774-a7e7-f677385aa69c",
                    "sense_definition": "To force someone to walk somewhere quickly or firmly.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "walking_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The security guard <w>marched him out</w> of the store immediately.",
                            "target_lang_translations": {
                                "ru": "Охранник немедленно <w>вывел его</w> из магазина."
                            }
                        },
                        {
                            "text": "She <w>marched</w> the children up the stairs to bed.",
                            "target_lang_translations": {
                                "ru": "Она решительно <w>повела</w> детей вверх по лестнице спать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "march someone out",
                        "march someone off",
                        "march someone into"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то быстро или решительно идти куда-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "принудительно сопровождать"
                            },
                            {
                                "target_lang_word": "выводить",
                                "target_lang_sense_clarification": "заставлять выйти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1aaa8ec8-e65b-3817-8e6c-b9d2bc2fac85",
                    "sense_definition": "To make steady or inevitable progress.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "progression_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As <w>time marches on</w>, we all get older and wiser.",
                            "target_lang_translations": {
                                "ru": "Пока <w>время идет</w>, мы все становимся старше и мудрее."
                            }
                        },
                        {
                            "text": "Technology <w>marches</w> ahead at a rapid pace every year.",
                            "target_lang_translations": {
                                "ru": "Технологии <w>шагают</w> вперед быстрыми темпами с каждым годом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "march on",
                        "time marches on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершать неуклонное или неизбежное движение вперед (о времени или событиях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "о времени или событиях"
                            },
                            {
                                "target_lang_word": "двигаться",
                                "target_lang_sense_clarification": "развиваться вперед"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9fec499a-d51f-345a-a997-653779e4351f",
                    "sense_definition": "A formal, rhythmic way of walking, used especially by soldiers, bands, or in ceremonies.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "walking_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The army is <w>on the march</w> towards the capital city.",
                            "target_lang_translations": {
                                "ru": "Армия находится <w>на марше</w> к столице."
                            }
                        },
                        {
                            "text": "The soldiers began their long <w>march</w> back to the camp.",
                            "target_lang_translations": {
                                "ru": "Солдаты начали свой долгий <w>переход</w> обратно в лагерь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on the march",
                        "slow march",
                        "quick march",
                        "long march"
                    ],
                    "target_lang_definitions": {
                        "ru": "Формальный, ритмичный способ ходьбы, используемый солдатами или оркестрами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марш",
                                "target_lang_sense_clarification": "строевой шаг или передвижение войск"
                            },
                            {
                                "target_lang_word": "поход",
                                "target_lang_sense_clarification": "передвижение войск"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09806fd1-4582-3a19-8925-960c1f6fecc8",
                    "sense_definition": "A public event where many people walk together to express support or opposition to a cause.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "protest_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They organized a <w>protest march</w> through the city center.",
                            "target_lang_translations": {
                                "ru": "Они организовали <w>марш протеста</w> через центр города."
                            }
                        },
                        {
                            "text": "Thousands of people joined the <w>march</w> for equal rights.",
                            "target_lang_translations": {
                                "ru": "Тысячи людей присоединились к <w>шествию</w> за равные права."
                            }
                        }
                    ],
                    "synonyms": [
                        "protest",
                        "rally",
                        "parade"
                    ],
                    "common_phrases": [
                        "protest march",
                        "peace march",
                        "go on a march"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публичное мероприятие, на котором множество людей идут вместе, выражая поддержку или протест."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марш",
                                "target_lang_sense_clarification": "демонстрация"
                            },
                            {
                                "target_lang_word": "шествие",
                                "target_lang_sense_clarification": "массовое движение людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7c88d45-a50b-3423-b8f9-02dcb2910da8",
                    "sense_definition": "A piece of music with a strong regular rhythm, originally written for marching to.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "music_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The organist played the <w>wedding march</w> beautifully as she entered.",
                            "target_lang_translations": {
                                "ru": "Органист прекрасно сыграл <w>свадебный марш</w>, когда она вошла."
                            }
                        },
                        {
                            "text": "The school band practiced the new <w>march</w> for the parade.",
                            "target_lang_translations": {
                                "ru": "Школьный оркестр репетировал новый <w>марш</w> для парада."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wedding march",
                        "funeral march",
                        "military march"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальное произведение с сильным регулярным ритмом, изначально написанное для сопровождения ходьбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марш",
                                "target_lang_sense_clarification": "музыкальный жанр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bbfc075-366c-3152-9f31-ccc729d89e06",
                    "sense_definition": "Steady forward movement or development that cannot be stopped.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "progression_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You cannot stop the relentless <w>march of time</w>.",
                            "target_lang_translations": {
                                "ru": "Невозможно остановить неумолимый <w>ход времени</w>."
                            }
                        },
                        {
                            "text": "The <w>march</w> of technology has improved communication across the world.",
                            "target_lang_translations": {
                                "ru": "<w>Развитие</w> технологий улучшило связь во всем мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "progression",
                        "advancement"
                    ],
                    "common_phrases": [
                        "march of time",
                        "march of progress",
                        "march of history"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неуклонное движение вперед или развитие, которое невозможно остановить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ход",
                                "target_lang_sense_clarification": "течение времени или событий"
                            },
                            {
                                "target_lang_word": "течение",
                                "target_lang_sense_clarification": "развитие процесса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8142be85-7842-3d2a-ac6e-442b86d2b6ec",
                    "sense_definition": "A border region or frontier area, especially one defended by military forces in the past.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "border_region",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many ancient castles still stand in the <w>Welsh Marches</w>.",
                            "target_lang_translations": {
                                "ru": "Многие древние замки все еще стоят в <w>Валлийских марках</w>."
                            }
                        },
                        {
                            "text": "The lords of the <w>marches</w> were responsible for defending the border.",
                            "target_lang_translations": {
                                "ru": "Лорды <w>марок</w> отвечали за оборону границы."
                            }
                        }
                    ],
                    "synonyms": [
                        "frontier",
                        "borderland"
                    ],
                    "common_phrases": [
                        "Welsh Marches"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "Used primarily in historical contexts referring to medieval borderlands."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пограничный регион или область, особенно охраняемая военными силами в прошлом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марка",
                                "target_lang_sense_clarification": "историческая пограничная область"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
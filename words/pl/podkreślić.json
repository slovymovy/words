                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подчеркнуть",
                                "target_lang_sense_clarification": "выделить важность"
                            },
                            {
                                "target_lang_word": "отметить",
                                "target_lang_sense_clarification": "обратить внимание"
                            }
                        ]
                    }
                },
                {

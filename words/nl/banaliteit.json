    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0e473b76-2fae-3ccb-a62e-0ce4a9ed1624",
                    "sense_definition": "De eigenschap of toestand van alledaags, platvloers of niet-origineel zijn.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De filosoof Hannah Arendt schreef een bekend boek over de <w>banaliteit</w> van het kwaad.",
                            "target_lang_translations": {
                                "en": "The philosopher Hannah Arendt wrote a famous book about the <w>banality</w> of evil."
                            }
                        },
                        {
                            "text": "Hij stoorde zich aan de <w>banaliteit</w> van de gesprekken op het feestje.",
                            "target_lang_translations": {
                                "en": "He was annoyed by the <w>banality</w> of the conversations at the party."
                            }
                        }
                    ],
                    "synonyms": [
                        "alledaagsheid",
                        "platvloersheid"
                    ],
                    "antonyms": [
                        "originaliteit",
                        "diepzinnigheid"
                    ],
                    "common_phrases": [
                        "de banaliteit van het kwaad"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality or state of being everyday, common, or unoriginal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "banality",
                                "target_lang_sense_clarification": "the quality of being unoriginal or boring"
                            },
                            {
                                "target_lang_word": "triteness",
                                "target_lang_sense_clarification": "lack of originality or freshness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a424e855-143f-3cc2-923b-0b8a13c7bebe",
                    "sense_definition": "Een opmerking of gedachte die niet origineel is en vaak wordt gebruikt; een cliché.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "concrete_instance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de receptie wisselden de gasten slechts enkele beleefde <w>banaliteiten</w> uit.",
                            "target_lang_translations": {
                                "en": "During the reception, the guests exchanged only a few polite <w>banalities</w>."
                            }
                        },
                        {
                            "text": "De schrijver wilde voorkomen dat hij zou <w>vervallen in banaliteiten</w> in zijn nieuwe roman.",
                            "target_lang_translations": {
                                "en": "The writer wanted to avoid <w>falling into banalities</w> in his new novel."
                            }
                        }
                    ],
                    "synonyms": [
                        "gemeenplaats",
                        "cliché",
                        "platitude"
                    ],
                    "common_phrases": [
                        "vervallen in banaliteiten",
                        "banaliteiten uitwisselen"
                    ],
                    "target_lang_definitions": {
                        "en": "A remark or thought that is not original and is often used; a cliché."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "platitude",
                                "target_lang_sense_clarification": "an unoriginal or obvious remark"
                            },
                            {
                                "target_lang_word": "banality",
                                "target_lang_sense_clarification": "a trite or obvious remark"
                            },
                            {
                                "target_lang_word": "cliché",
                                "target_lang_sense_clarification": "a phrase or opinion that is overused"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "overleggen",
        "overlegging"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9e9749af-8853-396b-a061-328d2029d573",
                    "sense_definition": "Het gezamenlijk bespreken van een onderwerp om tot een oordeel, oplossing of besluit te komen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "discussion_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na kort <w>overleg</w> besloten we om het voorstel te accepteren.",
                            "target_lang_translations": {
                                "en": "After brief <w>deliberation</w>, we decided to accept the proposal."
                            }
                        },
                        {
                            "text": "Er is veel <w>overleg</w> nodig tussen de verschillende afdelingen om dit project te laten slagen.",
                            "target_lang_translations": {
                                "en": "Much <w>consultation</w> is needed between the different departments to make this project succeed."
                            }
                        }
                    ],
                    "synonyms": [
                        "bespreking",
                        "beraad",
                        "consultatie"
                    ],
                    "common_phrases": [
                        "in overleg met",
                        "na overleg",
                        "overleg plegen",
                        "in overleg treden"
                    ],
                    "target_lang_definitions": {
                        "en": "Joint discussion or consultation to reach a judgment, solution, or decision."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "consultation",
                                "target_lang_sense_clarification": "act of consulting or discussing together"
                            },
                            {
                                "target_lang_word": "deliberation",
                                "target_lang_sense_clarification": "careful consideration or discussion"
                            },
                            {
                                "target_lang_word": "discussion",
                                "target_lang_sense_clarification": "general exchange of views"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e055da2-367b-3686-aad9-1e7c5bb7825d",
                    "sense_definition": "Een georganiseerde bijeenkomst of een vast orgaan waarin partijen regelmatig met elkaar spreken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "discussion_concrete",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het wekelijkse <w>overleg</w> met het management is verplaatst naar dinsdag.",
                            "target_lang_translations": {
                                "en": "The weekly <w>meeting</w> with management has been moved to Tuesday."
                            }
                        },
                        {
                            "text": "De vakbonden hebben het <w>overleg</w> met de werkgevers tijdelijk opgeschort.",
                            "target_lang_translations": {
                                "en": "The unions have temporarily suspended the <w>consultation</w> with the employers."
                            }
                        }
                    ],
                    "synonyms": [
                        "vergadering",
                        "bijeenkomst"
                    ],
                    "common_phrases": [
                        "periodiek overleg",
                        "bestuurlijk overleg",
                        "werkoverleg"
                    ],
                    "target_lang_definitions": {
                        "en": "An organized meeting or regular body where parties speak regularly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "meeting",
                                "target_lang_sense_clarification": "scheduled gathering"
                            },
                            {
                                "target_lang_word": "consultation",
                                "target_lang_sense_clarification": "formal meeting structure"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7dc46f6a-d9d5-3401-b9b7-fd618897e01c",
                    "sense_definition": "Находящийся, происходящий или совершающийся на улице, под открытым небом.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "locational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Яркий <w>уличный</w> фонарь освещал вход в подъезд.",
                            "target_lang_translations": {
                                "en": "A bright <w>street</w> lamp lit up the entrance to the building."
                            }
                        },
                        {
                            "text": "Через открытое окно в комнату доносился громкий <w>уличный</w> шум.",
                            "target_lang_translations": {
                                "en": "Loud <w>street</w> noise drifted into the room through the open window."
                            }
                        }
                    ],
                    "common_phrases": [
                        "уличный фонарь",
                        "уличный музыкант",
                        "уличный шум",
                        "уличная торговля"
                    ],
                    "target_lang_definitions": {
                        "en": "Located, occurring, or taking place on the street, outdoors."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "street",
                                "target_lang_sense_clarification": "general attributive"
                            },
                            {
                                "target_lang_word": "outdoor",
                                "target_lang_sense_clarification": "happening outside"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67ec92db-d3ac-3d81-bc10-75f48eec6443",
                    "sense_definition": "Живущий на улице, не имеющий дома или хозяев (обычно о животных).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот рыжий <w>уличный</w> кот приходит к нам каждый вечер за едой.",
                            "target_lang_translations": {
                                "en": "This ginger <w>stray</w> cat comes to us every evening for food."
                            }
                        },
                        {
                            "text": "Во дворе громко лаяла большая <w>уличная</w> собака.",
                            "target_lang_translations": {
                                "en": "A big <w>stray</w> dog was barking loudly in the yard."
                            }
                        }
                    ],
                    "synonyms": [
                        "бездомный",
                        "бродячий"
                    ],
                    "antonyms": [
                        "внеуличный"
                    ],
                    "common_phrases": [
                        "уличный кот",
                        "уличная собака",
                        "уличный пёс"
                    ],
                    "target_lang_definitions": {
                        "en": "Living on the street, homeless (usually regarding animals)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stray",
                                "target_lang_sense_clarification": "homeless animal"
                            },
                            {
                                "target_lang_word": "street",
                                "target_lang_sense_clarification": "living on the street"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b40c081-059f-307d-9b22-b38f088502b6",
                    "sense_definition": "Предназначенный для ношения или использования вне дома, на улице.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "locational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, снимите грязную <w>уличную</w> обувь в коридоре.",
                            "target_lang_translations": {
                                "en": "Please take off your dirty <w>outdoor</w> shoes in the hallway."
                            }
                        },
                        {
                            "text": "Зимой для прогулок необходима тёплая <w>уличная</w> одежда.",
                            "target_lang_translations": {
                                "en": "In winter, warm <w>outdoor</w> clothing is necessary for walks."
                            }
                        }
                    ],
                    "common_phrases": [
                        "уличная обувь",
                        "уличная одежда"
                    ],
                    "target_lang_definitions": {
                        "en": "Intended for wearing or use outside the home."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "outdoor",
                                "target_lang_sense_clarification": "clothing or shoes"
                            },
                            {
                                "target_lang_word": "street",
                                "target_lang_sense_clarification": "worn outside"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd73b47b-8aa3-3827-85e3-c5f0f54d0003",
                    "sense_definition": "Связанный с улицей как объекту городской инфраструктуры (обычно о транспорте или движении).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "locational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Интенсивное <w>уличное</w> движение затрудняло переход дороги.",
                            "target_lang_translations": {
                                "en": "Heavy <w>street</w> traffic made crossing the road difficult."
                            }
                        },
                        {
                            "text": "Городской транспорт традиционно подразделяется на <w>уличный</w> и внеуличный.",
                            "target_lang_translations": {
                                "en": "Urban transport is traditionally divided into <w>street</w> and off-street transport."
                            }
                        }
                    ],
                    "antonyms": [
                        "внеуличный"
                    ],
                    "common_phrases": [
                        "уличное движение",
                        "уличный транспорт",
                        "уличный комитет"
                    ],
                    "target_lang_definitions": {
                        "en": "Related to the street as urban infrastructure (transport/traffic)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "street",
                                "target_lang_sense_clarification": "infrastructure or traffic"

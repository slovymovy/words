    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a0f488eb-ea4e-3898-8e79-2db90b7836f4",
                    "sense_definition": "Горная система в Азии, расположенная на территории России, Казахстана, Монголии и Китая.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "geo_physical",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Каждое лето тысячи туристов отправляются в поход по <w>Алтаю</w>.",
                            "target_lang_translations": {
                                "en": "Every summer, thousands of tourists go hiking in the <w>Altai</w>."
                            }
                        },
                        {
                            "text": "Природа <w>Алтая</w> славится своими чистыми реками и высокими горами.",
                            "target_lang_translations": {
                                "en": "The nature of the <w>Altai</w> is famous for its clean rivers and high mountains."
                            }
                        }
                    ],
                    "synonyms": [
                        "Алтайские горы"
                    ],
                    "common_phrases": [
                        "Горный Алтай",
                        "поездка на Алтай",
                        "природа Алтая",
                        "золотой Алтай"
                    ],
                    "target_lang_definitions": {
                        "en": "A mountain system in Asia, located on the territory of Russia, Kazakhstan, Mongolia, and China."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Altai",
                                "target_lang_sense_clarification": "mountain range"
                            },
                            {
                                "target_lang_word": "Altay",
                                "target_lang_sense_clarification": "mountain range (variant spelling)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00f1e764-0954-3eb0-9e82-e7d8cb5ee8f8",
                    "sense_definition": "Республика в составе Российской Федерации, расположенная в южной части Сибири (столица — Горно-Алтайск).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "geo_admin",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Глава <w>Алтая</w> провел встречу с представителями туристического бизнеса.",
                            "target_lang_translations": {
                                "en": "The head of <w>Altai</w> held a meeting with representatives of the tourism business."
                            }
                        },
                        {
                            "text": "Этот национальный праздник широко отмечается во всей Республике <w>Алтай</w>.",
                            "target_lang_translations": {
                                "en": "This national holiday is widely celebrated throughout the <w>Altai</w> Republic."
                            }
                        }
                    ],
                    "synonyms": [
                        "Республика Алтай"
                    ],
                    "common_phrases": [
                        "правительство Алтая",
                        "народы Алтая"
                    ],
                    "target_lang_definitions": {
                        "en": "A republic within the Russian Federation, located in the southern part of Siberia (capital — Gorno-Altaysk)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Altai",
                                "target_lang_sense_clarification": "republic in Russia"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e4dc408-07b8-380a-af53-81481db15f4d",
                    "sense_definition": "Алтайский край, субъект Российской Федерации с административным центром в городе Барнаул.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "geo_admin",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Мои родственники живут на <w>Алтае</w>, недалеко от Барнаула.",
                            "target_lang_translations": {
                                "en": "My relatives live in the <w>Altai</w>, not far from Barnaul."
                            }
                        },
                        {
                            "text": "В этом году на <w>Алтае</w> собрали рекордный урожай пшеницы.",
                            "target_lang_translations": {
                                "en": "This year, a record wheat harvest was gathered in the <w>Altai</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Алтайский край"
                    ],
                    "common_phrases": [
                        "губернатор Алтая",
                        "поля Алтая"
                    ],
                    "target_lang_definitions": {
                        "en": "Altai Krai, a federal subject of the Russian Federation with its administrative center in the city of Barnaul."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Altai",
                                "target_lang_sense_clarification": "region in Russia (Altai Krai)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fb29f06-67e5-36e6-9615-963f32c97cd6",
                    "sense_definition": "Округ в Синьцзян-Уйгурском автономном районе Китая.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "geo_foreign",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Китайский <w>Алтай</w> привлекает путешественников своими уникальными ландшафтами.",
                            "target_lang_translations": {
                                "en": "The Chinese <w>Altay</w> attracts travelers with its unique landscapes."
                            }

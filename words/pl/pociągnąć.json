            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d99cf1b6-8325-333a-8476-74b0692044b2",
                    "sense_definition": "Gwałtownie przyciągnąć coś do siebie lub przesunąć coś, używając siły fizycznej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopiec mocno <w>pociągnął</w> psa za smycz, żeby go zatrzymać przed wejściem na ulicę.",
                            "target_lang_translations": {
                                "ru": "Мальчик сильно <w>дёрнул</w> собаку за поводок, чтобы остановить её перед выходом на улицу."
                            }
                        },
                        {
                            "text": "Musisz mocno <w>pociągnąć</w> za klamkę, ponieważ te stare drzwi często się zacinają.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно сильно <w>потянуть</w> за ручку, так как эта старая дверь часто заедает."
                            }
                        }
                    ],
                    "synonyms": [
                        "szarpnąć"
                    ],
                    "antonyms": [
                        "popchnąć"
                    ],
                    "common_phrases": [
                        "pociągnąć za sznurek",
                        "pociągnąć za spust",
                        "pociągnąć za klamkę",
                        "pociągnąć za język"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко притянуть что-то к себе или сдвинуть с места, применяя физическую силу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потянуть",
                                "target_lang_sense_clarification": "сдвинуть с места или притянуть"
                            },
                            {
                                "target_lang_word": "дёрнуть",
                                "target_lang_sense_clarification": "резко потянуть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05510646-a0ef-3507-8f73-5efed2ab2c9f",
                    "sense_definition": "Spowodować wystąpienie określonych skutków, następstw lub konsekwencji.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "causality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta nieprzemyślana decyzja może <w>pociągnąć</w> za sobą bardzo poważne konsekwencje finansowe.",
                            "target_lang_translations": {
                                "ru": "Это необдуманное решение может <w>повлечь</w> за собой очень серьезные финансовые последствия."
                            }
                        },
                        {
                            "text": "Błąd kierowcy <w>pociągnął</w> za sobą tragiczny wypadek na autostradzie.",
                            "target_lang_translations": {
                                "ru": "Ошибка водителя <w>повлекла</w> за собой трагическую аварию на автостраде."
                            }
                        }
                    ],
                    "synonyms": [
                        "spowodować",
                        "wywołać"
                    ],
                    "common_phrases": [
                        "pociągnąć za sobą skutki",
                        "pociągnąć za sobą konsekwencje",
                        "pociągnąć za sobą ofiary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызвать определенные последствия, результаты или исходы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повлечь",
                                "target_lang_sense_clarification": "вызвать последствия (обычно с 'за собой')"
                            },
                            {
                                "target_lang_word": "привести",
                                "target_lang_sense_clarification": "к каким-либо последствиям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fe281c6-3d25-3550-97ee-6cc66632b063",
                    "sense_definition": "Forma dokonana czasownika <w>pociągać</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedyś to zagadnienie mnie pociągało, ale teraz coś innego zdołało mnie <w>pociągnąć</w> w nowym kierunku.",
                            "target_lang_translations": {
                                "ru": "Когда-то этот вопрос меня привлекал, но теперь что-то другое смогло <w>увлечь</w> меня в новом направлении."
                            }
                        },
                        {
                            "text": "On zwykle pociąga za sznurki w tej firmie, ale wczoraj to dyrektor musiał <w>pociągnąć</w> za ten najważniejszy.",
                            "target_lang_translations": {
                                "ru": "Обычно он дергает за ниточки в этой фирме, но вчера директору пришлось <w>дёрнуть</w> за самую важную."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "perfective aspect"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершенный вид глагола 'pociągać' (в значении увлекать или тянуть)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увлечь",
                                "target_lang_sense_clarification": "заинтересовать"
                            },
                            {
                                "target_lang_word": "потянуть",
                                "target_lang_sense_clarification": "физическое действие или направление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81186b17-2074-31e0-9bfc-dfbdee40342b",
                    "sense_definition": "Wypić pewną ilość płynu, zazwyczaj alkoholu, w szybki i zdecydowany sposób.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spragniony turysta chwycił butelkę i <w>pociągnął</w> duży łyk zimnej wody.",
                            "target_lang_translations": {
                                "ru": "Измученный жаждой турист схватил бутылку и <w>отпил</w> большой глоток холодной воды."
                            }
                        },
                        {
                            "text": "Na imprezie kolega namówił go, żeby <w>pociągnął</w> wino prosto z gąsiora.",
                            "target_lang_translations": {
                                "ru": "На вечеринке друг уговорил его, чтобы он <w>хлебнул</w> вина прямо из бутыли."
                            }
                        }
                    ],
                    "synonyms": [
                        "łyknąć",
                        "wypić"
                    ],
                    "common_phrases": [
                        "pociągnąć z butelki",
                        "pociągnąć łyk",
                        "pociągnąć z gwinta"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпить некоторое количество жидкости (обычно алкоголя) быстрым и решительным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хлебнуть",
                                "target_lang_sense_clarification": "разговорное, выпить глоток"
                            },
                            {
                                "target_lang_word": "отпить",
                                "target_lang_sense_clarification": "сделать глоток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "939cb3ed-e9e7-3502-aa27-5017e3ca95e3",
                    "sense_definition": "Przejechać narzędziem, takim jak pędzel, po powierzchni w celu pokrycia jej warstwą substancji.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Malarz postanowił <w>pociągnąć</w> ścianę jeszcze jedną warstwą niebieskiej farby.",
                            "target_lang_translations": {
                                "ru": "Маляр решил <w>покрыть</w> стену еще одним слоем синей краски."
                            }
                        },
                        {
                            "text": "Wystarczy raz <w>pociągnąć</w> pędzlem, aby odświeżyć ten stary, drewniany mebel.",
                            "target_lang_translations": {
                                "ru": "Достаточно один раз <w>провести</w> кистью, чтобы освежить эту старую деревянную мебель."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomalować"
                    ],
                    "common_phrases": [
                        "pociągnąć pędzlem",
                        "pociągnąć farbą",
                        "pociągnąć kreskę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Провести инструментом (например, кистью) по поверхности, чтобы покрыть её слоем вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провести",
                                "target_lang_sense_clarification": "кистью или инструментом"
                            },
                            {
                                "target_lang_word": "покрыть",
                                "target_lang_sense_clarification": "нанести слой (краски)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18c98b2f-34b1-3c35-ad1c-9597d2cf0576",
                    "sense_definition": "O wietrze lub powietrzu: zacząć wiać lub przemieścić się, tworząc odczuwalny przewiew.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "meteorological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy otworzył drzwi na klatkę schodową, z korytarza nieprzyjemnie <w>pociągnęło</w> wilgocią i chłodem.",
                            "target_lang_translations": {
                                "ru": "Когда он открыл дверь на лестничную клетку, из коридора неприятно <w>потянуло</w> сыростью и холодом."
                            }
                        },
                        {
                            "text": "Siedziałem przy nieszczelnym oknie i nagle <w>pociągnęło</w> zimnym powietrzem z zewnątrz.",
                            "target_lang_translations": {
                                "ru": "Я сидел у неплотно закрытого окна, и вдруг снаружи <w>потянуло</w> холодным воздухом."
                            }
                        }
                    ],
                    "synonyms": [
                        "powiać"
                    ],
                    "common_phrases": [
                        "pociągnęło chłodem",
                        "pociągnęło od okna"
                    ],
                    "target_lang_definitions": {
                        "ru": "О ветре или воздухе: начать дуть или перемещаться, создавая ощутимый сквозняк."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потянуть",
                                "target_lang_sense_clarification": "о сквозняке, холоде или запахе"
                            },
                            {
                                "target_lang_word": "повеять",
                                "target_lang_sense_clarification": "о ветре или запахе"
                            }
                        ]
                    }
                }
            ]
        }
    ]

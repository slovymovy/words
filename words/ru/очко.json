    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fe114502-66a7-3d30-abed-8b085bb18cb4",
                    "sense_definition": "Единица счёта в спортивных играх и соревнованиях, используемая для определения победителя.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "score",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша команда выиграла этот матч, набрав решающее <w>очко</w> на последних минутах.",
                            "target_lang_translations": {
                                "en": "Our team won this match by scoring the deciding <w>point</w> in the final minutes."
                            }
                        },
                        {
                            "text": "За правильный ответ в викторине участник получает одно <w>очко</w>.",
                            "target_lang_translations": {
                                "en": "For a correct answer in the quiz, the participant receives one <w>point</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "набрать очки",
                        "счёт по очкам",
                        "штрафное очко"
                    ],
                    "target_lang_definitions": {
                        "en": "A unit of scoring in sports games and competitions, used to determine the winner."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "point",
                                "target_lang_sense_clarification": "unit of scoring"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9eb0148-a5ad-3d47-9861-b273f2f105b2",
                    "sense_definition": "Азартная карточная игра, также известная как «двадцать одно».",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "По вечерам мужчины собирались во дворе и играли в <w>очко</w> на мелочь.",
                            "target_lang_translations": {
                                "en": "In the evenings, the men would gather in the courtyard and play <w>blackjack</w> for small change."
                            }
                        },
                        {
                            "text": "Он хорошо знал правила и часто выигрывал, когда мы садились играть в <w>очко</w>.",
                            "target_lang_translations": {
                                "en": "He knew the rules well and often won when we sat down to play <w>blackjack</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "двадцать одно"
                    ],
                    "common_phrases": [
                        "играть в очко"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A gambling card game, also known as \"twenty-one\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blackjack",
                                "target_lang_sense_clarification": "card game"
                            },
                            {
                                "target_lang_word": "twenty-one",
                                "target_lang_sense_clarification": "card game"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bfbe7a1-0936-370c-8d80-209039fb6089",
                    "sense_definition": "Метка на игральной карте или грани кубика, обозначающая числовое значение.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "game_element",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На выпавшем кубике было всего одно <w>очко</w>.",
                            "target_lang_translations": {
                                "en": "There was only one <w>pip</w> on the rolled die."
                            }
                        },
                        {
                            "text": "Игрок подсчитал <w>очки</w> на своих картах и понял, что победил.",
                            "target_lang_translations": {
                                "en": "The player counted the <w>points</w> on his cards and realized he had won."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A mark on a playing card or face of a die, denoting a numerical value."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pip",
                                "target_lang_sense_clarification": "on a die or domino"
                            },
                            {
                                "target_lang_word": "point",
                                "target_lang_sense_clarification": "numerical value"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "741aafe0-c8ea-3887-81ae-c97681d5f510",
                    "sense_definition": "Отверстие в полу туалета или сам унитаз (обычно грязный или примитивный).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "toilet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В деревенском туалете вместо унитаза было простое деревянное <w>очко</w>.",
                            "target_lang_translations": {
                                "en": "In the village toilet, instead of a bowl, there was just a simple wooden <w>hole</w>."
                            }
                        },
                        {
                            "text": "Он случайно уронил ключи прямо в <w>очко</w> и долго не мог их достать.",
                            "target_lang_translations": {
                                "en": "He accidentally dropped his keys right into the <w>hole</w> and couldn't get them out for a long time."
                            }
                        }
                    ],
                    "synonyms": [
                        "толчок",
                        "сортир",
                        "нужник",
                        "уборная",
                        "туалет",
                        "клозет",
                        "гальюн",
                        "дальняк"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "грубое"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A hole in the floor of a toilet or the toilet bowl itself (usually dirty or primitive)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hole",
                                "target_lang_sense_clarification": "in a latrine"
                            },
                            {
                                "target_lang_word": "squat toilet",
                                "target_lang_sense_clarification": "sanitary fixture"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9c9f07f-d89b-324d-b407-f50cf01a9a8c",
                    "sense_definition": "Грубое название заднего прохода (ануса).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В уличных драках и тюремном жаргоне слово <w>очко</w> используется как оскорбление или угроза.",
                            "target_lang_translations": {
                                "en": "In street fights and prison slang, the word <w>asshole</w> is used as an insult or threat."
                            }
                        },
                        {
                            "text": "От сильного испуга у него, как говорится, <w>очко</w> сыграло.",
                            "target_lang_translations": {
                                "en": "From sheer fright, his <w>asshole</w> puckered, as the saying goes."
                            }
                        }
                    ],
                    "synonyms": [
                        "жопник"
                    ],
                    "common_phrases": [
                        "очко играет"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "вульгарное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A rude name for the anus."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "asshole",
                                "target_lang_sense_clarification": "vulgar"
                            },
                            {
                                "target_lang_word": "butthole",
                                "target_lang_sense_clarification": "vulgar"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d21a55a-5949-3e9d-a051-2444f513e4a9",
                    "sense_definition": "Небольшое отверстие, ячейка в сети или петля.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hole_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рыба была мелкой и легко проходила сквозь <w>очко</w> рыболовной сети.",
                            "target_lang_translations": {
                                "en": "The fish was small and easily passed through the <w>mesh</w> of the fishing net."
                            }
                        },
                        {
                            "text": "Мастер аккуратно продел толстую нитку в <w>очко</w>.",
                            "target_lang_translations": {
                                "en": "The craftsman carefully threaded the thick string into the <w>eye</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "глазок",
                        "огон"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "специальное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A small hole, cell in a net, or loop."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mesh",
                                "target_lang_sense_clarification": "of a net"
                            },
                            {
                                "target_lang_word": "eye",
                                "target_lang_sense_clarification": "loop or hole"
                            },
                            {
                                "target_lang_word": "eyelet",
                                "target_lang_sense_clarification": "small hole"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2075f6e2-37dc-3055-ac72-918221d57336",
                    "sense_definition": "Оценка ситуации как хорошей, нормальной или удачной.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "evaluation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не переживай за экзамен, там всё будет <w>очко</w>.",
                            "target_lang_translations": {
                                "en": "Don't worry about the exam, everything will be <w>cool</w>."
                            }
                        },
                        {
                            "text": "Погода наладилась, настроение <w>очко</w>, можно идти гулять.",
                            "target_lang_translations": {
                                "en": "The weather has cleared up, the mood is <w>awesome</w>, we can go for a walk."
                            }
                        }
                    ],
                    "antonyms": [
                        "плохо",
                        "ужасно",
                        "отвратительно",
                        "кошмарно",
                        "нехорошо"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Evaluation of a situation as good, normal, or successful."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cool",
                                "target_lang_sense_clarification": "slang"
                            },
                            {
                                "target_lang_word": "awesome",
                                "target_lang_sense_clarification": "slang"
                            }
                        ]
                    }

                {
                    "sense_id": "f8568068-8887-3771-a770-eafd7b2feef5",
                    "sense_definition": "De eigenschap van enorm zwaar, groot en massief te zijn.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>massiviteit</w> van het oude kasteel was indrukwekkend om te zien.",
                            "target_lang_translations": {
                                "en": "The <w>massiveness</w> of the old castle was impressive to see.",
                                "ru": "<w>Массивность</w> старого замка выглядела впечатляюще."
                            }
                        },
                        {
                            "text": "Door de <w>massiviteit</w> van de rotsblokken konden we ze niet verplaatsen.",
                            "target_lang_translations": {
                                "en": "Due to the <w>massiveness</w> of the boulders, we could not move them.",
                                "ru": "Из-за <w>массивности</w> валунов мы не могли их сдвинуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwaarte",
                        "gewicht",
                        "massa"
                    ],
                    "common_phrases": [
                        "indrukwekkende massiviteit",
                        "pure massiviteit"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality of being enormously heavy, large, and solid.",
                        "ru": "Свойство быть очень тяжелым, большим и массивным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "massiveness",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e1fb8c8b-dbd8-3498-a6e4-b89e296ac77e",
                    "sense_definition": "Dorosły samiec kozy, często kojarzony z charakterystycznym, nieprzyjemnym zapachem i brodą.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_male",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na górskiej łące pasł się duży, rogaty <cap>.",
                            "target_lang_translations": {
                                "ru": "На горном лугу пасся большой рогатый <w>козёл</w>."
                            }
                        },
                        {
                            "text": "Ten <cap> wydziela tak intensywny zapach, że czuć go z daleka.",
                            "target_lang_translations": {
                                "ru": "Этот <w>козёл</w> издаёт такой сильный запах, что его чувствуешь издалека."
                            }
                        }
                    ],
                    "synonyms": [
                        "łap"
                    ],
                    "common_phrases": [
                        "śmierdzieć jak cap",
                        "uparty jak cap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взрослый самец козы, часто ассоциируемый с характерным неприятным запахом и бородой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "козёл",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a36dec2-6ce7-3bf6-9691-badf0fffc9d3",
                    "sense_definition": "Pospolite, obraźliwe określenie mężczyzny uważanego za głupiego, ograniczonego lub złośliwego.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "person_insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie słuchaj go, to zwykły <cap>, który na niczym się nie zna.",
                            "target_lang_translations": {
                                "ru": "Не слушай его, это обычный <w>козёл</w>, который ни в чём не разбирается."
                            }
                        },
                        {
                            "text": "Ale z niego <cap>, znowu zapomniał zamknąć drzwi.",
                            "target_lang_translations": {
                                "ru": "Ну и <w>болван</w> же он, опять забыл закрыть дверь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "głupi cap"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубое, оскорбительное название мужчины, считающегося глупым, ограниченным или злобным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "козёл",
                                "target_lang_sense_clarification": "ругательство"
                            },
                            {
                                "target_lang_word": "болван",
                                "target_lang_sense_clarification": "глупец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4477c52a-2a9a-3174-8aaa-b978eded25c9",
                    "sense_definition": "Pogardliwe określenie starszego mężczyzny, często postrzeganego jako lubieżny lub zrzędliwy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jakiś stary <cap> zaczepiał młode dziewczyny w parku.",
                            "target_lang_translations": {
                                "ru": "Какой-то <w>старый хрыч</w> приставал к молодым девушкам в парке."
                            }
                        },
                        {
                            "text": "Nie bądź takim zrzędliwym <capem> i daj nam spokój.",
                            "target_lang_translations": {
                                "ru": "Не будь таким ворчливым <w>старым козлом</w> и оставь нас в покое."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stary cap"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Презрительное название пожилого мужчины, часто воспринимаемого как похотливый или ворчливый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "старый хрыч",
                                "target_lang_sense_clarification": "о старике"
                            },
                            {
                                "target_lang_word": "старый козёл",
                                "target_lang_sense_clarification": "ругательство"
                            }
                        ]
                    }
                }
            ]
        },
        {

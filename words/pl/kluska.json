    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f285f044-802a-3a4e-945b-ff0aaa320226",
                    "sense_definition": "Kawałek ciasta z mąki i wody, często z dodatkiem jajek lub ziemniaków, gotowany we wrzątku; popularny dodatek do zup lub dań mięsnych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W niedzielę na obiad zawsze jemy rosół z domowymi <kluskami>.",
                            "target_lang_translations": {
                                "ru": "В воскресенье на обед мы всегда едим бульон с домашней <лапшой>."
                            }
                        },
                        {
                            "text": "Moja babcia robi najlepsze <kluski> leniwe z cukrem i cynamonem.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка готовит лучшие ленивые <вареники> с сахаром и корицей."
                            }
                        }
                    ],
                    "synonyms": [
                        "makaron",
                        "kopytko",
                        "pyza"
                    ],
                    "common_phrases": [
                        "kluski śląskie",
                        "kluski leniwe",
                        "kluski kładzione",
                        "kluski z makiem",
                        "rosół z kluskami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусочек теста из муки и воды, часто с добавлением яиц или картофеля, сваренный в кипятке; популярная добавка к супам или мясным блюдам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клёцка",
                                "target_lang_sense_clarification": "кулинарное изделие"
                            },
                            {
                                "target_lang_word": "галушка",
                                "target_lang_sense_clarification": "вид клёцек"
                            },
                            {
                                "target_lang_word": "лапша",
                                "target_lang_sense_clarification": "в контексте супов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6672b08-2eb8-3741-a356-58f98d7d0fb4",
                    "sense_definition": "Potoczne, często żartobliwe lub lekko obraźliwe określenie osoby otyłej, pulchnej (często używane pieszczotliwie wobec małych dzieci).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "person_desc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako dziecko byłem małą <kluską>, ale w liceum zacząłem uprawiać sport.",
                            "target_lang_translations": {
                                "ru": "В детстве я был маленьким <пончиком>, но в лицее начал заниматься спортом."
                            }
                        },
                        {
                            "text": "Zobacz, jaka słodka <kluska> leży w tym wózku i się uśmiecha.",
                            "target_lang_translations": {
                                "ru": "Посмотри, какая сладкая <пышка> лежит в этой коляске и улыбается."
                            }
                        }
                    ],
                    "synonyms": [
                        "grubas",
                        "pulpet",
                        "pączek",
                        "baryła"
                    ],
                    "antonyms": [
                        "chudzielec",
                        "szkielet"
                    ],
                    "common_phrases": [
                        "słodka kluska"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, часто шутливое или слегка обидное название полного, пухлого человека (часто используется ласково по отношению к маленьким детям)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пончик",
                                "target_lang_sense_clarification": "о полном человеке"
                            },
                            {
                                "target_lang_word": "пухляш",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "пышка",
                                "target_lang_sense_clarification": "обычно о женщине или ребенке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e36c0095-7f93-3870-b2e0-2463e1c129db",
                    "sense_definition": "Potoczne określenie osoby niespornej, powolnej, niezaradnej życiowo lub mało energicznej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_desc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź taka <kluska>, musisz wreszcie wziąć sprawy w swoje ręce.",
                            "target_lang_translations": {
                                "ru": "Не будь такой <размазнёй>, ты должен наконец взять дело в свои руки."
                            }
                        },
                        {
                            "text": "Ten nowy pracownik to straszna <kluska>, nic nie potrafi szybko załatwić.",
                            "target_lang_translations": {
                                "ru": "Этот новый сотрудник — страшная <рохля>, ничего не может быстро уладить."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciamajda",
                        "fujara",
                        "niedołęga",
                        "ofiara losu",
                        "niezdara"
                    ],
                    "common_phrases": [
                        "ciepłe kluchy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название нерасторопного, медлительного, неприспособленного к жизни или малоэнергичного человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размазня",
                                "target_lang_sense_clarification": "о бесхарактерном человеке"
                            },
                            {
                                "target_lang_word": "рохля",
                                "target_lang_sense_clarification": "о медлительном человеке"
                            },
                            {
                                "target_lang_word": "тюфяк",
                                "target_lang_sense_clarification": "о вялом человеке"
                            }
                        ]
                    }
                }
            ]
        }
    ]

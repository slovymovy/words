{
    "word_family": [
        "przeglądać",
        "przejrzenie",
        "przejrzysty",
        "przejrzystość"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d2315ad-91a0-3b1b-ac73-222948b3f572",
                    "sense_definition": "Obejrzeć coś, na ogół pobieżnie, w celu zaznajomienia się z treścią lub sprawdzenia stanu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "visual_check",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę szybko <przejrzeć> notatki przed dzisiejszym egzaminem.",
                            "target_lang_translations": {
                                "ru": "Мне нужно быстро <просмотреть> конспекты перед сегодняшним экзаменом."
                            }
                        },
                        {
                            "text": "Czy zdążyłeś już <przejrzeć> gazetę i sprawdzić wiadomości?",
                            "target_lang_translations": {
                                "ru": "Ты уже успел <просмотреть> газету и проверить новости?"
                            }
                        }
                    ],
                    "synonyms": [
                        "przekartkować",
                        "skontrolować",
                        "sprawdzić",
                        "rzucić okiem"
                    ],
                    "antonyms": [
                        "wczytać się",
                        "przestudiować"
                    ],
                    "common_phrases": [
                        "przejrzeć gazetę",
                        "przejrzeć dokumenty",
                        "przejrzeć pocztę",
                        "przejrzeć na wylot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осмотреть что-то, обычно бегло, чтобы ознакомиться с содержанием или проверить состояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просмотреть",
                                "target_lang_sense_clarification": "бегло ознакомиться"
                            },
                            {
                                "target_lang_word": "пересмотреть",
                                "target_lang_sense_clarification": "проверить заново или многое"
                            },
                            {
                                "target_lang_word": "проглядеть",
                                "target_lang_sense_clarification": "быстро прочитать или осмотреть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e375bb3f-4c14-37e0-a899-f21b08f48020",
                    "sense_definition": "Zobaczyć swoje własne odbicie, na przykład w lustrze lub szybie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "visual_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wyjściem z domu lubię <przejrzeć> się w dużym lustrze.",
                            "target_lang_translations": {
                                "ru": "Перед выходом из дома я люблю <посмотреться> в большое зеркало."
                            }
                        },
                        {
                            "text": "Możesz <przejrzeć> się w witrynie sklepowej, żeby poprawić fryzurę.",
                            "target_lang_translations": {
                                "ru": "Ты можешь <посмотреться> в витрину магазина, чтобы поправить прическу."
                            }
                        }
                    ],
                    "synonyms": [
                        "obejrzeć się"
                    ],
                    "common_phrases": [
                        "przejrzeć się w lustrze",
                        "przejrzeć się w szybie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увидеть собственное отражение, например, в зеркале или стекле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посмотреться",
                                "target_lang_sense_clarification": "в зеркало"
                            },
                            {
                                "target_lang_word": "оглядеть себя",
                                "target_lang_sense_clarification": "оценить свой внешний вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b80c0cb-464b-3689-9d55-b5dd96b0f689",
                    "sense_definition": "Rozpoznać czyjeś prawdziwe zamiary lub charakter, mimo prób ich ukrycia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cognitive_understanding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szybko udało mi się <przejrzeć> jego nieszczere intencje.",
                            "target_lang_translations": {
                                "ru": "Мне быстро удалось <разгадать> его неискренние намерения."
                            }
                        },
                        {
                            "text": "Ona potrafi <przejrzeć> każdego kłamcę w zaledwie kilka minut.",
                            "target_lang_translations": {
                                "ru": "Она может <раскусить> любого лжеца всего за несколько минут."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozszyfrować",
                        "rozgryźć",
                        "zdemaskować"
                    ],
                    "common_phrases": [
                        "przejrzeć kogoś na wylot",
                        "przejrzeć czyjąś grę",
                        "przejrzeć zamiary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распознать чьи-то истинные намерения или характер, несмотря на попытки их скрыть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскусить",
                                "target_lang_sense_clarification": "понять истинную сущность"
                            },
                            {
                                "target_lang_word": "разгадать",
                                "target_lang_sense_clarification": "намерения или замыслы"
                            },
                            {
                                "target_lang_word": "видеть насквозь",
                                "target_lang_sense_clarification": "понимать без слов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "682d1f61-9f12-3176-a48a-da82716fa4f1",
                    "sense_definition": "Zrozumieć swój błąd lub poznać prawdę po okresie niewiedzy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cognitive_understanding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dopiero po latach <przejrzał> na oczy i zrozumiał, że był oszukiwany.",
                            "target_lang_translations": {
                                "ru": "Только спустя годы он <прозрел> и понял, что его обманывали."
                            }
                        },
                        {
                            "text": "Musisz w końcu <przejrzeć> na oczy i zobaczyć, jak wygląda rzeczywistość.",
                            "target_lang_translations": {
                                "ru": "Ты должен наконец <открыть глаза> и увидеть, как выглядит реальность."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrozumieć",
                        "uświadomić sobie",
                        "oprzytomnieć"
                    ],
                    "antonyms": [
                        "łudzić się"
                    ],
                    "common_phrases": [
                        "przejrzeć na oczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осознать свою ошибку или узнать правду после периода неведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозреть",
                                "target_lang_sense_clarification": "понять истину"
                            },
                            {
                                "target_lang_word": "открыть глаза",
                                "target_lang_sense_clarification": "узнать правду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb72cf70-899c-3cec-93c0-8fc7678d535f",
                    "sense_definition": "O roślinach i ich owocach: stać się zbyt dojrzałym.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Agrest na krzakach już <przejrzał> i zaczyna opadać na ziemię.",
                            "target_lang_translations": {
                                "ru": "Крыжовник на кустах уже <перезрел> и начинает падать на землю."
                            }
                        },
                        {
                            "text": "Jeśli owoce <przejrzeją>, nie będą nadawały się do długiego transportu.",
                            "target_lang_translations": {
                                "ru": "Если фрукты <перезреют>, они не будут годны для длительной транспортировки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О растениях и плодах: стать слишком спелым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перезреть",
                                "target_lang_sense_clarification": "о плодах и ягодах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
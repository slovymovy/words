                },
                {
                    "sense_id": "5e344087-5b3e-3984-bf16-9cde41546fbc",
                    "sense_definition": "The people who speak the Bench language.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "names",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "The <w>Bench</w> have a rich cultural history.",
                            "target_lang_translations": {
                                "ru": "Народ <w>бенч</w> имеет богатую культурную историю."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Народ, говорящий на языке бенч."
                    },
                    "translations": {

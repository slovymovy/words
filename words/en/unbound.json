{
    "word_family": [
        "unbind",
        "binding",
        "boundless",
        "bound"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e2ec462d-db68-317a-ac5d-a204c9fa817b",
                    "sense_definition": "Not tied up or restricted by physical bonds.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_freedom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prisoner realized his hands were <unbound> and immediately tried to escape.",
                            "target_lang_translations": {
                                "ru": "Заключенный понял, что его руки <развязаны>, и немедленно попытался сбежать."
                            }
                        },
                        {
                            "text": "She decided to leave her hair <unbound> so it fell freely over her shoulders.",
                            "target_lang_translations": {
                                "ru": "Она решила оставить волосы <распущенными>, чтобы они свободно падали на плечи."
                            }
                        }
                    ],
                    "synonyms": [
                        "untied",
                        "loose",
                        "free",
                        "unfettered"
                    ],
                    "antonyms": [
                        "bound",
                        "tied",
                        "chained"
                    ],
                    "common_phrases": [
                        "unbound hair",
                        "hands unbound",
                        "remain unbound"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не связанный физическими узами или ограничениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развязанный",
                                "target_lang_sense_clarification": "о веревках, узлах или руках"
                            },
                            {
                                "target_lang_word": "распущенный",
                                "target_lang_sense_clarification": "о волосах"
                            },
                            {
                                "target_lang_word": "освобожденный",
                                "target_lang_sense_clarification": "от оков или физических ограничений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70cc9ee3-91f2-3c10-930e-5024f8851df6",
                    "sense_definition": "Not limited by obligations, rules, or constraints.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_freedom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist felt <unbound> by traditional styles and experimented with new techniques.",
                            "target_lang_translations": {
                                "ru": "Художник чувствовал себя <свободным> от традиционных стилей и экспериментировал с новыми техниками."
                            }
                        },
                        {
                            "text": "As a freelancer, he is <unbound> by the strict schedule of a corporate office.",
                            "target_lang_translations": {
                                "ru": "Как фрилансер, он <не связан> жестким графиком работы в офисе."
                            }
                        }
                    ],
                    "synonyms": [
                        "unconstrained",
                        "unrestricted",
                        "free",
                        "liberated"
                    ],
                    "antonyms": [
                        "obligated",
                        "constrained",
                        "restricted"
                    ],
                    "common_phrases": [
                        "unbound by convention",
                        "unbound by rules"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не ограниченный обязательствами, правилами или рамками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неограниченный",
                                "target_lang_sense_clarification": "свободный от рамок и правил"
                            },
                            {
                                "target_lang_word": "несвязанный",
                                "target_lang_sense_clarification": "не имеющий обязательств"
                            },
                            {
                                "target_lang_word": "свободный",
                                "target_lang_sense_clarification": "общий смысл отсутствия ограничений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "866f480b-df80-3e19-8fdb-1d7207e9cdcb",
                    "sense_definition": "Referring to a book or papers that are not fastened together or covered with a hard cover.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The library stores the <unbound> journals in special boxes to protect them from damage.",
                            "target_lang_translations": {
                                "ru": "Библиотека хранит <непереплетенные> журналы в специальных коробках для защиты от повреждений."
                            }
                        },
                        {
                            "text": "He discovered an <unbound> manuscript in the attic that appeared to be centuries old.",
                            "target_lang_translations": {
                                "ru": "Он обнаружил на чердаке <непереплетенную> рукопись, которой, казалось, было несколько веков."
                            }
                        }
                    ],
                    "synonyms": [
                        "loose-leaf"
                    ],
                    "antonyms": [
                        "bound",
                        "hardback"
                    ],
                    "common_phrases": [
                        "unbound volume",
                        "unbound copy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к книге или бумагам, которые не скреплены вместе или не имеют твердой обложки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непереплетенный",
                                "target_lang_sense_clarification": "о книгах, рукописях или журналах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "940fbbcc-cdb0-3411-985e-ee5f18c84cea",
                    "sense_definition": "Not held together by nuclear forces or gravity; capable of spontaneous decay or separation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Because the particle is <unbound>, it will quickly split into smaller components.",
                            "target_lang_translations": {
                                "ru": "Поскольку частица <не связана>, она быстро распадется на более мелкие компоненты."
                            }
                        },
                        {
                            "text": "In this high-energy state, the electrons become <unbound> from the nucleus.",
                            "target_lang_translations": {
                                "ru": "В этом высокоэнергетическом состоянии электроны становятся <свободными> от ядра."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не удерживаемый ядерными силами или гравитацией; способный к спонтанному распаду или разделению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несвязанный",
                                "target_lang_sense_clarification": "термин в физике (о частицах)"
                            },
                            {
                                "target_lang_word": "свободный",
                                "target_lang_sense_clarification": "не находящийся в связанном состоянии"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "45c226dc-3bc2-34c2-9901-c34425870950",
                    "sense_definition": "Simple past and past participle of <w>unbind</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <unbound> the package carefully to reveal the gift inside.",
                            "target_lang_translations": {
                                "ru": "Она осторожно <развязала> пакет, чтобы показать подарок внутри."
                            }
                        },
                        {
                            "text": "Once the ropes were <unbound>, the horse galloped away into the field.",
                            "target_lang_translations": {
                                "ru": "Как только веревки были <развязаны>, лошадь ускакала в поле."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense/participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола unbind (развязывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развязал",
                                "target_lang_sense_clarification": "глагол прошедшего времени"
                            },
                            {
                                "target_lang_word": "развязанный",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
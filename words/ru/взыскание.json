    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0b15a4b-3789-3d82-ab08-21e19edcf96b",
                    "sense_definition": "Официальное наказание за нарушение дисциплины, правил или невыполнение обязанностей.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За систематические опоздания на работу на сотрудника наложили дисциплинарное <взыскание>.",
                            "target_lang_translations": {
                                "en": "A disciplinary <w>penalty</w> was imposed on the employee for systematic lateness to work."
                            }
                        },
                        {
                            "text": "Командир решил не применять строгое <взыскание> к солдату за этот проступок.",
                            "target_lang_translations": {
                                "en": "The commander decided not to apply a strict <w>punishment</w> to the soldier for this misconduct."
                            }
                        }
                    ],
                    "synonyms": [
                        "наказа́ние"
                    ],
                    "common_phrases": [
                        "наложить взыскание",
                        "дисциплинарное взыскание",
                        "снять взыскание"
                    ],
                    "target_lang_definitions": {
                        "en": "An official punishment for a violation of discipline, rules, or failure to fulfill duties."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "penalty",
                                "target_lang_sense_clarification": "official punishment"
                            },
                            {
                                "target_lang_word": "punishment",
                                "target_lang_sense_clarification": "disciplinary measure"
                            },
                            {
                                "target_lang_word": "sanction",
                                "target_lang_sense_clarification": "formal penalty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b19e93cb-77b5-3252-bb37-ba99c37219c8",
                    "sense_definition": "Принудительное истребование денежных средств или имущества в счёт погашения долга или штрафа.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Банк подал иск в суд на принудительное <взыскание> долга по ипотечному кредиту.",
                            "target_lang_translations": {
                                "en": "The bank filed a lawsuit for the forced <w>recovery</w> of the mortgage debt."
                            }
                        },
                        {
                            "text": "Судебные приставы занимаются <взысканием> штрафов с нарушителей правил дорожного движения.",
                            "target_lang_translations": {
                                "en": "Bailiffs are responsible for the <w>collection</w> of fines from traffic violators."
                            }
                        }
                    ],
                    "common_phrases": [
                        "взыскание долга",
                        "судебное взыскание",
                        "обратить взыскание"
                    ],
                    "target_lang_definitions": {
                        "en": "The compulsory claiming of funds or property to settle a debt or fine."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "recovery",
                                "target_lang_sense_clarification": "of debts or damages"
                            },
                            {
                                "target_lang_word": "collection",
                                "target_lang_sense_clarification": "of payments or fines"
                            },
                            {
                                "target_lang_word": "foreclosure",
                                "target_lang_sense_clarification": "seizing property"
                            }
                        ]
                    }
                }
            ]
        }
    ]

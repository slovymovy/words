                    "sense_id": "a697dcfc-e1dd-39cf-b8b1-e5f5648b6b81",
                    "sense_definition": "Precies hetzelfde in aantal, grootte, waarde of rang; niet verschillend.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "equal_same",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Volgens de grondwet zijn alle mensen <w>gelijk</w>.",
                            "target_lang_translations": {
                                "en": "According to the constitution, all people are <w>equal</w>.",
                                "ru": "Согласно конституции, все люди <w>равны</w>."
                            }
                        },
                        {
                            "text": "We hebben de taart in vier <w>gelijke</w> stukken gesneden.",
                            "target_lang_translations": {
                                "en": "We cut the cake into four <w>equal</w> pieces.",
                                "ru": "Мы разрезали торт на четыре <w>равных</w> куска."
                            }
                        }
                    ],
                    "synonyms": [
                        "hetzelfde"
                    ],
                    "antonyms": [
                        "ongelijk",
                        "tegengesteld"
                    ],
                    "common_phrases": [
                        "op gelijke voet",
                        "van gelijke grootte",
                        "gelijke rechten",
                        "gelijk spel"
                    ],
                    "target_lang_definitions": {
                        "en": "Exactly the same in number, size, value, or rank; not different.",
                        "ru": "Точно такой же по количеству, размеру, ценности или рангу; не отличающийся."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "equal",
                                "target_lang_sense_clarification": "same in quantity, size, value, or status"
                            },
                            {
                                "target_lang_word": "same",
                                "target_lang_sense_clarification": "identical; not different"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "равный",
                                "target_lang_sense_clarification": "одинаковый по количеству, размеру или статусу"
                            },
                            {
                                "target_lang_word": "одинаковый",
                                "target_lang_sense_clarification": "идентичный; не отличающийся"
                            }
                        ]
                    }
                }
            ]
        },
        {

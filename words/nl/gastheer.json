    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7a12f96f-1cbb-3672-aba8-6ffc41c69c9c",
                    "sense_definition": "Een man die gasten ontvangt en verzorgt, bijvoorbeeld door eten en drinken aan te bieden",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_host",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vader is niet zo'n goede <w>gastheer</w>.",
                            "target_lang_translations": {
                                "en": "My father is not such a good <w>host</w>.",
                                "ru": "Мой отец не очень хороший <w>хозяин</w>."
                            }
                        },
                        {
                            "text": "Hij moest onverwacht naar huis om de <w>gastheer</w> te spelen voor het bezoek.",
                            "target_lang_translations": {
                                "en": "He had to go home unexpectedly to play <w>host</w> to the visitors.",
                                "ru": "Ему пришлось неожиданно поехать домой, чтобы сыграть роль <w>хозяина</w> для гостей."
                            }
                        }
                    ],
                    "antonyms": [
                        "gast",
                        "genodigde"
                    ],
                    "common_phrases": [
                        "goede gastheer zijn",
                        "de gastheer spelen"
                    ],
                    "target_lang_definitions": {
                        "en": "A man who receives and takes care of guests, for example by offering food and drink",
                        "ru": "Мужчина, который принимает гостей и заботится о них, например, предлагая еду и напитки"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "host",
                                "target_lang_sense_clarification": "person receiving guests"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хозяин",
                                "target_lang_sense_clarification": "человек, принимающий гостей"
                            },
                            {
                                "target_lang_word": "принимающая сторона",
                                "target_lang_sense_clarification": "формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df15ebf0-a441-3d7b-bab5-50963e552d73",
                    "sense_definition": "Een organisatie, stad of land dat een evenement organiseert en faciliteert voor deelnemers",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_host",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Frankrijk is dit jaar de <w>gastheer</w> van de Olympische Spelen.",
                            "target_lang_translations": {
                                "en": "France is the <w>host</w> of the Olympic Games this year.",
                                "ru": "В этом году Франция — <w>хозяйка</w> Олимпийских игр."
                            }
                        },
                        {
                            "text": "De stad treedt op als <w>gastheer</w> voor het internationale congres.",
                            "target_lang_translations": {
                                "en": "The city acts as <w>host</w> for the international congress.",
                                "ru": "Город выступает <w>организатором</w> международного конгресса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gastheer van het toernooi",
                        "optreden als gastheer"
                    ],
                    "target_lang_definitions": {
                        "en": "An organization, city, or country that organizes and facilitates an event for participants",
                        "ru": "Организация, город или страна, организующая мероприятие и принимающая участников"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "host",
                                "target_lang_sense_clarification": "organizer of an event"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "организатор",
                                "target_lang_sense_clarification": "мероприятия"
                            },
                            {
                                "target_lang_word": "принимающая сторона",
                                "target_lang_sense_clarification": "страна или город"
                            },
                            {
                                "target_lang_word": "хозяин",
                                "target_lang_sense_clarification": "места проведения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "612f0aaf-bf98-3df3-99c6-8bb7fa9d3cf1",
                    "sense_definition": "Een medewerker die gasten verwelkomt en begeleidt bij evenementen of in de horeca",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "social_host",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>gastheer</w> wees ons een mooie tafel aan bij het raam.",
                            "target_lang_translations": {
                                "en": "The <w>host</w> showed us to a nice table by the window.",
                                "ru": "<w>Метрдотель</w> показал нам хороший столик у окна."
                            }
                        },
                        {
                            "text": "Hij werkt in het weekend als <w>gastheer</w> bij grote evenementen.",
                            "target_lang_translations": {
                                "en": "He works as a <w>host</w> at large events on the weekend.",
                                "ru": "По выходным он работает <w>хостом</w> на крупных мероприятиях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "werken als gastheer"
                    ],
                    "target_lang_definitions": {
                        "en": "An employee who welcomes and guides guests at events or in the hospitality industry",
                        "ru": "Сотрудник, который встречает и сопровождает гостей на мероприятиях или в ресторане"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "host",
                                "target_lang_sense_clarification": "hospitality employee"
                            },
                            {
                                "target_lang_word": "greeter",
                                "target_lang_sense_clarification": "person who welcomes guests"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "администратор",
                                "target_lang_sense_clarification": "в ресторане"
                            },
                            {
                                "target_lang_word": "хост",
                                "target_lang_sense_clarification": "на мероприятии"
                            },
                            {
                                "target_lang_word": "метрдотель",
                                "target_lang_sense_clarification": "старший официант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60d1b48f-fcb3-3e19-962c-f120c5b3a689",
                    "sense_definition": "Een organisme dat een ander organisme, zoals een parasiet of virus, draagt en voedt",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biological_host",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De teek gebruikt de hond als <w>gastheer</w> om bloed te zuigen.",
                            "target_lang_translations": {
                                "en": "The tick uses the dog as a <w>host</w> to suck blood.",
                                "ru": "Клещ использует собаку как <w>хозяина</w>, чтобы пить кровь."
                            }
                        },
                        {
                            "text": "Dit virus heeft een specifieke <w>gastheer</w> nodig om zich te kunnen vermenigvuldigen.",
                            "target_lang_translations": {
                                "en": "This virus needs a specific <w>host</w> to be able to reproduce.",
                                "ru": "Этому вирусу нужен специфический <w>хозяин</w> для размножения."
                            }
                        }
                    ],
                    "synonyms": [
                        "gastorganisme"
                    ],
                    "common_phrases": [
                        "gastheer voor parasieten",
                        "tussentijdse gastheer"
                    ],
                    "target_lang_definitions": {
                        "en": "An organism that carries and feeds another organism, such as a parasite or virus",
                        "ru": "Организм, который носит и питает другой организм, например паразита или вирус"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "host",
                                "target_lang_sense_clarification": "biology"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хозяин",
                                "target_lang_sense_clarification": "биологический термин"
                            },
                            {
                                "target_lang_word": "носитель",
                                "target_lang_sense_clarification": "организм, содержащий паразита"
                            }
                        ]
                    }
                }
            ]
        }

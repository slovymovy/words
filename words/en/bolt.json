                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рвануть",
                                "target_lang_sense_clarification": "резко побежать"
                            },
                            {
                                "target_lang_word": "удрать",
                                "target_lang_sense_clarification": "сбежать"
                            },
                            {
                                "target_lang_word": "понести",
                                "target_lang_sense_clarification": "о лошади"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a44b0c38-8b4c-3a85-bc6b-7ff96f1d6c02",
                    "sense_definition": "To swallow food very quickly without chewing it properly.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sudden_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hungry dog <w>bolted</w> its dinner in just a few seconds.",
                            "target_lang_translations": {
                                "ru": "Голодная собака <w>заглотила</w> свой ужин всего за несколько секунд."
                            }
                        },
                        {
                            "text": "You should not <w>bolt</w> your food; take time to enjoy it.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует <w>глотать</w> еду не жуя; удели время тому, чтобы насладиться ею."
                            }
                        }
                    ],
                    "synonyms": [
                        "gulp",
                        "wolf",
                        "devour"
                    ],
                    "common_phrases": [
                        "bolt down food"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глотать пищу очень быстро, не пережевывая ее должным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заглатывать",
                                "target_lang_sense_clarification": "глотать целиком"
                            },
                            {
                                "target_lang_word": "глотать не жуя",
                                "target_lang_sense_clarification": "быстро есть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b7c3d52-7079-395d-b99f-90de1d9e7196",

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d208381b-d4cf-393e-a82c-ce0ea7601e6b",
                    "sense_definition": "Następujący bezpośrednio po pierwszym; zajmujący miejsce numer dwa w szeregu lub kolejności.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ordinal_number",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkam na <w>drugim</w> piętrze w tym wysokim bloku.",
                            "target_lang_translations": {
                                "ru": "Я живу на <w>втором</w> этаже в этом высоком доме."
                            }
                        },
                        {
                            "text": "To już <w>drugi</w> raz, kiedy proszę cię o ciszę.",
                            "target_lang_translations": {
                                "ru": "Это уже <w>второй</w> раз, когда я прошу тебя о тишине."
                            }
                        }
                    ],
                    "synonyms": [
                        "2.",
                        "wtóry"
                    ],
                    "antonyms": [
                        "pierwszy"
                    ],
                    "common_phrases": [
                        "drugie śniadanie",
                        "drugie miejsce",
                        "po drugie",
                        "drugie piętro"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следующий непосредственно за первым; занимающий место номер два в ряду или последовательности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "второй",
                                "target_lang_sense_clarification": "порядковый номер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d91f3285-3715-3b2b-84ab-b3308be089a5",
                    "sense_definition": "Ten pozostały z dwóch; inny, przeciwległy lub odmienny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "the_other",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy koniecznie spojrzeć na ten problem z <w>drugiej</w> strony.",
                            "target_lang_translations": {
                                "ru": "Мы обязательно должны взглянуть на эту проблему с <w>другой</w> стороны."
                            }
                        },
                        {
                            "text": "Zgubiłem rękawiczkę i nie mogę znaleźć <w>drugiej</w> do pary.",
                            "target_lang_translations": {
                                "ru": "Я потерял перчатку и не могу найти <w>вторую</w> пару."
                            }
                        }
                    ],
                    "synonyms": [
                        "następny",
                        "jakiś",
                        "pewien"
                    ],
                    "common_phrases": [
                        "z drugiej strony",
                        "na drugi dzień",
                        "druga połowa",
                        "drugi koniec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставшийся из двух; другой, противоположный или отличный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "другой",
                                "target_lang_sense_clarification": "иной, противоположный"
                            },
                            {
                                "target_lang_word": "второй",
                                "target_lang_sense_clarification": "парный предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "674ed0fb-5227-3def-91de-77173b95a047",
                    "sense_definition": "Kolejny taki sam lub podobny; następny, dodatkowy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "additional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy człowiek zasługuje na to, by dostać <w>drugą</w> szansę.",
                            "target_lang_translations": {
                                "ru": "Каждый человек заслуживает того, чтобы получить <w>второй</w> шанс."
                            }
                        },
                        {
                            "text": "Kupiłem sobie <w>drugi</w> samochód, ponieważ stary ciągle się psuł.",
                            "target_lang_translations": {
                                "ru": "Я купил себе <w>вторую</w> машину, так как старая постоянно ломалась."
                            }
                        }
                    ],
                    "synonyms": [
                        "dodatkowy",
                        "jeszcze jeden",
                        "następny",
                        "wtóry"
                    ],
                    "antonyms": [
                        "ostatni"
                    ],
                    "common_phrases": [
                        "druga szansa",
                        "drugie życie",
                        "druga młodość",
                        "drugie dno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очередной такой же или похожий; следующий, дополнительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "второй",
                                "target_lang_sense_clarification": "еще один, дополнительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a09b5732-5abb-33c7-855e-a9368bd9d132",
                    "sense_definition": "Używane w liczbie mnogiej do określenia pozostałych osób lub rzeczy z grupy, w opozycji do tych wymienionych wcześniej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "others",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jedni ludzie wolą góry, a <w>drudzy</w> zdecydowanie wybierają morze.",
                            "target_lang_translations": {
                                "ru": "Одни люди предпочитают горы, а <w>другие</w> определенно выбирают море."
                            }
                        },
                        {
                            "text": "Nie oglądaj się na to, co powiedzą <w>drudzy</w>.",
                            "target_lang_translations": {
                                "ru": "Не оглядывайся на то, что скажут <w>другие</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "następny",
                        "jakiś",
                        "pewien"
                    ],
                    "common_phrases": [
                        "jedni i drudzy",
                        "to czy tamto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется во множественном числе для обозначения остальных людей или вещей в группе, в противопоставление упомянутым ранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "другие",
                                "target_lang_sense_clarification": "остальные люди"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "efab8c00-dd8c-3cc9-9fbc-5bf49ee7e1be",
                    "sense_definition": "Drugi dzień danego miesiąca.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "date",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Urodziny mojej siostry wypadają <w>drugiego</w> marca.",
                            "target_lang_translations": {
                                "ru": "День рождения моей сестры выпадает на <w>второе</w> марта."
                            }
                        },
                        {
                            "text": "Spotkajmy się <w>drugiego</w> o godzinie siedemnastej w kawiarni.",
                            "target_lang_translations": {
                                "ru": "Давай встретимся <w>второго</w> в семнадцать часов в кафе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drugiego maja",
                        "drugiego stycznia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Второй день данного месяца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "второе",
                                "target_lang_sense_clarification": "число месяца"
                            }
                        ]
                    }
                }
            ]
        }
    ]

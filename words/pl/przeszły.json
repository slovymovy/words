    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0c236a02-43d0-3eac-8667-a1c20c326525",
                    "sense_definition": "Taki, który wystąpił bezpośrednio przed obecnym; ostatni w kolejności chronologicznej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W <w>przeszłym</w> roku pojechaliśmy na wakacje nad morze.",
                            "target_lang_translations": {
                                "ru": "В <w>прошлом</w> году мы поехали в отпуск на море."
                            }
                        },
                        {
                            "text": "Nie pamiętam dokładnie, co robiłem w <w>przeszłym</w> tygodniu.",
                            "target_lang_translations": {
                                "ru": "Я не помню точно, что делал на <w>прошлой</w> неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "zeszły",
                        "ubiegły",
                        "poprzedni"
                    ],
                    "antonyms": [
                        "przyszły"
                    ],
                    "common_phrases": [
                        "w przeszłym roku",
                        "w przeszłym tygodniu",
                        "przeszły miesiąc"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, который был непосредственно перед нынешним; последний в хронологическом порядке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прошлый",
                                "target_lang_sense_clarification": "предыдущий (о времени)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff5ac0c6-2d95-30fd-86da-1f1daf3a8c74",
                    "sense_definition": "Odnoszący się do kategorii gramatycznej wyrażającej czynności, które miały miejsce wcześniej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na dzisiejszej lekcji omówimy czas <w>przeszły</w> czasowników.",
                            "target_lang_translations": {
                                "ru": "На сегодняшнем уроке мы обсудим <w>прошедшее</w> время глаголов."
                            }
                        },
                        {
                            "text": "To zdanie jest napisane w czasie <w>przeszłym</w>.",
                            "target_lang_translations": {
                                "ru": "Это предложение написано в <w>прошедшем</w> времени."
                            }
                        }
                    ],
                    "antonyms": [
                        "przyszły"
                    ],
                    "common_phrases": [
                        "czas przeszły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к грамматической категории, выражающей действия, которые происходили ранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прошедший",
                                "target_lang_sense_clarification": "грамматическое время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ca9ec42-0228-3940-ac96-b799fee0cd3a",
                    "sense_definition": "Taki, który już minął, stał się częścią historii i nie jest już aktualny.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy wyciągnąć wnioski z <w>przeszłych</w> błędów, aby ich nie powtarzać.",
                            "target_lang_translations": {
                                "ru": "Мы должны извлечь уроки из <w>прошлых</w> ошибок, чтобы не повторять их."
                            }
                        },
                        {
                            "text": "Wspomnienia <w>przeszłych</w> dni często wracają do mnie wieczorami.",
                            "target_lang_translations": {
                                "ru": "Воспоминания о <w>минувших</w> днях часто возвращаются ко мне по вечерам."
                            }
                        }
                    ],
                    "synonyms": [
                        "poprzedni",
                        "ubiegły",
                        "zeszły"
                    ],
                    "antonyms": [
                        "przyszły"
                    ],
                    "common_phrases": [
                        "przeszłe pokolenia",
                        "przeszłe czasy",
                        "przeszłe lata"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, который уже миновал, стал частью истории и больше не актуален."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прошлый",
                                "target_lang_sense_clarification": "бывший, прежний"
                            },
                            {
                                "target_lang_word": "минувший",
                                "target_lang_sense_clarification": "ушедший в прошлое"
                            },
                            {
                                "target_lang_word": "прошедший",
                                "target_lang_sense_clarification": "истекший"
                            }
                        ]
                    }
                }
            ]
        },
        {

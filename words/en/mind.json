{
    "word_family": [
        "mindful",
        "mindless",
        "mindset",
        "remind",
        "reminder"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e26c717a-a19b-3cf6-b8ee-ceba31b96aa9",
                    "sense_definition": "To be annoyed, bothered, or upset by something; typically used in questions and negative sentences.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_objection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do not <w>mind</w> waiting for you if you are busy.",
                            "target_lang_translations": {
                                "ru": "Я не <w>возражаю</w> подождать тебя, если ты занят."
                            }
                        },
                        {
                            "text": "Do you <w>mind</w> if I open the window for some fresh air?",
                            "target_lang_translations": {
                                "ru": "Вы не <w>возражаете</w>, если я открою окно, чтобы впустить свежий воздух?"
                            }
                        },
                        {
                            "text": "Please <w>mind</w> the noise while we are fixing the roof.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>обращайте внимания</w> на шум, пока мы чиним крышу."
                            }
                        }
                    ],
                    "synonyms": [
                        "object",
                        "care"
                    ],
                    "common_phrases": [
                        "never mind",
                        "do you mind",
                        "would you mind",
                        "I don't mind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возражать, быть недовольным чем-либо или расстраиваться из-за чего-то; обычно используется в вопросах и отрицательных предложениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возражать",
                                "target_lang_sense_clarification": "высказывать несогласие или недовольство"
                            },
                            {
                                "target_lang_word": "быть против",
                                "target_lang_sense_clarification": "иметь отрицательное отношение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afea690c-aa14-377a-93e4-91f54c5603b0",
                    "sense_definition": "To be careful of something or to watch out for potential danger.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_caution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>mind</w> the gap between the train and the platform.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>остерегаться</w> зазора между поездом и платформой."
                            }
                        },
                        {
                            "text": "Please <w>mind</w> your head on the low beam when you enter.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>берегите</w> голову от удара о низкую балку при входе."
                            }
                        }
                    ],
                    "synonyms": [
                        "watch",
                        "heed"
                    ],
                    "antonyms": [
                        "ignore"
                    ],
                    "common_phrases": [
                        "mind the gap",
                        "mind your step",
                        "mind your head"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть осторожным по отношению к чему-либо или остерегаться возможной опасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остерегаться",
                                "target_lang_sense_clarification": "беречься опасности"
                            },
                            {
                                "target_lang_word": "быть осторожным",
                                "target_lang_sense_clarification": "проявлять осторожность"
                            },
                            {
                                "target_lang_word": "следить",
                                "target_lang_sense_clarification": "быть внимательным к чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78810e49-97f6-37bf-8bc6-c600e40a1ec4",
                    "sense_definition": "To take care of someone or something for a short period of time.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Could you please <w>mind</w> my bag while I go to the restroom?",
                            "target_lang_translations": {
                                "ru": "Не могли бы вы <w>присмотреть</w> за моей сумкой, пока я выйду в туалет?"
                            }
                        },
                        {
                            "text": "She asked her neighbor to <w>mind</w> the dog for the weekend.",
                            "target_lang_translations": {
                                "ru": "Она попросила соседа <w>присмотреть</w> за собакой на выходных."
                            }
                        }
                    ],
                    "synonyms": [
                        "look after",
                        "watch"
                    ],
                    "antonyms": [
                        "neglect"
                    ],
                    "common_phrases": [
                        "mind the shop",
                        "mind the baby",
                        "mind the house"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заботиться о ком-то или чем-то в течение короткого периода времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присматривать",
                                "target_lang_sense_clarification": "наблюдать с целью ухода или надзора"
                            },
                            {
                                "target_lang_word": "следить",
                                "target_lang_sense_clarification": "наблюдать, заботясь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c578e5a-2d71-3a98-9a51-8d4a6d2f6933",
                    "sense_definition": "To pay attention to something, often used when telling someone to focus on their own affairs.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should just <w>mind</w> your own business and stop asking questions.",
                            "target_lang_translations": {
                                "ru": "Тебе следует просто <w>заниматься</w> своим делом и перестать задавать вопросы."
                            }
                        },
                        {
                            "text": "He told the rude child to <w>mind</w> his manners at the table.",
                            "target_lang_translations": {
                                "ru": "Он сказал грубому ребенку <w>следить</w> за своими манерами за столом."
                            }
                        }
                    ],
                    "synonyms": [
                        "heed"
                    ],
                    "common_phrases": [
                        "mind your own business",
                        "mind your manners",
                        "mind your language"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращать внимание на что-то, часто используется, когда кому-то говорят сосредоточиться на своих собственных делах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заниматься",
                                "target_lang_sense_clarification": "делать свое дело"
                            },
                            {
                                "target_lang_word": "следить",
                                "target_lang_sense_clarification": "контролировать (например, манеры)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34041be6-b185-39a2-830f-92fa7166295a",
                    "sense_definition": "To obey someone, typically a parent or authority figure.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_obedience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The little boy promised to <w>mind</w> his mother and behave well.",
                            "target_lang_translations": {
                                "ru": "Маленький мальчик пообещал <w>слушаться</w> маму и вести себя хорошо."
                            }
                        },
                        {
                            "text": "If you do not <w>mind</w> the teacher, you might get in trouble.",
                            "target_lang_translations": {
                                "ru": "Если ты не будешь <w>слушаться</w> учителя, у тебя могут быть неприятности."
                            }
                        }
                    ],
                    "synonyms": [
                        "obey"
                    ],
                    "antonyms": [
                        "disobey"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слушаться кого-то, обычно родителя или авторитетную фигуру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слушаться",
                                "target_lang_sense_clarification": "подчиняться указаниям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2038016b-d4e3-372b-ae77-ebacca9e058b",
                    "sense_definition": "Used to emphasize a contrast, exception, or something important to remember.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The house is very small, but it has a huge garden, <w>mind</w> you.",
                            "target_lang_translations": {
                                "ru": "Дом очень маленький, но, <w>заметьте</w>, у него огромный сад."
                            }
                        },
                        {
                            "text": "I am not very hungry, although I will eat a little fruit, <w>mind</w>.",
                            "target_lang_translations": {
                                "ru": "Я не очень голоден, хотя, <w>учти</w>, я съем немного фруктов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mind you"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in British and Irish English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для подчеркивания контраста, исключения или чего-то важного, что нужно запомнить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заметьте",
                                "target_lang_sense_clarification": "обратите внимание"
                            },
                            {
                                "target_lang_word": "учтите",
                                "target_lang_sense_clarification": "примите к сведению"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f346f857-c337-3890-a62a-3d41036470fb",
                    "sense_definition": "A person's opinion, decision, or plan.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have changed my <w>mind</w> and I will go to the party.",
                            "target_lang_translations": {
                                "ru": "Я изменил свое <w>решение</w> и пойду на вечеринку."
                            }
                        },
                        {
                            "text": "Please make up your <w>mind</w> about which movie you want to watch.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, прими <w>решение</w> о том, какой фильм ты хочешь посмотреть."
                            }
                        }
                    ],
                    "synonyms": [
                        "opinion",
                        "decision",
                        "view"
                    ],
                    "common_phrases": [
                        "change your mind",
                        "make up your mind",
                        "speak your mind",
                        "to my mind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мнение, решение или план человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решение",
                                "target_lang_sense_clarification": "принятый план действий"
                            },
                            {
                                "target_lang_word": "мнение",
                                "target_lang_sense_clarification": "точка зрения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea45a269-9d3c-3dc0-bd8c-31d7bf94c953",
                    "sense_definition": "The part of a person that enables them to think, reason, feel emotions, and be aware of things.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_faculty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Reading books is a great way to expand your <w>mind</w>.",
                            "target_lang_translations": {
                                "ru": "Чтение книг — отличный способ развить свой <w>ум</w>."
                            }
                        },
                        {
                            "text": "Despite his old age, his <w>mind</w> was still very sharp and active.",
                            "target_lang_translations": {
                                "ru": "Несмотря на преклонный возраст, его <w>ум</w> был все еще очень острым и активным."
                            }
                        }
                    ],
                    "synonyms": [
                        "intellect",
                        "brain"
                    ],
                    "common_phrases": [
                        "state of mind",
                        "peace of mind",
                        "open mind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть личности, которая позволяет мыслить, рассуждать, чувствовать эмоции и осознавать вещи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ум",
                                "target_lang_sense_clarification": "способность мыслить"
                            },
                            {
                                "target_lang_word": "интеллект",
                                "target_lang_sense_clarification": "мыслительные способности"
                            },
                            {
                                "target_lang_word": "рассудок",
                                "target_lang_sense_clarification": "способность здраво рассуждать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7868c60-59d3-3282-b10b-0940264963f3",
                    "sense_definition": "Your memory, attention, or thoughts at a specific moment.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_memory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please keep these safety rules in <w>mind</w> while working.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, имейте эти правила безопасности в <w>виду</w> во время работы."
                            }
                        },
                        {
                            "text": "I have a lot on my <w>mind</w> right now because of the exams.",
                            "target_lang_translations": {
                                "ru": "У меня сейчас много всего на <w>уме</w> из-за экзаменов."
                            }
                        },
                        {
                            "text": "Her name has completely gone out of my <w>mind</w>.",
                            "target_lang_translations": {
                                "ru": "Ее имя совершенно вылетело у меня из <w>головы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "memory",
                        "attention",
                        "thoughts"
                    ],
                    "common_phrases": [
                        "bear in mind",
                        "keep in mind",
                        "on my mind",
                        "come to mind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Память, внимание или мысли в конкретный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "память",
                                "target_lang_sense_clarification": "способность помнить"
                            },
                            {
                                "target_lang_word": "мысли",
                                "target_lang_sense_clarification": "то, о чем думаешь"
                            },
                            {
                                "target_lang_word": "внимание",
                                "target_lang_sense_clarification": "сосредоточенность мыслей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8de13900-bbe1-3b8d-be69-25905ae04f4c",
                    "sense_definition": "A healthy mental state; sanity.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_sanity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I thought I was losing my <w>mind</w> when I heard that strange noise.",
                            "target_lang_translations": {
                                "ru": "Я думал, что схожу с <w>ума</w>, когда услышал этот странный шум."
                            }
                        },
                        {
                            "text": "You must be out of your <w>mind</w> to pay that much for a coffee.",
                            "target_lang_translations": {
                                "ru": "Ты, должно быть, выжил из <w>ума</w>, чтобы платить столько за кофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "sanity",
                        "reason"
                    ],
                    "antonyms": [
                        "insanity"
                    ],
                    "common_phrases": [
                        "lose one's mind",
                        "out of one's mind",
                        "sound mind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здоровое психическое состояние; рассудок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассудок",
                                "target_lang_sense_clarification": "здравый ум"
                            },
                            {
                                "target_lang_word": "ум",
                                "target_lang_sense_clarification": "психическое здоровье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b2029ab-d047-3856-8fde-9c95ba294dcf",
                    "sense_definition": "A person considered as an intellectual force or a great thinker.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was considered one of the greatest scientific <w>minds</w> of the century.",
                            "target_lang_translations": {
                                "ru": "Он считался одним из величайших научных <w>умов</w> столетия."
                            }
                        },
                        {
                            "text": "We need to bring the best <w>minds</w> together to solve this problem.",
                            "target_lang_translations": {
                                "ru": "Нам нужно собрать лучшие <w>умы</w> вместе, чтобы решить эту проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "thinker",
                        "intellectual",
                        "genius"
                    ],
                    "common_phrases": [
                        "great minds",
                        "meeting of minds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, рассматриваемый как интеллектуальная сила или великий мыслитель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ум",
                                "target_lang_sense_clarification": "интеллектуал"
                            },
                            {
                                "target_lang_word": "мыслитель",
                                "target_lang_sense_clarification": "человек глубоких мыслей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
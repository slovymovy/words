        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "63c4991e-0304-3144-b43f-63c0e075a4fc",
                    "sense_definition": "Een trefwoord dat aan een digitaal bestand wordt gekoppeld om het te classificeren of vindbaar te maken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "digital_identifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt een <w>tag</w> toevoegen aan de foto zodat vrienden hem zien.",
                            "target_lang_translations": {
                                "ru": "Ты можешь добавить <w>тег</w> к фотографии, чтобы друзья её увидели.",
                                "en": "You can add a <w>tag</w> to the photo so friends see it."
                            }
                        },
                        {
                            "text": "Door op de <w>tag</w> te klikken, vind je alle gerelateerde berichten.",
                            "target_lang_translations": {
                                "ru": "Нажав на <w>тег</w>, ты найдешь все связанные сообщения.",
                                "en": "By clicking on the <w>tag</w>, you will find all related posts."
                            }
                        }
                    ],
                    "synonyms": [
                        "label"
                    ],
                    "common_phrases": [
                        "een tag toevoegen",
                        "zoeken op tag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ключевое слово, прикрепляемое к цифровому файлу для его классификации или облегчения поиска.",
                        "en": "A keyword attached to a digital file to classify it or make it findable."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тег",
                                "target_lang_sense_clarification": "ключевое слово / метаданные"
                            },

{
    "word_family": [
        "pobór",
        "poborca"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5eb53266-1b89-3cdf-8ac4-3077a65808f0",
                    "sense_definition": "Ściągnąć dane, pliki lub oprogramowanie z internetu na komputer lub inne urządzenie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <w>pobrać</w> nowy program antywirusowy na mój laptop.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>скачать</w> новую антивирусную программу на мой ноутбук."
                            }
                        },
                        {
                            "text": "Czy udało ci się już <w>pobrać</w> ten duży plik z załącznika?",
                            "target_lang_translations": {
                                "ru": "Тебе уже удалось <w>скачать</w> этот большой файл из вложения?"
                            }
                        }
                    ],
                    "synonyms": [
                        "ściągnąć"
                    ],
                    "antonyms": [
                        "załadować"
                    ],
                    "common_phrases": [
                        "pobrać plik",
                        "pobrać aplikację",
                        "pobrać dane",
                        "pobrać aktualizację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скачать данные, файлы или программное обеспечение из интернета на компьютер или другое устройство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скачать",
                                "target_lang_sense_clarification": "файлы, программы"
                            },
                            {
                                "target_lang_word": "загрузить",
                                "target_lang_sense_clarification": "данные"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5e3758c-a591-392f-9f1a-bd791b37ae80",
                    "sense_definition": "Zawrzeć związek małżeński; zostać mężem i żoną (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi rodzice <w>pobrali</w> się trzydzieści lat temu w małym kościele.",
                            "target_lang_translations": {
                                "ru": "Мои родители <w>поженились</w> тридцать лет назад в маленькой церкви."
                            }
                        },
                        {
                            "text": "Anna i Piotr planują się <w>pobrać</w> w przyszłym roku.",
                            "target_lang_translations": {
                                "ru": "Анна и Петр планируют <w>пожениться</w> в следующем году."
                            }
                        }
                    ],
                    "synonyms": [
                        "ściągnąć"
                    ],
                    "common_phrases": [
                        "pobrać się w kościele",
                        "pobrać się w urzędzie",
                        "chcieć się pobrać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вступить в брак; стать мужем и женой (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожениться",
                                "target_lang_sense_clarification": "вступить в брак"
                            },
                            {
                                "target_lang_word": "расписаться",
                                "target_lang_sense_clarification": "разг. зарегистрировать брак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6cb032a-e889-3b47-81ce-bb38981977aa",
                    "sense_definition": "Wziąć lub otrzymać coś, często w sposób oficjalny, systematyczny lub medyczny (np. pieniądze, próbki).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "general_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pielęgniarka musi delikatnie <w>pobrać</w> krew pacjenta do analizy.",
                            "target_lang_translations": {
                                "ru": "Медсестра должна аккуратно <w>взять</w> кровь пациента для анализа."
                            }
                        },
                        {
                            "text": "Z twojego konta bankowego bank może <w>pobrać</w> opłatę za prowadzenie rachunku.",
                            "target_lang_translations": {
                                "ru": "Банк может <w>снять</w> с твоего банковского счета плату за обслуживание."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pobrać krew",
                        "pobrać opłatę",
                        "pobrać gotówkę",
                        "pobrać wymaz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взять или получить что-либо, часто официально, систематически или в медицинских целях (например, деньги, образцы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взять",
                                "target_lang_sense_clarification": "кровь, образцы"
                            },
                            {
                                "target_lang_word": "снять",
                                "target_lang_sense_clarification": "деньги со счета, плату"
                            },
                            {
                                "target_lang_word": "взыскать",
                                "target_lang_sense_clarification": "официально плату, налог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17e5bdad-8730-31d1-afe5-1f0ba17420ea",
                    "sense_definition": "Przybrać na wadze lub poprawić swój wygląd fizyczny (często o zwierzętach).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po dobrym jedzeniu ta chuda krowa szybko się <w>pobrała</w>.",
                            "target_lang_translations": {
                                "ru": "На хорошем корме эта худая корова быстро <w>поправилась</w>."
                            }
                        },
                        {
                            "text": "Gospodarz zauważył, że zwierzęta <w>pobrały</w> się na wiosnę.",
                            "target_lang_translations": {
                                "ru": "Хозяин заметил, что животные <w>оправились</w> весной."
                            }
                        }
                    ],
                    "synonyms": [
                        "utyć"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "używane głównie w gwarach lub języku potocznym wsi"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Набрать вес или улучшить свой внешний вид (часто о животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поправиться",
                                "target_lang_sense_clarification": "набрать вес"
                            },
                            {
                                "target_lang_word": "оправиться",
                                "target_lang_sense_clarification": "о животных: улучшить вид"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
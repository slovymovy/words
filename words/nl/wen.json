                    "sense_id": "13659572-4b1f-3e17-a812-155c5d23b628",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>wennen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>wen</w> langzaam aan het koude weer in dit land.",
                            "target_lang_translations": {
                                "en": "I am slowly <w>getting used to</w> the cold weather in this country.",
                                "ru": "Я медленно <w>привыкаю</w> к холодной погоде в этой стране."
                            }
                        },
                        {
                            "text": "Ik <w>wen</w> maar moeilijk aan de nieuwe regels op school.",
                            "target_lang_translations": {
                                "en": "I find it hard to <w>adjust</w> to the new rules at school.",
                                "ru": "Я с трудом <w>привыкаю</w> к новым правилам в школе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "er aan wennen",
                        "snel wennen"
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'wennen' (to get used to).",
                        "ru": "Первое лицо единственного числа настоящего времени от глагола wennen (привыкать)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get used to",
                                "target_lang_sense_clarification": "process of becoming accustomed"
                            },
                            {
                                "target_lang_word": "adjust",
                                "target_lang_sense_clarification": "to adapt to new conditions"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "привыкать",
                                "target_lang_sense_clarification": "адаптироваться к чему-либо"
                            },
                            {
                                "target_lang_word": "свыкаться",
                                "target_lang_sense_clarification": "смиряться с чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ea36d21-521d-34c0-bd79-069b7fbf5edc",
                    "sense_definition": "Gebiedende wijs van <w>wennen</w>.",

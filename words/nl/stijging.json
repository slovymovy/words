            "senses": [
                {
                    "sense_id": "06c7e05f-fc49-34bd-9603-92e6fe9a1f2c",
                    "sense_definition": "Het proces van meer, hoger of groter worden in aantal, waarde of niveau.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>stijging</w> van de kosten werd gecompenseerd door meer verkoop.",
                            "target_lang_translations": {
                                "en": "The <w>increase</w> in costs was compensated by higher sales.",
                                "ru": "<w>Рост</w> расходов был компенсирован увеличением продаж."
                            }
                        },
                        {
                            "text": "Er is een lichte <w>stijging</w> in de temperatuur voorspeld voor morgen.",
                            "target_lang_translations": {
                                "en": "A slight <w>rise</w> in temperature is predicted for tomorrow.",
                                "ru": "На завтра прогнозируется небольшое <w>повышение</w> температуры."
                            }
                        }
                    ],
                    "synonyms": [
                        "toename",
                        "groei"
                    ],
                    "antonyms": [
                        "daling"
                    ],
                    "common_phrases": [
                        "lichte stijging",
                        "sterke stijging",
                        "stijging van prijzen",
                        "flinke stijging"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of becoming more, higher, or larger in number, value, or level.",
                        "ru": "Процесс увеличения количества, стоимости или уровня."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "increase",
                                "target_lang_sense_clarification": "becoming larger in quantity or value"
                            },
                            {
                                "target_lang_word": "rise",
                                "target_lang_sense_clarification": "becoming higher or greater"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рост",
                                "target_lang_sense_clarification": "процесс становления больше или выше (например, цены, экономика)"
                            },
                            {
                                "target_lang_word": "повышение",
                                "target_lang_sense_clarification": "переход на более высокий уровень (например, температура, зарплата)"
                            },
                            {
                                "target_lang_word": "увеличение",
                                "target_lang_sense_clarification": "становение больше по количеству или размеру"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78f41139-7973-37b5-9b34-ab2908491c4c",
                    "sense_definition": "De fysieke beweging naar boven of een gedeelte van een weg dat omhoog gaat.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>stijging</w> van het vliegtuig verliep heel rustig en soepel.",
                            "target_lang_translations": {
                                "en": "The <w>ascent</w> of the plane went very quietly and smoothly.",
                                "ru": "<w>Набор высоты</w> самолёта прошёл очень тихо и плавно."
                            }
                        },
                        {
                            "text": "Fietsers hebben vaak moeite met de steile <w>stijging</w> van deze heuvel.",
                            "target_lang_translations": {
                                "en": "Cyclists often struggle with the steep <w>incline</w> of this hill.",
                                "ru": "Велосипедистам часто трудно даётся крутой <w>подъём</w> на этот холм."
                            }
                        }
                    ],
                    "antonyms": [
                        "daling"
                    ],
                    "common_phrases": [
                        "steile stijging",
                        "langzame stijging"
                    ],
                    "target_lang_definitions": {
                        "en": "The physical movement upwards or a section of a road that goes up.",
                        "ru": "Физическое движение вверх или участок дороги, идущий вверх."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ascent",
                                "target_lang_sense_clarification": "physical upward movement"
                            },
                            {
                                "target_lang_word": "rise",
                                "target_lang_sense_clarification": "upward slope or movement"
                            },
                            {
                                "target_lang_word": "incline",
                                "target_lang_sense_clarification": "slope of a road or hill"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подъём",

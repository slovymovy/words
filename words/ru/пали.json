                    ],
                    "common_phrases": [
                        "пали из пушек",
                        "не пали"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative singular form of the verb *палить* (to shoot from a firearm or to burn with fire)."
                    },
                    "translations": {

            "senses": [
                {
                    "sense_id": "f52f8be3-2a9e-38a5-bc59-9272338087f9",
                    "sense_definition": "Krzyczeć bardzo głośno, wrzeszczeć lub głośno płakać (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sound_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dlaczego ten mały chłopiec ciągle <drze> się w sklepie?",
                            "target_lang_translations": {
                                "ru": "Почему этот маленький мальчик постоянно <орёт> в магазине?"
                            }
                        },
                        {
                            "text": "Sąsiedzi zaczęli się <drzeć> na siebie w środku nocy.",
                            "target_lang_translations": {
                                "ru": "Соседи начали <орать> друг на друга посреди ночи."
                            }
                        }
                    ],
                    "synonyms": [
                        "kłócić się",
                        "rwać"
                    ],
                    "antonyms": [
                        "szeptać",
                        "milczeć"
                    ],
                    "common_phrases": [
                        "drzeć się wniebogłosy",
                        "drzeć się na kogoś",
                        "przestać się drzeć"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "very common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кричать очень громко, вопить или громко плакать (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "орать",
                                "target_lang_sense_clarification": "разговорное, грубое"
                            },
                            {
                                "target_lang_word": "вопить",
                                "target_lang_sense_clarification": "издавать громкие крики"
                            },
                            {
                                "target_lang_word": "надрываться",
                                "target_lang_sense_clarification": "громко кричать или плакать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7232a64c-9368-343b-9068-12d1e1f5f299",
                    "sense_definition": "Rozrywać coś na kawałki, niszczyć materiał lub papier używając siły.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przypływie złości zaczął <drzeć> list od swojej byłej dziewczyny.",
                            "target_lang_translations": {
                                "ru": "В приступе гнева он начал <рвать> письмо от своей бывшей девушки."
                            }
                        },
                        {
                            "text": "Nie powinieneś <drzeć> tych ważnych dokumentów na małe kawałki.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует <рвать> эти важные документы на мелкие кусочки."
                            }
                        }
                    ],
                    "synonyms": [
                        "rwać"
                    ],
                    "antonyms": [
                        "zszywać",
                        "sklejać"
                    ],
                    "common_phrases": [

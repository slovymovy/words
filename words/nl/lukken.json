                },
                {
                    "sense_id": "a6978a67-faf7-38d4-820f-b0f7ff8149f8",
                    "sense_definition": "Erin slagen om een bepaalde handeling te volbrengen",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "succes_hebben",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is me eindelijk <w>gelukt</w> om hem te bereiken.",
                            "target_lang_translations": {
                                "ru": "Мне наконец-то <w>удалось</w> с ним связаться.",
                                "en": "I finally <w>managed</w> to reach him."
                            }
                        },
                        {
                            "text": "Het <w>lukt</w> haar niet om op tijd te komen.",
                            "target_lang_translations": {
                                "ru": "У неё не <w>получается</w> прийти вовремя.",
                                "en": "She does not <w>manage</w> to arrive on time."
                            }
                        }
                    ],
                    "synonyms": [
                        "slagen"
                    ],
                    "antonyms": [
                        "falen"
                    ],
                    "common_phrases": [
                        "het is gelukt",
                        "het lukt niet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Суметь выполнить определенное действие",
                        "en": "To succeed in accomplishing a specific action"
                    },
                    "translations": {
                        "ru": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "95206b4f-aa55-34c3-a157-57d7898211f0",
                    "sense_definition": "Действие по глаголу <w>щекотать</w>; ощущение лёгкого прикосновения к коже, вызывающее смех или непроизвольные движения.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я почувствовал на лице лёгкий <щёкот> тёплого ветра.",
                            "target_lang_translations": {
                                "en": "I felt the light <tickle> of the warm wind on my face."
                            }
                        },
                        {
                            "text": "Странный <щёкот> в носу заставил его громко чихнуть.",
                            "target_lang_translations": {
                                "en": "A strange <tickle> in his nose made him sneeze loudly."
                            }
                        }
                    ],
                    "synonyms": [
                        "щекотка",
                        "щекотание"
                    ],
                    "common_phrases": [
                        "лёгкий щёкот",
                        "тёплый щёкот",
                        "щёкот травы"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "слово в этом значении устарело, сейчас обычно используется «щекотка»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The act of tickling; a sensation of light touch on the skin causing laughter or involuntary movements."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tickle",
                                "target_lang_sense_clarification": "sensation or act"
                            },
                            {
                                "target_lang_word": "tickling",
                                "target_lang_sense_clarification": "the act of tickling"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07e24816-0389-3459-99f5-39c29d16c99b",
                    "sense_definition": "Характерные звуки пения некоторых птиц (соловья, сороки), напоминающие частые, отрывистые щелчки или стрекотание.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из густых кустов доносился громкий соловьиный <щёкот>.",
                            "target_lang_translations": {
                                "en": "A loud nightingale <trill> came from the thick bushes."
                            }
                        },
                        {
                            "text": "Весенний лес наполнился радостным птичьим <щёкотом> и свистом.",
                            "target_lang_translations": {
                                "en": "The spring forest was filled with joyful bird <warbling> and whistling."
                            }
                        }
                    ],
                    "synonyms": [
                        "трели",
                        "щекотание"
                    ],
                    "common_phrases": [
                        "соловьиный щёкот",
                        "птичий щёкот",
                        "щёкот сороки"
                    ],
                    "target_lang_definitions": {
                        "en": "The characteristic sounds of certain birds (nightingales, magpies), resembling frequent, abrupt clicks or chattering."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trill",
                                "target_lang_sense_clarification": "rapid alternating sound"
                            },
                            {
                                "target_lang_word": "warble",
                                "target_lang_sense_clarification": "singing with trills"
                            },
                            {
                                "target_lang_word": "jug",
                                "target_lang_sense_clarification": "sound of a nightingale"
                            }
                        ]
                    }
                }
            ]
        }
    ]

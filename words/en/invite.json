{
    "word_family": [
        "invitation",
        "inviting",
        "invitingly",
        "inviter",
        "invitational"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9d7393b7-e580-3e5e-905c-6a53b1b98123",
                    "sense_definition": "To ask someone to go somewhere or do something, such as attend an event or come to a house.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>invited</w> our friends round for dinner last night.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером мы <w>пригласили</w> друзей к себе на ужин."
                            }
                        },
                        {
                            "text": "She <w>invited</w> me to her birthday party next weekend.",
                            "target_lang_translations": {
                                "ru": "Она <w>пригласила</w> меня на свой день рождения в следующие выходные."
                            }
                        }
                    ],
                    "synonyms": [
                        "ask"
                    ],
                    "common_phrases": [
                        "invite over",
                        "invite to dinner",
                        "invite inside",
                        "invite to a party"
                    ],
                    "target_lang_definitions": {
                        "ru": "Просить кого-либо пойти куда-либо или сделать что-либо, например, посетить мероприятие или прийти в гости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приглашать",
                                "target_lang_sense_clarification": "общее значение, несовершенный вид"
                            },
                            {
                                "target_lang_word": "пригласить",
                                "target_lang_sense_clarification": "общее значение, совершенный вид"
                            },
                            {
                                "target_lang_word": "звать",
                                "target_lang_sense_clarification": "разговорное, менее формальное"
                            },
                            {
                                "target_lang_word": "позвать",
                                "target_lang_sense_clarification": "разговорное, совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a069eeb-5cdc-3176-b865-e40232a03748",
                    "sense_definition": "To request something formally or politely.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "formal_request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee <w>invites</w> suggestions from the public regarding the new park.",
                            "target_lang_translations": {
                                "ru": "Комитет <w>приглашает</w> общественность вносить предложения касательно нового парка."
                            }
                        },
                        {
                            "text": "I <w>invite</w> you all to be seated before the presentation begins.",
                            "target_lang_translations": {
                                "ru": "Я <w>прошу</w> всех занять свои места перед началом презентации."
                            }
                        }
                    ],
                    "synonyms": [
                        "request",
                        "ask for"
                    ],
                    "common_phrases": [
                        "invite questions",
                        "invite comments",
                        "invite applications"
                    ],
                    "target_lang_definitions": {
                        "ru": "Формально или вежливо просить о чем-либо или предлагать сделать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просить",
                                "target_lang_sense_clarification": "вежливая просьба"
                            },
                            {
                                "target_lang_word": "предлагать",
                                "target_lang_sense_clarification": "побуждать к действию"
                            },
                            {
                                "target_lang_word": "приглашать",
                                "target_lang_sense_clarification": "в контексте официальных предложений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fcedb81-944e-37da-8a0d-e69851494125",
                    "sense_definition": "To act in a way that makes a particular result or reaction likely to happen, often a negative one.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "causation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Leaving your car unlocked on this street is just <w>inviting</w> theft.",
                            "target_lang_translations": {
                                "ru": "Оставлять машину незапертой на этой улице — значит просто <w>провоцировать</w> кражу."
                            }
                        },
                        {
                            "text": "His aggressive attitude tends to <w>invite</w> trouble wherever he goes.",
                            "target_lang_translations": {
                                "ru": "Его агрессивное поведение, как правило, <w>навлекает</w> неприятности, куда бы он ни пошел."
                            }
                        }
                    ],
                    "synonyms": [
                        "encourage",
                        "provoke"
                    ],
                    "common_phrases": [
                        "invite trouble",
                        "invite criticism",
                        "invite disaster"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя так, что это провоцирует определенные (часто негативные) последствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навлекать",
                                "target_lang_sense_clarification": "притягивать неприятности"
                            },
                            {
                                "target_lang_word": "провоцировать",
                                "target_lang_sense_clarification": "вызывать реакцию"
                            },
                            {
                                "target_lang_word": "способствовать",
                                "target_lang_sense_clarification": "создавать условия для чего-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "614fdba7-1579-3ee4-b07d-5f04f7688ccd",
                    "sense_definition": "To attract, allure, or tempt someone or something to come closer.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "attraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cool water of the lake <w>invited</w> us to swim on that hot afternoon.",
                            "target_lang_translations": {
                                "ru": "Прохладная вода озера <w>манила</w> нас искупаться в этот жаркий полдень."
                            }
                        },
                        {
                            "text": "Shady groves and soft grass <w>invite</w> easy sleep.",
                            "target_lang_translations": {
                                "ru": "Тенистые рощи и мягкая трава <w>располагают</w> к легкому сну."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary or poetic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Привлекать, манить или соблазнять подойти ближе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "манить",
                                "target_lang_sense_clarification": "притягивать к себе"
                            },
                            {
                                "target_lang_word": "привлекать",
                                "target_lang_sense_clarification": "вызывать интерес"
                            },
                            {
                                "target_lang_word": "звать",
                                "target_lang_sense_clarification": "поэтическое использование"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2cc0fd3d-4bad-3db4-8fb5-05e429bf491c",
                    "sense_definition": "An invitation.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Did you get an <w>invite</w> to the wedding next month?",
                            "target_lang_translations": {
                                "ru": "Ты получил <w>приглашение</w> на свадьбу в следующем месяце?"
                            }
                        },
                        {
                            "text": "Thanks for the <w>invite</w>, but I already have plans for Friday.",
                            "target_lang_translations": {
                                "ru": "Спасибо за <w>приглашение</w>, но у меня уже есть планы на пятницу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "send an invite",
                        "get an invite",
                        "accept an invite"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informal short form of invitation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приглашение (существительное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приглашение",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "пригласительный",
                                "target_lang_sense_clarification": "разговорное сокращение от 'пригласительный билет'"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
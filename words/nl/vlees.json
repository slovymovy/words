                {
                    "sense_id": "aa8f3dac-d01a-3883-8387-9e729e91cc4f",
                    "sense_definition": "Spierweefsel van dieren dat gegeten wordt als voedsel.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een vegetariër eet geen <w>vlees</w>.",
                            "target_lang_translations": {
                                "en": "A vegetarian does not eat <w>meat</w>.",
                                "ru": "Вегетарианец не ест <w>мяса</w>."
                            }
                        },
                        {
                            "text": "We eten vanavond aardappelen met groente en een stukje <w>vlees</w>.",
                            "target_lang_translations": {
                                "en": "Tonight we are eating potatoes with vegetables and a piece of <w>meat</w>.",
                                "ru": "Сегодня вечером мы едим картофель с овощами и кусочком <w>мяса</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "vis"
                    ],
                    "common_phrases": [
                        "vlees eten",
                        "rood vlees",
                        "wit vlees",
                        "stukje vlees"
                    ],
                    "target_lang_definitions": {
                        "en": "Muscle tissue of animals that is eaten as food.",
                        "ru": "Мышечная ткань животных, употребляемая в пищу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "meat",
                                "target_lang_sense_clarification": "animal tissue used as food"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мясо",
                                "target_lang_sense_clarification": "продукт питания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bca4a9a2-f905-378b-97d4-b93eceee019a",
                    "sense_definition": "Het zachte weefsel van het lichaam van mens of dier, bestaande uit spieren en vet.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De diepe snee in zijn arm ging tot in het <w>vlees</w>.",
                            "target_lang_translations": {
                                "en": "The deep cut in his arm went into the <w>flesh</w>.",
                                "ru": "Глубокий порез на его руке дошел до самого <w>мяса</w>."
                            }
                        },
                        {
                            "text": "Hij is mijn eigen <w>vlees</w> en bloed.",
                            "target_lang_translations": {
                                "en": "He is my own <w>flesh</w> and blood.",
                                "ru": "Он — моя собственная <w>плоть</w> и кровь."
                            }
                        }
                    ],
                    "antonyms": [
                        "bot"
                    ],
                    "common_phrases": [
                        "vlees en bloed",
                        "in eigen vlees snijden"
                    ],
                    "target_lang_definitions": {
                        "en": "The soft tissue of the human or animal body, consisting of muscle and fat.",
                        "ru": "Мягкие ткани тела человека или животного, состоящие из мышц и жира."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flesh",
                                "target_lang_sense_clarification": "soft tissue of the body"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "плоть",
                                "target_lang_sense_clarification": "ткани тела, книжн."
                            },
                            {
                                "target_lang_word": "мясо",
                                "target_lang_sense_clarification": "мышечная ткань, разг."
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17792352-c65d-3f24-b2bf-31fc63e03bd0",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>vlezen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>vlees</w> de huiden voordat ze worden gelooid.",
                            "target_lang_translations": {
                                "en": "I <w>flesh</w> the hides before they are tanned.",
                                "ru": "Я <w>мездрю</w> шкуры перед дублением."
                            }
                        },
                        {
                            "text": "Als ik het vlees van het bot haal, <w>vlees</w> ik het bot.",
                            "target_lang_translations": {
                                "en": "When I take the meat off the bone, I <w>flesh</w> the bone.",
                                "ru": "Когда я снимаю мясо с кости, я <w>очищаю</w> кость."
                            }
                        }
                    ],
                    "traits": [

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "da6e7ce3-c87e-3ff3-a4bf-fecb2cd3d866",
                    "sense_definition": "Uwalniać się od kogoś lub czegoś, co jest zbędne, kłopotliwe, niechciane lub szkodliwe; czynność ta niemal zawsze występuje w połączeniu z zaimkiem zwrotnym „się”.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy regularnie <pozbywać> się starych ubrań, których już nie nosimy.",
                            "target_lang_translations": {
                                "ru": "Мы должны регулярно <избавляться> от старой одежды, которую уже не носим."
                            }
                        },
                        {
                            "text": "Firma stara się <pozbywać> toksycznych odpadów w sposób bezpieczny dla środowiska.",
                            "target_lang_translations": {
                                "ru": "Компания старается <избавляться> от токсичных отходов безопасным для окружающей среды способом."
                            }
                        },
                        {
                            "text": "Nie powinieneś tak łatwo <pozbywać> się marzeń tylko z powodu jednej porażki.",
                            "target_lang_translations": {
                                "ru": "Ты не должен так легко <отказываться> от мечты только из-за одной неудачи."
                            }
                        }
                    ],
                    "synonyms": [
                        "uwalniać się",
                        "wyzbywać się",
                        "usuwać",
                        "rozstawać się",
                        "gubić"
                    ],
                    "antonyms": [
                        "gromadzić",
                        "zachowywać",
                        "zbierać",
                        "kolekcjonować"
                    ],
                    "common_phrases": [
                        "pozbywać się problemu",
                        "pozbywać się śmieci",
                        "pozbywać się złudzeń",
                        "pozbywać się dowodów",
                        "pozbywać się odpowiedzialności"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wymaga zaimka zwrotnego „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Освобождаться от кого-либо или чего-либо ненужного, обременительного или вредного; это действие почти всегда употребляется с возвратным местоимением «się»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избавляться",
                                "target_lang_sense_clarification": "освобождаться от чего-то ненужного или неприятного"
                            },
                            {
                                "target_lang_word": "освобождаться",
                                "target_lang_sense_clarification": "становиться свободным от чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb129459-b08f-347d-95ba-bb3d3c8caab6",
                    "sense_definition": "Sprawiać, że ktoś traci coś cennego lub ważnego; pozbawiać kogoś czegoś.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Okrutna wojna potrafiła w jedną chwilę <pozbywać> ludzi całego dorobku życia.",
                            "target_lang_translations": {
                                "ru": "Жестокая война могла в одно мгновение <лишать> людей всего нажитого за жизнь."
                            }
                        },
                        {
                            "text": "Nie chciałbym <pozbywać> cię nadziei, ale szanse na sukces są niewielkie.",
                            "target_lang_translations": {
                                "ru": "Я не хотел бы <лишать> тебя надежды, но шансы на успех невелики."
                            }
                        }
                    ],
                    "synonyms": [
                        "pozbywać"
                    ],
                    "antonyms": [
                        "obdarowywać",
                        "zapewniać"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie zastępowane przez „pozbawiać”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять кого-то терять что-то ценное или важное; лишать кого-то чего-то (устаревшее значение, сейчас чаще используется «pozbawiać»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лишать",
                                "target_lang_sense_clarification": "отнимать что-либо у кого-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]

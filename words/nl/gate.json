{
    "word_family": [
        "instapgate"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8eeb67e4-53d7-37e8-b080-3f023cd6904e",
                    "sense_definition": "Een doorgang op een vliegveld waar passagiers wachten om in het vliegtuig te stappen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "airport_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten opschieten om op tijd bij de <w>gate</w> te zijn voor onze vlucht.",
                            "target_lang_translations": {
                                "ru": "Нам нужно поторопиться, чтобы вовремя быть у <w>выхода на посадку</w> на наш рейс."
                            }
                        },
                        {
                            "text": "De passagiers zaten bij de <w>gate</w> te wachten tot ze mochten instappen.",
                            "target_lang_translations": {
                                "ru": "Пассажиры сидели у <w>выхода на посадку</w> и ждали разрешения войти в самолет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naar de gate",
                        "bij de gate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выход на посадку в аэропорту, где пассажиры ожидают посадки в самолет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выход на посадку",
                                "target_lang_sense_clarification": "стандартный термин в аэропорту"
                            },
                            {
                                "target_lang_word": "гейт",
                                "target_lang_sense_clarification": "разговорное, заимствование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a42335ea-d8ac-3dc2-87ab-894b65b2ca89",
                    "sense_definition": "Een poortje bij een ingang of uitgang dat controleert op diefstal of verboden voorwerpen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "security_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het alarm ging af toen de klant met het onbetaalde artikel door de <w>gate</w> liep.",
                            "target_lang_translations": {
                                "ru": "Сигнализация сработала, когда покупатель с неоплаченным товаром прошел через <w>рамку</w>."
                            }
                        },
                        {
                            "text": "Bij de bibliotheek moet je door een <w>gate</w> die registreert of je boeken geleend zijn.",
                            "target_lang_translations": {
                                "ru": "В библиотеке нужно пройти через <w>рамку</w>, которая регистрирует, оформлены ли книги."
                            }
                        }
                    ],
                    "synonyms": [
                        "detectiepoortje"
                    ],
                    "common_phrases": [
                        "door de gate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рамка безопасности или сенсорные ворота на входе или выходе для предотвращения краж или обнаружения предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рамка",
                                "target_lang_sense_clarification": "устройство контроля (металлоискатель или антивор)"
                            },
                            {
                                "target_lang_word": "противокражные ворота",
                                "target_lang_sense_clarification": "в магазинах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26a1ceeb-4056-33df-8ee5-60a75a6521c1",
                    "sense_definition": "Een schandaal of affaire, vaak met betrekking tot politiek of corruptie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kranten stonden wekenlang vol over de nieuwe politieke <w>gate</w>.",
                            "target_lang_translations": {
                                "ru": "Газеты неделями пестрели статьями о новом политическом <w>скандале</w>."
                            }
                        },
                        {
                            "text": "Wat begon als een klein incident, groeide al snel uit tot een ware <w>gate</w>.",
                            "target_lang_translations": {
                                "ru": "То, что начиналось как мелкий инцидент, быстро переросло в настоящий <w>скандал</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "schandaal",
                        "affaire"
                    ],
                    "target_lang_definitions": {
                        "ru": "Политический или коррупционный скандал (часто по аналогии с Уотергейтом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скандал",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "афера",
                                "target_lang_sense_clarification": "сомнительная сделка или событие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8f53d068-9f65-34dc-8234-2d3fefff17f4",
                    "sense_definition": "Ozdoba noszona na szyi, wykonana zazwyczaj z metalu, kamieni szlachetnych, koralików lub innych materiałów dekoracyjnych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "jewelry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gabrysia miała na sobie przepiękny <w>naszyjnik</w> z howlitów.",
                            "target_lang_translations": {
                                "ru": "На Габрисе было прекрасное <w>ожерелье</w> из говлита."
                            }
                        },
                        {
                            "text": "Na rocznicę ślubu mąż podarował jej złoty <w>naszyjnik</w>.",
                            "target_lang_translations": {
                                "ru": "На годовщину свадьбы муж подарил ей золотое <w>ожерелье</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "złoty naszyjnik",
                        "naszyjnik z pereł",
                        "nosić naszyjnik",
                        "zapiąć naszyjnik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшение, которое носят на шее, обычно изготовленное из металла, драгоценных камней, бисера или других декоративных материалов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ожерелье",
                                "target_lang_sense_clarification": "украшение"
                            },
                            {
                                "target_lang_word": "колье",
                                "target_lang_sense_clarification": "с драгоценными камнями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "504779b6-34a6-3820-bce4-0d58de40a5bc",
                    "sense_definition": "Obroża z kolcami zakładana psu na szyję, służąca do tresury lub ochrony.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_gear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właściciel założył psu <w>naszyjnik</w>, aby łatwiej go kontrolować podczas spaceru.",
                            "target_lang_translations": {
                                "ru": "Хозяин надел на собаку <w>строгий ошейник</w>, чтобы легче контролировать её во время прогулки."
                            }
                        },
                        {
                            "text": "Metalowa kolczatka to rodzaj ostrego <w>naszyjnika</w> dla psów obronnych.",
                            "target_lang_translations": {
                                "ru": "Металлический парфорс — это разновидность острого <w>ошейника</w> для сторожевых собак."
                            }
                        }
                    ],
                    "synonyms": [
                        "kolczatka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ошейник с шипами, надеваемый на шею собаки, используемый для дрессировки или защиты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строгий ошейник",
                                "target_lang_sense_clarification": "для дрессировки"
                            },
                            {
                                "target_lang_word": "парфорс",
                                "target_lang_sense_clarification": "специальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "836b9b1e-dac5-3994-8837-fc7350cac43c",
                    "sense_definition": "Element zbroi rycerskiej chroniący szyję.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "armor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz poprawił stalowy <w>naszyjnik</w> przed rozpoczęciem turnieju.",
                            "target_lang_translations": {
                                "ru": "Рыцарь поправил стальное <w>ожерелье</w> перед началом турнира."
                            }
                        },
                        {
                            "text": "Ten zabytkowy <w>naszyjnik</w> był częścią pełnej zbroi płytowej.",
                            "target_lang_translations": {
                                "ru": "Это старинное <w>ожерелье</w> было частью полного латного доспеха."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент рыцарских доспехов, защищающий шею."

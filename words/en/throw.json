    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5c3e1aeb-e827-36a5-9df8-c1108bea67e2",
                    "sense_definition": "To send something through the air with force, usually by a movement of the arm and hand.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>throw</w> the ball to the dog.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>брось</w> мяч собаке."
                            }
                        },
                        {
                            "text": "He <w>threw</w> the empty bottle into the recycling bin.",
                            "target_lang_translations": {
                                "ru": "Он <w>выбросил</w> пустую бутылку в мусорную корзину."
                            }
                        }
                    ],
                    "synonyms": [
                        "hurl",
                        "toss",
                        "fling",
                        "pitch",
                        "cast",
                        "chuck",
                        "lob"
                    ],
                    "antonyms": [

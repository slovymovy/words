{
    "word_family": [
        "столь",
        "столечко",
        "постольку"
    ],
    "entries": [
        {
            "pos": "numeral",
            "senses": [
                {
                    "sense_id": "c7b03777-f0cf-3cf5-a964-74db8d207182",
                    "sense_definition": "Такое количество; так много (указывает на большое или определённое число людей, предметов или объём вещества).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня никогда не было <столько> свободного времени.",
                            "target_lang_translations": {
                                "en": "I have never had <so much> free time."
                            }
                        },
                        {
                            "text": "На улице выпало <столько> снега, что машины не могли проехать.",
                            "target_lang_translations": {
                                "en": "<So much> snow fell outside that cars couldn't get through."
                            }
                        }
                    ],
                    "synonyms": [
                        "так много",
                        "масса",
                        "куча",
                        "уйма"
                    ],
                    "antonyms": [
                        "мало",
                        "немного"
                    ],
                    "common_phrases": [
                        "столько же",
                        "столько всего",
                        "столько раз",
                        "столько лет"
                    ],
                    "target_lang_definitions": {
                        "en": "So much or so many; such an amount (indicating a large or specific number of people, objects, or volume of substance)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "so much",
                                "target_lang_sense_clarification": "used with uncountable nouns or abstract quantities"
                            },
                            {
                                "target_lang_word": "so many",
                                "target_lang_sense_clarification": "used with countable nouns"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "13eb4c52-5bc4-3f5a-b3c9-222bc99f70d6",
                    "sense_definition": "В такой степени, в такой мере (часто используется при сопоставлении или для описания интенсивности действия).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не <столько> устал, сколько просто хочу побыть в тишине.",
                            "target_lang_translations": {
                                "en": "I am not <so much> tired as I just want to be in silence."
                            }
                        },
                        {
                            "text": "Он <столько> работал в этом месяце, что заслужил премию.",
                            "target_lang_translations": {
                                "en": "He worked <so much> this month that he earned a bonus."
                            }
                        }
                    ],
                    "synonyms": [
                        "так",
                        "до такой степени",
                        "настолько"
                    ],
                    "common_phrases": [
                        "не столько сколько"
                    ],
                    "target_lang_definitions": {
                        "en": "To such a degree or extent (often used in comparisons or to describe the intensity of an action)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "so much",
                                "target_lang_sense_clarification": "adverb indicating degree or intensity"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
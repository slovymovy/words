    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "509a139d-59ef-3ae1-bc8f-a0f8a25568e0",
                    "sense_definition": "Mały, pospolity ptak o szarym i brązowym upierzeniu, żyjący blisko siedzib ludzkich, często spotykany w miastach i na wsiach.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ptaki",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mały <w>wróbel</w> usiadł na parapecie i zaczął głośno ćwierkać.",
                            "target_lang_translations": {
                                "ru": "Маленький <w>воробей</w> сел на подоконник и начал громко чирикать."
                            }
                        },
                        {
                            "text": "Zimą często dokarmiamy <w>wróble</w> ziarnem oraz okruchami chleba.",
                            "target_lang_translations": {
                                "ru": "Зимой мы часто подкармливаем <w>воробьёв</w> зерном и хлебными крошками."
                            }
                        }
                    ],
                    "synonyms": [
                        "wróbel domowy",
                        "wróbel zwyczajny"
                    ],
                    "common_phrases": [
                        "szary wróbel",
                        "ćwierkający wróbel",
                        "stado wróbli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькая, распространенная птица с серым и коричневым оперением, живущая вблизи человеческого жилья, часто встречающаяся в городах и деревнях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воробей",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49ecad65-c893-3cd2-aaa1-f22d6fe31d6b",
                    "sense_definition": "Ustawione pionowo snopy zboża, które tworzą daszek chroniący kłosy przed deszczem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rolnictwo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po żniwach na polu stały rzędy <w>wróbli</w> czekających na zwózkę.",
                            "target_lang_translations": {
                                "ru": "После жатвы на поле стояли ряды <w>бабок</w>, ожидающих вывоза."
                            }
                        },
                        {
                            "text": "Rolnik ułożył zboże w <w>wróble</w>, aby szybciej wyschło na słońcu.",
                            "target_lang_translations": {
                                "ru": "Фермер сложил зерно в <w>бабки</w>, чтобы оно быстрее высохло на солнце."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "określenie stosowane w tradycyjnym rolnictwie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикально установленные снопы зерна, образующие навес, защищающий колосья от дождя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бабка",
                                "target_lang_sense_clarification": "способ укладки снопов"
                            },
                            {
                                "target_lang_word": "суслон",
                                "target_lang_sense_clarification": "сельское хозяйство"
                            }
                        ]
                    }
                }
            ]
        },
        {

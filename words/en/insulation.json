{
    "word_family": [
        "insulate",
        "insulator"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cbfaf24f-7b1f-3c47-88d7-03626955de96",
                    "sense_definition": "Material used to prevent heat, cold, noise, or electricity from passing through something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The builders put extra <w>insulation</w> in the roof to reduce heating costs.",
                            "target_lang_translations": {
                                "ru": "Строители уложили дополнительную <w>изоляцию</w> на крышу, чтобы снизить расходы на отопление."
                            }
                        },
                        {
                            "text": "You should check the electrical <w>insulation</w> on these old wires before using them.",
                            "target_lang_translations": {
                                "ru": "Перед использованием этих старых проводов следует проверить их электрическую <w>изоляцию</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lagging",
                        "padding",
                        "lining"
                    ],
                    "common_phrases": [
                        "thermal insulation",
                        "sound insulation",
                        "roof insulation",
                        "thick insulation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Материал, используемый для предотвращения прохождения тепла, холода, шума или электричества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изоляция",
                                "target_lang_sense_clarification": "материал"
                            },
                            {
                                "target_lang_word": "утеплитель",
                                "target_lang_sense_clarification": "теплоизоляционный материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2dc887f-cb49-3795-ac00-21f0f7f0c025",
                    "sense_definition": "The act of protecting something with a non-conducting material, or the state of being protected from heat, sound, or electricity.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "act_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thick fur provides excellent <w>insulation</w> for animals living in the arctic.",
                            "target_lang_translations": {
                                "ru": "Густой мех обеспечивает отличную <w>изоляцию</w> животным, обитающим в Арктике."
                            }
                        },
                        {
                            "text": "Proper <w>insulation</w> of the pipes is necessary to prevent them from bursting in winter.",
                            "target_lang_translations": {
                                "ru": "Правильная <w>изоляция</w> труб необходима, чтобы предотвратить их разрыв зимой."
                            }
                        }
                    ],
                    "synonyms": [
                        "protection",
                        "shielding",
                        "isolation"
                    ],
                    "antonyms": [
                        "exposure",
                        "conduction"
                    ],
                    "common_phrases": [
                        "provide insulation",
                        "insulation against",
                        "electrical insulation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по защите чего-либо непроводящим материалом или состояние защищенности от тепла, звука или электричества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изоляция",
                                "target_lang_sense_clarification": "процесс или состояние"
                            },
                            {
                                "target_lang_word": "изолирование",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
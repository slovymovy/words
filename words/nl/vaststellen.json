    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6b462cda-405a-3c30-a339-b44847af36a6",
                    "sense_definition": "Een definitieve beslissing nemen over iets, zoals een tijdstip, prijs of regel.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "besluitvorming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten nog een datum voor de vergadering <w>vaststellen</w>.",
                            "target_lang_translations": {
                                "ru": "Нам нужно еще <w>назначить</w> дату собрания."
                            }
                        },
                        {
                            "text": "De overheid heeft nieuwe regels <w>vastgesteld</w> voor het verkeer.",
                            "target_lang_translations": {
                                "ru": "Правительство <w>установило</w> новые правила дорожного движения."
                            }
                        }
                    ],
                    "synonyms": [
                        "bepalen"
                    ],
                    "common_phrases": [
                        "een datum vaststellen",
                        "de prijs vaststellen",
                        "regels vaststellen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать окончательное решение о чем-либо, например, о времени, цене или правилах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устанавливать",
                                "target_lang_sense_clarification": "правила, цены, порядок"
                            },
                            {
                                "target_lang_word": "назначать",
                                "target_lang_sense_clarification": "дату, время, встречу"
                            },
                            {
                                "target_lang_word": "определять",
                                "target_lang_sense_clarification": "решать, делать выбор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4daea945-05a6-39b3-a76b-a1a0299c343b",
                    "sense_definition": "Door onderzoek of waarneming zeker weten dat iets zo is.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "waarneming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts kon de oorzaak van de ziekte niet direct <w>vaststellen</w>.",
                            "target_lang_translations": {
                                "ru": "Врач не смог сразу <w>установить</w> причину болезни."
                            }
                        },
                        {
                            "text": "De politie probeert de identiteit van de dader <w>vast</w> te <w>stellen</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция пытается <w>установить</w> личность преступника."
                            }
                        }
                    ],
                    "synonyms": [
                        "bepalen"
                    ],
                    "common_phrases": [
                        "de schade vaststellen",
                        "een diagnose vaststellen",
                        "de dood vaststellen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выявлять факт или получать точные сведения путем исследования или наблюдения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устанавливать",

{
    "word_family": [
        "dojrzeć",
        "dojrzewać",
        "dojrzałość",
        "dojrzewanie",
        "niedojrzały"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "defad1f0-e528-3db1-9be6-4114de782ecf",
                    "sense_definition": "O owocach, warzywach lub organizmach żywych: taki, który jest w pełni rozwinięty fizycznie, gotowy do zbioru lub osiągnął wiek dorosły.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym sadzie rosną słodkie i <dojrzałe> jabłka.",
                            "target_lang_translations": {
                                "ru": "В нашем саду растут сладкие и <спелые> яблоки."
                            }
                        },
                        {
                            "text": "Kiedy banany są <dojrzałe>, ich skórka robi się żółta.",
                            "target_lang_translations": {
                                "ru": "Когда бананы <зрелые>, их кожура становится желтой."
                            }
                        }
                    ],
                    "synonyms": [
                        "dorosły"
                    ],
                    "antonyms": [
                        "niedojrzały"
                    ],
                    "common_phrases": [
                        "dojrzałe owoce",
                        "dojrzały wiek",
                        "dojrzała cera"
                    ],
                    "target_lang_definitions": {
                        "ru": "О фруктах, овощах или живых организмах: полностью физически развитый, готовый к сбору или достигший взрослого возраста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спелый",
                                "target_lang_sense_clarification": "о фруктах, овощах"
                            },
                            {
                                "target_lang_word": "зрелый",
                                "target_lang_sense_clarification": "полностью развившийся"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c73a04f-cc54-3567-a7ed-d28f9faa8314",
                    "sense_definition": "O człowieku lub jego zachowaniu: cechujący się rozwagą, mądrością życiową i odpowiedzialnością typową dla osób dorosłych.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo młodego wieku, podjął bardzo <dojrzałą> decyzję.",
                            "target_lang_translations": {
                                "ru": "Несмотря на юный возраст, он принял очень <зрелое> решение."
                            }
                        },
                        {
                            "text": "Szukam pracownika, który jest <dojrzały> emocjonalnie i potrafi pracować w zespole.",
                            "target_lang_translations": {
                                "ru": "Я ищу сотрудника, который <зрелый> эмоционально и умеет работать в команде."
                            }
                        }
                    ],
                    "synonyms": [
                        "odpowiedzialny",
                        "rozsądny",
                        "dorosły"
                    ],
                    "antonyms": [
                        "niedojrzały",
                        "infantylny",
                        "dziecinny",
                        "nieodpowiedzialny"
                    ],
                    "common_phrases": [
                        "dojrzała decyzja",
                        "dojrzały emocjonalnie",
                        "dojrzały człowiek"
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке или его поведении: отличающийся рассудительностью, житейской мудростью и ответственностью, свойственной взрослым людям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зрелый",
                                "target_lang_sense_clarification": "мудрый, обдуманный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0be8c44d-1663-30ee-82ba-32ee55e06e62",
                    "sense_definition": "Forma czasu przeszłego czasownika <dojrzeć> dla rodzaju niemęskoosobowego w liczbie mnogiej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Śliwki na drzewie już <dojrzały> i można je zbierać.",
                            "target_lang_translations": {
                                "ru": "Сливы на дереве уже <созрели>, и их можно собирать."
                            }
                        },
                        {
                            "text": "Jego córki w końcu <dojrzały> do wyprowadzki z domu.",
                            "target_lang_translations": {
                                "ru": "Его дочери наконец <созрели> для того, чтобы съехать из дома."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czas przeszły, liczba mnoga, rodzaj niemęskoosobowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма прошедшего времени глагола <dojrzeć> (созреть) для множественного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "созрели",
                                "target_lang_sense_clarification": "глагол, прошедшее время"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
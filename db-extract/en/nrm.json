                "senses": [
                    {
                        "sense_id": "8d5a8fd1-e40d-3d37-ba65-00c494f9e08e",
                        "entry_id": "9eb9c4e4-75f2-3b05-ad9c-c4dcf4516627",
                        "glosses": [

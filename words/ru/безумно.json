    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b2735c8c-431d-37ef-b296-595ba64af7a3",
                    "sense_definition": "В высшей степени, чрезвычайно, очень сильно.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>безумно</w> рад тебя видеть после долгой разлуки.",
                            "target_lang_translations": {
                                "en": "I am <w>insanely</w> glad to see you after a long separation."
                            }
                        },
                        {
                            "text": "Этот старый город <w>безумно</w> красив в лучах заката.",
                            "target_lang_translations": {
                                "en": "This old city is <w>insanely</w> beautiful in the rays of the sunset."
                            }
                        }
                    ],
                    "synonyms": [
                        "безрассудно"
                    ],
                    "common_phrases": [
                        "безумно рад",
                        "безумно люблю",
                        "безумно нравится",
                        "безумно красиво"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To an extreme degree, tremendously, very strongly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "insanely",
                                "target_lang_sense_clarification": "used as an intensifier for adjectives or verbs"
                            },
                            {
                                "target_lang_word": "madly",
                                "target_lang_sense_clarification": "extremely; often used with feelings like love or joy"
                            },
                            {
                                "target_lang_word": "terribly",
                                "target_lang_sense_clarification": "colloquial intensifier meaning very"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3eb619ce-6efd-356b-8ac0-4284b7c5b88b",
                    "sense_definition": "Безрассудно, как сумасшедший, не слушаясь голоса разума.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он поступил <w>безумно</w>, отказавшись от такой выгодной сделки.",
                            "target_lang_translations": {
                                "en": "He acted <w>foolishly</w> by refusing such a profitable deal."
                            }
                        },
                        {
                            "text": "В гневе он кричал <w>безумно</w> и размахивал руками.",
                            "target_lang_translations": {
                                "en": "In anger, he screamed <w>madly</w> and waved his arms."
                            }
                        }
                    ],
                    "synonyms": [
                        "безрассудно"
                    ],
                    "antonyms": [
                        "разумно"
                    ],
                    "common_phrases": [
                        "поступить безумно",
                        "выглядеть безумно"
                    ],
                    "target_lang_definitions": {
                        "en": "Recklessly, like a madman, without listening to reason."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "recklessly",
                                "target_lang_sense_clarification": "without regard for consequences"
                            },
                            {
                                "target_lang_word": "madly",
                                "target_lang_sense_clarification": "in a wild or uncontrolled manner"
                            },
                            {
                                "target_lang_word": "foolishly",
                                "target_lang_sense_clarification": "lacking good sense or judgment"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4449be69-9068-3120-b14e-abdb8c6bb6e6",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>безумный</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его желание покорить эту вершину было <w>безумно</w>.",
                            "target_lang_translations": {
                                "en": "His desire to conquer this peak was <w>insane</w>."
                            }
                        },
                        {
                            "text": "Лицо его было <w>безумно</w> и выражало крайний ужас.",
                            "target_lang_translations": {
                                "en": "His face was <w>mad</w> and expressed extreme horror."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short form of the neuter singular adjective 'insane' or 'mad'."
                    },
                    "translations": {

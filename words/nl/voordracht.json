                {
                    "sense_id": "c12e1c84-a7c0-36db-967d-613d11abc497",
                    "sense_definition": "Het officieel voorstellen of aanbevelen van een persoon voor een bepaalde functie, ambt of prijs.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nomination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>voordracht</w> van de nieuwe burgemeester was unaniem.",
                            "target_lang_translations": {
                                "en": "The <w>nomination</w> of the new mayor was unanimous.",
                                "ru": "<w>Выдвижение</w> нового мэра было единогласным."
                            }
                        },
                        {
                            "text": "Hij werd benoemd op <w>voordracht</w> van de raad van commissarissen.",
                            "target_lang_translations": {
                                "en": "He was appointed on the <w>recommendation</w> of the Supervisory Board.",
                                "ru": "Он был назначен по <w>представлению</w> наблюдательного совета."
                            }
                        }
                    ],

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "13cd439c-b04b-3e32-a189-8a5ec34a5978",
                    "sense_definition": "Внутренняя моральная сила, решимость, состояние души человека или группы людей.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "inner_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Команда сохраняла высокий боевой <w>дух</w> до самого конца игры.",
                            "target_lang_translations": {
                                "en": "The team maintained high fighting <w>spirit</w> until the very end of the game."
                            }
                        },
                        {
                            "text": "Он не мог собраться с <w>духом</w>, чтобы признаться в своей ошибке.",
                            "target_lang_translations": {
                                "en": "He could not summon the <w>courage</w> to admit his mistake."
                            }
                        }
                    ],
                    "synonyms": [
                        "душа",
                        "материя"
                    ],
                    "antonyms": [
                        "слабость",
                        "малодушие"
                    ],
                    "common_phrases": [
                        "падать духом",
                        "собраться с духом",
                        "боевой дух",
                        "присутствие духа",
                        "сильный духом"
                    ],
                    "target_lang_definitions": {
                        "en": "Inner moral strength, determination, or the state of mind of a person or group."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spirit",
                                "target_lang_sense_clarification": "inner character, disposition, or emotional state"
                            },
                            {
                                "target_lang_word": "morale",
                                "target_lang_sense_clarification": "confidence and discipline of a group"
                            },
                            {
                                "target_lang_word": "courage",
                                "target_lang_sense_clarification": "mental strength to withstand danger or difficulty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "265ea079-6aa3-3935-a375-6149c9d3f39b",
                    "sense_definition": "Сверхъестественное бесплотное существо, призрак.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "supernatural",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "В народных сказках лесной <w>дух</w> часто помогает заблудившимся путникам.",
                            "target_lang_translations": {
                                "en": "In folk tales, a forest <w>spirit</w> often helps lost travelers."
                            }
                        },
                        {
                            "text": "Они верили, что злой <w>дух</w> обитает в этой старой пещере.",
                            "target_lang_translations": {
                                "en": "They believed that an evil <w>spirit</w> inhabited this old cave."
                            }
                        }
                    ],
                    "synonyms": [
                        "призрак",
                        "привидение"
                    ],
                    "common_phrases": [
                        "злой дух",
                        "вызывать духов",
                        "лесной дух"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often used in folklore or mythology contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A supernatural incorporeal being or phantom."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spirit",
                                "target_lang_sense_clarification": "supernatural entity, often inhabiting a place or object"
                            },
                            {
                                "target_lang_word": "ghost",
                                "target_lang_sense_clarification": "soul of a dead person appearing to the living"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df7896cb-2c31-3172-91c1-c9778ce62884",
                    "sense_definition": "Дыхание (обычно используется в устойчивых выражениях).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "breath",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы бежали на станцию во весь <w>дух</w>, чтобы не опоздать на поезд.",
                            "target_lang_translations": {
                                "en": "We ran to the station at full <w>speed</w> so as not to miss the train."
                            }
                        },
                        {
                            "text": "От невероятной красоты горного пейзажа у меня захватило <w>дух</w>.",
                            "target_lang_translations": {
                                "en": "The incredible beauty of the mountain landscape took my <w>breath</w> away."
                            }
                        }
                    ],
                    "common_phrases": [
                        "перевести дух",
                        "захватывает дух",
                        "во весь дух",
                        "испустить дух",
                        "на одном духу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Breath (used primarily in idioms relating to breathing or speed)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "breath",
                                "target_lang_sense_clarification": "air inhaled and exhaled"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d429cca-8e8c-35b0-b285-17cb252eea27",
                    "sense_definition": "Сущность, истинный смысл, основное направление или характер чего-либо.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "essence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это современное здание было построено в <w>духе</w> классической архитектуры.",
                            "target_lang_translations": {
                                "en": "This modern building was built in the <w>spirit</w> of classical architecture."
                            }
                        },
                        {
                            "text": "Судья должен следовать не только букве, но и <w>духу</w> закона.",
                            "target_lang_translations": {
                                "en": "A judge must follow not only the letter, but also the <w>spirit</w> of the law."
                            }
                        }
                    ],
                    "common_phrases": [
                        "в духе времени",
                        "в этом духе",
                        "буква и дух"
                    ],
                    "target_lang_definitions": {
                        "en": "The essence, true meaning, or prevailing character of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spirit",
                                "target_lang_sense_clarification": "real meaning or intent, as opposed to the letter"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eae495b7-1508-31b8-982c-acc2abc31513",
                    "sense_definition": "Запах, воздух (часто специфический, тяжелый или связанный с чем-то конкретным).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "scent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из открытой двери кухни шёл вкусный хлебный <w>дух</w>.",
                            "target_lang_translations": {
                                "en": "A delicious bread <w>smell</w> was coming from the open kitchen door."
                            }
                        },
                        {
                            "text": "В старом подвале стоял тяжёлый и сырой <w>дух</w>.",
                            "target_lang_translations": {
                                "en": "There was a heavy and damp <w>odor</w> in the old basement."
                            }
                        }
                    ],
                    "common_phrases": [
                        "тяжёлый дух",
                        "русский дух"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e946b159-337b-3a28-aa7e-4db222752a24",
                    "sense_definition": "Een geografisch gebied met bepaalde kenmerken, zoals cultuur, taal of bestuur",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is een perfecte <w>regio</w> om gewassen te telen vanwege de schone lucht.",
                            "target_lang_translations": {
                                "en": "This is a perfect <w>region</w> for growing crops because of the clean air.",
                                "ru": "Это идеальный <w>регион</w> для выращивания урожая благодаря чистому воздуху."
                            }
                        },
                        {
                            "text": "Wij gaan dit jaar op vakantie naar een warme <w>regio</w> in Zuid-Europa.",
                            "target_lang_translations": {
                                "en": "We are going on vacation to a warm <w>region</w> in Southern Europe this year.",
                                "ru": "В этом году мы поедем в отпуск в теплый <w>регион</w> на юге Европы."
                            }
                        }
                    ],
                    "synonyms": [
                        "gebied"
                    ],
                    "common_phrases": [
                        "in de regio",
                        "de hele regio",
                        "omliggende regio"
                    ],
                    "target_lang_definitions": {
                        "en": "A geographical area with specific characteristics, such as culture, language, or administration",
                        "ru": "Географическая область с определенными характеристиками, такими как культура, язык или административное устройство"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "region",
                                "target_lang_sense_clarification": "geographical area"
                            },
                            {
                                "target_lang_word": "area",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "district",
                                "target_lang_sense_clarification": "administrative division"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "регион",
                                "target_lang_sense_clarification": "географическая зона"
                            },
                            {
                                "target_lang_word": "область",
                                "target_lang_sense_clarification": "территория"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50def177-df78-305a-9e1d-e715f74d110b",
                    "sense_definition": "Het deel van Nederland dat niet tot de Randstad behoort",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er gaat de komende jaren meer subsidie naar musea in de <w>regio</w>.",
                            "target_lang_translations": {
                                "en": "In the coming years, more subsidies will go to museums in <w>the provinces</w>.",
                                "ru": "В ближайшие годы музеи в <w>провинции</w> получат больше субсидий."
                            }
                        },
                        {
                            "text": "De politiek moet meer aandacht besteden aan de problemen in de <w>regio</w>.",
                            "target_lang_translations": {
                                "en": "Politics needs to pay more attention to the problems in <w>regional areas</w>.",
                                "ru": "Политики должны уделять больше внимания проблемам в <w>регионах</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "Randstad"
                    ],
                    "common_phrases": [
                        "in de regio"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Nederland"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The part of the country outside the main urban center (specifically outside the Randstad in the Netherlands)",
                        "ru": "Часть страны за пределами главного экономического центра (в Нидерландах — за пределами Рандстада)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the provinces",

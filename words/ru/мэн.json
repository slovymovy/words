                    ],
                    "target_lang_definitions": {
                        "en": "A state in the northeastern United States."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Maine",
                                "target_lang_sense_clarification": "US state"
                            }
                        ]

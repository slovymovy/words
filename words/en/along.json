    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "b72d12e3-e462-3704-8f9d-92bd73b94864",
                    "sense_definition": "Moving in a constant direction on a road, path, or area.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We walked <w>along</w> the beach for an hour.",
                            "target_lang_translations": {
                                "ru": "Мы целый час гуляли <w>вдоль</w> пляжа."
                            }
                        },
                        {
                            "text": "Cars drove fast <w>along</w> the highway.",
                            "target_lang_translations": {
                                "ru": "Машины быстро ехали <w>по</w> шоссе."
                            }
                        }
                    ],
                    "synonyms": [
                        "endlong"
                    ],
                    "common_phrases": [
                        "along the road",
                        "along the street",
                        "along the way",
                        "along the path"
                    ],
                    "target_lang_definitions": {
                        "ru": "Движение в постоянном направлении по дороге, тропинке или местности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вдоль",
                                "target_lang_sense_clarification": "в направлении длины"
                            },
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "движение по поверхности или пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "effb8be2-c594-398d-af4a-4e376556f0c5",
                    "sense_definition": "Situated in a line next to the length of something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trees were planted <w>along</w> the river bank.",
                            "target_lang_translations": {
                                "ru": "Деревья были посажены <w>вдоль</w> берега реки."
                            }
                        },
                        {
                            "text": "There are beautiful houses <w>along</w> the coast.",
                            "target_lang_translations": {
                                "ru": "<w>Вдоль</w> побережья стоят красивые дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "alongst"
                    ],
                    "common_phrases": [
                        "along the river",
                        "along the coast",
                        "along the side",
                        "along the edge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположенный в линию вдоль длины чего-либо."

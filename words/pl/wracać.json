                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возвращаться",
                                "target_lang_sense_clarification": "о моде, проблемах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4effee27-b983-33e6-82a8-59f156fb5a83",
                    "sense_definition": "Oddawać coś właścicielowi lub przywracać czemuś dawną cechę.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "transitive_restore",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To magiczne miejsce <w>wraca</w> ludziom nadzieję i spokój.",
                            "target_lang_translations": {
                                "ru": "Это волшебное место <w>возвращает</w> людям надежду и покой."
                            }
                        },
                        {
                            "text": "Wiejskie powietrze szybko <w>wróciło</w> jej utracone siły.",
                            "target_lang_translations": {
                                "ru": "Деревенский воздух быстро <w>вернул</w> ей утраченные силы."
                            }
                        }

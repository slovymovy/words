                        "examples": [
                            {
                                "example_id": "0dc6f53c-1d85-45b4-a8b5-a87c7259568e",
                                "sense_id": "2060b010-4f8f-3a1f-8116-56ead9567375",
                                "text": "And the bright green belt on his body he bore, oblique, like a baldrick, bound at his side below his left shoulder, laced in a knot..."
                            },
                            {
                                "example_id": "0d7a4492-7584-4180-a52d-6e9a6a884aca",
                                "sense_id": "2060b010-4f8f-3a1f-8116-56ead9567375",
                                "text": "That a woman conceiv'd me, I thank her; that she brought me up, I likewise give her most humble thanks; but that I will have a rechate winded in my forehead, or hang my bugle in an invisible baldrick, all women shall pardon me."
                            },
                            {
                                "example_id": "bf1bbf6e-5a43-4fd2-a793-0c362a57e4c1",
                                "sense_id": "2060b010-4f8f-3a1f-8116-56ead9567375",
                                "text": "And from his blazoned baldrick slung, a mighty silver bugle hung..."
                            },
                            {
                                "example_id": "24bb6a93-07cb-420e-8d96-9c7390860f75",
                                "sense_id": "2060b010-4f8f-3a1f-8116-56ead9567375",
                                "text": "The sword was carried in a belt of buff or other leather girded round the body, or thrown over the right shoulder, these shoulder belts were called baudricks."
                            }
                        ]
                    }

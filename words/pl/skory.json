    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d6193a13-9b9d-3996-a76f-91411c379a2d",
                    "sense_definition": "Mający chęć i gotowość do zrobienia czegoś; wyrażający ochotę na podjęcie jakiegoś działania.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "willingness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe kotki są zazwyczaj bardzo <w>skore</w> do zabawy i biegania.",
                            "target_lang_translations": {
                                "ru": "Маленькие котята обычно очень <w>охочи</w> до игр и беготни."
                            }
                        },
                        {
                            "text": "Mój sąsiad jest zawsze <w>skory</w> do pomocy przy naprawie samochodu.",
                            "target_lang_translations": {
                                "ru": "Мой сосед всегда <w>готов</w> помочь с ремонтом машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "chętny",
                        "ochoczy",
                        "skwapliwy"
                    ],
                    "antonyms": [
                        "nieskory",
                        "niechętny"
                    ],
                    "common_phrases": [
                        "skory do pomocy",
                        "skory do zabawy",
                        "skory do żartów",
                        "skory do współpracy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий желание и готовность сделать что-то; выражающий охоту к какому-либо действию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовый",
                                "target_lang_sense_clarification": "выражающий согласие или намерение совершить действие"
                            },
                            {
                                "target_lang_word": "охочий",
                                "target_lang_sense_clarification": "имеющий сильное желание или пристрастие (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84974f70-be0f-3003-b595-592be1d2da9e",
                    "sense_definition": "Łatwo ulegający jakimś emocjom lub tendencjom; mający naturalną predyspozycję do określonych zachowań, często gwałtownych.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "willingness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Człowiek <w>skory</w> do gniewu często żałuje wypowiedzianych w nerwach słów.",
                            "target_lang_translations": {
                                "ru": "Человек, <w>скорый</w> на гнев, часто жалеет о словах, сказанных сгоряча."
                            }
                        },
                        {
                            "text": "Po wypiciu alkoholu mężczyzna stawał się agresywny i <w>skory</w> do bitki.",
                            "target_lang_translations": {
                                "ru": "После употребления алкоголя мужчина становился агрессивным и <w>готовым</w> к драке."
                            }
                        }
                    ],
                    "synonyms": [
                        "skłonny"
                    ],
                    "antonyms": [
                        "nieskory"
                    ],
                    "common_phrases": [
                        "skory do gniewu",
                        "skory do kłótni",
                        "skory do bitki",
                        "skory do płaczu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легко поддающийся эмоциям или порывам; имеющий природную склонность к определенному поведению, часто резкому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скорый",
                                "target_lang_sense_clarification": "в идиомах: легко переходящий в какое-либо состояние (напр. скорый на гнев)"
                            },
                            {
                                "target_lang_word": "склонный",
                                "target_lang_sense_clarification": "имеющий предрасположенность к чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81031313-89da-3f28-9a46-d6d19a70baf2",
                    "sense_definition": "Poruszający się z dużą prędkością lub działający szybko.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Posłaniec był <w>skory</w> i dostarczył królewski list jeszcze przed zachodem słońca.",
                            "target_lang_translations": {
                                "ru": "Гонец был <w>быстр</w> и доставил королевское письмо еще до заката."
                            }
                        },
                        {
                            "text": "Jego <w>skory</w> umysł błyskawicznie znalazł rozwiązanie tego skomplikowanego problemu.",
                            "target_lang_translations": {
                                "ru": "Его <w>быстрый</w> ум молниеносно нашел решение этой сложной проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "szybki",
                        "prędki",
                        "chyży",
                        "rączy"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie rzadko używane w znaczeniu fizycznej prędkości"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "stylizowany lub dawny język"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигающийся с большой скоростью или действующий быстро (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быстрый",
                                "target_lang_sense_clarification": "проворный, совершаемый за короткое время"
                            },
                            {
                                "target_lang_word": "скорый",
                                "target_lang_sense_clarification": "прямое значение скорости (устар./книжн.)"
                            }
                        ]
                    }
                }
            ]
        },

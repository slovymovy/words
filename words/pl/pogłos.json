{
    "word_family": [
        "pogłosowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5de36e0a-cfa2-38d2-978f-c515dbfb9e9e",
                    "sense_definition": "Zjawisko akustyczne polegające na przedłużeniu trwania dźwięku w zamkniętym pomieszczeniu na skutek odbijania się fal dźwiękowych od przeszkód.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W pustych korytarzach czy wielkich salach <w>pogłos</w> jest zazwyczaj bardzo długi.",
                            "target_lang_translations": {
                                "ru": "В пустых коридорах или больших залах <w>эхо</w> обычно очень долгое."
                            }
                        },
                        {
                            "text": "Ich szepty niosły się wyraźnie po jaskini dzięki naturalnemu <w>pogłosowi</w>.",
                            "target_lang_translations": {
                                "ru": "Их шепот отчетливо разносился по пещере благодаря естественному <w>эху</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "echo",
                        "rezonans"
                    ],
                    "common_phrases": [
                        "duży pogłos",
                        "słyszeć pogłos",
                        "pogłos w sali"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акустическое явление, при котором звук продолжает звучать в закрытом помещении из-за отражения звуковых волн от препятствий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эхо",
                                "target_lang_sense_clarification": "отражение звука"
                            },
                            {
                                "target_lang_word": "отзвук",
                                "target_lang_sense_clarification": "остаточное звучание"
                            },
                            {
                                "target_lang_word": "реверберация",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7450f96a-1574-3835-8455-bf662701a971",
                    "sense_definition": "Sztuczny efekt stosowany w muzyce i nagraniach, imitujący naturalne zachowanie się dźwięku w przestrzeni.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W latach pięćdziesiątych studia nagraniowe wyposażane były w urządzenia do wytwarzania sztucznego <w>pogłosu</w>.",
                            "target_lang_translations": {
                                "ru": "В пятидесятые годы студии звукозаписи оснащались устройствами для создания искусственной <w>реверберации</w>."
                            }
                        },
                        {
                            "text": "Czasem w trakcie pracy nad miksem piosenki możemy zauważyć, że przesadziliśmy z <w>pogłosem</w>.",
                            "target_lang_translations": {
                                "ru": "Иногда в процессе сведения песни мы можем заметить, что переборщили с <w>реверберацией</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dodać pogłos",
                        "sztuczny pogłos",
                        "pogłos cyfrowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственный эффект в музыке и звукозаписи, имитирующий естественное отражение звука в пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реверберация",
                                "target_lang_sense_clarification": "звуковой эффект"
                            },
                            {
                                "target_lang_word": "эхо",
                                "target_lang_sense_clarification": "эффект повторения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fabfcb6-a1ee-3ec6-8dda-bd6c75c6f7c9",
                    "sense_definition": "Nawiązanie do wcześniejszych zdarzeń, ślad przeszłości lub oddźwięk, jaki wywołała jakaś sytuacja.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej starej legendzie odnajdujemy wyraźny <w>pogłos</w> dawnych wierzeń pogańskich.",
                            "target_lang_translations": {
                                "ru": "В этой старой легенде мы находим явный <w>отголосок</w> древних языческих верований."
                            }
                        },
                        {
                            "text": "Artykuł opublikowany w gazecie miał szeroki <w>pogłos</w> w środowisku politycznym.",
                            "target_lang_translations": {
                                "ru": "Статья, опубликованная в газете, имела широкий <w>резонанс</w> в политических кругах."
                            }
                        }
                    ],
                    "synonyms": [
                        "echo"
                    ],
                    "common_phrases": [
                        "pogłos wydarzeń",
                        "szeroki pogłos"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связь с прошлыми событиями, след прошлого или общественная реакция на какую-либо ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отголосок",
                                "target_lang_sense_clarification": "след прошлого"
                            },
                            {
                                "target_lang_word": "резонанс",
                                "target_lang_sense_clarification": "реакция общества"
                            },
                            {
                                "target_lang_word": "отклик",
                                "target_lang_sense_clarification": "ответная реакция"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
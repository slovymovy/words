                        "examples": [
                            {
                                "example_id": "fc28260e-a665-44e7-a2b5-8ba6f46c2097",
                                "sense_id": "2301a5c0-52b4-390a-910e-951beb423edb",
                                "text": "Looked at from RNA’s point of view, DNA is just a passive archive of information, a dull hunk of a telephone directory; it is RNA that looks up the numbers, establishes the connections and determines how long each call will last."
                            }
                        ]

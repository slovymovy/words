    "entries": [
        {
            "pos": "numeral",
            "senses": [
                {
                    "sense_id": "3b0ac031-c991-3b31-a201-c6a539beb414",
                    "sense_definition": "Nummer tien in een volgorde; wat na de negende komt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ordinal_number",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is al de <w>tiende</w> keer dat ik je probeer te bellen.",
                            "target_lang_translations": {
                                "ru": "Это уже <w>десятый</w> раз, когда я пытаюсь тебе дозвониться."
                            }
                        },
                        {
                            "text": "Zij woont op de <w>tiende</w> verdieping van dat hoge gebouw.",
                            "target_lang_translations": {
                                "ru": "Она живет на <w>десятом</w> этаже того высокого здания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de tiende keer",
                        "tiende eeuw",
                        "tiende verjaardag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Номер десять по порядку; следующий за девятым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "десятый",
                                "target_lang_sense_clarification": "порядковое числительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebd77069-bd64-38eb-8645-18d929dd3295",
                    "sense_definition": "Wat een van de tien gelijke delen vormt; tien procent.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "fraction_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Slechts een <w>tiende</w> deel van de bevolking stemde voor het voorstel.",
                            "target_lang_translations": {
                                "ru": "Лишь одна <w>десятая</w> часть населения проголосовала за это предложение."
                            }
                        },
                        {
                            "text": "Hij kreeg een <w>tiende</w> aandeel in de winst van het bedrijf.",
                            "target_lang_translations": {
                                "ru": "Он получил одну <w>десятую</w> долю в прибыли компании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het tiende deel",
                        "een tiende penning"
                    ],
                    "target_lang_definitions": {
                        "ru": "Составляющий одну из десяти равных частей; десять процентов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "десятая",
                                "target_lang_sense_clarification": "часть, доля"
                            }

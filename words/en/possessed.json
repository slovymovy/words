                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "владел",
                                "target_lang_sense_clarification": "имел в собственности"
                            },
                            {
                                "target_lang_word": "обладал",
                                "target_lang_sense_clarification": "имел качество или навык"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a7838b7e-e414-3dbe-a9dc-55e59d6c3215",

                        "ru": [
                            {
                                "target_lang_word": "на ногах",
                                "target_lang_sense_clarification": "бодрствующий"
                            },
                            {
                                "target_lang_word": "встал",
                                "target_lang_sense_clarification": "проснулся"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e3f42c2-0b18-338a-8cb3-bbb5fc064bd9",
                    "sense_definition": "Lichamelijk of geestelijk volledig uitgeput.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "toestand_moe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na die lange wandeling in de bergen was hij echt <w>op</w>.",
                            "target_lang_translations": {
                                "en": "After that long walk in the mountains, he was really <w>exhausted</w>.",
                                "ru": "После той долгой прогулки в горах он был действительно <w>без сил</w>."
                            }
                        },
                        {
                            "text": "Ze is helemaal <w>op</w> van de stress op haar werk.",
                            "target_lang_translations": {
                                "en": "She is completely <w>worn out</w> from the stress at work.",
                                "ru": "Она совершенно <w>вымотана</w> из-за стресса на работе."

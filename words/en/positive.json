                            "target_lang_translations": {
                                "ru": "Нам следует попытаться сосредоточиться на <w>положительных сторонах</w> ситуации."
                            }
                        },
                        {
                            "text": "One of the main <w>positives</w> of this job is the flexible schedule.",
                            "target_lang_translations": {
                                "ru": "Один из главных <w>плюсов</w> этой работы — гибкий график."
                            }
                        }
                    ],
                    "antonyms": [
                        "negative#Noun"
                    ],
                    "common_phrases": [
                        "positives and negatives",

                            },
                            {
                                "target_lang_word": "сражаться",
                                "target_lang_sense_clarification": "вести бой, биться с врагом"
                            },
                            {
                                "target_lang_word": "драться",
                                "target_lang_sense_clarification": "участвовать в физической потасовке"
                            }

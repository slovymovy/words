            "senses": [
                {
                    "sense_id": "67ff0ae7-1d83-308a-8444-e61ff48a4887",
                    "sense_definition": "Een persoon, meestal een kind, die ondeugend is en streken uithaalt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kleine <w>schabbernak</w> heeft voor de grap zout in de suikerpot gedaan.",
                            "target_lang_translations": {
                                "ru": "Маленький <w>проказник</w> ради шутки насыпал соль в сахарницу."
                            }
                        },
                        {
                            "text": "Mijn neefje is een echte <w>schabbernak</w> die de hele dag door het huis rent.",
                            "target_lang_translations": {
                                "ru": "Мой племянник — настоящий <w>сорванец</w>, который целый день носится по дому."
                            }
                        }
                    ],
                    "synonyms": [
                        "kwajongen",
                        "rekel",
                        "belhamel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обычно ребенок, который шалит и устраивает проделки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сорванец",
                                "target_lang_sense_clarification": "озорной, бойкий ребенок"
                            },
                            {
                                "target_lang_word": "проказник",
                                "target_lang_sense_clarification": "тот, кто любит шалить"
                            },
                            {
                                "target_lang_word": "шалун",
                                "target_lang_sense_clarification": "баловник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67fdf894-c58c-34c0-b009-90349c5ee042",
                    "sense_definition": "Iemand die er onverzorgd uitziet of die men niet vertrouwt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zou die vieze <w>schabbernak</w> nooit binnenlaten in mijn huis.",
                            "target_lang_translations": {
                                "ru": "Я бы никогда не пустил этого грязного <w>оборванца</w> к себе в дом."
                            }
                        },
                        {
                            "text": "Die <w>schabbernak</w> heeft me opgelicht met de verkoop van die kapotte fiets.",
                            "target_lang_translations": {
                                "ru": "Этот <w>прохвост</w> обманул меня, продав сломанный велосипед."
                            }
                        }
                    ],
                    "synonyms": [
                        "schobbejak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который выглядит неопрятно или не вызывает доверия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оборванец",
                                "target_lang_sense_clarification": "человек в грязной, рваной одежде"
                            },
                            {
                                "target_lang_word": "прохвост",
                                "target_lang_sense_clarification": "негодяй, плут"
                            },
                            {
                                "target_lang_word": "проходимец",
                                "target_lang_sense_clarification": "жулик, человек с темным прошлым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c2f77e3-8a29-3284-998a-f576fcb82924",
                    "sense_definition": "Een woning of gebouw dat in zeer slechte staat verkeert.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze wonen al jaren in dat tochtige <w>schabbernak</w> aan de rand van het bos.",
                            "target_lang_translations": {
                                "ru": "Они уже много лет живут в этой продуваемой <w>развалюхе</w> на опушке леса."
                            }
                        },
                        {
                            "text": "De gemeente heeft besloten dat het oude <w>schabbernak</w> gesloopt moet worden.",
                            "target_lang_translations": {
                                "ru": "Муниципалитет решил, что эту старую <w>лачугу</w> нужно снести."
                            }
                        }
                    ],
                    "synonyms": [
                        "bouwval",
                        "krot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жилье или здание, находящееся в очень плохом состоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развалюха",
                                "target_lang_sense_clarification": "ветхое строение"
                            },
                            {
                                "target_lang_word": "лачуга",
                                "target_lang_sense_clarification": "бедное, маленькое жилище"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c000b8f7-c08d-3f53-8514-5b52bc8a234c",
                    "sense_definition": "Kleding die er vreemd, oud of onverzorgd uitziet.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij liep in zijn bloot <w>schabbernak</w> door de regen naar huis.",
                            "target_lang_translations": {
                                "ru": "Он шел домой под дождем совершенно <w>голым</w>."
                            }
                        },
                        {
                            "text": "Trek dat oude <w>schabbernak</w> uit en doe iets netjes aan voor het feest.",
                            "target_lang_translations": {
                                "ru": "Сними это старое <w>тряпье</w> и надень что-нибудь приличное для вечеринки."
                            }
                        }
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2a84297f-2bb2-3f92-bbfc-d6e87a3b8f4f",
                    "sense_definition": "To cover a surface or object with a liquid colour.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to <w>paint</w> the kitchen walls yellow to make the room brighter.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>покрасить</w> стены кухни в желтый цвет, чтобы сделать комнату светлее."
                            }
                        },
                        {
                            "text": "He helped his father <w>paint</w> the fence in the backyard.",
                            "target_lang_translations": {
                                "ru": "Он помог отцу <w>покрасить</w> забор на заднем дворе."
                            }
                        }
                    ],
                    "synonyms": [
                        "face card",
                        "ink"
                    ],
                    "common_phrases": [
                        "paint the walls",
                        "paint over",
                        "freshly painted"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать поверхность или объект жидким красящим веществом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красить",
                                "target_lang_sense_clarification": "наносить краску"
                            },
                            {
                                "target_lang_word": "покрасить",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7f57930-b9a3-370d-9018-04dbf99ca1a1",
                    "sense_definition": "To create a picture or design using liquid colours.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist used oils to <w>paint</w> a beautiful portrait of the queen.",
                            "target_lang_translations": {
                                "ru": "Художник использовал масляные краски, чтобы <w>написать</w> прекрасный портрет королевы."
                            }
                        },
                        {
                            "text": "Children love to <w>paint</w> pictures of animals and flowers.",
                            "target_lang_translations": {
                                "ru": "Дети любят <w>рисовать</w> картинки с животными и цветами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "paint a picture",
                        "paint a portrait",
                        "paint a landscape"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать картину или рисунок, используя жидкие краски."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рисовать",
                                "target_lang_sense_clarification": "создавать изображение"
                            },
                            {
                                "target_lang_word": "писать",
                                "target_lang_sense_clarification": "создавать картину (о живописи)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84176956-239c-353b-8ad1-3e43bea87059",
                    "sense_definition": "To describe or portray someone or something in a particular way.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_depict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The news report <w>painted</w> a gloomy picture of the current economic situation.",
                            "target_lang_translations": {
                                "ru": "В новостном репортаже <w>обрисовали</w> мрачную картину текущей экономической ситуации."
                            }
                        },
                        {
                            "text": "She tried to <w>paint</w> her opponent as a liar during the debate.",
                            "target_lang_translations": {
                                "ru": "Она пыталась <w>выставить</w> своего оппонента лжецом во время дебатов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "paint a picture of",
                        "paint someone as"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывать или изображать кого-либо или что-либо определенным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "описывать",
                                "target_lang_sense_clarification": "рассказывать о чем-то"
                            },
                            {
                                "target_lang_word": "изображать",
                                "target_lang_sense_clarification": "представлять в каком-либо свете"
                            },
                            {
                                "target_lang_word": "рисовать",
                                "target_lang_sense_clarification": "перен. воссоздавать образы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "100c6822-b2c1-3693-b695-4ca12df5905f",
                    "sense_definition": "To draw or render an element on a computer screen within a graphical user interface.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The browser takes a few milliseconds to <w>paint</w> the complex layout onto the screen.",
                            "target_lang_translations": {
                                "ru": "Браузеру требуется несколько миллисекунд, чтобы <w>отрисовать</w> сложный макет на экране."
                            }
                        },
                        {
                            "text": "If the window is minimized, the operating system does not need to <w>paint</w> its contents.",
                            "target_lang_translations": {
                                "ru": "Если окно свернуто, операционной системе не нужно <w>прорисовывать</w> его содержимое."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рисовать или отображать элемент на экране компьютера в графическом интерфейсе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрисовывать",
                                "target_lang_sense_clarification": "комп. выводить на экран"
                            },
                            {
                                "target_lang_word": "прорисовывать",
                                "target_lang_sense_clarification": "отображать детали"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0db4092-6308-3bd2-94d4-5bcc9d2699c9",
                    "sense_definition": "To detect an object using radar or a laser beam.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_radar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The military radar was able to <w>paint</w> the target from miles away.",
                            "target_lang_translations": {
                                "ru": "Военный радар смог <w>подсветить</w> цель за много миль."
                            }
                        },
                        {
                            "text": "Once the system <w>paints</w> the object, it tracks its speed and direction.",
                            "target_lang_translations": {
                                "ru": "Как только система <w>захватывает</w> объект, она отслеживает его скорость и направление."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнаруживать объект с помощью радара или лазерного луча."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подсвечивать",
                                "target_lang_sense_clarification": "воен. лазером или радаром"
                            },
                            {
                                "target_lang_word": "захватывать",
                                "target_lang_sense_clarification": "цель"
                            }
                        ]
                    }
                }
            ]
        },
        {

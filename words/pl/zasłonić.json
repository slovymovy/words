        "przesłona"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ada1322d-1fa0-3ae1-95b5-ab5f809dc003",
                    "sense_definition": "Umieścić coś przed czymś lub kimś, aby to ukryć, uczynić niewidocznym lub zablokować dostęp światła.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "covering_blocking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <zasłonić> okna, ponieważ słońce świeci mi prosto w oczy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <завесить> окна, так как солнце светит мне прямо в глаза."
                            }
                        },
                        {
                            "text": "Wysoki budynek całkowicie <zasłonił> nam piękny widok na góry.",
                            "target_lang_translations": {
                                "ru": "Высокое здание полностью <заслонило> нам прекрасный вид на горы."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakryć",
                        "przesłonić",
                        "ukryć"
                    ],
                    "antonyms": [
                        "odsłonić",
                        "odkryć"
                    ],
                    "common_phrases": [
                        "zasłonić okno",
                        "zasłonić widok",
                        "zasłonić oczy",
                        "zasłonić zasłony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить что-то перед чем-то или кем-то, чтобы скрыть это, сделать невидимым или перекрыть доступ света."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заслонить",
                                "target_lang_sense_clarification": "перекрыть вид или свет"
                            },
                            {
                                "target_lang_word": "завесить",
                                "target_lang_sense_clarification": "закрыть тканью, шторами"
                            },
                            {
                                "target_lang_word": "закрыть",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f88487fa-6dd9-3972-b9f1-7a31844f163b",
                    "sense_definition": "Ochronić się lub ukryć, stawiając coś przed sobą lub zakrywając część ciała (wymaga użycia zaimka zwrotnego „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "self_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopiec odruchowo <zasłonił> się rękami, by uniknąć uderzenia piłką.",
                            "target_lang_translations": {
                                "ru": "Мальчик рефлекторно <закрылся> руками, чтобы избежать удара мячом."
                            }
                        },
                        {
                            "text": "Podejrzany <zasłonił> się niepamięcią i odmówił składania zeznań.",
                            "target_lang_translations": {
                                "ru": "Подозреваемый <прикрылся> потерей памяти и отказался давать показания."
                            }
                        }
                    ],
                    "synonyms": [
                        "osłonić się",
                        "zakryć się",
                        "schować się"
                    ],
                    "antonyms": [
                        "odsłonić się",
                        "wychylić się"
                    ],
                    "common_phrases": [
                        "zasłonić się ręką",
                        "zasłonić się przed ciosem",
                        "zasłonić się milczeniem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защитить себя или спрятаться, выставив что-то перед собой или прикрыв часть тела (требует возвратного местоимения)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикрыться",
                                "target_lang_sense_clarification": "использовать как защиту или оправдание"
                            },
                            {
                                "target_lang_word": "заслониться",
                                "target_lang_sense_clarification": "защититься от чего-то"
                            },
                            {
                                "target_lang_word": "закрыться",
                                "target_lang_sense_clarification": "спрятаться, оградиться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
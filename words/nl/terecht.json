    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a4c31da5-9576-38e4-83e1-454997b3e528",
                    "sense_definition": "Gebaseerd op de waarheid, feiten of een juist oordeel; rechtvaardig.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "justified_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar gaf de leerling een <w>terechte</w> waarschuwing voor zijn gedrag.",
                            "target_lang_translations": {
                                "en": "The teacher gave the student a <w>justified</w> warning for his behavior.",
                                "ru": "Учитель сделал ученику <w>справедливое</w> замечание за его поведение."
                            }
                        },
                        {
                            "text": "Het is volkomen <w>terecht</w> dat zij de wedstrijd heeft gewonnen.",
                            "target_lang_translations": {
                                "en": "It is completely <w>right</w> that she won the match.",
                                "ru": "Совершенно <w>справедливо</w>, что она выиграла соревнование."
                            }
                        }
                    ],
                    "synonyms": [
                        "gegrond",
                        "juist",
                        "rechtvaardig"
                    ],
                    "antonyms": [
                        "onterecht"
                    ],
                    "common_phrases": [
                        "een terechte vraag",
                        "volkomen terecht",
                        "een terechte opmerking"
                    ],
                    "target_lang_definitions": {
                        "en": "Based on truth, facts, or correct judgment; just.",
                        "ru": "Основанный на истине, фактах или правильном суждении; справедливый."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "justified",
                                "target_lang_sense_clarification": "well-founded or legitimate"
                            },
                            {
                                "target_lang_word": "right",
                                "target_lang_sense_clarification": "correct or fair"
                            },
                            {
                                "target_lang_word": "appropriate",
                                "target_lang_sense_clarification": "suitable for the situation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "справедливый",
                                "target_lang_sense_clarification": "честный, правильный"
                            },
                            {
                                "target_lang_word": "обоснованный",
                                "target_lang_sense_clarification": "подкрепленный фактами"
                            },
                            {
                                "target_lang_word": "заслуженный",
                                "target_lang_sense_clarification": "достойный награды или наказания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8a850997-4274-3982-bf25-dc0aad15cc51",
                    "sense_definition": "Op een rechtvaardige manier; met een goede reden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "justified_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij merkte <w>terecht</w> op dat we te laat waren vertrokken.",
                            "target_lang_translations": {
                                "en": "He <w>rightly</w> noted that we had left too late.",
                                "ru": "Он <w>справедливо</w> заметил, что мы выехали слишком поздно."
                            }
                        },
                        {
                            "text": "Ze was boos op haar broer, en <w>terecht</w>.",
                            "target_lang_translations": {
                                "en": "She was angry with her brother, and <w>rightly so</w>.",
                                "ru": "Она злилась на брата, и <w>справедливо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "met recht"
                    ],
                    "antonyms": [
                        "ten onrechte",
                        "onterecht"
                    ],
                    "common_phrases": [
                        "en terecht"
                    ],
                    "target_lang_definitions": {
                        "en": "In a fair way; with good reason.",
                        "ru": "Справедливым образом; имея на то веские причины."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rightly",
                                "target_lang_sense_clarification": "with good reason"
                            },
                            {
                                "target_lang_word": "justifiably",
                                "target_lang_sense_clarification": "in a manner that can be defended"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "справедливо",
                                "target_lang_sense_clarification": "правильно, по праву"
                            },
                            {
                                "target_lang_word": "заслуженно",
                                "target_lang_sense_clarification": "по заслугам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cab7dc5e-2bdd-3326-a4dd-849e6d7488af",
                    "sense_definition": "Weer gevonden of op de juiste plaats aanwezig.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gelukkig is mijn gestolen fiets weer <w>terecht</w>.",
                            "target_lang_translations": {
                                "en": "Fortunately, my stolen bike is <w>back</w>.",
                                "ru": "К счастью, мой украденный велосипед <w>нашелся</w>."
                            }
                        },
                        {
                            "text": "Weet jij of de vermiste kat al <w>terecht</w> is?",
                            "target_lang_translations": {
                                "en": "Do you know if the missing cat has been <w>found</w> yet?",
                                "ru": "Ты не знаешь, пропавшая кошка уже <w>нашлась</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "teruggevonden"
                    ],
                    "common_phrases": [
                        "weer terecht zijn",
                        "niet terecht zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "Found again or present at the correct place.",
                        "ru": "Найденный снова или находящийся в нужном месте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "found",
                                "target_lang_sense_clarification": "discovered after being lost"
                            },
                            {
                                "target_lang_word": "back",
                                "target_lang_sense_clarification": "returned to the original place"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "найтись",
                                "target_lang_sense_clarification": "глагол, используемый для перевода состояния 'terecht zijn'"
                            },
                            {
                                "target_lang_word": "найденный",
                                "target_lang_sense_clarification": "обнаруженный после пропажи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05fe4b0d-9330-3623-8457-99467b86948c",
                    "sense_definition": "Deel van een scheidbaar werkwoord dat een bestemming, resultaat of mogelijkheid aangeeft (zoals in terechtkomen of terechtkunnen).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verbal_particle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik weet niet waar ik met mijn klachten <w>terecht</w> kan.",
                            "target_lang_translations": {
                                "en": "I don't know where I can <w>turn</w> with my complaints.",
                                "ru": "Я не знаю, куда я могу <w>обратиться</w> со своими жалобами."
                            }
                        },
                        {
                            "text": "Uiteindelijk is hij in een klein dorpje <w>terecht</w> gekomen.",
                            "target_lang_translations": {
                                "en": "Eventually he <w>ended up</w> in a small village.",
                                "ru": "В конце концов он <w>очутился</w> в маленькой деревушке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "terecht kunnen",
                        "terecht komen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "part of separable verbs"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Part of a separable verb indicating a destination, result, or possibility (as in 'end up' or 'turn to').",
                        "ru": "Часть отделяемого глагола, указывающая на место назначения, результат или возможность."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "end up",
                                "target_lang_sense_clarification": "indicating the result or final location"
                            },
                            {
                                "target_lang_word": "turn to",
                                "target_lang_sense_clarification": "indicating where one can go for help"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обратиться",
                                "target_lang_sense_clarification": "в контексте 'terecht kunnen' (к кому-то)"
                            },
                            {
                                "target_lang_word": "очутиться",
                                "target_lang_sense_clarification": "в контексте 'terecht komen' (попасть куда-то)"
                            },
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "оказаться в каком-то месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90270d25-81c0-3646-bb56-1b7de2631d87",
                    "sense_definition": "Voor de rechter verschijnen om beoordeeld te worden (altijd in combinatie met 'staan').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte moest gisteren <w>terecht</w> staan voor fraude.",
                            "target_lang_translations": {
                                "en": "The suspect had to stand <w>trial</w> for fraud yesterday.",
                                "ru": "Подозреваемый должен был вчера <w>предстать перед судом</w> за мошенничество."
                            }
                        },
                        {
                            "text": "Vele oorlogsmisdadigers stonden jaren later alsnog <w>terecht</w>.",
                            "target_lang_translations": {
                                "en": "Many war criminals still stood <w>trial</w> years later.",
                                "ru": "Многие военные преступники годы спустя все же <w>предстали перед судом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "terecht staan"
                    ],
                    "target_lang_definitions": {
                        "en": "To appear before a judge to be judged (always combined with 'staan').",
                        "ru": "Предстать перед судьей для вынесения приговора."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand trial",
                                "target_lang_sense_clarification": "legal proceeding"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "предстать перед судом",
                                "target_lang_sense_clarification": "судиться, отвечать перед законом"
                            }
                        ]
                    }
                }
            ]
        }
    ]

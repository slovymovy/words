    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f06beed3-07af-3e48-bc01-e997d56e8d36",
                    "sense_definition": "A place where goods are bought and sold, such as a store, market, or bazaar.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "trade_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We stopped at the local <w>mart</w> to pick up some milk and bread.",
                            "target_lang_translations": {
                                "ru": "Мы остановились у местного <w>магазина</w>, чтобы купить молока и хлеба."
                            }
                        },
                        {
                            "text": "Farmers from all over the county brought their sheep to the cattle <w>mart</w>.",
                            "target_lang_translations": {
                                "ru": "Фермеры со всего округа привезли своих овец на скотный <w>рынок</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "market",
                        "store",
                        "shop",
                        "bazaar",
                        "marketplace"
                    ],
                    "common_phrases": [
                        "cattle mart",
                        "food mart",
                        "mini-mart",
                        "trade mart"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где покупаются и продаются товары, например, магазин, рынок или базар."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рынок",
                                "target_lang_sense_clarification": "место торговли"
                            },
                            {
                                "target_lang_word": "магазин",
                                "target_lang_sense_clarification": "розничная торговля"
                            },
                            {
                                "target_lang_word": "торговый центр",
                                "target_lang_sense_clarification": "крупный комплекс"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "457c0984-32ae-3e3f-9185-6a4382800e23",
                    "sense_definition": "A diminutive of the male given name <w>Martin</w>.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "I asked <w>Mart</w> if he wanted to join us for dinner tonight.",
                            "target_lang_translations": {
                                "ru": "Я спросил <w>Марта</w>, не хочет ли он поужинать с нами сегодня."
                            }
                        },
                        {
                            "text": "Everyone calls him <w>Mart</w>, even though his full name is Martin.",
                            "target_lang_translations": {
                                "ru": "Все зовут его <w>Март</w>, хотя его полное имя — Мартин."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short form of a name"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма мужского имени Мартин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Март",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5b2ee6d2-b5a8-3fe4-8cb3-14eddbc36771",
                    "sense_definition": "To buy, sell, or trade goods.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "trade_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the old days, merchants would travel to the city to <w>mart</w> their wares.",
                            "target_lang_translations": {
                                "ru": "В старину купцы ездили в город, чтобы <w>торговать</w> своими товарами."
                            }
                        },
                        {
                            "text": "He sought to <w>mart</w> his produce for a fair price at the fair.",
                            "target_lang_translations": {
                                "ru": "Он стремился <w>продать</w> свою продукцию по справедливой цене на ярмарке."
                            }
                        }
                    ],
                    "synonyms": [
                        "trade",
                        "traffic",
                        "sell",
                        "merchandise"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rarely used in modern English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покупать, продавать или обменивать товары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торговать",
                                "target_lang_sense_clarification": "вести торговлю"
                            },
                            {
                                "target_lang_word": "продавать",
                                "target_lang_sense_clarification": "сбывать товар"
                            }
                        ]
                    }
                }
            ]
        }
    ]

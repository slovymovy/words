                    "sense_id": "e7401b5e-0142-31a2-b29e-9307cff2cd84",
                    "sense_definition": "Voorzien van nieuwe elektrische energie, zoals bij een batterij of accu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "electricity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet even wachten tot mijn telefoon volledig is <w>opgeladen</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно немного подождать, пока мой телефон полностью <w>зарядится</w>."
                            }
                        },
                        {
                            "text": "Je kunt de batterij van deze camera eenvoudig via een USB-kabel <w>opladen</w>.",
                            "target_lang_translations": {
                                "ru": "Вы можете легко <w>зарядить</w> аккумулятор этой камеры через USB-кабель."
                            }
                        }
                    ],
                    "antonyms": [
                        "ontladen"
                    ],
                    "common_phrases": [

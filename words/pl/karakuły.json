{
    "word_family": [
        "karakuł",
        "karakułowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "356120db-38aa-33bf-b332-96f025cd92c9",
                    "sense_definition": "Futro wykonane ze skór jagniąt rasy karakuł, charakteryzujące się gęstym, skręconym włosiem, lub uszyte z niego okrycie wierzchnie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_materials",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia zawsze zakładała swoje eleganckie <w>karakuły</w> na ważne uroczystości rodzinne.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка всегда надевала свой элегантный <w>каракуль</w> на важные семейные торжества."
                            }
                        },
                        {
                            "text": "W szafie wisiały stare, czarne <w>karakuły</w>, które pamiętały jeszcze czasy przedwojenne.",
                            "target_lang_translations": {
                                "ru": "В шкафу висел старый черный <w>каракуль</w>, который помнил еще довоенные времена."
                            }
                        }
                    ],
                    "synonyms": [
                        "futro karakułowe"
                    ],
                    "common_phrases": [
                        "czarne karakuły",
                        "czapka z karakułów",
                        "kołnierz z karakułów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мех, изготовленный из шкур ягнят каракульской породы, отличающийся густым, завитком ворсом, или верхняя одежда из этого меха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каракуль",
                                "target_lang_sense_clarification": "мех"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e2f55d7-275b-38e4-bc7d-162a45400a01",
                    "sense_definition": "Forma liczby mnogiej od rzeczownika <w>karakuł</w> (rasa owcy).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na rozległych stepach Azji Środkowej pasły się liczne <w>karakuły</w>.",
                            "target_lang_translations": {
                                "ru": "На обширных степях Средней Азии паслись многочисленные <w>каракульские овцы</w>."
                            }
                        },
                        {
                            "text": "Miejscowi rolnicy hodują <w>karakuły</w> głównie dla ich cennych skór i wełny.",
                            "target_lang_translations": {
                                "ru": "Местные фермеры разводят <w>каракульских овец</w> в основном ради их ценных шкур и шерсти."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stado karakułów",
                        "hodowla karakułów"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Овцы каракульской породы (множественное число)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каракульские овцы",
                                "target_lang_sense_clarification": "животные"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
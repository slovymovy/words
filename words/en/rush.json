{
    "word_family": [
        "rusher",
        "rushy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7f74a65f-f13a-3793-99d7-a2531bf3c110",
                    "sense_definition": "To hurry; to perform a task with great haste, often not properly or without thinking carefully.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_hurry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not <w>rush</w> your breakfast; we have plenty of time.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>торопись</w> с завтраком, у нас куча времени."
                            }
                        },
                        {
                            "text": "She <w>rushed</w> to catch the bus before it left the station.",
                            "target_lang_translations": {
                                "ru": "Она <w>поспешила</w>, чтобы успеть на автобус, пока он не уехал со станции."
                            }
                        }
                    ],
                    "synonyms": [
                        "hurry",
                        "hasten",
                        "make haste",
                        "speed",
                        "bustle",
                        "hie"
                    ],
                    "antonyms": [
                        "drag"
                    ],
                    "common_phrases": [
                        "rush off",
                        "rush into",
                        "rush to judgment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торопиться; выполнять задачу с большой поспешностью, часто небрежно или необдуманно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торопиться",
                                "target_lang_sense_clarification": "делать что-либо быстрее, чем следует"
                            },
                            {
                                "target_lang_word": "спешить",
                                "target_lang_sense_clarification": "стараться успеть, действовать быстро"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e73ef8a2-029c-3cd9-96b3-e6b90172c2b7",
                    "sense_definition": "To flow or move forward rapidly or noisily.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_move_fast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The floodwaters <w>rushed</w> through the narrow valley.",
                            "target_lang_translations": {
                                "ru": "Паводковые воды <w>хлынули</w> через узкую долину."
                            }
                        },
                        {
                            "text": "A cold wind <w>rushed</w> through the open window.",
                            "target_lang_translations": {
                                "ru": "Холодный ветер <w>ворвался</w> в открытое окно."
                            }
                        }
                    ],
                    "synonyms": [
                        "fly",
                        "zoom",
                        "hurtle",
                        "speed",
                        "zip"
                    ],
                    "common_phrases": [
                        "rush by",
                        "rush in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стремительно течь или двигаться вперед, часто с шумом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хлынуть",
                                "target_lang_sense_clarification": "о жидкости: потечь сильным потоком"
                            },
                            {
                                "target_lang_word": "нестись",
                                "target_lang_sense_clarification": "о ветре или потоке: быстро двигаться"
                            },
                            {
                                "target_lang_word": "мчаться",
                                "target_lang_sense_clarification": "очень быстро двигаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfab3dad-1460-3a83-b2b0-60ecf92f243f",
                    "sense_definition": "To cause to move or act with unusual haste.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_hurry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctors <w>rushed</w> the patient to the emergency room.",
                            "target_lang_translations": {
                                "ru": "Врачи экстренно <w>доставили</w> пациента в реанимацию."
                            }
                        },
                        {
                            "text": "Don't <w>rush</w> me while I am trying to think.",
                            "target_lang_translations": {
                                "ru": "Не <w>торопи</w> меня, пока я пытаюсь думать."
                            }
                        }
                    ],
                    "synonyms": [
                        "hustle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять двигаться или действовать с необычной поспешностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торопить",
                                "target_lang_sense_clarification": "подгонять кого-либо"
                            },
                            {
                                "target_lang_word": "подгонять",
                                "target_lang_sense_clarification": "заставлять делать быстрее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05a9f668-8d05-33e8-9395-7acc65b1e312",
                    "sense_definition": "To make a swift or sudden attack.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_attack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers decided to <w>rush</w> the enemy position at dawn.",
                            "target_lang_translations": {
                                "ru": "Солдаты решили <w>штурмовать</w> вражескую позицию на рассвете."
                            }
                        },
                        {
                            "text": "Security guards <w>rushed</w> the intruder before he could reach the stage.",
                            "target_lang_translations": {
                                "ru": "Охранники <w>набросились</w> на нарушителя, прежде чем он успел добраться до сцены."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершать быструю или внезапную атаку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бросаться",
                                "target_lang_sense_clarification": "стремительно нападать"
                            },
                            {
                                "target_lang_word": "штурмовать",
                                "target_lang_sense_clarification": "идти на приступ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8e69008-b6db-335b-8e1d-2aa68a9f4a06",
                    "sense_definition": "To attack an opponent with a large swarm of units.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_attack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player decided to <w>rush</w> the opponent's base early in the game.",
                            "target_lang_translations": {
                                "ru": "Игрок решил <w>зарашить</w> базу противника в начале игры."
                            }
                        },
                        {
                            "text": "If you <w>rush</w> them now, they won't have time to build defenses.",
                            "target_lang_translations": {
                                "ru": "Если ты <w>атакуешь</w> их сейчас, у них не будет времени построить оборону."
                            }
                        }
                    ],
                    "synonyms": [
                        "zerg"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gaming"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Атаковать противника большим количеством юнитов (в играх)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рашить",
                                "target_lang_sense_clarification": "игровой сленг: быстро атаковать"
                            },
                            {
                                "target_lang_word": "атаковать массой",
                                "target_lang_sense_clarification": "нападать большим числом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e5f0b5fa-40fc-3c8f-ac17-128ad0e2ad2d",
                    "sense_definition": "General haste or urgency.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_haste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am in a <w>rush</w> to get to the meeting on time.",
                            "target_lang_translations": {
                                "ru": "Я <w>спешу</w>, чтобы попасть на встречу вовремя."
                            }
                        },
                        {
                            "text": "There is no <w>rush</w>, so please take your time.",
                            "target_lang_translations": {
                                "ru": "Никакой <w>спешки</w> нет, так что, пожалуйста, не торопитесь."
                            }
                        }
                    ],
                    "synonyms": [
                        "hurry",
                        "bustle"
                    ],
                    "common_phrases": [
                        "in a rush",
                        "what's the rush",
                        "mad rush"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общая поспешность или срочность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спешка",
                                "target_lang_sense_clarification": "торопливость в действиях"
                            },
                            {
                                "target_lang_word": "суматоха",
                                "target_lang_sense_clarification": "беспорядочная поспешность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06575847-450e-3671-ad46-09a9cba57bdc",
                    "sense_definition": "A sudden forward motion or surge.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He made a sudden <w>rush</w> for the door when the alarm sounded.",
                            "target_lang_translations": {
                                "ru": "Он сделал резкий <w>рывок</w> к двери, когда зазвучала сигнализация."
                            }
                        },
                        {
                            "text": "We experienced a <w>rush</w> of customers during the lunch hour.",
                            "target_lang_translations": {
                                "ru": "В обеденный перерыв у нас был <w>наплыв</w> посетителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "flush"
                    ],
                    "common_phrases": [
                        "rush of blood",
                        "gold rush"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное движение вперед или наплыв."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наплыв",
                                "target_lang_sense_clarification": "большое скопление людей"
                            },
                            {
                                "target_lang_word": "рывок",
                                "target_lang_sense_clarification": "резкое движение"
                            },
                            {
                                "target_lang_word": "порыв",
                                "target_lang_sense_clarification": "резкое движение воздуха или чувства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b151fd6-f867-3dda-a88e-bf537b053583",
                    "sense_definition": "A sudden, brief exhilaration, for instance the pleasurable sensation produced by a stimulant or excitement.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Riding the roller coaster gave me an incredible <w>rush</w>.",
                            "target_lang_translations": {
                                "ru": "Катание на американских горках вызвало у меня невероятный <w>прилив адреналина</w>."
                            }
                        },
                        {
                            "text": "He felt a <w>rush</w> of adrenaline as he stepped onto the stage.",
                            "target_lang_translations": {
                                "ru": "Он почувствовал <w>прилив</w> адреналина, выходя на сцену."
                            }
                        }
                    ],
                    "common_phrases": [
                        "adrenaline rush",
                        "sugar rush"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное, кратковременное возбуждение или прилив сил."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прилив",
                                "target_lang_sense_clarification": "резкое ощущение энергии или эмоций"
                            },
                            {
                                "target_lang_word": "выброс",
                                "target_lang_sense_clarification": "например, адреналина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15022777-94e1-34b0-8bf9-cc17f13c3771",
                    "sense_definition": "Any of several stiff plants of the genus Juncus, growing in marshes or near water.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The basket was woven from dried <w>rushes</w> gathered by the river.",
                            "target_lang_translations": {
                                "ru": "Корзина была сплетена из сушеного <w>камыша</w>, собранного у реки."
                            }
                        },
                        {
                            "text": "We walked through the tall <w>rushes</w> along the muddy bank.",
                            "target_lang_translations": {
                                "ru": "Мы шли сквозь высокие <w>камыши</w> вдоль илистого берега."
                            }
                        }
                    ],
                    "synonyms": [
                        "juncus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жесткие растения рода Juncus, растущие на болотах или у воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "камыш",
                                "target_lang_sense_clarification": "общеупотребительное название"
                            },
                            {
                                "target_lang_word": "тростник",
                                "target_lang_sense_clarification": "высокое водное растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c9c04d6-2188-311a-a7e7-bcc8e20a7729",
                    "sense_definition": "A regulated period of recruitment in fraternities and sororities.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_fraternity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to join a sorority during <w>rush</w> week.",
                            "target_lang_translations": {
                                "ru": "Она решила вступить в женское общество во время недели <w>набора</w>."
                            }
                        },
                        {
                            "text": "The university campus was busy with events for fraternity <w>rush</w>.",
                            "target_lang_translations": {
                                "ru": "Университетский кампус был занят мероприятиями по <w>вербовке</w> в братства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rush week"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Период набора новых членов в студенческие братства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набор",
                                "target_lang_sense_clarification": "прием новых членов"
                            },
                            {
                                "target_lang_word": "вербовка",
                                "target_lang_sense_clarification": "привлечение в организацию"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "46e5857a-4501-38e2-bb93-5f81afce0a2c",
                    "sense_definition": "Performed with, or requiring urgency or great haste.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_urgent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This is a <w>rush</w> order, so please prioritize it above everything else.",
                            "target_lang_translations": {
                                "ru": "Это <w>срочный</w> заказ, поэтому, пожалуйста, поставьте его в приоритет."
                            }
                        },
                        {
                            "text": "We cannot afford to do a <w>rush</w> job on such an important project.",
                            "target_lang_translations": {
                                "ru": "Мы не можем позволить себе <w>халтурить</w> (делать работу в спешке) над таким важным проектом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rush job",
                        "rush hour",
                        "rush order"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполняемый в спешке или требующий срочности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срочный",
                                "target_lang_sense_clarification": "требующий немедленного исполнения"
                            },
                            {
                                "target_lang_word": "спешный",
                                "target_lang_sense_clarification": "делаемый в торопях"
                            }
                        ]
                    }
                }
            ]
        },

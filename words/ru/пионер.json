                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Конные <пионеры> быстро построили переправу через реку для основной армии.",
                            "target_lang_translations": {
                                "en": "Mounted <pioneers> quickly built a crossing over the river for the main army."
                            }

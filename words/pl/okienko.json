    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ca96777e-96fb-3ab0-bb42-f13158b88994",
                    "sense_definition": "Zdrobnienie od <w>okno</w>; małe okno.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_window",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez małe <w>okienko</w> w piwnicy wpadało niewiele światła.",
                            "target_lang_translations": {
                                "ru": "Через маленькое <w>окошко</w> в подвале попадало мало света."
                            }
                        },
                        {
                            "text": "Dziecko narysowało domek, który miał tylko jedno <w>okienko</w>.",
                            "target_lang_translations": {
                                "ru": "Ребенок нарисовал домик, у которого было только одно <w>окошко</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "małe okienko",
                        "otworzyć okienko",
                        "okienko piwniczne"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова <w>okno</w>; маленькое окно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окошко",
                                "target_lang_sense_clarification": "маленькое окно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e145dc6-9ef4-39b6-9eb8-7925e8e0e995",
                    "sense_definition": "Miejsce, stoisko lub przeszklony otwór w urzędzie, banku lub na poczcie, przy którym obsługiwani są klienci.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "service_point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znów musiałem stać godzinę na poczcie, bo tylko jedno <w>okienko</w> było otwarte.",
                            "target_lang_translations": {
                                "ru": "Мне снова пришлось час стоять на почте, потому что было открыто только одно <w>окошко</w>."
                            }
                        },
                        {
                            "text": "Proszę podejść do <w>okienka</w> numer trzy, aby odebrać paczkę.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подойдите к <w>окошку</w> номер три, чтобы забрать посылку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "podejść do okienka",
                        "czynne okienko",
                        "okienko kasowe",
                        "praca przy okienku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, стойка или застекленный проем в учреждении, банке или на почте, где обслуживают клиентов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окошко",
                                "target_lang_sense_clarification": "в учреждении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f6e2508-5adb-3d27-8e52-5d8fb8682687",
                    "sense_definition": "Wolny czas między zajęciami, lekcjami lub wykładami w planie dnia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_gap",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W poniedziałek mam <w>okienko</w> po fizyce, potem mam jeszcze dwie godziny matematyki.",
                            "target_lang_translations": {
                                "ru": "В понедельник у меня <w>окно</w> после физики, потом еще два часа математики."
                            }
                        },
                        {
                            "text": "Studenci poszli na kawę, ponieważ mieli dwugodzinne <w>okienko</w>.",
                            "target_lang_translations": {
                                "ru": "Студенты пошли на кофе, так как у них было двухчасовое <w>окно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wolna godzina"
                    ],
                    "common_phrases": [
                        "mieć okienko",
                        "okienko w planie",
                        "długie okienko"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "school and university context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободное время между занятиями, уроками или лекциями в расписании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окно",
                                "target_lang_sense_clarification": "в расписании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "536c9999-4316-3ae3-be27-bedcea73c097",
                    "sense_definition": "Przezroczysty fragment koperty, przez który widać adres odbiorcy.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_window",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz tak złożyć list, aby adres był widoczny przez <w>okienko</w>.",
                            "target_lang_translations": {
                                "ru": "Ты должен так сложить письмо, чтобы адрес был виден через <w>окошко</w>."
                            }
                        },
                        {
                            "text": "W biurze używamy zazwyczaj kopert z prawym <w>okienkiem</w>.",
                            "target_lang_translations": {
                                "ru": "В офисе мы обычно используем конверты с правым <w>окошком</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "koperta z okienkiem",
                        "adres w okienku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прозрачная часть конверта, через которую виден адрес получателя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окошко",
                                "target_lang_sense_clarification": "в конверте"

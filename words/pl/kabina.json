            "senses": [
                {
                    "sense_id": "413fe78e-8b99-3117-8260-bc8e114d934b",
                    "sense_definition": "Niewielkie, wydzielone pomieszczenie lub wnęka przeznaczona do wykonywania określonej czynności, np. kąpieli, przebierania się lub rozmowy telefonicznej",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "utility_room",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W łazience znajduje się nowoczesna <kabina> prysznicowa z hydromasażem."
                        },
                        {
                            "text": "Wszedłem do <kabiny>, żeby przymierzyć spodnie, które wybrałem w sklepie."
                        }
                    ],
                    "common_phrases": [
                        "kabina prysznicowa",
                        "kabina telefoniczna",
                        "kabina do głosowania",
                        "kabina przymierzalni"
                    ]
                },
                {

                },
                {
                    "sense_id": "bd28035b-cb26-33fe-b70c-c29ee5bff9e0",
                    "sense_definition": "The young of certain animals, such as the fox, beaver, or ferret.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A fox <w>kit</w> waited near the den for its mother to return.",
                            "target_lang_translations": {
                                "ru": "Лисий <w>детёныш</w> ждал у норы возвращения матери."
                            }
                        },
                        {
                            "text": "The beaver <w>kit</w> is learning how to swim.",
                            "target_lang_translations": {
                                "ru": "<w>Детёныш</w> бобра учится плавать."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Детёныш определенных животных, таких как лиса, бобр или хорек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детёныш",
                                "target_lang_sense_clarification": "животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4761fc77-57df-3551-af49-c8256d69ab23",
                    "sense_definition": "Clothing or personal effects.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He took his <w>kit</w> off and jumped into the pool.",
                            "target_lang_translations": {
                                "ru": "Он снял с себя <w>одежду</w> и прыгнул в бассейн."
                            }
                        },
                        {
                            "text": "Get your <w>kit</w> on, we are leaving in five minutes.",
                            "target_lang_translations": {
                                "ru": "Надевай свои <w>шмотки</w>, мы уходим через пять минут."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get your kit off"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }

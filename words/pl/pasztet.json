                {
                    "sense_id": "58253c67-833a-3579-a0e9-6d5ab4322630",
                    "sense_definition": "Bardzo brzydka osoba, najczęściej dziewczyna lub kobieta.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To bardzo niekulturalne nazywać jakąkolwiek dziewczynę <w>pasztetem</w>.",
                            "target_lang_translations": {
                                "ru": "Очень некультурно называть любую девушку <w>уродиной</w>."
                            }

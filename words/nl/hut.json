{
    "word_family": [
        "blokhut",
        "berghut",
        "trekkershut",
        "strandhut"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "af84a5eb-c35f-3c63-b4f9-b845c298ecd6",
                    "sense_definition": "Een eenvoudige, vaak kleine en primitieve woning of schuilplaats, meestal gemaakt van hout of andere natuurlijke materialen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "dwelling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen spelen buiten en bouwen een <w>hut</w> van takken.",
                            "target_lang_translations": {
                                "ru": "Дети играют на улице и строят <w>шалаш</w> из веток."
                            }
                        },
                        {
                            "text": "Hij woonde jarenlang in een klein, houten <w>hutje</w> in het bos.",
                            "target_lang_translations": {
                                "ru": "Он много лет жил в маленькой деревянной <w>хижине</w> в лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "kot",
                        "hok"
                    ],
                    "antonyms": [
                        "paleis",
                        "villa"
                    ],
                    "common_phrases": [
                        "een hut bouwen",
                        "een houten hut",
                        "hutje op de hei"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простая, часто небольшая и примитивная постройка или укрытие, обычно сделанная из дерева или природных материалов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хижина",
                                "target_lang_sense_clarification": "примитивное жилище"
                            },
                            {
                                "target_lang_word": "шалаш",
                                "target_lang_sense_clarification": "укрытие из веток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5ab81b6-2d74-393e-898e-3e23edac4baa",
                    "sense_definition": "Een kamer of slaapverblijf voor passagiers of bemanning aan boord van een schip.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ship_room",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de cruise sliepen we in een comfortabele <w>hut</w> met uitzicht op zee.",
                            "target_lang_translations": {
                                "ru": "Во время круиза мы спали в комфортабельной <w>каюте</w> с видом на море."
                            }
                        },
                        {
                            "text": "De kapitein ging naar zijn <w>hut</w> om de kaart te bestuderen.",
                            "target_lang_translations": {
                                "ru": "Капитан пошел в свою <w>каюту</w>, чтобы изучить карту."
                            }
                        }
                    ],
                    "synonyms": [
                        "cabine"
                    ],
                    "common_phrases": [
                        "in de hut",
                        "een hut boeken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Комната или спальное место для пассажиров или экипажа на борту корабля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каюта",
                                "target_lang_sense_clarification": "на корабле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9eb545ea-e779-3ae3-9f03-a53db12d06eb",
                    "sense_definition": "Een eenvoudig verblijf in de bergen of natuur waar wandelaars en klimmers kunnen overnachten of schuilen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "recreation_lodging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een lange wandeling kwamen we eindelijk aan bij de <w>hut</w>.",
                            "target_lang_translations": {
                                "ru": "После долгой прогулки мы наконец добрались до <w>хижины</w>."
                            }
                        },
                        {
                            "text": "In de Alpen trekken veel toeristen in de zomer van <w>hut</w> naar <w>hut</w>.",
                            "target_lang_translations": {
                                "ru": "Летом в Альпах многие туристы переходят от <w>хижины</w> к <w>хижине</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een hut in de bergen",
                        "van hut naar hut"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простой дом в горах или на природе, где туристы и альпинисты могут переночевать или укрыться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хижина",
                                "target_lang_sense_clarification": "в горах"
                            },
                            {
                                "target_lang_word": "горный приют",
                                "target_lang_sense_clarification": "место для ночлега туристов"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "gold",
        "golden",
        "gild",
        "gilder",
        "gilding"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "957262b1-48f4-3fe6-ac2b-6b69c9ffd59c",
                    "sense_definition": "To make something appear golden or bright like gold.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "make_golden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rising sun began to <w>engolden</w> the peaks of the distant mountains.",
                            "target_lang_translations": {
                                "ru": "Восходящее солнце начало <w>золотить</w> вершины далеких гор."
                            }
                        },
                        {
                            "text": "As evening approached, the light served to <w>engolden</w> the horizon with a warm glow.",
                            "target_lang_translations": {
                                "ru": "С приближением вечера свет начал <w>озарять золотом</w> горизонт теплым сиянием."
                            }
                        }
                    ],
                    "synonyms": [
                        "gild",
                        "brighten",
                        "illuminate"
                    ],
                    "antonyms": [
                        "darken",
                        "tarnish"
                    ],
                    "common_phrases": [
                        "engolden the horizon",
                        "engolden the clouds",
                        "engolden the sky"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо золотым или сияющим, подобно золоту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "золотить",
                                "target_lang_sense_clarification": "придавать золотой оттенок или блеск"
                            },
                            {
                                "target_lang_word": "озарять золотом",
                                "target_lang_sense_clarification": "освещать золотистым светом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c481f36-5705-3bd1-a8bc-7ef6e514e9ba",
                    "sense_definition": "To improve the quality or value of something; to make better.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "improve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His wise words seemed to <w>engolden</w> the spirit of the weary travelers.",
                            "target_lang_translations": {
                                "ru": "Его мудрые слова, казалось, <w>обогатили</w> дух усталых путников."
                            }
                        },
                        {
                            "text": "She hoped that the new policy would <w>engolden</w> the community's future prospects.",
                            "target_lang_translations": {
                                "ru": "Она надеялась, что новая политика <w>улучшит</w> будущие перспективы сообщества."
                            }
                        }
                    ],
                    "synonyms": [
                        "enrich",
                        "enhance",
                        "ameliorate"
                    ],
                    "antonyms": [
                        "worsen",
                        "degrade"
                    ],
                    "common_phrases": [
                        "engolden the spirit",
                        "engolden the mind",
                        "engolden the moment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Улучшать качество или ценность чего-либо; делать лучше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обогащать",
                                "target_lang_sense_clarification": "делать богаче содержанием или ценностью"
                            },
                            {
                                "target_lang_word": "облагораживать",
                                "target_lang_sense_clarification": "делать более возвышенным или качественным"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
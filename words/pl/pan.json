        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "c381008d-18c1-353d-b094-2805a3fb3b01",
                    "sense_definition": "Zwrot grzecznościowy zastępujący zaimek osobowy drugiej osoby („ty”) przy zwracaniu się do mężczyzny; gramatycznie łączy się z czasownikiem w trzeciej osobie liczby pojedynczej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy może <w>pan</w> otworzyć okno?",
                            "target_lang_translations": {
                                "ru": "Не могли бы <w>вы</w> открыть окно?"
                            }
                        },
                        {
                            "text": "Gdzie <w>pan</w> kupił tę gazetę?",
                            "target_lang_translations": {
                                "ru": "Где <w>вы</w> купили эту газету?"
                            }
                        },
                        {
                            "text": "Przepraszam, czy to <w>pana</w> parasol?",
                            "target_lang_translations": {
                                "ru": "Извините, это <w>ваш</w> зонт?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "proszę pana",
                        "czy pan",
                        "niech pan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливое обращение, заменяющее местоимение «ты» при обращении к мужчине; грамматически сочетается с глаголом в третьем лице единственного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вы",
                                "target_lang_sense_clarification": "вежливое обращение"
                            },
                            {
                                "target_lang_word": "господин",
                                "target_lang_sense_clarification": "формальное обращение"
                            }
                        ]
                    }
                }
            ]
        },
        {

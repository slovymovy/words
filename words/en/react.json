    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "545f8a12-7197-3bc8-9850-f31027d7d241",
                    "sense_definition": "To act in a particular way as a direct result of something else.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "How did she <w>react</w> to the news about the promotion?",
                            "target_lang_translations": {
                                "ru": "Как она <w>отреагировала</w> на новость о повышении?"
                            }
                        },
                        {
                            "text": "The market <w>reacted</w> positively to the sudden announcement.",
                            "target_lang_translations": {
                                "ru": "Рынок позитивно <w>отреагировал</w> на внезапное объявление."
                            }
                        }
                    ],
                    "synonyms": [
                        "take",
                        "take"
                    ],
                    "common_phrases": [
                        "react to",
                        "react quickly",
                        "react badly",
                        "react with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действовать определенным образом в ответ на что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реагировать",
                                "target_lang_sense_clarification": "несовершенный вид"
                            },
                            {
                                "target_lang_word": "отреагировать",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd851ae5-8f29-3ef7-9083-24f4191c017d",
                    "sense_definition": "To express an opinion or feeling about a social media post using a specific icon or emoji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>react</w> to the message with a thumbs up if you agree.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>отреагировать</w> на сообщение пальцем вверх, если согласны."
                            }
                        },
                        {
                            "text": "He <w>reacted</w> with a laughing face to my serious comment.",
                            "target_lang_translations": {
                                "ru": "Он <w>отреагировал</w> смеющимся смайликом на мой серьезный комментарий."
                            }
                        }
                    ],
                    "common_phrases": [
                        "react to a post",
                        "react with an emoji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать мнение или чувство по поводу поста в социальных сетях с помощью эмодзи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отреагировать",
                                "target_lang_sense_clarification": "оставить реакцию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0661881-8bd7-3292-9de2-6cabfb065b2a",
                    "sense_definition": "To undergo a chemical change when mixed with another substance.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "chemistry_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Potassium <w>reacts</w> violently when it comes into contact with water.",
                            "target_lang_translations": {
                                "ru": "Калий бурно <w>реагирует</w> при контакте с водой."
                            }
                        },
                        {
                            "text": "Acids <w>react</w> with bases to form salts and water.",
                            "target_lang_translations": {
                                "ru": "Кислоты <w>реагируют</w> с основаниями, образуя соли и воду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "react with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергаться химическому изменению при взаимодействии с другим веществом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реагировать",
                                "target_lang_sense_clarification": "химический процесс"
                            },
                            {
                                "target_lang_word": "вступать в реакцию",
                                "target_lang_sense_clarification": "устойчивое выражение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d13d5301-f942-3038-ac8a-12c025e9906f",
                    "sense_definition": "An emoji or icon used to express a reaction to a post on social media.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I got so many sad <w>reacts</w> on my post about the cancelled event.",
                            "target_lang_translations": {
                                "ru": "Я получил так много грустных <w>реакций</w> на мой пост об отмене мероприятия."
                            }
                        },
                        {
                            "text": "Don't forget to leave a <w>react</w> if you enjoyed the video.",
                            "target_lang_translations": {
                                "ru": "Не забудьте оставить <w>реакцию</w>, если вам понравилось видео."
                            }
                        }

{
    "word_family": [
        "draai",
        "draaien",
        "omdraaiing",
        "verdraaien",
        "terugdraaien",
        "aandraaien",
        "uitdraaien"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "eb2a9524-f405-30f0-a986-ac5677f17ef3",
                    "sense_definition": "Iets op de andere zijde leggen of een halve slag laten maken, zodat de achterkant of onderkant zichtbaar wordt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil jij het vlees in de pan even <w>omdraaien</w>?",
                            "target_lang_translations": {
                                "en": "Could you <w>turn</w> the meat <w>over</w> in the pan for a moment?"
                            }
                        },
                        {
                            "text": "Hij <w>draaide</w> de bladzijde van zijn boek <w>om</w>.",
                            "target_lang_translations": {
                                "en": "He <w>turned</w> the page of his book <w>over</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "keren",
                        "wentelen"
                    ],
                    "common_phrases": [
                        "de bladzijde omdraaien",
                        "het matras omdraaien",
                        "een kaart omdraaien"
                    ],
                    "target_lang_definitions": {
                        "en": "To place something on its other side or rotate it so the back or bottom becomes visible."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn over",
                                "target_lang_sense_clarification": "flip to the other side"
                            },
                            {
                                "target_lang_word": "flip",
                                "target_lang_sense_clarification": "turn quickly"
                            },
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "move a page"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f30e7a5d-ad91-37af-8c7d-2558972e4d1d",
                    "sense_definition": "Zichzelf of een voertuig in de tegenovergestelde richting draaien om terug te gaan of naar achteren te kijken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze <w>draaide</w> zich <w>om</w> toen ze haar naam hoorde roepen.",
                            "target_lang_translations": {
                                "en": "She <w>turned around</w> when she heard her name being called."
                            }
                        },
                        {
                            "text": "De straat liep dood, dus we moesten met de auto <w>omdraaien</w>.",
                            "target_lang_translations": {
                                "en": "The street was a dead end, so we had to <w>turn around</w> with the car."
                            }
                        }
                    ],
                    "synonyms": [
                        "omkeren",
                        "rechtsomkeert maken"
                    ],
                    "antonyms": [
                        "doorlopen",
                        "verdergaan"
                    ],
                    "common_phrases": [
                        "zich omdraaien",
                        "snel omdraaien"
                    ],
                    "target_lang_definitions": {
                        "en": "To rotate oneself or a vehicle in the opposite direction to go back or look behind."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn around",
                                "target_lang_sense_clarification": "rotate 180 degrees"
                            },
                            {
                                "target_lang_word": "turn back",
                                "target_lang_sense_clarification": "return in the opposite direction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "baedb33e-1e90-3632-a0c3-6cb65783b512",
                    "sense_definition": "Een situatie, volgorde of verhouding compleet veranderen, vaak in het tegendeel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In dit spel gaan we de rollen <w>omdraaien</w>.",
                            "target_lang_translations": {
                                "en": "In this game, we are going to <w>reverse</w> the roles."
                            }
                        },
                        {
                            "text": "Je mag mijn woorden niet <w>omdraaien</w> om gelijk te krijgen.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>twist</w> my words to get your way."
                            }
                        }
                    ],
                    "synonyms": [
                        "verwisselen",
                        "omkeren"
                    ],
                    "common_phrases": [
                        "de rollen omdraaien",
                        "elk dubbeltje omdraaien",
                        "de woorden omdraaien"
                    ],
                    "target_lang_definitions": {
                        "en": "To completely change a situation, order, or relationship, often to the contrary."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reverse",
                                "target_lang_sense_clarification": "swap roles or order"
                            },
                            {
                                "target_lang_word": "twist",
                                "target_lang_sense_clarification": "distort the meaning of words"
                            },
                            {
                                "target_lang_word": "turn around",
                                "target_lang_sense_clarification": "change a situation positively"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cff33ee8-5ccc-3fcc-ba4c-88152ac5b433",
                    "sense_definition": "De handeling van het keren of een draai maken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met een snelle <w>omdraai</w> stond hij plotseling weer voor me.",
                            "target_lang_translations": {
                                "en": "With a quick <w>turn</w>, he was suddenly standing in front of me again."
                            }
                        },
                        {
                            "text": "De <w>omdraai</w> in het beleid kwam voor velen als een verrassing.",
                            "target_lang_translations": {
                                "en": "The <w>turnaround</w> in policy came as a surprise to many."
                            }
                        }
                    ],
                    "synonyms": [
                        "wending",
                        "draai",
                        "ommekeer"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of turning or making a turn."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "physical movement"
                            },
                            {
                                "target_lang_word": "turnaround",
                                "target_lang_sense_clarification": "reversal of policy or situation"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
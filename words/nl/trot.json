                            {
                                "target_lang_word": "trots",
                                "target_lang_sense_clarification": "to move at a trot (he/she/it)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d29fc1c0-a0a8-328e-b2ad-72be515bd3dc",
                    "sense_definition": "Gebiedende wijs van <w>trotten</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Trot</w> eens wat sneller door, we hebben haast!",
                            "target_lang_translations": {
                                "en": "<w>Trot</w> along a bit faster, we're in a hurry!"
                            }

{
    "word_family": [
        "pronunciation",
        "pronouncement",
        "mispronounce",
        "pronounceable",
        "unpronounceable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "35c4791e-cf5c-344f-adeb-acd5906b7e26",
                    "sense_definition": "To make the sound of a word or letter with your voice.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "articulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please tell me how to <w>pronounce</w> your name correctly.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, скажите, как правильно <w>произносить</w> ваше имя."
                            }
                        },
                        {
                            "text": "Students often find it difficult to <w>pronounce</w> certain English vowels.",
                            "target_lang_translations": {
                                "ru": "Студентам часто трудно <w>произносить</w> определенные английские гласные."
                            }
                        }
                    ],
                    "synonyms": [
                        "articulate",
                        "say",
                        "utter",
                        "vocalize"
                    ],
                    "common_phrases": [
                        "pronounce correctly",
                        "hard to pronounce",
                        "pronounce a word"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произносить звуки слова или буквы с помощью голоса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "произносить",
                                "target_lang_sense_clarification": "издавать звуки речи, выговаривать слова"
                            },
                            {
                                "target_lang_word": "выговаривать",
                                "target_lang_sense_clarification": "произносить членораздельно или с усилием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ee135a3-1424-3a93-a53c-a7cec3eb6d29",
                    "sense_definition": "To state something officially, formally, or authoritatively.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "declaration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor arrived at the scene and <w>pronounced</w> the victim dead.",
                            "target_lang_translations": {
                                "ru": "Врач прибыл на место происшествия и <w>объявил</w> о смерти пострадавшего."
                            }
                        },
                        {
                            "text": "At the end of the ceremony, the priest will <w>pronounce</w> them husband and wife.",
                            "target_lang_translations": {
                                "ru": "В конце церемонии священник <w>объявит</w> их мужем и женой."
                            }
                        }
                    ],
                    "synonyms": [
                        "declare",
                        "proclaim",
                        "announce",
                        "decree"
                    ],
                    "common_phrases": [
                        "pronounce dead",
                        "pronounce judgment",
                        "pronounce sentence",
                        "pronounce man and wife"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально или торжественно заявлять о чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объявлять",
                                "target_lang_sense_clarification": "официально сообщать"
                            },
                            {
                                "target_lang_word": "провозглашать",
                                "target_lang_sense_clarification": "торжественно объявлять"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8ae86c72-6f7b-36be-a52c-abc69112d52f",
                    "sense_definition": "Подготовить и осуществить какое-либо мероприятие, наладить какое-либо дело.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <организовать> небольшую вечеринку для наших друзей в эту субботу.",
                            "target_lang_translations": {
                                "en": "We decided to <organize> a small party for our friends this Saturday."
                            }
                        },
                        {
                            "text": "Директор поручил мне <организовать> важную встречу с новыми партнёрами.",
                            "target_lang_translations": {
                                "en": "The director instructed me to <arrange> an important meeting with new partners."
                            }
                        }
                    ],
                    "common_phrases": [
                        "организовать встречу",
                        "организовать поездку",
                        "организовать выставку",
                        "организовать праздник"
                    ],
                    "target_lang_definitions": {
                        "en": "To prepare and carry out an event or manage an affair."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "organize",
                                "target_lang_sense_clarification": "an event or party"
                            },
                            {
                                "target_lang_word": "arrange",
                                "target_lang_sense_clarification": "a meeting or gathering"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0de95ae5-0d41-32b7-9ac7-8a38a6f1ec6a",
                    "sense_definition": "Создать, основать или учредить новую организацию, группу или предприятие.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студенты хотят <организовать> свой собственный клуб любителей кино.",
                            "target_lang_translations": {
                                "en": "Students want to <set up> their own movie lovers' club."
                            }
                        },
                        {
                            "text": "Ему удалось <организовать> успешный бизнес всего за несколько месяцев.",
                            "target_lang_translations": {
                                "en": "He managed to <establish> a successful business in just a few months."
                            }
                        }
                    ],
                    "synonyms": [
                        "создать",
                        "основать",
                        "образовать",
                        "сформировать",
                        "учредить"
                    ],
                    "antonyms": [
                        "расформировать",
                        "распустить"
                    ],
                    "common_phrases": [
                        "организовать бизнес",
                        "организовать фирму",
                        "организовать комитет",
                        "организовать группу"
                    ],
                    "target_lang_definitions": {
                        "en": "To create, found, or establish a new organization, group, or business."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "set up",
                                "target_lang_sense_clarification": "a business or club"
                            },
                            {
                                "target_lang_word": "establish",
                                "target_lang_sense_clarification": "an organization"
                            },
                            {
                                "target_lang_word": "found",
                                "target_lang_sense_clarification": "a company"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f6cffb6-e7e5-3bf9-b33c-cb04b76a159f",
                    "sense_definition": "Внести порядок во что-либо, сделать что-либо планомерным и упорядоченным.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Очень важно правильно <организовать> своё рабочее время, чтобы всё успевать.",
                            "target_lang_translations": {
                                "en": "It is very important to properly <organize> your working time to get everything done."
                            }
                        },
                        {
                            "text": "Учитель помог детям <организовать> их деятельность на уроке.",
                            "target_lang_translations": {
                                "en": "The teacher helped the children <organize> their activity during the lesson."
                            }
                        }
                    ],
                    "common_phrases": [

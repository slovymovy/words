                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am tired of your constant <w>complaining</w>.",
                            "target_lang_translations": {
                                "ru": "Я устал от твоего постоянного <w>нытья</w>."
                            }

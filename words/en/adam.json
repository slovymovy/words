    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d61f73da-155a-35a5-aff3-796aceac8561",
                    "sense_definition": "A common male given name.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My brother <w>Adam</w> plays football very well.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>Адам</w> очень хорошо играет в футбол."
                            }
                        },
                        {
                            "text": "I asked <w>Adam</w> to help me with my homework.",
                            "target_lang_translations": {
                                "ru": "Я попросил <w>Адама</w> помочь мне с домашним заданием."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенное мужское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Адам",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2aea1297-3a35-3540-b391-7782205bb722",
                    "sense_definition": "The first man created by God according to the Bible and the Quran, considered the progenitor of the human race.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious_figure",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "The story of <w>Adam</w> and Eve is one of the most famous in the Bible.",
                            "target_lang_translations": {
                                "ru": "История <w>Адама</w> и Евы — одна из самых известных в Библии."
                            }
                        },
                        {
                            "text": "According to scripture, <w>Adam</w> lived in the Garden of Eden.",
                            "target_lang_translations": {
                                "ru": "Согласно писанию, <w>Адам</w> жил в Эдемском саду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Adam and Eve"
                    ],
                    "target_lang_definitions": {
                        "ru": "Первый человек, сотворенный Богом согласно Библии и Корану, считающийся прародителем человеческого рода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Адам",
                                "target_lang_sense_clarification": "библейский персонаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fa8d0f0-aa31-3046-b4de-9bf851f57fc7",
                    "sense_definition": "A family name (surname) derived from the given name Adam.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Adam</w> will be speaking at the conference tomorrow.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Адам</w> будет выступать на конференции завтра."
                            }
                        },
                        {
                            "text": "The <w>Adam</w> family has lived in this town for generations.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Адам</w> живет в этом городе уже несколько поколений."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "derived from the given name"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, образованная от имени Адам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Адам",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85bfddfc-a41f-3306-821e-f6e3071c6a11",
                    "sense_definition": "Relating to a neoclassical style of architecture, furniture, and interior design developed by the Adam brothers in the 18th century.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "The drawing room features a beautiful <w>Adam</w> fireplace with intricate details.",
                            "target_lang_translations": {
                                "ru": "В гостиной есть красивый камин в стиле <w>Адам</w> с замысловатыми деталями."
                            }
                        },
                        {
                            "text": "Many historic houses in London are decorated in the <w>Adam</w> style.",
                            "target_lang_translations": {
                                "ru": "Многие исторические дома в Лондоне оформлены в стиле <w>Адам</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Adam style"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относится к неоклассическому стилю архитектуры, мебели и дизайна интерьера, разработанному братьями Адам в XVIII веке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стиль Адам",
                                "target_lang_sense_clarification": "архитектурный стиль"
                            },
                            {
                                "target_lang_word": "адамовский стиль",
                                "target_lang_sense_clarification": "архитектура и дизайн"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a851077-94bb-3230-97c4-b795ce097dfd",
                    "sense_definition": "A title for Jesus Christ in Christian theology, contrasting him with the first man.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "religious_figure",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "The apostle Paul refers to Christ as the last <w>Adam</w> who brings life.",
                            "target_lang_translations": {
                                "ru": "Апостол Павел называет Христа последним <w>Адамом</w>, несущим жизнь."
                            }
                        },
                        {
                            "text": "Theology often compares the disobedience of the first man with the obedience of the second <w>Adam</w>.",
                            "target_lang_translations": {
                                "ru": "Богословие часто сравнивает непослушание первого человека с послушанием второго <w>Адама</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Jesus",
                        "Christ"
                    ],
                    "common_phrases": [
                        "Second Adam",
                        "Last Adam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул Иисуса Христа в христианском богословии, противопоставляющий его первому человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Адам",
                                "target_lang_sense_clarification": "титул Христа (Второй Адам)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "492efbf3-283e-30a2-bd93-82ca48defe1e",
                    "sense_definition": "Human nature in its unregenerate or sinful state; original sin.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "He struggled to suppress the old <w>Adam</w> that urged him to be selfish.",
                            "target_lang_translations": {
                                "ru": "Он боролся, чтобы подавить ветхого <w>Адама</w>, который побуждал его быть эгоистичным."
                            }
                        },
                        {
                            "text": "Despite his best efforts, a bit of the old <w>Adam</w> remained in his character.",
                            "target_lang_translations": {
                                "ru": "Несмотря на все его усилия, в его характере осталось немного от ветхого <w>Адама</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the old Adam"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary or theological context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человеческая природа в её невозрожденном или греховном состоянии; первородный грех."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ветхий Адам",
                                "target_lang_sense_clarification": "греховная природа человека"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "frequency": "High",
                    "semantic_group_id": "order_goods",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil graag twee kaartjes voor de voorstelling <w>bestellen</w>.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>заказать</w> два билета на представление."
                            }
                        },
                        {
                            "text": "Zullen we vanavond pizza <w>bestellen</w> of gaan we zelf koken?",
                            "target_lang_translations": {
                                "ru": "Мы <w>закажем</w> пиццу сегодня вечером или будем готовить сами?"
                            }
                        },
                        {
                            "text": "Kan ik hier ook een kopje koffie <w>bestellen</w>?",
                            "target_lang_translations": {
                                "ru": "Могу я здесь также <w>заказать</w> чашечку кофе?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "online bestellen",
                        "eten bestellen",
                        "vooraf bestellen",
                        "kaartjes bestellen"
                    ],
                    "target_lang_definitions": {

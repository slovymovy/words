                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роет канавы",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "чистит рвы",
                                "target_lang_sense_clarification": "обслуживание водотоков"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "343e5697-aa4b-32ab-a29f-8a8254d48e11",
                    "sense_definition": "Een smalle, gegraven watergang die dient voor de afwatering of als afscheiding tussen weilanden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De auto vloog over een <w>sloot</w> en kwam in een weiland tot stilstand.",
                            "target_lang_translations": {
                                "ru": "Машина перелетела через <w>канаву</w> и остановилась на лугу."
                            }
                        },
                        {
                            "text": "De kinderen probeerden met een polsstok over de brede <w>sloot</w> te springen.",
                            "target_lang_translations": {
                                "ru": "Дети пытались перепрыгнуть через широкую <w>канаву</w> с шестом."
                            }
                        }
                    ],
                    "synonyms": [

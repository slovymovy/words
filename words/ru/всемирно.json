    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "83e83dce-3491-3395-8e0c-ae5fdf8c9008",
                    "sense_definition": "Будучи известным всему человечеству, признаваемым всеми людьми; в мировом масштабе (обычно перед прилагательными или причастиями).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот музей стал <w>всемирно</w> известным благодаря своей уникальной коллекции картин.",
                            "target_lang_translations": {
                                "en": "This museum became <w>world</w>-famous thanks to its unique collection of paintings."
                            }
                        },
                        {
                            "text": "Учёный получил <w>всемирно</w> признанную награду за свои исследования в области физики.",
                            "target_lang_translations": {
                                "en": "The scientist received a <w>universally</w> recognized award for his research in the field of physics."
                            }
                        }
                    ],
                    "common_phrases": [
                        "всемирно известный",
                        "всемирно признанный",
                        "всемирно прославленный"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to indicate that something is known, recognized, or acknowledged by people all over the world."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "universally",
                                "target_lang_sense_clarification": "recognized or acknowledged by everyone"
                            },
                            {
                                "target_lang_word": "worldwide",
                                "target_lang_sense_clarification": "known throughout the world"
                            },
                            {
                                "target_lang_word": "internationally",
                                "target_lang_sense_clarification": "known across nations"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35e07794-71e4-30b9-a2bd-8ef8874eb1dc",
                    "sense_definition": "Во всём мире, распространяясь на весь мир или охватывая всё человечество.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

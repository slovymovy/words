            "senses": [
                {
                    "sense_id": "5d21fe22-68b4-3523-b05d-dc91132b9505",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>trappen</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>trap</w> de bal zo ver mogelijk weg.",
                            "target_lang_translations": {
                                "en": "I <w>kick</w> the ball away as far as possible.",
                                "ru": "Я <w>пинаю</w> мяч как можно дальше."
                            }
                        },
                        {
                            "text": "Ik <w>trap</w> niet in die flauwe grap van jou.",
                            "target_lang_translations": {
                                "en": "I'm not <w>falling</w> for that lame joke of yours.",
                                "ru": "Я не <w>поведусь</w> на твою глупую шутку."
                            }

                {
                    "sense_id": "ba37f4fc-8bb4-30be-b8f2-64307b022c7c",
                    "sense_definition": "Sprawdzać stan zdrowia osoby lub zwierzęcia, wykonując odpowiednie czynności medyczne.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz dokładnie <w>bada</w> pacjenta w swoim gabinecie.",
                            "target_lang_translations": {
                                "ru": "Врач тщательно <w>осматривает</w> пациента в своем кабинете."
                            }
                        },
                        {
                            "text": "Pielęgniarka musi regularnie <w>badać</w> puls chorego.",
                            "target_lang_translations": {
                                "ru": "Медсестра должна регулярно <w>проверять</w> пульс больного."
                            }
                        }
                    ],
                    "synonyms": [
                        "studiować"
                    ],
                    "common_phrases": [
                        "badać pacjenta",
                        "badać krew",
                        "badać puls",
                        "badać serce"
                    ],
                    "target_lang_definitions": {

{
    "word_family": [
        "cancellation",
        "cancellable",
        "canceler"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c2eef74d-2105-31d9-a0e9-548f07280098",
                    "sense_definition": "To decide that an organized event or arrangement will not take place; to annul an agreement.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>cancelled</w> his order on their website.",
                            "target_lang_translations": {
                                "ru": "Он <w>отменил</w> свой заказ на их сайте."
                            }
                        },
                        {
                            "text": "We had to <w>cancel</w> the picnic because of the heavy rain.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <w>отменить</w> пикник из-за сильного дождя."
                            }
                        }
                    ],
                    "synonyms": [
                        "call off",
                        "annul",
                        "revoke",
                        "void",
                        "withdraw"
                    ],
                    "antonyms": [
                        "book",
                        "schedule",
                        "confirm",
                        "arrange"
                    ],
                    "common_phrases": [
                        "cancel a meeting",
                        "cancel a flight",
                        "cancel a reservation",
                        "cancel an order"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять решение о том, что запланированное мероприятие не состоится; аннулировать соглашение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отменить",
                                "target_lang_sense_clarification": "самый общий перевод"
                            },
                            {
                                "target_lang_word": "аннулировать",
                                "target_lang_sense_clarification": "официально (заказ, соглашение)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b944f38-907d-3928-8399-f82a9c4d3660",
                    "sense_definition": "To stop the production or broadcasting of a television show or series.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The network decided to <w>cancel</w> the show due to low ratings.",
                            "target_lang_translations": {
                                "ru": "Телесеть решила <w>закрыть</w> шоу из-за низких рейтингов."
                            }
                        },
                        {
                            "text": "My favorite series was <w>cancelled</w> after just one season.",
                            "target_lang_translations": {
                                "ru": "Мой любимый сериал <w>закрыли</w> всего после одного сезона."
                            }
                        }
                    ],
                    "synonyms": [
                        "discontinue",
                        "ax",
                        "drop"
                    ],
                    "antonyms": [
                        "renew",
                        "commission"
                    ],
                    "common_phrases": [
                        "cancel a show",
                        "cancel a series"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекратить производство или трансляцию телешоу или сериала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрыть",
                                "target_lang_sense_clarification": "о телепередаче или сериале"
                            },
                            {
                                "target_lang_word": "снять с эфира",
                                "target_lang_sense_clarification": "прекратить вещание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d188e647-5774-3bdf-8029-6a38c1eeb2de",
                    "sense_definition": "To remove a common factor from both the numerator and denominator of a fraction, or from both sides of an equation.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>cancel</w> the twos to simplify the fraction.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>сократить</w> двойки, чтобы упростить дробь."
                            }
                        },
                        {
                            "text": "If you <w>cancel</w> x from both sides, the answer becomes clear.",
                            "target_lang_translations": {
                                "ru": "Если <w>сократить</w> икс с обеих сторон, ответ станет ясен."
                            }
                        }
                    ],
                    "synonyms": [
                        "simplify",
                        "reduce"
                    ],
                    "common_phrases": [
                        "cancel factors",
                        "cancel out"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалить общий множитель из числителя и знаменателя дроби или из обеих частей уравнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сократить",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0ab0316-a643-39c6-ac86-e6e46cb48b6b",
                    "sense_definition": "To cease to provide support to a public figure or organization, usually due to offensive behavior or opinions.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many fans decided to <w>cancel</w> the singer after the scandal.",
                            "target_lang_translations": {
                                "ru": "Многие фанаты решили <w>отменить</w> певца после скандала."
                            }
                        },
                        {
                            "text": "You cannot simply <w>cancel</w> everyone who disagrees with you.",
                            "target_lang_translations": {
                                "ru": "Нельзя просто <w>отменять</w> всех, кто с вами не согласен."
                            }
                        }
                    ],
                    "synonyms": [
                        "boycott",
                        "blacklist",
                        "ostracize"
                    ],
                    "antonyms": [
                        "support",
                        "endorse"
                    ],
                    "common_phrases": [
                        "cancel culture",
                        "get cancelled"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать поддерживать публичную личность или организацию, обычно из-за оскорбительного поведения (культура отмены)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отменить",
                                "target_lang_sense_clarification": "сленг, калька с английского"
                            },
                            {
                                "target_lang_word": "бойкотировать",
                                "target_lang_sense_clarification": "отказаться от поддержки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "808fd88d-3187-3e13-a0c3-84cf346332a7",
                    "sense_definition": "To neutralize or negate the effect of something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The corrective feedback mechanism <w>cancels</w> out the noise.",
                            "target_lang_translations": {
                                "ru": "Механизм корректирующей обратной связи <w>нейтрализует</w> шум."
                            }
                        },
                        {
                            "text": "His generosity helps to <w>cancel</w> out his past mistakes.",
                            "target_lang_translations": {
                                "ru": "Его щедрость помогает <w>сгладить</w> его прошлые ошибки."
                            }
                        }
                    ],
                    "synonyms": [
                        "offset",
                        "counterbalance",
                        "neutralize",
                        "negate"
                    ],
                    "common_phrases": [
                        "cancel out",
                        "cancel each other out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нейтрализовать или свести на нет действие чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нейтрализовать",
                                "target_lang_sense_clarification": "устранить эффект"
                            },
                            {
                                "target_lang_word": "свести на нет",
                                "target_lang_sense_clarification": "фразеологизм"
                            },
                            {
                                "target_lang_word": "уравновесить",
                                "target_lang_sense_clarification": "компенсировать действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c215054-4002-33aa-bbe4-36373baf99e6",
                    "sense_definition": "To mark a postage stamp or ticket so that it cannot be reused.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This machine <w>cancels</w> the letters that have a valid zip code.",
                            "target_lang_translations": {
                                "ru": "Эта машина <w>гасит</w> письма с действительным почтовым индексом."
                            }
                        },
                        {
                            "text": "The conductor will <w>cancel</w> your ticket on the train.",
                            "target_lang_translations": {
                                "ru": "Кондуктор <w>погасит</w> ваш билет в поезде."
                            }
                        }
                    ],
                    "synonyms": [
                        "invalidate",
                        "void"
                    ],
                    "common_phrases": [
                        "cancel a stamp"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поставить отметку на почтовой марке или билете, чтобы предотвратить их повторное использование."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погасить",
                                "target_lang_sense_clarification": "о марке или долге"
                            },
                            {
                                "target_lang_word": "аннулировать",
                                "target_lang_sense_clarification": "сделать недействительным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e5131f6-bcf7-3ab3-a3c7-752f02b1aaf4",
                    "sense_definition": "To cross out something with lines to delete or invalidate it.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The editor <w>cancelled</w> the paragraph with a red pen.",
                            "target_lang_translations": {
                                "ru": "Редактор <w>зачеркнул</w> абзац красной ручкой."
                            }
                        },
                        {
                            "text": "Please <w>cancel</w> the wrong entry in the logbook.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>вычеркните</w> неверную запись в журнале."
                            }
                        }
                    ],
                    "synonyms": [
                        "cross out",
                        "strike out",
                        "delete"
                    ],
                    "common_phrases": [
                        "cancel a line"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перечеркнуть что-либо линиями, чтобы удалить или сделать недействительным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зачеркнуть",
                                "target_lang_sense_clarification": "провести черту поверх"
                            },
                            {
                                "target_lang_word": "вычеркнуть",
                                "target_lang_sense_clarification": "удалить из списка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43e52102-bc18-3bea-ab30-f6f75e512fa1",
                    "sense_definition": "To kill someone.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mob boss ordered his men to <w>cancel</w> the witness.",
                            "target_lang_translations": {
                                "ru": "Главарь мафии приказал своим людям <w>убрать</w> свидетеля."
                            }
                        },
                        {
                            "text": "He threatened to <w>cancel</w> anyone who stood in his way.",
                            "target_lang_translations": {
                                "ru": "Он угрожал <w>прикончить</w> любого, кто встанет у него на пути."
                            }
                        }
                    ],
                    "synonyms": [
                        "kill",
                        "eliminate",
                        "take out"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Убить кого-то (криминальный сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убрать",
                                "target_lang_sense_clarification": "криминальный жаргон"
                            },
                            {
                                "target_lang_word": "ликвидировать",
                                "target_lang_sense_clarification": "устранить физически"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bba9f548-c232-38e8-b8fc-e50c5149c122",
                    "sense_definition": "A printed page or section that is suppressed or replaced due to an error.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The binder removed the <w>cancel</w> before stitching the book.",
                            "target_lang_translations": {
                                "ru": "Переплетчик удалил <w>заменяющую страницу</w> перед сшиванием книги."
                            }
                        },
                        {
                            "text": "This edition contains a <w>cancel</w> replacing the erroneous page.",
                            "target_lang_translations": {
                                "ru": "Это издание содержит <w>вклейку</w>, заменяющую ошибочную страницу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "printing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Печатная страница или раздел, заменяющие ошибочные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заменяющая страница",
                                "target_lang_sense_clarification": "полиграфия"
                            },
                            {
                                "target_lang_word": "вклейка",
                                "target_lang_sense_clarification": "исправление в книге"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "345c304c-6a8d-3116-9f69-946861170e49",
                    "sense_definition": "A mark made on a postage stamp to show it has been used.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>cancel</w> on this stamp is from 1920.",
                            "target_lang_translations": {
                                "ru": "<w>Штемпель</w> на этой марке датирован 1920 годом."
                            }
                        },
                        {
                            "text": "Collectors look for a clear <w>cancel</w> on the envelope.",
                            "target_lang_translations": {
                                "ru": "Коллекционеры ищут четкое <w>гашение</w> на конверте."
                            }
                        }
                    ],
                    "synonyms": [
                        "cancellation"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "philately"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отметка на почтовой марке, показывающая, что она была использована."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штемпель",
                                "target_lang_sense_clarification": "печать на почте"
                            },
                            {
                                "target_lang_word": "гашение",
                                "target_lang_sense_clarification": "филателия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "55c3648c-124b-3c96-88f8-ff19fd1e5790",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Cancel</w> signed the guestbook.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Кансел</w> расписался в гостевой книге."
                            }
                        },
                        {
                            "text": "I met a woman named Maria <w>Cancel</w>.",
                            "target_lang_translations": {
                                "ru": "Я встретил женщину по имени Мария <w>Кансел</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кансел",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
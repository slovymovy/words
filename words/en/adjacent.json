    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b857a2ef-3a05-30ad-b37b-8a2bf3c3c1d5",
                    "sense_definition": "Lying near, close, or contiguous; neighboring; sharing a common boundary.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_proximity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Because the conference room is filled, we will have our meeting in the <w>adjacent</w> room.",
                            "target_lang_translations": {
                                "ru": "Так как конференц-зал занят, мы проведем встречу в <w>соседней</w> комнате."
                            }
                        },
                        {
                            "text": "They decided to buy the <w>adjacent</w> plot of land to expand their garden.",
                            "target_lang_translations": {
                                "ru": "Они решили купить <w>прилегающий</w> участок земли, чтобы расширить свой сад."
                            }
                        }
                    ],
                    "synonyms": [
                        "abutting",
                        "adjoining",
                        "contiguous",
                        "near"
                    ],
                    "antonyms": [
                        "apart",
                        "distant",
                        "nonadjacent"
                    ],
                    "common_phrases": [
                        "adjacent building",
                        "adjacent area",
                        "adjacent room",
                        "immediately adjacent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся рядом, поблизости или граничащий с чем-либо; соседний."

                {
                    "sense_id": "e018967d-52eb-32af-b1cc-04942bb90011",
                    "sense_definition": "A small gun that is held in and fired from one hand.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police officer drew his <w>pistol</w> when he saw the suspect reaching for a weapon.",

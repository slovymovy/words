            "senses": [
                {
                    "sense_id": "f9502215-b121-328c-9d1a-0b3947246f2e",
                    "sense_definition": "Zonder baard; niet voorzien van haargroei op de kin en wangen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>baardloze</w> wangen waren rood van de kou.",
                            "target_lang_translations": {
                                "en": "His <w>beardless</w> cheeks were red from the cold.",
                                "ru": "Его <w>безбородые</w> щеки покраснели от холода."
                            }
                        },
                        {
                            "text": "Hij heeft een <w>baardloos</w> gezicht omdat hij zich elke ochtend scheert.",
                            "target_lang_translations": {
                                "en": "He has a <w>beardless</w> face because he shaves every morning.",
                                "ru": "У него <w>безбородое</w> лицо, потому что он бреется каждое утро."
                            }
                        }
                    ],
                    "synonyms": [
                        "glad"
                    ],
                    "antonyms": [
                        "bebaard"
                    ],
                    "common_phrases": [
                        "een baardloos gezicht",
                        "een baardloze kin"
                    ],
                    "target_lang_definitions": {
                        "en": "Without a beard; lacking hair growth on the chin and cheeks.",
                        "ru": "Без бороды; не имеющий волосяного покрова на подбородке и щеках."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beardless",
                                "target_lang_sense_clarification": "lacking a beard"
                            },
                            {
                                "target_lang_word": "smooth-faced",
                                "target_lang_sense_clarification": "having no facial hair"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "безбородый",
                                "target_lang_sense_clarification": "не имеющий бороды"
                            },
                            {
                                "target_lang_word": "бритый",
                                "target_lang_sense_clarification": "гладко выбритый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c076c24-83c4-3a81-9d4d-da3f970ea2a9",
                    "sense_definition": "Jong en onervaren, verwijzend naar iemand die nog niet volwassen genoeg is voor baardgroei.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>baardloze</w> soldaten wisten nog niet wat hen te wachten stond.",
                            "target_lang_translations": {
                                "en": "The <w>beardless</w> soldiers did not yet know what awaited them.",
                                "ru": "<w>Безбородые</w> солдаты еще не знали, что их ждет."
                            }
                        },
                        {
                            "text": "Als <w>baardloze</w> jongeling begon hij aan zijn carrière in de politiek.",
                            "target_lang_translations": {
                                "en": "As a <w>beardless</w> youth, he began his career in politics.",
                                "ru": "Будучи <w>безбородым</w> юнцом, он начал свою карьеру в политике."
                            }
                        }
                    ],
                    "synonyms": [
                        "jong",
                        "onmondig"
                    ],
                    "antonyms": [
                        "ervaren",
                        "volwassen"
                    ],
                    "common_phrases": [
                        "een baardloze jongeling"
                    ],
                    "target_lang_definitions": {
                        "en": "Young and inexperienced, referring to someone not yet mature enough to grow a beard.",
                        "ru": "Молодой и неопытный; о человеке, который еще недостаточно взрослый для роста бороды."
                    },
                    "translations": {

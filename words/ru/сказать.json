{
    "word_family": [
        "рассказ",
        "сказка",
        "подсказка",
        "пересказ",
        "высказывание",
        "сказуемое",
        "предсказание",
        "иносказание",
        "рассказчик",
        "сказитель"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1b746c89-8c40-3475-be37-703f8b5fa8de",
                    "sense_definition": "Выразить мысль, мнение или чувство словами в устной речи.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "speech_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <сказать> тебе правду о том, что случилось вчера.",
                            "target_lang_translations": {
                                "en": "I want to <tell> you the truth about what happened yesterday."
                            }
                        },
                        {
                            "text": "Он хотел что-то <сказать>, но передумал и просто промолчал.",
                            "target_lang_translations": {
                                "en": "He wanted to <say> something, but changed his mind and just remained silent."
                            }
                        }
                    ],
                    "synonyms": [
                        "произнести",
                        "промолвить",
                        "вымолвить"
                    ],
                    "antonyms": [
                        "промолчать"
                    ],
                    "common_phrases": [
                        "честно сказать",
                        "сказать слово",
                        "сказать правду",
                        "хочу сказать"
                    ],
                    "target_lang_definitions": {
                        "en": "To express a thought, opinion, or feeling in words using oral speech."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "say",
                                "target_lang_sense_clarification": "to pronounce words or express thoughts"
                            },
                            {
                                "target_lang_word": "utter",
                                "target_lang_sense_clarification": "to produce speech sounds or words"
                            },
                            {
                                "target_lang_word": "speak",
                                "target_lang_sense_clarification": "to express thoughts verbally"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f196b02-02b3-3d5a-9c41-b469798d2f2c",
                    "sense_definition": "Сообщить или изложить какую-либо информацию (устно или письменно).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "speech_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама <сказала>, что мы поедем к бабушке в следующие выходные.",
                            "target_lang_translations": {
                                "en": "Mom <said> that we would go to grandma's next weekend."
                            }
                        },
                        {
                            "text": "Ты можешь <сказать> мне по секрету, какой подарок ты купил?",
                            "target_lang_translations": {
                                "en": "Can you <tell> me in secret what gift you bought?"
                            }
                        }
                    ],
                    "synonyms": [
                        "сообщить",
                        "изложить",
                        "передать"
                    ],
                    "common_phrases": [
                        "сказать по секрету",
                        "сказать новость",
                        "сказать спасибо"
                    ],
                    "target_lang_definitions": {
                        "en": "To communicate or set forth information (orally or in writing)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tell",
                                "target_lang_sense_clarification": "to communicate information to someone"
                            },
                            {
                                "target_lang_word": "say",
                                "target_lang_sense_clarification": "to state a fact or opinion"
                            },
                            {
                                "target_lang_word": "inform",
                                "target_lang_sense_clarification": "to give information"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47a6aef9-9071-318a-9857-bf6b5e8132cd",
                    "sense_definition": "Дать распоряжение, приказ или указание сделать что-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель <сказал> ученикам открыть учебники на десятой странице.",
                            "target_lang_translations": {
                                "en": "The teacher <told> the students to open their textbooks to page ten."
                            }
                        },
                        {
                            "text": "Начальник <сказал> подготовить этот важный отчет к завтрашнему утру.",
                            "target_lang_translations": {
                                "en": "The boss <told> us to prepare this important report by tomorrow morning."
                            }
                        }
                    ],
                    "synonyms": [
                        "приказать",
                        "распорядиться",
                        "объявить"
                    ],
                    "common_phrases": [
                        "сказал сделать",
                        "велел сказать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies ordering someone to do something"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To give an order, command, or instruction to do something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tell",
                                "target_lang_sense_clarification": "to instruct or order someone to do something"
                            },
                            {
                                "target_lang_word": "order",
                                "target_lang_sense_clarification": "to give an authoritative command"
                            },
                            {
                                "target_lang_word": "command",
                                "target_lang_sense_clarification": "to direct with authority"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd096b4e-a6f1-39d7-9230-adef27139b3a",
                    "sense_definition": "Сделать вывод, предположить или оценить что-либо (часто используется во вводных конструкциях).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "thought",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Трудно <сказать>, кто победит в этом сложном соревновании.",
                            "target_lang_translations": {
                                "en": "It is hard to <say> who will win in this difficult competition."
                            }
                        },
                        {
                            "text": "Можно <сказать>, что этот проект стал самым успешным в году.",
                            "target_lang_translations": {
                                "en": "One could <say> that this project has become the most successful of the year."
                            }
                        }
                    ],
                    "synonyms": [
                        "предположить",
                        "подумать",
                        "заключить"
                    ],
                    "common_phrases": [
                        "трудно сказать",
                        "можно сказать",
                        "так сказать",
                        "нечего сказать"
                    ],
                    "target_lang_definitions": {
                        "en": "To draw a conclusion, suppose, or evaluate something (often used in introductory phrases)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "say",
                                "target_lang_sense_clarification": "to express an opinion or assumption"
                            },
                            {
                                "target_lang_word": "tell",
                                "target_lang_sense_clarification": "to discern or judge (e.g., hard to tell)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19fe6da5-cb3c-342e-99b9-67ec8c70d516",
                    "sense_definition": "Передать или сообщить что-либо порученное.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "speech_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Слуга поклонился и <сказал> барину, что долгожданные гости уже прибыли.",
                            "target_lang_translations": {
                                "en": "The servant bowed and <told> the master that the long-awaited guests had already arrived."
                            }
                        },
                        {
                            "text": "Прошу <сказать> ему, что я буду ждать ответа до самого вечера.",
                            "target_lang_translations": {
                                "en": "Please <tell> him that I will wait for an answer until the very evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "передать"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To convey or communicate a message that has been entrusted to one."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tell",
                                "target_lang_sense_clarification": "to pass on information to a recipient"
                            },
                            {
                                "target_lang_word": "convey",
                                "target_lang_sense_clarification": "to transmit a message"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
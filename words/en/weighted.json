    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f07a173f-8014-372d-89fe-5d1e98292aa1",
                    "sense_definition": "Having weights attached to it to make it heavier or keep it in place.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_weight",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She sleeps much better at night when she uses a <w>weighted</w> blanket.",
                            "target_lang_translations": {
                                "ru": "Она спит гораздо лучше ночью, когда использует <w>утяжелённое</w> одеяло."
                            }
                        },
                        {
                            "text": "The diver wore a <w>weighted</w> belt to help him stay underwater.",
                            "target_lang_translations": {
                                "ru": "Дайвер надел <w>утяжелённый</w> пояс, чтобы оставаться под водой."
                            }
                        }
                    ],
                    "synonyms": [
                        "loaded",
                        "heavy"
                    ],
                    "antonyms": [
                        "unweighted",
                        "light"
                    ],
                    "common_phrases": [
                        "weighted blanket",
                        "weighted vest",
                        "weighted base"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабжённый дополнительным весом для увеличения тяжести или фиксации на месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утяжелённый",
                                "target_lang_sense_clarification": "с добавленным весом"
                            },
                            {
                                "target_lang_word": "с грузом",
                                "target_lang_sense_clarification": "имеющий груз для веса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "857daad5-3f5b-3c6d-80d0-bf3391cf5d89",
                    "sense_definition": "Calculated by multiplying components by factors to reflect their relative importance.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mathematical_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher calculated a <w>weighted</w> average to determine the final grades.",
                            "target_lang_translations": {
                                "ru": "Учитель вычислил <w>взвешенное</w> среднее, чтобы определить итоговые оценки."
                            }
                        },
                        {
                            "text": "Economists use a <w>weighted</w> index to track changes in consumer prices.",
                            "target_lang_translations": {
                                "ru": "Экономисты используют <w>взвешенный</w> индекс для отслеживания изменений потребительских цен."
                            }
                        }
                    ],
                    "synonyms": [
                        "adjusted"
                    ],
                    "antonyms": [
                        "unweighted",
                        "simple"
                    ],
                    "common_phrases": [
                        "weighted average",
                        "weighted mean",
                        "weighted score"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "statistics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассчитанный путём умножения компонентов на коэффициенты, отражающие их относительную важность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взвешенный",
                                "target_lang_sense_clarification": "статистический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c6f1006-a4d9-3220-9820-8c351dd0b3f2",
                    "sense_definition": "Biased or arranged to favor a specific party or outcome.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bias",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The selection process appeared to be <w>weighted</w> in favor of internal candidates.",
                            "target_lang_translations": {
                                "ru": "Процесс отбора казался <w>смещённым</w> в пользу внутренних кандидатов."
                            }
                        },
                        {
                            "text": "Critics argued that the voting system was <w>weighted</w> against smaller political parties.",
                            "target_lang_translations": {
                                "ru": "Критики утверждали, что система голосования была <w>настроена</w> против малых политических партий."
                            }
                        }
                    ],
                    "synonyms": [
                        "biased",
                        "rigged",
                        "slanted"
                    ],
                    "antonyms": [
                        "fair",
                        "balanced",
                        "impartial"
                    ],
                    "common_phrases": [
                        "weighted against",
                        "weighted in favor of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предвзятый или организованный таким образом, чтобы благоприятствовать определённой стороне или результату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смещённый",
                                "target_lang_sense_clarification": "имеющий перекос в чью-то пользу"
                            },
                            {
                                "target_lang_word": "предвзятый",
                                "target_lang_sense_clarification": "необъективный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de5dab13-6c8c-3357-8181-713df51f13e7",
                    "sense_definition": "Having numerical values assigned to edges in a graph or network.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mathematical_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The algorithm calculates the shortest path through a <w>weighted</w> graph.",
                            "target_lang_translations": {
                                "ru": "Алгоритм вычисляет кратчайший путь через <w>взвешенный</w> граф."
                            }
                        },
                        {
                            "text": "In a <w>weighted</w> network, each connection has a specific cost associated with it.",
                            "target_lang_translations": {
                                "ru": "Во <w>взвешенной</w> сети каждое соединение имеет определённую стоимость."
                            }
                        }
                    ],
                    "antonyms": [
                        "unweighted"
                    ],
                    "common_phrases": [
                        "weighted graph",
                        "weighted edge"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий числовые значения (веса), присвоенные рёбрам графа или сети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взвешенный",
                                "target_lang_sense_clarification": "математический термин (о графах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57dea79f-0b11-366f-b4c4-e2e455b368a1",
                    "sense_definition": "Containing a large proportion of a specific quality or element.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_weight",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The novel is <w>weighted</w> with complex historical details.",
                            "target_lang_translations": {
                                "ru": "Роман <w>насыщен</w> сложными историческими деталями."
                            }
                        },
                        {
                            "text": "Her silence was <w>weighted</w> with unspoken emotion.",
                            "target_lang_translations": {
                                "ru": "Её молчание было <w>наполнено</w> невысказанными эмоциями."
                            }
                        }
                    ],
                    "synonyms": [
                        "laden",
                        "heavy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий большую долю определённого качества или элемента; насыщенный чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "насыщенный",
                                "target_lang_sense_clarification": "полный чего-либо"
                            },
                            {
                                "target_lang_word": "отягощённый",
                                "target_lang_sense_clarification": "наполненный чем-то тяжелым (эмоционально или физически)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "47a743d9-f858-33e8-8d66-8e36cad90628",

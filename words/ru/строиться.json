    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6692248b-486c-320f-afd9-e01c9856f642",
                    "sense_definition": "Возводиться, создаваться (о зданиях, сооружениях и конструкциях).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "construction_passive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В центре нашего города <строится> новый торговый центр.",
                            "target_lang_translations": {
                                "en": "A new shopping center <is being built> in our city center."
                            }
                        },
                        {
                            "text": "У нас во дворе <строится> гараж для машин.",
                            "target_lang_translations": {
                                "en": "A garage for cars <is being built> in our yard."
                            }
                        }
                    ],
                    "synonyms": [
                        "возводиться",
                        "сооружаться"
                    ],
                    "antonyms": [
                        "сноситься",
                        "рушиться"
                    ],
                    "common_phrases": [
                        "дом строится",
                        "активно строиться",
                        "строиться дорога"
                    ],
                    "target_lang_definitions": {
                        "en": "To be erected or created (referring to buildings, structures, and constructions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be built",
                                "target_lang_sense_clarification": "passive voice, general construction"
                            },
                            {
                                "target_lang_word": "be under construction",
                                "target_lang_sense_clarification": "current state of being built"
                            },
                            {
                                "target_lang_word": "be constructed",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99279f18-ddb8-38b5-9a0f-027ed9999e03",
                    "sense_definition": "Основываться на чём-либо, иметь что-либо своей базой или причиной (об отношениях, теориях, планах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_basis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его доводы <строятся> на непроверенной информации из интернета.",
                            "target_lang_translations": {
                                "en": "His arguments <are based> on unverified information from the internet."
                            }
                        },
                        {
                            "text": "Крепкие отношения в семье <строятся> на взаимном доверии.",
                            "target_lang_translations": {
                                "en": "Strong family relationships <are built> on mutual trust."
                            }
                        }
                    ],
                    "synonyms": [
                        "сооружаться",
                        "выстраиваться",
                        "возводиться"
                    ],
                    "common_phrases": [
                        "строиться на доверии",
                        "строиться на фактах",
                        "планы строятся"
                    ],
                    "target_lang_definitions": {
                        "en": "To be founded on something, to have something as a basis or cause (referring to relationships, theories, plans)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be based",
                                "target_lang_sense_clarification": "rely on a foundation"
                            },
                            {
                                "target_lang_word": "be built",
                                "target_lang_sense_clarification": "abstract foundation"
                            },
                            {
                                "target_lang_word": "rest",
                                "target_lang_sense_clarification": "depend on"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61d1eb13-ccae-3f4d-b56f-4c479b714be4",
                    "sense_definition": "Становиться в строй, занимать места в ряду или определённом порядке.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "formation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир громко приказал солдатам <строиться> на плацу.",
                            "target_lang_translations": {
                                "en": "The commander loudly ordered the soldiers to <fall in> on the parade ground."
                            }
                        },
                        {
                            "text": "Школьники начали <строиться> парами перед входом в музей.",
                            "target_lang_translations": {
                                "en": "The schoolchildren began to <line up> in pairs before the entrance to the museum."
                            }
                        }
                    ],
                    "synonyms": [
                        "выстраиваться"
                    ],
                    "antonyms": [
                        "расходиться"
                    ],
                    "common_phrases": [
                        "строиться в шеренгу",
                        "команда строиться",
                        "строиться по росту"
                    ],
                    "target_lang_definitions": {
                        "en": "To get into formation, to take places in a row or specific order."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "line up",
                                "target_lang_sense_clarification": "general formation"
                            },
                            {
                                "target_lang_word": "fall in",
                                "target_lang_sense_clarification": "military formation"
                            },
                            {
                                "target_lang_word": "form up",
                                "target_lang_sense_clarification": "assemble in order"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "658ecf5a-0065-383b-90e2-48c4080984b0",
                    "sense_definition": "Строить себе дом или другие постройки.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "construction_active",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой сосед сейчас <строится> на даче и редко бывает дома.",
                            "target_lang_translations": {
                                "en": "My neighbor is currently <building a house> at his dacha and is rarely at home."
                            }
                        },
                        {
                            "text": "Мы решили <строиться> за городом, чтобы жить ближе к природе.",
                            "target_lang_translations": {
                                "en": "We decided to <build a house> in the countryside to live closer to nature."
                            }
                        }
                    ],
                    "common_phrases": [
                        "начать строиться",
                        "строиться на участке",
                        "собираться строиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To build a house or other structures for oneself."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "build a house",
                                "target_lang_sense_clarification": "implied object"
                            },
                            {
                                "target_lang_word": "build",
                                "target_lang_sense_clarification": "engage in construction"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "łańcuch",
        "łańcuchowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d57ecb7f-89b9-3299-b8b1-e444fe3b9778",
                    "sense_definition": "Element drobnej biżuterii w postaci cienkiego splotu ogniw, noszony zazwyczaj na szyi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "jewelry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dostała na urodziny piękny złoty <w>łańcuszek</w>.",
                            "target_lang_translations": {
                                "ru": "Она получила на день рождения красивую золотую <w>цепочку</w>."
                            }
                        },
                        {
                            "text": "Zgubiłem mój srebrny <w>łańcuszek</w> podczas kąpieli w morzu.",
                            "target_lang_translations": {
                                "ru": "Я потерял свою серебряную <w>цепочку</w>, когда купался в море."
                            }
                        }
                    ],
                    "common_phrases": [
                        "złoty łańcuszek",
                        "srebrny łańcuszek",
                        "łańcuszek z wisiorkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ювелирное изделие в виде тонкого переплетения звеньев, которое обычно носят на шее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цепочка",
                                "target_lang_sense_clarification": "украшение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28190224-99d4-3446-ac6e-6563843bff1a",
                    "sense_definition": "Zdrobienie od: <w>łańcuch</w>; mały, cienki łańcuch łączący elementy lub służący do zabezpieczenia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "general_diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Korek do wanny jest przymocowany na metalowym <w>łańcuszku</w>.",
                            "target_lang_translations": {
                                "ru": "Пробка для ванны прикреплена на металлической <w>цепочке</w>."
                            }
                        },
                        {
                            "text": "Drzwi wejściowe były zamknięte na dodatkowy <w>łańcuszek</w>.",
                            "target_lang_translations": {
                                "ru": "Входная дверь была закрыта на дополнительную <w>цепочку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "reg. śl. lańcuszek"
                    ],
                    "common_phrases": [
                        "łańcuszek do kluczy",
                        "łańcuszek zabezpieczający"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zdrobienie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительное от \"цепь\"; маленькая, тонкая цепь, соединяющая элементы или служащая для защиты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цепочка",
                                "target_lang_sense_clarification": "маленькая цепь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c90116a5-4354-33a8-a797-8d869235ddd7",
                    "sense_definition": "Długa, kolorowa ozdoba choinkowa wykonana z połączonych ogniw papierowych lub tworzywa sztucznego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "decoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci robiły w szkole długi <w>łańcuszek</w> z kolorowego papieru.",
                            "target_lang_translations": {
                                "ru": "Дети делали в школе длинную <w>цепочку</w> из цветной бумаги."
                            }
                        },
                        {
                            "text": "Na naszej choince wisi błyszczący <w>łańcuszek</w>.",
                            "target_lang_translations": {
                                "ru": "На нашей ёлке висит блестящая <w>гирлянда</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "łańcuszek na choinkę",
                        "kolorowy łańcuszek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинное цветное ёлочное украшение, сделанное из соединенных бумажных или пластиковых колец."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гирлянда",
                                "target_lang_sense_clarification": "украшение"
                            },
                            {
                                "target_lang_word": "цепочка",
                                "target_lang_sense_clarification": "из бумаги или колец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "105feb28-1322-3087-a529-d7c56a5cd9b1",
                    "sense_definition": "Wiadomość przesyłana do wielu osób z prośbą o jej dalsze rozpowszechnianie do kolejnych odbiorców.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wierzę w żaden <w>łańcuszek</w> szczęścia, który przychodzi mailem.",
                            "target_lang_translations": {
                                "ru": "Я не верю ни в какие <w>письма счастья</w>, которые приходят по электронной почте."
                            }
                        },
                        {
                            "text": "Ten internetowy <w>łańcuszek</w> krąży w sieci od wielu lat.",
                            "target_lang_translations": {
                                "ru": "Это интернет-<w>письмо счастья</w> бродит по сети уже много лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "łańcuszek szczęścia",
                        "łańcuszek internetowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщение, отправляемое множеству людей с просьбой распространить его дальше следующим получателям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "письмо счастья",
                                "target_lang_sense_clarification": "рассылка"
                            },
                            {
                                "target_lang_word": "цепочка",
                                "target_lang_sense_clarification": "сообщений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf4258b1-4fdb-3236-86d0-65456b659762",
                    "sense_definition": "Rodzaj ściegu krawieckiego lub szydełkowego, który wygląda jak seria połączonych pętelek.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "crafts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wyhaftowała serwetkę ozdobnym <w>łańcuszkiem</w>.",
                            "target_lang_translations": {
                                "ru": "Она вышила салфетку декоративным <w>тамбурным швом</w>."
                            }
                        },
                        {
                            "text": "Nauka szydełkowania zaczyna się zazwyczaj od zrobienia <w>łańcuszka</w>.",
                            "target_lang_translations": {
                                "ru": "Обучение вязанию крючком обычно начинается с создания <w>цепочки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ścieg łańcuszek",
                        "robić łańcuszek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид швейного или вязального стежка, который выглядит как серия соединенных петель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цепочка",
                                "target_lang_sense_clarification": "вязание"
                            },
                            {
                                "target_lang_word": "тамбурный шов",
                                "target_lang_sense_clarification": "вышивка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
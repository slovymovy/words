                {
                    "sense_id": "a1feeee9-22b0-3751-af6c-ad036ee52c3f",
                    "sense_definition": "Betalen met een bankpas via een elektronisch betaalsysteem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "banking_actions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kan ik hier ook contactloos <w>pinnen</w>?",
                            "target_lang_translations": {
                                "en": "Can I also <w>pay by card</w> contactlessly here?",
                                "ru": "Могу я здесь <w>расплатиться картой</w> бесконтактно?"
                            }
                        },
                        {
                            "text": "Zij heeft de boodschappen bij de kassa <w>gepind</w>.",
                            "target_lang_translations": {
                                "en": "She <w>paid by card</w> for the groceries at the checkout.",
                                "ru": "Она <w>оплатила картой</w> покупки на кассе."
                            }
                        }
                    ],
                    "synonyms": [
                        "afrekenen",
                        "betalen"
                    ],
                    "common_phrases": [
                        "contactloos pinnen",
                        "bij de kassa pinnen",
                        "een bedrag pinnen"
                    ],
                    "target_lang_definitions": {
                        "en": "To pay using a debit card via an electronic payment system.",
                        "ru": "Платить банковской картой через электронную платежную систему."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay by card",
                                "target_lang_sense_clarification": "general action"
                            },
                            {
                                "target_lang_word": "pay by debit card",
                                "target_lang_sense_clarification": "specific payment method"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "платить картой",
                                "target_lang_sense_clarification": "совершать оплату безналичным способом"
                            },
                            {
                                "target_lang_word": "расплачиваться",
                                "target_lang_sense_clarification": "отдавать деньги за покупку (обычно картой)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46735ae7-5262-3f56-9e79-466a8dc20d61",
                    "sense_definition": "Contant geld opnemen uit een geldautomaat met een bankpas.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "banking_actions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet even geld <w>pinnen</w> voor de markt.",
                            "target_lang_translations": {
                                "en": "I need to <w>withdraw</w> some cash for the market.",
                                "ru": "Мне нужно <w>снять деньги</w> для рынка."
                            }
                        },
                        {
                            "text": "Hij <w>pinde</w> vijftig euro uit de muur.",
                            "target_lang_translations": {
                                "en": "He <w>withdrew</w> fifty euros from the ATM.",
                                "ru": "Он <w>снял</w> пятьдесят евро в банкомате."
                            }
                        }
                    ],
                    "synonyms": [
                        "opnemen"
                    ],
                    "antonyms": [
                        "storten"
                    ],
                    "common_phrases": [
                        "geld pinnen",
                        "uit de muur pinnen"
                    ],
                    "target_lang_definitions": {
                        "en": "To withdraw cash from an ATM using a bank card.",
                        "ru": "Снимать наличные в банкомате с помощью банковской карты."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "withdraw cash",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "get cash out",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "take out money",
                                "target_lang_sense_clarification": "common usage"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "снимать деньги",
                                "target_lang_sense_clarification": "обналичивать средства"
                            },
                            {
                                "target_lang_word": "снимать наличные",
                                "target_lang_sense_clarification": "получать бумажные деньги в банкомате"
                            }
                        ]
                    }
                },
                {

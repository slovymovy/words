{
    "word_family": [
        "laughter",
        "laughable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "56cb3368-b51c-3eac-9749-5446d13f9d81",
                    "sense_definition": "To make sounds and movements of the face that show happiness or amusement.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "laugh_action_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baby started to <w>laugh</w> when the dog chased its tail.",
                            "target_lang_translations": {
                                "ru": "Ребенок начал <w>смеяться</w>, когда собака погналась за своим хвостом."
                            }
                        },
                        {
                            "text": "I always <w>laugh</w> out loud when I watch this funny movie.",
                            "target_lang_translations": {
                                "ru": "Я всегда громко <w>смеюсь</w>, когда смотрю этот смешной фильм."
                            }
                        }
                    ],
                    "synonyms": [
                        "chuckle",
                        "giggle",
                        "chortle",
                        "guffaw"
                    ],
                    "antonyms": [
                        "cry",
                        "weep"
                    ],
                    "common_phrases": [
                        "laugh out loud",
                        "make someone laugh",
                        "laugh at a joke"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать звуки и совершать движения лицом, выражающие счастье или веселье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смеяться",
                                "target_lang_sense_clarification": "издавать звуки веселья"
                            },
                            {
                                "target_lang_word": "хохотать",
                                "target_lang_sense_clarification": "громко смеяться"
                            },
                            {
                                "target_lang_word": "рассмеяться",
                                "target_lang_sense_clarification": "начать смеяться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "047c41de-250b-3046-9887-4cb26009acaa",
                    "sense_definition": "To treat someone or something with ridicule or scorn; to make fun of.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "laugh_mockery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is very rude to <w>laugh</w> at people when they make a mistake.",
                            "target_lang_translations": {
                                "ru": "Очень грубо <w>смеяться</w> над людьми, когда они совершают ошибку."
                            }
                        },
                        {
                            "text": "They used to <w>laugh</w> at his ideas, but now he is famous.",
                            "target_lang_translations": {
                                "ru": "Раньше они <w>смеялись</w> над его идеями, но теперь он знаменит."
                            }
                        }
                    ],
                    "synonyms": [
                        "snicker",
                        "snigger"
                    ],
                    "common_phrases": [
                        "laugh at someone",
                        "laugh in someone's face"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относиться к кому-то или чему-то с насмешкой или презрением; высмеивать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смеяться",
                                "target_lang_sense_clarification": "подвергать насмешкам"
                            },
                            {
                                "target_lang_word": "насмехаться",
                                "target_lang_sense_clarification": "зло шутить над кем-то"
                            },
                            {
                                "target_lang_word": "высмеивать",
                                "target_lang_sense_clarification": "выставлять в смешном виде"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9a67b7f5-8e80-34ef-aaf9-2c9ca683fdda",
                    "sense_definition": "The sound or act of laughing; an expression of happiness or amusement.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "laugh_action_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her loud <w>laugh</w> filled the room and made everyone smile.",
                            "target_lang_translations": {
                                "ru": "Ее громкий <w>смех</w> наполнил комнату и заставил всех улыбнуться."
                            }
                        },
                        {
                            "text": "We had a good <w>laugh</w> about the misunderstanding yesterday.",
                            "target_lang_translations": {
                                "ru": "Мы от души <w>посмеялись</w> над вчерашним недоразумением."
                            }
                        }
                    ],
                    "synonyms": [
                        "chuckle",
                        "giggle",
                        "guffaw"
                    ],
                    "common_phrases": [
                        "have a laugh",
                        "a loud laugh",
                        "burst into a laugh"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук или действие смеха; выражение счастья или веселья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смех",
                                "target_lang_sense_clarification": "звуки веселья"
                            },
                            {
                                "target_lang_word": "хохот",
                                "target_lang_sense_clarification": "громкий смех"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1991eb84-e4e0-34c5-beff-d301c8f15dc7",
                    "sense_definition": "A person who is fun to be with and has a good sense of humor.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "laugh_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should invite Tom to the party because he is such a good <w>laugh</w>.",
                            "target_lang_translations": {
                                "ru": "Тебе стоит пригласить Тома на вечеринку, потому что он такой <w>весельчак</w>."
                            }
                        },
                        {
                            "text": "She is always a <w>laugh</w> when we go out for dinner.",
                            "target_lang_translations": {
                                "ru": "С ней всегда <w>весело</w>, когда мы идем ужинать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "a good laugh",
                        "be a laugh"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe a fun person"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, с которым весело и у которого хорошее чувство юмора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "весельчак",
                                "target_lang_sense_clarification": "веселый человек"
                            },
                            {
                                "target_lang_word": "забавный",
                                "target_lang_sense_clarification": "смешной человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfa551de-7fe1-31d0-b988-d1633cd65375",
                    "sense_definition": "Something that is funny, ridiculous, or not to be taken seriously.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "laugh_ridiculous",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We only entered the singing competition for a <w>laugh</w>.",
                            "target_lang_translations": {
                                "ru": "Мы записались на конкурс певцов только ради <w>смеха</w>."
                            }
                        },
                        {
                            "text": "The idea that he could win the race is an absolute <w>laugh</w>.",
                            "target_lang_translations": {
                                "ru": "Мысль о том, что он может выиграть гонку — это просто <w>смех</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "joke"
                    ],
                    "common_phrases": [
                        "just for a laugh",
                        "that is a laugh"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe a situation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то смешное, нелепое или то, что нельзя воспринимать всерьез."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смех",
                                "target_lang_sense_clarification": "нечто нелепое"
                            },
                            {
                                "target_lang_word": "умора",
                                "target_lang_sense_clarification": "очень смешная ситуация"
                            },
                            {
                                "target_lang_word": "шутка",
                                "target_lang_sense_clarification": "несерьезное дело"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
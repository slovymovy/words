{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "65fd762a-6c60-3a02-9504-6b602dd55249",
                    "sense_definition": "A casual greeting similar to hi or hello.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Yo</w>, are you coming to the party tonight?",
                            "target_lang_translations": {
                                "ru": "<w>Привет</w>, ты идешь сегодня на вечеринку?"
                            }
                        },
                        {
                            "text": "<w>Yo</w> guys, let's go get some food.",
                            "target_lang_translations": {
                                "ru": "<w>Здоро́во</w>, народ, погнали поедим."
                            }
                        }
                    ],
                    "synonyms": [
                        "hi",
                        "hey",
                        "heyo"
                    ],
                    "common_phrases": [
                        "yo man",
                        "yo dude",
                        "yo what's up"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "very common in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное приветствие, похожее на «привет» или «здравствуйте»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привет",
                                "target_lang_sense_clarification": "стандартное неформальное приветствие"
                            },
                            {
                                "target_lang_word": "йоу",
                                "target_lang_sense_clarification": "сленговое приветствие, заимствованное из английского"
                            },
                            {
                                "target_lang_word": "здоро́во",
                                "target_lang_sense_clarification": "разговорное приветствие, чаще среди мужчин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a42cbb15-9a2d-3ce2-b629-744b09f73f14",
                    "sense_definition": "An exclamation used to get someone's attention.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "attention_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Yo</w>, look at that huge bird over there!",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>, посмотри на ту огромную птицу вон там!"
                            }
                        },
                        {
                            "text": "<w>Yo</w>, listen to what I am saying.",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>, послушай, что я говорю."
                            }
                        }
                    ],
                    "synonyms": [
                        "hey",
                        "oi"
                    ],
                    "common_phrases": [
                        "yo look",
                        "yo listen",
                        "yo wait"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, используемый для привлечения чьего-либо внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эй",
                                "target_lang_sense_clarification": "оклик для привлечения внимания"
                            },
                            {
                                "target_lang_word": "йоу",
                                "target_lang_sense_clarification": "сленговый оклик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e3b2736-543a-3742-a386-91c6c3b65121",
                    "sense_definition": "An expression of surprise, excitement, or approval.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Yo</w>, that trick was absolutely amazing!",
                            "target_lang_translations": {
                                "ru": "<w>Вау</w>, этот трюк был просто потрясающим!"
                            }
                        },
                        {
                            "text": "<w>Yo</w>, I can't believe we actually won the game.",
                            "target_lang_translations": {
                                "ru": "<w>Ого</w>, не могу поверить, что мы реально выиграли игру."
                            }
                        }
                    ],
                    "common_phrases": [
                        "yo that's crazy",
                        "yo no way"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение удивления, восторга или одобрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вау",
                                "target_lang_sense_clarification": "выражение восхищения"
                            },
                            {
                                "target_lang_word": "ого",
                                "target_lang_sense_clarification": "выражение удивления"
                            },
                            {
                                "target_lang_word": "круто",
                                "target_lang_sense_clarification": "выражение одобрения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "793e768f-a990-3815-9d80-1a368ba3bed5",
                    "sense_definition": "Used at the end of a sentence for emphasis.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "emphasis_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This situation is really messed up, <w>yo</w>.",
                            "target_lang_translations": {
                                "ru": "Ситуация реально паршивая, <w>йоу</w>."
                            }
                        },
                        {
                            "text": "I am telling you the truth, <w>yo</w>.",
                            "target_lang_translations": {
                                "ru": "Я говорю тебе правду, <w>йоу</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в конце предложения для эмоционального усиления сказанного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "йоу",
                                "target_lang_sense_clarification": "сленговая частица для ритма или акцента"
                            },
                            {
                                "target_lang_word": "чувак",
                                "target_lang_sense_clarification": "обращение, часто используемое в конце фраз для связки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "596d8b8d-da32-39e2-9dcd-c54a405fe8bf",
                    "sense_definition": "Used to indicate one's presence, for example during a roll call.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "response_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When the teacher called his name, he shouted \"<w>Yo</w>!\" from the back.",
                            "target_lang_translations": {
                                "ru": "Когда учитель назвал его имя, он крикнул: «<w>Я</w>!» с задней парты."
                            }
                        },
                        {
                            "text": "Is everyone here? <w>Yo</w>!",
                            "target_lang_translations": {
                                "ru": "Все здесь? <w>Я</w>!"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения своего присутствия, например, во время переклички."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "я",
                                "target_lang_sense_clarification": "стандартный ответ на перекличке"
                            },
                            {
                                "target_lang_word": "здесь",
                                "target_lang_sense_clarification": "подтверждение присутствия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "adf5860d-e0cf-3425-8423-7689af33eea6",
                    "sense_definition": "Pronunciation spelling of <w>your</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Is that <w>yo</w> car parked outside?",
                            "target_lang_translations": {
                                "ru": "Это <w>твоя</w> машина припаркована снаружи?"
                            }
                        },
                        {
                            "text": "Make sure you finish <w>yo</w> homework before dinner.",
                            "target_lang_translations": {
                                "ru": "Убедись, что ты закончил <w>свою</w> домашнюю работу перед ужином."
                            }
                        }
                    ],
                    "common_phrases": [
                        "yo mama"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "representing AAVE or casual speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фонетическое написание слова «your» (твой), характерное для разговорной речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "твой",
                                "target_lang_sense_clarification": "притяжательное местоимение"
                            },
                            {
                                "target_lang_word": "свой",
                                "target_lang_sense_clarification": "возвратное притяжательное местоимение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "7732b072-f208-3909-a6e1-8e82845708ac",
                    "sense_definition": "A gender-neutral third-person singular pronoun used to refer to a person (he or she).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Yo</w> was walking down the street when I saw him.",
                            "target_lang_translations": {
                                "ru": "<w>Он</w> шел по улице, когда я увидел его."
                            }
                        },
                        {
                            "text": "I don't know where <w>yo</w> went after the meeting.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, куда <w>он</w> пошел после собрания."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Baltimore / AAVE"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гендерно-нейтральное местоимение третьего лица (он или она), используемое в некоторых диалектах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "он",
                                "target_lang_sense_clarification": "мужской род"
                            },
                            {
                                "target_lang_word": "она",
                                "target_lang_sense_clarification": "женский род"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "00d4bba6-1561-3bfb-b0f7-c28b39b88280",
                    "sense_definition": "A knitting technique where the yarn is passed over the needle to create a loop or hole.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>yo</w> before knitting the next two stitches together.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно сделать <w>накид</w> перед тем, как провязать следующие две петли вместе."
                            }
                        },
                        {
                            "text": "The pattern calls for a <w>yo</w> to create a decorative eyelet.",
                            "target_lang_translations": {
                                "ru": "Узор требует <w>накида</w> для создания декоративного отверстия."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation for yarn over"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Техника вязания, при которой нить накидывается на спицу для создания петли или отверстия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накид",
                                "target_lang_sense_clarification": "термин вязания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "9a098fbc-9a90-3986-adb7-aa873be2d7da",
                    "sense_definition": "Initialism of the Special Region of Yogyakarta, a province of Indonesia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We are planning a trip to <w>Yo</w> next summer.",
                            "target_lang_translations": {
                                "ru": "Мы планируем поездку в <w>Джокьякарту</w> следующим летом."
                            }
                        },
                        {
                            "text": "The culture in <w>Yo</w> is very rich and traditional.",
                            "target_lang_translations": {
                                "ru": "Культура в <w>Джокьякарте</w> очень богатая и традиционная."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от названия особого округа Джокьякарта, провинции Индонезии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Джокьякарта",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
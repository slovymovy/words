            "pos": "verb",
            "senses": [
                {
                    "sense_id": "655a4582-0166-3192-900c-ef2509e5929e",
                    "sense_definition": "Mówić rzeczy zabawne, opowiadać dowcipy w celu rozśmieszenia kogoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "humor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój wujek uwielbia <żartować> i zawsze rozbawia całą rodzinę.",
                            "target_lang_translations": {
                                "ru": "Мой дядя обожает <w>шутить</w> и всегда веселит всю семью."
                            }
                        },
                        {
                            "text": "Przestań ciągle <żartować> i zajmij się pracą.",
                            "target_lang_translations": {
                                "ru": "Перестань постоянно <w>шутить</w> и займись работой."
                            }
                        }
                    ],
                    "synonyms": [
                        "dowcipkować"
                    ],
                    "common_phrases": [
                        "lubić żartować",
                        "często żartować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить что-то смешное, рассказывать анекдоты с целью рассмешить кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шутить",
                                "target_lang_sense_clarification": "рассказывать шутки"
                            },
                            {
                                "target_lang_word": "острить",
                                "target_lang_sense_clarification": "говорить остроумно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8813d0b3-d332-3303-aa00-969d759a5c92",
                    "sense_definition": "Mówić nieprawdę dla zabawy, wprowadzać kogoś w błąd w celach humorystycznych; nie mówić poważnie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "humor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bierz tego do siebie, on tylko <żartował>.",
                            "target_lang_translations": {
                                "ru": "Не принимай это близко к сердцу, он просто <w>шутил</w>."
                            }
                        },
                        {
                            "text": "Chyba sobie <żartujesz>, to nie może być prawda.",
                            "target_lang_translations": {
                                "ru": "Ты, наверное, <w>шутишь</w>, это не может быть правдой."
                            }
                        }
                    ],
                    "synonyms": [
                        "droczyć się"
                    ],
                    "common_phrases": [
                        "chyba żartujesz",
                        "żartować sobie",
                        "żartować z kogoś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить неправду ради забавы; говорить не всерьез."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шутить",
                                "target_lang_sense_clarification": "говорить не всерьез"
                            },
                            {
                                "target_lang_word": "разыгрывать",
                                "target_lang_sense_clarification": "подшучивать над кем-то"
                            }
                        ]
                    }
                }
            ]
        }

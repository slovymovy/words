                },
                {
                    "sense_id": "28b1b499-8b22-39c9-9fac-736a537eb225",
                    "sense_definition": "О физическом ощущении холода, которое испытывает человек",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "feeling_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Закрой, пожалуйста, окно, мне немного <w>прохладно</w>.",
                            "target_lang_translations": {
                                "en": "Please close the window, I feel a bit <w>chilly</w>."
                            }
                        },
                        {
                            "text": "Если тебе будет <w>прохладно</w>, надень мою куртку.",
                            "target_lang_translations": {
                                "en": "If you feel <w>chilly</w>, put on my jacket."
                            }
                        }
                    ],
                    "synonyms": [
                        "зябко",
                        "холодно"
                    ],
                    "antonyms": [
                        "тепло",
                        "жарко"
                    ],
                    "common_phrases": [
                        "мне прохладно",
                        "ему прохладно"
                    ],
                    "target_lang_definitions": {
                        "en": "Regarding a physical sensation of cold experienced by a person"
                    },
                    "translations": {

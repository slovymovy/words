    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "79094f1f-4717-32f9-b475-ebc2ba8feb90",
                    "sense_definition": "Действующее лицо художественного произведения (книги, спектакля, фильма, игры).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Главный <w>персонаж</w> фильма спасает мир от катастрофы.",
                            "target_lang_translations": {
                                "en": "The main <w>character</w> of the movie saves the world from catastrophe."
                            }
                        },
                        {
                            "text": "Этот вымышленный <w>персонаж</w> стал популярнее реальных актёров.",
                            "target_lang_translations": {
                                "en": "This fictional <w>character</w> became more popular than real actors."
                            }
                        }
                    ],
                    "synonyms": [
                        "герой",
                        "лицо"
                    ],
                    "antonyms": [
                        "прототип"
                    ],
                    "common_phrases": [
                        "главный персонаж",
                        "вымышленный персонаж",
                        "второстепенный персонаж",
                        "отрицательный персонаж"
                    ],
                    "target_lang_definitions": {
                        "en": "A person or being involved in the action of a story, movie, play, or game."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "character",
                                "target_lang_sense_clarification": "in a story or play"
                            },
                            {
                                "target_lang_word": "personage",
                                "target_lang_sense_clarification": "literary term for a character"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b659b40c-1c33-3eec-b10a-1bb176bc8615",
                    "sense_definition": "Человек, личность (часто с оттенком иронии или о ком-то странном, выделяющемся).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На вечеринке появился довольно странный <w>персонаж</w> в шляпе.",
                            "target_lang_translations": {
                                "en": "A rather strange <w>character</w> in a hat appeared at the party."
                            }
                        },
                        {
                            "text": "Он известный в городе <w>персонаж</w>, о котором ходит много слухов.",
                            "target_lang_translations": {
                                "en": "He is a well-known <w>character</w> in the city about whom many rumors circulate."
                            }
                        }
                    ],
                    "synonyms": [
                        "особа",
                        "личность"
                    ],
                    "common_phrases": [
                        "странный персонаж",
                        "известный персонаж",
                        "забавный персонаж"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe a real person, often ironically"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person or individual, often implying they are eccentric, distinctive, or notable."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "character",
                                "target_lang_sense_clarification": "eccentric or notable person"
                            },
                            {
                                "target_lang_word": "personage",
                                "target_lang_sense_clarification": "individual (often ironic or formal)"
                            }
                        ]
                    }
                }
            ]
        }

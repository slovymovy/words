{
    "word_family": [
        "accompaniment",
        "accompanist"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "026f738d-5f22-3d93-a540-bcef407c5b24",
                    "sense_definition": "To go somewhere with someone as a companion or helper.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My friend decided to <w>accompany</w> me to the doctor because I was nervous.",
                            "target_lang_translations": {
                                "ru": "Мой друг решил <w>сопроводить</w> меня к врачу, потому что я нервничал."
                            }
                        },
                        {
                            "text": "Children under the age of ten must be <w>accompanied</w> by an adult.",
                            "target_lang_translations": {
                                "ru": "Детей младше десяти лет должны <w>сопровождать</w> взрослые."
                            }
                        }
                    ],
                    "synonyms": [
                        "go with",
                        "escort",
                        "attend"
                    ],
                    "common_phrases": [
                        "accompany a friend",
                        "accompany a group",
                        "accompany someone home",
                        "be accompanied by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идти или ехать куда-либо с кем-то в качестве спутника или помощника"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопровождать",
                                "target_lang_sense_clarification": "следовать вместе с кем-то"
                            },
                            {
                                "target_lang_word": "сопроводить",
                                "target_lang_sense_clarification": "совершенный вид: пойти вместе с кем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40328a43-f463-3fd1-936d-b77eeaae39c4",
                    "sense_definition": "To happen, appear, or exist at the same time as something else.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thunder almost always <w>accompanies</w> lightning during a heavy storm.",
                            "target_lang_translations": {
                                "ru": "Гром почти всегда <w>сопровождает</w> молнию во время сильной грозы."
                            }
                        },
                        {
                            "text": "A high fever often <w>accompanies</w> the flu and other viral infections.",
                            "target_lang_translations": {
                                "ru": "Высокая температура часто <w>сопровождает</w> грипп и другие вирусные инфекции."
                            }
                        }
                    ],
                    "synonyms": [
                        "he attends",
                        "go with",
                        "he attends"
                    ],
                    "common_phrases": [
                        "accompany a change",
                        "symptoms accompany",
                        "accompany the growth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить, появляться или существовать одновременно с чем-либо другим"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопровождать",
                                "target_lang_sense_clarification": "происходить одновременно"
                            },
                            {
                                "target_lang_word": "сопутствовать",
                                "target_lang_sense_clarification": "быть неразрывно связанным, идти вместе (об удаче, обстоятельствах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4655cab6-2068-3267-a2fe-95fdda9e894a",
                    "sense_definition": "To play a musical instrument to support a singer or another musician.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pianist agreed to <w>accompany</w> the choir during their final performance.",
                            "target_lang_translations": {
                                "ru": "Пианист согласился <w>аккомпанировать</w> хору во время их финального выступления."
                            }
                        },
                        {
                            "text": "She asked her brother to <w>accompany</w> her on the guitar while she sang.",
                            "target_lang_translations": {
                                "ru": "Она попросила брата <w>аккомпанировать</w> ей на гитаре, пока она пела."
                            }
                        }
                    ],
                    "common_phrases": [
                        "accompany on the piano",
                        "accompany a singer",
                        "accompany on guitar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Играть на музыкальном инструменте для сопровождения певца или другого музыканта"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аккомпанировать",
                                "target_lang_sense_clarification": "подыгрывать на инструменте"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
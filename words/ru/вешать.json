    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "97fb4747-5f0a-3f21-9ec0-d78b8db890c0",
                    "sense_definition": "Помещать предмет так, чтобы он свисал вниз, не касаясь пола, обычно прикрепляя к чему-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_hang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я всегда <вешаю> свою куртку на крючок в прихожей.",
                            "target_lang_translations": {
                                "en": "I always <hang> my jacket on the hook in the hallway."
                            }
                        },
                        {
                            "text": "Мы решили <вешать> новые картины на эту стену.",
                            "target_lang_translations": {
                                "en": "We decided to <hang> new paintings on this wall."
                            }
                        }
                    ],
                    "synonyms": [
                        "подвешивать"
                    ],
                    "antonyms": [
                        "снимать"
                    ],
                    "common_phrases": [
                        "вешать одежду",
                        "вешать бельё",
                        "вешать на стену",
                        "вешать на гвоздь"
                    ],
                    "target_lang_definitions": {
                        "en": "To place an object so that it is suspended from above, usually by attaching it to a hook or nail."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hang",
                                "target_lang_sense_clarification": "to suspend something"
                            },
                            {
                                "target_lang_word": "hang up",
                                "target_lang_sense_clarification": "to put on a hook or hanger"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03eb1c00-d1d7-32a4-85e1-0b80be3b1855",
                    "sense_definition": "Определять вес чего-либо с помощью весов.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_weigh",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Продавец начал <вешать> яблоки для покупателя.",
                            "target_lang_translations": {
                                "en": "The seller started to <weigh> the apples for the customer."
                            }
                        },
                        {
                            "text": "На рынке принято <вешать> продукты очень точно.",
                            "target_lang_translations": {
                                "en": "At the market, it is customary to <weigh> products very precisely."
                            }
                        }
                    ],
                    "synonyms": [
                        "взвешивать"
                    ],
                    "common_phrases": [
                        "вешать овощи",
                        "вешать в граммах",
                        "вешать товар"
                    ],
                    "target_lang_definitions": {
                        "en": "To measure how heavy something is using scales."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weigh",
                                "target_lang_sense_clarification": "to measure weight"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "472a2717-ce95-3c61-9f0b-a2e49cc4bfa2",
                    "sense_definition": "Подвергать смертной казни через повешение.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_hang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старину пиратов могли <вешать> прямо на кораблях.",
                            "target_lang_translations": {
                                "en": "In the old days, they could <hang> pirates right on the ships."
                            }
                        },
                        {
                            "text": "Судья приговорил <вешать> особо опасных преступников.",
                            "target_lang_translations": {
                                "en": "The judge ordered to <hang> particularly dangerous criminals."
                            }
                        }
                    ],
                    "common_phrases": [
                        "приговорить вешать"
                    ],
                    "target_lang_definitions": {
                        "en": "To execute someone by suspending them by the neck."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hang",
                                "target_lang_sense_clarification": "execution method"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e53ea26-b5a3-3fe1-8c12-fad11f07a6c7",
                    "sense_definition": "Перекладывать ответственность, вину или неприятные обязанности на другого человека.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_hang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не нужно <вешать> на меня свои проблемы и ошибки.",
                            "target_lang_translations": {
                                "en": "You don't need to <dump> your problems and mistakes on me."
                            }
                        },
                        {
                            "text": "Начальник любит <вешать> всех собак на своих подчинённых.",
                            "target_lang_translations": {
                                "en": "The boss likes to <pin> all the blame on his subordinates."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вешать всех собак",
                        "вешать ярлыки",
                        "вешать долги",
                        "вешать нос"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "неодобрительно"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To unfairly shift responsibility, blame, or unpleasant tasks onto another person."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pin",
                                "target_lang_sense_clarification": "to place blame on someone"
                            },
                            {
                                "target_lang_word": "dump",
                                "target_lang_sense_clarification": "to unload problems on someone"
                            },
                            {
                                "target_lang_word": "hang",
                                "target_lang_sense_clarification": "figurative, to attribute blame"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
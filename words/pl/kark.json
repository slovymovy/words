            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ef592ef4-63a7-32b8-b054-eeb320a5bfa2",
                    "sense_definition": "Tylna część szyi, znajdująca się powyżej pleców.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po całym dniu pracy przy biurku strasznie boli mnie <w>kark</w>.",
                            "target_lang_translations": {
                                "ru": "После целого дня работы за столом у меня ужасно болит <w>шея</w>."
                            }
                        },
                        {
                            "text": "Cześnik nieznacznie dotknął ręką <w>kark</w> klaczy, sprawdzając jej kondycję.",
                            "target_lang_translations": {
                                "ru": "Чесник слегка коснулся рукой <w>загривка</w> кобылы, проверяя её состояние."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieć na karku",
                        "skręcić kark",
                        "zgiąć kark",
                        "nadstawiać karku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Задняя часть шеи, расположенная выше спины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шея",
                                "target_lang_sense_clarification": "часть тела"
                            },
                            {
                                "target_lang_word": "загривок",
                                "target_lang_sense_clarification": "у животных или разг. у человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c524800-32fd-32f7-8e73-badaa42f7c34",
                    "sense_definition": "Mocno umięśniony mężczyzna, często kojarzony z siłą fizyczną i agresywnym zachowaniem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wejściem do klubu stał wielki <w>kark</w> i pilnował porządku.",
                            "target_lang_translations": {
                                "ru": "Перед входом в клуб стоял огромный <w>качок</w> и следил за порядком."
                            }
                        },
                        {
                            "text": "Ten <w>kark</w> spędza na siłowni każdą wolną chwilę, żeby budować masę.",
                            "target_lang_translations": {
                                "ru": "Этот <w>качок</w> проводит в спортзале каждую свободную минуту, чтобы набрать массу."
                            }
                        }
                    ],
                    "synonyms": [
                        "mięśniak",
                        "osiłek"
                    ],
                    "common_phrases": [
                        "napakowany kark",
                        "wielki kark"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мускулистый мужчина, часто ассоциируемый с физической силой и агрессивным поведением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "качок",
                                "target_lang_sense_clarification": "разг."
                            },
                            {
                                "target_lang_word": "амбал",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8202cddd-333c-3701-917a-f2482d72147f",
                    "sense_definition": "Wąska część wału wiatraka, na której opiera się on i obraca.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas renowacji zabytkowego młyna wymieniono zniszczony <w>kark</w> wału.",
                            "target_lang_translations": {
                                "ru": "Во время реставрации старинной мельницы заменили разрушенную <w>шейку</w> вала."
                            }
                        },
                        {
                            "text": "Drewniany <w>kark</w> musi być regularnie smarowany, aby wiatrak działał płynnie.",
                            "target_lang_translations": {
                                "ru": "Деревянная <w>шейка</w> должна регулярно смазываться, чтобы мельница работала плавно."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкая часть вала ветряной мельницы, на которую он опирается и вращается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шейка",
                                "target_lang_sense_clarification": "техн."
                            }
                        ]
                    }
                }
            ]
        }
    ]

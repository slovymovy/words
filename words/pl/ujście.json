    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9de10eb2-4a1e-3b3c-96ba-41becd8fc850",
                    "sense_definition": "Miejsce, w którym rzeka lub inny ciek wodny wpada do większego zbiornika wodnego (morza, jeziora) lub innej rzeki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wisła ma swoje <w>ujście</w> w Morzu Bałtyckim, tworząc szeroką deltę.",
                            "target_lang_translations": {
                                "ru": "Висла имеет свое <w>устье</w> в Балтийском море, образуя широкую дельту."
                            }
                        },
                        {
                            "text": "Wędkarze często spotykają się przy <w>ujściu</w> rzeki, gdzie woda jest głębsza.",
                            "target_lang_translations": {
                                "ru": "Рыбаки часто встречаются у <w>устья</w> реки, где вода глубже."
                            }
                        }
                    ],
                    "antonyms": [
                        "źródło"
                    ],
                    "common_phrases": [
                        "ujście rzeki",
                        "u ujścia",
                        "szerokie ujście",
                        "ujście Wisły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где река впадает в море, озеро или другую реку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устье",
                                "target_lang_sense_clarification": "реки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5643d900-0307-33d0-a745-219ca5afbc33",
                    "sense_definition": "Otwór lub miejsce, przez które coś wypływa lub wydostaje się na zewnątrz; także w sensie przenośnym sposób na rozładowanie emocji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "general_outlet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz znaleźć jakieś zdrowe <w>ujście</w> dla swojego gniewu i frustracji.",
                            "target_lang_translations": {
                                "ru": "Ты должен найти какой-то здоровый <w>выход</w> для своего гнева и разочарования."
                            }
                        },
                        {
                            "text": "Zatkane <w>ujście</w> rury kanalizacyjnej spowodowało zalanie piwnicy.",
                            "target_lang_translations": {
                                "ru": "Засоренный <w>выход</w> канализационной трубы привел к затоплению подвала."
                            }
                        }
                    ],
                    "synonyms": [
                        "wylot",
                        "upust",
                        "wyjście"
                    ],
                    "antonyms": [
                        "wlot"
                    ],
                    "common_phrases": [
                        "znaleźć ujście",
                        "ujście emocji",
                        "ujście lufy",
                        "ujście gazu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отверстие для выхода чего-либо наружу или способ разрядки эмоций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выход",
                                "target_lang_sense_clarification": "отверстие; разрядка эмоций"
                            },
                            {
                                "target_lang_word": "отдушина",
                                "target_lang_sense_clarification": "для чувств, эмоций"
                            },
                            {
                                "target_lang_word": "устье",
                                "target_lang_sense_clarification": "техническое отверстие, жерло"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2942a929-d4ff-3ebf-bd95-4fd0ffa5578d",
                    "sense_definition": "Rzeczownik odczasownikowy od słowa <w>ujść</w>, oznaczający proces wydostawania się, ucieczki lub ubytku czegoś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gwałtowne <w>ujście</w> pary z kotła wywołało ogromny hałas w fabryce.",
                            "target_lang_translations": {
                                "ru": "Резкий <w>выход</w> пара из котла вызвал огромный шум на заводе."
                            }
                        },
                        {
                            "text": "Niekontrolowane <w>ujście</w> tajnych informacji zaszkodziło reputacji firmy.",
                            "target_lang_translations": {
                                "ru": "Неконтролируемая <w>утечка</w> секретной информации навредила репутации компании."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "rzeczownik odczasownikowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс выхода наружу, истечения или утечки чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утечка",
                                "target_lang_sense_clarification": "информации, газа"
                            },
                            {
                                "target_lang_word": "выход",
                                "target_lang_sense_clarification": "пара, газов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "83d87e1c-8e22-39ff-811a-43a65bd3dcc2",
                    "sense_definition": "Miasto w zachodniej Polsce, w województwie wielkopolskim, położone przy ujściu rzeki Gwdy do Noteci.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Moi dziadkowie przeprowadzili się do <w>Ujścia</w> zaraz po wojnie.",
                            "target_lang_translations": {
                                "ru": "Мои дедушка и бабушка переехали в <w>Уйсце</w> сразу после войны."
                            }
                        },
                        {
                            "text": "Burmistrz <w>Ujścia</w> ogłosił nowy plan rewitalizacji rynku miejskiego.",
                            "target_lang_translations": {
                                "ru": "Мэр <w>Уйсце</w> объявил о новом плане ревитализации городской площади."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в западной Польше, в Великопольском воеводстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Уйсце",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                }
            ]
        }

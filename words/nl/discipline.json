{
    "word_family": [
        "disciplinair",
        "disciplineren"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f4c22b12-b61d-302c-903f-cb42af623851",
                    "sense_definition": "Het vermogen om je eigen gedrag en verlangens onder controle te houden om een doel te bereiken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "self_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je hebt veel <w>discipline</w> nodig om elke dag te studeren voor je examen.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно много <w>дисциплины</w>, чтобы каждый день готовиться к экзамену."
                            }
                        },
                        {
                            "text": "Dankzij zijn ijzeren <w>discipline</w> wist hij zijn conditie snel te verbeteren.",
                            "target_lang_translations": {
                                "ru": "Благодаря своей железной <w>дисциплине</w> он смог быстро улучшить свою физическую форму."
                            }
                        }
                    ],
                    "synonyms": [
                        "zelfbeheersing",
                        "zelfcontrole"
                    ],
                    "common_phrases": [
                        "ijzeren discipline",
                        "discipline opbrengen",
                        "gebrek aan discipline"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность контролировать свое поведение и желания для достижения цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самодисциплина",
                                "target_lang_sense_clarification": "контроль над собой"
                            },
                            {
                                "target_lang_word": "дисциплина",
                                "target_lang_sense_clarification": "организованность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "653d6202-08a9-3a95-8ab0-221913436fc1",
                    "sense_definition": "Het strikt gehoorzamen aan regels, voorschriften en bevelen, vaak binnen een organisatie of groep.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "obedience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar probeerde met strenge regels de <w>discipline</w> in de klas te herstellen.",
                            "target_lang_translations": {
                                "ru": "Учитель пытался восстановить <w>дисциплину</w> в классе с помощью строгих правил."
                            }
                        },
                        {
                            "text": "In het leger heerst een strakke <w>discipline</w> die voor iedereen geldt.",
                            "target_lang_translations": {
                                "ru": "В армии царит строгая <w>дисциплина</w>, обязательная для всех."
                            }
                        }
                    ],
                    "synonyms": [
                        "tucht"
                    ],
                    "common_phrases": [
                        "strakke discipline",
                        "orde en discipline",
                        "militaire discipline"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строгое соблюдение правил и предписаний, часто внутри организации или группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дисциплина",
                                "target_lang_sense_clarification": "соблюдение порядка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f37a7595-f587-3625-89c5-14174cc0319e",
                    "sense_definition": "Een specifiek onderdeel of vakgebied binnen de wetenschap, kunst of sport.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "branch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is gespecialiseerd in een medische <w>discipline</w> die zich richt op het hart.",
                            "target_lang_translations": {
                                "ru": "Она специализируется на медицинской <w>дисциплине</w>, связанной с сердцем."
                            }
                        },
                        {
                            "text": "Bij de Olympische Spelen is zwemmen een populaire <w>discipline</w>.",
                            "target_lang_translations": {
                                "ru": "На Олимпийских играх плавание — популярная <w>дисциплина</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tak"
                    ],
                    "common_phrases": [
                        "olympische discipline",
                        "wetenschappelijke discipline",
                        "nieuwe discipline"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная область знаний, науки или вид спорта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дисциплина",
                                "target_lang_sense_clarification": "вид спорта или науки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
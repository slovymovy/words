                    ],
                    "common_phrases": [
                        "interactive process",
                        "interactive teaching",
                        "interactive approach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предполагающий общение или взаимодействие между людьми или предметами, при котором они влияют друг на друга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интерактивный",
                                "target_lang_sense_clarification": "основанный на взаимодействии"
                            },
                            {
                                "target_lang_word": "взаимодействующий",
                                "target_lang_sense_clarification": "влияющий друг на друга"
                            }
                        ]
                    }
                }
            ]
        },
        {

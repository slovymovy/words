{
    "word_family": [
        "foreigner",
        "foreignness",
        "foreignize"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "639b6e05-6feb-30e3-a356-4251ec18a888",
                    "sense_definition": "Coming from, located in, or belonging to a country that is not one's own.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geopolitical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She speaks three <w>foreign</w> languages fluently.",
                            "target_lang_translations": {
                                "ru": "Она свободно говорит на трех <w>иностранных</w> языках."
                            }
                        },
                        {
                            "text": "We enjoy trying <w>foreign</w> food when we travel abroad.",
                            "target_lang_translations": {
                                "ru": "Нам нравится пробовать <w>иностранную</w> еду, когда мы путешествуем за границу."
                            }
                        },
                        {
                            "text": "Many <w>foreign</w> students come to this university to study engineering.",
                            "target_lang_translations": {
                                "ru": "Многие <w>иностранные</w> студенты приезжают в этот университет изучать инженерное дело."
                            }
                        }
                    ],
                    "synonyms": [
                        "overseas",
                        "international"
                    ],
                    "antonyms": [
                        "domestic",
                        "native"
                    ],
                    "common_phrases": [
                        "foreign language",
                        "foreign country",
                        "foreign currency",
                        "foreign culture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий из другой страны, находящийся в ней или принадлежащий ей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иностра́нный",
                                "target_lang_sense_clarification": "относящийся к другому государству, гражданам или языкам"
                            },
                            {
                                "target_lang_word": "зарубе́жный",
                                "target_lang_sense_clarification": "находящийся или изданный за границей"
                            },
                            {
                                "target_lang_word": "заграни́чный",
                                "target_lang_sense_clarification": "привезенный из-за границы или относящийся к поездкам туда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "689ba1a4-0c78-3474-83e0-d52cf8e7603f",
                    "sense_definition": "Relating to government dealings or relationships with other nations.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "geopolitical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The president announced a significant change in <w>foreign</w> policy.",
                            "target_lang_translations": {
                                "ru": "Президент объявил о значительных изменениях во <w>внешней</w> политике."
                            }
                        },
                        {
                            "text": "The minister of <w>foreign</w> affairs met with diplomats yesterday.",
                            "target_lang_translations": {
                                "ru": "Министр <w>иностранных</w> дел встретился с дипломатами вчера."
                            }
                        }
                    ],
                    "synonyms": [
                        "international"
                    ],
                    "antonyms": [
                        "domestic"
                    ],
                    "common_phrases": [
                        "foreign policy",
                        "foreign affairs",
                        "foreign trade",
                        "foreign minister"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к правительственным делам или отношениям с другими государствами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вне́шний",
                                "target_lang_sense_clarification": "в контексте политики и отношений (внешняя политика)"
                            },
                            {
                                "target_lang_word": "иностра́нный",
                                "target_lang_sense_clarification": "в названиях ведомств (Министерство иностранных дел)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9db47e0-9e39-3389-b889-9e5c8c1d6960",
                    "sense_definition": "Coming from outside a system or living organism and not naturally belonging to it.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "external_alien",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor had to remove a <w>foreign</w> body from the patient's eye.",
                            "target_lang_translations": {
                                "ru": "Врачу пришлось извлечь <w>инородное</w> тело из глаза пациента."
                            }
                        },
                        {
                            "text": "The immune system is designed to attack <w>foreign</w> substances.",
                            "target_lang_translations": {
                                "ru": "Иммунная система предназначена для атаки на <w>инородные</w> вещества."
                            }
                        }
                    ],
                    "synonyms": [
                        "alien",
                        "extraneous"
                    ],
                    "antonyms": [
                        "natural",
                        "innate"
                    ],
                    "common_phrases": [
                        "foreign body",
                        "foreign substance",
                        "foreign object"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical or medical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приходящий извне системы или живого организма и не свойственный ему естественным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иноро́дный",
                                "target_lang_sense_clarification": "чуждый организму или среде (тело, предмет)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "664db693-8aa2-3fe1-97fa-4635cd7615c5",
                    "sense_definition": "Strange or unfamiliar to someone's experience, nature, or habits.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "external_alien",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The concept of relaxing on a beach was completely <w>foreign</w> to him.",
                            "target_lang_translations": {
                                "ru": "Концепция отдыха на пляже была ему совершенно <w>чужда</w>."
                            }
                        },
                        {
                            "text": "Dishonesty is <w>foreign</w> to her nature.",
                            "target_lang_translations": {
                                "ru": "Нечестность <w>чужда</w> ее натуре."
                            }
                        }
                    ],
                    "synonyms": [
                        "alien",
                        "strange"
                    ],
                    "antonyms": [
                        "natural",
                        "innate"
                    ],
                    "common_phrases": [
                        "foreign to someone",
                        "foreign to one's nature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Странный или непривычный для чьего-либо опыта, натуры или привычек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чу́ждый",
                                "target_lang_sense_clarification": "далекий по духу, несвойственный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cece078c-34af-3750-878e-ca3f678d5b1e",
                    "sense_definition": "A luxury car manufactured in a foreign country.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He likes to drive fast <w>foreigns</w> on the weekend.",
                            "target_lang_translations": {
                                "ru": "Ему нравится гонять на быстрых <w>иномарках</w> по выходным."
                            }
                        },
                        {
                            "text": "The music video featured expensive jewelry and shiny <w>foreigns</w>.",
                            "target_lang_translations": {
                                "ru": "В музыкальном клипе были дорогие украшения и блестящие <w>иномарки</w>."
                            }

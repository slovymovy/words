            "senses": [
                {
                    "sense_id": "9c3fcc2e-b412-3574-b359-89f76ed28b06",
                    "sense_definition": "Inhouden, bevatten of als onderdeel hebben.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "containment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De cursus zal verschillende onderwerpen <w>omvatten</w>, waaronder grammatica en spreekvaardigheid.",
                            "target_lang_translations": {
                                "ru": "Курс будет <w>охватывать</w> различные темы, включая грамматику и навыки устной речи.",
                                "en": "The course will <w>include</w> various topics, including grammar and speaking skills."
                            }
                        },
                        {
                            "text": "Zijn takenpakket <w>omvat</w> het beheren van de agenda en het beantwoorden van e-mails.",
                            "target_lang_translations": {
                                "ru": "Его круг обязанностей <w>включает</w> ведение расписания и ответы на электронные письма.",
                                "en": "His duties <w>include</w> managing the agenda and answering emails."
                            }
                        }
                    ],
                    "synonyms": [
                        "bevatten",
                        "inhouden",
                        "insluiten",
                        "behelzen"
                    ],
                    "antonyms": [
                        "uitsluiten"
                    ],
                    "common_phrases": [
                        "het plan omvat",
                        "kosten omvatten",
                        "maatregelen omvatten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержать, включать в себя или иметь в качестве составной части.",
                        "en": "To contain, include, or have as a part."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "включать",
                                "target_lang_sense_clarification": "содержать как часть целого"
                            },
                            {
                                "target_lang_word": "охватывать",
                                "target_lang_sense_clarification": "распространяться на что-либо, касаться множества тем"
                            },
                            {
                                "target_lang_word": "содержать",
                                "target_lang_sense_clarification": "иметь внутри себя"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "include",
                                "target_lang_sense_clarification": "to contain as part of a whole"
                            },
                            {
                                "target_lang_word": "encompass",
                                "target_lang_sense_clarification": "to include comprehensively"
                            },
                            {
                                "target_lang_word": "comprise",
                                "target_lang_sense_clarification": "to consist of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "867fc2db-1c8b-38b5-b18e-42dad7a11577",
                    "sense_definition": "Ergens omheen grijpen of het aan alle kanten omgeven.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "envelopment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <w>omvatte</w> de warme mok met beide handen om op te warmen.",
                            "target_lang_translations": {
                                "ru": "Она <w>обхватила</w> теплую кружку обеими руками, чтобы согреться.",
                                "en": "She <w>clasped</w> the warm mug with both hands to warm up."
                            }
                        },
                        {
                            "text": "De oude stadsmuren <w>omvatten</w> het historische centrum volledig.",
                            "target_lang_translations": {
                                "ru": "Старые городские стены полностью <w>окружают</w> исторический центр.",
                                "en": "The old city walls completely <w>surround</w> the historic center."
                            }
                        }
                    ],
                    "synonyms": [
                        "omsluiten",
                        "omarmen",
                        "omringen"
                    ],
                    "common_phrases": [
                        "met handen omvatten",
                        "blik omvatten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обхватывать что-либо или окружать со всех сторон.",
                        "en": "To grasp around something or surround it on all sides."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обхватывать",
                                "target_lang_sense_clarification": "брать руками вокруг"
                            },
                            {
                                "target_lang_word": "окружать",
                                "target_lang_sense_clarification": "находиться вокруг чего-либо"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "clasp",
                                "target_lang_sense_clarification": "to hold with hands around something"
                            },
                            {
                                "target_lang_word": "surround",
                                "target_lang_sense_clarification": "to be present on all sides"
                            },
                            {
                                "target_lang_word": "enclose",
                                "target_lang_sense_clarification": "to shut in on all sides"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "102e9011-64db-3580-9030-fa80c0ed6374",
                    "sense_definition": "Każda z dwóch bocznych części twarzy, znajdująca się poniżej oka i obok nosa.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko miało czerwone <w>policzki</w> od mrozu i wiatru.",
                            "target_lang_translations": {
                                "ru": "У ребенка были красные <w>щёки</w> от мороза и ветра."
                            }
                        },
                        {
                            "text": "Na pożegnanie pocałował ją delikatnie w prawy <w>policzek</w>.",
                            "target_lang_translations": {
                                "ru": "На прощание он нежно поцеловал её в правую <w>щёку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "polik",
                        "jagoda"
                    ],
                    "common_phrases": [
                        "nadstawić drugi policzek",
                        "pocałować w policzek",
                        "rumieńce na policzkach",
                        "szczypać w policzki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Каждая из двух боковых частей лица, находящаяся ниже глаза и рядом с носом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щека",
                                "target_lang_sense_clarification": "часть лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b626bb7-40a7-33c7-8cb4-ca1083736006",
                    "sense_definition": "Uderzenie otwartą dłonią w twarz.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za to kłamstwo wymierzyła mu siarczysty <w>policzek</w>.",
                            "target_lang_translations": {
                                "ru": "За эту ложь она отвесила ему звонкую <w>пощёчину</w>."
                            }
                        },
                        {
                            "text": "Ten niespodziewany <w>policzek</w> sprawił, że mężczyzna aż zachwiał się na nogach.",
                            "target_lang_translations": {
                                "ru": "Эта неожиданная <w>пощёчина</w> заставила мужчину пошатнуться."
                            }
                        }
                    ],
                    "synonyms": [
                        "spoliczkowanie",
                        "reg. śl. facka"
                    ],
                    "common_phrases": [
                        "wymierzyć policzek",
                        "dostać policzek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удар открытой ладонью по лицу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пощёчина",
                                "target_lang_sense_clarification": "удар"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ff9052e-a0c3-3aea-9cef-103a15632d50",
                    "sense_definition": "Czyn lub słowa będące ciężką obrazą honoru lub godności osobistej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego odmowa przyjęcia nagrody była dla organizatorów wielkim <w>policzkiem</w>.",
                            "target_lang_translations": {
                                "ru": "Его отказ принять награду стал для организаторов большой <w>пощёчиной</w>."
                            }
                        },
                        {
                            "text": "To publiczne oskarżenie stanowiło bolesny <w>policzek</w> dla całej jego rodziny.",
                            "target_lang_translations": {
                                "ru": "Это публичное обвинение стало болезненной <w>пощёчиной</w> для всей его семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "obraza",
                        "afront"
                    ],
                    "common_phrases": [
                        "policzek dla dumy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поступок или слова, являющиеся тяжким оскорблением чести или личного достоинства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пощёчина",
                                "target_lang_sense_clarification": "оскорбление"
                            },
                            {
                                "target_lang_word": "оскорбление",
                                "target_lang_sense_clarification": "унижение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1e4b666-0218-37d2-9688-9dce236bd2c8",
                    "sense_definition": "Boczna belka biegu schodowego, na której opierają się stopnie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stolarz musiał wymienić pęknięty <w>policzek</w> w starych drewnianych schodach.",
                            "target_lang_translations": {
                                "ru": "Столяр должен был заменить треснувшую <w>тетиву</w> в старой деревянной лестнице."
                            }
                        },
                        {
                            "text": "Każdy <w>policzek</w> tej konstrukcji został ozdobiony rzeźbionym ornamentem.",
                            "target_lang_translations": {
                                "ru": "Каждая <w>тетива</w> этой конструкции была украшена резным орнаментом."
                            }
                        }
                    ],
                    "synonyms": [
                        "belka policzkowa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Боковая балка лестничного марша, на которую опираются ступени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тетива",
                                "target_lang_sense_clarification": "лестницы"
                            },
                            {
                                "target_lang_word": "щека",
                                "target_lang_sense_clarification": "деталь конструкции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67ebc701-1f70-3ed2-8e27-396fce4e3aa2",
                    "sense_definition": "Część kolby broni palnej służąca do oparcia twarzy podczas celowania.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "weaponry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa kolba karabinu ma regulowany <w>policzek</w>, co znacznie ułatwia celowanie.",
                            "target_lang_translations": {
                                "ru": "У нового приклада винтовки есть регулируемая <w>щека</w>, что значительно облегчает прицеливание."
                            }
                        },
                        {
                            "text": "Strzelec oparł twarz o drewniany <w>policzek</w> broni i wstrzymał oddech.",
                            "target_lang_translations": {
                                "ru": "Стрелок прижался лицом к деревянной <w>щеке</w> оружия и задержал дыхание."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть приклада огнестрельного оружия, служащая для опоры щеки во время прицеливания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щека",
                                "target_lang_sense_clarification": "оружия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ec64ac5d-d905-3256-b8e5-ca5c805ee835",
                    "sense_definition": "Cechujący się życzliwością, serdecznością i gotowością do pomocy; taki, jakiego oczekuje się od bliskiej osoby.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>przyjacielski</w> uśmiech sprawił, że od razu poczułam się lepiej w nowym miejscu.",
                            "target_lang_translations": {
                                "ru": "Его <w>дружеская</w> улыбка помогла мне сразу почувствовать себя лучше на новом месте."
                            }
                        },
                        {
                            "text": "Mimo rywalizacji w pracy, łączą ich bardzo <w>przyjacielskie</w> relacje.",
                            "target_lang_translations": {
                                "ru": "Несмотря на соперничество на работе, их связывают очень <w>дружеские</w> отношения."
                            }
                        }
                    ],
                    "synonyms": [
                        "życzliwy",
                        "koleżeński",
                        "braterski"
                    ],
                    "antonyms": [
                        "wrogi",
                        "nieprzyjacielski"
                    ],
                    "common_phrases": [
                        "przyjacielski gest",
                        "przyjacielski uśmiech",
                        "przyjacielska atmosfera",
                        "przyjacielska dłoń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризующийся доброжелательностью, сердечностью и готовностью помочь; такой, какого ожидают от близкого человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дружеский",
                                "target_lang_sense_clarification": "выражающий дружеское расположение, теплый"
                            },
                            {
                                "target_lang_word": "дружелюбный",
                                "target_lang_sense_clarification": "настроенный благожелательно"
                            },
                            {
                                "target_lang_word": "приятельский",
                                "target_lang_sense_clarification": "непринужденный, свойственный приятелям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2df05bc1-5eea-3fe3-86ab-76998405f763",
                    "sense_definition": "Odnoszący się do przyjaciela, należący do niego lub związany z przyjaźnią.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To była czysto <w>przyjacielska</w> przysługa, więc nie oczekuję za nią żadnej zapłaty.",
                            "target_lang_translations": {
                                "ru": "Это была чисто <w>дружеская</w> услуга, поэтому я не жду за неё никакой оплаты."
                            }
                        },
                        {
                            "text": "Udzielę ci dobrej, <w>przyjacielskiej</w> rady: uważaj na swoje wydatki.",
                            "target_lang_translations": {
                                "ru": "Дам тебе хороший <w>дружеский</w> совет: следи за своими расходами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przyjacielska przysługa",
                        "rada przyjacielska",
                        "obowiązek przyjacielski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к другу, принадлежащий ему или связанный с дружбой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дружеский",
                                "target_lang_sense_clarification": "исходящий от друга, свойственный дружбе"
                            },
                            {
                                "target_lang_word": "приятельский",
                                "target_lang_sense_clarification": "относящийся к приятелю"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b01ef9c2-ec84-3970-a9b4-3da9c71582fa",
                    "sense_definition": "Дать правильный ответ, основываясь на предположении, интуиции или случайности.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "guess_answer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты никогда не <угадаешь>, кого я сегодня встретил.",
                            "target_lang_translations": {
                                "en": "You will never <guess> who I met today."
                            }
                        },
                        {
                            "text": "Студент не знал материала, но случайно <угадал> правильный ответ.",
                            "target_lang_translations": {
                                "en": "The student didn't know the material but accidentally <guessed> the right answer."
                            }
                        }
                    ],
                    "synonyms": [
                        "отгадать"
                    ],
                    "antonyms": [
                        "ошибиться"
                    ],
                    "common_phrases": [
                        "угадать загадку",
                        "попробуй угадать",
                        "угадать число",
                        "угадать слово"
                    ],
                    "target_lang_definitions": {
                        "en": "To give the correct answer based on assumption, intuition, or chance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "guess",
                                "target_lang_sense_clarification": "to answer correctly by chance or intuition"
                            },
                            {
                                "target_lang_word": "guess correctly",
                                "target_lang_sense_clarification": "to succeed in guessing"
                            },
                            {
                                "target_lang_word": "get right",
                                "target_lang_sense_clarification": "to answer or do something correctly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c01034a0-e316-3c5d-be20-0b91004a207f",
                    "sense_definition": "Правильно определить или понять что-либо скрытое (мысли, чувства, намерения) по каким-либо признакам.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "discern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она сразу <угадала> моё настроение по выражению лица.",
                            "target_lang_translations": {
                                "en": "She immediately <guessed> my mood by the expression on my face."
                            }
                        },
                        {
                            "text": "Я пытался <угадать> его намерения, но он был непроницаем.",
                            "target_lang_translations": {
                                "en": "I tried to <figure out> his intentions, but he was inscrutable."
                            }
                        }
                    ],
                    "synonyms": [
                        "распознать",
                        "понять",
                        "определить"
                    ],
                    "common_phrases": [
                        "угадать мысли",
                        "угадать желание",
                        "угадать по глазам"
                    ],
                    "target_lang_definitions": {
                        "en": "To correctly determine or understand something hidden (thoughts, feelings, intentions) by observation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "guess",
                                "target_lang_sense_clarification": "to correctly deduce thoughts or feelings"
                            },
                            {
                                "target_lang_word": "divine",
                                "target_lang_sense_clarification": "to discover by intuition or insight"
                            },
                            {
                                "target_lang_word": "figure out",
                                "target_lang_sense_clarification": "to understand someone's hidden meaning"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8405bec4-4272-3a3b-92d8-c9a3c7bfb067",
                    "sense_definition": "Заранее представить или предсказать то, что произойдёт в будущем.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "predict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Трудно <угадать>, как изменятся цены в следующем году.",
                            "target_lang_translations": {
                                "en": "It is hard to <predict> how prices will change next year."
                            }
                        },
                        {
                            "text": "Автор книги удивительно точно <угадал> развитие современных технологий.",
                            "target_lang_translations": {
                                "en": "The author of the book surprisingly accurately <foresaw> the development of modern technologies."
                            }
                        }
                    ],
                    "synonyms": [
                        "предугадать",
                        "предвидеть",
                        "предсказать"
                    ],
                    "common_phrases": [
                        "угадать будущее",
                        "угадать исход"
                    ],
                    "target_lang_definitions": {
                        "en": "To foresee or predict what will happen in the future."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "predict",
                                "target_lang_sense_clarification": "to say what will happen in the future"
                            },
                            {
                                "target_lang_word": "foresee",
                                "target_lang_sense_clarification": "to anticipate future events"
                            },
                            {
                                "target_lang_word": "guess",
                                "target_lang_sense_clarification": "to estimate a future outcome"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2eb86f6-3355-3c9e-87f7-3b711808bdf0",
                    "sense_definition": "Сделать что-либо удачно, точно или вовремя (обычно используется с предлогом «с»).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "succeed_choice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы отлично <угадали> с погодой для пикника.",
                            "target_lang_translations": {
                                "en": "We really <lucked out> with the weather for the picnic."
                            }
                        },
                        {
                            "text": "Надеюсь, я <угадал> с размером рубашки для тебя.",
                            "target_lang_translations": {
                                "en": "I hope I <got> the shirt size for you <right>."
                            }
                        }
                    ],
                    "synonyms": [
                        "попасть",
                        "подгадать"
                    ],
                    "antonyms": [
                        "прогадать"
                    ],
                    "common_phrases": [
                        "угадать с подарком",
                        "угадать с размером",
                        "угадать момент"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To make a successful choice or do something at the right time (often used with 'with')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get right",
                                "target_lang_sense_clarification": "to make the correct choice"
                            },
                            {
                                "target_lang_word": "pick the right",
                                "target_lang_sense_clarification": "to choose successfully"
                            },
                            {
                                "target_lang_word": "luck out",
                                "target_lang_sense_clarification": "to have good luck with a choice (colloquial)"
                            }
                        ]
                    }
                }
            ]
        }

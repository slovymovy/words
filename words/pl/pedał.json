    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a601f671-d76a-3fa1-92dd-403345cf985b",
                    "sense_definition": "Dźwignia naciskana stopą, służąca do napędzania pojazdu (np. roweru) lub sterowania mechanizmem (np. w samochodzie, instrumencie muzycznym).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mechanical_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam wrażenie, że <w>pedał</w> sprzęgła w moim samochodzie nie działa poprawnie.",
                            "target_lang_translations": {
                                "ru": "У меня такое чувство, что <w>педаль</w> сцепления в моей машине работает неправильно."
                            }
                        },
                        {
                            "text": "Rowerzysta mocno naciskał na każdy <w>pedał</w>, aby podjechać pod stromą górę.",
                            "target_lang_translations": {
                                "ru": "Велосипедист с силой давил на каждую <w>педаль</w>, чтобы заехать на крутую гору."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pedał gazu",
                        "pedał hamulca",
                        "pedał sprzęgła",
                        "nacisnąć pedał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рычаг, нажимаемый ногой, который служит для приведения в движение транспортного средства или управления механизмом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "педаль",
                                "target_lang_sense_clarification": "часть механизма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74c77782-bfd6-3ead-8b8a-465dbe9410ab",
                    "sense_definition": "Wulgarne i obraźliwe określenie mężczyzny homoseksualnego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_pejorative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Co się gapisz, <w>pedale</w>?!",
                            "target_lang_translations": {
                                "ru": "Чего уставился, <w>пидор</w>?!"
                            }
                        },
                        {
                            "text": "Używanie słowa <w>pedał</w> w miejscu publicznym jest uznawane za mowę nienawiści.",
                            "target_lang_translations": {
                                "ru": "Использование слова «<w>пидор</w>» в общественном месте считается языком вражды."
                            }
                        }
                    ],
                    "synonyms": [
                        "pedryl",
                        "pedzio"
                    ],
                    "traits": [

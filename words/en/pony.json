        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f7d2422d-c687-383a-8820-893b2fbcc172",
                    "sense_definition": "A small horse; specifically, any of several small breeds of horse under 14.2 hands at the withers.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children were excited to go for a ride on the <w>pony</w>.",
                            "target_lang_translations": {
                                "ru": "Дети были в восторге от поездки на <w>пони</w>."
                            }
                        },
                        {
                            "text": "She keeps a Shetland <w>pony</w> in the paddock behind her house.",
                            "target_lang_translations": {
                                "ru": "Она держит шетлендского <w>пони</w> в загоне за домом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Shetland pony",
                        "ride a pony",
                        "pony club"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькая лошадь; конкретно, любая из нескольких низкорослых пород лошадей (ниже 147 см в холке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пони",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {

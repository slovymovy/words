                    "sense_id": "a394c628-aeed-32a9-9308-ac15cf578718",
                    "sense_definition": "Plural of <w>master</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "plural_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The servants were expected to obey their <masters> without question.",
                            "target_lang_translations": {
                                "ru": "Слуги должны были беспрекословно подчиняться своим <хозяевам>."
                            }
                        },
                        {
                            "text": "We visited the museum to see paintings by the old <masters>.",
                            "target_lang_translations": {
                                "ru": "Мы посетили музей, чтобы увидеть картины старых <мастеров>."
                            }
                        }
                    ],
                    "synonyms": [
                        "experts",
                        "rulers",
                        "owners"
                    ],
                    "antonyms": [
                        "apprentices",
                        "servants",
                        "novices"
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова master (хозяин, мастер)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хозяева",
                                "target_lang_sense_clarification": "владельцы или правители"
                            },
                            {
                                "target_lang_word": "мастера",
                                "target_lang_sense_clarification": "великие художники или специалисты"
                            },
                            {

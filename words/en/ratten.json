    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b02df502-1931-38ea-b85d-c1c240c8f2b8",
                    "sense_definition": "To sabotage machinery or tools as part of an industrial dispute, particularly the tools of a workman who went against the union.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The angry workers threatened to <w>ratten</w> the machinery if their demands were not met.",
                            "target_lang_translations": {
                                "ru": "Разгневанные рабочие угрожали <w>испортить</w> оборудование, если их требования не будут выполнены."
                            }
                        },
                        {
                            "text": "He was accused of trying to <w>ratten</w> the tools of the strike-breakers during the dispute.",
                            "target_lang_translations": {
                                "ru": "Его обвинили в попытке <w>испортить</w> инструменты штрейкбрехеров во время конфликта."
                            }
                        }
                    ],
                    "synonyms": [
                        "sabotage"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to 19th-century trade union practices"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Портить или прятать инструменты и оборудование в ходе трудового конфликта (особенно инструменты рабочих, не поддержавших забастовку)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "саботировать",
                                "target_lang_sense_clarification": "вредить производству"
                            },
                            {
                                "target_lang_word": "портить инструменты",
                                "target_lang_sense_clarification": "в знак протеста"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "081cfd0c-ac55-34ae-9472-473656719ded",
                    "sense_definition": "Alternative form of <w>ratton</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer set a trap to catch the large <w>ratten</w> in the barn.",
                            "target_lang_translations": {
                                "ru": "Фермер поставил ловушку, чтобы поймать большую <w>крысу</w> в сарае."
                            }
                        },
                        {
                            "text": "She screamed when she saw a <w>ratten</w> scurrying across the kitchen floor.",
                            "target_lang_translations": {

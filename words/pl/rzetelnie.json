                    "sense_id": "cb554d04-5058-373a-8cf1-9416941e1633",
                    "sense_definition": "W sposób dokładny, solidny i staranny, wykonując zadania bez błędów i z należytą uwagą.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "thoroughness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracownik ten zawsze <w>rzetelnie</w> wykonuje powierzone mu zadania.",
                            "target_lang_translations": {
                                "ru": "Этот сотрудник всегда <w>добросовестно</w> выполняет порученные ему задачи."
                            }
                        },
                        {
                            "text": "Raport został przygotowany bardzo <w>rzetelnie</w> i zawierał wszystkie dane.",
                            "target_lang_translations": {
                                "ru": "Отчет был подготовлен очень <w>тщательно</w> и содержал все данные."
                            }
                        }
                    ],
                    "synonyms": [
                        "solidnie",
                        "dokładnie",
                        "starannie",
                        "sumiennie"
                    ],
                    "antonyms": [
                        "niedbale",
                        "powierzchownie",
                        "byle jak"
                    ],
                    "common_phrases": [
                        "rzetelnie pracować",
                        "rzetelnie wykonywać obowiązki",
                        "rzetelnie przygotowany"
                    ],
                    "target_lang_definitions": {

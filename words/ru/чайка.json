    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "833c295f-b4bc-3064-9568-d75a74f7bc34",
                    "sense_definition": "Морская или речная птица с длинными крыльями, густым оперением и громким криком.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "birds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Над морем кружила белая <w>чайка</w>.",
                            "target_lang_translations": {
                                "en": "A white <w>seagull</w> circled over the sea."
                            }
                        },
                        {
                            "text": "Мы услышали громкий крик <w>чайки</w> и посмотрели в небо.",
                            "target_lang_translations": {
                                "en": "We heard the loud cry of a <w>gull</w> and looked up at the sky."
                            }
                        }
                    ],
                    "common_phrases": [
                        "крик чайки",
                        "белая чайка",
                        "стая чаек"
                    ],
                    "target_lang_definitions": {
                        "en": "A seabird or river bird with long wings, thick plumage, and a loud cry."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seagull",
                                "target_lang_sense_clarification": "common name"
                            },
                            {
                                "target_lang_word": "gull",
                                "target_lang_sense_clarification": "general term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3a8054e-6cf6-38bc-93d7-71b990cbbc8b",
                    "sense_definition": "Форма родительного падежа единственного числа существительного <w>чаёк</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Налей мне, пожалуйста, горячего <w>чайка</w>.",
                            "target_lang_translations": {
                                "en": "Please pour me some hot <w>tea</w>."
                            }
                        },
                        {
                            "text": "После мороза хочется выпить крепкого <w>чайка</w>.",
                            "target_lang_translations": {
                                "en": "After the frost, one wants to drink some strong <w>tea</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "попить чайка",
                        "горячего чайка"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive singular of чаёк"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive singular form of the noun 'чаёк' (diminutive of tea)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tea",
                                "target_lang_sense_clarification": "diminutive form"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51e06df8-9a0a-368d-8069-19e5bab1bf8b",
                    "sense_definition": "Человек, который постоянно выпрашивает что-либо у других, любитель получить что-то бесплатно.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не будь <w>чайкой</w>, купи свои сигареты сам.",
                            "target_lang_translations": {
                                "en": "Don't be a <w>moocher</w>, buy your own cigarettes."
                            }
                        },
                        {
                            "text": "Этот <w>чайка</w> опять съел весь наш обед.",
                            "target_lang_translations": {
                                "en": "That <w>freeloader</w> ate all our lunch again."
                            }
                        }
                    ],
                    "synonyms": [
                        "попрошайка"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person who constantly begs for things from others, someone who likes to get things for free."
                    },
                    "translations": {

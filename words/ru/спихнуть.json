          "frequency": "Middle",
          "semantic_group_id": "physical_push",
          "name_type": "no",
          "examples": [
            {
              "text": "Мы с трудом смогли <спихнуть> тяжелую лодку в воду."
            },
            {
              "text": "Кот забрался на полку и случайно <спихнул> вазу на пол."
            }
          ],
          "synonyms": [
            "столкнуть"
          ],
          "common_phrases": [

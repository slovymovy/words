            "senses": [
                {
                    "sense_id": "4c666862-5d36-3bb1-aa1b-1391b49fcd95",
                    "sense_definition": "Een plek of kamer die speciaal is ingericht om in te spelen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen vermaken zich prima in de nieuwe <w>speelruimte</w> van de school.",
                            "target_lang_translations": {
                                "ru": "Дети отлично проводят время в новой <w>игровой комнате</w> школы."
                            }
                        },
                        {
                            "text": "Het hotel heeft een grote <w>speelruimte</w> waar gasten kunnen tafeltennissen.",
                            "target_lang_translations": {
                                "ru": "В отеле есть большая <w>игровая зона</w>, где гости могут поиграть в настольный теннис."
                            }
                        }
                    ],
                    "synonyms": [
                        "speelkamer",
                        "speelplek"
                    ],
                    "common_phrases": [
                        "veilige speelruimte",
                        "grote speelruimte",
                        "openbare speelruimte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или комната, специально оборудованные для игр."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игровая комната",
                                "target_lang_sense_clarification": "отдельное помещение"
                            },
                            {
                                "target_lang_word": "игровая зона",
                                "target_lang_sense_clarification": "выделенное пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a591ae2-4383-3de2-8dba-1ae0bd07da7c",
                    "sense_definition": "De mogelijkheid of vrijheid om iets naar eigen inzicht te doen of om af te wijken van regels.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_freedom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de strenge regels hebben we helaas maar weinig <w>speelruimte</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за строгих правил у нас, к сожалению, мало <w>свободы действий</w>."
                            }
                        },
                        {
                            "text": "Er is in het budget nog wat financiële <w>speelruimte</w> voor onvoorziene kosten.",
                            "target_lang_translations": {
                                "ru": "В бюджете еще есть некоторый финансовый <w>запас</w> на непредвиденные расходы."
                            }
                        }
                    ],
                    "synonyms": [
                        "speling",
                        "marge",
                        "bewegingsvrijheid"
                    ],
                    "common_phrases": [
                        "weinig speelruimte",
                        "financiële speelruimte",
                        "extra speelruimte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возможность или свобода действовать по своему усмотрению или отклоняться от правил."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свобода действий",
                                "target_lang_sense_clarification": "независимость в решениях"
                            },
                            {
                                "target_lang_word": "простор",
                                "target_lang_sense_clarification": "возможность для размаха"
                            },
                            {
                                "target_lang_word": "поле для маневра",
                                "target_lang_sense_clarification": "возможность выбора тактики"
                            }
                        ]
                    }
                }
            ]

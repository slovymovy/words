    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d6025ef7-6593-3244-ad6f-16c6874a7b37",
                    "sense_definition": "The lightest chemical element (symbol H), with an atomic number of 1.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "chemical_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sun is primarily composed of <w>hydrogen</w> and helium.",
                            "target_lang_translations": {
                                "ru": "Солнце состоит преимущественно из <w>водорода</w> и гелия."
                            }
                        },
                        {
                            "text": "Water molecules contain two atoms of <w>hydrogen</w> and one of oxygen.",
                            "target_lang_translations": {
                                "ru": "Молекулы воды содержат два атома <w>водорода</w> и один атом кислорода."
                            }
                        }
                    ],
                    "synonyms": [
                        "element 1"
                    ],
                    "common_phrases": [
                        "hydrogen bomb",
                        "hydrogen peroxide",
                        "liquid hydrogen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самый легкий химический элемент (символ H) с атомным номером 1."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водород",
                                "target_lang_sense_clarification": "химический элемент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f0babcd-6f7b-3df2-9b73-90a63a3ca474",
                    "sense_definition": "Molecular hydrogen (H₂), a colourless, odourless, and highly flammable gas.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "chemical_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The airship was filled with <w>hydrogen</w> gas to make it lighter than air.",
                            "target_lang_translations": {
                                "ru": "Дирижабль был наполнен газообразным <w>водородом</w>, чтобы сделать его легче воздуха."
                            }
                        },
                        {
                            "text": "Cars powered by <w>hydrogen</w> emit only water vapor from the tailpipe.",
                            "target_lang_translations": {
                                "ru": "Автомобили, работающие на <w>водороде</w>, выбрасывают из выхлопной трубы только водяной пар."
                            }
                        }
                    ],
                    "synonyms": [
                        "dihydrogen"
                    ],
                    "common_phrases": [
                        "hydrogen gas",
                        "green hydrogen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Молекулярный водород (H₂), бесцветный газ без запаха, обладающий высокой горючестью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водород",
                                "target_lang_sense_clarification": "газ / простое вещество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3506c85c-cd24-39b6-ae66-90a8166cce37",
                    "sense_definition": "The specific isotope hydrogen-1, also known as protium, distinct from deuterium and tritium.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "chemical_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Standard <w>hydrogen</w> is the most abundant isotope in the universe.",
                            "target_lang_translations": {
                                "ru": "Обычный <w>водород</w> — самый распространенный изотоп во Вселенной."
                            }
                        },
                        {
                            "text": "Scientists compared the properties of deuterium with those of ordinary <w>hydrogen</w>.",
                            "target_lang_translations": {
                                "ru": "Ученые сравнили свойства дейтерия со свойствами обычного <w>водорода</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "scientific"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Изотоп водород-1, также известный как протий, в отличие от дейтерия и трития."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водород",
                                "target_lang_sense_clarification": "изотоп / протий"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
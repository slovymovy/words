                    "sense_id": "0119e7de-139b-3b22-bb59-465131564158",
                    "sense_definition": "Wojskowy mundur polowy lub ubranie cywilne uszyte z tkaniny w maskujący wzór.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "moro_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W militarnym demobilu kupiłem sobie dwa <w>mora</w>.",
                            "target_lang_translations": {
                                "ru": "В магазине военной одежды я купил себе два <w>камуфляжа</w>."
                            }
                        },
                        {
                            "text": "Na wycieczkę do lasu najlepiej ubrać się w wygodne <w>moro</w>.",
                            "target_lang_translations": {
                                "ru": "Для похода в лес лучше всего одеться в удобный <w>камуфляж</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mundur",
                        "panterka"
                    ],
                    "common_phrases": [

{
    "word_family": [
        "przypomnienie",
        "pamiętać",
        "zapominać",
        "wspominać",
        "upominać",
        "zapamiętać",
        "pamięć"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "113d712f-97fd-34d8-af9e-712987a5e7b1",
                    "sense_definition": "Zwracać komuś uwagę na coś, żeby o tym nie zapomniał; sprawiać, że ktoś o czymś pamięta.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "memory_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano mama <przypomina> mi o zjedzeniu śniadania.",
                            "target_lang_translations": {
                                "ru": "Каждое утро мама <напоминает> мне о завтраке."
                            }
                        },
                        {
                            "text": "Musisz ciągle <przypominać> pracownikom o nowych zasadach bezpieczeństwa.",
                            "target_lang_translations": {
                                "ru": "Ты должен постоянно <напоминать> сотрудникам о новых правилах безопасности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przypominać o terminie",
                        "przypominać o obietnicy",
                        "ciągle przypominać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращать чье-то внимание на что-либо, чтобы человек об этом не забыл; заставлять помнить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напоминать",
                                "target_lang_sense_clarification": "обращать внимание, чтобы не забыли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab51de9e-3ae8-38d4-a1df-ecca03b34504",
                    "sense_definition": "Przywoływać w pamięci informacje lub zdarzenia; uświadamiać sobie coś, co się wcześniej zapomniało (używane z zaimkiem zwrotnym „sobie”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "memory_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właśnie <przypominam> sobie, gdzie położyłem zapasowe klucze.",
                            "target_lang_translations": {
                                "ru": "Я как раз <вспоминаю>, куда положил запасные ключи."
                            }
                        },
                        {
                            "text": "Zupełnie nie <przypominam> sobie, żebym cię tu zapraszał.",
                            "target_lang_translations": {
                                "ru": "Совершенно не <припоминаю>, чтобы я тебя сюда приглашал."
                            }
                        }
                    ],
                    "synonyms": [
                        "wspomnieć się"
                    ],
                    "antonyms": [
                        "różnić się"
                    ],
                    "common_phrases": [
                        "przypominać sobie coś",
                        "nie przypominać sobie",
                        "przypominać sobie fakty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восстанавливать в памяти информацию или события; вспоминать то, что было забыто (обычно используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вспоминать",
                                "target_lang_sense_clarification": "восстанавливать в памяти"
                            },
                            {
                                "target_lang_word": "припоминать",
                                "target_lang_sense_clarification": "с трудом вспоминать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16145a94-29fa-3fcb-b559-f90f59f5198d",
                    "sense_definition": "Być podobnym do kogoś lub czegoś z wyglądu, cech lub zachowania; budzić skojarzenia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "similarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary budynek <przypomina> średniowieczny zamek.",
                            "target_lang_translations": {
                                "ru": "Это старое здание <напоминает> средневековый замок."
                            }
                        },
                        {
                            "text": "Z profilu bardzo <przypominasz> swoją babcię.",
                            "target_lang_translations": {
                                "ru": "В профиль ты очень <напоминаешь> свою бабушку."
                            }
                        }
                    ],
                    "antonyms": [
                        "różnić się"
                    ],
                    "common_phrases": [
                        "przypominać kogoś",
                        "przypominać kształtem",
                        "bardzo kogoś przypominać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть похожим на кого-либо или что-либо внешне или по поведению; вызывать ассоциации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напоминать",
                                "target_lang_sense_clarification": "быть похожим"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3062c58d-e255-3c92-a820-9259a5a93540",
                    "sense_definition": "Dingen, mensen of informatie bij elkaar brengen om een geheel of groep te vormen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar <verzamelt> alle toetsen aan het einde van de les.",
                            "target_lang_translations": {
                                "ru": "Учитель <собирает> все контрольные работы в конце урока."
                            }
                        },
                        {
                            "text": "We moeten eerst meer informatie <verzamelen> voordat we een beslissing nemen.",
                            "target_lang_translations": {
                                "ru": "Нам нужно сначала <собрать> больше информации, прежде чем принимать решение."
                            }
                        }
                    ],
                    "synonyms": [
                        "bijeenbrengen"
                    ],
                    "antonyms": [
                        "ontzamelen",
                        "ontzamelen"
                    ],
                    "common_phrases": [
                        "moed verzamelen",
                        "informatie verzamelen",
                        "bewijs verzamelen",
                        "krachten verzamelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собирать вещи, людей или информацию вместе, чтобы сформировать целое или группу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "объединять, накапливать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6625840-07cd-30c4-8d7b-f5933f422aac",
                    "sense_definition": "Op een bepaalde plaats bij elkaar komen (vaak wederkerend gebruikt).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De demonstranten <verzamelden> zich op het grote plein in het centrum.",
                            "target_lang_translations": {
                                "ru": "Протестующие <собрались> на большой площади в центре."
                            }
                        },
                        {
                            "text": "We <verzamelen> morgenochtend om acht uur bij de ingang van het park.",
                            "target_lang_translations": {
                                "ru": "Мы <собираемся> завтра утром в восемь часов у входа в парк."
                            }
                        }
                    ],
                    "synonyms": [
                        "bijeenkomen"
                    ],
                    "antonyms": [
                        "ontzamelen",
                        "ontzamelen"
                    ],
                    "common_phrases": [
                        "zich verzamelen",
                        "verzamelen bij"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сходиться или встречаться в определенном месте (часто используется как возвратный глагол)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "встречаться группой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db2d05b3-92f4-3270-a771-7b3e6a216cf3",
                    "sense_definition": "Specifieke voorwerpen zoeken en bewaren als hobby of om een collectie aan te leggen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn grootvader <verzamelt> al jaren oude munten uit verschillende landen.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка уже много лет <коллекционирует> старинные монеты из разных стран."
                            }
                        },
                        {
                            "text": "Veel kinderen vinden het leuk om stickers van voetballers te <verzamelen>.",
                            "target_lang_translations": {
                                "ru": "Многим детям нравится <собирать> наклейки с футболистами."
                            }
                        }
                    ],
                    "synonyms": [
                        "bijeenbrengen"
                    ],
                    "antonyms": [
                        "ontzamelen"
                    ],
                    "common_phrases": [
                        "postzegels verzamelen",
                        "kunst verzamelen",
                        "munten verzamelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искать и сохранять определенные предметы в качестве хобби или для создания коллекции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коллекционировать",
                                "target_lang_sense_clarification": "хобби"
                            },
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "составлять коллекцию"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
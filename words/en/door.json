            "senses": [
                {
                    "sense_id": "a7140c09-d6f3-39b9-8812-d96f6882ab02",
                    "sense_definition": "To hit a passing cyclist or pedestrian by suddenly opening the door of a vehicle.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Always check your mirror so you do not <w>door</w> a cyclist.",
                            "target_lang_translations": {
                                "ru": "Всегда смотри в зеркало, чтобы не <w>сбить дверью</w> велосипедиста."
                            }
                        },
                        {
                            "text": "He was <w>doored</w> by a taxi passenger who didn't look before exiting.",
                            "target_lang_translations": {
                                "ru": "Его <w>сбил дверью</w> пассажир такси, который не посмотрел перед выходом."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in cycling contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбить проезжающего велосипедиста или пешехода, внезапно открыв дверь транспортного средства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбить дверью",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "ударить дверью",
                                "target_lang_sense_clarification": "действие"
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2edae0fc-cc26-3515-bb7c-78ac7f29edce",
                    "sense_definition": "Естественный розовый или красноватый цвет щёк, обычно свидетельствующий о здоровье, молодости или свежести.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "facial_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На её щеках играл здоровый <w>румянец</w>, несмотря на усталость.",
                            "target_lang_translations": {
                                "en": "A healthy <w>glow</w> played on her cheeks, despite her fatigue."
                            }
                        },
                        {
                            "text": "Морозный воздух вызвал яркий <w>румянец</w> на лицах детей.",
                            "target_lang_translations": {
                                "en": "The frosty air brought a bright <w>flush</w> to the children's faces."
                            }
                        }
                    ],
                    "antonyms": [
                        "бледность"
                    ],
                    "common_phrases": [
                        "здоровый румянец",
                        "яркий румянец",
                        "румянец на щеках"
                    ],
                    "target_lang_definitions": {
                        "en": "Natural pink or reddish color of the cheeks, usually indicating health, youth, or freshness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "glow",
                                "target_lang_sense_clarification": "healthy color"
                            },
                            {
                                "target_lang_word": "rosiness",
                                "target_lang_sense_clarification": "pink color"
                            },
                            {
                                "target_lang_word": "bloom",
                                "target_lang_sense_clarification": "youthful freshness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c71c694c-4c63-3144-b179-4409c8632022",
                    "sense_definition": "Временное покраснение лица от прилива крови, вызванное смущением, волнением, стыдом или физическим напряжением.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "facial_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От неожиданного комплимента она густо покраснела, и <w>румянец</w> долго не сходил с её лица.",
                            "target_lang_translations": {
                                "en": "She turned deep red from the unexpected compliment, and the <w>blush</w> did not leave her face for a long time."
                            }
                        },
                        {
                            "text": "Внезапный <w>румянец</w> выдал его сильное волнение при встрече.",
                            "target_lang_translations": {
                                "en": "A sudden <w>flush</w> betrayed his intense agitation upon meeting."
                            }
                        }
                    ],
                    "synonyms": [
                        "краска"
                    ],
                    "antonyms": [
                        "бледность"
                    ],
                    "common_phrases": [
                        "залиться румянцем",
                        "легкий румянец",
                        "густой румянец",
                        "вспыхнул румянец"
                    ],
                    "target_lang_definitions": {
                        "en": "Temporary reddening of the face caused by embarrassment, excitement, shame, or physical exertion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blush",
                                "target_lang_sense_clarification": "from embarrassment or shame"
                            },
                            {
                                "target_lang_word": "flush",
                                "target_lang_sense_clarification": "sudden redness from emotion or heat"
                            },
                            {
                                "target_lang_word": "color",
                                "target_lang_sense_clarification": "redness in the face"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbe8bab6-e14c-3c9e-88a3-122ccf3964cf",
                    "sense_definition": "Розоватый или красноватый отблеск света (обычно утренней или вечерней зари) на небе или предметах.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Последний <w>румянец</w> заката медленно угасал на горизонте.",
                            "target_lang_translations": {
                                "en": "The last <w>glow</w> of the sunset was slowly fading on the horizon."
                            }
                        },
                        {
                            "text": "Нежный <w>румянец</w> утренней зари окрасил верхушки заснеженных гор.",
                            "target_lang_translations": {
                                "en": "The gentle <w>flush</w> of the morning dawn colored the peaks of the snow-capped mountains."
                            }
                        }
                    ],
                    "common_phrases": [
                        "румянец зари",
                        "румянец заката"
                    ],
                    "target_lang_definitions": {
                        "en": "A pinkish or reddish reflection of light (usually of dawn or sunset) on the sky or objects."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "glow",
                                "target_lang_sense_clarification": "of light"
                            },
                            {
                                "target_lang_word": "flush",
                                "target_lang_sense_clarification": "poetic redness of the sky"
                            }
                        ]
                    }
                }
            ]
        }
    ]

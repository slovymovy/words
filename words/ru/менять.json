                    "common_phrases": [
                        "менять деньги",
                        "менять валюту",
                        "менять местами"
                    ],
                    "target_lang_definitions": {
                        "en": "To give one object and receive another in return"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exchange",
                                "target_lang_sense_clarification": "to give and receive reciprocally"
                            },
                            {

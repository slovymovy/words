                {
                    "sense_id": "cc3c516c-8b03-370f-a6e0-2feb8703c33a",
                    "sense_definition": "Pisemne polecenie wystawione przez lekarza, upoważniające pacjenta do zakupu określonych leków w aptece.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz wypisał mi nową <w>receptę</w> na antybiotyk, którą muszę zrealizować dzisiaj.",
                            "target_lang_translations": {
                                "ru": "Врач выписал мне новый <w>рецепт</w> на антибиотик, по которому я должен купить лекарство сегодня."
                            }
                        },
                        {
                            "text": "Niestety twoja <w>recepta</w> jest już nieważna i nie mogę wydać tego leku.",
                            "target_lang_translations": {
                                "ru": "К сожалению, твой <w>рецепт</w> уже недействителен, и я не могу выдать это лекарство."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wypisać receptę",
                        "zrealizować receptę",
                        "recepta lekarska",
                        "ważność recepty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Письменное предписание врача, разрешающее пациенту приобрести определенные лекарства в аптеке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рецепт",
                                "target_lang_sense_clarification": "медицинский документ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0d59097-fae9-3644-9eea-d2762d9924d2",
                    "sense_definition": "Gotowy, sprawdzony sposób postępowania lub metoda pozwalająca na osiągnięcie zamierzonego celu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciężka praca i determinacja to najlepsza <w>recepta</w> na sukces w każdej dziedzinie.",
                            "target_lang_translations": {
                                "ru": "Упорный труд и решительность — это лучший <w>рецепт</w> успеха в любой области."
                            }
                        },
                        {
                            "text": "Nie istnieje jedna uniwersalna <w>recepta</w> na szczęśliwe i bezproblemowe życie.",
                            "target_lang_translations": {
                                "ru": "Не существует одного универсального <w>рецепта</w> счастливой и беззаботной жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "przepis"
                    ],
                    "common_phrases": [
                        "recepta na sukces",
                        "gotowa recepta",
                        "recepta na życie",
                        "sprawdzona recepta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовый, проверенный способ действий или метод, позволяющий достичь намеченной цели."
                    },
                    "translations": {

            "senses": [
                {
                    "sense_id": "7d16404b-bfe7-3efc-a1e9-a1f436aa53e3",
                    "sense_definition": "Doprowadzić do zgody i zakończenia sporu między skłóconymi osobami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "unification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel próbował <pogodzić> dwóch uczniów po bójce na przerwie.",
                            "target_lang_translations": {
                                "ru": "Учитель пытался <w>помирить</w> двух учеников после драки на перемене."
                            }
                        },
                        {
                            "text": "Udało mi się <pogodzić> zwaśnione strony i zakończyć ten długi konflikt.",
                            "target_lang_translations": {
                                "ru": "Мне удалось <w>примирить</w> враждующие стороны и положить конец этому долгому конфликту."
                            }
                        }
                    ],
                    "synonyms": [
                        "pojednać"
                    ],
                    "antonyms": [
                        "skłócić",
                        "poróżnić"
                    ],
                    "common_phrases": [
                        "pogodzić zwaśnione strony",
                        "pogodzić braci",
                        "pogodzić sąsiadów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести к согласию и прекращению спора между поссорившимися людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помирить",
                                "target_lang_sense_clarification": "восстановить хорошие отношения между людьми"
                            },
                            {
                                "target_lang_word": "примирить",
                                "target_lang_sense_clarification": "уладить конфликт или вражду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae02e477-3c01-3865-be79-8ce1945f2257",
                    "sense_definition": "Zaakceptować trudną sytuację, stratę lub fakt, którego nie można zmienić (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz w końcu <pogodzić> się z losem i żyć dalej.",
                            "target_lang_translations": {
                                "ru": "Ты должен наконец <w>смириться</w> с судьбой и жить дальше."
                            }
                        },
                        {
                            "text": "Ona nie potrafi <pogodzić> się z porażką w ostatnich zawodach.",
                            "target_lang_translations": {
                                "ru": "Она не может <w>смириться</w> с поражением в последних соревнованиях."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaakceptować"
                    ],
                    "antonyms": [
                        "buntować się",
                        "sprzeciwiać się"
                    ],
                    "common_phrases": [
                        "pogodzić się z losem",
                        "pogodzić się z sytuacją",
                        "pogodzić się z faktem",
                        "pogodzić się z porażką"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "requires reflexive pronoun 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять трудную ситуацию, потерю или факт, который невозможно изменить (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смириться",
                                "target_lang_sense_clarification": "принять неизбежное"
                            },
                            {
                                "target_lang_word": "примириться",
                                "target_lang_sense_clarification": "перестать противиться чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8437999b-0c01-37ab-8acb-4643e424a886",
                    "sense_definition": "Zorganizować różne sprawy lub obowiązki tak, aby można je było wykonywać jednocześnie bez konfliktu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "unification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bardzo trudno jest <pogodzić> pracę na pełen etat z wychowywaniem dzieci.",
                            "target_lang_translations": {
                                "ru": "Очень трудно <w>совместить</w> работу на полную ставку с воспитанием детей."
                            }
                        },
                        {
                            "text": "Musisz <pogodzić> naukę do egzaminów z regularnymi treningami sportowymi.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>совместить</w> подготовку к экзаменам с регулярными спортивными тренировками."
                            }
                        }
                    ],
                    "synonyms": [
                        "połączyć",
                        "zgrać"
                    ],
                    "common_phrases": [
                        "pogodzić pracę z domem",
                        "pogodzić obowiązki",
                        "pogodzić sprzeczne interesy",
                        "pogodzić ogień z wodą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организовать разные дела или обязанности так, чтобы их можно было выполнять одновременно без конфликта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совместить",
                                "target_lang_sense_clarification": "сочетать разные действия или обязанности"
                            },
                            {
                                "target_lang_word": "сочетать",
                                "target_lang_sense_clarification": "гармонично соединять"
                            }
                        ]
                    }
                }
            ]
        }

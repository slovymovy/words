    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "649b83df-5857-32e5-b2f7-c8a2891828a4",
                    "sense_definition": "Zdolność do szybkiego i głębokiego rozumienia istoty rzeczy, bystrość umysłu i spostrzegawczość.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_traits",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego niezwykła <w>przenikliwość</w> pozwoliła mu szybko rozwiązać skomplikowaną zagadkę kryminalną.",
                            "target_lang_translations": {
                                "ru": "Его необычайная <w>проницательность</w> позволила ему быстро разгадать сложную криминальную загадку."
                            }
                        },
                        {
                            "text": "Dzięki dużej <w>przenikliwości</w> umysłu potrafiła przewidzieć ruchy konkurencji na rynku.",
                            "target_lang_translations": {
                                "ru": "Благодаря большой <w>проницательности</w> ума она смогла предвидеть действия конкурентов на рынке."
                            }
                        }
                    ],
                    "synonyms": [
                        "wnikliwość",
                        "bystrość",
                        "domyślność",
                        "spostrzegawczość"
                    ],
                    "antonyms": [
                        "tępota",
                        "bezmyślność",
                        "naiwność"
                    ],
                    "common_phrases": [
                        "przenikliwość umysłu",
                        "niezwykła przenikliwość",
                        "zaskakująca przenikliwość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность быстро и глубоко понимать суть вещей, острота ума и наблюдательность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проницательность",
                                "target_lang_sense_clarification": "способность глубоко понимать суть"
                            },
                            {
                                "target_lang_word": "острота",
                                "target_lang_sense_clarification": "ума или восприятия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd06bdec-d583-3da6-b951-517de1fc1b6e",
                    "sense_definition": "Cecha tego, co jest ostre, przejmujące i głęboko oddziałujące na zmysły, na przykład chłodu, bólu lub czyjegoś wzroku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sensory_qualities",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Przenikliwość</w> mroźnego wiatru sprawiała, że wszyscy marzyli o powrocie do ciepłego domu.",
                            "target_lang_translations": {
                                "ru": "<w>Пронзительность</w> морозного ветра заставляла всех мечтать о возвращении в тёплый дом."
                            }
                        },
                        {
                            "text": "Czułem na sobie <w>przenikliwość</w> jego spojrzenia, które zdawało się czytać w moich myślach.",
                            "target_lang_translations": {
                                "ru": "Я чувствовал на себе <w>пронзительность</w> его взгляда, который, казалось, читал мои мысли."
                            }
                        }
                    ],
                    "synonyms": [
                        "ostrość",
                        "dokuczliwość",
                        "przejmującość"
                    ],
                    "antonyms": [
                        "łagodność"
                    ],
                    "common_phrases": [
                        "przenikliwość spojrzenia",
                        "przenikliwość wiatru",
                        "przenikliwość bólu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство быть острым, пронизывающим, например, о холоде, боли или взгляде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пронзительность",
                                "target_lang_sense_clarification": "о ветре, холоде или взгляде"
                            },
                            {
                                "target_lang_word": "острота",
                                "target_lang_sense_clarification": "о взгляде или боли"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
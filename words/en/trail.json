{
    "word_family": [
        "trailer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a2d61b1e-b5d4-37ac-81f8-efb693f12332",
                    "sense_definition": "A route for travel over land, especially a narrow, unpaved pathway for use by hikers or horseback riders.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "path_route",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We walked along a steep mountain <w>trail</w> to reach the summit.",
                            "target_lang_translations": {
                                "ru": "Мы шли по крутой горной <w>тропе</w>, чтобы добраться до вершины."
                            }
                        },
                        {
                            "text": "The hiking <w>trail</w> winds through the dense forest.",
                            "target_lang_translations": {
                                "ru": "Туристическая <w>тропа</w> петляет через густой лес."
                            }
                        }
                    ],
                    "synonyms": [
                        "path",
                        "footpath",
                        "track",
                        "dirt track"
                    ],
                    "common_phrases": [
                        "hiking trail",
                        "nature trail",
                        "mountain trail"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маршрут для путешествия по суше, особенно узкая, немощеная дорожка для туристов или всадников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тропа",
                                "target_lang_sense_clarification": "узкая протоптанная дорожка"
                            },
                            {
                                "target_lang_word": "тропинка",
                                "target_lang_sense_clarification": "маленькая узкая тропа"
                            },
                            {
                                "target_lang_word": "маршрут",
                                "target_lang_sense_clarification": "путь следования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34114803-841c-311b-8f89-e46a40104cac",
                    "sense_definition": "The track, marks, or scent left behind by someone or something that has passed.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "marks_signs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hunters followed the <w>trail</w> of the wounded animal.",
                            "target_lang_translations": {
                                "ru": "Охотники шли по <w>следу</w> раненого животного."
                            }
                        },
                        {
                            "text": "The jet left a white vapour <w>trail</w> across the blue sky.",
                            "target_lang_translations": {
                                "ru": "Реактивный самолет оставил белый инверсионный <w>след</w> в голубом небе."
                            }
                        }
                    ],
                    "synonyms": [
                        "track",
                        "spoor",
                        "sign"
                    ],
                    "common_phrases": [
                        "paper trail",
                        "trail of blood",
                        "trail of destruction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следы, метки или запах, оставленные кем-то или чем-то прошедшим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "след",
                                "target_lang_sense_clarification": "отпечаток или признак присутствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a17bd80c-8924-3305-aac4-7aa2cbc67030",
                    "sense_definition": "A route or series of places visited in a specific order, often for a particular purpose.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "path_route",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The politicians are busy on the campaign <w>trail</w> before the election.",
                            "target_lang_translations": {
                                "ru": "Политики заняты в предвыборной <w>поездке</w> перед выборами."
                            }
                        },
                        {
                            "text": "Tourists often follow the famous whisky <w>trail</w> in Scotland.",
                            "target_lang_translations": {
                                "ru": "Туристы часто следуют по знаменитому <w>маршруту</w> виски в Шотландии."
                            }
                        }
                    ],
                    "synonyms": [
                        "track",
                        "path"
                    ],
                    "common_phrases": [
                        "campaign trail"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маршрут или ряд мест, посещаемых в определенном порядке, часто с конкретной целью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маршрут",
                                "target_lang_sense_clarification": "путь следования по местам"
                            },
                            {
                                "target_lang_word": "поездка",
                                "target_lang_sense_clarification": "путешествие по ряду мест"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8665390b-89d0-38ca-a935-3eaf0427773c",
                    "sense_definition": "To follow behind someone or something, often to track or hunt them.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "follow_track",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The detective <w>trailed</w> the suspect through the city streets.",
                            "target_lang_translations": {
                                "ru": "Детектив <w>выслеживал</w> подозреваемого на улицах города."
                            }
                        },
                        {
                            "text": "Wolves <w>trailed</w> the herd for miles before attacking.",
                            "target_lang_translations": {
                                "ru": "Волки <w>шли по следу</w> стада много миль перед нападением."
                            }
                        }
                    ],
                    "synonyms": [
                        "track",
                        "footpath",
                        "path",
                        "track"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следовать за кем-то или чем-то, часто с целью выслеживания или охоты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выслеживать",
                                "target_lang_sense_clarification": "искать, идя по следам"
                            },
                            {
                                "target_lang_word": "идти по следу",
                                "target_lang_sense_clarification": "преследовать по оставленным знакам"
                            },
                            {
                                "target_lang_word": "следить",
                                "target_lang_sense_clarification": "наблюдать, следуя сзади"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5089eb9-70e4-31f0-ad6f-61e979a4942c",
                    "sense_definition": "To be losing or behind an opponent in a competition or score.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "losing_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our team was <w>trailing</w> by ten points at halftime.",
                            "target_lang_translations": {
                                "ru": "Наша команда <w>отставала</w> на десять очков в перерыве."
                            }
                        },
                        {
                            "text": "The candidate is currently <w>trailing</w> in the opinion polls.",
                            "target_lang_translations": {
                                "ru": "Кандидат в настоящее время <w>отстает</w> в опросах общественного мнения."
                            }
                        }
                    ],
                    "synonyms": [
                        "track",
                        "path"
                    ],
                    "antonyms": [
                        "lead"
                    ],
                    "common_phrases": [
                        "trail behind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проигрывать или отставать от соперника в соревновании или счете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отставать",
                                "target_lang_sense_clarification": "быть позади в развитии или счете"
                            },
                            {
                                "target_lang_word": "проигрывать",
                                "target_lang_sense_clarification": "уступать в счете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e53f242-caf5-330e-84fc-20bcd34c3cd2",
                    "sense_definition": "To drag something behind on the ground.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "drag_hang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The child walked sadly, <w>trailing</w> his blanket in the dirt.",
                            "target_lang_translations": {
                                "ru": "Ребенок грустно шел, <w>волоча</w> свое одеяло по грязи."
                            }
                        },
                        {
                            "text": "Be careful not to <w>trail</w> your coat through the mud.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, не <w>волочи</w> свое пальто по грязи."
                            }
                        }
                    ],
                    "synonyms": [
                        "track",
                        "path"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тащить что-то сзади по земле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волочить",
                                "target_lang_sense_clarification": "тянуть по земле"
                            },
                            {
                                "target_lang_word": "тащить",
                                "target_lang_sense_clarification": "перемещать с усилием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8cbc930-18e1-3117-a0ba-638c2f7b2b6c",
                    "sense_definition": "To hang loosely and drag along the ground or behind something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "drag_hang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bride's long white dress <w>trailed</w> behind her.",
                            "target_lang_translations": {
                                "ru": "Длинное белое платье невесты <w>тянулось</w> за ней."
                            }
                        },
                        {
                            "text": "Ivy vines <w>trailed</w> down the old stone wall.",
                            "target_lang_translations": {
                                "ru": "Плющ <w>свисал</w> вниз по старой каменной стене."
                            }
                        }
                    ],
                    "synonyms": [
                        "track",
                        "path"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свисать свободно и волочиться по земле или позади чего-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тянуться",
                                "target_lang_sense_clarification": "простираться вслед"
                            },
                            {
                                "target_lang_word": "свисать",
                                "target_lang_sense_clarification": "висеть вниз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b824fd2d-e686-329e-92aa-3660e06b8347",
                    "sense_definition": "To move or walk slowly and wearily, often falling behind others.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "move_slowly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tired children <w>trailed</w> behind their parents on the way home.",
                            "target_lang_translations": {
                                "ru": "Уставшие дети <w>плелись</w> позади родителей по дороге домой."
                            }
                        },
                        {
                            "text": "He <w>trailed</w> into the office late after a sleepless night.",
                            "target_lang_translations": {
                                "ru": "Он <w>притащился</w> в офис с опозданием после бессонной ночи."
                            }
                        }
                    ],
                    "synonyms": [
                        "track",
                        "path"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться или идти медленно и устало, часто отставая от других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плестись",
                                "target_lang_sense_clarification": "идти медленно, с трудом"
                            },
                            {
                                "target_lang_word": "тащиться",
                                "target_lang_sense_clarification": "разг. идти медленно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "046d7384-be54-3688-87fc-3b7eae9c194c",
                    "sense_definition": "A city in British Columbia, Canada.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place_name",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "My cousin lives in <w>Trail</w>, British Columbia.",
                            "target_lang_translations": {
                                "ru": "Мой двоюродный брат живет в <w>Трейле</w>, Британская Колумбия."
                            }
                        },
                        {
                            "text": "We visited the city of <w>Trail</w> during our trip to Canada.",
                            "target_lang_translations": {
                                "ru": "Мы посетили город <w>Трейл</w> во время нашей поездки в Канаду."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в Британской Колумбии, Канада."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Трейл",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23560568-5af3-36ce-973f-289018a7dcc7",
                    "sense_definition": "A city in Polk County, Minnesota, United States.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place_name",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "The small city of <w>Trail</w> is located in Minnesota.",
                            "target_lang_translations": {
                                "ru": "Небольшой город <w>Трейл</w> расположен в Миннесоте."
                            }
                        },
                        {
                            "text": "He was born in <w>Trail</w>, Minnesota.",
                            "target_lang_translations": {
                                "ru": "Он родился в <w>Трейле</w>, штат Миннесота."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в округе Полк, штат Миннесота, США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Трейл",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b23db7a8-427b-3206-b757-dac49857d0d4",
                    "sense_definition": "Stwierdzać, że coś jest prawdą lub zgadzać się z czyjąś opinią, często pomimo wcześniejszych wątpliwości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "admit_truth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <w>przyznać</w>, że twój pomysł jest naprawdę genialny.",
                            "target_lang_translations": {
                                "ru": "Должен <w>признать</w>, что твоя идея действительно гениальна."
                            }
                        },
                        {
                            "text": "Niechętnie <w>przyznaję</w> ci rację w tym sporze.",
                            "target_lang_translations": {
                                "ru": "Я неохотно <w>признаю</w> твою правоту в этом споре."
                            }
                        }
                    ],
                    "synonyms": [
                        "potwierdzać",
                        "zgadzać się"
                    ],
                    "antonyms": [
                        "zaprzeczać",
                        "negować"
                    ],
                    "common_phrases": [
                        "przyznawać rację",
                        "muszę przyznać",
                        "przyznawać słuszność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Утверждать, что что-то является правдой, или соглашаться с чьим-то мнением, часто вопреки прежним сомнениям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "признавать",
                                "target_lang_sense_clarification": "факт, истину, правоту"
                            },
                            {
                                "target_lang_word": "соглашаться",
                                "target_lang_sense_clarification": "с мнением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55ff6bad-e1bc-325a-87d7-8939c83339e7",
                    "sense_definition": "Oficjalnie przydzielać coś komuś, na przykład nagrodę, pieniądze, tytuł lub uprawnienia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "grant_award",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jury w tym roku nie <w>przyznawało</w> żadnych wyróżnień.",
                            "target_lang_translations": {
                                "ru": "В этом году жюри не <w>присуждало</w> никаких наград."
                            }
                        },
                        {
                            "text": "Uniwersytet co roku <w>przyznaje</w> stypendia najlepszym studentom.",
                            "target_lang_translations": {
                                "ru": "Университет каждый год <w>присуждает</w> стипендии лучшим студентам."
                            }
                        }
                    ],
                    "synonyms": [
                        "przydzielać",
                        "nadawać",
                        "udzielać"
                    ],
                    "antonyms": [
                        "odbierać",
                        "odmawiać"
                    ],
                    "common_phrases": [
                        "przyznawać nagrodę",
                        "przyznawać środki",
                        "przyznawać obywatelstwo",
                        "przyznawać punkty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально предоставлять что-либо кому-либо, например, награду, деньги, титул или права."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присуждать",
                                "target_lang_sense_clarification": "награду, премию, звание"
                            },
                            {
                                "target_lang_word": "предоставлять",
                                "target_lang_sense_clarification": "права, гражданство"
                            },
                            {
                                "target_lang_word": "назначать",
                                "target_lang_sense_clarification": "стипендию, пособие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee0a0a85-1832-3b10-82e6-ceb1e1e60e5b",
                    "sense_definition": "Potwierdzać swój udział w czymś (zazwyczaj negatywnym) lub ujawniać prawdę o sobie; używane z zaimkiem zwrotnym „się”.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "confess_guilt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oskarżony nie <w>przyznaje</w> się do winy przed sądem.",
                            "target_lang_translations": {
                                "ru": "Обвиняемый не <w>признаёт</w> своей вины перед судом."
                            }
                        },
                        {
                            "text": "Trudno jest <w>przyznać</w> się do błędu przed innymi ludźmi.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>признаться</w> в ошибке перед другими людьми."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyznawać"
                    ],
                    "antonyms": [
                        "wypierać się"
                    ],
                    "common_phrases": [
                        "przyznawać się do winy",
                        "przyznawać się do błędu",
                        "przyznawać się bez bicia"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подтверждать свое участие в чем-либо (обычно негативном) или раскрывать правду о себе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "признаваться",
                                "target_lang_sense_clarification": "в содеянном, в ошибке"
                            },
                            {
                                "target_lang_word": "сознаваться",
                                "target_lang_sense_clarification": "в вине"
                            }
                        ]
                    }
                }
            ]
        }
    ]

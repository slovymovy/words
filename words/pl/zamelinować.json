{
    "word_family": [
        "melina"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a346d647-f592-39e7-b190-8cebc555eb4b",
                    "sense_definition": "Ukryć kogoś lub siebie w bezpiecznym, odosobnionym miejscu (na melinie), zazwyczaj w celu uniknięcia spotkania z kimś lub ucieczki przed prawem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hiding_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę się gdzieś <zamelinować> na kilka dni, żeby nikt mnie nie znalazł.",
                            "target_lang_translations": {
                                "ru": "Мне нужно где-то <заныкаться> на несколько дней, чтобы меня никто не нашел."
                            }
                        },
                        {
                            "text": "Przestępcy postanowili <zamelinować> się w opuszczonym magazynie za miastem.",
                            "target_lang_translations": {
                                "ru": "Преступники решили <отсидеться> в заброшенном складе за городом."
                            }
                        }
                    ],
                    "synonyms": [
                        "ukryć",
                        "zaszyć",
                        "zadekować"
                    ],
                    "antonyms": [
                        "ujawnić",
                        "pokazać"
                    ],
                    "common_phrases": [
                        "zamelinować się na chacie",
                        "zamelinować się u kumpla"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo używane w mowie potocznej, często w kontekście kryminalnym lub żartobliwym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спрятать кого-то или спрятаться самому в безопасном, уединенном месте, обычно чтобы избежать встречи с кем-то или скрыться от закона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заныкаться",
                                "target_lang_sense_clarification": "слэнг; спрятаться"
                            },
                            {
                                "target_lang_word": "отсидеться",
                                "target_lang_sense_clarification": "переждать опасность в укрытии"
                            },
                            {
                                "target_lang_word": "залечь на дно",
                                "target_lang_sense_clarification": "фразеологизм; скрыться, прекратить активность"
                            },
                            {
                                "target_lang_word": "схорониться",
                                "target_lang_sense_clarification": "просторечие; спрятаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1bdd1ec-0523-336d-90a1-822fa3000d5a",
                    "sense_definition": "Schować jakiś przedmiot w miejscu niedostępnym dla innych, często w pośpiechu lub w tajemnicy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hiding_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodziej zdążył <zamelinować> skradzione pieniądze pod podłogą, zanim przyjechała policja.",
                            "target_lang_translations": {
                                "ru": "Вор успел <заныкать> украденные деньги под полом до приезда полиции."
                            }
                        },
                        {
                            "text": "Nie pamiętam, gdzie mogłem <zamelinować> zapasowe klucze do samochodu.",
                            "target_lang_translations": {
                                "ru": "Не помню, куда я мог <засунуть> запасные ключи от машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "schować",
                        "ukryć",
                        "zadekować"
                    ],
                    "antonyms": [
                        "znaleźć",
                        "wyciągnąć"
                    ],
                    "common_phrases": [
                        "zamelinować towar",
                        "zamelinować fanty"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спрятать какой-либо предмет в недоступном для других месте, часто в спешке или тайно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заныкать",
                                "target_lang_sense_clarification": "слэнг; спрятать вещь"
                            },
                            {
                                "target_lang_word": "припрятать",
                                "target_lang_sense_clarification": "спрятать на будущее или для сохранности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b581600d-66df-35f9-b4e7-fb615db06d68",
                    "sense_definition": "Plaatsvindend of komend voordat iets anders begint; in de tijd eerder komend.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "preceding_relational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er was een korte toespraak <w>voorafgaand</w> aan het concert.",
                            "target_lang_translations": {
                                "en": "There was a short speech <w>prior</w> to the concert.",
                                "ru": "<w>Перед</w> концертом была короткая речь."
                            }
                        },
                        {
                            "text": "We dronken samen koffie <w>voorafgaand</w> aan de belangrijke vergadering.",
                            "target_lang_translations": {
                                "en": "We drank coffee together <w>prior</w> to the important meeting.",
                                "ru": "Мы пили кофе вместе <w>перед</w> важным собранием."
                            }
                        }
                    ],
                    "common_phrases": [
                        "voorafgaand aan"
                    ],
                    "target_lang_definitions": {
                        "en": "Occurring or coming before something else begins; coming earlier in time.",
                        "ru": "Происходящий или идущий перед началом чего-либо другого; более ранний по времени."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prior",
                                "target_lang_sense_clarification": "often used in the phrase 'prior to'"
                            },
                            {
                                "target_lang_word": "preceding",
                                "target_lang_sense_clarification": "coming before in time or order"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "предшествующий",
                                "target_lang_sense_clarification": "идущий перед чем-то во времени или порядке"
                            },
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "часто используется в конструкции 'voorafgaand aan' (до/перед)"
                            },
                            {
                                "target_lang_word": "перед",
                                "target_lang_sense_clarification": "указывает на предшествование событию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0153a5c5-dc6f-3e81-8e6e-6561ea14a554",
                    "sense_definition": "Wat direct ervoor gebeurd is of plaatsvond; vorig.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "preceding_attributive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de <w>voorafgaande</w> week hebben we hard gewerkt aan het project.",
                            "target_lang_translations": {
                                "en": "In the <w>previous</w> week we worked hard on the project.",
                                "ru": "На <w>предыдущей</w> неделе мы усердно работали над проектом."
                            }
                        },
                        {
                            "text": "Het <w>voorafgaande</w> hoofdstuk besprak de basisprincipes van de theorie.",
                            "target_lang_translations": {
                                "en": "The <w>preceding</w> chapter discussed the basic principles of the theory.",
                                "ru": "<w>Предыдущая</w> глава обсуждала основные принципы теории."
                            }
                        }
                    ],
                    "synonyms": [
                        "vorig",
                        "eerder"
                    ],
                    "antonyms": [
                        "volgend",
                        "later"
                    ],
                    "common_phrases": [
                        "de voorafgaande dag",
                        "het voorafgaande jaar"
                    ],
                    "target_lang_definitions": {
                        "en": "That which happened or took place directly before; previous.",
                        "ru": "То, что произошло или имело место непосредственно перед этим; предыдущий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "previous",
                                "target_lang_sense_clarification": "existing or occurring before in time or order"
                            },
                            {
                                "target_lang_word": "preceding",
                                "target_lang_sense_clarification": "immediately previous"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "предыдущий",
                                "target_lang_sense_clarification": "существующий или происходящий ранее"
                            },
                            {
                                "target_lang_word": "предшествующий",
                                "target_lang_sense_clarification": "непосредственно предшествующий"
                            }
                        ]
                    }
                }
            ]
        },

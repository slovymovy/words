            "senses": [
                {
                    "sense_id": "cc26673c-a631-31c2-a71c-9452f584bb13",
                    "sense_definition": "To live or reside in a specific place, such as a house, apartment, or room.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reside",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family has <w>occupied</w> this farmhouse for over fifty years.",
                            "target_lang_translations": {
                                "ru": "Семья <w>занимает</w> этот фермерский дом уже более пятидесяти лет."
                            }
                        },
                        {
                            "text": "Most of the apartments in the building are <w>occupied</w> by students.",
                            "target_lang_translations": {
                                "ru": "Большинство квартир в этом здании <w>заняты</w> студентами."
                            }
                        }
                    ],
                    "synonyms": [
                        "coitize",
                        "employ",
                        "busy",
                        "go to bed with",
                        "sleep with",
                        "copulate with"
                    ],
                    "antonyms": [

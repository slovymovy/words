{
    "word_family": [
        "strugać",
        "strug",
        "strużka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b43897a2-8663-3926-a6af-dda494a29283",
                    "sense_definition": "Ciecz płynąca wąskim, ciągłym pasmem lub silnym strumieniem, często pod ciśnieniem lub w dużej ilości.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "stream_liquid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z pękniętej rury tryskała silna <struga> zimnej wody.",
                            "target_lang_translations": {
                                "ru": "Из лопнувшей трубы била сильная <струя> холодной воды."
                            }
                        },
                        {
                            "text": "Samochody jechały powoli przez gęste <strugi> ulewnego deszczu.",
                            "target_lang_translations": {
                                "ru": "Автомобили медленно ехали сквозь густые <потоки> проливного дождя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "strugi deszczu",
                        "lać się strugami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкость, текущая узкой, непрерывной полосой или сильным потоком, часто под давлением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "струя",
                                "target_lang_sense_clarification": "узкий поток жидкости"
                            },
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "движущаяся масса воды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46b034cb-c2b9-324e-a34c-97e07d3620f9",
                    "sense_definition": "Niewielki, naturalny ciek wodny płynący w wyżłobionym korycie; mała rzeczka.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "stream_brook",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez środek łąki płynęła czysta <struga>, w której poiły się sarny.",
                            "target_lang_translations": {
                                "ru": "Посреди луга текла чистая <речка>, из которой пили косули."
                            }
                        },
                        {
                            "text": "Dzieci puszczały papierowe łódki na bystrym nurcie leśnej <strugi>.",
                            "target_lang_translations": {
                                "ru": "Дети пускали бумажные кораблики по быстрому течению лесного <ручья>."
                            }
                        }
                    ],
                    "synonyms": [
                        "potok",
                        "potoczek",
                        "strumyczek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой естественный водный поток, текущий в русле; маленькая речка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ручей",
                                "target_lang_sense_clarification": "небольшой водный поток"
                            },
                            {
                                "target_lang_word": "речка",
                                "target_lang_sense_clarification": "маленькая река"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3b0e2154-8c67-31d2-9f50-821c6cd675cc",
                    "sense_definition": "Forma czasownika <w>strugać</w> (3. osoba liczby pojedynczej, czas teraźniejszy).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek siedzi na ganku i <struga> kawałek drewna scyzorykiem.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка сидит на крыльце и <строгает> кусок дерева перочинным ножом."
                            }
                        },
                        {
                            "text": "Każdego wieczora uczeń <struga> ołówki na następny dzień szkoły.",
                            "target_lang_translations": {
                                "ru": "Каждый вечер ученик <точит> карандаши на следующий школьный день."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "3. osoba liczby pojedynczej, czas teraźniejszy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола <w>строгать</w> (3-е лицо единственного числа, настоящее время)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строгает",
                                "target_lang_sense_clarification": "обрабатывает дерево"
                            },
                            {
                                "target_lang_word": "точит",
                                "target_lang_sense_clarification": "заостряет (карандаши)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9a079129-aaf6-3f70-ba90-ee6513d4a95b",
                    "sense_definition": "Forma dopełniacza liczby pojedynczej rzeczownika <w>strug</w> (narzędzie stolarskie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stolarz nie mógł znaleźć swojego ulubionego <struga> w warsztacie.",
                            "target_lang_translations": {
                                "ru": "Столяр не мог найти своего любимого <рубанка> в мастерской."
                            }
                        },
                        {
                            "text": "Do wygładzenia tej deski użycie ręcznego <struga> będzie niezbędne.",
                            "target_lang_translations": {
                                "ru": "Для выравнивания этой доски использование ручного <рубанка> будет необходимо."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "dopełniacz liczby pojedynczej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа единственного числа существительного <w>рубанок</w> (столярный инструмент)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рубанка",
                                "target_lang_sense_clarification": "форма слова"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "72e8b53d-c99e-37d1-946c-8ad10dec5652",
                    "sense_definition": "Biały, błyszczący metal szlachetny, surowiec do wyrobu między innymi biżuterii i monet",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłam wczoraj piękne kolczyki ze <w>srebra</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера я купила красивые серьги из <w>серебра</w>."
                            }
                        },
                        {
                            "text": "Czyste <w>srebro</w> jest zbyt miękkie do produkcji trwałych narzędzi.",
                            "target_lang_translations": {
                                "ru": "Чистое <w>серебро</w> слишком мягкое для изготовления прочных инструментов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "biżuteria ze srebra",
                        "czyste srebro",
                        "monety ze srebra"
                    ],
                    "target_lang_definitions": {
                        "ru": "Белый блестящий драгоценный металл, сырьё для изготовления ювелирных изделий и монет"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серебро",
                                "target_lang_sense_clarification": "металл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bec6eeb9-c9ac-3e41-baba-7a392e534682",
                    "sense_definition": "Srebrny medal przyznawany za zajęcie drugiego miejsca w zawodach sportowych",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polscy siatkarze zdobyli <w>srebro</w> na ostatnich igrzyskach olimpijskich.",
                            "target_lang_translations": {
                                "ru": "Польские волейболисты завоевали <w>серебро</w> на последних Олимпийских играх."
                            }
                        },
                        {
                            "text": "Nasz zawodnik był zawiedziony, że udało mu się wywalczyć tylko <w>srebro</w>.",
                            "target_lang_translations": {
                                "ru": "Наш спортсмен был разочарован тем, что ему удалось завоевать только <w>серебро</w>."
                            }
                        }
                    ],
                    "common_phrases": [

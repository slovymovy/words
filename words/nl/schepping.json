                },
                {
                    "sense_id": "60818cbe-0b58-3a1a-a896-ee1171f20b3f",
                    "sense_definition": "Iets dat gemaakt of ontworpen is, vaak een kunstwerk of uitvinding.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De modeontwerper liet zijn nieuwste <w>schepping</w> zien op de catwalk.",
                            "target_lang_translations": {
                                "ru": "Модельер показал своё новейшее <w>творение</w> на подиуме."
                            }
                        },
                        {
                            "text": "Dit gebouw is een bijzondere <w>schepping</w> van de beroemde architect.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7afdfc90-76f0-3d8a-a1af-b15eaf5080f7",
                    "sense_definition": "Восстановление обветшавших или разрушенных памятников искусства, архитектуры или старинных предметов в их первоначальном виде.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "restoration_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Когда проходила <w>реставрация</w> здания, фасад закрыли специальной сеткой.",
                            "target_lang_translations": {
                                "en": "When the building's <w>restoration</w> was taking place, the facade was covered with a special mesh."
                            }
                        },
                        {
                            "text": "В музее завершилась длительная <w>реставрация</w> знаменитой картины.",
                            "target_lang_translations": {
                                "en": "The lengthy <w>restoration</w> of the famous painting has been completed at the museum."
                            }
                        }
                    ],
                    "synonyms": [
                        "восстановление"
                    ],
                    "antonyms": [
                        "уничтожение"
                    ],
                    "common_phrases": [
                        "реставрация здания",
                        "реставрация картины",
                        "реставрация памятника",
                        "провести реставрацию"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of repairing and returning a building, work of art, or antique object to its original condition."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "restoration",
                                "target_lang_sense_clarification": "of buildings, art, etc."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e58831ed-d9d4-3fab-a9b6-8df38785bf23",
                    "sense_definition": "Восстановление свергнутого политического строя или ранее правившей династии.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "restoration_political",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Историки обсуждают причины, по которым <w>реставрация</w> монархии не состоялась.",
                            "target_lang_translations": {
                                "en": "Historians are discussing the reasons why the <w>restoration</w> of the monarchy did not happen."
                            }
                        },
                        {
                            "text": "После переворота началась постепенная <w>реставрация</w> старых порядков.",
                            "target_lang_translations": {
                                "en": "After the coup, a gradual <w>restoration</w> of the old order began."
                            }
                        }
                    ],
                    "synonyms": [
                        "восстановление"
                    ],
                    "antonyms": [
                        "ликвидация"
                    ],
                    "common_phrases": [
                        "реставрация монархии",
                        "реставрация режима",
                        "реставрация династии"
                    ],
                    "target_lang_definitions": {
                        "en": "The return of a former ruler, dynasty, or political system to power."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "restoration",
                                "target_lang_sense_clarification": "political"
                            }
                        ]
                    }
                }
            ]
        }

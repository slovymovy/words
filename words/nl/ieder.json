                },
                {
                    "sense_id": "604f79b2-2f58-3262-a32b-24582eb48d1d",
                    "sense_definition": "Om het even welk; willekeurig welk (vaak gebruikt in vaste uitdrukkingen).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "any_arbitrary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten in <w>ieder</w> geval zorgen dat we op tijd zijn.",
                            "target_lang_translations": {
                                "ru": "Мы в <w>любом</w> случае должны позаботиться о том, чтобы быть вовремя."
                            }
                        },
                        {
                            "text": "Je kunt me op <w>ieder</w> gewenst moment bellen voor advies.",
                            "target_lang_translations": {
                                "ru": "Ты можешь позвонить мне за советом в <w>любой</w> момент."
                            }
                        }
                    ],
                    "synonyms": [
                        "elk"
                    ],
                    "common_phrases": [
                        "in ieder geval",
                        "te ieder tijde"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой; какой угодно (часто используется в устойчивых выражениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любой",
                                "target_lang_sense_clarification": "какой угодно"
                            },
                            {
                                "target_lang_word": "всякий",
                                "target_lang_sense_clarification": "различный, любой"
                            }
                        ]
                    }
                }
            ]
        },
        {

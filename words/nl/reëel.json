                    ],
                    "target_lang_definitions": {
                        "en": "Adjusted for the influence of inflation; expressed in purchasing power.",
                        "ru": "Скорректированный с учетом инфляции; выраженный в покупательной способности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "real",
                                "target_lang_sense_clarification": "economics"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "реальный",
                                "target_lang_sense_clarification": "экономический термин (о доходе, стоимости)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5c4efa6-a329-35ca-84fa-7457e02d086a",

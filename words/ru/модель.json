    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "87f48602-4987-3edd-81cc-ba6fe9271742",
                    "sense_definition": "Тип, марка или образец конструкции какого-либо изделия (например, автомобиля, телефона или бытовой техники).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "product_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В магазине появилась новая <модель> этого телефона.",
                            "target_lang_translations": {
                                "en": "A new <model> of this phone has appeared in the store."
                            }
                        },
                        {
                            "text": "Эта <модель> машины очень популярна среди водителей.",
                            "target_lang_translations": {
                                "en": "This car <model> is very popular among drivers."
                            }
                        }
                    ],
                    "synonyms": [
                        "марка",
                        "тип"
                    ],
                    "common_phrases": [
                        "новая модель",
                        "старая модель",
                        "модель телефона",
                        "последняя модель"
                    ],
                    "target_lang_definitions": {
                        "en": "A specific type, design, or version of a product (e.g., a car, phone, or appliance)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "model",
                                "target_lang_sense_clarification": "product version or design"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea9f953e-376b-3b6a-b1e7-3a23529e7061",
                    "sense_definition": "Человек (обычно женщина), который демонстрирует одежду на показах мод или позирует для фотографий в журналах.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "person_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Известная <модель> вышла на подиум в красивом платье.",
                            "target_lang_translations": {
                                "en": "A famous <model> walked out onto the catwalk in a beautiful dress."
                            }
                        },
                        {
                            "text": "Моя сестра мечтает работать <моделью> в большом агентстве.",
                            "target_lang_translations": {
                                "en": "My sister dreams of working as a <model> in a big agency."
                            }
                        }
                    ],
                    "synonyms": [
                        "манекенщица"
                    ],
                    "common_phrases": [
                        "работать моделью",
                        "топ-модель",
                        "известная модель",
                        "агентство моделей"
                    ],
                    "target_lang_definitions": {
                        "en": "A person employed to display clothes, fashion items, or pose for photographs."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "model",
                                "target_lang_sense_clarification": "fashion profession"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed98ebbc-db35-347c-8ef7-552dd8fed454",
                    "sense_definition": "Уменьшенная копия реального объекта (здания, корабля, самолёта), сделанная с сохранением пропорций.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На столе у архитектора стояла точная <модель> будущего здания.",
                            "target_lang_translations": {
                                "en": "An exact <model> of the future building stood on the architect's table."
                            }
                        },
                        {
                            "text": "Мальчик любит клеить сборные <модели> танков и самолётов.",
                            "target_lang_translations": {
                                "en": "The boy likes gluing together assembly <models> of tanks and airplanes."
                            }
                        }
                    ],
                    "synonyms": [
                        "макет"
                    ],
                    "common_phrases": [
                        "модель самолёта",
                        "сборная модель",
                        "масштабная модель",
                        "собирать модели"
                    ],
                    "target_lang_definitions": {
                        "en": "A small-scale copy or representation of a physical object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "model",
                                "target_lang_sense_clarification": "miniature replica"
                            },
                            {
                                "target_lang_word": "scale model",
                                "target_lang_sense_clarification": "precise reduced copy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56de8edd-2546-3132-8030-a7f6ec00e30e",
                    "sense_definition": "Схема, теория или абстрактное описание, которое объясняет какой-либо процесс, явление или систему.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учёные создали компьютерную <модель> изменения климата на планете.",
                            "target_lang_translations": {
                                "en": "Scientists created a computer <model> of climate change on the planet."
                            }
                        },
                        {
                            "text": "Эта экономическая <модель> помогает делать точные прогнозы развития рынка.",
                            "target_lang_translations": {
                                "en": "This economic <model> helps make accurate forecasts of market development."
                            }
                        }
                    ],
                    "synonyms": [
                        "схема",
                        "представление"
                    ],
                    "common_phrases": [
                        "математическая модель",
                        "экономическая модель",
                        "модель поведения",
                        "ролевая модель"
                    ],
                    "target_lang_definitions": {
                        "en": "A theoretical description or simulation used to explain a system, process, or phenomenon."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "model",
                                "target_lang_sense_clarification": "theoretical or computer simulation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46c405ec-ec84-3902-bca0-46cb0d5baa60",
                    "sense_definition": "Человек, который позирует художнику или скульптору для создания произведения искусства.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Художник долго искал подходящую <модель> для своего нового портрета.",
                            "target_lang_translations": {
                                "en": "The artist looked for a suitable <model> for his new portrait for a long time."
                            }
                        },
                        {
                            "text": "Уставшая <модель> попросила сделать перерыв после часа позирования.",
                            "target_lang_translations": {
                                "en": "The tired <model> asked to take a break after an hour of posing."
                            }
                        }
                    ],
                    "synonyms": [
                        "натурщица"
                    ],
                    "common_phrases": [
                        "живая модель",
                        "искать модель",
                        "рисовать с модели"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who poses for an artist, sculptor, or photographer to create a work of art."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "model",
                                "target_lang_sense_clarification": "subject for art"
                            },
                            {
                                "target_lang_word": "sitter",
                                "target_lang_sense_clarification": "person posing for a portrait"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0d9567b-a94a-32be-a402-4d68f5ff5b1d",
                    "sense_definition": "Образец изделия, с которого снимается форма для отливки или воспроизведения в другом материале.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мастер аккуратно подготовил восковую <модель> для литья бронзовой статуэтки.",
                            "target_lang_translations": {
                                "en": "The craftsman carefully prepared the wax <model> for casting the bronze figurine."
                            }
                        },
                        {
                            "text": "С этой гипсовой <модели> будут делать несколько металлических копий.",
                            "target_lang_translations": {
                                "en": "Several metal copies will be made from this plaster <model>."
                            }
                        }
                    ],
                    "synonyms": [
                        "образец"
                    ],
                    "common_phrases": [
                        "восковая модель",
                        "литейная модель",
                        "изготовление модели"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An original object or pattern used to create a mold for casting or reproduction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "model",
                                "target_lang_sense_clarification": "template for molding"
                            },
                            {
                                "target_lang_word": "pattern",
                                "target_lang_sense_clarification": "technical casting term"
                            }
                        ]
                    }
                }
            ]
        }
    ]

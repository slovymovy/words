            "pos": "verb",
            "senses": [
                {
                    "sense_id": "94024430-cbdc-35a7-814b-4598431f889f",
                    "sense_definition": "Переносить тяжести или громоздкие предметы с места на место, часто с усилием.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "carry_drag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Грузчики целый день <таскали> тяжёлые коробки на пятый этаж без лифта.",
                            "target_lang_translations": {
                                "en": "The movers <lugged> heavy boxes to the fifth floor without an elevator all day."
                            }
                        },
                        {
                            "text": "Тебе вредно <таскать> такие тяжести, лучше попроси кого-нибудь о помощи.",
                            "target_lang_translations": {
                                "en": "It is bad for you to <carry> such heavy loads; you'd better ask someone for help."
                            }
                        }
                    ],
                    "synonyms": [
                        "носить",
                        "перетаскивать"
                    ],
                    "common_phrases": [
                        "таскать тяжести",
                        "таскать мешки",
                        "таскать воду",
                        "еле ноги таскать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies effort or repetitive action compared to neutral 'носить'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To carry heavy or bulky objects from place to place, often with effort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "general term for moving something while holding it"
                            },
                            {
                                "target_lang_word": "lug",
                                "target_lang_sense_clarification": "to carry something heavy or clumsy with effort"
                            },
                            {
                                "target_lang_word": "haul",
                                "target_lang_sense_clarification": "to pull or carry with effort"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d0351cb-348f-38dc-a04c-0a660a796389",
                    "sense_definition": "Водить кого-либо за собой повсюду, часто против воли или без надобности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "carry_drag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители вынуждены везде <таскать> маленького ребёнка с собой, так как не с кем его оставить.",
                            "target_lang_translations": {
                                "en": "The parents are forced to <drag> their small child <around> with them everywhere, as there is no one to leave him with."
                            }
                        },
                        {
                            "text": "Она любила <таскать> мужа по магазинам и выставкам, хотя он этому сопротивлялся.",
                            "target_lang_translations": {
                                "en": "She loved to <drag> her husband <around> shops and exhibitions, even though he resisted it."
                            }
                        }
                    ],
                    "synonyms": [
                        "водить"
                    ],
                    "common_phrases": [
                        "таскать за собой",
                        "таскать по судам",
                        "таскать по врачам"

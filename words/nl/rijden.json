                {
                    "sense_id": "4b29e061-6b11-32fe-9a3e-708b5de60368",
                    "sense_definition": "Zich verplaatsen met behulp van een voertuig, zoals een auto of fiets",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>rij</w> elke dag met de auto naar mijn werk.",
                            "target_lang_translations": {
                                "en": "I <w>drive</w> to work by car every day.",
                                "ru": "Я каждый день <w>езжу</w> на работу на машине."
                            }
                        },
                        {
                            "text": "Zullen we samen naar het feest <w>rijden</w>?",
                            "target_lang_translations": {
                                "en": "Shall we <w>drive</w> to the party together?",
                                "ru": "Может, <w>поедем</w> на вечеринку вместе?"
                            }
                        }
                    ],
                    "synonyms": [
                        "chaufferen",
                        "sturen"
                    ],
                    "antonyms": [
                        "lopen",
                        "wandelen",
                        "stilstaan"
                    ],
                    "common_phrases": [
                        "auto rijden",
                        "te hard rijden",
                        "zwart rijden",
                        "links rijden"
                    ],
                    "target_lang_definitions": {
                        "en": "To move using a vehicle, such as a car or bicycle",
                        "ru": "Перемещаться с помощью транспортного средства, например, на машине или велосипеде"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "operate a car/vehicle"
                            },
                            {
                                "target_lang_word": "ride",
                                "target_lang_sense_clarification": "a bicycle/motorcycle"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ездить",
                                "target_lang_sense_clarification": "регулярно или в разных направлениях"
                            },
                            {
                                "target_lang_word": "ехать",
                                "target_lang_sense_clarification": "двигаться в конкретный момент в одном направлении"
                            },
                            {
                                "target_lang_word": "водить",
                                "target_lang_sense_clarification": "управлять автомобилем"
                            }

            "senses": [
                {
                    "sense_id": "93f1e1cf-cf5b-3f34-898c-28ae3daa0538",
                    "sense_definition": "De lucht die zich buiten gebouwen bevindt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door het open raam kwam frisse <w>buitenlucht</w> naar binnen.",
                            "target_lang_translations": {
                                "en": "Fresh <w>outside air</w> came in through the open window.",
                                "ru": "Сквозь открытое окно внутрь поступал свежий <w>уличный воздух</w>."
                            }
                        },
                        {
                            "text": "De <w>buitenlucht</w> is meestal veel schoner en dus gezonder dan de binnenlucht.",
                            "target_lang_translations": {
                                "en": "The <w>outdoor air</w> is usually much cleaner and therefore healthier than indoor air.",
                                "ru": "<w>Наружный воздух</w> обычно намного чище и, следовательно, полезнее, чем воздух внутри помещений."
                            }
                        }
                    ],
                    "antonyms": [
                        "binnenlucht"
                    ],
                    "common_phrases": [
                        "frisse buitenlucht",
                        "gezonde buitenlucht"
                    ],
                    "target_lang_definitions": {
                        "en": "The air found outside buildings.",
                        "ru": "Воздух, находящийся за пределами зданий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "outside air",
                                "target_lang_sense_clarification": "air outside of a building"
                            },
                            {

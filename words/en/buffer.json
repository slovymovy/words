            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "6c22bf98-c886-32f0-ab5b-cfbee60ae876",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Buffer</w> was known in the town for his bakery.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Баффер</w> был известен в городе своей пекарней."
                            }
                        },
                        {
                            "text": "We invited the <w>Buffer</w> family to the dinner party.",
                            "target_lang_translations": {

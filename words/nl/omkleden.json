                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переодеваться",
                                "target_lang_sense_clarification": "менять одежду"
                            },
                            {
                                "target_lang_word": "сменить одежду",
                                "target_lang_sense_clarification": "надеть чистое или другое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57c00dce-9ac8-330e-9460-1fa69eb2dca3",
                    "sense_definition": "Onderbouwen of voorzien van argumenten en redenen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "substantiate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De advocaat <omkleedde> zijn pleidooi met overtuigende bewijzen.",
                            "target_lang_translations": {
                                "ru": "Адвокат <подкрепил> свою речь убедительными доказательствами."
                            }
                        },
                        {
                            "text": "Elk verzoek tot subsidie moet met redenen <omkleed> zijn.",
                            "target_lang_translations": {
                                "ru": "Каждый запрос на субсидию должен быть <обоснован> причинами."
                            }
                        }
                    ],
                    "synonyms": [
                        "onderbouwen",
                        "motiveren",
                        "staven"
                    ],
                    "common_phrases": [
                        "met redenen omkleed"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обосновывать или подкреплять аргументами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обосновывать",
                                "target_lang_sense_clarification": "подкреплять доводами"
                            },
                            {
                                "target_lang_word": "подкреплять",
                                "target_lang_sense_clarification": "усиливать доказательствами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2dcbfd9-e636-320c-b834-1be37f63eb3b",
                    "sense_definition": "Aan alle kanten omgeven, bedekken of omhullen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "substantiate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dichte mist <omkleedde> de toppen van de bergen.",
                            "target_lang_translations": {
                                "ru": "Густой туман <окутал> вершины гор."
                            }
                        },
                        {
                            "text": "Het mysterie dat deze oude legende <omkleedt>, is nooit opgelost.",
                            "target_lang_translations": {
                                "ru": "Тайна, которая <окутывает> эту древнюю легенду, так и не была разгадана."
                            }
                        }
                    ],
                    "synonyms": [
                        "omhullen",
                        "omgeven",
                        "bedekken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Окружать, покрывать или окутывать со всех сторон."
                    },

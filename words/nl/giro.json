                {
                    "sense_id": "7cfe74a8-74ba-3167-8e38-098697bb2a8f",
                    "sense_definition": "De veelgebruikte naam voor de Ronde van Italië, een belangrijke meerdaagse wielerwedstrijd.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cycling_event",
                    "name_type": "event_name",
                    "examples": [
                        {
                            "text": "Veel bekende renners doen dit jaar mee aan de <w>giro</w>.",
                            "target_lang_translations": {
                                "en": "Many famous riders are participating in the <w>Giro</w> this year.",
                                "ru": "В этом году многие известные гонщики участвуют в <w>Джиро</w>."
                            }
                        },
                        {
                            "text": "Hij droomt ervan om ooit een etappe in de <w>giro</w> te winnen.",
                            "target_lang_translations": {
                                "en": "He dreams of one day winning a stage in the <w>Giro</w>.",
                                "ru": "Он мечтает когда-нибудь выиграть этап на <w>Джиро</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Ronde van Italië"
                    ],
                    "common_phrases": [
                        "de Giro winnen",
                        "etappe in de Giro",
                        "tijdens de Giro"
                    ],
                    "target_lang_definitions": {
                        "en": "The common name for the Tour of Italy, a major multi-day cycling race.",
                        "ru": "Распространенное название «Джиро д’Италия», крупной многодневной велогонки."

{
    "word_family": [
        "plainly",
        "plainness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "35327cc3-d7e8-3563-8405-b7e9c94facb6",
                    "sense_definition": "Simple in design, character, or appearance; without decoration, pattern, or luxury.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "simple_unaltered",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to wear a <w>plain</w> white dress to the summer party.",
                            "target_lang_translations": {
                                "ru": "Она решила надеть <w>простое</w> белое платье на летнюю вечеринку."
                            }
                        },
                        {
                            "text": "The room was clean but <w>plain</w>, with only a bed and a small desk.",
                            "target_lang_translations": {
                                "ru": "Комната была чистой, но <w>простой</w>, всего лишь с кроватью и маленьким столом."
                            }
                        }
                    ],
                    "synonyms": [
                        "simple",
                        "unadorned",
                        "bare",
                        "modest",
                        "unembellished"
                    ],
                    "antonyms": [
                        "fancy",
                        "decorative",
                        "ornate"
                    ],
                    "common_phrases": [
                        "plain paper",
                        "plain clothes",
                        "plain background"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простой по дизайну, характеру или внешнему виду; без украшений, узоров или излишеств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просто́й",
                                "target_lang_sense_clarification": "несложный, обычный, без украшений"
                            },
                            {
                                "target_lang_word": "одното́нный",
                                "target_lang_sense_clarification": "об одежде или ткани: одного цвета, без узора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "beae925e-6bbb-35a2-8bb7-53cd7415a285",
                    "sense_definition": "Having no additional ingredients, spices, or flavors added.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "simple_unaltered",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I prefer eating <w>plain</w> yogurt with a little bit of fresh fruit.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю есть <w>натуральный</w> йогурт с небольшим количеством свежих фруктов."
                            }
                        },
                        {
                            "text": "The children asked for <w>plain</w> pasta without any sauce or cheese.",
                            "target_lang_translations": {
                                "ru": "Дети попросили <w>простые</w> макароны без соуса и сыра."
                            }
                        }
                    ],
                    "synonyms": [
                        "simple",
                        "unseasoned"
                    ],
                    "common_phrases": [
                        "plain water",
                        "plain chocolate",
                        "plain flour"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без добавления дополнительных ингредиентов, специй или ароматизаторов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "без доба́вок",
                                "target_lang_sense_clarification": "об еде: в чистом виде, без примесей"
                            },
                            {
                                "target_lang_word": "натура́льный",
                                "target_lang_sense_clarification": "без искусственных добавок (часто о йогурте)"
                            },
                            {
                                "target_lang_word": "просто́й",
                                "target_lang_sense_clarification": "обычный, без изысков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d32d1d64-1c54-343e-babb-6f4c4d0cdbd3",
                    "sense_definition": "Easy to see, hear, or understand; clear to the mind.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "obvious_clear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was <w>plain</w> to everyone in the room that he was very nervous.",
                            "target_lang_translations": {
                                "ru": "Всем в комнате было <w>очевидно</w>, что он очень нервничал."
                            }
                        },
                        {
                            "text": "Please write the instructions in <w>plain</w> English so everyone can understand.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, напишите инструкции на <w>простом</w> английском, чтобы все могли понять."
                            }
                        }
                    ],
                    "synonyms": [
                        "obvious",
                        "clear",
                        "explicit",
                        "evident"
                    ],
                    "common_phrases": [
                        "plain to see",
                        "make it plain",
                        "plain English"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкий для восприятия или понимания; очевидный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очеви́дный",
                                "target_lang_sense_clarification": "ясный, несомненный"
                            },
                            {
                                "target_lang_word": "поня́тный",
                                "target_lang_sense_clarification": "доступный для понимания"
                            },
                            {
                                "target_lang_word": "просто́й",
                                "target_lang_sense_clarification": "несложный для понимания (о языке)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4952e7d8-0cff-33f5-b172-9ce6c2bff802",
                    "sense_definition": "Used to emphasize that something is bad, foolish, or extreme; total or absolute.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "obvious_clear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is just <w>plain</w> wrong to treat people with such disrespect.",
                            "target_lang_translations": {
                                "ru": "Относиться к людям с таким неуважением — просто <w>совершенно</w> неправильно."
                            }
                        },
                        {
                            "text": "His excuse for being late was just <w>plain</w> nonsense.",
                            "target_lang_translations": {
                                "ru": "Его оправдание опоздания было <w>полной</w> чушью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "plain stupid",
                        "plain wrong",
                        "just plain crazy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления отрицательной характеристики; полный, абсолютный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по́лный",
                                "target_lang_sense_clarification": "абсолютный (о чепухе, глупости)"
                            },
                            {
                                "target_lang_word": "соверше́нный",
                                "target_lang_sense_clarification": "абсолютный, безусловный"
                            },
                            {
                                "target_lang_word": "про́сто",
                                "target_lang_sense_clarification": "усилительная частица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a351890-d2c9-37ca-bc39-8f0ba04ecd8b",
                    "sense_definition": "Honest and direct in speech or behavior, sometimes to the point of being rude.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "honest_direct",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Let me be <w>plain</w> with you: I do not agree with your plan at all.",
                            "target_lang_translations": {
                                "ru": "Позволь мне быть с тобой <w>откровенным</w>: я вовсе не согласен с твоим планом."
                            }
                        },
                        {
                            "text": "He is a <w>plain</w> man who always speaks the truth without hesitation.",
                            "target_lang_translations": {
                                "ru": "Он <w>прямой</w> человек, который всегда говорит правду, не колеблясь."
                            }
                        }
                    ],
                    "synonyms": [
                        "frank",
                        "honest",
                        "sincere"
                    ],
                    "common_phrases": [
                        "plain speaking",
                        "plain truth",
                        "be plain with someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Честный и прямой в речи или поведении, иногда до грубости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямо́й",
                                "target_lang_sense_clarification": "откровенный, без утайки"
                            },
                            {
                                "target_lang_word": "открове́нный",
                                "target_lang_sense_clarification": "искренний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7a03f83-590c-3001-87c3-5184b74fd883",
                    "sense_definition": "Not beautiful or attractive; ordinary-looking.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "appearance_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She worried that she had a <w>plain</w> face compared to her glamorous sister.",
                            "target_lang_translations": {
                                "ru": "Она переживала, что у нее <w>невзрачное</w> лицо по сравнению с ее гламурной сестрой."
                            }
                        },
                        {
                            "text": "The actor played a <w>plain</w> woman who discovers her inner beauty.",
                            "target_lang_translations": {
                                "ru": "Актер сыграл <w>некрасивую</w> женщину, которая открывает свою внутреннюю красоту."
                            }
                        }
                    ],
                    "synonyms": [
                        "ordinary"
                    ],
                    "common_phrases": [
                        "plain face",
                        "plain Jane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Некрасивый или непривлекательный; с заурядной внешностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невзра́чный",
                                "target_lang_sense_clarification": "не привлекающий внимания внешностью"
                            },
                            {
                                "target_lang_word": "некраси́вый",
                                "target_lang_sense_clarification": "лишенный красоты"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc859d23-9961-3bdb-9115-51550ab80a0e",
                    "sense_definition": "A large area of flat land with few trees.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Huge herds of buffalo used to roam across the grassy <w>plain</w>.",
                            "target_lang_translations": {
                                "ru": "Огромные стада бизонов раньше бродили по травянистой <w>равнине</w>."
                            }
                        },
                        {
                            "text": "The river winds its way through the coastal <w>plain</w> before reaching the sea.",
                            "target_lang_translations": {
                                "ru": "Река извивается по прибрежной <w>равнине</w>, прежде чем достичь моря."
                            }
                        }
                    ],
                    "synonyms": [
                        "flatland",
                        "grassland"
                    ],
                    "common_phrases": [
                        "coastal plain",
                        "grassy plain",
                        "flood plain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обширный участок ровной земли с небольшим количеством деревьев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "равни́на",
                                "target_lang_sense_clarification": "географический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "5c0b96e7-35d3-3cd5-bfb6-49c6211df007",
                    "sense_definition": "Used to emphasize an adjective or verb, meaning 'simply' or 'completely'.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adverb_intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was just <w>plain</w> exhausted after working for twelve hours straight.",
                            "target_lang_translations": {
                                "ru": "Я был <w>просто</w> без сил после двенадцати часов работы подряд."
                            }
                        },
                        {
                            "text": "He <w>plain</w> forgot to call his mother on her birthday.",
                            "target_lang_translations": {
                                "ru": "Он <w>просто</w> забыл позвонить матери в день ее рождения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "just plain",
                        "plain tired"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления прилагательного или глагола в значении «просто» или «совершенно»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "про́сто",
                                "target_lang_sense_clarification": "наречие/частица"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "94ba2c33-406c-37cd-a108-7919e9ae9612",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Plain</w> signed the guestbook at the hotel reception.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Плейн</w> расписался в гостевой книге на стойке регистрации отеля."
                            }
                        },
                        {
                            "text": "We are looking for the records of a family named <w>Plain</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ищем записи о семье по фамилии <w>Плейн</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Плейн",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
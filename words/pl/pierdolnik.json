            "pos": "noun",
            "senses": [
                {
                    "sense_id": "287dc504-c7f7-39a6-8a7f-7b7477dbb8a8",
                    "sense_definition": "Duży bałagan, nieporządek, chaos lub miejsce, w którym panuje całkowity brak organizacji.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "disorder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz wreszcie posprzątać ten straszny <w>pierdolnik</w> w swoim pokoju.",
                            "target_lang_translations": {
                                "ru": "Ты должен наконец убрать этот ужасный <w>срач</w> в своей комнате."
                            }
                        },
                        {
                            "text": "W dokumentach firmowych panuje taki <w>pierdolnik</w>, że niczego nie mogę znaleźć.",
                            "target_lang_translations": {
                                "ru": "В документах фирмы такой <w>бардак</w>, что я ничего не могу найти."
                            }
                        }
                    ],
                    "synonyms": [
                        "burdel",
                        "syf",
                        "bajzel",
                        "pieprznik",
                        "rozgardiasz"
                    ],
                    "antonyms": [
                        "porządek",
                        "ład"
                    ],
                    "common_phrases": [
                        "ogarnąć ten pierdolnik",
                        "zrobić pierdolnik",
                        "niezły pierdolnik"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой беспорядок, хаос или место, где царит полная неорганизованность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срач",
                                "target_lang_sense_clarification": "вульгарно; сильный беспорядок, грязь"
                            },
                            {
                                "target_lang_word": "бардак",
                                "target_lang_sense_clarification": "разговорное; беспорядок, неразбериха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4cc351e-a11e-3e66-85ff-62a52a22017c",
                    "sense_definition": "Niewielki przedmiot, część, element lub urządzenie, którego nazwy mówiący nie zna, nie pamiętamy lub nie chce wymieniać.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podaj mi ten mały metalowy <w>pierdolnik</w>, który leży na stole.",
                            "target_lang_translations": {
                                "ru": "Подай мне ту маленькую металлическую <w>хуёвину</w>, которая лежит на столе."
                            }
                        },
                        {
                            "text": "Do uruchomienia tej maszyny potrzebny jest jeden brakujący <w>pierdolnik</w>.",
                            "target_lang_translations": {
                                "ru": "Для запуска этой машины нужна одна недостающая <w>хреновина</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dynks",
                        "wihajster",
                        "ustrojstwo",
                        "dinks"
                    ],
                    "common_phrases": [
                        "taki mały pierdolnik",
                        "ten cały pierdolnik"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой предмет, деталь или устройство, название которого говорящий не знает, не помнит или не хочет называть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хуёвина",
                                "target_lang_sense_clarification": "вульгарно; неопределенный предмет"
                            },
                            {
                                "target_lang_word": "хреновина",
                                "target_lang_sense_clarification": "разговорное; штуковина, деталь"
                            },
                            {
                                "target_lang_word": "фиговина",
                                "target_lang_sense_clarification": "разговорное; эвфемизм для неопределенного предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dff88779-0bc1-3233-a018-de4ce0b91c28",
                    "sense_definition": "Potoczne, często lekceważące określenie internatu, bursy lub domu studenckiego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez całe liceum musiałem mieszkać w starym szkolnym <w>pierdolniku</w>.",
                            "target_lang_translations": {
                                "ru": "Всю старшую школу мне пришлось жить в старом школьном <w>гадюшнике</w>."
                            }
                        },
                        {
                            "text": "Wracamy do naszego <w>pierdolnika</w> zaraz po zakończeniu ostatnich zajęć.",
                            "target_lang_translations": {
                                "ru": "Мы возвращаемся в нашу <w>общагу</w> сразу после окончания последних занятий."
                            }
                        }
                    ],
                    "synonyms": [
                        "internat",
                        "akademik",
                        "bursa"
                    ],
                    "common_phrases": [
                        "mieszkać w pierdolniku",
                        "wracać do pierdolnika"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "slang uczniowski lub studencki"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, часто пренебрежительное название интерната или общежития."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гадюшник",
                                "target_lang_sense_clarification": "пренебрежительно; неприятное, грязное место жительства"
                            },
                            {
                                "target_lang_word": "клоповник",
                                "target_lang_sense_clarification": "пренебрежительно; плохое, грязное жилье"
                            },
                            {
                                "target_lang_word": "общага",
                                "target_lang_sense_clarification": "разговорное; общежитие"
                            }
                        ]
                    }
                }
            ]

                {
                    "sense_id": "794593bd-6364-3f17-8f32-498c240ba2fe",
                    "sense_definition": "Znajdujący się w większej odległości fizycznej lub w relacjach; stopień wyższy od przymiotnika <w>daleki</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_distance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zdjęciu widać góry na <w>dalszym</w> planie, tuż za lasem.",
                            "target_lang_translations": {
                                "ru": "На фотографии видны горы на <w>дальнем</w> плане, сразу за лесом."
                            }
                        },
                        {
                            "text": "To jest moja <w>dalsza</w> rodzina, więc rzadko się z nimi spotykam.",
                            "target_lang_translations": {
                                "ru": "Это мои <w>дальние</w> родственники, поэтому я редко с ними встречаюсь."
                            }
                        }
                    ],
                    "synonyms": [
                        "odleglejszy"
                    ],
                    "antonyms": [
                        "bliższy"
                    ],
                    "common_phrases": [
                        "dalsza rodzina",
                        "dalszy plan",
                        "dalsza podróż"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся на большем физическом расстоянии или в более отдаленном родстве; сравнительная степень прилагательного <w>daleki</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "более далекий",
                                "target_lang_sense_clarification": "сравнительная степень"
                            },
                            {
                                "target_lang_word": "дальний",
                                "target_lang_sense_clarification": "о родственниках, планах, расстоянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "696a8155-1428-3737-be32-f974f45fd27b",
                    "sense_definition": "Taki, który następuje później w czasie lub jest kolejnym etapem jakiegoś procesu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "temporal_sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czekamy na <w>dalsze</w> instrukcje od naszego kierownika projektu.",
                            "target_lang_translations": {
                                "ru": "Мы ждем <w>дальнейших</w> инструкций от нашего руководителя проекта."
                            }
                        },
                        {
                            "text": "Nie mam żadnych <w>dalszych</w> pytań do świadka w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "У меня нет никаких <w>дальнейших</w> вопросов к свидетелю по этому делу."
                            }
                        }
                    ],
                    "synonyms": [
                        "późniejszy",
                        "kolejny",
                        "następny"
                    ],
                    "antonyms": [
                        "wcześniejszy",
                        "poprzedni",
                        "dotychczasowy"
                    ],
                    "common_phrases": [
                        "w dalszym ciągu",
                        "bez dalszej zwłoki",
                        "do dalszego odwołania",
                        "dalsze losy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий позже по времени или являющийся следующим этапом какого-либо процесса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дальнейший",

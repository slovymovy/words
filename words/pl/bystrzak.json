                {
                    "sense_id": "91313567-8872-3152-8f81-77625fdf5596",
                    "sense_definition": "Osoba inteligentna, sprytna i szybko orientująca się w nowej sytuacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intellect_positive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy pracownik to prawdziwy <w>bystrzak</w>, bo wszystkiego nauczył się w jeden dzień.",
                            "target_lang_translations": {
                                "ru": "Этот новый сотрудник — настоящий <w>умница</w>, так как всему научился за один день."
                            }
                        },
                        {
                            "text": "Mój syn to mały <w>bystrzak</w>, ponieważ już w przedszkolu umie czytać i liczyć.",
                            "target_lang_translations": {
                                "ru": "Мой сын — маленький <w>умница</w>, так как уже в детском саду умеет читать и считать."
                            }
                        }
                    ],
                    "synonyms": [
                        "spryciarz"
                    ],
                    "common_phrases": [
                        "niezły bystrzak",
                        "prawdziwy bystrzak",
                        "mały bystrzak"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Умный, сообразительный человек, который быстро ориентируется в новой ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умница",
                                "target_lang_sense_clarification": "о сообразительном человеке"
                            },
                            {
                                "target_lang_word": "смышлёный малый",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "светлая голова",
                                "target_lang_sense_clarification": "фразеологизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05eb640e-bc61-3a63-9a3f-e1569525b945",
                    "sense_definition": "Osoba, która zachowała się niemądrze, bezmyślnie lub wolno kojarzy fakty (używane ironicznie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "intellect_negative_ironic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ale z ciebie <w>bystrzak</w>, skoro zatrzasnąłeś kluczyki wewnątrz samochodu.",
                            "target_lang_translations": {
                                "ru": "Ну ты и <w>умник</w>, раз захлопнул ключи внутри машины."
                            }
                        },
                        {
                            "text": "Jakiś <w>bystrzak</w> zaparkował auto na środku skrzyżowania i zablokował cały ruch.",
                            "target_lang_translations": {
                                "ru": "Какой-то <w>умник</w> припарковал машину посреди перекрёстка и заблокировал всё движение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ale z ciebie bystrzak"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "użycie potoczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который повёл себя глупо, бездумно или медленно соображает (используется иронически)."
                    },

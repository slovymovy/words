                "translations": [
                    {
                        "translation_id": "fc529318-3c06-480e-9084-ce215215f7f8",
                        "source_entry_id": "0c0b1fc0-e545-3a92-b4f0-f1381eb5907d",
                        "source_sense_id": "ac16a126-b701-38e7-80a1-11f632962968",
                        "source_sense_description": "eine Person, die gerne ist und trinkt",
                        "target_lang_word": "Gourmet",
                        "target_lang_code": "de",
                        "target_sense_ids": [],
                        "cross_references": [
                            {
                                "target_word": "Gourmet",
                                "target_lang_code": "de",
                                "target_senses": [
                                    {
                                        "sense_id": "845ee8ec-b31d-3a1c-9f09-bda6282cf009",
                                        "entry_id": "feb55a76-600e-30a7-9610-1c18aff44c99",
                                        "glosses": [
                                            "Person, die gerne gut zubereitete, raffinierte Speisen isst und deren Güte beurteilen kann"
                                        ],
                                        "tags": [],
                                        "examples": [
                                            {
                                                "example_id": "af77bc95-ceb6-4c86-9485-0a1892504936",
                                                "sense_id": "845ee8ec-b31d-3a1c-9f09-bda6282cf009",
                                                "text": "„Schließlich legte Chabrol, der Gourmet, Genießer und Rotweinkenner, seine Drehorte stets in die Nähe von zwei, drei hervorragenden Restaurants.“"
                                            },
                                            {
                                                "example_id": "c5cc9f3f-2cb6-4ccf-adbb-8b5dc3e95904",
                                                "sense_id": "845ee8ec-b31d-3a1c-9f09-bda6282cf009",
                                                "text": "„Ob Vierländer Ente, Linsensuppe mit Räucheraal und pochiertem Wachtelei oder karamelisierter Ziegenkäse mit Walnußpesto und Kräuterbrot, für Gourmets blieb kein Wunsch offen.“"
                                            },
                                            {
                                                "example_id": "d8dcbe92-f2b2-41e3-809e-b15b38432b4e",
                                                "sense_id": "845ee8ec-b31d-3a1c-9f09-bda6282cf009",
                                                "text": "„Die Menschen im Kaukasus sind in mancher Hinsicht Gourmets.“"
                                            },
                                            {
                                                "example_id": "30dd2ddd-5c9a-4d7a-8f42-6a398cc22e14",
                                                "sense_id": "845ee8ec-b31d-3a1c-9f09-bda6282cf009",
                                                "text": "„Wer viel isst, tut dies nicht unbedingt mit Genuss - nur wenige Gourmands dürfen sich auch als Gourmets bezeichnen.“"
                                            },
                                            {
                                                "example_id": "4e9f07ae-922a-4af1-831d-667abc09df40",
                                                "sense_id": "845ee8ec-b31d-3a1c-9f09-bda6282cf009",
                                                "text": "„Er war froh über das Getue, das von Levetzov jetzt wegen des Essens machte, um zu zeigen, daß er ein Gourmet sei.“"
                                            }
                                        ],
                                        "sense_index_json": "\"1\""
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "translation_id": "813011bd-4ad2-4508-8c83-ec57ec2e4ac4",
                        "source_entry_id": "0c0b1fc0-e545-3a92-b4f0-f1381eb5907d",
                        "source_sense_id": "ac16a126-b701-38e7-80a1-11f632962968",
                        "source_sense_description": "eine Person, die gerne ist und trinkt",
                        "target_lang_word": "Feinschmecker",
                        "target_lang_code": "de",
                        "target_sense_ids": [],
                        "cross_references": [
                            {
                                "target_word": "Feinschmecker",
                                "target_lang_code": "de",
                                "target_senses": [
                                    {
                                        "sense_id": "af897dc3-a03b-395a-abdd-e7b4c44f7ff7",
                                        "entry_id": "0b48cff2-6476-36c4-99ff-11734a3e61ae",
                                        "glosses": [
                                            "Mensch mit einem verwöhnten und/oder anspruchsvollen Geschmack und einem ausgeprägten Sinn für feine Speisen"
                                        ],
                                        "tags": [],
                                        "examples": [
                                            {
                                                "example_id": "9d8eb7a9-4abc-4f6c-bf06-e7d32ccec5fb",
                                                "sense_id": "af897dc3-a03b-395a-abdd-e7b4c44f7ff7",
                                                "text": "„In keiner anderen deutschen Stadt gibt es so viele chinesische Restaurants wie in Berlin. Als Ende der 20er Jahre bereits das erste in der Hauptstadt eröffnete, bejubelten die Feinschmecker die \"neue, kulinarische Weltoffenheit\" der Metropole.“"
                                            },
                                            {
                                                "example_id": "ba16db79-b1b4-48da-8c4b-379eef1fc0f5",
                                                "sense_id": "af897dc3-a03b-395a-abdd-e7b4c44f7ff7",
                                                "text": "„Er war ein schöner Mann, ein Feinschmecker und Schlemmer und sehr geistreich.“"
                                            }
                                        ],
                                        "sense_index_json": "\"1\""
                                    }
                                ]
                            }
                        ]
                    }

{
    "word_family": [
        "gemeenschap"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "443a306e-2b0c-3102-b9c2-b16898781e4a",
                    "sense_definition": "Bedoeld om door alle leden van een groep gebruikt te worden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "shared_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In dit studentenhuis is er een <w>gemeenschappelijke</w> keuken.",
                            "target_lang_translations": {
                                "en": "In this student house, there is a <w>communal</w> kitchen.",
                                "ru": "В этом студенческом общежитии есть <w>общая</w> кухня."
                            }
                        },
                        {
                            "text": "De bewoners maken samen schoon in de <w>gemeenschappelijke</w> ruimte.",
                            "target_lang_translations": {
                                "en": "The residents clean the <w>common</w> room together.",
                                "ru": "Жильцы вместе делают уборку в <w>общем</w> помещении."
                            }
                        }
                    ],
                    "synonyms": [
                        "openbaar",
                        "collectief"
                    ],
                    "antonyms": [
                        "privé"
                    ],
                    "common_phrases": [
                        "gemeenschappelijk gebruik",
                        "gemeenschappelijke ruimte",
                        "gemeenschappelijke voorziening"
                    ],
                    "target_lang_definitions": {
                        "en": "Intended to be used by all members of a group.",
                        "ru": "Предназначенный для использования всеми членами группы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "communal",
                                "target_lang_sense_clarification": "used for facilities or resources shared by a specific group"
                            },
                            {
                                "target_lang_word": "shared",
                                "target_lang_sense_clarification": "used or possessed by multiple people"
                            },
                            {
                                "target_lang_word": "common",
                                "target_lang_sense_clarification": "belonging to or open to the public or a group"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "общий",
                                "target_lang_sense_clarification": "принадлежащий всем, используемый сообща"
                            },
                            {
                                "target_lang_word": "коллективный",
                                "target_lang_sense_clarification": "общественный, групповой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54cf5787-d73a-3164-81c3-7ba44851a92e",
                    "sense_definition": "Iets wat je samen met anderen doet, organiseert of uitvoert.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "shared_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten samenwerken aan ons <w>gemeenschappelijke</w> doel.",
                            "target_lang_translations": {
                                "en": "We must work together on our <w>common</w> goal.",
                                "ru": "Мы должны работать вместе над нашей <w>общей</w> целью."
                            }
                        },
                        {
                            "text": "Het <w>gemeenschappelijke</w> project was een groot succes voor het team.",
                            "target_lang_translations": {
                                "en": "The <w>joint</w> project was a great success for the team.",
                                "ru": "<w>Совместный</w> проект стал большим успехом для команды."
                            }
                        }
                    ],
                    "synonyms": [
                        "gezamenlijk"
                    ],
                    "antonyms": [
                        "privé"
                    ],
                    "common_phrases": [
                        "gemeenschappelijk doel",
                        "gemeenschappelijk overleg",
                        "gemeenschappelijk beleid"
                    ],
                    "target_lang_definitions": {
                        "en": "Something done, organized, or carried out together with others.",
                        "ru": "То, что делается, организуется или выполняется совместно с другими."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "joint",
                                "target_lang_sense_clarification": "done by two or more people or groups working together"
                            },
                            {
                                "target_lang_word": "common",
                                "target_lang_sense_clarification": "shared by two or more people, such as a goal or purpose"
                            },
                            {
                                "target_lang_word": "collective",
                                "target_lang_sense_clarification": "formed or done by a group acting as a whole"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "совместный",
                                "target_lang_sense_clarification": "выполняемый сообща, объединенный"
                            },
                            {
                                "target_lang_word": "общий",
                                "target_lang_sense_clarification": "единый для всех участников (например, цель)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5741cc9d-08fe-35a1-962a-3c0158c6ff93",
                    "sense_definition": "Wat betrekking heeft op twee of meer personen of zaken tegelijk; wat men deelt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mutual_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jan en ik kwamen erachter dat we een <w>gemeenschappelijke</w> vriend hebben.",
                            "target_lang_translations": {
                                "en": "Jan and I discovered that we have a <w>mutual</w> friend.",
                                "ru": "Ян и я выяснили, что у нас есть <w>общий</w> друг."
                            }
                        },
                        {
                            "text": "Nederland en Duitsland delen een lange <w>gemeenschappelijke</w> grens.",
                            "target_lang_translations": {
                                "en": "The Netherlands and Germany share a long <w>common</w> border.",
                                "ru": "Нидерланды и Германия имеют протяженную <w>общую</w> границу."
                            }
                        }
                    ],
                    "synonyms": [
                        "gedeeld",
                        "wederzijds"
                    ],
                    "common_phrases": [
                        "gemeenschappelijke vriend",
                        "gemeenschappelijke belangen",
                        "gemeenschappelijke deler",
                        "gemeenschappelijke basis"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to two or more people or things at the same time; what is shared.",
                        "ru": "Относящийся к двум или более лицам или предметам одновременно; то, что их объединяет."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "common",
                                "target_lang_sense_clarification": "shared by two or more parties, e.g., a friend or characteristic"
                            },
                            {
                                "target_lang_word": "mutual",
                                "target_lang_sense_clarification": "held in common by two or more parties"
                            },
                            {
                                "target_lang_word": "shared",
                                "target_lang_sense_clarification": "possessed or experienced by more than one person"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "общий",
                                "target_lang_sense_clarification": "взаимный, имеющийся у обоих"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
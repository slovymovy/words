            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9f8c8447-78ce-3d21-ac4e-0b051436288c",
                    "sense_definition": "Budynek oferujący podróżnym płatne zakwaterowanie, noclegi oraz dodatkowe usługi.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zatrzymałem się z przyjaciółmi w <w>hotelu</w> na Dworcowej.",
                            "target_lang_translations": {
                                "ru": "Я остановился с друзьями в <w>гостинице</w> на Дворцовой."
                            }
                        },
                        {
                            "text": "Ten luksusowy <w>hotel</w> oferuje gościom basen i siłownię.",
                            "target_lang_translations": {
                                "ru": "Этот роскошный <w>отель</w> предлагает гостям бассейн и тренажёрный зал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "doba hotelowa",
                        "rezerwować hotel",
                        "pokój w hotelu",
                        "obsługa hotelowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание, предлагающее путешественникам платное проживание, ночлег и дополнительные услуги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гостиница",
                                "target_lang_sense_clarification": "место проживания"
                            },
                            {
                                "target_lang_word": "отель",
                                "target_lang_sense_clarification": "гостиница"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6423f98-238e-35d1-8642-06532792828a",
                    "sense_definition": "Potoczne, często ironiczne określenie więzienia lub aresztu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za kradzież samochodu złodziej trafił do państwowego <w>hotelu</w> na trzy lata.",
                            "target_lang_translations": {
                                "ru": "За кражу автомобиля вор попал в государственный <w>отель</w> (тюрьму) на три года."
                            }
                        },
                        {
                            "text": "Nie uśmiecha mi się siedzieć w tym <w>hotelu</w> za kratami.",
                            "target_lang_translations": {
                                "ru": "Мне не улыбается сидеть в этом <w>отеле</w> за решёткой."
                            }
                        }
                    ],
                    "synonyms": [
                        "więzienie",
                        "areszt",
                        "paka",
                        "kić"
                    ],
                    "common_phrases": [
                        "państwowy hotel"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "ironicznie o więzieniu"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, часто ироничное название тюрьмы или места заключения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тюрьма",
                                "target_lang_sense_clarification": "место заключения"

            "senses": [
                {
                    "sense_id": "f620cf18-7e54-3b3f-9b3a-0701c3c090dc",
                    "sense_definition": "Afkomstig van de eerste maker of uit de eerste periode; niet nagemaakt of later aangepast.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "oorspronkelijk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>originele</w> schilderij hangt in een museum in Parijs.",
                            "target_lang_translations": {
                                "en": "The <w>original</w> painting hangs in a museum in Paris.",
                                "ru": "<w>Оригинальная</w> картина висит в музее в Париже."
                            }
                        },
                        {
                            "text": "Wij hebben het oude huis weer in de <w>originele</w> staat teruggebracht.",
                            "target_lang_translations": {
                                "en": "We have restored the old house to its <w>original</w> state.",
                                "ru": "Мы вернули старый дом в его <w>первоначальное</w> состояние."
                            }
                        }
                    ],
                    "synonyms": [
                        "onorigineel"
                    ],
                    "antonyms": [
                        "nagemaakt",
                        "gekopieerd"
                    ],
                    "common_phrases": [
                        "in originele staat",
                        "de originele versie"
                    ],
                    "target_lang_definitions": {
                        "en": "Coming from the first maker or the earliest period; not imitated or modified later.",
                        "ru": "Созданный первым автором или относящийся к первому периоду; не поддельный и не измененный впоследствии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "original",
                                "target_lang_sense_clarification": "preceding all others; initial"
                            },
                            {
                                "target_lang_word": "authentic",
                                "target_lang_sense_clarification": "genuine; not a copy"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подлинный",
                                "target_lang_sense_clarification": "настоящий, не подделка"
                            },
                            {
                                "target_lang_word": "первоначальный",
                                "target_lang_sense_clarification": "исходный, каким был в начале"
                            },
                            {
                                "target_lang_word": "оригинальный",
                                "target_lang_sense_clarification": "не копия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c691b66-a4b9-3e3d-b491-177984f3b75a",
                    "sense_definition": "Met nieuwe, eigen ideeën bedacht en niet van anderen overgenomen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creatief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leerling bedacht een heel <w>originele</w> oplossing voor het probleem.",
                            "target_lang_translations": {
                                "en": "The student came up with a very <w>original</w> solution to the problem.",
                                "ru": "Ученик придумал очень <w>оригинальное</w> решение проблемы."
                            }
                        },
                        {
                            "text": "Hij is een <w>originele</w> denker die altijd met verrassende plannen komt.",
                            "target_lang_translations": {
                                "en": "He is an <w>original</w> thinker who always comes up with surprising plans.",
                                "ru": "Он <w>оригинальный</w> мыслитель, который всегда предлагает неожиданные планы."
                            }
                        }
                    ],
                    "synonyms": [
                        "onorigineel"
                    ],
                    "antonyms": [
                        "onorigineel",
                        "afgezaagd"
                    ],
                    "common_phrases": [
                        "een origineel idee",
                        "origineel zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "Conceived with new, personal ideas and not copied from others.",
                        "ru": "Придуманный с использованием новых, собственных идей, а не заимствованный у других."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "original",
                                "target_lang_sense_clarification": "fresh; different; new"

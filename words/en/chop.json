                },
                {
                    "sense_id": "2cce5ee2-7064-3b6f-a588-17283f1e14cb",
                    "sense_definition": "A knife, especially one used as a weapon.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gang member was arrested for carrying a <w>chop</w> in public.",
                            "target_lang_translations": {
                                "ru": "Члена банды арестовали за ношение <w>ножа</w> в общественном месте."
                            }
                        },
                        {
                            "text": "Police seized a large <w>chop</w> during the search of the suspect's home.",
                            "target_lang_translations": {
                                "ru": "Полиция изъяла большой <w>тесак</w> во время обыска дома подозреваемого."
                            }
                        }

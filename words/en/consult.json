{
    "word_family": [
        "consultant",
        "consultation",
        "consultative",
        "consultancy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2ab7c080-6e18-3919-94c6-e9d3cf8da6c1",
                    "sense_definition": "To ask for information or advice from someone who has special knowledge.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "seek_advice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>consult</w> a doctor if the pain continues for more than a day.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>проконсультироваться</w> с врачом, если боль не пройдет в течение дня."
                            }
                        },
                        {
                            "text": "They decided to <w>consult</w> a lawyer before signing the contract.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>проконсультироваться</w> с юристом перед подписанием контракта."
                            }
                        }
                    ],
                    "synonyms": [
                        "seek"
                    ],
                    "common_phrases": [
                        "consult a doctor",
                        "consult a lawyer",
                        "consult an expert"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращаться к кому-либо за информацией или советом (особенно к человеку, обладающему специальными знаниями)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "консультироваться",
                                "target_lang_sense_clarification": "обращаться к специалисту"
                            },
                            {
                                "target_lang_word": "советоваться",
                                "target_lang_sense_clarification": "просить совета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ad0554e-7c26-312d-8978-acf12c80ffbb",
                    "sense_definition": "To look for information in a book, map, or other resource.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "seek_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>consult</w> a map to find the best route to the station.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>свериться</w> с картой, чтобы найти лучший маршрут до станции."
                            }
                        },
                        {
                            "text": "Students often <w>consult</w> the dictionary when they find unknown words.",
                            "target_lang_translations": {
                                "ru": "Студенты часто <w>обращаются</w> к словарю, когда встречают незнакомые слова."
                            }
                        }
                    ],
                    "synonyms": [
                        "refer",
                        "see"
                    ],
                    "common_phrases": [
                        "consult a dictionary",
                        "consult a map",
                        "consult the manual"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искать информацию в книге, на карте или в другом справочном ресурсе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сверяться",
                                "target_lang_sense_clarification": "проверять информацию по источнику"
                            },
                            {
                                "target_lang_word": "обращаться",
                                "target_lang_sense_clarification": "использовать источник информации"
                            },
                            {
                                "target_lang_word": "заглядывать",
                                "target_lang_sense_clarification": "быстро смотреть (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8138346-94aa-3421-8f7e-4d9053c48e96",
                    "sense_definition": "To discuss something with someone to get their permission or opinion before making a decision.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "seek_advice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>consult</w> with my partner before I buy such an expensive car.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>посоветоваться</w> с партнером, прежде чем покупать такую дорогую машину."
                            }
                        },
                        {
                            "text": "The manager did not <w>consult</w> the staff about the new office rules.",
                            "target_lang_translations": {
                                "ru": "Менеджер не <w>посоветовался</w> с сотрудниками по поводу новых правил офиса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "consult with colleagues",
                        "consult with staff"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обсуждать что-либо с кем-либо, чтобы узнать мнение или получить согласие перед принятием решения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "советоваться",
                                "target_lang_sense_clarification": "обсуждать перед решением"
                            },
                            {
                                "target_lang_word": "совещаться",
                                "target_lang_sense_clarification": "обсуждать совместно (официально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad8c472d-669e-3d55-bd68-7dfb2d18ff72",
                    "sense_definition": "To provide professional advice or services to an organization for a fee.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "professional_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She retired from the university and now <w>consults</w> for several technology firms.",
                            "target_lang_translations": {
                                "ru": "Она ушла из университета и теперь <w>консультирует</w> несколько технологических фирм."
                            }
                        },
                        {
                            "text": "He <w>consults</w> on environmental policy for the local government.",
                            "target_lang_translations": {
                                "ru": "Он <w>консультирует</w> местное правительство по вопросам экологической политики."
                            }
                        }
                    ],
                    "common_phrases": [
                        "consult for a company",
                        "consult on a project"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставлять профессиональные советы или услуги организации за плату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "консультировать",
                                "target_lang_sense_clarification": "давать профессиональные советы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e27d149-fa5a-3329-a793-fa6b5beab3dd",
                    "sense_definition": "To take into account or consider feelings or interests.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "consideration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He did not <w>consult</w> his own safety when he rushed into the burning building.",
                            "target_lang_translations": {
                                "ru": "Он не <w>подумал</w> о собственной безопасности, когда бросился в горящее здание."
                            }
                        },
                        {
                            "text": "We must <w>consult</w> the best interests of the community when making these laws.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>учитывать</w> интересы общества при принятии этих законов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "consult one's wishes",
                        "consult one's interests"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать во внимание или учитывать чьи-либо чувства или интересы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "учитывать",
                                "target_lang_sense_clarification": "принимать во внимание"
                            },
                            {
                                "target_lang_word": "считаться",
                                "target_lang_sense_clarification": "принимать в расчет"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0c46a4b8-5431-3df1-b11b-c0c95d6d114e",
                    "sense_definition": "A meeting with an expert, especially a doctor, to seek advice.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_meeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The surgeon requested a cardiology <w>consult</w> before proceeding with the operation.",
                            "target_lang_translations": {
                                "ru": "Хирург запросил <w>консультацию</w> кардиолога перед началом операции."
                            }
                        },
                        {
                            "text": "I have a quick <w>consult</w> scheduled with the specialist this afternoon.",
                            "target_lang_translations": {
                                "ru": "У меня запланирована короткая <w>консультация</w> со специалистом сегодня днем."
                            }
                        }
                    ],
                    "synonyms": [
                        "consultation"
                    ],
                    "common_phrases": [
                        "a medical consult",
                        "request a consult"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used as a shorthand for consultation, particularly in medical contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Встреча с экспертом, особенно с врачом, для получения совета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "консультация",
                                "target_lang_sense_clarification": "прием у специалиста"
                            },
                            {
                                "target_lang_word": "консилиум",
                                "target_lang_sense_clarification": "совещание врачей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "enkelvoud",
        "enkeling"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "ddb02934-2df9-3c5e-aa78-39aa022b898c",
                    "sense_definition": "Een klein aantal; niet veel, maar wel een paar.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er staan nog <w>enkele</w> fouten in de tekst die verbeterd moeten worden.",
                            "target_lang_translations": {
                                "en": "There are still <w>a few</w> errors in the text that need to be corrected.",
                                "ru": "В тексте есть еще <w>несколько</w> ошибок, которые нужно исправить."
                            }
                        },
                        {
                            "text": "Ik heb <w>enkele</w> vragen over de nieuwe regels.",
                            "target_lang_translations": {
                                "en": "I have <w>a few</w> questions about the new rules.",
                                "ru": "У меня есть <w>несколько</w> вопросов о новых правилах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een enkele keer",
                        "enkele dagen",
                        "enkele minuten"
                    ],
                    "target_lang_definitions": {
                        "en": "A small number; not many, but a few.",
                        "ru": "Небольшое количество; не много, но есть пара."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "a few",
                                "target_lang_sense_clarification": "A small number of things"
                            },
                            {
                                "target_lang_word": "some",
                                "target_lang_sense_clarification": "An indefinite quantity"
                            },
                            {
                                "target_lang_word": "several",
                                "target_lang_sense_clarification": "More than two but not many"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "несколько",
                                "target_lang_sense_clarification": "неопределенное малое количество"
                            },
                            {
                                "target_lang_word": "некоторые",
                                "target_lang_sense_clarification": "часть группы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "56016012-37fc-3759-91c5-e63981494460",
                    "sense_definition": "Uitsluitend; niet meer dan; alleen maar.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit product is <w>enkel</w> online te koop en niet in de winkel.",
                            "target_lang_translations": {
                                "en": "This product is available <w>only</w> online and not in the store.",
                                "ru": "Этот продукт можно купить <w>только</w> онлайн, а не в магазине."
                            }
                        },
                        {
                            "text": "Hij drinkt <w>enkel</w> water en nooit frisdrank.",
                            "target_lang_translations": {
                                "en": "He drinks <w>only</w> water and never soda.",
                                "ru": "Он пьет <w>только</w> воду и никогда газировку."
                            }
                        }
                    ],
                    "synonyms": [
                        "alleen",
                        "uitsluitend",
                        "slechts"
                    ],
                    "common_phrases": [
                        "enkel en alleen",
                        "niet enkel"
                    ],
                    "target_lang_definitions": {
                        "en": "Exclusively; nothing more than; only.",
                        "ru": "Исключительно; не более чем; только."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "only",
                                "target_lang_sense_clarification": "Exclusively; no one or nothing else"
                            },
                            {
                                "target_lang_word": "just",
                                "target_lang_sense_clarification": "Simply; merely"
                            },
                            {
                                "target_lang_word": "solely",
                                "target_lang_sense_clarification": "Not involving anyone or anything else"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "исключительно"
                            },
                            {
                                "target_lang_word": "лишь",
                                "target_lang_sense_clarification": "ограничение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e5e8a127-2d8d-3899-87ce-db3bbc8f3ce2",
                    "sense_definition": "Het gewricht dat de voet met het been verbindt.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij verstuikte zijn <w>enkel</w> tijdens het hardlopen in het bos.",
                            "target_lang_translations": {
                                "en": "He sprained his <w>ankle</w> while running in the forest.",
                                "ru": "Он подвернул <w>лодыжку</w> во время бега в лесу."
                            }
                        },
                        {
                            "text": "Na de lange wandeling waren mijn <w>enkels</w> een beetje dik.",
                            "target_lang_translations": {
                                "en": "After the long walk, my <w>ankles</w> were a bit swollen.",
                                "ru": "После долгой прогулки мои <w>щиколотки</w> немного отекли."
                            }
                        }
                    ],
                    "common_phrases": [
                        "verstuikte enkel",
                        "gebroken enkel"
                    ],
                    "target_lang_definitions": {
                        "en": "The joint connecting the foot to the leg.",
                        "ru": "Сустав, соединяющий стопу с ногой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ankle",
                                "target_lang_sense_clarification": "The skeletal joint connecting the foot with the leg"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лодыжка",
                                "target_lang_sense_clarification": "часть тела"
                            },
                            {
                                "target_lang_word": "щиколотка",
                                "target_lang_sense_clarification": "разговорный синоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c87f3ae4-c574-3db6-961d-e74bb6141c96",
                    "sense_definition": "Een spelvorm bij sporten zoals tennis waarbij één speler tegen één andere speler speelt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij het tennistoernooi won zij de finale in het <w>enkel</w>.",
                            "target_lang_translations": {
                                "en": "At the tennis tournament, she won the final in the <w>singles</w>.",
                                "ru": "На теннисном турнире она выиграла финал в <w>одиночном разряде</w>."
                            }
                        },
                        {
                            "text": "Hij speelt liever <w>enkel</w> dan dubbel omdat hij graag alleen verantwoordelijk is.",
                            "target_lang_translations": {
                                "en": "He prefers playing <w>singles</w> rather than doubles because he likes being solely responsible.",
                                "ru": "Он предпочитает играть <w>одиночные матчи</w>, а не парные, потому что любит нести ответственность сам."
                            }
                        }
                    ],
                    "antonyms": [
                        "dubbel"
                    ],
                    "common_phrases": [
                        "heren enkel",
                        "dames enkel"
                    ],
                    "target_lang_definitions": {
                        "en": "A match in sports like tennis played by one player against another.",
                        "ru": "Вид игры в спорте (например, в теннисе), где один игрок играет против другого."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "singles",
                                "target_lang_sense_clarification": "A game with one player on each side, as in tennis"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "одиночный разряд",
                                "target_lang_sense_clarification": "в спорте"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8e971513-29ed-3928-90a7-cb1ca4d7368e",
                    "sense_definition": "Uit één stuk bestaand of één keer voorkomend; niet dubbel.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil graag een kaartje voor een <w>enkele</w> reis naar Amsterdam.",
                            "target_lang_translations": {
                                "en": "I would like a ticket for a <w>single</w> journey to Amsterdam.",
                                "ru": "Я хотел бы билет в <w>одну</w> сторону до Амстердама."
                            }
                        },
                        {
                            "text": "In dit raam zit nog <w>enkel</w> glas in plaats van dubbelglas.",
                            "target_lang_translations": {
                                "en": "This window still has <w>single</w> glazing instead of double glazing.",
                                "ru": "В этом окне стоит <w>одинарное</w> стекло вместо двойного."
                            }
                        }
                    ],
                    "antonyms": [
                        "dubbel"
                    ],
                    "common_phrases": [
                        "enkele reis",
                        "enkel glas"
                    ],
                    "target_lang_definitions": {
                        "en": "Consisting of one piece or occurring once; not double.",
                        "ru": "Состоящий из одной части или встречающийся один раз; не двойной."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "single",
                                "target_lang_sense_clarification": "Not double; individual"
                            },
                            {
                                "target_lang_word": "one-way",
                                "target_lang_sense_clarification": "Referring to a ticket for travel in one direction only"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "одинарный",
                                "target_lang_sense_clarification": "не двойной"
                            },
                            {
                                "target_lang_word": "единичный",
                                "target_lang_sense_clarification": "отдельный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
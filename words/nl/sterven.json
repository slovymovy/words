            "pos": "verb",
            "senses": [
                {
                    "sense_id": "376d4423-55d8-3dd9-b144-b32b09ea5f08",
                    "sense_definition": "Ophouden met leven; het einde van het leven bereiken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "death_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De beroemde schrijver is op negentigjarige leeftijd <w>gestorven</w>.",
                            "target_lang_translations": {
                                "en": "The famous writer <w>died</w> at the age of ninety.",
                                "ru": "Знаменитый писатель <w>умер</w> в возрасте девяноста лет."
                            }
                        },
                        {
                            "text": "Zonder voldoende water zullen de bloemen in de tuin snel <w>sterven</w>.",
                            "target_lang_translations": {
                                "en": "Without enough water, the flowers in the garden will quickly <w>die</w>.",
                                "ru": "Без достаточного количества воды цветы в саду быстро <w>погибнут</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "doodgaan",
                        "overlijden",
                        "omkomen",
                        "heengaan"
                    ],
                    "antonyms": [
                        "leven",
                        "geboren worden"
                    ],
                    "common_phrases": [
                        "sterven van de honger",
                        "op sterven liggen",
                        "sterven in het harnas"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop living; to reach the end of life.",
                        "ru": "Перестать жить; достичь конца жизни."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "die",
                                "target_lang_sense_clarification": "general term for stopping living"
                            },
                            {
                                "target_lang_word": "pass away",
                                "target_lang_sense_clarification": "polite or euphemistic term, usually for people"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "умирать",
                                "target_lang_sense_clarification": "общий термин для обозначения прекращения жизни"
                            },
                            {
                                "target_lang_word": "скончаться",
                                "target_lang_sense_clarification": "вежливый или официальный термин, обычно о людях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf2eea59-8e93-39e0-9b94-2b01669393f8",
                    "sense_definition": "Langzaam verdwijnen of niet meer hoorbaar zijn (meestal van geluid of licht).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fading_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het gelach <w>stierf</w> weg toen de leraar het lokaal binnenkwam.",
                            "target_lang_translations": {
                                "en": "The laughter <w>died</w> away when the teacher entered the classroom.",
                                "ru": "Смех <w>замер</w>, когда учитель вошел в класс."
                            }
                        },
                        {
                            "text": "Haar stem <w>stierf</w> weg in de wind en niemand kon haar horen.",
                            "target_lang_translations": {
                                "en": "Her voice <w>died</w> away in the wind and no one could hear her.",
                                "ru": "Ее голос <w>замер</w> на ветру, и никто не мог ее услышать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het geluid stierf weg"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To slowly disappear or cease to be audible (usually of sound or light).",
                        "ru": "Медленно исчезать или переставать быть слышным (обычно о звуке или свете)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "die away",
                                "target_lang_sense_clarification": "to gradually become inaudible or disappear"
                            },
                            {
                                "target_lang_word": "fade",
                                "target_lang_sense_clarification": "to lose brightness or loudness"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "замирать",
                                "target_lang_sense_clarification": "постепенно исчезать или становиться неслышным"
                            },
                            {
                                "target_lang_word": "угасать",
                                "target_lang_sense_clarification": "терять яркость или громкость"
                            }
                        ]
                    }
                }
            ]
        }
    ]

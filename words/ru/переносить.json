    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "58d3f4ed-c9c0-3c2c-b27d-ee96bb5e2c48",
                    "sense_definition": "Перемещать кого-либо или что-либо с одного места на другое, держа в руках или используя транспорт.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочие начали <w>переносить</w> мебель из грузовика в квартиру.",
                            "target_lang_translations": {
                                "en": "The workers started to <w>carry</w> the furniture from the truck into the apartment."
                            }
                        },
                        {
                            "text": "Отец помогал <w>переносить</w> тяжелые коробки в гараж.",
                            "target_lang_translations": {
                                "en": "Father helped <w>carry</w> the heavy boxes into the garage."
                            }
                        }
                    ],
                    "synonyms": [
                        "перемещать",
                        "перетаскивать",
                        "нести"
                    ],
                    "antonyms": [
                        "оставлять"
                    ],
                    "common_phrases": [
                        "переносить вещи",
                        "переносить через дорогу",
                        "переносить на руках"
                    ],
                    "target_lang_definitions": {
                        "en": "To move someone or something from one place to another, holding it in one's hands or using transport."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "to move while holding"
                            },
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "general change of place"
                            },
                            {
                                "target_lang_word": "transport",
                                "target_lang_sense_clarification": "to move using a vehicle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac6de865-82d0-3010-bc15-8786f7efb91a",
                    "sense_definition": "Назначать другое время или дату для выполнения чего-либо; откладывать на более поздний срок.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_management",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за плохой погоды нам пришлось <w>переносить</w> пикник на следующие выходные.",
                            "target_lang_translations": {
                                "en": "Due to bad weather, we had to <w>reschedule</w> the picnic for next weekend."
                            }
                        },
                        {
                            "text": "Директор не любит <w>переносить</w> важные встречи.",
                            "target_lang_translations": {
                                "en": "The director does not like to <w>reschedule</w> important meetings."
                            }
                        }
                    ],
                    "synonyms": [
                        "откладывать",
                        "сдвигать"
                    ],
                    "common_phrases": [
                        "переносить встречу",
                        "переносить на завтра",
                        "переносить сроки"
                    ],
                    "target_lang_definitions": {
                        "en": "To appoint a different time or date for doing something; to postpone to a later time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reschedule",
                                "target_lang_sense_clarification": "to assign a new time"
                            },
                            {
                                "target_lang_word": "postpone",
                                "target_lang_sense_clarification": "to delay until later"
                            },
                            {
                                "target_lang_word": "put off",
                                "target_lang_sense_clarification": "phrasal verb for delaying"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1cc4507-0b4e-3288-9efd-e7baaea63698",
                    "sense_definition": "Испытывать и выдерживать что-либо неприятное, тяжелое (боль, страдания, трудные условия), сохраняя самообладание.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "endurance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой друг умеет стойко <w>переносить</w> любые жизненные трудности.",
                            "target_lang_translations": {
                                "en": "My friend knows how to steadfastly <w>endure</w> any of life's hardships."
                            }
                        },
                        {
                            "text": "Пациент плохо <w>переносит</w> этот вид лекарства.",
                            "target_lang_translations": {
                                "en": "The patient does not <w>tolerate</w> this type of medication well."
                            }
                        }
                    ],
                    "synonyms": [
                        "выдерживать",
                        "терпеть",
                        "переживать"
                    ],
                    "antonyms": [
                        "сдаваться"
                    ],
                    "common_phrases": [
                        "плохо переносить",
                        "переносить боль",
                        "переносить жару",
                        "легко переносить"
                    ],
                    "target_lang_definitions": {
                        "en": "To experience and withstand something unpleasant or difficult (pain, suffering, difficult conditions) while maintaining self-control."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "endure",
                                "target_lang_sense_clarification": "to suffer patiently"
                            },
                            {
                                "target_lang_word": "bear",
                                "target_lang_sense_clarification": "to withstand a burden or pain"
                            },
                            {
                                "target_lang_word": "tolerate",
                                "target_lang_sense_clarification": "to accept or handle (often medical)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06bbb9a0-0453-381c-8178-41c5f0b4cea9",
                    "sense_definition": "Мысленно перемещать себя в другое время или место; распространять чувства или отношения на другие объекты.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хорошая книга может <w>переносить</w> читателя в волшебный мир.",
                            "target_lang_translations": {
                                "en": "A good book can <w>transport</w> the reader to a magical world."
                            }
                        },
                        {
                            "text": "Не следует <w>переносить</w> своё плохое настроение на коллег.",
                            "target_lang_translations": {
                                "en": "One should not <w>transfer</w> one's bad mood to colleagues."
                            }
                        }
                    ],
                    "synonyms": [
                        "проецировать",
                        "распространять"
                    ],
                    "common_phrases": [
                        "переносить опыт",
                        "переносить чувства",
                        "переносить в прошлое"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To mentally move oneself to another time or place; to extend feelings or attitudes to other objects."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "transport",
                                "target_lang_sense_clarification": "to move mentally or emotionally"
                            },
                            {
                                "target_lang_word": "transfer",
                                "target_lang_sense_clarification": "to shift feelings or qualities"
                            },
                            {
                                "target_lang_word": "project",
                                "target_lang_sense_clarification": "to attribute feelings to others"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84406c96-a47e-3244-b84d-42b152600849",
                    "sense_definition": "Записывать часть слова, которая не помещается на строке, на следующей строке в соответствии с правилами правописания.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "typography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель объяснял детям, как правильно <w>переносить</w> слова.",
                            "target_lang_translations": {
                                "en": "The teacher explained to the children how to correctly <w>hyphenate</w> words."
                            }
                        },
                        {
                            "text": "Компьютерная программа автоматически <w>переносит</w> длинные слова на новую строку.",
                            "target_lang_translations": {
                                "en": "The computer program automatically <w>carries over</w> long words to a new line."
                            }
                        }
                    ],
                    "common_phrases": [
                        "переносить слова",
                        "переносить по слогам"
                    ],
                    "target_lang_definitions": {
                        "en": "To write the part of a word that does not fit on a line on the next line according to spelling rules."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hyphenate",
                                "target_lang_sense_clarification": "to split a word with a hyphen"
                            },
                            {
                                "target_lang_word": "carry over",
                                "target_lang_sense_clarification": "to move text to the next line"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2bce14e7-3092-3bc6-9699-c7dd50a864b6",
                    "sense_definition": "The arts, customs, lifestyles, background, and habits that characterize a particular society, nation, or group.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sociological_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Learning a new language often involves understanding the <w>culture</w> of the people who speak it.",
                            "target_lang_translations": {
                                "ru": "Изучение нового языка часто включает в себя понимание <w>культуры</w> людей, которые на нем говорят."
                            }
                        },
                        {
                            "text": "She experienced significant <w>culture</w> shock when she moved from a small village to a huge metropolis.",
                            "target_lang_translations": {
                                "ru": "Она испытала сильный <w>культурный</w> шок, когда переехала из маленькой деревни в огромный мегаполис."
                            }
                        }
                    ],
                    "synonyms": [
                        "society",
                        "civilization",
                        "customs",
                        "heritage",
                        "way of life"
                    ],
                    "antonyms": [
                        "nature"
                    ],
                    "common_phrases": [
                        "popular culture",
                        "culture shock",
                        "corporate culture",
                        "western culture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусство, обычаи, образ жизни и привычки, характеризующие определенное общество, нацию или группу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "культура",
                                "target_lang_sense_clarification": "общество, цивилизация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72760c0f-378d-3934-ba1a-f53eb090d0be",
                    "sense_definition": "A growth of bacteria or other biological cells in a specially prepared medium for scientific study.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biological_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor took a throat <w>culture</w> to check for a bacterial infection.",
                            "target_lang_translations": {
                                "ru": "Врач взял <w>посев</w> из горла, чтобы проверить наличие бактериальной инфекции."
                            }
                        },
                        {
                            "text": "Scientists examine the cell <w>culture</w> under a microscope to monitor its growth.",
                            "target_lang_translations": {
                                "ru": "Ученые исследуют клеточную <w>культуру</w> под микроскопом, чтобы наблюдать за ее ростом."
                            }
                        }
                    ],
                    "synonyms": [
                        "growth"
                    ],
                    "common_phrases": [
                        "tissue culture",
                        "bacterial culture",
                        "throat culture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Колония бактерий или других биологических клеток, выращенная в специально подготовленной среде для научного изучения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "культура",
                                "target_lang_sense_clarification": "биология, клетки"
                            },
                            {
                                "target_lang_word": "посев",
                                "target_lang_sense_clarification": "медицинский анализ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16d65d00-890c-38ea-b69b-0067f15c7390",
                    "sense_definition": "The cultivation and care of plants or animals; the act of growing or breeding.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "agricultural_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>culture</w> of orchids requires precise control of temperature and humidity.",
                            "target_lang_translations": {
                                "ru": "<w>Выращивание</w> орхидей требует точного контроля температуры и влажности."
                            }
                        },
                        {
                            "text": "He wrote a comprehensive guide on the <w>culture</w> of silkworms for textile production.",
                            "target_lang_translations": {
                                "ru": "Он написал подробное руководство по <w>разведению</w> шелкопряда для текстильного производства."
                            }
                        }
                    ],
                    "synonyms": [
                        "cultivation",
                        "farming",
                        "rearing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возделывание и уход за растениями или животными; процесс выращивания или разведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разведение",
                                "target_lang_sense_clarification": "животных или растений"
                            },
                            {
                                "target_lang_word": "выращивание",
                                "target_lang_sense_clarification": "растений"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "001033a3-af9e-305b-87b2-876817da876b",
                    "sense_definition": "To grow bacteria or cells in a prepared medium for scientific analysis.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biological_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Researchers <w>culture</w> these microorganisms to study their resistance to antibiotics.",
                            "target_lang_translations": {
                                "ru": "Исследователи <w>культивируют</w> эти микроорганизмы, чтобы изучить их устойчивость к антибиотикам."
                            }
                        },
                        {
                            "text": "We need to <w>culture</w> the sample for several days before we can identify the pathogen.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>культивировать</w> образец в течение нескольких дней, прежде чем мы сможем идентифицировать патоген."
                            }
                        }
                    ],
                    "synonyms": [
                        "grow",
                        "cultivate",
                        "incubate"
                    ],
                    "common_phrases": [
                        "culture cells",
                        "culture bacteria"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выращивать бактерии или клетки в подготовленной среде для научного анализа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "культивировать",
                                "target_lang_sense_clarification": "в лабораторных условиях"
                            },
                            {
                                "target_lang_word": "выращивать",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

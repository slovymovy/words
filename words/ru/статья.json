            "senses": [
                {
                    "sense_id": "c1f52d82-5e8f-3add-9ceb-b895eb02e483",
                    "sense_definition": "Публицистическое или научное сочинение небольшого размера в сборнике, журнале или газете.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "text_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я прочитал интересную <w>статью</w> о космосе в научном журнале.",
                            "target_lang_translations": {
                                "en": "I read an interesting <w>article</w> about space in a scientific journal."
                            }
                        },
                        {
                            "text": "Эта <w>статья</w> в газете рассказывает о прогнозе погоды на неделю.",
                            "target_lang_translations": {
                                "en": "This newspaper <w>article</w> talks about the weather forecast for the week."
                            }
                        }
                    ],
                    "synonyms": [
                        "публикация",
                        "заметка",
                        "материал",
                        "текст"
                    ],
                    "common_phrases": [
                        "интересная статья",
                        "научная статья",
                        "статья в газете",
                        "автор статьи",
                        "написать статью"
                    ],
                    "target_lang_definitions": {
                        "en": "A journalistic or scientific composition of small size in a collection, magazine, or newspaper."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "article",
                                "target_lang_sense_clarification": "journalism, general"
                            },
                            {
                                "target_lang_word": "paper",
                                "target_lang_sense_clarification": "scientific"
                            },
                            {
                                "target_lang_word": "piece",
                                "target_lang_sense_clarification": "journalistic writing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c11abbaf-b0b9-3123-b274-dedb65647fe3",
                    "sense_definition": "Самостоятельный раздел, пункт в юридическом документе, законе или словаре.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "document_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Адвокат внимательно изучил новую <w>статью</w> уголовного кодекса.",
                            "target_lang_translations": {
                                "en": "The lawyer carefully studied the new <w>article</w> of the criminal code."
                            }
                        },
                        {
                            "text": "Каждая словарная <w>статья</w> содержит толкование слова и примеры.",
                            "target_lang_translations": {
                                "en": "Each dictionary <w>entry</w> contains a definition of the word and examples."
                            }
                        }
                    ],
                    "synonyms": [
                        "пункт",
                        "параграф"
                    ],
                    "common_phrases": [
                        "статья закона",
                        "словарная статья",
                        "согласно статье",
                        "нарушение статьи"
                    ],
                    "target_lang_definitions": {
                        "en": "An independent section or point in a legal document, law, or dictionary."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "article",
                                "target_lang_sense_clarification": "legal document"
                            },
                            {
                                "target_lang_word": "entry",
                                "target_lang_sense_clarification": "dictionary"
                            },
                            {
                                "target_lang_word": "clause",
                                "target_lang_sense_clarification": "legal provision"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2f1caeb-c236-3bd5-a1e7-0023773102a9",
                    "sense_definition": "Раздел в бюджете или финансовом документе, обозначающий вид дохода или расхода.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Самая большая <w>статья</w> расходов в нашей семье — это продукты питания.",
                            "target_lang_translations": {
                                "en": "The largest expense <w>item</w> in our family is food."
                            }
                        },
                        {
                            "text": "Директор предложил сократить эту <w>статью</w> бюджета для экономии средств.",
                            "target_lang_translations": {
                                "en": "The director proposed cutting this budget <w>item</w> to save funds."
                            }
                        }
                    ],
                    "synonyms": [
                        "пункт",
                        "графа"

            "senses": [
                {
                    "sense_id": "664a8ea1-d6d4-3fc9-b9e3-9db25f9d83b6",
                    "sense_definition": "Used to express farewell or goodbye.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He waved his hand and shouted <w>sayonara</w> as the bus drove away.",
                            "target_lang_translations": {
                                "ru": "Он помахал рукой и крикнул «<w>сайонара</w>», когда автобус отъехал."
                            }
                        },
                        {
                            "text": "It is time to say <w>sayonara</w> to this old computer.",
                            "target_lang_translations": {
                                "ru": "Пришло время сказать <w>сайонара</w> этому старому компьютеру."
                            }
                        }
                    ],
                    "synonyms": [
                        "goodbye",
                        "farewell",
                        "bye",
                        "adios",
                        "ciao",
                        "adieu",
                        "so long",
                        "cheerio"
                    ],
                    "common_phrases": [
                        "say sayonara"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения прощания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сайонара",
                                "target_lang_sense_clarification": "японское прощание"
                            },
                            {
                                "target_lang_word": "прощай",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3c6f0c81-6448-3e85-b464-f6fa7f9b2ac0",
                    "sense_definition": "An utterance of goodbye or an act of wishing farewell.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We exchanged quick <w>sayonaras</w> before heading to our separate flights.",
                            "target_lang_translations": {
                                "ru": "Мы обменялись короткими <w>прощаниями</w> перед тем, как отправиться на свои рейсы."
                            }
                        },
                        {
                            "text": "The party served as a final <w>sayonara</w> to our retiring colleague.",
                            "target_lang_translations": {
                                "ru": "Вечеринка послужила последним <w>прощанием</w> с нашим уходящим на пенсию коллегой."
                            }
                        }
                    ],
                    "synonyms": [
                        "farewell",
                        "goodbye"
                    ],
                    "common_phrases": [
                        "final sayonara",
                        "bid sayonara"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слова прощания или акт прощания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прощание",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

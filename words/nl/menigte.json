            "senses": [
                {
                    "sense_id": "0f15b6d4-36f5-3290-bb9b-5e4249826c45",
                    "sense_definition": "Een grote groep mensen die zich dicht bij elkaar bevindt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>menigte</w> was op weg van het station naar het stadion.",
                            "target_lang_translations": {
                                "en": "The <w>crowd</w> was on its way from the station to the stadium.",
                                "ru": "<w>Толпа</w> направлялась от вокзала к стадиону."
                            }
                        },
                        {
                            "text": "Ik raakte mijn vrienden al snel kwijt in de drukke <w>menigte</w>.",
                            "target_lang_translations": {
                                "en": "I quickly lost my friends in the busy <w>crowd</w>.",
                                "ru": "Я быстро потерял друзей в густой <w>толпе</w>."
                            }
                        },
                        {
                            "text": "De politie probeerde de woedende <w>menigte</w> op het plein onder controle te houden.",
                            "target_lang_translations": {
                                "en": "The police tried to keep the angry <w>crowd</w> in the square under control.",
                                "ru": "Полиция пыталась держать под контролем разъяренную <w>толпу</w> на площади."
                            }
                        }
                    ],
                    "synonyms": [
                        "massa",
                        "drom"
                    ],
                    "antonyms": [
                        "enkeling"
                    ],
                    "common_phrases": [
                        "in de menigte",
                        "een grote menigte",
                        "de woedende menigte"
                    ],
                    "target_lang_definitions": {
                        "en": "A large group of people gathered closely together.",
                        "ru": "Большая группа людей, собравшихся вместе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crowd",
                                "target_lang_sense_clarification": "group of people"
                            },
                            {
                                "target_lang_word": "throng",
                                "target_lang_sense_clarification": "densely packed group"
                            },
                            {
                                "target_lang_word": "multitude",
                                "target_lang_sense_clarification": "large number of people (formal)"
                            }
                        ],
                        "ru": [
                            {

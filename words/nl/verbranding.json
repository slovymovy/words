                },
                {
                    "sense_id": "65c1c0e8-f9d9-3c69-930e-d305132dbe3a",
                    "sense_definition": "Een scheikundig proces waarbij een stof reageert met zuurstof en warmte produceert.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "chemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor <w>verbranding</w> is altijd zuurstof nodig.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "794a577d-5d3b-32cd-924b-64d48aba176f",
                    "sense_definition": "Ulicznik, chuligan lub człowiek wywodzący się z marginesu społecznego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_outcast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod sklepem stał jakiś <knajak> i zaczepiał przechodniów o drobne.",
                            "target_lang_translations": {
                                "ru": "У магазина стоял какой-то <босяк> и приставал к прохожим, выпрашивая мелочь."
                            }
                        },
                        {
                            "text": "Rodzice zabraniali jej spotykać się z tymi osiedlowymi <knajakami>.",
                            "target_lang_translations": {
                                "ru": "Родители запрещали ей встречаться с этими дворовыми <хулиганами>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ulicznik",
                        "lump"
                    ],
                    "common_phrases": [
                        "pospolity knajak",
                        "miejski knajak"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo o zabarwieniu potocznym, często pejoratywne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уличный хулиган, шпана или человек из социальных низов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хулиган",
                                "target_lang_sense_clarification": "человек, грубо нарушающий общественный порядок"
                            },
                            {
                                "target_lang_word": "шпана",
                                "target_lang_sense_clarification": "мелкий уличный преступник или хулиган (собирательное)"
                            },
                            {
                                "target_lang_word": "босяк",
                                "target_lang_sense_clarification": "опустившийся человек, бродяга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf703e03-3e82-3d74-b02d-c19a58dc6b9c",
                    "sense_definition": "Osoba sprytna, przebiegła, potrafiąca radzić sobie w życiu, często w sposób nieuczciwy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clever_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest stary <knajak>, który zawsze znajdzie sposób na łatwy zarobek.",
                            "target_lang_translations": {
                                "ru": "Это старый <пройдоха>, который всегда найдет способ легко заработать."
                            }
                        },
                        {
                            "text": "Nie ufaj mu, bo to <knajak> i na pewno spróbuje cię oszukać.",
                            "target_lang_translations": {
                                "ru": "Не доверяй ему, потому что он <жулик> и наверняка попытается тебя обмануть."
                            }
                        }
                    ],
                    "synonyms": [
                        "cwaniak",
                        "spryciarz"
                    ],
                    "common_phrases": [
                        "stary knajak",
                        "niezły knajak"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хитрый, изворотливый человек, умеющий устраиваться в жизни, часто нечестным путем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пройдоха",
                                "target_lang_sense_clarification": "пронырливый, жуликоватый человек"
                            },
                            {
                                "target_lang_word": "ловкач",
                                "target_lang_sense_clarification": "человек, умеющий найти выход из любой ситуации"
                            },
                            {
                                "target_lang_word": "жулик",
                                "target_lang_sense_clarification": "человек, склонный к обману и мошенничеству"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ebdabc6b-2193-3202-8eef-845da052e455",
                    "sense_definition": "Iets vernietigen of laten verteren door vuur",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fire_destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer <w>verbrandde</w> het tuinafval achter zijn huis.",
                            "target_lang_translations": {
                                "ru": "Фермер <w>сжег</w> садовый мусор за своим домом."
                            }
                        },
                        {
                            "text": "Hij heeft al zijn oude brieven in de open haard <w>verbrand</w>.",
                            "target_lang_translations": {
                                "ru": "Он <w>сжег</w> все свои старые письма в камине."
                            }
                        }
                    ],
                    "synonyms": [
                        "opbranden"
                    ],
                    "common_phrases": [
                        "afval verbranden",
                        "hout verbranden",
                        "alles verbranden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уничтожать что-либо или давать чему-либо исчезнуть в огне"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сжигать",
                                "target_lang_sense_clarification": "уничтожать огнем"
                            },
                            {
                                "target_lang_word": "жечь",
                                "target_lang_sense_clarification": "предавать огню"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1421cdb9-8ca9-3422-8f2c-b15236cd0073",
                    "sense_definition": "Rood en pijnlijk worden van de huid door blootstelling aan de zon",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "body_injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Smeer je goed in met zonnebrandcrème, anders zul je <w>verbranden</w>.",
                            "target_lang_translations": {
                                "ru": "Хорошо намажься солнцезащитным кремом, иначе ты <w>обгоришь</w>."
                            }
                        },
                        {
                            "text": "Mijn rug is gisteren erg <w>verbrand</w> op het strand.",
                            "target_lang_translations": {
                                "ru": "Моя спина вчера сильно <w>обгорела</w> на пляже."
                            }
                        }
                    ],
                    "common_phrases": [
                        "snel verbranden",
                        "in de zon verbranden",
                        "levend verbranden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покраснение и болезненность кожи из-за воздействия солнца"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обгорать",
                                "target_lang_sense_clarification": "получать солнечный ожог"
                            },
                            {
                                "target_lang_word": "сгорать",
                                "target_lang_sense_clarification": "разг. о сильном загаре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de313d93-1252-3fd7-9093-78eebac2cae5",
                    "sense_definition": "Door vuur of hitte verteerd worden",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "fire_destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het oude houten huis <w>verbrandde</w> tot de grond toe.",
                            "target_lang_translations": {
                                "ru": "Старый деревянный дом <w>сгорел</w> дотла."
                            }
                        },
                        {
                            "text": "Tijdens de droge zomer <w>verbrandden</w> grote stukken bos.",
                            "target_lang_translations": {
                                "ru": "Во время засушливого лета <w>сгорели</w> большие участки леса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "langzaam verbranden",
                        "volledig verbranden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть уничтоженным огнем или жаром"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сгорать",
                                "target_lang_sense_clarification": "уничтожаться огнем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97675f89-b46f-3a67-894f-f40136c5c418",
                    "sense_definition": "Beschadigen of verwonden door extreme hitte of bijtende chemische stoffen",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "body_injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wees voorzichtig, want dit zuur kan je huid ernstig <w>verbranden</w>.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, так как эта кислота может серьезно <w>обжечь</w> твою кожу."
                            }
                        },
                        {
                            "text": "Hij <w>verbrandde</w> zijn hand aan de hete pan.",
                            "target_lang_translations": {
                                "ru": "Он <w>обжег</w> руку о горячую сковороду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich verbranden aan",
                        "vingers verbranden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повредить или травмировать тело сильным жаром или химикатами"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обжигать",
                                "target_lang_sense_clarification": "наносить ожог"
                            },
                            {
                                "target_lang_word": "обжигаться",
                                "target_lang_sense_clarification": "получать ожог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "654e0fd3-108c-31d1-9c35-05aec12b05ca",
                    "sense_definition": "Iemands reputatie of carrière kapotmaken waardoor diegene niet meer geloofwaardig is",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door het grote schandaal is de minister politiek volledig <w>verbrand</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за крупного скандала министр политически полностью <w>дискредитирован</w>."
                            }
                        },
                        {
                            "text": "Hij heeft zichzelf <w>verbrand</w> door vertrouwelijke informatie te lekken.",
                            "target_lang_translations": {
                                "ru": "Он <w>дискредитировал</w> себя, слив конфиденциальную информацию."
                            }

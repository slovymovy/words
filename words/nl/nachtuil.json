                {
                    "sense_id": "0984fa13-59f1-3f05-abc4-f17cbb55bdcf",
                    "sense_definition": "Iemand die vooral 's avonds en 's nachts actief is en laat naar bed gaat.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "person_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben een echte <w>nachtuil</w> en werk het liefst als iedereen slaapt.",
                            "target_lang_translations": {
                                "en": "I am a real <w>night owl</w> and prefer working when everyone is asleep.",
                                "ru": "Я настоящая <w>сова</w> и предпочитаю работать, когда все спят."
                            }
                        },
                        {
                            "text": "Mijn broer is een <w>nachtuil</w>, dus verwacht niet dat hij vroeg opstaat.",
                            "target_lang_translations": {
                                "en": "My brother is a <w>night owl</w>, so don't expect him to get up early.",
                                "ru": "Мой брат — <w>сова</w>, так что не жди, что он рано встанет."
                            }
                        }
                    ],
                    "synonyms": [
                        "nachtbraker"
                    ],
                    "antonyms": [
                        "vroege vogel",
                        "ochtendmens"
                    ],
                    "common_phrases": [
                        "een echte nachtuil"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who is active mainly in the evening and at night and goes to bed late.",
                        "ru": "Человек, который наиболее активен в вечернее и ночное время и поздно ложится спать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "night owl",
                                "target_lang_sense_clarification": "idiomatic, person active at night"
                            },
                            {
                                "target_lang_word": "night person",
                                "target_lang_sense_clarification": "person with a late chronotype"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сова",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "полуночник",
                                "target_lang_sense_clarification": "любитель не спать по ночам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45ca4650-5005-3b48-bb88-f9eebdc7735b",
                    "sense_definition": "Een vogel uit de familie van de uilen die voornamelijk in de nacht actief is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_bird",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het donkere bos hoorden we het geluid van een <w>nachtuil</w>.",
                            "target_lang_translations": {
                                "en": "In the dark forest, we heard the sound of a <w>night owl</w>.",
                                "ru": "В темном лесу мы услышали крик <w>совы</w>."
                            }
                        },
                        {
                            "text": "De <w>nachtuil</w> vliegt geruisloos door de lucht op zoek naar een prooi.",
                            "target_lang_translations": {
                                "en": "The <w>night owl</w> flies silently through the air in search of prey.",
                                "ru": "<w>Сова</w> бесшумно летит по воздуху в поисках добычи."
                            }
                        }
                    ],
                    "synonyms": [
                        "uil"
                    ],
                    "target_lang_definitions": {

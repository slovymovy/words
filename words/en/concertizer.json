            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a3d1e728-6d0f-3466-867a-51d7201c82ed",
                    "sense_definition": "One who performs in concerts.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "performer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The famous pianist was a frequent <w>concertizer</w> throughout Europe during the late nineteenth century.",
                            "target_lang_translations": {
                                "ru": "Знаменитый пианист был частым <w>концертантом</w> в Европе в конце девятнадцатого века."
                            }
                        },
                        {
                            "text": "As a dedicated <w>concertizer</w>, she spent most of the year traveling from city to city.",
                            "target_lang_translations": {
                                "ru": "Будучи активным <w>концертантом</w>, она проводила большую часть года в разъездах из города в город."
                            }
                        }
                    ],
                    "synonyms": [
                        "performer",
                        "concert artist"
                    ],
                    "common_phrases": [
                        "frequent concertizer",
                        "active concertizer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто выступает с концертами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "концертант",
                                "target_lang_sense_clarification": "артист, дающий концерты"
                            },
                            {
                                "target_lang_word": "гастролёр",
                                "target_lang_sense_clarification": "артист, выступающий в поездках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b18e3804-4233-30dd-b40e-efa06a7f4422",
                    "sense_definition": "One who adapts music for concert performance.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "adapter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>concertizer</w> reworked the simple folk song into a grand piece for the orchestra.",
                            "target_lang_translations": {
                                "ru": "<w>Аранжировщик</w> переработал простую народную песню в грандиозное произведение для оркестра."
                            }
                        },
                        {
                            "text": "He is known as a skilled <w>concertizer</w> of classical piano miniatures.",
                            "target_lang_translations": {
                                "ru": "Он известен как искусный <w>аранжировщик</w> классических фортепианных миниатюр."
                            }
                        }
                    ],
                    "synonyms": [
                        "arranger",
                        "adapter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто адаптирует музыку для концертного исполнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аранжировщик",
                                "target_lang_sense_clarification": "создатель музыкальных обработок"
                            }
                        ]
                    }
                }
            ]
        }

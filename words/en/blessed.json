    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c3934e14-5d4b-32f3-955f-79160d6e40f9",
                    "sense_definition": "Simple past and past participle of <w>bless</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The priest <w>blessed</w> the couple before they left the church.",
                            "target_lang_translations": {
                                "ru": "Священник <w>благословил</w> пару, перед тем как они покинули церковь."
                            }
                        },
                        {
                            "text": "She <w>blessed</w> the food before serving dinner to the guests.",
                            "target_lang_translations": {
                                "ru": "Она <w>благословила</w> еду перед тем, как подать ужин гостям."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола bless (благословлять)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благословил",
                                "target_lang_sense_clarification": "прошедшее время"
                            },
                            {
                                "target_lang_word": "благословлённый",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7b4b2e04-0114-3a1b-bc4c-8e94fc7417f8",
                    "sense_definition": "Having divine aid, protection, or being made holy; often used to describe someone who is lucky or fortunate.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sacred_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She felt truly <w>blessed</w> to have such a supportive family.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала себя по-настоящему <w>благословенной</w>, имея такую поддерживающую семью."
                            }
                        },
                        {
                            "text": "The <w>blessed</w> water was used during the baptism ceremony.",
                            "target_lang_translations": {
                                "ru": "<w>Святая</w> вода использовалась во время церемонии крещения."
                            }
                        }
                    ],
                    "synonyms": [
                        "holy",
                        "sacred",
                        "hallowed"
                    ],
                    "antonyms": [
                        "cursed",
                        "unholy"
                    ],
                    "common_phrases": [
                        "blessed with",
                        "truly blessed",
                        "blessed life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий божественной помощью, защитой или ставший святым; часто используется для описания удачливого или счастливого человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благословенный",
                                "target_lang_sense_clarification": "обладающий благодатью"
                            },
                            {
                                "target_lang_word": "святой",
                                "target_lang_sense_clarification": "священный, обладающий святостью"
                            },
                            {
                                "target_lang_word": "счастливый",
                                "target_lang_sense_clarification": "удачливый (в переносном смысле)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "224461d6-2e6e-3cc9-85d9-6a487247f9a1",
                    "sense_definition": "Used as an intensifier to express annoyance or emphasis, similar to 'damned'.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I haven't had a single <w>blessed</w> moment of peace all day.",
                            "target_lang_translations": {
                                "ru": "У меня не было ни одной <w>чёртовой</w> минуты покоя за весь день."
                            }
                        },
                        {
                            "text": "He didn't do a <w>blessed</w> thing to help us move the furniture.",
                            "target_lang_translations": {
                                "ru": "Он не сделал ни <w>черта</w>, чтобы помочь нам переставить мебель."
                            }
                        }
                    ],
                    "synonyms": [
                        "hallowed"
                    ],
                    "common_phrases": [
                        "every blessed day",
                        "not a blessed thing"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used for emphasis"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления, чтобы выразить раздражение или эмфазу; эвфемизм для слов «проклятый» или «чёртов»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёртов",
                                "target_lang_sense_clarification": "разговорное, выражение раздражения"
                            },
                            {
                                "target_lang_word": "несчастный",
                                "target_lang_sense_clarification": "усилительная частица, выражающая недовольство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d96f523e-5421-3fbd-bfa5-3bcf96196aa9",
                    "sense_definition": "A title indicating the beatification of a person, allowing public veneration.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sacred_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The church was dedicated to the <w>Blessed</w> Virgin Mary.",
                            "target_lang_translations": {
                                "ru": "Церковь была посвящена <w>Пресвятой</w> Деве Марии."
                            }
                        },
                        {
                            "text": "Many pilgrims traveled to visit the shrine of the <w>Blessed</w> martyr.",
                            "target_lang_translations": {
                                "ru": "Многие паломники ехали посетить гробницу <w>блаженного</w> мученика."
                            }
                        }
                    ],
                    "synonyms": [
                        "revered",
                        "venerated"
                    ],
                    "common_phrases": [
                        "Blessed Virgin",
                        "Blessed Sacrament"
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул, указывающий на беатификацию (причисление к лику блаженных) человека в христианской традиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блаженный",
                                "target_lang_sense_clarification": "титул святого"
                            },
                            {
                                "target_lang_word": "пресвятой",
                                "target_lang_sense_clarification": "титул (обычно по отношению к Деве Марии)"
                            }
                        ]
                    }
                }
            ]
        },

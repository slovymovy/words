    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "998ccdfb-3057-3b6f-81a2-3b838ae7b85e",
                    "sense_definition": "Popularny, ciemny napój pobudzający, przygotowywany z palonych i zmielonych ziaren kawowca.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "beverage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdego ranka piję filiżankę gorącej <w>kawy</w> z mlekiem.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я выпиваю чашку горячего <w>кофе</w> с молоком."
                            }
                        },
                        {
                            "text": "Czy masz ochotę na pyszną <w>kawę</w> i kawałek ciasta?",
                            "target_lang_translations": {
                                "ru": "Хочешь вкусного <w>кофе</w> и кусочек торта?"
                            }
                        }
                    ],
                    "synonyms": [
                        "mała czarna"
                    ],
                    "common_phrases": [
                        "pić kawę",
                        "parzyć kawę",
                        "kawa z mlekiem",
                        "czarna kawa",
                        "filiżanka kawy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Популярный темный бодрящий напиток, приготовленный из жареных и молотых зерен кофейного дерева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кофе",
                                "target_lang_sense_clarification": "напиток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c87e46a7-ee9b-3be2-92ad-508f0de1cf24",
                    "sense_definition": "Palone i zmielone ziarna rośliny kawowca, używane do parzenia napoju; także produkt w postaci proszku lub granulek.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę iść do sklepu, bo skończyła nam się <w>kawa</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно идти в магазин, потому что у нас закончился <w>кофе</w>."
                            }
                        },
                        {
                            "text": "Najlepiej smakuje świeżo zmielona <w>kawa</w> wysokiej jakości.",
                            "target_lang_translations": {
                                "ru": "Лучше всего на вкус свежемолотый <w>кофе</w> высокого качества."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mielić kawę",
                        "ziarna kawy",
                        "kawa rozpuszczalna",
                        "kawa ziarnista"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жареные и молотые зерна кофейного дерева, используемые для заваривания напитка; также продукт в виде порошка или гранул."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кофе",
                                "target_lang_sense_clarification": "зерна или порошок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50294051-464d-337f-b989-6d3c3ccb211b",
                    "sense_definition": "Krótkie spotkanie towarzyskie, podczas którego pije się ten napój i rozmawia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dawno się nie widzieliśmy, może wyskoczymy jutro na <w>kawę</w>?",
                            "target_lang_translations": {
                                "ru": "Мы давно не виделись, может, выберемся завтра на <w>кофе</w>?"
                            }
                        },
                        {
                            "text": "Zaprosiłem koleżankę z pracy na <w>kawę</w> do nowej kawiarni.",
                            "target_lang_translations": {
                                "ru": "Я пригласил коллегу с работы на <w>кофе</w> в новую кофейню."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iść na kawę",
                        "umówić się na kawę",
                        "zaprosić na kawę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткая дружеская встреча, во время которой пьют этот напиток и разговаривают."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кофе",
                                "target_lang_sense_clarification": "встреча"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15fd825e-d50f-3c4d-900a-a171f2aa2cd4",
                    "sense_definition": "Napój przypominający smakiem kawę, ale sporządzany z palonych ziaren zbóż, cykorii lub innych roślin, nie zawierający kofeiny.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "beverage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci w przedszkolu często piją na śniadanie <w>kawę</w> zbożową.",
                            "target_lang_translations": {
                                "ru": "Дети в детском саду часто пьют на завтрак злаковый <w>кофе</w>."
                            }
                        },
                        {
                            "text": "Wieczorem wolę pić <w>kawę</w> bez kofeiny, żeby lepiej spać.",
                            "target_lang_translations": {
                                "ru": "Вечером я предпочитаю пить <w>кофе</w> без кофеина, чтобы лучше спать."
                            }
                        }
                    ],
                    "synonyms": [
                        "kawa zbożowa",
                        "inka"
                    ],
                    "common_phrases": [
                        "kawa zbożowa",
                        "kawa bezkofeinowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напиток, напоминающий по вкусу кофе, но приготовленный из жареных зерен злаков, цикория или других растений, не содержащий кофеина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кофейный напиток",
                                "target_lang_sense_clarification": "заменитель"
                            },
                            {
                                "target_lang_word": "кофе",
                                "target_lang_sense_clarification": "суррогат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "005ccb1e-5a0e-344d-b042-1b8649404abe",
                    "sense_definition": "Roślina tropikalna (drzewo lub krzew), której owoce zawierają ziarna służące do produkcji kawy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ogrodzie botanicznym widzieliśmy kwitnącą <w>kawę</w>.",
                            "target_lang_translations": {
                                "ru": "В ботаническом саду мы видели цветущее <w>кофейное дерево</w>."
                            }
                        },
                        {
                            "text": "Uprawa <w>kawy</w> wymaga specyficznego klimatu i odpowiedniej wysokości.",
                            "target_lang_translations": {
                                "ru": "Выращивание <w>кофе</w> требует специфического климата и соответствующей высоты."
                            }
                        }
                    ],
                    "synonyms": [
                        "kawowiec"
                    ],
                    "common_phrases": [
                        "plantacja kawy",
                        "uprawa kawy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тропическое растение (дерево или кустарник), плоды которого содержат зерна, используемые для производства кофе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кофейное дерево",
                                "target_lang_sense_clarification": "растение"
                            },
                            {
                                "target_lang_word": "кофе",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e4d802a-797e-3542-8043-3d713f1176c4",
                    "sense_definition": "Ptak z rodziny krukowatych; regionalne lub dawne określenie kawki.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na dachu starego domu siedziała czarna <w>kawa</w>.",
                            "target_lang_translations": {
                                "ru": "На крыше старого дома сидела черная <w>галка</w>."
                            }
                        },
                        {
                            "text": "W tym regionie starsi ludzie wciąż nazywają tego ptaka <w>kawą</w>.",
                            "target_lang_translations": {
                                "ru": "В этом регионе пожилые люди все еще называют эту птицу <w>галкой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kawka"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "używane rzadko, głównie regionalnie lub dawniej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Птица из семейства врановых; региональное или устаревшее название галки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "галка",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                }
            ]
        },

                        {
                            "text": "Вчера <Вера> пришла на работу раньше всех.",
                            "target_lang_translations": {
                                "en": "Yesterday <Vera> came to work earlier than everyone else."
                            }
                        }
                    ],
                    "target_lang_definitions": {

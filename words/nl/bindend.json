            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f12f310a-2188-389f-a6a8-4ead73cb194a",
                    "sense_definition": "Waaraan men zich moet houden; wat verplicht is en niet vrijblijvend.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het advies van de commissie is <w>bindend</w> voor het bestuur.",
                            "target_lang_translations": {
                                "en": "The committee's advice is <w>binding</w> for the board.",
                                "ru": "Рекомендация комиссии является <w>обязательной</w> для правления."
                            }
                        },
                        {
                            "text": "We hebben een <w>bindende</w> afspraak gemaakt over de leveringstijden.",
                            "target_lang_translations": {
                                "en": "We have made a <w>binding</w> agreement regarding the delivery times.",
                                "ru": "Мы заключили <w>обязательное</w> соглашение о сроках поставки."
                            }
                        }
                    ],
                    "synonyms": [
                        "verplicht"
                    ],
                    "antonyms": [
                        "vrijblijvend"
                    ],
                    "common_phrases": [
                        "bindend advies",
                        "bindende afspraken",
                        "bindend verklaren"
                    ],
                    "target_lang_definitions": {
                        "en": "Something one must adhere to; what is mandatory and not optional.",
                        "ru": "Обязательный для исполнения; то, чего необходимо придерживаться и что не является добровольным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "binding",
                                "target_lang_sense_clarification": "imposing a legal or moral obligation"
                            },
                            {
                                "target_lang_word": "mandatory",
                                "target_lang_sense_clarification": "compulsory, required by rule"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обязательный",
                                "target_lang_sense_clarification": "который необходимо выполнять (о правилах, решениях)"
                            },
                            {
                                "target_lang_word": "имеющий обязательную силу",
                                "target_lang_sense_clarification": "юридический контекст"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02fda86f-8951-3423-97cc-fe36db6490b3",
                    "sense_definition": "Wat mensen of dingen met elkaar verbindt of samenbrengt.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muziek vormt vaak een <w>bindende</w> factor tussen jongeren uit verschillende culturen.",
                            "target_lang_translations": {
                                "en": "Music often forms a <w>unifying</w> factor between young people from different cultures.",
                                "ru": "Музыка часто служит <w>связующим</w> фактором для молодежи из разных культур."
                            }
                        },
                        {
                            "text": "Het gedeelde verleden was het enige <w>bindende</w> element in hun relatie.",
                            "target_lang_translations": {
                                "en": "The shared past was the only <w>binding</w> element in their relationship.",
                                "ru": "Общее прошлое было единственным <w>связующим</w> элементом в их отношениях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bindende factor",
                        "bindend element"
                    ],
                    "target_lang_definitions": {
                        "en": "That which connects or brings people or things together.",
                        "ru": "То, что соединяет или объединяет людей или предметы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unifying",
                                "target_lang_sense_clarification": "bringing together to form a unit"
                            },
                            {
                                "target_lang_word": "binding",
                                "target_lang_sense_clarification": "serving to connect or hold together"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "связующий",
                                "target_lang_sense_clarification": "служащий для связи, соединения"
                            },
                            {
                                "target_lang_word": "объединяющий",
                                "target_lang_sense_clarification": "сплачивающий людей"
                            }
                        ]
                    }
                }
            ]
        },

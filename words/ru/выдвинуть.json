    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "69d176ac-295c-38ad-baeb-52b4130dcc5b",
                    "sense_definition": "Вытащить или переместить наружу то, что было вдвинуто или находилось внутри.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я попросил брата <выдвинуть> нижний ящик письменного стола.",
                            "target_lang_translations": {
                                "en": "I asked my brother to <pull out> the bottom drawer of the desk."
                            }
                        },
                        {
                            "text": "Она попыталась аккуратно <выдвинуть> коробку из шкафа.",
                            "target_lang_translations": {
                                "en": "She tried to carefully <pull out> the box from the closet."
                            }
                        }
                    ],
                    "synonyms": [
                        "вытащить",
                        "извлечь"
                    ],
                    "antonyms": [
                        "задвинуть"
                    ],
                    "common_phrases": [
                        "выдвинуть ящик"
                    ],
                    "target_lang_definitions": {
                        "en": "To pull out or move outward something that was pushed in or located inside."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pull out",
                                "target_lang_sense_clarification": "drawers, objects"
                            },
                            {
                                "target_lang_word": "slide out",
                                "target_lang_sense_clarification": "smooth movement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01aef048-7114-3496-9443-cd841b58e737",
                    "sense_definition": "Переместить предмет вперёд или на более видное, открытое место.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <выдвинуть> диван на середину комнаты для удобства.",
                            "target_lang_translations": {
                                "en": "We decided to <pull> the sofa <out> to the middle of the room for convenience."
                            }
                        },
                        {
                            "text": "Командир отдал приказ <выдвинуть> войска к линии фронта.",
                            "target_lang_translations": {
                                "en": "The commander gave the order to <advance> the troops to the front line."
                            }
                        }
                    ],
                    "synonyms": [
                        "выставить",
                        "продвинуть"
                    ],
                    "antonyms": [
                        "задвинуть"
                    ],
                    "common_phrases": [
                        "выдвинуть стол",
                        "выдвинуть войска"
                    ],
                    "target_lang_definitions": {
                        "en": "To move an object forward or to a more prominent, open place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move forward",
                                "target_lang_sense_clarification": "general physical movement"
                            },
                            {
                                "target_lang_word": "advance",
                                "target_lang_sense_clarification": "troops, military context"
                            },
                            {
                                "target_lang_word": "push forward",
                                "target_lang_sense_clarification": "move to the front"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a707b40-6bcf-3c1e-9dda-d7b1e621b89d",
                    "sense_definition": "Предложить идею, требование, аргумент или обвинение для обсуждения и рассмотрения.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_proposal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учёный смог <выдвинуть> новую гипотезу о происхождении вселенной.",
                            "target_lang_translations": {
                                "en": "The scientist was able to <put forward> a new hypothesis about the origin of the universe."
                            }
                        },
                        {
                            "text": "Прокурор готовится <выдвинуть> официальное обвинение против подозреваемого.",
                            "target_lang_translations": {
                                "en": "The prosecutor is preparing to <bring> formal charges against the suspect."
                            }
                        }
                    ],
                    "synonyms": [
                        "предложить",
                        "привести"
                    ],
                    "common_phrases": [
                        "выдвинуть идею",
                        "выдвинуть обвинение",
                        "выдвинуть требование",
                        "выдвинуть аргумент"
                    ],
                    "target_lang_definitions": {
                        "en": "To propose an idea, requirement, argument, or accusation for discussion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put forward",
                                "target_lang_sense_clarification": "an idea, theory, or argument"
                            },
                            {
                                "target_lang_word": "bring",
                                "target_lang_sense_clarification": "charges or accusations"
                            },
                            {
                                "target_lang_word": "advance",
                                "target_lang_sense_clarification": "a hypothesis or theory"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb7cbf04-0bc0-301b-83ae-3ab5fa10f716",
                    "sense_definition": "Предложить чью-либо кандидатуру для назначения на должность или получения награды.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_proposal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Коллектив единогласно решил <выдвинуть> его на пост директора.",
                            "target_lang_translations": {
                                "en": "The team unanimously decided to <nominate> him for the post of director."
                            }
                        },
                        {
                            "text": "Кинокритики хотят <выдвинуть> этот фильм на престижную премию.",
                            "target_lang_translations": {
                                "en": "Film critics want to <nominate> this film for a prestigious award."
                            }
                        }
                    ],
                    "synonyms": [
                        "номинировать",
                        "рекомендовать",
                        "представить"
                    ],
                    "common_phrases": [
                        "выдвинуть кандидата",
                        "выдвинуть на пост"
                    ],
                    "target_lang_definitions": {
                        "en": "To propose someone's candidacy for a position or award."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nominate",
                                "target_lang_sense_clarification": "for a post or award"
                            },
                            {
                                "target_lang_word": "put forward",
                                "target_lang_sense_clarification": "a candidate"
                            }
                        ]
                    }
                }
            ]
        }
    ]

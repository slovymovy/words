{
    "word_family": [
        "dragnet",
        "dragster",
        "dragline"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a7c70261-d83b-3bbf-9140-2cc4d016905b",
                    "sense_definition": "To move a digital item on a computer screen by clicking and holding a button on a mouse or trackpad.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can simply <w>drag</w> the file into the folder to copy it.",
                            "target_lang_translations": {
                                "ru": "Вы можете просто <w>перетащить</w> файл в папку, чтобы скопировать его."
                            }
                        },
                        {
                            "text": "Click the image and <w>drag</w> it to the center of the document.",
                            "target_lang_translations": {
                                "ru": "Нажмите на изображение и <w>перетащите</w> его в центр документа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drag and drop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать цифровой объект на экране компьютера, нажав и удерживая кнопку мыши или трекпада."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перета́скивать",
                                "target_lang_sense_clarification": "перемещать объект на экране"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7708051-2017-32ec-bb90-2540155ce258",
                    "sense_definition": "To pull someone or something along a surface, often with difficulty or effort.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We had to <w>drag</w> the heavy table across the room.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <w>тащить</w> тяжелый стол через всю комнату."
                            }
                        },
                        {
                            "text": "The tired child was <w>dragging</w> his teddy bear behind him.",
                            "target_lang_translations": {
                                "ru": "Уставший ребенок <w>волочил</w> за собой плюшевого мишку."
                            }
                        }
                    ],
                    "synonyms": [
                        "pull",
                        "lug",
                        "tug",
                        "draw",
                        "hale"
                    ],
                    "antonyms": [
                        "push"
                    ],
                    "common_phrases": [
                        "drag away",
                        "drag out of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тянуть кого-то или что-то по поверхности, часто с трудом или усилием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тащи́ть",
                                "target_lang_sense_clarification": "тянуть с усилием"
                            },
                            {
                                "target_lang_word": "волочи́ть",
                                "target_lang_sense_clarification": "тянуть по земле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7828f22-afbb-3f13-93b1-8f8b38306411",
                    "sense_definition": "To pass or proceed very slowly and tediously, often making time feel longer.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_passage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The last hour of the work day seemed to <w>drag</w> forever.",
                            "target_lang_translations": {
                                "ru": "Казалось, что последний час рабочего дня <w>тянется</w> вечно."
                            }
                        },
                        {
                            "text": "Time starts to <w>drag</w> when you are waiting for an important phone call.",
                            "target_lang_translations": {
                                "ru": "Время начинает <w>тянуться</w>, когда ждешь важного телефонного звонка."
                            }
                        }
                    ],
                    "antonyms": [
                        "rush"
                    ],
                    "common_phrases": [
                        "drag on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить или протекать очень медленно и утомительно (о времени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тяну́ться",
                                "target_lang_sense_clarification": "медленно проходить (о времени)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "035c75c8-934b-32fa-8d39-4199985c2235",
                    "sense_definition": "To touch, rub, or scrape against a surface while moving.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "movement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car was so low that its muffler began to <w>drag</w> on the speed bump.",
                            "target_lang_translations": {
                                "ru": "Машина была такой низкой, что ее глушитель начал <w>скрести</w> по \"лежачему полицейскому\"."
                            }
                        },
                        {
                            "text": "Her long dress was <w>dragging</w> in the mud as she walked.",
                            "target_lang_translations": {
                                "ru": "Ее длинное платье <w>волочилось</w> по грязи, пока она шла."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Касаться, тереться или скрести о поверхность во время движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волочи́ться",
                                "target_lang_sense_clarification": "тащиться по земле, задевая её"
                            },
                            {
                                "target_lang_word": "скрести́",
                                "target_lang_sense_clarification": "издавать скрежет при трении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4192ac3-1ab6-3108-a2a5-807ece1073f6",
                    "sense_definition": "To criticize, mock, or humiliate someone harshly.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "People on social media started to <w>drag</w> the celebrity for his rude comments.",
                            "target_lang_translations": {
                                "ru": "Люди в социальных сетях начали <w>гнобить</w> знаменитость за его грубые комментарии."
                            }
                        },
                        {
                            "text": "She didn't mean to <w>drag</w> her friend, but her joke was quite offensive.",
                            "target_lang_translations": {
                                "ru": "Она не хотела <w>высмеять</w> подругу, но ее шутка была довольно обидной."
                            }
                        }
                    ],
                    "synonyms": [
                        "criticize",
                        "roast",
                        "bash",
                        "slam",
                        "scold",
                        "berate",
                        "lambaste"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жестко критиковать, высмеивать или унижать кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гноби́ть",
                                "target_lang_sense_clarification": "сленг: унижать, травить"
                            },
                            {
                                "target_lang_word": "критикова́ть",
                                "target_lang_sense_clarification": "высказывать неодобрение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b7af0af-a9d4-3d40-97d8-09407d232ead",
                    "sense_definition": "To inhale smoke from a cigarette, cigar, or pipe.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He paused to <w>drag</w> deeply on his cigarette before answering.",
                            "target_lang_translations": {
                                "ru": "Он сделал паузу, чтобы глубоко <w>затянуться</w> сигаретой, прежде чем ответить."
                            }
                        },
                        {
                            "text": "She watched him <w>drag</w> on the cigar and blow smoke rings.",
                            "target_lang_translations": {
                                "ru": "Она смотрела, как он <w>затягивается</w> сигарой и пускает кольца дыма."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вдыхать дым сигареты, сигары или трубки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затя́гиваться",
                                "target_lang_sense_clarification": "вдыхать дым"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ea9b2070-aab4-3232-b4af-193242057f92",
                    "sense_definition": "A boring, annoying, or disappointing person, thing, or situation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "annoyance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Travelling to work during the rush hour is a real <w>drag</w>.",
                            "target_lang_translations": {
                                "ru": "Поездка на работу в час пик — это настоящая <w>обуза</w>."
                            }
                        },
                        {
                            "text": "It is such a <w>drag</w> that we have to stay inside on a sunny day.",
                            "target_lang_translations": {
                                "ru": "Такая <w>тоска</w>, что нам приходится сидеть дома в солнечный день."
                            }
                        }
                    ],
                    "common_phrases": [
                        "a real drag",
                        "such a drag"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скучный, раздражающий или разочаровывающий человек, вещь или ситуация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обу́за",
                                "target_lang_sense_clarification": "тягостная обязанность или вещь"
                            },
                            {
                                "target_lang_word": "нудя́тина",
                                "target_lang_sense_clarification": "разг.: что-то очень скучное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49083179-e240-34b6-95bc-2ddbce781793",
                    "sense_definition": "Clothing typically worn by the opposite sex, often worn for performance or entertainment purposes.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The actor performed the entire play in <w>drag</w> to great applause.",
                            "target_lang_translations": {
                                "ru": "Актер сыграл весь спектакль в <w>женской одежде</w> под бурные аплодисменты."
                            }
                        },
                        {
                            "text": "We went to a club to watch a famous <w>drag</w> queen perform.",
                            "target_lang_translations": {
                                "ru": "Мы пошли в клуб, чтобы посмотреть выступление известной <w>дрэг</w>-квин."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in drag",
                        "drag queen",
                        "drag show"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одежда, характерная для противоположного пола, часто используемая для выступлений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "же́нская оде́жда",
                                "target_lang_sense_clarification": "в контексте переодевания мужчины"
                            },
                            {
                                "target_lang_word": "дрэг",
                                "target_lang_sense_clarification": "стиль одежды и выступлений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82e8ddb0-b722-383c-ba0a-b60bf47c6ab9",
                    "sense_definition": "The force that slows down an object moving through air or liquid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When designing cars, manufacturers try to reduce aerodynamic <w>drag</w>.",
                            "target_lang_translations": {
                                "ru": "При проектировании автомобилей производители стараются уменьшить аэродинамическое <w>сопротивление</w>."
                            }
                        },
                        {
                            "text": "The swimmer wore a special suit to minimize water <w>drag</w>.",
                            "target_lang_translations": {
                                "ru": "Пловец надел специальный костюм, чтобы минимизировать <w>сопротивление</w> воды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aerodynamic drag"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сила, замедляющая движение объекта сквозь воздух или жидкость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопротивле́ние",
                                "target_lang_sense_clarification": "физическая сила"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d8b98b6-766f-385d-b7f4-83d67f9af0d1",
                    "sense_definition": "An act of inhaling smoke from a cigarette or pipe.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He took a long <w>drag</w> from his cigarette and sighed.",
                            "target_lang_translations": {
                                "ru": "Он сделал длинную <w>затяжку</w> сигаретой и вздохнул."
                            }
                        },
                        {
                            "text": "Can I have a <w>drag</w> of your cigarette before you finish it?",
                            "target_lang_translations": {
                                "ru": "Можно мне сделать <w>затяжку</w> твоей сигареты, пока ты ее не закончил?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "take a drag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт вдыхания дыма из сигареты или трубки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затя́жка",
                                "target_lang_sense_clarification": "вдох дыма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a94ae14-edb6-382e-8f46-e9363b599a68",
                    "sense_definition": "A main street or road in a town.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We drove down the main <w>drag</w> looking for a place to eat.",
                            "target_lang_translations": {
                                "ru": "Мы ехали по главной <w>улице</w> в поисках места, где можно поесть."
                            }
                        },
                        {
                            "text": "Most of the shops on the main <w>drag</w> are closed on Sundays.",
                            "target_lang_translations": {
                                "ru": "Большинство магазинов на главной <w>улице</w> закрыты по воскресеньям."
                            }
                        }
                    ],
                    "common_phrases": [
                        "main drag"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Главная улица или дорога в городе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гла́вная у́лица",
                                "target_lang_sense_clarification": "центральная улица"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
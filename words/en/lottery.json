    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc4700fb-175c-3848-ac7c-8c9103aad366",
                    "sense_definition": "A game of chance in which people buy numbered tickets to win prizes.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dave plays the <w>lottery</w> almost every week, but always picks different numbers.",
                            "target_lang_translations": {
                                "ru": "Дейв играет в <w>лотерею</w> почти каждую неделю, но всегда выбирает разные числа."
                            }
                        },
                        {
                            "text": "If I ever win the <w>lottery</w>, I will buy a big house by the sea.",
                            "target_lang_translations": {
                                "ru": "Если я когда-нибудь выиграю в <w>лотерею</w>, я куплю большой дом у моря."
                            }
                        }
                    ],
                    "synonyms": [
                        "raffle",
                        "lotto",
                        "sweepstake"
                    ],
                    "common_phrases": [
                        "win the lottery",
                        "play the lottery",
                        "lottery ticket",
                        "national lottery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Азартная игра, в которой люди покупают билеты с номерами, чтобы выиграть призы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лотерея",
                                "target_lang_sense_clarification": "розыгрыш призов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9f138e6-e04e-32ab-86fc-bd91e6349c91",
                    "sense_definition": "A situation or event whose outcome is determined by luck rather than skill or planning.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is a bit of a <w>lottery</w> whether we will get a good table in that diner.",
                            "target_lang_translations": {
                                "ru": "Получим ли мы хороший столик в той закусочной — это своего рода <w>лотерея</w>."
                            }
                        },
                        {
                            "text": "Finding a parking space in the city center is often a complete <w>lottery</w>.",
                            "target_lang_translations": {
                                "ru": "Поиск парковочного места в центре города — это часто полная <w>лотерея</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gamble",
                        "toss-up"
                    ],
                    "antonyms": [
                        "certainty",
                        "sure thing"
                    ],
                    "common_phrases": [
                        "a bit of a lottery",
                        "genetic lottery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация или событие, исход которого зависит от удачи, а не от навыков или планирования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лотерея",
                                "target_lang_sense_clarification": "дело случая"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коренастый",
                                "target_lang_sense_clarification": "крепкого сложения"
                            },
                            {
                                "target_lang_word": "приземистый",
                                "target_lang_sense_clarification": "низкий и широкий"
                            }
                        ]
                    }
                }
            ]
        }
    ]

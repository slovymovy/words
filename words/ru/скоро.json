                    "sense_id": "bc3216ee-f2e7-3b7e-b494-b89716bac112",
                    "sense_definition": "Через небольшой промежуток времени; в ближайшем будущем.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_future",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Скоро</w> придёт поезд и мы поедем домой.",
                            "target_lang_translations": {
                                "en": "The train will arrive <w>soon</w> and we will go home."
                            }
                        },
                        {
                            "text": "Мы очень <w>скоро</w> увидимся с нашими друзьями.",
                            "target_lang_translations": {
                                "en": "We will see our friends very <w>soon</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "вскоре"
                    ],
                    "antonyms": [
                        "нескоро"
                    ],
                    "common_phrases": [
                        "очень скоро",
                        "совсем скоро",
                        "как скоро"
                    ],
                    "target_lang_definitions": {
                        "en": "After a short period of time; in the near future."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "soon",
                                "target_lang_sense_clarification": "in the near future"
                            }
                        ]
                    }
                },
                {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2cf81ade-291e-3c33-83a6-389ed8093a8d",
                    "sense_definition": "Gruba, wzorzysta tkanina służąca do przykrywania podłogi, pełniąca funkcję ozdobną i izolacyjną.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "floor_covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W salonie przed kominkiem leży miękki, wełniany <w>dywan</w>.",
                            "target_lang_translations": {
                                "ru": "В гостиной перед камином лежит мягкий шерстяной <w>ковёр</w>."
                            }
                        },
                        {
                            "text": "Muszę wytrzepać <w>dywan</w>, ponieważ zebrało się w nim dużo kurzu.",
                            "target_lang_translations": {
                                "ru": "Мне нужно выбить <w>ковёр</w>, так как в нём скопилось много пыли."
                            }
                        }
                    ],
                    "synonyms": [
                        "kobierzec"
                    ],
                    "common_phrases": [
                        "latający dywan",
                        "czerwony dywan",
                        "zamiatać pod dywan",
                        "trzepać dywan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Толстая узорчатая ткань, служащая для покрытия пола, выполняющая декоративную и изоляционную функцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ковёр",
                                "target_lang_sense_clarification": "покрытие для пола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d06807e4-8013-3bb8-a0a5-52df8e638c8c",
                    "sense_definition": "Ozdobna tkanina przeznaczona do zawieszenia na ścianie jako dekoracja.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "floor_covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na ścianie w korytarzu wisiał stary, perski <w>dywan</w>.",
                            "target_lang_translations": {
                                "ru": "На стене в коридоре висел старый персидский <w>ковёр</w>."
                            }
                        },
                        {
                            "text": "Ten zabytkowy <w>dywan</w> jest zbyt cenny, by kłaść go na podłodze.",
                            "target_lang_translations": {
                                "ru": "Этот старинный <w>ковёр</w> слишком ценен, чтобы класть его на пол."
                            }
                        }
                    ],
                    "synonyms": [
                        "kobierzec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративная ткань, предназначенная для подвешивания на стену в качестве украшения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ковёр",
                                "target_lang_sense_clarification": "настенное украшение"
                            },
                            {
                                "target_lang_word": "гобелен",
                                "target_lang_sense_clarification": "тканая картина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5aa569c1-f6ca-3499-9177-84af1bafe9ad",
                    "sense_definition": "Gruba warstwa czegoś, co pokrywa płasko dużą powierzchnię.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiosną całą łąkę pokrywał kolorowy <w>dywan</w> kwiatów.",
                            "target_lang_translations": {
                                "ru": "Весной весь луг покрывал разноцветный <w>ковёр</w> цветов."
                            }
                        },
                        {
                            "text": "Jesienią w parku leży złoty <w>dywan</w> z opadłych liści.",
                            "target_lang_translations": {
                                "ru": "Осенью в парке лежит золотой <w>ковёр</w> из опавших листьев."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dywan kwiatów",
                        "dywan liści",
                        "dywan śniegu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Толстый слой чего-либо, покрывающий большую поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ковёр",
                                "target_lang_sense_clarification": "сплошной покров"
                            },
                            {
                                "target_lang_word": "покров",
                                "target_lang_sense_clarification": "слой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61ac4977-bc08-3b81-9ff2-c3d1f17514c0",
                    "sense_definition": "Rada państwowa lub urząd dworski w dawnych krajach muzułmańskich, zwłaszcza w Imperium Osmańskim.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_council",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sułtan zwołał <w>dywan</w>, aby omówić strategię nadchodzącej wojny.",
                            "target_lang_translations": {
                                "ru": "Султан созвал <w>диван</w>, чтобы обсудить стратегию предстоящей войны."
                            }
                        },
                        {
                            "text": "Decyzje podejmowane przez <w>dywan</w> miały ogromny wpływ na politykę imperium.",
                            "target_lang_translations": {
                                "ru": "Решения, принимаемые <w>диваном</w>, имели огромное влияние на политику империи."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historyczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Государственный совет или придворное ведомство в бывших мусульманских странах, особенно в Османской империи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "диван",
                                "target_lang_sense_clarification": "историч. совет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed07416a-6082-3fc8-a0d0-d164970c3611",
                    "sense_definition": "Zbiór utworów poetyckich jednego autora w literaturze Bliskiego Wschodu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "work_of_art_name",
                    "examples": [
                        {
                            "text": "Student literatury analizował klasyczny <w>dywan</w> perskiego poety.",
                            "target_lang_translations": {
                                "ru": "Студент, изучающий литературу, анализировал классический <w>диван</w> персидского поэта."
                            }
                        },
                        {
                            "text": "Ten <w>dywan</w> zawiera najpiękniejsze wiersze o miłości i winie.",
                            "target_lang_translations": {
                                "ru": "Этот <w>диван</w> содержит прекраснейшие стихи о любви и вине."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сборник поэтических произведений одного автора в литературе Ближнего Востока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "диван",
                                "target_lang_sense_clarification": "литературный сборник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f72b855-09b9-34b7-ac92-ab17fc3a262f",
                    "sense_definition": "Rodzaj mebla do siedzenia lub spania; wersalka.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Goście usiedli na wygodnym <w>dywanie</w> w gościnnym pokoju.",
                            "target_lang_translations": {
                                "ru": "Гости сели на удобный <w>диван</w> в гостиной."
                            }
                        },
                        {
                            "text": "Po ciężkim dniu pracy lubię położyć się na <w>dywanie</w>.",
                            "target_lang_translations": {
                                "ru": "После тяжелого рабочего дня я люблю прилечь на <w>диван</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "regionalny (Bukowina)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид мебели для сидения или сна; диван-кровать."
                    },
                    "translations": {

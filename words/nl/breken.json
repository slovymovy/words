    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1dcec8bc-f613-31bc-9aab-56ff365668d5",
                    "sense_definition": "Iets met geweld in stukken verdelen of kapotmaken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_breaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft per ongeluk de dure vaas <w>gebroken</w>.",
                            "target_lang_translations": {
                                "en": "He accidentally <w>broke</w> the expensive vase.",
                                "ru": "Он случайно <w>разбил</w> дорогую вазу."
                            }
                        },
                        {
                            "text": "De harde wind <w>brak</w> de takken van de bomen.",
                            "target_lang_translations": {
                                "en": "The strong wind <w>broke</w> the branches off the trees.",
                                "ru": "Сильный ветер <w>сломал</w> ветки деревьев."
                            }
                        }
                    ],
                    "synonyms": [
                        "kapot maken",
                        "stukslaan",
                        "verbrijzelen"
                    ],
                    "antonyms": [
                        "maken",
                        "repareren"
                    ],
                    "common_phrases": [
                        "een been breken",
                        "in stukken breken",
                        "het brood breken"
                    ],
                    "target_lang_definitions": {
                        "en": "To divide something into pieces with force or destroy it.",
                        "ru": "Разделять что-либо на части силой или приводить в негодность."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "to separate into pieces"
                            },
                            {
                                "target_lang_word": "smash",
                                "target_lang_sense_clarification": "to break violently"
                            },
                            {
                                "target_lang_word": "shatter",
                                "target_lang_sense_clarification": "to break into many pieces"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ломать",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "разбивать",
                                "target_lang_sense_clarification": "о хрупких предметах, стекле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b21af28-a509-3897-8764-61aa596c121d",
                    "sense_definition": "In stukken vallen of kapotgaan.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_breaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het glas <w>brak</w> toen het op de stenen vloer viel.",
                            "target_lang_translations": {
                                "en": "The glass <w>broke</w> when it fell on the stone floor.",
                                "ru": "Стакан <w>разбился</w>, когда упал на каменный пол."
                            }
                        },
                        {
                            "text": "Door de hitte is de ruit plotseling <w>gebroken</w>.",
                            "target_lang_translations": {
                                "en": "Due to the heat, the window suddenly <w>broke</w>.",
                                "ru": "Из-за жары оконное стекло внезапно <w>треснуло</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kapot maken",
                        "stukslaan"
                    ],
                    "antonyms": [
                        "helen"
                    ],
                    "common_phrases": [
                        "het ijs breekt",
                        "de spiegel breekt"
                    ],
                    "target_lang_definitions": {
                        "en": "To fall into pieces or become broken.",
                        "ru": "Разваливаться на части или приходить в негодность."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "to fall into pieces"
                            },
                            {
                                "target_lang_word": "shatter",
                                "target_lang_sense_clarification": "to break suddenly into many pieces"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "разбиваться",
                                "target_lang_sense_clarification": "о хрупких предметах"
                            },
                            {
                                "target_lang_word": "ломаться",
                                "target_lang_sense_clarification": "общий смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa83c187-6434-34f1-b6ed-5e8a62429d3a",
                    "sense_definition": "Een barrière, stilte, weerstand of bestaande toestand met kracht beëindigen of forceren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_breaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De atleet probeert vandaag het wereldrecord te <w>breken</w>.",
                            "target_lang_translations": {
                                "en": "The athlete is trying to <w>break</w> the world record today.",
                                "ru": "Атлет пытается сегодня <w>побить</w> мировой рекорд."
                            }
                        },
                        {
                            "text": "Iemand moet tijdens de vergadering de pijnlijke stilte <w>breken</w>.",
                            "target_lang_translations": {
                                "en": "Someone needs to <w>break</w> the painful silence during the meeting.",
                                "ru": "Кто-то должен <w>прервать</w> тягостное молчание во время собрания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een record breken",
                        "de stilte breken",
                        "het verzet breken",
                        "een belofte breken"
                    ],
                    "target_lang_definitions": {
                        "en": "To forcefully end or overcome a barrier, silence, resistance, or existing state.",
                        "ru": "Преодолевать барьер, нарушать тишину или обещание, превосходить достижение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "a record, silence, or promise"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "о рекорде"
                            },
                            {
                                "target_lang_word": "нарушать",
                                "target_lang_sense_clarification": "о тишине, обещании, правилах"
                            },
                            {
                                "target_lang_word": "прерывать",
                                "target_lang_sense_clarification": "о молчании или состоянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e35977c5-9339-3d80-beb8-06a32fd800d8",
                    "sense_definition": "De controle over emoties verliezen en instorten of heel erg verdrietig worden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotional_breaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <w>brak</w> toen ze het slechte nieuws over haar vader hoorde.",
                            "target_lang_translations": {
                                "en": "She <w>broke down</w> when she heard the bad news about her father.",
                                "ru": "Она <w>сломалась</w>, когда услышала плохие новости об отце."
                            }
                        },
                        {
                            "text": "Hij probeerde sterk te blijven, maar uiteindelijk <w>brak</w> hij toch.",
                            "target_lang_translations": {
                                "en": "He tried to stay strong, but eventually he <w>broke</w>.",
                                "ru": "Он пытался оставаться сильным, но в конце концов всё же <w>сломался</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mijn hart breekt",
                        "breken van verdriet"
                    ],
                    "target_lang_definitions": {
                        "en": "To lose control of emotions and collapse or become very sad.",
                        "ru": "Терять контроль над эмоциями, впадать в отчаяние или сильную печаль."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break down",
                                "target_lang_sense_clarification": "emotionally"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "to lose spirit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сломаться",
                                "target_lang_sense_clarification": "психологически"
                            },
                            {
                                "target_lang_word": "не выдержать",
                                "target_lang_sense_clarification": "потерять самообладание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08fff680-5371-3c75-9fde-0235a831943b",
                    "sense_definition": "Het plotseling veranderen of overslaan van de toonhoogte van de stem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "voice_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de puberteit <w>breekt</w> de stem van de meeste jongens.",
                            "target_lang_translations": {
                                "en": "In puberty, most boys' voices <w>break</w>.",
                                "ru": "В подростковом возрасте у большинства мальчиков <w>ломается</w> голос."
                            }
                        },
                        {
                            "text": "Zijn stem <w>brak</w> van emotie toen hij de toespraak hield.",
                            "target_lang_translations": {
                                "en": "His voice <w>broke</w> with emotion when he gave the speech.",
                                "ru": "Его голос <w>срывался</w> от волнения, когда он произносил речь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zijn stem breekt"
                    ],
                    "target_lang_definitions": {
                        "en": "The sudden change or skipping of the pitch of the voice.",
                        "ru": "Внезапное изменение или срыв высоты голоса."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "of a voice"
                            },
                            {
                                "target_lang_word": "crack",
                                "target_lang_sense_clarification": "to change pitch suddenly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ломаться",
                                "target_lang_sense_clarification": "о голосе в переходном возрасте"
                            },
                            {
                                "target_lang_word": "срываться",
                                "target_lang_sense_clarification": "о голосе из-за эмоций"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f191ff3d-5e95-389f-b656-ca07ef9a3978",
                    "sense_definition": "De richting van stralen, zoals licht, veranderen bij de overgang naar een andere stof.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physics_refraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het water <w>breekt</w> het licht, waardoor het rietje krom lijkt.",
                            "target_lang_translations": {
                                "en": "The water <w>refracts</w> the light, making the straw look bent.",
                                "ru": "Вода <w>преломляет</w> свет, из-за чего соломинка кажется изогнутой."
                            }
                        },
                        {
                            "text": "Een prisma kan wit licht <w>breken</w> in alle kleuren van de regenboog.",
                            "target_lang_translations": {
                                "en": "A prism can <w>break</w> white light into all the colors of the rainbow.",
                                "ru": "Призма может <w>преломлять</w> белый свет во все цвета радуги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "licht breken",
                        "stralen breken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To change the direction of rays, such as light, when passing into another substance.",
                        "ru": "Изменять направление лучей (например, света) при переходе в другую среду."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "refract",
                                "target_lang_sense_clarification": "physics term"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "light"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "преломлять",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
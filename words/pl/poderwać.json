    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f28cefc0-af15-3b88-aad8-83728b961324",
                    "sense_definition": "Nawiązać z kimś znajomość w celach romantycznych lub seksualnych, często używając uroku osobistego.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek próbował <poderwać> dziewczynę, którą spotkał w kawiarni.",
                            "target_lang_translations": {
                                "ru": "Марек пытался <закадрить> девушку, которую встретил в кафе."
                            }
                        },
                        {
                            "text": "Nie jest łatwo <poderwać> kogoś, gdy jest się nieśmiałym.",
                            "target_lang_translations": {
                                "ru": "Нелегко <подцепить> кого-то, когда ты застенчив."
                            }
                        }
                    ],
                    "synonyms": [
                        "uwieść",
                        "zdobyć"
                    ],
                    "common_phrases": [
                        "poderwać dziewczynę",
                        "poderwać chłopaka",
                        "próbować poderwać"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Завязать знакомство с кем-либо с целью флирта или романтических отношений, часто используя личное обаяние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закадрить",
                                "target_lang_sense_clarification": "разг., соблазнить или увлечь кого-то"
                            },
                            {
                                "target_lang_word": "подцепить",
                                "target_lang_sense_clarification": "разг., познакомиться с кем-то для отношений"
                            },
                            {
                                "target_lang_word": "склеить",
                                "target_lang_sense_clarification": "жарг., познакомиться с целью флирта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6ea4959-5291-32f3-b284-6a5fc7ee1e44",
                    "sense_definition": "Gwałtownym, energicznym ruchem unieść coś w górę lub sprawić, że maszyna wzbije się w powietrze.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pilot w ostatniej chwili zdołał <poderwać> samolot i uniknąć zderzenia.",
                            "target_lang_translations": {
                                "ru": "Пилот в последний момент сумел <поднять> самолет <в воздух> и избежать столкновения."
                            }
                        },
                        {
                            "text": "Na dźwięk niespodziewanego hałasu pies gwałtownie <poderwał> łeb.",
                            "target_lang_translations": {
                                "ru": "При звуке неожиданного шума собака резко <вскинула> голову."
                            }
                        }
                    ],
                    "synonyms": [
                        "unieść",
                        "podnieść"
                    ],
                    "antonyms": [
                        "opuścić"
                    ],
                    "common_phrases": [
                        "poderwać samolot",
                        "poderwać głowę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резким движением поднять что-то вверх или заставить взлететь (о самолете или птице)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резко поднять",
                                "target_lang_sense_clarification": "быстрым движением переместить вверх"
                            },
                            {
                                "target_lang_word": "вскинуть",
                                "target_lang_sense_clarification": "о голове или глазах"
                            },
                            {
                                "target_lang_word": "поднять в воздух",
                                "target_lang_sense_clarification": "о самолете или птицах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35dbbba4-a854-3e62-a4b3-1c7a85a1548b",
                    "sense_definition": "Zmusić kogoś do szybkiego wstania z miejsca lub pobudzić grupę ludzi do nagłego działania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głośny alarm <poderwał> wszystkich mieszkańców bloku ze snu.",
                            "target_lang_translations": {
                                "ru": "Громкая сигнализация <подняла> всех жильцов дома ото сна."
                            }
                        },
                        {
                            "text": "Dowódca jednym rozkazem <poderwał> żołnierzy do ataku.",
                            "target_lang_translations": {
                                "ru": "Командир одним приказом <поднял> солдат в атаку."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmobilizować",
                        "zerwać"
                    ],
                    "common_phrases": [
                        "poderwać do walki",
                        "poderwać na równe nogi",
                        "poderwać z łóżka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то быстро встать или побудить группу людей к немедленному действию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поднять",
                                "target_lang_sense_clarification": "заставить встать или действовать"
                            },
                            {
                                "target_lang_word": "поднять на ноги",
                                "target_lang_sense_clarification": "фразеологизм, побудить к активности"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "26b7e9f8-154d-3792-a86d-95330ef7f5a8",
                    "sense_definition": "Jedna z dwóch połówek kuli, najczęściej w odniesieniu do kuli ziemskiej lub mózgu; <w>półkula</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geo_anat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lewa <w>hemisfera</w> mózgu odpowiada zazwyczaj za zdolności językowe i analityczne.",
                            "target_lang_translations": {
                                "ru": "Левое <w>полушарие</w> мозга обычно отвечает за языковые и аналитические способности."
                            }
                        },
                        {
                            "text": "Klimat na południowej <w>hemisferze</w> różni się znacząco od tego na północy.",
                            "target_lang_translations": {
                                "ru": "Климат в южном <w>полушарии</w> значительно отличается от климата на севере."
                            }
                        }
                    ],
                    "synonyms": [
                        "półkula"
                    ],
                    "common_phrases": [
                        "hemisfera mózgu",
                        "hemisfera zachodnia",
                        "hemisfera wschodnia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из двух половин шара, обычно в контексте земного шара или мозга; полушарие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полушарие",
                                "target_lang_sense_clarification": "мозга или Земли"
                            },
                            {
                                "target_lang_word": "гемисфера",
                                "target_lang_sense_clarification": "научный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61995ee7-8a3c-3ec4-b978-7fd1d54c478c",
                    "sense_definition": "Każda z dwóch części sfery niebieskiej rozdzielonych płaszczyzną horyzontu lub równika.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "astro",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Astronomowie dokładnie zbadali gwiazdy widoczne na północnej <w>hemisferze</w> nieba.",
                            "target_lang_translations": {
                                "ru": "Астрономы тщательно изучили звезды, видимые в северном <w>полушарии</w> неба."
                            }
                        },
                        {
                            "text": "Obserwacja tej komety jest możliwa tylko na południowej <w>hemisferze</w>.",
                            "target_lang_translations": {
                                "ru": "Наблюдение этой кометы возможно только в южном <w>полушарии</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "półkula niebieska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Каждая из двух частей небесной сферы, разделенных плоскостью горизонта или экватора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полушарие",
                                "target_lang_sense_clarification": "небесной сферы"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "3e5e092d-fb51-317e-81c0-74b4432c6784",
                    "sense_definition": "Употребляется для обозначения субъекта, который имеет что-либо или обладает чем-либо (соответствует глаголу «иметь»).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<У> меня есть интересная книга для тебя.",
                            "target_lang_translations": {
                                "en": "I <w>have</w> an interesting book for you."
                            }
                        },
                        {
                            "text": "Скажите, <у> вас есть свободное время сегодня вечером?",
                            "target_lang_translations": {
                                "en": "Tell me, do you <w>have</w> free time this evening?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "у меня есть",
                        "у тебя есть",
                        "у нас есть",
                        "у вас есть"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to denote a subject who has something or possesses something (corresponds to the verb \"to have\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "functional translation indicating possession"
                            },
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "literal translation, rarely used in modern English for possession"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ab5828d-11c2-3ed6-91d9-afac5d355b63",
                    "sense_definition": "Указывает на нахождение в непосредственной близости, рядом с кем-либо или чем-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы договорились встретиться <у> входа в парк.",
                            "target_lang_translations": {
                                "en": "We agreed to meet <w>at</w> the entrance to the park."
                            }
                        },
                        {
                            "text": "Моя бабушка живёт в маленьком домике <у> моря.",
                            "target_lang_translations": {
                                "en": "My grandmother lives in a small house <w>by</w> the sea."
                            }
                        }
                    ],
                    "synonyms": [
                        "около",
                        "возле",
                        "рядом",
                        "близ",
                        "недалеко"
                    ],
                    "common_phrases": [
                        "у окна",
                        "у двери",
                        "у моря",
                        "у входа"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates being in immediate proximity, next to someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "indicating specific location"
                            },
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "indicating proximity"
                            },
                            {
                                "target_lang_word": "near",
                                "target_lang_sense_clarification": "indicating close distance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0da8c6ad-2c4e-3cc9-bfc1-c4b96c129e6f",
                    "sense_definition": "Указывает на принадлежность части целому, свойства предмету или физического состояния субъекту (часто используется с частями тела).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "attribute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вчера <у> меня сильно болела голова.",
                            "target_lang_translations": {
                                "en": "Yesterday <w>my</w> head hurt badly."
                            }
                        },
                        {
                            "text": "Одна ножка <у> этого старого стула сломалась.",
                            "target_lang_translations": {
                                "en": "One leg <w>of</w> this old chair broke."
                            }
                        }
                    ],
                    "common_phrases": [
                        "у меня болит",
                        "ножка у стула",
                        "глаза у кошки"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates belonging of a part to a whole, properties to an object, or physical state to a subject."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of",
                                "target_lang_sense_clarification": "indicating a part belonging to a whole"
                            },
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "functional translation for physical states"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86060f97-ab39-3dd1-abc3-025f0c23a11c",
                    "sense_definition": "Указывает на лицо или источник, от которого исходит действие, информация или объект (отобрать, узнать, спросить).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу попросить <у> тебя прощения за опоздание.",
                            "target_lang_translations": {
                                "en": "I want to ask forgiveness <w>from</w> you for being late."
                            }
                        },
                        {
                            "text": "Студент решил узнать <у> друга домашнее задание.",
                            "target_lang_translations": {
                                "en": "The student decided to find out the homework <w>from</w> a friend."
                            }
                        }
                    ],
                    "common_phrases": [
                        "узнать у друзей",
                        "спросить у учителя",
                        "взять у брата"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the person or source from which an action, information, or object proceeds."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from",
                                "target_lang_sense_clarification": "indicating source or origin"
                            },
                            {
                                "target_lang_word": "of",
                                "target_lang_sense_clarification": "used with verbs of asking"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "04ed35f2-be79-34fb-9b9e-dbf781bcc732",
                    "sense_definition": "Выражает сильные эмоции: страх, удивление, угрозу или укоризну.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<У>, какой ты хитрый, всё предусмотрел!",
                            "target_lang_translations": {
                                "en": "<w>Oh</w>, how cunning you are, you foresaw everything!"
                            }
                        },
                        {
                            "text": "<У>, как холодно сегодня на улице!",
                            "target_lang_translations": {
                                "en": "<w>Ooh</w>, how cold it is outside today!"
                            }
                        }
                    ],
                    "synonyms": [
                        "о",
                        "ах"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное выражение эмоций"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Expresses strong emotions: fear, surprise, threat, or reproach."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oh",
                                "target_lang_sense_clarification": "expression of surprise or emotion"
                            },
                            {
                                "target_lang_word": "ooh",
                                "target_lang_sense_clarification": "expression of sensation or emotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cb73421-f6bb-38d7-a125-d5ae4548a32e",
                    "sense_definition": "Звукоподражание вою волка, собаки или завыванию ветра.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ветер за окном выл протяжно и грустно: <у>-у-у.",
                            "target_lang_translations": {
                                "en": "The wind outside the window howled long and sadly: <w>ooo</w>-ooo-ooo."
                            }
                        },
                        {
                            "text": "Голодный волк в лесу затянул своё <у>-у-у.",
                            "target_lang_translations": {
                                "en": "A hungry wolf in the forest began his <w>howl</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Onomatopoeia for the howling of a wolf, dog, or wind."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "howl",
                                "target_lang_sense_clarification": "description of the sound"
                            },
                            {
                                "target_lang_word": "ooo",
                                "target_lang_sense_clarification": "onomatopoeic representation"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a3dd0b2c-c3c7-3105-afdf-f171a4c76a36",
                    "sense_definition": "Название двадцать первой буквы русского алфавита.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "letter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Слово «улитка» начинается на букву <у>.",
                            "target_lang_translations": {
                                "en": "The word \"ulitka\" (snail) starts with the letter <w>U</w>."
                            }
                        },
                        {
                            "text": "Маленький ребёнок учился писать букву <у>.",
                            "target_lang_translations": {
                                "en": "The small child was learning to write the letter <w>U</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "буква у",
                        "прописная у",
                        "строчная у"
                    ],
                    "target_lang_definitions": {
                        "en": "Name of the twenty-first letter of the Russian alphabet."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "U",
                                "target_lang_sense_clarification": "the Cyrillic letter U"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f40dc0d-7719-3bae-af06-448cb0647a2b",
                    "sense_definition": "Один из крупнейших диалектов или языков китайской группы.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "language",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Лингвисты изучают особенности диалекта <у>.",
                            "target_lang_translations": {
                                "en": "Linguists study the features of the <w>Wu</w> dialect."
                            }
                        },
                        {
                            "text": "В этом регионе многие говорят на языке <у>.",
                            "target_lang_translations": {
                                "en": "In this region, many speak the <w>Wu</w> language."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "One of the largest dialects or languages of the Chinese group."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Wu",
                                "target_lang_sense_clarification": "Chinese language group"
                            },
                            {
                                "target_lang_word": "Wu Chinese",
                                "target_lang_sense_clarification": "Chinese language group"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "42f9dbf3-a857-3261-8e49-ade7796f3cc0",
                    "sense_definition": "Распространённая китайская фамилия.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Господин <У> прибыл на конференцию вовремя.",
                            "target_lang_translations": {
                                "en": "Mr. <w>Wu</w> arrived at the conference on time."
                            }
                        },
                        {
                            "text": "Мы познакомились с коллегой по фамилии <У>.",
                            "target_lang_translations": {
                                "en": "We met a colleague with the surname <w>Wu</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "китайская фамилия"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Common Chinese surname."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Wu",
                                "target_lang_sense_clarification": "Chinese surname"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "60860913-5829-3eb3-8671-bb698da3b6a1",
                    "sense_definition": "Poświęcać swój czas, uwagę i energię jakiejś czynności, pasji lub osobie (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "devote_oneself",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wolnym czasie lubię <oddawać> się marzeniom o dalekich podróżach.",
                            "target_lang_translations": {
                                "ru": "В свободное время я люблю <предаваться> мечтам о далеких путешествиях."
                            }
                        },
                        {
                            "text": "Po przejściu na emeryturę dziadek zaczął całkowicie <oddawać> się swojemu hobby.",
                            "target_lang_translations": {
                                "ru": "После выхода на пенсию дедушка начал полностью <отдаваться> своему хобби."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oddawać się pracy",
                        "oddawać się marzeniom",
                        "oddawać się lekturze",
                        "oddawać się pasji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Посвящать свое время, внимание и энергию какому-либо занятию, страсти или человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предаваться",
                                "target_lang_sense_clarification": "посвящать себя (мечтам, размышлениям)"
                            },
                            {
                                "target_lang_word": "отдаваться",
                                "target_lang_sense_clarification": "уходить с головой в занятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a9d9ea1-38ea-3cbd-a76d-aba6013810e3",
                    "sense_definition": "Okazywać komuś szacunek, cześć lub uznanie poprzez gesty lub słowa (często w utartych zwrotach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "pay_respect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy zgromadzeni na placu chcieli <oddawać> hołd poległym żołnierzom.",
                            "target_lang_translations": {
                                "ru": "Все собравшиеся на площади хотели <отдать> дань уважения павшим солдатам."
                            }
                        },
                        {
                            "text": "Należy <oddawać> sprawiedliwość tym, którzy ciężko pracowali na ten sukces.",
                            "target_lang_translations": {
                                "ru": "Нужно <отдавать> должное тем, кто усердно работал ради этого успеха."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oddawać cześć",
                        "oddawać hołd",
                        "oddawać sprawiedliwość",
                        "oddawać honory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать кому-либо уважение, почтение или признание жестами или словами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдавать",
                                "target_lang_sense_clarification": "в выражениях: отдавать честь, дань уважения"
                            },
                            {
                                "target_lang_word": "возздавать",
                                "target_lang_sense_clarification": "выражать почтение или справедливость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c7d2ee2-7bfc-3bd7-b54b-0aaa6761ff31",
                    "sense_definition": "Wydalać z organizmu produkty przemiany materii (głównie mocz).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physiological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent ma problemy z nerkami i nie może prawidłowo <oddawać> moczu.",
                            "target_lang_translations": {
                                "ru": "У пациента проблемы с почками, и он не может нормально <испускать> мочу."
                            }
                        },
                        {
                            "text": "Lekarz zapytał go, jak często musi w nocy wstawać i <oddawać> mocz.",
                            "target_lang_translations": {
                                "ru": "Врач спросил его, как часто ему приходится вставать ночью и <испускать> мочу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oddawać mocz",
                        "oddawać stolec"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or medical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделять из организма продукты обмена веществ (в основном мочу)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выделять",
                                "target_lang_sense_clarification": "о физиологических процессах"
                            },
                            {
                                "target_lang_word": "испускать",
                                "target_lang_sense_clarification": "выводить из организма"
                            }
                        ]
                    }
                }
            ]
        }

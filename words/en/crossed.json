    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b1d8cd2e-afbb-303b-b004-a8221ea19c5e",
                    "sense_definition": "Simple past and past participle of <w>cross</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>crossed</w> the street carefully to get to the park.",
                            "target_lang_translations": {
                                "ru": "Мы осторожно <w>перешли</w> улицу, чтобы попасть в парк."
                            }
                        },
                        {
                            "text": "The thought never <w>crossed</w> my mind until you mentioned it.",
                            "target_lang_translations": {
                                "ru": "Эта мысль никогда не <w>приходила</w> мне в голову, пока ты не упомянул об этом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "crossed the line",
                        "crossed my mind",
                        "crossed paths"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола cross."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пересёк",
                                "target_lang_sense_clarification": "прошёл через что-либо"
                            },
                            {
                                "target_lang_word": "перешёл",
                                "target_lang_sense_clarification": "переместился на другую сторону"
                            },
                            {
                                "target_lang_word": "скрестил",
                                "target_lang_sense_clarification": "сложил крест-накрест"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1da0b46e-2bd3-3d9e-b044-0b527953d933",
                    "sense_definition": "Placed over one another, especially regarding limbs.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She sat comfortably in the armchair with <w>crossed</w> legs.",
                            "target_lang_translations": {
                                "ru": "Она удобно сидела в кресле со <w>скрещёнными</w> ногами."
                            }
                        },
                        {
                            "text": "He stood by the door with his arms <w>crossed</w>.",
                            "target_lang_translations": {
                                "ru": "Он стоял у двери со <w>скрещёнными</w> руками."
                            }
                        }
                    ],
                    "synonyms": [
                        "folded"
                    ],
                    "antonyms": [
                        "uncrossed",
                        "open"
                    ],
                    "common_phrases": [
                        "crossed legs",
                        "crossed arms",
                        "fingers crossed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положенные друг на друга (обычно о конечностях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрещённый",
                                "target_lang_sense_clarification": "о руках или ногах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdf89073-8e22-3715-808b-2ba5a8988407",
                    "sense_definition": "Marked with a line drawn through it, often to indicate cancellation or a specific banking restriction.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You cannot cash a <w>crossed</w> check directly at the counter.",
                            "target_lang_translations": {
                                "ru": "Вы не можете обналичить <w>кроссированный</w> чек непосредственно в кассе."
                            }
                        },
                        {
                            "text": "All the completed tasks were <w>crossed</w> out on the list.",
                            "target_lang_translations": {
                                "ru": "Все выполненные задачи в списке были <w>вычеркнуты</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cancelled"
                    ],
                    "common_phrases": [
                        "crossed check",
                        "crossed out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помеченный линией, проведённой через что-либо; в банковском деле — чек с ограничением по обналичиванию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроссированный",
                                "target_lang_sense_clarification": "банковский термин"
                            },
                            {
                                "target_lang_word": "зачёркнутый",
                                "target_lang_sense_clarification": "вычеркнутый из списка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c25fb5e-2ad2-3db3-aedc-75ca3af694fb",
                    "sense_definition": "Shaped like a cross; cruciate.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The botanist identified the plant by its <w>crossed</w> petals.",
                            "target_lang_translations": {
                                "ru": "Ботаник определил растение по его <w>крестообразным</w> лепесткам."
                            }
                        },
                        {
                            "text": "They found a <w>crossed</w> structure embedded in the ancient rock.",
                            "target_lang_translations": {
                                "ru": "Они обнаружили <w>крестообразную</w> структуру, вкрапленную в древнюю породу."
                            }
                        }
                    ],
                    "synonyms": [
                        "cruciate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий форму креста; крестообразный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крестообразный",
                                "target_lang_sense_clarification": "форма"

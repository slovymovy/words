    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e62afca6-ed78-3665-b3b8-4386f264a920",
                    "sense_definition": "A game played on a large open course where players hit a small hard ball into a series of holes using special clubs, trying to use as few strokes as possible.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sport_game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He plays <w>golf</w> with his colleagues every Sunday morning.",
                            "target_lang_translations": {
                                "ru": "Он играет в <w>гольф</w> с коллегами каждое воскресное утро."
                            }
                        },
                        {
                            "text": "We spent the afternoon at the <w>golf</w> course practicing our swing.",
                            "target_lang_translations": {
                                "ru": "Мы провели день на поле для <w>гольфа</w>, тренируя удары."
                            }
                        }
                    ],
                    "common_phrases": [
                        "play golf",
                        "golf course",
                        "golf club",
                        "golf ball",
                        "round of golf"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игра на большом открытом поле, в которой игроки ударами специальных клюшек загоняют маленький твердый мяч в серию лунок, стараясь сделать как можно меньше ударов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гольф",
                                "target_lang_sense_clarification": "спортивная игра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed92dd61-0698-3210-b57a-a9dab4db40e2",
                    "sense_definition": "The word assigned to the letter G in the NATO phonetic alphabet.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot confirmed the code by saying alpha, bravo, <w>golf</w>.",
                            "target_lang_translations": {
                                "ru": "Пилот подтвердил код, сказав: альфа, браво, <w>гольф</w>."
                            }
                        },
                        {
                            "text": "In the phonetic alphabet, <w>golf</w> stands for G.",
                            "target_lang_translations": {
                                "ru": "В фонетическом алфавите <w>гольф</w> обозначает букву G."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "alternative letter-case form of Golf"
                        }
                    ],
                    "target_lang_definitions": {

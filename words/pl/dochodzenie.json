            "senses": [
                {
                    "sense_id": "bf2c7df8-9c20-326c-a14d-82f8513b6107",
                    "sense_definition": "Ogół czynności urzędowych, prowadzonych zazwyczaj przez policję lub prokuraturę, mających na celu wyjaśnienie okoliczności przestępstwa i zebranie dowodów.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal_investigation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja natychmiast rozpoczęła szczegółowe <w>dochodzenie</w> w sprawie wczorajszego włamania.",
                            "target_lang_translations": {
                                "ru": "Полиция немедленно начала тщательное <w>расследование</w> вчерашнего взлома."
                            }
                        },
                        {
                            "text": "Mimo że prokuratura zdecydowała się umorzyć <w>dochodzenie</w>, dziennikarze nadal badali tę sprawę.",
                            "target_lang_translations": {
                                "ru": "Хотя прокуратура решила прекратить <w>следствие</w>, журналисты продолжали расследовать это дело."
                            }
                        }
                    ],
                    "synonyms": [
                        "śledztwo",
                        "postępowanie"
                    ],
                    "common_phrases": [
                        "prowadzić dochodzenie",
                        "wszcząć dochodzenie",
                        "umorzyć dochodzenie",
                        "dochodzenie w sprawie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Комплекс официальных действий, проводимых обычно полицией или прокуратурой, с целью выяснения обстоятельств преступления и сбора доказательств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расследование",
                                "target_lang_sense_clarification": "процесс выяснения обстоятельств"
                            },
                            {
                                "target_lang_word": "следствие",
                                "target_lang_sense_clarification": "юридическая процедура"
                            },
                            {
                                "target_lang_word": "дознание",
                                "target_lang_sense_clarification": "форма предварительного расследования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b252e8d7-24f8-3835-af61-762c94af4b6c",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>dochodzić</w>; proces dążenia do wyegzekwowania czegoś (np. praw, roszczeń) lub powracania do określonego stanu (np. zdrowia).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sądowe <w>dochodzenie</w> swoich praw w sporze z pracodawcą może trwać bardzo długo.",
                            "target_lang_translations": {
                                "ru": "Судебное <w>отстаивание</w> своих прав в споре с работодателем может длиться очень долго."
                            }
                        },
                        {
                            "text": "Jej powolne <w>dochodzenie</w> do siebie po ciężkiej chorobie było wielką ulgą dla rodziny.",
                            "target_lang_translations": {
                                "ru": "Её медленное <w>восстановление</w> после тяжёлой болезни стало огромным облегчением для семьи."
                            }
                        }
                    ],
                    "common_phrases": [

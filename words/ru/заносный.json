    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "01fca1ff-2014-39cc-bc40-9e0c286e82af",
                    "sense_definition": "Относящийся к снежному заносу; образованный наметенным ветром снегом или песком.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_drift",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На проселочной дороге лежал глубокий <w>заносный</w> снег, мешавший проезду машин.",
                            "target_lang_translations": {
                                "en": "Deep <w>drifted</w> snow lay on the country road, hindering the passage of cars."
                            }
                        },
                        {
                            "text": "Трактор медленно расчищал <w>заносные</w> сугробы у ворот гаража.",
                            "target_lang_translations": {
                                "en": "The tractor was slowly clearing the <w>drifted</w> snowbanks by the garage gates."
                            }
                        }
                    ],
                    "synonyms": [
                        "наносный"
                    ],
                    "common_phrases": [
                        "заносный снег"
                    ],
                    "target_lang_definitions": {
                        "en": "Formed by wind-blown snow or sand; relating to drifts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drifted",
                                "target_lang_sense_clarification": "accumulated by wind, usually referring to snow or sand"
                            },
                            {
                                "target_lang_word": "wind-blown",
                                "target_lang_sense_clarification": "carried or piled up by the wind"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15298b09-8272-3c5d-96f4-2ad5d144ebaa",
                    "sense_definition": "Попавший куда-либо извне, принесенный со стороны; чужеродный, не местный.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "foreign_origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это <w>заносное</w> растение быстро распространилось по всему саду, вытесняя местные виды.",
                            "target_lang_translations": {
                                "en": "This <w>introduced</w> plant spread quickly throughout the garden, displacing local species."
                            }
                        },
                        {
                            "text": "Старики считали этот обычай <w>заносным</w> и отказывались его соблюдать.",
                            "target_lang_translations": {
                                "en": "The old men considered this custom <w>alien</w> and refused to observe it."
                            }
                        }
                    ],
                    "synonyms": [
                        "чужеродный",
                        "пришлый",
                        "инородный",
                        "наносный"
                    ],
                    "antonyms": [
                        "местный",
                        "коренной"
                    ],
                    "common_phrases": [
                        "заносный вид",
                        "заносная инфекция"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "в общем значении; в биологии и медицине термин используется как специальный"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Brought from outside; foreign, not native."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "alien",
                                "target_lang_sense_clarification": "foreign origin, often regarding species or customs"
                            },
                            {
                                "target_lang_word": "introduced",
                                "target_lang_sense_clarification": "brought into a new environment (biology)"
                            },
                            {
                                "target_lang_word": "imported",
                                "target_lang_sense_clarification": "brought from another place"
                            }
                        ]
                    }
                }
            ]
        }
    ]

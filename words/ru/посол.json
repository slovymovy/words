            "senses": [
                {
                    "sense_id": "ab8d9e0d-8afb-3dd5-a138-8c944d7d3002",
                    "sense_definition": "Дипломатический представитель высшего ранга в иностранном государстве.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "representative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новый <w>посол</w> вручил верительные грамоты президенту.",
                            "target_lang_translations": {
                                "en": "The new <w>ambassador</w> presented his credentials to the president."
                            }
                        },
                        {
                            "text": "На официальном приёме присутствовал <w>посол</w> Франции.",
                            "target_lang_translations": {
                                "en": "The French <w>ambassador</w> attended the official reception."
                            }
                        }
                    ],
                    "synonyms": [
                        "амбассадор"
                    ],
                    "common_phrases": [
                        "чрезвычайный посол",
                        "посол мира",
                        "отозвать посла"
                    ],
                    "target_lang_definitions": {
                        "en": "A diplomatic representative of the highest rank sent to a foreign country."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ambassador",
                                "target_lang_sense_clarification": "highest ranking diplomat"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea2658f9-0c95-3df0-a1e0-a4e0c3dffe99",
                    "sense_definition": "Человек, которого отправили с каким-либо поручением или вестью; посланец.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "representative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Князь отправил <w>посла</w> к врагам с предложением мира.",
                            "target_lang_translations": {
                                "en": "The prince sent an <w>envoy</w> to the enemies with a peace offer."
                            }
                        },
                        {
                            "text": "В город прибыл <w>посол</w> с важным письмом от короля.",
                            "target_lang_translations": {
                                "en": "A <w>messenger</w> arrived in the city with an important letter from the king."
                            }

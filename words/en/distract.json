    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "afb35afd-45d6-345b-805d-36faf77975f8",
                    "sense_definition": "To divert the attention of someone; to draw focus away from something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "divert_attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not <w>distract</w> the driver while the bus is moving.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>отвлекайте</w> водителя во время движения автобуса."
                            }
                        },
                        {
                            "text": "The loud noise outside began to <w>distract</w> the students from their exam.",
                            "target_lang_translations": {
                                "ru": "Громкий шум с улицы начал <w>отвлекать</w> студентов от экзамена."
                            }
                        },
                        {
                            "text": "He tried to <w>distract</w> attention from his mistake by changing the subject.",
                            "target_lang_translations": {
                                "ru": "Он попытался <w>отвлечь</w> внимание от своей ошибки, сменив тему."
                            }
                        }
                    ],
                    "synonyms": [
                        "divert",
                        "sidetrack",
                        "preoccupy"
                    ],
                    "antonyms": [
                        "focus",
                        "concentrate"
                    ],
                    "common_phrases": [
                        "distract from",
                        "distract attention",
                        "easily distracted",
                        "distract someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отвлекать чье-либо внимание; переключать фокус с чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвлекать",
                                "target_lang_sense_clarification": "внимание (несов. вид)"
                            },
                            {
                                "target_lang_word": "отвлечь",
                                "target_lang_sense_clarification": "внимание (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44b5421d-849f-3cb5-9460-cc4797bc5fb6",
                    "sense_definition": "To make someone crazy or insane; to drive to mental confusion.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "make_insane",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The overwhelming grief threatened to <w>distract</w> his mind completely.",
                            "target_lang_translations": {
                                "ru": "Невыносимое горе грозило окончательно <w>лишить</w> его <w>рассудка</w>."
                            }
                        },
                        {
                            "text": "Such a terrible sequence of events would <w>distract</w> even the strongest soul.",
                            "target_lang_translations": {
                                "ru": "Такая ужасная череда событий <w>свела бы с ума</w> даже самую сильную душу."
                            }
                        }
                    ],
                    "synonyms": [
                        "madden",
                        "craze",
                        "derange"
                    ],
                    "antonyms": [
                        "calm",
                        "soothe"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Modern usage prefers 'drive to distraction' or 'madden'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сводить с ума; лишать рассудка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сводить с ума",
                                "target_lang_sense_clarification": "лишать разума"
                            },
                            {
                                "target_lang_word": "лишать рассудка",
                                "target_lang_sense_clarification": "приводить в замешательство"
                            }
                        ]
                    }
                }
            ]
        },
        {

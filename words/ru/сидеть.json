                    ],
                    "synonyms": [
                        "подходить",
                        "облегать",
                        "смотреться"
                    ],
                    "antonyms": [
                        "висеть"
                    ],
                    "common_phrases": [
                        "хорошо сидеть",
                        "плохо сидеть",
                        "сидеть как влитой"

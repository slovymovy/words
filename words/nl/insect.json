                {
                    "sense_id": "99e1c0de-b491-3085-bd42-c7fbb41a2599",
                    "sense_definition": "Een klein dier met zes poten en een lichaam dat in drie delen is verdeeld, vaak met vleugels.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer vliegen er veel <w>insecten</w> rond de lamp.",
                            "target_lang_translations": {
                                "en": "In the summer, many <w>insects</w> fly around the lamp.",
                                "ru": "Летом вокруг лампы летает много <w>насекомых</w>."
                            }
                        },
                        {
                            "text": "Het kabinet wil dat Nederlanders vaker <w>insecten</w> eten.",
                            "target_lang_translations": {
                                "en": "The cabinet wants Dutch people to eat <w>insects</w> more often.",
                                "ru": "Правительство хочет, чтобы голландцы чаще ели <w>насекомых</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kerfdier"
                    ],
                    "common_phrases": [
                        "vliegende insecten",
                        "door een insect gestoken",
                        "insecten bestrijden"
                    ],
                    "target_lang_definitions": {
                        "en": "A small animal with six legs and a body divided into three parts, often with wings.",
                        "ru": "Маленькое животное с шестью ногами и телом, разделенным на три части, часто с крыльями."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "insect",
                                "target_lang_sense_clarification": "small animal with six legs"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "насекомое",
                                "target_lang_sense_clarification": "класс беспозвоночных животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f346d0a-a5b5-3465-87d8-5640c49bb651",
                    "sense_definition": "Elk klein kruipend diertje, ook als het biologisch gezien geen insect is (zoals een spin).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mensen noemen een spin vaak ten onrechte een <w>insect</w>.",
                            "target_lang_translations": {
                                "en": "People often incorrectly call a spider an <w>insect</w>.",
                                "ru": "Люди часто ошибочно называют паука <w>насекомым</w>."
                            }
                        },
                        {
                            "text": "Haal dat vieze <w>insect</w> uit mijn kamer!",
                            "target_lang_translations": {
                                "en": "Get that dirty <w>bug</w> out of my room!",
                                "ru": "Убери это мерзкое <w>насекомое</w> из моей комнаты!"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "gebruikt voor elk klein beestje"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Any small crawling animal, even if not biologically an insect (such as a spider).",
                        "ru": "Любое маленькое ползающее существо, даже если биологически оно не является насекомым (например, паук)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bug",
                                "target_lang_sense_clarification": "informal, any small crawling creature"
                            },
                            {
                                "target_lang_word": "insect",
                                "target_lang_sense_clarification": "general term for small creatures"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "насекомое",
                                "target_lang_sense_clarification": "общее название для мелких ползающих существ"
                            },
                            {
                                "target_lang_word": "букашка",
                                "target_lang_sense_clarification": "разговорное: мелкое насекомое или жучок"
                            }
                        ]
                    }
                }
            ]
        }
    ]

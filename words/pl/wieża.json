    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0edf102f-1ed5-322e-b095-5793df85af98",
                    "sense_definition": "Wysoka budowla o małej płaszczyźnie podstawy w stosunku do wysokości, wolno stojąca lub będąca częścią innego budynku.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z wysokiej <w>wieży</w> roztaczał się piękny widok na całe miasto.",
                            "target_lang_translations": {
                                "ru": "С высокой <w>башни</w> открывался прекрасный вид на весь город."
                            }
                        },
                        {
                            "text": "Podobno o północy na <w>wieży</w> tego starego zamku pojawia się duch.",
                            "target_lang_translations": {
                                "ru": "Говорят, что в полночь на <w>башне</w> этого старого замка появляется призрак."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wieża Eiffla",
                        "wieża kościelna",
                        "wieża ciśnień",
                        "wieża kontrolna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокое сооружение с малым основанием относительно высоты, стоящее отдельно или являющееся частью другого здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "башня",
                                "target_lang_sense_clarification": "архитектурное сооружение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ffdd169-8d2e-36c4-a327-9847e8e5355a",
                    "sense_definition": "Figura w szachach mogąca poruszać się o dowolną liczbę pól w rzędach i kolumnach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "chess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jednoczesny ruch królem i <w>wieżą</w> nazywa się roszadą.",
                            "target_lang_translations": {
                                "ru": "Одновременный ход королём и <w>ладьёй</w> называется рокировкой."
                            }
                        },
                        {
                            "text": "W końcówce partii białe miały przewagę jednej <w>wieży</w>.",
                            "target_lang_translations": {
                                "ru": "В эндшпиле у белых было преимущество в одну <w>ладью</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ruch wieżą",
                        "końcówka wieżowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шахматная фигура, которая может ходить на любое число полей по горизонтали и вертикали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ладья",
                                "target_lang_sense_clarification": "шахматная фигура"
                            },
                            {
                                "target_lang_word": "тура",
                                "target_lang_sense_clarification": "разговорное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df22651d-b6b3-3b3f-8820-111dc3ca34c8",
                    "sense_definition": "Zestaw elektronicznego sprzętu odtwarzającego muzykę, złożony z ustawionych na sobie elementów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "electronics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiliśmy sobie nową <w>wieżę</w> stereo do salonu.",
                            "target_lang_translations": {
                                "ru": "Мы купили себе новый <w>музыкальный центр</w> в гостиную."
                            }
                        },
                        {
                            "text": "Głośna muzyka z <w>wieży</w> sąsiada nie dawała mi spać.",
                            "target_lang_translations": {
                                "ru": "Громкая музыка из <w>музыкального центра</w> соседа не давала мне спать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wieża stereo",
                        "wieża Hi-Fi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Комплект электронной аппаратуры для воспроизведения музыки, состоящий из установленных друг на друга элементов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "музыкальный центр",
                                "target_lang_sense_clarification": "аудиосистема"
                            },
                            {
                                "target_lang_word": "стереосистема",
                                "target_lang_sense_clarification": "аудиотехника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7492d588-0453-3c58-899f-213ea9c5d2d2",
                    "sense_definition": "Wystający, obrotowy element czołgu, okrętu lub fortyfikacji, w którym zamontowane jest uzbrojenie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czołg powoli obracał <w>wieżę</w> w poszukiwaniu celu.",
                            "target_lang_translations": {
                                "ru": "Танк медленно поворачивал <w>башню</w> в поисках цели."
                            }
                        },
                        {
                            "text": "Pocisk przeciwpancerny trafił prosto w <w>wieżę</w> pojazdu.",
                            "target_lang_translations": {
                                "ru": "Бронебойный снаряд попал прямо в <w>башню</w> машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "wieżyczka"
                    ],
                    "common_phrases": [
                        "wieża czołgu",
                        "wieża artyleryjska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступающая вращающаяся часть танка, корабля или укрепления, в которой установлено вооружение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "башня",

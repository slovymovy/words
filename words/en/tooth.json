    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "22413a9d-26f6-3850-8aa7-b32626db6b86",
                    "sense_definition": "A hard, white structure in the mouth of humans and animals used for biting and chewing.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "bio_tooth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baby cried all night because his first <w>tooth</w> was coming through.",
                            "target_lang_translations": {
                                "ru": "Ребенок плакал всю ночь, потому что у него прорезался первый <w>зуб</w>."
                            }
                        },
                        {
                            "text": "You should brush every <w>tooth</w> in your mouth twice a day.",
                            "target_lang_translations": {
                                "ru": "Тебе следует чистить каждый <w>зуб</w> во рту дважды в день."
                            }
                        }
                    ],
                    "synonyms": [
                        "toothypeg"
                    ],
                    "common_phrases": [
                        "brush your teeth",
                        "front tooth",
                        "wisdom tooth",
                        "loose tooth",
                        "milk tooth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердое костное образование в ротовой полости человека и животных, используемое для откусывания и пережевывания пищи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зуб",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9654aa31-4e81-3fac-b050-6f375bb26805",
                    "sense_definition": "A strong liking or desire, particularly for tasty food.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have a sweet <w>tooth</w> and cannot resist buying chocolate.",
                            "target_lang_translations": {
                                "ru": "У меня <w>пристрастие</w> к сладкому, и я не могу удержаться от покупки шоколада."
                            }
                        },
                        {
                            "text": "Her sweet <w>tooth</w> often leads her to the local bakery for treats.",
                            "target_lang_translations": {
                                "ru": "Ее <w>тяга</w> к сладкому часто приводит ее в местную пекарню за угощениями."
                            }
                        }
                    ],
                    "synonyms": [
                        "fondness",
                        "appetite",
                        "taste",
                        "palate"
                    ],
                    "common_phrases": [
                        "sweet tooth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное пристрастие или желание, особенно к вкусной еде (обычно используется в выражении «sweet tooth» — сладкоежка)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пристрастие",
                                "target_lang_sense_clarification": "склонность"
                            },
                            {
                                "target_lang_word": "тяга",
                                "target_lang_sense_clarification": "сильное желание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af4a9c5b-d944-3f1b-b8ff-db5e29554cd0",
                    "sense_definition": "A sharp, pointed projection on the cutting edge of a saw or similar tool.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mech_projection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "One <w>tooth</w> on the saw blade was bent, making it hard to cut straight.",
                            "target_lang_translations": {
                                "ru": "Один <w>зуб</w> на полотне пилы погнулся, из-за чего было трудно пилить ровно."
                            }
                        },
                        {
                            "text": "Be careful not to touch the sharp <w>tooth</w> of the metal cutter.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, не коснись острого <w>зубца</w> резака по металлу."
                            }
                        }
                    ],
                    "synonyms": [
                        "sawtooth"
                    ],
                    "common_phrases": [
                        "saw tooth",
                        "jagged tooth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Острый выступ на режущей кромке пилы или подобного инструмента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зуб",
                                "target_lang_sense_clarification": "пилы"
                            },
                            {
                                "target_lang_word": "зубец",
                                "target_lang_sense_clarification": "острый выступ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d710883d-4770-367c-ae96-e5f31076308d",
                    "sense_definition": "A projecting part on the edge of a gear or cog that connects with other parts to transmit motion.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mech_projection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic replaced the gear because a <w>tooth</w> had snapped off under pressure.",
                            "target_lang_translations": {
                                "ru": "Механик заменил шестерню, потому что под давлением отломился <w>зуб</w>."
                            }
                        },
                        {
                            "text": "Each <w>tooth</w> of the cog must be greased to ensure smooth operation.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>зуб</w> шестерни нужно смазать, чтобы обеспечить плавную работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gear tooth",
                        "cog tooth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступающая часть на краю шестерни или зубчатого колеса, которая сцепляется с другими частями для передачи движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зуб",
                                "target_lang_sense_clarification": "шестерни"
                            },
                            {
                                "target_lang_word": "зубец",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04441fd9-8665-332a-a25b-9fe6c82aa8c9",
                    "sense_definition": "The rough texture of a surface, such as paper or canvas, that allows paint or charcoal to adhere to it.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This drawing paper has enough <w>tooth</w> to hold the layers of pastel.",
                            "target_lang_translations": {
                                "ru": "У этой бумаги для рисования достаточная <w>зернистость</w>, чтобы удерживать слои пастели."
                            }
                        },
                        {
                            "text": "Artists prefer a canvas with some <w>tooth</w> when working with heavy oil paints.",
                            "target_lang_translations": {
                                "ru": "Художники предпочитают холст с некоторой <w>шероховатостью</w> при работе с тяжелыми масляными красками."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fine tooth",
                        "surface tooth"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шероховатая фактура поверхности, например бумаги или холста, обеспечивающая сцепление с краской или углем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зернистость",
                                "target_lang_sense_clarification": "бумаги"
                            },
                            {
                                "target_lang_word": "шероховатость",
                                "target_lang_sense_clarification": "фактура поверхности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b4b75a75-b1b1-34ae-838a-a3ba57b111b1",
                    "sense_definition": "To lock or mesh together, as the projections of gears do.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gears must <w>tooth</w> together perfectly for the clock to keep time.",
                            "target_lang_translations": {
                                "ru": "Шестерни должны идеально <w>сцепляться</w>, чтобы часы показывали точное время."
                            }
                        },
                        {
                            "text": "If the wheels do not <w>tooth</w> correctly, the machine will jam immediately.",
                            "target_lang_translations": {
                                "ru": "Если колеса не <w>сцепляются</w> правильно, машина немедленно заклинит."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сцепляться или входить в зацепление (о зубьях шестерен)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сцепляться",
                                "target_lang_sense_clarification": "о механизмах"
                            },
                            {
                                "target_lang_word": "зацепляться",
                                "target_lang_sense_clarification": "входить в паз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a02a628-a06f-3ccc-9127-1e8ba91973e7",
                    "sense_definition": "To cut a jagged edge or indentations into a material.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mason decided to <w>tooth</w> the stone block to help the mortar stick.",
                            "target_lang_translations": {
                                "ru": "Каменщик решил <w>зазубрить</w> каменный блок, чтобы раствор лучше держался."
                            }
                        },
                        {
                            "text": "You need to <w>tooth</w> the edge of the wood before joining the pieces.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>зазубрить</w> край дерева перед соединением деталей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать зубцы или зазубрины на материале."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зазубривать",
                                "target_lang_sense_clarification": "делать края неровными"
                            },
                            {
                                "target_lang_word": "нарезать зубья",
                                "target_lang_sense_clarification": "технический процесс"
                            }
                        ]
                    }
                }
            ]
        },

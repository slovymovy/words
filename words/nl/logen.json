            "senses": [
                {
                    "sense_id": "612f0b22-6a32-314b-ad1d-096ae5637dc7",
                    "sense_definition": "Vervoeging van <w>liegen</w>: onvoltooid verleden tijd meervoud.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>logen</w> niet toen we zeiden dat we thuis waren.",
                            "target_lang_translations": {
                                "en": "We didn't <w>lie</w> when we said we were home.",
                                "ru": "Мы не <w>лгали</w>, когда говорили, что были дома."
                            }
                        },
                        {
                            "text": "De verdachten <w>logen</w> tegen de politie over hun betrokkenheid.",
                            "target_lang_translations": {
                                "en": "The suspects <w>lied</w> to the police about their involvement.",
                                "ru": "Подозреваемые <w>лгали</w> полиции о своей причастности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wij logen",
                        "zij logen",
                        "jullie logen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Conjugation of 'liegen' (to lie): past tense plural.",
                        "ru": "Прошедшее время глагола liegen (лгать) во множественном числе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lied",
                                "target_lang_sense_clarification": "past tense of 'to lie' (tell untruths)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лгали",
                                "target_lang_sense_clarification": "прошедшее время мн.ч."
                            },
                            {
                                "target_lang_word": "врали",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bee920d8-cc64-3525-a22b-2a042f66eb56",
                    "sense_definition": "Materiaal behandelen met of langdurig blootstellen aan een bijtende vloeistof (loog) om het te reinigen of te bewerken.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De fabriek <w>loogde</w> het hout om de oude verflagen te verwijderen.",
                            "target_lang_translations": {
                                "en": "The factory <w>treated</w> the wood <w>with lye</w> to remove the old layers of paint.",
                                "ru": "Фабрика <w>выщелачивала</w> древесину, чтобы удалить старые слои краски."
                            }
                        },
                        {
                            "text": "Vroeger <w>logen</w> mensen hun textiel om het witter te maken.",
                            "target_lang_translations": {
                                "en": "In the past, people <w>treated</w> their textiles <w>with lye</w> to whiten them.",
                                "ru": "Раньше люди <w>бучили</w> свой текстиль, чтобы отбелить его."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To treat material with or expose for a long time to a caustic liquid (lye) to clean or process it.",
                        "ru": "Обрабатывать материал щелочью или подвергать длительному воздействию едкой жидкости для очистки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to treat with lye",
                                "target_lang_sense_clarification": "chemical treatment"
                            },
                            {
                                "target_lang_word": "to leach",
                                "target_lang_sense_clarification": "to wash or treat with a liquid"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выщелачивать",
                                "target_lang_sense_clarification": "химическая обработка"
                            },
                            {
                                "target_lang_word": "бучить",
                                "target_lang_sense_clarification": "обрабатывать белье щелочью"
                            },
                            {
                                "target_lang_word": "обрабатывать щелочью",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        },

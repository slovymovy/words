    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "beb0a73f-7eb9-37a7-8cf2-7551d4788711",
                    "sense_definition": "The official role, position, or responsibilities of a pastor.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "office_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old minister found his <w>pastorate</w> wearying and eventually decided to retire.",
                            "target_lang_translations": {
                                "ru": "Старый священнослужитель находил своё <w>пасторство</w> утомительным и в конце концов решил уйти на покой."
                            }
                        },
                        {
                            "text": "She felt called to enter the <w>pastorate</w> after years of serving as a lay leader.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала призвание к <w>пасторству</w> после многих лет служения в качестве лидера мирян."
                            }
                        }
                    ],
                    "synonyms": [
                        "pastorage"
                    ],
                    "common_phrases": [
                        "enter the pastorate",
                        "accept a pastorate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная роль, должность или обязанности пастора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пасторство",
                                "target_lang_sense_clarification": "должность или деятельность"
                            },
                            {
                                "target_lang_word": "пастырское служение",
                                "target_lang_sense_clarification": "выполнение обязанностей"
                            },
                            {
                                "target_lang_word": "должность пастора",
                                "target_lang_sense_clarification": "официальный пост"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f83d7cad-e4b5-3040-89c8-956d404f8062",
                    "sense_definition": "The specific period of time during which a pastor serves a congregation.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>pastorate</w> was marked by significant changes in church policy regarding community outreach.",
                            "target_lang_translations": {
                                "ru": "Его <w>пасторство</w> ознаменовалось значительными изменениями в церковной политике касательно работы с общественностью."
                            }
                        },
                        {
                            "text": "During her ten-year <w>pastorate</w>, the congregation doubled in size and built a new hall.",
                            "target_lang_translations": {
                                "ru": "За время её десятилетнего <w>пасторства</w> приход увеличился вдвое, и был построен новый зал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "during his pastorate",
                        "a long pastorate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный период времени, в течение которого пастор служит в приходе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пасторство",
                                "target_lang_sense_clarification": "период пребывания в должности"
                            },
                            {
                                "target_lang_word": "время служения",
                                "target_lang_sense_clarification": "срок службы пастора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8bc4c6c-f2a4-32a4-89c8-5e6035ac94ad",
                    "sense_definition": "A collective group or body consisting of multiple pastors.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>pastorate</w> voted on new bylaws at the annual denominational conference.",
                            "target_lang_translations": {
                                "ru": "<w>Пасторат</w> проголосовал за новый устав на ежегодной конфессиональной конференции."
                            }
                        },
                        {
                            "text": "Members of the local <w>pastorate</w> meet monthly to discuss shared community issues.",
                            "target_lang_translations": {
                                "ru": "Члены местного <w>пастората</w> встречаются ежемесячно для обсуждения общих проблем общины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the local pastorate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Коллективная группа или орган, состоящий из нескольких пасторов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пасторат",
                                "target_lang_sense_clarification": "коллективный орган"
                            },
                            {
                                "target_lang_word": "совет пасторов",
                                "target_lang_sense_clarification": "группа священнослужителей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
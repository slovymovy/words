            "senses": [
                {
                    "sense_id": "9e4cd7ec-32e5-3f59-a7a0-944fa8a4797a",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>stempelen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>stempel</w> elke dag mijn kaart als ik op het werk aankom.",
                            "target_lang_translations": {
                                "en": "I <w>stamp</w> my card every day when I arrive at work.",
                                "ru": "Я <w>штампую</w> свою карту каждый день, когда прихожу на работу."
                            }
                        },
                        {
                            "text": "Als ik de brieven <w>stempel</w>, leg ik ze daarna op de stapel.",
                            "target_lang_translations": {
                                "en": "When I <w>stamp</w> the letters, I put them on the pile afterwards.",
                                "ru": "Когда я <w>штампую</w> письма, я кладу их потом в стопку."
                            }
                        }

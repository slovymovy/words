    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6efca852-95c2-3f5b-978c-4cee5a05ea8e",
                    "sense_definition": "Przemieścić się w dół za pomocą pojazdu, nart, roweru lub urządzenia takiego jak winda.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy ostrożnie <zjechać> z tej stromej góry, ponieważ droga jest bardzo śliska.",
                            "target_lang_translations": {
                                "ru": "Мы должны осторожно <съехать> с этой крутой горы, так как дорога очень скользкая."
                            }
                        },
                        {
                            "text": "Czy moglibyśmy <zjechać> windą na parter, zamiast iść schodami?",
                            "target_lang_translations": {
                                "ru": "Могли бы мы <спуститься> на лифте на первый этаж вместо того, чтобы идти по лестнице?"
                            }
                        }
                    ],
                    "synonyms": [
                        "opuścić się",
                        "zsunąć się"
                    ],
                    "antonyms": [
                        "wjechać",
                        "wyjechać"
                    ],
                    "common_phrases": [
                        "zjechać na dół",
                        "zjechać windą",
                        "zjechać ze stoku",
                        "zjechać na sankach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спуститься вниз, используя транспортное средство, лыжи, велосипед или лифт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "съехать",
                                "target_lang_sense_clarification": "вниз по склону или поверхности"
                            },
                            {
                                "target_lang_word": "спуститься",
                                "target_lang_sense_clarification": "на лифте или вниз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1d7a54c-4c0a-3058-a48f-b83a8ae540f5",
                    "sense_definition": "Zmienić tor jazdy, kierując pojazd z głównej drogi na pobocze, parking lub w boczną drogę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "driving_maneuver",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca musiał natychmiast <zjechać> na pas awaryjny z powodu awarii silnika.",
                            "target_lang_translations": {
                                "ru": "Водитель был вынужден немедленно <съехать> на аварийную полосу из-за поломки двигателя."
                            }
                        },
                        {
                            "text": "Pamiętaj, żeby <zjechać> z autostrady na najbliższym węźle, inaczej nadłożymy drogi.",
                            "target_lang_translations": {
                                "ru": "Помни, что нужно <съехать> с автострады на ближайшей развязке, иначе мы сделаем крюк."
                            }
                        }
                    ],
                    "synonyms": [
                        "skręcić",
                        "zbaczać"
                    ],
                    "antonyms": [
                        "wjechać"
                    ],
                    "common_phrases": [
                        "zjechać na pobocze",
                        "zjechać z drogi",
                        "zjechać na bok",
                        "zjechać do bazy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить направление движения, съехав с главной дороги на обочину, парковку или второстепенную дорогу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "съехать",
                                "target_lang_sense_clarification": "с дороги на обочину"
                            },
                            {
                                "target_lang_word": "свернуть",
                                "target_lang_sense_clarification": "повернуть в сторону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc939d50-c33c-38e2-a0e7-76a631ffc1b8",
                    "sense_definition": "Przybyć do określonego miejsca docelowego, zazwyczaj po przebyciu dłuższej drogi (często o grupie osób).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na nasze wesele ma <zjechać> rodzina z najdalszych zakątków kraju.",
                            "target_lang_translations": {
                                "ru": "На нашу свадьбу должна <съехаться> семья из самых отдаленных уголков страны."
                            }
                        },
                        {
                            "text": "Po zakończeniu misji wszystkie patrole muszą <zjechać> do bazy przed zmrokiem.",
                            "target_lang_translations": {
                                "ru": "После завершения задания все патрули должны <вернуться> на базу до темноты."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyjechać",
                        "dotrzeć",
                        "ściągnąć"
                    ],
                    "antonyms": [
                        "wyjechać",
                        "rozjechać się"
                    ],
                    "common_phrases": [
                        "zjechać do domu",
                        "zjechać na święta",
                        "zjechać z całego świata"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибыть в место назначения, обычно проделав долгий путь (часто о группе людей)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "съехаться",
                                "target_lang_sense_clarification": "собраться вместе"
                            },
                            {
                                "target_lang_word": "прибыть",
                                "target_lang_sense_clarification": "приехать в место назначения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "251204d5-37ca-31e0-b48a-27df786794c8",
                    "sense_definition": "Zgromadzić się w jednym miejscu, przybywając z różnych stron (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy uczestnicy wycieczki powinni <zjechać> się na parkingu przed godziną ósmą.",
                            "target_lang_translations": {
                                "ru": "Все участники экскурсии должны <съехаться> на парковке до восьми часов."
                            }
                        },
                        {
                            "text": "Dawni znajomi ze szkoły postanowili <zjechać> się w rodzinnym mieście po wielu latach.",
                            "target_lang_translations": {
                                "ru": "Старые школьные друзья решили <съехаться> в родном городе спустя много лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgromadzić się",
                        "zebrać się",
                        "spotkać się"
                    ],
                    "antonyms": [
                        "rozjechać się"
                    ],
                    "common_phrases": [
                        "zjechać się w jedno miejsce",
                        "zjechać się tłumnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собраться в одном месте, прибыв с разных сторон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "съехаться",
                                "target_lang_sense_clarification": "собраться в одном месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fee6333-ad0f-3d24-8a78-d1ee9c1a2e53",
                    "sense_definition": "Bardzo surowo kogoś ocenić, skrytykować lub wyzwać, często używając ostrych słów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "criticism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szef potrafi niesprawiedliwie <zjechać> pracownika przy całym zespole za drobny błąd.",
                            "target_lang_translations": {
                                "ru": "Начальник может несправедливо <разнести> сотрудника перед всей командой за мелкую ошибку."
                            }
                        },
                        {
                            "text": "Recenzenci postanowili <zjechać> ten film, nie zostawiając na nim suchej nitki.",
                            "target_lang_translations": {
                                "ru": "Рецензенты решили <раскритиковать> этот фильм, не оставив от него камня на камне."
                            }
                        }
                    ],
                    "synonyms": [
                        "skrytykować",
                        "zrugać",
                        "zbesztać",
                        "opieprzyć"
                    ],
                    "antonyms": [
                        "pochwalić"
                    ],
                    "common_phrases": [
                        "zjechać kogoś z góry na dół",
                        "równo kogoś zjechać",
                        "zjechać jak burą sukę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень строго оценить, раскритиковать или отругать кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разнести",
                                "target_lang_sense_clarification": "сильно раскритиковать"
                            },
                            {
                                "target_lang_word": "отчитать",
                                "target_lang_sense_clarification": "строго выговорить"
                            },
                            {
                                "target_lang_word": "обругать",
                                "target_lang_sense_clarification": "грубо высказаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a18f726e-7da6-30fd-9d3d-712397932a23",
                    "sense_definition": "Szybko opuścić jakieś miejsce; uciec lub odejść (często używane w trybie rozkazującym jako żądanie odejścia).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "leaving_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli nie chcesz mieć kłopotów, powinieneś stąd jak najszybciej <zjechać>.",
                            "target_lang_translations": {
                                "ru": "Если не хочешь проблем, тебе стоит отсюда как можно скорее <свалить>."
                            }
                        },
                        {
                            "text": "Kazałem mu <zjechać> z mojego terenu i nigdy więcej tutaj nie wracać.",
                            "target_lang_translations": {
                                "ru": "Я велел ему <убираться> с моей территории и никогда больше сюда не возвращаться."
                            }
                        }
                    ],
                    "synonyms": [
                        "spadać",
                        "wynosić się",
                        "zmywać się",
                        "zwiać",
                        "spieprzać"
                    ],
                    "antonyms": [
                        "zostać"
                    ],
                    "common_phrases": [
                        "zjeżdżaj stąd",
                        "zjechać z oczu"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро покинуть место; уйти или убежать (слэнг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свалить",
                                "target_lang_sense_clarification": "быстро уйти (слэнг)"
                            },
                            {
                                "target_lang_word": "убраться",
                                "target_lang_sense_clarification": "уйти прочь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35b437a3-ebae-3ddb-8b4a-9b5d3e8330dc",
                    "sense_definition": "W żargonie drukarskim: zakończyć drukowanie określonego nakładu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "printing_jargon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Operator maszyny musiał szybko <zjechać> cały nakład gazet przed poranną dystrybucją.",
                            "target_lang_translations": {
                                "ru": "Оператор машины должен был быстро <откатать> весь тираж газет до утренней дистрибуции."
                            }
                        },
                        {
                            "text": "Udało nam się <zjechać> to zlecenie dwie godziny przed planowanym terminem.",
                            "target_lang_translations": {
                                "ru": "Нам удалось <отпечатать> этот заказ за два часа до запланированного срока."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydrukować",
                        "ukończyć druk"
                    ],
                    "common_phrases": [
                        "zjechać nakład",
                        "zjechać zlecenie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "jargon drukarski"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В типографском жаргоне: закончить печать определенного тиража."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "откатать",
                                "target_lang_sense_clarification": "напечатать тираж"
                            },
                            {
                                "target_lang_word": "отпечатать",
                                "target_lang_sense_clarification": "завершить печать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
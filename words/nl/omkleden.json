                    "sense_definition": "Andere kleren aandoen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga me even <omkleden> voordat we naar het feest gaan.",
                            "target_lang_translations": {
                                "ru": "Я пойду быстро <переоденусь>, прежде чем мы пойдем на вечеринку."
                            }
                        },
                        {
                            "text": "Na de voetbalwedstrijd hebben we ons direct <omgekleed>.",
                            "target_lang_translations": {
                                "ru": "После футбольного матча мы сразу <переоделись>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verkleden",
                        "verschonen"
                    ],
                    "common_phrases": [
                        "zich omkleden",
                        "snel omkleden",
                        "even omkleden"
                    ],
                    "target_lang_definitions": {

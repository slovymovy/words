                {
                    "sense_id": "b8fbb67e-48c7-31d5-a8e8-514853809393",
                    "sense_definition": "Крупный город на юге Украины, расположенный на реке Днепр; административный центр Запорожской области.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "city_sense",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Мой друг переехал в <w>Запорожье</w> несколько лет назад.",
                            "target_lang_translations": {
                                "en": "My friend moved to <w>Zaporizhzhia</w> a few years ago."
                            }
                        },
                        {
                            "text": "Современное <w>Запорожье</w> является важным промышленным центром.",
                            "target_lang_translations": {
                                "en": "Modern <w>Zaporizhzhia</w> is an important industrial center."
                            }
                        }
                    ],
                    "synonyms": [
                        "Александровск"
                    ],
                    "common_phrases": [
                        "город Запорожье",
                        "поездка в Запорожье",
                        "центр Запорожья"
                    ],
                    "target_lang_definitions": {
                        "en": "A large city in southern Ukraine located on the Dnieper River; the administrative center of the Zaporizhzhia Oblast."
                    },
                    "translations": {

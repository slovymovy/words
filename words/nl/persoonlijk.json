            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7367eba4-3229-3068-9428-d2f5ef098ff4",
                    "sense_definition": "Behorend bij of gericht op een specifiek persoon, niet algemeen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is mijn <w>persoonlijke</w> mening over de situatie.",
                            "target_lang_translations": {
                                "en": "This is my <w>personal</w> opinion on the situation.",
                                "ru": "Это мое <w>личное</w> мнение о ситуации."
                            }
                        },
                        {
                            "text": "Je mag geen <w>persoonlijke</w> gegevens delen met vreemden.",
                            "target_lang_translations": {
                                "en": "You must not share <w>personal</w> data with strangers.",
                                "ru": "Нельзя делиться <w>личными</w> данными с незнакомцами."
                            }
                        }
                    ],
                    "synonyms": [
                        "individueel",
                        "eigen"
                    ],
                    "antonyms": [
                        "onpersoonlijk"
                    ],
                    "common_phrases": [
                        "persoonlijke mening",
                        "persoonlijke gegevens",
                        "persoonlijke bezittingen"
                    ],
                    "target_lang_definitions": {
                        "en": "Belonging to or directed at a specific person, rather than being general.",
                        "ru": "Относящийся к конкретному человеку, собственный, не общий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "personal",
                                "target_lang_sense_clarification": "relating to a specific individual"
                            },
                            {
                                "target_lang_word": "individual",
                                "target_lang_sense_clarification": "distinct to one person"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "личный",
                                "target_lang_sense_clarification": "принадлежащий данному лицу"
                            },
                            {
                                "target_lang_word": "персональный",
                                "target_lang_sense_clarification": "индивидуальный, закрепленный за кем-то"

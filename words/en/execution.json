    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2125f500-1ce5-32a0-826a-c118478d9d0e",
                    "sense_definition": "The act of officially killing someone as a legal punishment for a crime.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prisoner was granted a stay of <w>execution</w> just hours before the scheduled time.",
                            "target_lang_translations": {
                                "ru": "Заключенному была предоставлена отсрочка <w>казни</w> всего за несколько часов до назначенного времени."
                            }
                        },
                        {
                            "text": "Many countries have abolished <w>execution</w> and replaced it with life imprisonment.",
                            "target_lang_translations": {
                                "ru": "Многие страны отменили смертную <w>казнь</w> и заменили её пожизненным заключением."
                            }
                        }
                    ],
                    "common_phrases": [
                        "public execution",
                        "stay of execution",
                        "face execution",
                        "await execution"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт официального лишения жизни человека в качестве законного наказания за преступление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "казнь",
                                "target_lang_sense_clarification": "высшая мера наказания"
                            },
                            {
                                "target_lang_word": "экзекуция",
                                "target_lang_sense_clarification": "телесное наказание или казнь (устар.)"
                            },
                            {
                                "target_lang_word": "расстрел",
                                "target_lang_sense_clarification": "вид смертной казни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23778308-a9e5-3687-a2ed-7e71bdaf5f70",
                    "sense_definition": "The act of performing a task, completing a piece of work, or putting a plan into action.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Although the strategy was sound, the <w>execution</w> was poor and led to failure.",
                            "target_lang_translations": {
                                "ru": "Хотя стратегия была здравой, <w>исполнение</w> было плохим и привело к провалу."
                            }
                        },
                        {
                            "text": "He was responsible for the successful <w>execution</w> of the entire project.",
                            "target_lang_translations": {
                                "ru": "Он отвечал за успешное <w>выполнение</w> всего проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "pursuance"
                    ],
                    "common_phrases": [
                        "put into execution",
                        "flawless execution",
                        "plan execution",
                        "poor execution"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс выполнения задачи, завершения работы или реализации плана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исполнение",
                                "target_lang_sense_clarification": "осуществление замысла или плана"
                            },
                            {
                                "target_lang_word": "выполнение",
                                "target_lang_sense_clarification": "завершение работы или задачи"
                            },
                            {
                                "target_lang_word": "реализация",
                                "target_lang_sense_clarification": "воплощение в жизнь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "313e510c-3654-37e5-909d-422dbfb8d5db",
                    "sense_definition": "The process by which a computer or virtual machine performs the instructions of a program.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The entire system slowed down significantly during the <w>execution</w> of the complex script.",
                            "target_lang_translations": {
                                "ru": "Вся система значительно замедлилась во время <w>выполнения</w> сложного скрипта."
                            }
                        },
                        {
                            "text": "Security protocols prevent the unauthorized <w>execution</w> of code on the server.",
                            "target_lang_translations": {
                                "ru": "Протоколы безопасности предотвращают несанкционированное <w>исполнение</w> кода на сервере."
                            }
                        }
                    ],
                    "common_phrases": [
                        "program execution",
                        "code execution",
                        "execution time",
                        "execution speed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс, посредством которого компьютер или виртуальная машина выполняет инструкции программы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выполнение",
                                "target_lang_sense_clarification": "работы программы или скрипта"
                            },
                            {
                                "target_lang_word": "исполнение",
                                "target_lang_sense_clarification": "программного кода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0defa1bb-eec0-32b3-b57f-7cb1fa28de3c",
                    "sense_definition": "The formal act of making a legal document valid or carrying out the terms of a court order or will.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer oversaw the proper <w>execution</w> of the will to ensure all heirs were satisfied.",
                            "target_lang_translations": {
                                "ru": "Юрист наблюдал за надлежащим <w>исполнением</w> завещания, чтобы убедиться, что все наследники удовлетворены."
                            }
                        },
                        {
                            "text": "They are waiting for the <w>execution</w> of the contract before starting the work.",
                            "target_lang_translations": {
                                "ru": "Они ждут <w>подписания</w> контракта перед началом работы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "execution of a will",
                        "writ of execution",
                        "execution of a deed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Формальный акт придания юридической силы документу или выполнение условий судебного приказа или завещания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исполнение",

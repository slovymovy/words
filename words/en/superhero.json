{
    "word_family": [
        "hero",
        "superheroine",
        "superheroic",
        "superheroism"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "46dbde19-4ff8-3c37-8bc0-5b4c3935e6bb",
                    "sense_definition": "A fictional character who fights crime, often possessing supernatural powers or special equipment.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fictional_characters",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Every child wants to be a <w>superhero</w> with the power to fly.",
                            "target_lang_translations": {
                                "ru": "Каждый ребенок хочет стать <w>супергероем</w>, обладающим способностью летать."
                            }
                        },
                        {
                            "text": "We went to the cinema to watch the latest <w>superhero</w> movie.",
                            "target_lang_translations": {
                                "ru": "Мы пошли в кинотеатр посмотреть новый фильм про <w>супергероев</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "supervillain"
                    ],
                    "common_phrases": [
                        "comic book superhero",
                        "favorite superhero",
                        "superhero movie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вымышленный персонаж, который борется с преступностью, часто обладая сверхъестественными способностями или специальным снаряжением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "супергерой",
                                "target_lang_sense_clarification": "персонаж со сверхспособностями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "686dfc6f-5b9b-3b98-9c06-d46bf8ce6f89",
                    "sense_definition": "A real person who is admired for doing something extremely difficult or helpful, as if they had special powers.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_praise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thanks for driving out here to help me; you are a real <w>superhero</w>!",
                            "target_lang_translations": {
                                "ru": "Спасибо, что приехал помочь мне, ты настоящий <w>супергерой</w>!"
                            }
                        },
                        {
                            "text": "The doctors and nurses worked like <w>superheroes</w> during the emergency.",
                            "target_lang_translations": {
                                "ru": "Врачи и медсестры работали как <w>супергерои</w> во время чрезвычайной ситуации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "real-life superhero",
                        "you are a superhero"
                    ],
                    "target_lang_definitions": {
                        "ru": "Реальный человек, которым восхищаются за совершение чего-то чрезвычайно сложного или полезного, словно он обладает особыми силами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "супергерой",
                                "target_lang_sense_clarification": "в переносном смысле, о человеке"
                            },
                            {
                                "target_lang_word": "герой",
                                "target_lang_sense_clarification": "человек, совершивший подвиг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
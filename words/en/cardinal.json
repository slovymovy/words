                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A bright red <w>cardinal</w> landed on the branch of the snowy pine tree.",
                            "target_lang_translations": {
                                "ru": "Ярко-красный <w>кардина́л</w> сел на ветку заснеженной сосны."
                            }

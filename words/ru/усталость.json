    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "686533dc-3c1a-3ea0-bcb8-ce70c2a02b75",
                    "sense_definition": "Состояние утомления, ослабления сил организма от продолжительной работы или какой-либо деятельности",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Глаза его закрывались от <w>усталости</w>.",
                            "target_lang_translations": {
                                "en": "His eyes were closing from <w>weariness</w>."
                            }
                        },
                        {
                            "text": "После долгого рабочего дня я чувствую сильную <w>усталость</w>.",
                            "target_lang_translations": {
                                "en": "After a long workday, I feel intense <w>fatigue</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "утомление",
                        "утомлённость"
                    ],
                    "antonyms": [
                        "свежесть"
                    ],
                    "common_phrases": [
                        "чувствовать усталость",
                        "сильная усталость",
                        "смертельная усталость",
                        "усталость от работы",
                        "приятная усталость"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of physical or mental exhaustion caused by prolonged work or activity"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fatigue",
                                "target_lang_sense_clarification": "physical or mental exhaustion"
                            },
                            {
                                "target_lang_word": "tiredness",
                                "target_lang_sense_clarification": "general state of needing rest"
                            },
                            {
                                "target_lang_word": "weariness",
                                "target_lang_sense_clarification": "feeling of being worn out"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48b1c9ce-4ca4-3e5d-bd66-6fd18394bb83",
                    "sense_definition": "Снижение прочности материала (обычно металла) под действием многократно повторяющихся нагрузок",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за интенсивного использования наступает <w>усталость</w> металла.",
                            "target_lang_translations": {
                                "en": "Due to intensive use, metal <w>fatigue</w> sets in."
                            }
                        },
                        {
                            "text": "Инженеры проверяют конструкции на <w>усталость</w> материала перед запуском в производство.",
                            "target_lang_translations": {
                                "en": "Engineers check structures for material <w>fatigue</w> before starting production."
                            }
                        }
                    ],
                    "common_phrases": [
                        "усталость металла",
                        "усталость материала"
                    ],
                    "target_lang_definitions": {
                        "en": "A reduction in the strength of a material (usually metal) caused by repeated stress or loading"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fatigue",
                                "target_lang_sense_clarification": "weakening of materials like metal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5b8d4f8-922a-3da0-bb55-eef2bf102dd8",
                    "sense_definition": "Истощение почвы, утрата питательных веществ, необходимых для роста растений",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чрезмерная эксплуатация земли без удобрений вызывает <w>усталость</w> почвы.",
                            "target_lang_translations": {
                                "en": "Excessive exploitation of land without fertilizers causes soil <w>exhaustion</w>."
                            }
                        },
                        {
                            "text": "Агрономы заметили признаки <w>усталости</w> почвы на этом участке поля.",
                            "target_lang_translations": {
                                "en": "Agronomists noticed signs of soil <w>fatigue</w> in this section of the field."
                            }
                        }
                    ],
                    "common_phrases": [
                        "усталость почвы"
                    ],
                    "target_lang_definitions": {
                        "en": "Depletion of soil nutrients necessary for plant growth"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exhaustion",
                                "target_lang_sense_clarification": "loss of soil fertility"
                            },
                            {
                                "target_lang_word": "fatigue",
                                "target_lang_sense_clarification": "agricultural term for soil depletion"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
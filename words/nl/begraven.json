    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b51a3748-1f8c-31b9-890a-d40f3dc1e1c9",
                    "sense_definition": "Een overledene in een kist of lijkwade in de aarde plaatsen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "funeral",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn opa werd vorige week op de begraafplaats <begraven>.",
                            "target_lang_translations": {
                                "en": "My grandpa was <w>buried</w> at the cemetery last week.",
                                "ru": "Моего дедушку на прошлой неделе <w>похоронили</w> на кладбище."
                            }
                        },
                        {
                            "text": "Zij willen hun overleden huisdier achter in de tuin <begraven>.",
                            "target_lang_translations": {
                                "en": "They want to <w>bury</w> their deceased pet in the back of the garden.",
                                "ru": "Они хотят <w>похоронить</w> своего умершего питомца в глубине сада."
                            }
                        }
                    ],
                    "antonyms": [
                        "opgraven"
                    ],
                    "common_phrases": [
                        "levend begraven",
                        "laten begraven"
                    ],
                    "target_lang_definitions": {
                        "en": "To place a dead body in the ground, usually as part of a funeral.",
                        "ru": "Помещать тело умершего в землю, обычно в гробу или саване."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bury",
                                "target_lang_sense_clarification": "a dead body"
                            },
                            {
                                "target_lang_word": "inter",
                                "target_lang_sense_clarification": "formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хоронить",
                                "target_lang_sense_clarification": "предавать земле"
                            },
                            {
                                "target_lang_word": "похоронить",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f775f397-6cb8-3961-a44d-665bb5e73c0c",
                    "sense_definition": "Iets onder de grond stoppen om het te verbergen of te bewaren.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "hiding_ground",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hond heeft zijn bot ergens in de tuin <begraven>.",
                            "target_lang_translations": {
                                "en": "The dog has <w>buried</w> his bone somewhere in the garden.",
                                "ru": "Собака <w>закопала</w> свою кость где-то в саду."
                            }
                        },
                        {
                            "text": "De piraten hebben de schat op een onbewoond eiland <begraven>.",
                            "target_lang_translations": {
                                "en": "The pirates have <w>buried</w> the treasure on a desert island.",
                                "ru": "Пираты <w>зарыли</w> сокровище на необитаемом острове."
                            }
                        }
                    ],
                    "antonyms": [
                        "opgraven"
                    ],
                    "common_phrases": [
                        "een schat begraven",
                        "diep begraven"
                    ],
                    "target_lang_definitions": {
                        "en": "To put an object under the ground to hide or keep it.",
                        "ru": "Класть что-либо под землю, чтобы спрятать или сохранить."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bury",
                                "target_lang_sense_clarification": "something underground"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "закапывать",
                                "target_lang_sense_clarification": "прятать в землю"
                            },
                            {
                                "target_lang_word": "зарывать",
                                "target_lang_sense_clarification": "помещать в грунт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbed54e1-6b1c-3212-afcb-4476cea97eb4",
                    "sense_definition": "Iets wegstoppen of bedekken zodat het niet meer zichtbaar is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hiding_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <begroef> zijn gezicht in zijn handen van verdriet.",
                            "target_lang_translations": {
                                "en": "He <w>buried</w> his face in his hands out of grief.",
                                "ru": "Он <w>спрятал</w> лицо в ладонях от горя."
                            }
                        },
                        {
                            "text": "Ze liep over straat met haar handen diep in haar zakken <begraven>.",
                            "target_lang_translations": {
                                "en": "She walked down the street with her hands <w>buried</w> deep in her pockets.",
                                "ru": "Она шла по улице, глубоко <w>спрятав</w> руки в карманы."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbergen",
                        "wegstoppen"
                    ],
                    "common_phrases": [
                        "gezicht begraven"
                    ],
                    "target_lang_definitions": {
                        "en": "To hide or cover something completely so it is not visible.",
                        "ru": "Прятать или накрывать что-либо так, чтобы оно не было видно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bury",
                                "target_lang_sense_clarification": "to cover or hide"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прятать",
                                "target_lang_sense_clarification": "скрывать из виду"
                            },
                            {
                                "target_lang_word": "скрывать",
                                "target_lang_sense_clarification": "делать невидимым"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "01f04740-65a5-3263-9fd7-7be7f5b288e5",
                    "sense_definition": "Duży, wystający i obwisły brzuch; słowo używane potocznie, często w kontekście osoby otyłej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "body_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego wielki <w>bandzioch</w> trząsł się rytmicznie, gdy głośno się śmiał.",
                            "target_lang_translations": {
                                "ru": "Его огромное <w>пузо</w> ритмично тряслось, когда он громко смеялся."
                            }
                        },
                        {
                            "text": "Musisz zacząć ćwiczyć, żeby zrzucić ten wystający <w>bandzioch</w>.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно начать заниматься спортом, чтобы убрать это торчащее <w>брюхо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "brzuch",
                        "bebech",
                        "samara"
                    ],
                    "common_phrases": [
                        "wielki bandzioch",
                        "hodować bandzioch"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой, выступающий и отвислый живот; разговорное слово, часто используемое по отношению к полным людям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пузо",
                                "target_lang_sense_clarification": "большой живот (разг.)"
                            },
                            {
                                "target_lang_word": "брюхо",
                                "target_lang_sense_clarification": "живот (груб.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb283953-5a0f-3fcb-89f3-2ab85698be1c",
                    "sense_definition": "Wnętrzności, narządy wewnętrzne lub żołądek, często w odniesieniu do zwierząt lub w kontekście trawienia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "body_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drapieżnik rozszarpał ofiarę i wyjadł jej wszystkie <w>bandziochy</w>.",
                            "target_lang_translations": {
                                "ru": "Хищник растерзал жертву и выел все её <w>потроха</w>."
                            }
                        },
                        {
                            "text": "Po zjedzeniu nieświeżego mięsa strasznie rozbolał go <w>bandzioch</w>.",
                            "target_lang_translations": {
                                "ru": "После того как он съел несвежее мясо, у него жутко разболелось <w>брюхо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wnętrzności",
                        "żołądek",
                        "bebech"
                    ],
                    "common_phrases": [
                        "napchać bandzioch"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренности, внутренние органы или желудок, часто в отношении животных или в контексте пищеварения."
                    },
                    "translations": {

            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "09b65eb6-de39-3c26-8e91-d0c7cd9ab71d",
                    "sense_definition": "Без сомнения, безусловно; выражает полную уверенность в истинности сказанного.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это <w>определённо</w> был самый лучший день нашего отпуска.",
                            "target_lang_translations": {
                                "en": "It was <w>definitely</w> the best day of our vacation."
                            }
                        },
                        {
                            "text": "Мы <w>определённо</w> успеем на последний автобус, если поторопимся.",
                            "target_lang_translations": {
                                "en": "We will <w>definitely</w> catch the last bus if we hurry."
                            }
                        }
                    ],
                    "synonyms": [
                        "безусловно",
                        "несомненно",
                        "точно"
                    ],
                    "antonyms": [
                        "предполагаемо"
                    ],
                    "common_phrases": [
                        "знать определённо",
                        "определённо да",
                        "определённо нет"
                    ],
                    "target_lang_definitions": {
                        "en": "Without doubt, absolutely; expresses complete confidence in the truth of what is said."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "definitely",
                                "target_lang_sense_clarification": "without doubt"
                            },
                            {
                                "target_lang_word": "certainly",
                                "target_lang_sense_clarification": "surely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48ad5151-e71b-3186-bda7-da723aa97834",
                    "sense_definition": "Ясно, точно, чётко и недвусмысленно.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <w>определённо</w> высказался по поводу новых правил работы.",
                            "target_lang_translations": {
                                "en": "He expressed himself <w>clearly</w> regarding the new work rules."
                            }
                        },
                        {
                            "text": "В инструкции <w>определённо</w> сказано, что устройство нельзя мочить.",
                            "target_lang_translations": {
                                "en": "The instructions <w>explicitly</w> state that the device must not get wet."
                            }
                        }
                    ],
                    "synonyms": [
                        "точно",
                        "чётко",
                        "ясно"
                    ],
                    "common_phrases": [
                        "сказать определённо",
                        "выразиться определённо"
                    ],
                    "target_lang_definitions": {
                        "en": "Clearly, precisely, distinctly, and unambiguously."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clearly",
                                "target_lang_sense_clarification": "in a clear manner"
                            },
                            {
                                "target_lang_word": "explicitly",
                                "target_lang_sense_clarification": "unambiguously"
                            },
                            {
                                "target_lang_word": "definitely",
                                "target_lang_sense_clarification": "decisively or precisely"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "stitch",
        "stitching"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c38e85c3-bce0-35c1-879f-32318c66dd45",
                    "sense_definition": "An embroidery stitch made over another stitch or edge.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tailor used a decorative <w>overstitch</w> to reinforce the hem of the garment.",
                            "target_lang_translations": {
                                "ru": "Портной использовал декоративный <w>обметочный шов</w>, чтобы укрепить подгиб изделия."
                            }
                        },
                        {
                            "text": "You can add texture to the design by applying a thick <w>overstitch</w>.",
                            "target_lang_translations": {
                                "ru": "Вы можете добавить текстуру дизайну, используя толстый <w>шов через край</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "decorative overstitch",
                        "overstitch wheel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вышивальный стежок, выполняемый поверх другого стежка или края ткани."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обметочный шов",
                                "target_lang_sense_clarification": "тип шва для обработки края"
                            },
                            {
                                "target_lang_word": "шов через край",
                                "target_lang_sense_clarification": "стежок, охватывающий край ткани"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e6362535-91f2-3ebd-8184-57bb6da5ff00",
                    "sense_definition": "To sew using stitches that pass over an edge or another stitch.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to <w>overstitch</w> the seams to prevent the fabric from fraying.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>обметать</w> швы, чтобы ткань не осыпалась."
                            }
                        },
                        {
                            "text": "It is important to <w>overstitch</w> the edge carefully for a clean and professional finish.",
                            "target_lang_translations": {
                                "ru": "Важно аккуратно <w>обметать</w> край для чистого и профессионального вида."
                            }
                        }
                    ],
                    "common_phrases": [
                        "overstitch the edge",
                        "overstitch the seam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шить стежками, проходящими через край ткани или поверх другого шва."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обметывать",
                                "target_lang_sense_clarification": "обрабатывать край ткани швом"
                            },
                            {
                                "target_lang_word": "шить через край",
                                "target_lang_sense_clarification": "выполнять стежки через край материала"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
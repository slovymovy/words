    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9cb53741-426c-3946-a520-a92cfd4afaab",
                    "sense_definition": "związany z południem (stroną świata), leżący na południu lub skierowany na południe",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiatr wieje w kierunku <w>południowym</w>.",
                            "target_lang_translations": {
                                "ru": "Ветер дует в <w>южном</w> направлении."
                            }
                        },
                        {
                            "text": "W tym roku spędzamy wakacje w <w>południowej</w> części kraju.",
                            "target_lang_translations": {
                                "ru": "В этом году мы проводим каникулы в <w>южной</w> части страны."
                            }
                        }
                    ],
                    "antonyms": [
                        "północny"
                    ],
                    "common_phrases": [
                        "Ameryka Południowa",
                        "biegun południowy",
                        "wiatr południowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "связанный с югом (стороной света), находящийся на юге или направленный на юг"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "южный",
                                "target_lang_sense_clarification": "сторона света"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8afdd404-236d-3181-9ac2-c378dfdab189",
                    "sense_definition": "odbywający się w południe (środek dnia), dotyczący tej pory",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spóźniłam się na nasze <w>południowe</w> spotkanie!",
                            "target_lang_translations": {
                                "ru": "Я опоздала на нашу <w>полуденную</w> встречу!"
                            }
                        },
                        {
                            "text": "Ostre <w>południowe</w> słońce raziło nas w oczy.",
                            "target_lang_translations": {
                                "ru": "Яркое <w>полуденное</w> солнце слепило нам глаза."
                            }
                        }
                    ],
                    "antonyms": [
                        "północny"
                    ],
                    "common_phrases": [
                        "słońce południowe",
                        "godzina południowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "происходящий в полдень, относящийся к середине дня"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полуденный",
                                "target_lang_sense_clarification": "время дня"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42dab7ed-939c-37aa-9c8c-a01f7849cd9d",
                    "sense_definition": "pochodzący z krajów o ciepłym klimacie, egzotyczny (zwykle o owocach lub roślinach)",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "origin_climate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mama kupiła trochę owoców <w>południowych</w>: pomarańczy, grejpfrutów i bananów.",
                            "target_lang_translations": {
                                "ru": "Мама купила немного <w>южных</w> фруктов: апельсинов, грейпфрутов и бананов."
                            }
                        },
                        {
                            "text": "W sklepie pojawiły się świeże owoce <w>południowe</w>.",
                            "target_lang_translations": {
                                "ru": "В магазине появились свежие <w>южные</w> фрукты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "owoce południowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "происходящий из стран с теплым климатом, экзотический (обычно о фруктах или растениях)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "южный",
                                "target_lang_sense_clarification": "о климате или происхождении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            }
                        },
                        {
                            "text": "Туристы <w>плотно</w> поели у костра и легли спать.",
                            "target_lang_translations": {
                                "en": "The tourists ate <w>heartily</w> by the campfire and went to sleep."
                            }
                        }
                    ],
                    "synonyms": [
                        "сытно",
                        "обильно",
                        "досыта"
                    ],
                    "common_phrases": [
                        "плотно поужинать",
                        "плотно поесть",
                        "плотно позавтракать"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "afe7329e-2beb-34f9-be7a-6ed08cc6d7bc",
                    "sense_definition": "Начать передвигаться по поверхности в лежачем положении (о людях) или на коротких ногах (о животных, насекомых).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленький ребёнок увидел яркую игрушку и быстро <w>пополз</w> к ней.",
                            "target_lang_translations": {
                                "en": "The small child saw a bright toy and quickly <w>started crawling</w> towards it."
                            }
                        },
                        {
                            "text": "Испуганная змея зашипела и <w>поползла</w> в густую траву.",
                            "target_lang_translations": {
                                "en": "The frightened snake hissed and <w>crawled</w> into the thick grass."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ребёнок пополз",
                        "змея поползла",
                        "поползти на коленях",
                        "поползти по пластунски"
                    ],
                    "target_lang_definitions": {
                        "en": "To start moving along a surface in a prone position (of people) or on short legs (of animals, insects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start crawling",
                                "target_lang_sense_clarification": "begin moving on hands and knees or belly"
                            },
                            {
                                "target_lang_word": "creep",
                                "target_lang_sense_clarification": "move slowly with the body close to the ground"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c125f5d-31d7-3ec7-8c5a-53693202cf9f",
                    "sense_definition": "Начать медленно двигаться, ехать или перемещаться (о транспорте, толпе, времени).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тяжёлый грузовик с трудом <w>пополз</w> вверх по крутому склону.",
                            "target_lang_translations": {
                                "en": "The heavy truck <w>crawled</w> up the steep slope with difficulty."
                            }
                        },
                        {
                            "text": "Очередь в кассу двигалась очень медленно, но наконец-то <w>поползла</w> вперёд.",
                            "target_lang_translations": {
                                "en": "The line at the checkout was moving very slowly, but finally <w>started crawling</w> forward."
                            }
                        }
                    ],
                    "synonyms": [
                        "поплестись",
                        "потащиться"
                    ],
                    "antonyms": [
                        "полететь",
                        "помчаться"
                    ],
                    "common_phrases": [
                        "время поползло",
                        "автобус пополз",
                        "пробка поползла",
                        "очередь поползла"
                    ],
                    "target_lang_definitions": {
                        "en": "To start moving, driving, or shifting very slowly (of transport, crowds, time)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crawl",
                                "target_lang_sense_clarification": "move very slowly (traffic, time)"
                            },
                            {
                                "target_lang_word": "inch",
                                "target_lang_sense_clarification": "move forward by small degrees"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3355d6e-463e-3943-92d1-3ceacca0fb3e",
                    "sense_definition": "Начать распространяться в разные стороны, захватывая пространство (о слухах, запахах, дыме, тумане).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spreading",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "По всему городу мгновенно <w>поползли</w> тревожные слухи о грядущих переменах.",
                            "target_lang_translations": {
                                "en": "Anxious rumors about impending changes instantly <w>spread</w> throughout the city."
                            }
                        },
                        {
                            "text": "От реки <w>пополз</w> густой белый туман, скрывая очертания домов.",
                            "target_lang_translations": {
                                "en": "A thick white fog <w>crept</w> from the river, hiding the outlines of the houses."
                            }
                        }
                    ],
                    "synonyms": [
                        "распространиться",
                        "разнестись"
                    ],
                    "common_phrases": [
                        "слухи поползли",
                        "туман пополз",
                        "дым пополз",
                        "запах пополз"
                    ],
                    "target_lang_definitions": {
                        "en": "To start spreading in different directions, covering space (of rumors, smells, smoke, fog)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spread",
                                "target_lang_sense_clarification": "extend over a larger area (rumors, smells)"
                            },
                            {
                                "target_lang_word": "creep",
                                "target_lang_sense_clarification": "move slowly and imperceptibly (fog, smoke)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36a4f9ac-3d61-3554-b4dc-41f222771de3",
                    "sense_definition": "Начать медленно сдвигаться вниз, осыпаться или терять устойчивость (о почве, предметах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sliding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После сильного дождя глинистый берег реки <w>пополз</w> вниз.",
                            "target_lang_translations": {
                                "en": "After the heavy rain, the clay riverbank <w>slid</w> down."
                            }
                        },
                        {
                            "text": "От удивления у него брови <w>поползли</w> на лоб.",
                            "target_lang_translations": {
                                "en": "In surprise, his eyebrows <w>crept up</w> his forehead."
                            }
                        }
                    ],
                    "synonyms": [
                        "сползти",
                        "осыпаться"
                    ],
                    "common_phrases": [
                        "берег пополз",
                        "грунт пополз",
                        "брови поползли",
                        "очки поползли"
                    ],
                    "target_lang_definitions": {
                        "en": "To start sliding down slowly, crumbling, or losing stability (of soil, objects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slide",
                                "target_lang_sense_clarification": "move smoothly along a surface due to gravity"
                            },
                            {
                                "target_lang_word": "give way",
                                "target_lang_sense_clarification": "collapse or slip downwards (soil)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d863db4-16b2-3809-ae0d-d587b193aa2f",
                    "sense_definition": "Начать рваться, распускаться (о ткани, вязаных вещах, чулках).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fabric_damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Девушка расстроилась, заметив, что на её новом чулке <w>поползла</w> стрелка.",
                            "target_lang_translations": {
                                "en": "The girl was upset when she noticed that a run <w>had started</w> in her new stocking."
                            }
                        },
                        {
                            "text": "Старая ткань не выдержала натяжения и с треском <w>поползла</w> по швам.",
                            "target_lang_translations": {
                                "en": "The old fabric could not withstand the tension and <w>came apart</w> at the seams with a ripping sound."
                            }
                        }
                    ],
                    "synonyms": [
                        "распуститься"
                    ],
                    "common_phrases": [
                        "стрелка поползла",
                        "шов пополз",
                        "ткань поползла"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To start tearing or unraveling (of fabric, knitted items, stockings)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "unravel vertically (hosiery)"
                            },
                            {
                                "target_lang_word": "come apart",
                                "target_lang_sense_clarification": "separate at the seams or tear"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "97869bd9-fd96-38c7-93a7-7ba000a2be53",
                    "sense_definition": "A historic city in North Yorkshire, England, famous for its cathedral and medieval walls.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "places",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We took a train to <w>York</w> to visit the famous railway museum.",
                            "target_lang_translations": {
                                "ru": "Мы сели на поезд до <w>Йорка</w>, чтобы посетить знаменитый железнодорожный музей."
                            }
                        },
                        {
                            "text": "The ancient walls of <w>York</w> attract tourists from all over the world.",
                            "target_lang_translations": {
                                "ru": "Древние стены <w>Йорка</w> привлекают туристов со всего мира."
                            }
                        }
                    ],
                    "common_phrases": [
                        "York Minster",
                        "Duke of York"
                    ],
                    "target_lang_definitions": {

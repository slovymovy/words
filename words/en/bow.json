                {
                    "sense_id": "556d74a5-61e2-3a2f-904f-b72297d63156",
                    "sense_definition": "A place name, such as a district in East London.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "He lives in <w>Bow</w>, which is a historic part of the East End of London.",
                            "target_lang_translations": {
                                "ru": "Он живет в <w>Боу</w>, исторической части лондонского Ист-Энда."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Географическое название, например, район в Восточном Лондоне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Боу",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        }

            "senses": [
                {
                    "sense_id": "0e775ade-0662-345d-8a2d-9151aba7e03d",
                    "sense_definition": "Sprawić, że ktoś przestaje spać i odzyskuje świadomość.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sleep_wake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <obudzić> mnie jutro o siódmej rano.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <разбудите> меня завтра в семь утра."
                            }
                        },
                        {
                            "text": "Głośny hałas na ulicy nagle <obudził> całą rodzinę.",
                            "target_lang_translations": {
                                "ru": "Громкий шум на улице внезапно <разбудил> всю семью."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbudzić"
                    ],
                    "antonyms": [
                        "uśpić"
                    ],
                    "common_phrases": [
                        "obudzić kogoś rano",
                        "obudzić ze snu",
                        "trudno kogoś obudzić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то проснуться, прервать чей-то сон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбудить",
                                "target_lang_sense_clarification": "прервать чей-то сон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23245cb2-3d6a-3127-861f-37bb7ffe6544",
                    "sense_definition": "Zakończyć sen i zacząć czuwać (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sleep_wake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziś <obudziłem> się bardzo wcześnie i byłem wypoczęty.",
                            "target_lang_translations": {
                                "ru": "Сегодня я <проснулся> очень рано и был отдохнувшим."
                            }
                        },
                        {
                            "text": "Czy możesz <obudzić> się bez nastawiania budzika?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <проснуться> без будильника?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zbudzić"
                    ],
                    "antonyms": [
                        "zasnąć"
                    ],
                    "common_phrases": [
                        "obudzić się rano",
                        "obudzić się wcześnie",
                        "obudzić się z krzykiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать спать и начать бодрствовать (обычно с возвратной частицей)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проснуться",
                                "target_lang_sense_clarification": "самому перестать спать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21dcbced-8ab5-375d-8c14-e91eb330c8b1",
                    "sense_definition": "Wywołać u kogoś silne emocje, wspomnienia, zainteresowanie lub inne stany wewnętrzne.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotions_cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta niezwykła podróż <obudziła> w niej pasję do fotografii.",
                            "target_lang_translations": {
                                "ru": "Это необыкновенное путешествие <пробудило> в ней страсть к фотографии."
                            }
                        },
                        {
                            "text": "Widok starego domu <obudził> wspomnienia z czasów dzieciństwa.",
                            "target_lang_translations": {
                                "ru": "Вид старого дома <пробудил> воспоминания из детства."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbudzić"
                    ],
                    "antonyms": [
                        "uśpić",
                        "stłumić"
                    ],
                    "common_phrases": [
                        "obudzić nadzieję",
                        "obudzić wspomnienia",
                        "obudzić ciekawość",
                        "obudzić zaufanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызвать сильные эмоции, воспоминания или интерес."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пробудить",

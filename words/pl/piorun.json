    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a01f2e90-c2b9-37f4-a2fd-9fc36c1a2b8a",
                    "sense_definition": "Gwałtowne wyładowanie elektryczne w atmosferze, któremu towarzyszy błysk i grzmot, występujące zwykle w czasie burzy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "weather_phenomenon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wczorajszej nawałnicy <w>piorun</w> uderzył w stare drzewo za naszym domem.",
                            "target_lang_translations": {
                                "ru": "Во время вчерашней бури <w>молния</w> ударила в старое дерево за нашим домом."
                            }
                        },
                        {
                            "text": "Huk był tak głośny, że wszyscy myśleliśmy, iż <w>piorun</w> trafił prosto w dach.",
                            "target_lang_translations": {
                                "ru": "Грохот был таким громким, что мы все подумали, что <w>молния</w> попала прямо в крышу."
                            }
                        }
                    ],
                    "synonyms": [
                        "grom"
                    ],
                    "common_phrases": [
                        "uderzenie pioruna",
                        "szybki jak piorun",
                        "piorun kulisty",
                        "chronić przed piorunami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный электрический разряд в атмосфере, сопровождающийся вспышкой и громом, обычно возникающий во время грозы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молния",
                                "target_lang_sense_clarification": "атмосферное явление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "2c315c60-9318-3473-a14b-44947c8e090b",
                    "sense_definition": "W mitologii słowiańskiej: bóg władający burzami i gromami; inna nazwa Peruna.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mythology",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Dawni Słowianie wznosili posągi, aby czcić boga o imieniu <w>Piorun</w>.",
                            "target_lang_translations": {
                                "ru": "Древние славяне воздвигали статуи, чтобы почитать бога по имени <w>Перун</w>."
                            }
                        },
                        {
                            "text": "Wierzono, że to <w>Piorun</w> zsyła deszcz potrzebny do wzrostu plonów.",
                            "target_lang_translations": {
                                "ru": "Верили, что именно <w>Перун</w> посылает дождь, необходимый для роста урожая."
                            }
                        }
                    ],
                    "synonyms": [
                        "Perun"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dotyczy mitologii"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В славянской мифологии: бог, повелевающий бурями и громом; другое имя Перуна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Перун",
                                "target_lang_sense_clarification": "бог-громовержец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0ef2d13-e3f0-3d2d-8a9f-d91e5e3eec94",
                    "sense_definition": "Polskie nazwisko.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Piorun</w> jest znanym specjalistą w dziedzinie energetyki.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Пёрун</w> — известный специалист в области энергетики."
                            }
                        },
                        {
                            "text": "Na liście obecności nie znalazłem nikogo o nazwisku <w>Piorun</w>.",
                            "target_lang_translations": {
                                "ru": "В списке присутствующих я не нашел никого по фамилии <w>Пёрун</w>."
                            }
                        }
                    ],
                    "traits": [

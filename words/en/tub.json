{
    "word_family": [
        "tubby",
        "tubful",
        "bathtub",
        "washtub"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9ac38d51-c810-34ff-8efe-e6345ffa0558",
                    "sense_definition": "A large, long container that is filled with water for sitting in and washing the body.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "bathing_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children love to splash around in the <w>tub</w> with their toys.",
                            "target_lang_translations": {
                                "ru": "Дети обожают плескаться в <w>ванне</w> со своими игрушками."
                            }
                        },
                        {
                            "text": "After a long day at work, I like to relax in a hot <w>tub</w>.",
                            "target_lang_translations": {
                                "ru": "После долгого рабочего дня я люблю расслабиться в горячей <w>ванне</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bathtub",
                        "bath"
                    ],
                    "common_phrases": [
                        "hot tub",
                        "in the tub",
                        "bath tub"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая продолговатая емкость, наполняемая водой, в которой сидят и моются."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ванна",
                                "target_lang_sense_clarification": "сантехника для мытья тела"
                            },
                            {
                                "target_lang_word": "корыто",
                                "target_lang_sense_clarification": "старая или примитивная емкость для мытья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0e1e85c-d8e4-3dc1-bf09-d41d8a9c289b",
                    "sense_definition": "A wide, open, round container used for holding, storing, or packing things.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "container_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a large <w>tub</w> of vanilla ice cream for the party.",
                            "target_lang_translations": {
                                "ru": "Она купила большую <w>банку</w> ванильного мороженого для вечеринки."
                            }
                        },
                        {
                            "text": "Please put the dirty dishes in the plastic <w>tub</w> on the counter.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, сложи грязную посуду в пластиковый <w>таз</w> на столешнице."
                            }
                        }
                    ],
                    "synonyms": [
                        "container",
                        "basin",
                        "pot",
                        "bin"
                    ],
                    "common_phrases": [
                        "tub of butter",
                        "plastic tub",
                        "tub of lard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Широкий открытый контейнер округлой формы для хранения или упаковки чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контейнер",
                                "target_lang_sense_clarification": "пластиковая емкость для еды или вещей"
                            },
                            {
                                "target_lang_word": "банка",
                                "target_lang_sense_clarification": "упаковка для мороженого, масла и т.д."
                            },
                            {
                                "target_lang_word": "кадка",
                                "target_lang_sense_clarification": "деревянная бочка, обычно для растений или солений"
                            },
                            {
                                "target_lang_word": "бадья",
                                "target_lang_sense_clarification": "большое открытое ведро или емкость"
                            },
                            {
                                "target_lang_word": "таз",
                                "target_lang_sense_clarification": "широкая емкость для стирки или мытья посуды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "147f9989-f7c7-3939-b5f4-3fe565152753",
                    "sense_definition": "An old, slow, or clumsy boat.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We crossed the river in a leaky old <w>tub</w> that looked unsafe.",
                            "target_lang_translations": {
                                "ru": "Мы переплыли реку на дырявом старом <w>корыте</w>, которое выглядело небезопасно."
                            }
                        },
                        {
                            "text": "The captain refused to sail that rusty <w>tub</w> across the ocean.",
                            "target_lang_translations": {
                                "ru": "Капитан отказался вести эту ржавую <w>посудину</w> через океан."
                            }
                        }
                    ],
                    "synonyms": [
                        "bucket",
                        "crate"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often derogatory or humorous"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старое, медленное или неуклюжее судно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корыто",
                                "target_lang_sense_clarification": "пренебрежительно о плохом судне"
                            },
                            {
                                "target_lang_word": "посудина",
                                "target_lang_sense_clarification": "разговорное название лодки или судна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39c4a8b0-3cc3-37ca-ab39-e8552ba1bfde",
                    "sense_definition": "A person who is very fat or obese.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was teased at school and called a <w>tub</w> by the other students.",
                            "target_lang_translations": {
                                "ru": "В школе его дразнили, и другие ученики называли его <w>бочкой</w>."
                            }
                        },
                        {
                            "text": "You should not be rude and call someone a <w>tub</w> just because of their weight.",
                            "target_lang_translations": {
                                "ru": "Не следует грубить и называть кого-то <w>жирным</w> только из-за веса."
                            }
                        }
                    ],
                    "synonyms": [
                        "fatty"
                    ],
                    "common_phrases": [
                        "tub of lard"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "offensive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень толстый человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бочка",
                                "target_lang_sense_clarification": "обидное прозвище толстого человека"
                            },
                            {
                                "target_lang_word": "тумба",
                                "target_lang_sense_clarification": "о неповоротливом, полном человеке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "890efd40-0c9d-321f-9930-6b1b9196cd94",
                    "sense_definition": "To wash oneself in a bathtub.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "bathing_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nanny <w>tubbed</w> the children before putting them to bed.",
                            "target_lang_translations": {
                                "ru": "Няня <w>искупала</w> детей перед тем, как уложить их спать."
                            }
                        },
                        {
                            "text": "He prefers to <w>tub</w> in cold water every morning to wake up.",
                            "target_lang_translations": {
                                "ru": "Он предпочитает <w>принимать ванну</w> в холодной воде каждое утро, чтобы проснуться."
                            }
                        }
                    ],
                    "synonyms": [
                        "bathe"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "somewhat dated"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мыться в ванне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "купать",
                                "target_lang_sense_clarification": "мыть кого-то в ванне"
                            },
                            {
                                "target_lang_word": "принимать ванну",
                                "target_lang_sense_clarification": "мыться самому"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2cd6f82-324c-3621-be6f-6863bd247925",
                    "sense_definition": "To plant or place something in a tub.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "container_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gardener <w>tubbed</w> the small trees to protect them from the frost.",
                            "target_lang_translations": {
                                "ru": "Садовник <w>посадил в кадки</w> маленькие деревья, чтобы защитить их от мороза."
                            }
                        },
                        {
                            "text": "We need to <w>tub</w> these plants before we move them to the patio.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>пересадить в кадки</w> эти растения, прежде чем мы перенесем их во дворик."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сажать или помещать что-либо в кадку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сажать в кадку",
                                "target_lang_sense_clarification": "о растениях"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
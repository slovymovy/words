{
    "word_family": [
        "certainly",
        "certainty",
        "uncertain",
        "uncertainty"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c07a11f8-6783-31fd-9ba9-c86b0e2a54bc",
                    "sense_definition": "Sure in one's mind; having no doubts about something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "subjective_certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>certain</w> that I left my keys on the table.",
                            "target_lang_translations": {
                                "ru": "Я <w>уверен</w>, что оставил ключи на столе."
                            }
                        },
                        {
                            "text": "She wanted to make <w>certain</w> that the door was locked before leaving.",
                            "target_lang_translations": {
                                "ru": "Она хотела <w>убедиться</w>, что дверь заперта, перед тем как уйти."
                            }
                        }
                    ],
                    "synonyms": [
                        "sure",
                        "confident",
                        "positive",
                        "confident"
                    ],
                    "antonyms": [
                        "uncertain",
                        "uncertain"
                    ],
                    "common_phrases": [
                        "make certain",
                        "know for certain",
                        "for certain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уверенный в чем-либо, не имеющий сомнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уверенный",
                                "target_lang_sense_clarification": "не сомневающийся"
                            },
                            {
                                "target_lang_word": "убеждённый",
                                "target_lang_sense_clarification": "твёрдо верящий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6aa649a-e545-3e80-ac2b-1879b6d07f41",
                    "sense_definition": "Specific and definite, but not explicitly named or described.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "specificity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The house has a <w>certain</w> charm that is hard to describe.",
                            "target_lang_translations": {
                                "ru": "В этом доме есть <w>некий</w> шарм, который трудно описать."
                            }
                        },
                        {
                            "text": "To a <w>certain</w> extent, I agree with your opinion.",
                            "target_lang_translations": {
                                "ru": "В <w>определённой</w> степени я согласен с вашим мнением."
                            }
                        }
                    ],
                    "synonyms": [
                        "definite",
                        "one",
                        "some#Pronoun"
                    ],
                    "common_phrases": [
                        "a certain extent",
                        "a certain amount",
                        "of a certain age"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный, но не названный прямо; некий, особенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "определённый",
                                "target_lang_sense_clarification": "конкретный, установленный"
                            },
                            {
                                "target_lang_word": "некий",
                                "target_lang_sense_clarification": "какой-то, неопределённый для слушателя"
                            },
                            {
                                "target_lang_word": "особый",
                                "target_lang_sense_clarification": "специфический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6622a69-8793-3b40-ac11-3908b4f63bdd",
                    "sense_definition": "Sure to happen; unavoidable or inevitable.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "objective_certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you do not study, failure is almost <w>certain</w>.",
                            "target_lang_translations": {
                                "ru": "Если ты не будешь учиться, провал почти <w>неизбежен</w>."
                            }
                        },
                        {
                            "text": "The soldiers faced <w>certain</w> death on the battlefield.",
                            "target_lang_translations": {
                                "ru": "Солдаты шли на <w>верную</w> смерть на поле битвы."
                            }
                        }
                    ],
                    "synonyms": [
                        "unavoidable",
                        "assured",
                        "sure",
                        "definite"
                    ],
                    "antonyms": [
                        "impossible"
                    ],
                    "common_phrases": [
                        "certain death",
                        "certain victory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неизбежный, неминуемый; то, что обязательно произойдет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неизбежный",
                                "target_lang_sense_clarification": "который нельзя предотвратить"
                            },
                            {
                                "target_lang_word": "верный",
                                "target_lang_sense_clarification": "гарантированный (например, смерть)"
                            },
                            {
                                "target_lang_word": "неминуемый",
                                "target_lang_sense_clarification": "обязательный к исполнению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1a1cc91-1e17-3063-b68d-f2c2dca1db50",
                    "sense_definition": "Established as a fact; not to be doubted or denied.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "objective_certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>certain</w> that the economy will recover eventually.",
                            "target_lang_translations": {
                                "ru": "<w>Несомненно</w>, что экономика в конце концов восстановится."
                            }
                        },
                        {
                            "text": "The police have no <w>certain</w> proof of his guilt yet.",
                            "target_lang_translations": {
                                "ru": "У полиции пока нет <w>достоверных</w> доказательств его вины."
                            }
                        }
                    ],
                    "synonyms": [
                        "clear",
                        "definite",
                        "unequivocal",
                        "unambiguous",
                        "doubtless"
                    ],
                    "antonyms": [
                        "uncertain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несомненный, бесспорный; установленный как факт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несомненный",
                                "target_lang_sense_clarification": "очевидный"
                            },
                            {
                                "target_lang_word": "бесспорный",
                                "target_lang_sense_clarification": "не вызывающий споров"
                            },
                            {
                                "target_lang_word": "достоверный",
                                "target_lang_sense_clarification": "подлинный, доказанный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "e8dfceb7-5ec1-3ef0-8323-88e81f312bce",
                    "sense_definition": "Used to refer to specific people or things without naming them individually.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "specificity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Certain</w> people are naturally good at mathematics.",
                            "target_lang_translations": {
                                "ru": "<w>Некоторые</w> люди от природы способны к математике."
                            }
                        },
                        {
                            "text": "We need to follow <w>certain</w> rules to stay safe.",
                            "target_lang_translations": {
                                "ru": "Нам нужно соблюдать <w>определённые</w> правила, чтобы оставаться в безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "some#Pronoun"
                    ],
                    "common_phrases": [
                        "certain people",
                        "certain things"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на конкретных людей или вещи без их называния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "некоторые",
                                "target_lang_sense_clarification": "часть группы"
                            },
                            {
                                "target_lang_word": "определённые",
                                "target_lang_sense_clarification": "конкретные, но не названные"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "50eae4d5-3d3d-3388-a957-3e011f269cff",
                    "sense_definition": "Unnamed or undescribed members of a group.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "specificity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Certain</w> of the documents were missing from the file.",
                            "target_lang_translations": {
                                "ru": "<w>Некоторые</w> из документов в папке отсутствовали."
                            }
                        },
                        {
                            "text": "She invited many friends, but <w>Certain</w> of them could not come.",
                            "target_lang_translations": {
                                "ru": "Она пригласила много друзей, но <w>некоторые</w> из них не смогли прийти."
                            }
                        }
                    ],
                    "synonyms": [
                        "some#Pronoun"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неназванные или неописанные члены группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "некоторые",
                                "target_lang_sense_clarification": "часть из целого"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "609ed9a9-e496-3431-ad75-7a5e2577076f",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Certain</w> arrived late to the meeting.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Сёртейн</w> опоздал на встречу."
                            }
                        },
                        {
                            "text": "The <w>Certain</w> family has lived here for generations.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Сёртейн</w> живет здесь уже несколько поколений."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сёртейн",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
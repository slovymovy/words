    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d8eee6ed-b1a9-3a72-b8a0-9f78cb9cf10f",
                    "sense_definition": "Część ubioru okrywająca stopę, noszona zazwyczaj pod butami.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano zakładam czyste <w>skarpety</w> do pracy.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я надеваю чистые <w>носки</w> на работу."
                            }
                        },
                        {
                            "text": "Muszę kupić nową parę ciepłych <w>skarpet</w> na zimę.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую пару тёплых <w>носков</w> на зиму."
                            }
                        }
                    ],
                    "synonyms": [
                        "skarpetka",
                        "gw. (Górny Śląsk) fuzekla, zoka"
                    ],
                    "common_phrases": [
                        "para skarpet",
                        "dziura w skarpecie",
                        "wełniane skarpety",
                        "śmierdzące skarpety"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть одежды, закрывающая ступню, которую обычно носят под обувью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "носок",
                                "target_lang_sense_clarification": "одежда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "081cce67-89ca-3e38-b9c8-509018ca7541",
                    "sense_definition": "Wskaźnik kierunku i siły wiatru w kształcie stożkowatego rękawa, używany na lotniskach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "aviation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pilot spojrzał na <w>skarpetę</w>, aby ocenić boczny wiatr.",
                            "target_lang_translations": {
                                "ru": "Пилот посмотрел на <w>ветровой рукав</w>, чтобы оценить боковой ветер."
                            }
                        },
                        {
                            "text": "Nad pasem startowym powiewała biało-czerwona <w>skarpeta</w>.",
                            "target_lang_translations": {
                                "ru": "Над взлётно-посадочной полосой развевался бело-красный <w>ветровой рукав</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rękaw"
                    ],
                    "common_phrases": [
                        "skarpeta wiatrowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указатель направления и силы ветра в форме конуса, используемый на аэродромах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ветровой рукав",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "ветроуказатель",
                                "target_lang_sense_clarification": "термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db2a843a-cbe7-3644-b656-5eae862f1961",
                    "sense_definition": "Potoczne określenie samochodu marki Syrena.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "automotive_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek z sentymentem wspomina swoją starą <w>skarpetę</w>.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка с ностальгией вспоминает свою старую <w>Сирену</w>."
                            }
                        },
                        {
                            "text": "Na zlocie zabytkowych pojazdów pojawiła się odnowiona <w>skarpeta</w>.",
                            "target_lang_translations": {
                                "ru": "На слёте ретро-автомобилей появилась отреставрированная <w>Сирена</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stara skarpeta",
                        "jeździć skarpetą"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",

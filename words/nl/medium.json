    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6609927f-6098-3d76-afb2-62d7b50e384f",
                    "sense_definition": "Een middel om informatie te verspreiden naar een groot publiek, zoals televisie, kranten, radio of internet.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication_means",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Televisie is nog steeds een krachtig <w>medium</w> voor reclame."
                        },
                        {
                            "text": "Via welk <w>medium</w> heb je dat nieuws vernomen?"
                        }
                    ],
                    "common_phrases": [
                        "sociale media",
                        "digitaal medium",
                        "via het medium"
                    ]
                },
                {
                    "sense_id": "f4cd459a-0aab-32eb-9cbe-e2e6252c5f87",
                    "sense_definition": "Een maat, meestal voor kleding, die tussen klein (small) en groot (large) in zit.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "size_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Helaas is de <w>medium</w> uitverkocht, we hebben alleen nog large."
                        },
                        {
                            "text": "Draag jij meestal een small of een <w>medium</w>?"
                        }
                    ],
                    "common_phrases": [
                        "een maatje medium"
                    ]
                },
                {
                    "sense_id": "68f41f2b-2e82-38b9-86f8-3d7947d8b825",
                    "sense_definition": "Iemand die beweert contact te kunnen maken met geesten van overledenen of andere dimensies.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "paranormal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>medium</w> gaf de vrouw een boodschap van haar overleden man."
                        },
                        {
                            "text": "Hij gelooft niet in paranormale zaken en noemt het <w>medium</w> een oplichter."
                        }
                    ],
                    "common_phrases": [
                        "een spiritueel medium",
                        "bezoek aan een medium"
                    ]
                },
                {
                    "sense_id": "850ba736-bc06-3480-a30f-68b584a58917",
                    "sense_definition": "Een hulpmiddel, instrument of tussenweg om een bepaald doel te bereiken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "communication_means",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Taal is het belangrijkste <w>medium</w> voor menselijke interactie."
                        },
                        {
                            "text": "Geld is slechts een <w>medium</w> om goederen en diensten uit te wisselen."
                        }
                    ]
                },
                {
                    "sense_id": "da8888c8-e100-3441-b027-672fc132b531",
                    "sense_definition": "Een stof of omgeving waarin een verschijnsel (zoals geluid of golven) zich verplaatst.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physics_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lucht is het <w>medium</w> waardoor geluidsgolven zich meestal voortplanten."
                        },
                        {
                            "text": "In een vacuüm ontbreekt een <w>medium</w> voor geluidsoverdracht."
                        }
                    ],
                    "common_phrases": [
                        "vloeibaar medium",
                        "transparant medium"
                    ]
                },
                {
                    "sense_id": "388f1c57-3e6b-329d-91bc-640aa18bcfcf",
                    "sense_definition": "Een fysiek voorwerp of materiaal waarop gegevens kunnen worden opgeslagen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physics_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De harde schijf is een betrouwbaar <w>medium</w> voor dataopslag."
                        },
                        {
                            "text": "Het programma paste nog maar net op het gekozen <w>medium</w>."
                        }
                    ],
                    "common_phrases": [
                        "digitaal medium"
                    ]
                }
            ]
        },
        {

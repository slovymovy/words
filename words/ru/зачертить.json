    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f3f6a280-7aae-3672-a1c2-b7049bad7b0f",
                    "sense_definition": "Покрыть поверхность множеством линий или штрихов, часто делая изображение неразборчивым или закрашивая его.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "covering_with_lines",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ребёнок взял чёрный маркер и полностью <w>зачертил</w> красивую картинку в книге.",
                            "target_lang_translations": {
                                "en": "The child took a black marker and completely <w>scribbled over</w> the beautiful picture in the book."
                            }
                        },
                        {
                            "text": "Студент <w>зачертил</w> ошибку в конспекте так, что её стало невозможно прочитать.",
                            "target_lang_translations": {
                                "en": "The student <w>scribbled over</w> the mistake in the notes so that it became impossible to read."
                            }
                        }
                    ],
                    "synonyms": [
                        "исчертить",
                        "заштриховать",
                        "зачёркать"
                    ],
                    "antonyms": [
                        "стереть",
                        "очистить"
                    ],
                    "common_phrases": [
                        "зачертить лист",
                        "зачертить карандашом",
                        "зачертить схему"
                    ],
                    "target_lang_definitions": {
                        "en": "To cover a surface with many lines or strokes, often making the image illegible or coloring over it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scribble over",
                                "target_lang_sense_clarification": "cover with messy lines"
                            },
                            {
                                "target_lang_word": "shade",
                                "target_lang_sense_clarification": "fill an area with lines"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6db49b7d-00dd-3529-8bbc-69b04520c48d",
                    "sense_definition": "Сделать быстрый рисунок или набросок чего-либо штрихами.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sketching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Путешественник достал блокнот и быстро <w>зачертил</w> вид старинного замка.",
                            "target_lang_translations": {
                                "en": "The traveler took out a notebook and quickly <w>sketched</w> the view of the ancient castle."
                            }
                        },
                        {
                            "text": "Архитектор <w>зачертил</w> внезапно пришедшую ему в голову идею фасада.",
                            "target_lang_translations": {
                                "en": "The architect <w>sketched</w> the idea for the facade that suddenly came to his mind."
                            }
                        }
                    ],
                    "synonyms": [
                        "зарисовать",
                        "набросать",
                        "начертить"
                    ],
                    "common_phrases": [
                        "зачертить план",
                        "зачертить вид"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "В современном языке чаще используются слова «зарисовать» или «набросать»."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To make a quick drawing or sketch of something using strokes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sketch",
                                "target_lang_sense_clarification": "draw quickly"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4c4c66e3-ad60-39e2-96c1-10a7d3e47ab4",
                    "sense_definition": "Уехать из одного места в разные стороны (о нескольких людях или транспортных средствах)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "separation_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После окончания праздника гости начали <w>разъезжаться</w> по домам.",
                            "target_lang_translations": {
                                "en": "After the celebration ended, the guests began to <w>depart</w> for their homes."
                            }
                        },
                        {
                            "text": "Мы <w>разъехались</w> в разные города, но продолжили общаться по телефону.",
                            "target_lang_translations": {
                                "en": "We <w>moved</w> to different cities but continued to communicate by phone."
                            }
                        }
                    ],
                    "synonyms": [
                        "разойтись",
                        "разбежаться"
                    ],
                    "antonyms": [
                        "съехаться",
                        "собраться"
                    ],
                    "common_phrases": [
                        "гости разъехались",
                        "разъехаться по домам",
                        "разъехаться в разные стороны"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave one place in different directions (about several people or vehicles)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go separate ways",
                                "target_lang_sense_clarification": "leave in different directions"
                            },
                            {
                                "target_lang_word": "depart",
                                "target_lang_sense_clarification": "leave a place or gathering"
                            },
                            {
                                "target_lang_word": "disperse",
                                "target_lang_sense_clarification": "scatter in different directions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56305397-79f7-3c69-ad23-6a39216524ea",
                    "sense_definition": "Перестать жить вместе в одной квартире или доме, переехав в разные места",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "separation_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Взрослые дети решили <w>разъехаться</w> с родителями и начать самостоятельную жизнь.",
                            "target_lang_translations": {
                                "en": "The adult children decided to <w>move out</w> from their parents and start an independent life."
                            }
                        },
                        {
                            "text": "После развода супруги быстро <w>разъехались</w> и разменяли квартиру.",
                            "target_lang_translations": {
                                "en": "After the divorce, the spouses quickly <w>moved apart</w> and exchanged the apartment."
                            }
                        }
                    ],
                    "synonyms": [
                        "расселиться"
                    ],
                    "antonyms": [
                        "съехаться"
                    ],
                    "common_phrases": [
                        "разъехаться с родителями",
                        "разъехаться по квартирам"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop living together in one apartment or house, moving to different places"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move apart",
                                "target_lang_sense_clarification": "stop living together"
                            },
                            {
                                "target_lang_word": "move out",
                                "target_lang_sense_clarification": "leave a shared home"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd3b243c-1fda-3c70-83fc-bd89705c5f34",
                    "sense_definition": "Осторожно проехать мимо друг друга, избежав столкновения (обычно на узкой дороге)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "traffic_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Улица была такой узкой, что две машины с трудом могли там <w>разъехаться</w>.",
                            "target_lang_translations": {
                                "en": "The street was so narrow that two cars could hardly <w>pass each other</w> there."
                            }
                        },
                        {
                            "text": "Водители аккуратно <w>разъехались</w> на повороте, не задев друг друга зеркалами.",
                            "target_lang_translations": {
                                "en": "The drivers carefully <w>passed</w> at the turn without grazing each other's mirrors."
                            }
                        }
                    ],
                    "synonyms": [
                        "разминуться"
                    ],
                    "antonyms": [
                        "столкнуться"
                    ],
                    "common_phrases": [
                        "с трудом разъехаться",
                        "едва разъехались"
                    ],
                    "target_lang_definitions": {
                        "en": "To drive past each other carefully, avoiding a collision (usually on a narrow road)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pass each other",
                                "target_lang_sense_clarification": "vehicles on a narrow road"
                            },
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "drive past without collision"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fbc77f3-f1d0-3bc5-a124-bb93b78d2398",
                    "sense_definition": "Не встретиться с кем-либо, находясь в пути или придя в условленное место в разное время",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "traffic_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы договорились встретиться у входа, но случайно <w>разъехались</w> в толпе.",
                            "target_lang_translations": {
                                "en": "We agreed to meet at the entrance but accidentally <w>missed each other</w> in the crowd."
                            }
                        },
                        {
                            "text": "Я поехал к нему, а он ко мне, так мы и <w>разъехались</w>.",
                            "target_lang_translations": {
                                "en": "I went to his place, and he came to mine, so we <w>missed each other</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "разминуться"
                    ],
                    "antonyms": [
                        "встретиться"
                    ],
                    "common_phrases": [
                        "мы разъехались",
                        "случайно разъехаться"
                    ],
                    "target_lang_definitions": {
                        "en": "Not to meet someone while traveling or arriving at a designated place at different times"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "miss each other",
                                "target_lang_sense_clarification": "fail to meet while traveling"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eafdf2c6-8242-34ff-8aba-deb158f07cc6",
                    "sense_definition": "Скользнуть в разные стороны, потеряв устойчивость (о ногах, лыжах, коньках)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_instability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На скользком льду у меня <w>разъехались</w> ноги, и я упал.",
                            "target_lang_translations": {
                                "en": "On the slippery ice, my legs <w>slid apart</w>, and I fell."
                            }
                        },
                        {
                            "text": "Лыжи новичка <w>разъехались</w> в стороны, и он потерял равновесие.",
                            "target_lang_translations": {
                                "en": "The beginner's skis <w>slid apart</w> to the sides, and he lost his balance."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ноги разъехались",
                        "лыжи разъехались"
                    ],
                    "target_lang_definitions": {
                        "en": "To slide in different directions, losing stability (about legs, skis, skates)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slide apart",
                                "target_lang_sense_clarification": "legs or skis losing stability"
                            },
                            {
                                "target_lang_word": "splay out",
                                "target_lang_sense_clarification": "legs spreading unintentionally"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5b739f3-29f2-35be-8a7f-4899bb097db0",
                    "sense_definition": "Разорваться по шву, сломаться или образовать щель (о предметах, одежде, мебели)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_instability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я попытался застегнуть куртку, но старая молния сразу <w>разъехалась</w>.",
                            "target_lang_translations": {
                                "en": "I tried to zip up the jacket, but the old zipper immediately <w>split</w>."
                            }
                        },
                        {
                            "text": "От сырости деревянные доски на полу рассохлись и <w>разъехались</w>.",
                            "target_lang_translations": {
                                "en": "Due to dampness, the wooden floorboards dried out and <w>came apart</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "расползтись",
                        "развалиться"
                    ],
                    "common_phrases": [
                        "молния разъехалась",
                        "швы разъехались"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech regarding objects"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To tear at the seam, break, or form a gap (about objects, clothes, furniture)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come apart",
                                "target_lang_sense_clarification": "break or separate at seams/joints"
                            },
                            {
                                "target_lang_word": "split",
                                "target_lang_sense_clarification": "zipper or seam opening"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "synonyms": [
                        "резкий",
                        "звенящий"
                    ],
                    "common_phrases": [
                        "металлический голос",
                        "металлический звук",
                        "металлический смех",
                        "металлические нотки"
                    ],
                    "target_lang_definitions": {
                        "en": "Ringing, harsh, lacking softness (about sound, voice, or laughter)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "metallic",

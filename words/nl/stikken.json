{
    "word_family": [
        "verstikken",
        "stiksel"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b766398c-21fc-38f7-ac38-22f5ac439873",
                    "sense_definition": "Geen adem meer kunnen halen door een belemmering van de luchtwegen of door zuurstofgebrek.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "suffocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pas op dat je niet in dat grote stuk appel gaat <w>stikken</w>.",
                            "target_lang_translations": {
                                "ru": "Смотри, не <w>подавись</w> этим большим куском яблока.",
                                "en": "Watch out that you don't <w>choke</w> on that big piece of apple."
                            }
                        },
                        {
                            "text": "Door de dichte rook in het gebouw dreigden de bewoners te <w>stikken</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за густого дыма в здании жильцы рисковали <w>задохнуться</w>.",
                                "en": "Due to the dense smoke in the building, the residents were in danger of <w>suffocating</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verstikken",
                        "smoren"
                    ],
                    "antonyms": [
                        "ademen"
                    ],
                    "common_phrases": [
                        "stikken in",
                        "bijna stikken",
                        "stikken van de hitte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не иметь возможности дышать из-за препятствия в дыхательных путях или недостатка кислорода.",
                        "en": "To be unable to breathe due to an obstruction of the airways or a lack of oxygen."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задыхаться",
                                "target_lang_sense_clarification": "от нехватки воздуха"
                            },
                            {
                                "target_lang_word": "давиться",
                                "target_lang_sense_clarification": "поперхнуться едой или предметом"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "choke",
                                "target_lang_sense_clarification": "to have the windpipe blocked by food or an object"
                            },
                            {
                                "target_lang_word": "suffocate",
                                "target_lang_sense_clarification": "to die or suffer from lack of air or oxygen"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b334e50f-503a-35e3-b82d-00e80caa8a03",
                    "sense_definition": "In zeer grote hoeveelheden aanwezig zijn of voorkomen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer <w>stikt</w> het bij het meer van de muggen.",
                            "target_lang_translations": {
                                "ru": "Летом у озера всё <w>кишит</w> комарами.",
                                "en": "In the summer, the lake <w>teems</w> with mosquitoes."
                            }
                        },
                        {
                            "text": "Het <w>stikte</w> in de oude stad van de toeristen.",
                            "target_lang_translations": {
                                "ru": "Старый город <w>кишел</w> туристами.",
                                "en": "The old city <w>swarmed</w> with tourists."
                            }
                        }
                    ],
                    "synonyms": [
                        "wemelen",
                        "barsten"
                    ],
                    "common_phrases": [
                        "stikken van de"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in the construction 'het stikt van de...'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Присутствовать или встречаться в очень больших количествах.",
                        "en": "To be present or occur in very large quantities."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кишеть",
                                "target_lang_sense_clarification": "быть переполненным чем-либо"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "teem",
                                "target_lang_sense_clarification": "to be full of or swarming with"
                            },
                            {
                                "target_lang_word": "swarm",
                                "target_lang_sense_clarification": "to be crowded or overrun with"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9189713-86a0-3f62-9160-068db53887e0",
                    "sense_definition": "Met naald en draad steken maken om textiel vast te zetten of te bewerken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sewing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kleermaker moet de zoom van de broek nog even <w>stikken</w>.",
                            "target_lang_translations": {
                                "ru": "Портной должен ещё <w>прострочить</w> подгиб брюк.",
                                "en": "The tailor still needs to <w>stitch</w> the hem of the trousers."
                            }
                        },
                        {
                            "text": "Zij heeft de twee lappen stof stevig aan elkaar <w>gestikt</w>.",
                            "target_lang_translations": {
                                "ru": "Она крепко <w>сшила</w> два куска ткани друг с другом.",
                                "en": "She <w>stitched</w> the two pieces of fabric firmly together."
                            }
                        }
                    ],
                    "synonyms": [
                        "naaien"
                    ],
                    "antonyms": [
                        "lostornen"
                    ],
                    "common_phrases": [
                        "een zoom stikken",
                        "dubbel stikken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать стежки иглой и ниткой для скрепления или обработки ткани.",
                        "en": "To make stitches with a needle and thread to fasten or process fabric."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строчить",
                                "target_lang_sense_clarification": "шить, обычно на швейной машине"
                            },
                            {
                                "target_lang_word": "сшивать",
                                "target_lang_sense_clarification": "соединять тканью"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "stitch",
                                "target_lang_sense_clarification": "to fasten or join with stitches"
                            },
                            {
                                "target_lang_word": "sew",
                                "target_lang_sense_clarification": "to join or attach by stitching"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
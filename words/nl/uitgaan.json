                        "ru": [
                            {
                                "target_lang_word": "заканчиваться",
                                "target_lang_sense_clarification": "подходить к концу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b095405-a49a-34d4-941e-1769b691cfa6",
                    "sense_definition": "Zijn oorsprong vinden bij; afkomstig zijn van.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "assumption_origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit initiatief moet van de bewoners zelf <uitgaan>.",
                            "target_lang_translations": {
                                "en": "This initiative must <come from> the residents themselves.",
                                "ru": "Эта инициатива должна <исходить> от самих жителей."
                            }
                        },
                        {
                            "text": "Er kan een gevaarlijke werking van die stof <uitgaan>.",
                            "target_lang_translations": {
                                "en": "A dangerous effect can <emanate from> that substance.",
                                "ru": "От этого вещества может <исходить> опасное воздействие."
                            }
                        }

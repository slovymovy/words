    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "405e545c-cbc2-344b-bacb-731896a6b2e0",
                    "sense_definition": "Zachowywać się w określony sposób; podejmować decyzje i działania w danej sytuacji, często w kontekście moralnym lub społecznym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wiem, jak mam <postępować> w tak trudnej sytuacji rodzinnej.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, как мне <поступать> в такой сложной семейной ситуации."
                            }
                        },
                        {
                            "text": "Zawsze staram się <postępować> uczciwie wobec moich przyjaciół i współpracowników.",
                            "target_lang_translations": {
                                "ru": "Я всегда стараюсь <поступать> честно по отношению к моим друзьям и коллегам."
                            }
                        }
                    ],
                    "synonyms": [
                        "zachowywać się",
                        "działać"
                    ],
                    "common_phrases": [
                        "postępować słusznie",
                        "postępować uczciwie",
                        "postępować etycznie",
                        "postępować właściwie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя определённым образом; принимать решения и совершать действия в конкретной ситуации, часто в моральном или социальном контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поступать",
                                "target_lang_sense_clarification": "совершать поступки, вести себя"
                            },
                            {
                                "target_lang_word": "действовать",
                                "target_lang_sense_clarification": "предпринимать действия, проявлять активность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a996c709-233a-3380-9bdb-335608259b15",
                    "sense_definition": "Wykonywać kolejne czynności zgodnie z ustalonym planem, zasadami, prawem lub instrukcją.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "action_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas instalacji nowego oprogramowania <postępowałem> dokładnie zgodnie z instrukcjami na ekranie.",
                            "target_lang_translations": {
                                "ru": "При установке нового программного обеспечения я <действовал> точно в соответствии с инструкциями на экране."
                            }
                        },
                        {
                            "text": "Policja musi <postępować> według ściśle określonych procedur, aby nie złamać prawa.",
                            "target_lang_translations": {
                                "ru": "Полиция должна <действовать> по строго определённым процедурам, чтобы не нарушить закон."
                            }
                        }
                    ],
                    "synonyms": [
                        "działać",
                        "procedować"
                    ],
                    "common_phrases": [
                        "postępować zgodnie z instrukcją",
                        "postępować według planu",
                        "postępować zgodnie z prawem",
                        "postępować rutynowo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять последовательные действия в соответствии с установленным планом, правилами, законом или инструкцией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "действовать",
                                "target_lang_sense_clarification": "согласно правилам, плану или инструкции"
                            },
                            {
                                "target_lang_word": "следовать",
                                "target_lang_sense_clarification": "инструкции или процедуре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9dc8c0de-77fc-3234-bc87-982db8550819",
                    "sense_definition": "Posuwać się naprzód w rozwoju; stawać się bardziej zaawansowanym, intensywnym lub zajmować coraz większy obszar (często o procesach, chorobach, czasie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "progress_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety jego choroba szybko <postępuje> i lekarze są zaniepokojeni.",
                            "target_lang_translations": {
                                "ru": "К сожалению, его болезнь быстро <прогрессирует>, и врачи обеспокоены."
                            }
                        },
                        {
                            "text": "Prace budowlane <postępują> zgodnie z harmonogramem i wkrótce zostaną zakończone.",
                            "target_lang_translations": {
                                "ru": "Строительные работы <продвигаются> согласно графику и скоро будут завершены."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozwijać się",
                        "nasilać się",
                        "posuwać się"
                    ],
                    "antonyms": [
                        "cofać się",
                        "ustępować"
                    ],
                    "common_phrases": [
                        "choroba postępuje",
                        "prace postępują",
                        "czas postępuje",
                        "postępować naprzód"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться вперёд в развитии; становиться более развитым, интенсивным или занимать всё большую область (часто о процессах, болезнях, времени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прогрессировать",
                                "target_lang_sense_clarification": "о болезни, явлении или развитии"
                            },
                            {
                                "target_lang_word": "продвигаться",
                                "target_lang_sense_clarification": "о работе, процессе или движении вперёд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dffbeb2-a6f8-353c-8bcf-c97e30e3dd7c",
                    "sense_definition": "Obchodzić się z kimś lub czymś w określony sposób; traktować kogoś lub używać czegoś.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z tymi niebezpiecznymi chemikaliami należy <postępować> bardzo ostrożnie.",
                            "target_lang_translations": {
                                "ru": "С этими опасными химикатами следует <обращаться> очень осторожно."
                            }
                        },
                        {
                            "text": "Nauczyciel wiedział, jak <postępować> z trudną młodzieżą, aby zdobyć jej zaufanie.",
                            "target_lang_translations": {
                                "ru": "Учитель знал, как <обращаться> с трудными подростками, чтобы завоевать их доверие."
                            }
                        }
                    ],
                    "synonyms": [
                        "obchodzić się",
                        "traktować"
                    ],
                    "common_phrases": [
                        "postępować ostrożnie",
                        "postępować z kimś łagodnie",
                        "postępować z czymś delikatnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взаимодействовать с кем-либо или чем-либо определённым образом; относиться к кому-либо или использовать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обращаться",
                                "target_lang_sense_clarification": "с кем-либо или чем-либо"
                            },
                            {
                                "target_lang_word": "обходиться",
                                "target_lang_sense_clarification": "вести себя с кем-то, использовать что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d00a22f-c514-39fa-83ff-da9d6f5cd0c3",
                    "sense_definition": "Poruszać się na koniu stępem (najwolniejszym chodem konia).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "progress_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Instruktor kazał nam <postępować> przez kilka minut, aby konie mogły się rozgrzać przed galopem.",
                            "target_lang_translations": {
                                "ru": "Инструктор велел нам <идти шагом> несколько минут, чтобы лошади могли разогреться перед галопом."
                            }
                        },
                        {
                            "text": "Jeźdźcy <postępowali> powoli wzdłuż lasu, ciesząc się spokojnym spacerem.",
                            "target_lang_translations": {
                                "ru": "Всадники медленно <ехали шагом> вдоль леса, наслаждаясь спокойной прогулкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "jechać stępem"
                    ],
                    "antonyms": [
                        "galopować",
                        "kłusować"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "terminologia jeździecka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться на лошади шагом (самым медленным аллюром)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти шагом",
                                "target_lang_sense_clarification": "о лошади или всаднике"
                            },
                            {
                                "target_lang_word": "ехать шагом",
                                "target_lang_sense_clarification": "передвигаться верхом"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "frequency": "High",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Покупатели часто приходят в магазин, чтобы <w>сравнивать</w> цены на разные товары."
                        },
                        {
                            "text": "Нельзя <w>сравнивать</w> мою работу с твоей, потому что у нас были разные задачи."
                        }
                    ],
                    "synonyms": [
                        "сопоставлять"
                    ],
                    "antonyms": [
                        "отличать"
                    ],
                    "common_phrases": [

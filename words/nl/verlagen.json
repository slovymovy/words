                {
                    "sense_id": "a52c98d1-50af-3c6e-9dfd-6387958a90a7",
                    "sense_definition": "Naar een lager niveau, bedrag, aantal of peil brengen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_lowering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De regering wil de belastingen volgend jaar <w>verlagen</w> om de koopkracht te verbeteren.",
                            "target_lang_translations": {
                                "en": "The government wants to <w>lower</w> taxes next year to improve purchasing power.",
                                "ru": "Правительство хочет <w>снизить</w> налоги в следующем году, чтобы улучшить покупательную способность."
                            }
                        },
                        {
                            "text": "Je kunt je maandelijkse kosten <w>verlagen</w> door minder energie te verbruiken.",
                            "target_lang_translations": {
                                "en": "You can <w>reduce</w> your monthly costs by consuming less energy.",
                                "ru": "Вы можете <w>снизить</w> свои ежемесячные расходы, потребляя меньше энергии."
                            }
                        },
                        {
                            "text": "De winkels <w>verlagen</w> hun prijzen tijdens de grote uitverkoop.",
                            "target_lang_translations": {
                                "en": "The stores <w>lower</w> their prices during the big sale.",
                                "ru": "Магазины <w>снижают</w> цены во время большой распродажи."
                            }
                        }
                    ],
                    "synonyms": [
                        "verminderen",
                        "reduceren"
                    ],
                    "antonyms": [
                        "verhogen"
                    ],
                    "common_phrases": [
                        "prijs verlagen",
                        "belasting verlagen",
                        "drempel verlagen",
                        "stem verlagen"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring to a lower level, amount, number, or standard.",
                        "ru": "Приводить к более низкому уровню, сумме, количеству или стандарту."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lower",

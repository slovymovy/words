    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "6e27efe3-d221-3fe5-be24-b7e90dcf0da0",
                    "sense_definition": "Een uitroep om iemand of iets onmiddellijk te laten stoppen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Halt</w>! Je mag hier niet zomaar naar binnen.",
                            "target_lang_translations": {
                                "en": "<w>Stop</w>! You are not allowed to just enter here.",
                                "ru": "<w>Стой</w>! Сюда нельзя просто так входить."
                            }
                        },
                        {
                            "text": "De politieagent stak zijn hand op en riep luid: \"<w>Halt</w>!\"",
                            "target_lang_translations": {
                                "en": "The police officer raised his hand and shouted loudly: \"<w>Halt</w>!\"",
                                "ru": "Полицейский поднял руку и громко крикнул: \"<w>Стой</w>!\""
                            }
                        }
                    ],
                    "synonyms": [
                        "stop",
                        "ho"
                    ],
                    "antonyms": [
                        "vooruit"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation to make someone or something stop immediately.",
                        "ru": "Возглас, используемый для того, чтобы заставить кого-то или что-то немедленно остановиться."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "command"
                            },
                            {
                                "target_lang_word": "halt",
                                "target_lang_sense_clarification": "formal command"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "стой",
                                "target_lang_sense_clarification": "приказ остановиться"
                            },
                            {
                                "target_lang_word": "стоп",
                                "target_lang_sense_clarification": "команда прекратить действие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ea01517c-0cb0-3fe3-9a41-2e542a87db32",
                    "sense_definition": "De toestand van stilstaan of het stoppen van een beweging of actie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De trein kwam langzaam tot <w>halt</w> op het station.",
                            "target_lang_translations": {
                                "en": "The train slowly came to a <w>halt</w> at the station.",
                                "ru": "Поезд медленно <w>остановился</w> на станции."
                            }
                        },
                        {
                            "text": "De wandelaars hielden even <w>halt</w> om van het uitzicht te genieten.",
                            "target_lang_translations": {
                                "en": "The hikers <w>stopped</w> for a moment to enjoy the view.",
                                "ru": "Туристы сделали небольшой <w>привал</w>, чтобы насладиться видом."
                            }
                        },
                        {
                            "text": "We moeten deze negatieve ontwikkeling een <w>halt</w> toeroepen.",
                            "target_lang_translations": {
                                "en": "We must put a <w>stop</w> to this negative development.",
                                "ru": "Мы должны положить <w>конец</w> этому негативному развитию."
                            }
                        }
                    ],
                    "synonyms": [
                        "stilstand",
                        "stop",
                        "pauze"
                    ],
                    "antonyms": [
                        "voortgang",
                        "beweging"
                    ],
                    "common_phrases": [
                        "halt houden",
                        "een halt toeroepen",
                        "tot halt komen"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of standing still or the stopping of a movement or action.",
                        "ru": "Состояние неподвижности или прекращение движения или действия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "halt",
                                "target_lang_sense_clarification": "cessation of movement"
                            },
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "act of stopping"
                            },
                            {
                                "target_lang_word": "standstill",
                                "target_lang_sense_clarification": "complete stop"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "остановка",
                                "target_lang_sense_clarification": "прекращение движения"
                            },
                            {
                                "target_lang_word": "привал",
                                "target_lang_sense_clarification": "отдых в пути"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "hekelen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "298a1702-eebd-394e-9e5f-31eb2ff4937c",
                    "sense_definition": "Een sterk gevoel van afkeer, weerzin of haat tegen iets of iemand.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb een enorme <w>hekel</w> aan vroeg opstaan in het weekend.",
                            "target_lang_translations": {
                                "en": "I have a huge <w>dislike</w> of getting up early on the weekend."
                            }
                        },
                        {
                            "text": "Zij heeft een <w>hekel</w> aan mensen die niet de waarheid spreken.",
                            "target_lang_translations": {
                                "en": "She has a <w>dislike</w> of people who do not speak the truth."
                            }
                        }
                    ],
                    "synonyms": [
                        "afkeer",
                        "weerzin",
                        "haat",
                        "antipathie"
                    ],
                    "antonyms": [
                        "voorliefde",
                        "liefhebberij"
                    ],
                    "common_phrases": [
                        "een hekel hebben aan",
                        "een hekel krijgen aan"
                    ],
                    "target_lang_definitions": {
                        "en": "A strong feeling of aversion, repugnance, or hatred towards something or someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dislike",
                                "target_lang_sense_clarification": "feeling of distaste or aversion"
                            },
                            {
                                "target_lang_word": "aversion",
                                "target_lang_sense_clarification": "strong feeling of dislike"
                            },
                            {
                                "target_lang_word": "hatred",
                                "target_lang_sense_clarification": "intense dislike"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ead0fff-c9e4-34b9-b1f6-567da47cf2ee",
                    "sense_definition": "Een werktuig met ijzeren pinnen dat vroeger werd gebruikt om vlas of hennep te kammen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer haalde de bundel vlas over de <w>hekel</w> om de vezels recht te trekken.",
                            "target_lang_translations": {
                                "en": "The farmer pulled the bundle of flax over the <w>hackle</w> to straighten the fibers."
                            }
                        },
                        {
                            "text": "In het streekmuseum kun je zien hoe een oude <w>hekel</w> precies werkt.",
                            "target_lang_translations": {
                                "en": "In the regional museum you can see exactly how an old <w>hackle</w> works."
                            }
                        }
                    ],
                    "common_phrases": [
                        "over de hekel halen"
                    ],
                    "target_lang_definitions": {
                        "en": "A tool with iron pins formerly used to comb flax or hemp."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hackle",
                                "target_lang_sense_clarification": "tool for combing flax"
                            },
                            {
                                "target_lang_word": "hatchel",
                                "target_lang_sense_clarification": "comb for separating flax fibers"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b5b62273-8d98-3ae1-8b3a-e99dff7e2098",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>hekelen</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_hekelen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>hekel</w> de manier waarop er met onze privacy wordt omgegaan.",
                            "target_lang_translations": {
                                "en": "I <w>denounce</w> the way our privacy is handled."
                            }
                        },
                        {
                            "text": "In mijn column <w>hekel</w> ik de bureaucratie bij de overheid.",
                            "target_lang_translations": {
                                "en": "In my column I <w>criticize</w> the bureaucracy within the government."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'hekelen' (to denounce or criticize)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "denounce",
                                "target_lang_sense_clarification": "to criticize publicly or strongly"
                            },
                            {
                                "target_lang_word": "criticize",
                                "target_lang_sense_clarification": "to indicate faults in a disapproving way"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2163cb0-458a-3eeb-9570-355527fc39a0",
                    "sense_definition": "Gebiedende wijs van <w>hekelen</w>.",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_hekelen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hekel</w> niet te snel, maar probeer eerst de situatie te begrijpen.",
                            "target_lang_translations": {
                                "en": "Do not <w>criticize</w> too quickly, but try to understand the situation first."
                            }
                        },
                        {
                            "text": "<w>Hekel</w> de misstanden, maar kom ook met een oplossing.",
                            "target_lang_translations": {
                                "en": "<w>Denounce</w> the abuses, but also come up with a solution."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative mood of 'hekelen' (to denounce or criticize)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "denounce",
                                "target_lang_sense_clarification": "to condemn openly"
                            },
                            {
                                "target_lang_word": "criticize",
                                "target_lang_sense_clarification": "to find fault with"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f773f299-2149-32c2-ba74-36a0dfcbca0a",
                    "sense_definition": "Een sportwedstrijd in goede banen leiden als scheidsrechter.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bond heeft een ervaren scheidsrechter aangewezen om de finale te <w>arbitreren</w>.",
                            "target_lang_translations": {
                                "ru": "Федерация назначила опытного рефери <w>судить</w> финал."
                            }
                        },
                        {
                            "text": "Het is soms erg lastig om een wedstrijd te <w>arbitreren</w> waarin veel overtredingen worden gemaakt.",
                            "target_lang_translations": {
                                "ru": "Иногда очень сложно <w>судить</w> матч, в котором совершается много нарушений."
                            }
                        }
                    ],
                    "synonyms": [
                        "fluiten",
                        "leiden"
                    ],
                    "common_phrases": [
                        "een wedstrijd arbitreren",
                        "de finale arbitreren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Судить спортивный матч, следить за соблюдением правил в качестве рефери."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судить",
                                "target_lang_sense_clarification": "матч, соревнование"
                            },
                            {
                                "target_lang_word": "быть судьей",
                                "target_lang_sense_clarification": "выполнять роль арбитра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6300463c-3819-31b0-989f-80ac599e6b60",
                    "sense_definition": "Een juridisch of zakelijk geschil beslechten door als onpartijdige partij een bindende uitspraak te doen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "conflict_resolution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De twee bedrijven hebben besloten hun conflict te laten <w>arbitreren</w> door een onafhankelijke commissie.",
                            "target_lang_translations": {
                                "ru": "Две компании решили поручить независимой комиссии <w>разрешить</w> их конфликт в арбитражном порядке."
                            }
                        },
                        {
                            "text": "Hij werd gevraagd om te <w>arbitreren</w> in de vastgelopen onderhandelingen tussen de vakbond en de werkgever.",
                            "target_lang_translations": {
                                "ru": "Его попросили <w>выступить арбитром</w> в зашедших в тупик переговорах между профсоюзом и работодателем."
                            }
                        }
                    ],
                    "synonyms": [
                        "beslechten",
                        "bemiddelen"
                    ],
                    "common_phrases": [
                        "een geschil arbitreren",
                        "een conflict arbitreren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрешать юридический или деловой спор, вынося обязательное решение в качестве нейтральной стороны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выступать арбитром",
                                "target_lang_sense_clarification": "в споре"
                            },
                            {
                                "target_lang_word": "разрешать спор",
                                "target_lang_sense_clarification": "в арбитражном порядке"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                        {
                            "text": "Из-за громкого шума на улице я с трудом мог <разбирать> слова друга.",
                            "target_lang_translations": {
                                "en": "Due to the loud noise on the street, I could hardly <w>make out</w> my friend's words."
                            }
                        }
                    ],
                    "synonyms": [
                        "распознавать",
                        "различать",
                        "выделять"
                    ],
                    "common_phrases": [
                        "с трудом разбирать",
                        "разбирать почерк",
                        "разбирать слова",
                        "разбирать дорогу"
                    ],
                    "target_lang_definitions": {

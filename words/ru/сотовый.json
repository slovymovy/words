        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a3ee7fa3-4fd0-38d3-bf94-cce6ed46f651",
                    "sense_definition": "Относящийся к мобильной радиотелефонной связи, зона покрытия которой делится на ячейки (соты).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mobile_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня разрядился <w>сотовый</w> телефон, и я не смог позвонить.",
                            "target_lang_translations": {
                                "en": "My <w>cell</w> phone battery died, and I couldn't make a call."
                            }
                        },
                        {
                            "text": "В этом районе города <w>сотовая</w> связь работает очень стабильно.",
                            "target_lang_translations": {
                                "en": "In this area of the city, <w>cellular</w> service works very stably."
                            }
                        }
                    ],
                    "synonyms": [
                        "мобильный"
                    ],
                    "antonyms": [
                        "стационарный"
                    ],
                    "common_phrases": [
                        "сотовый телефон",
                        "сотовая связь",
                        "сотовый оператор",
                        "сотовая сеть"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to a mobile telephone system that uses a network of short-range transmitters in overlapping zones or cells."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cellular",
                                "target_lang_sense_clarification": "relating to mobile network technology"
                            },
                            {
                                "target_lang_word": "mobile",
                                "target_lang_sense_clarification": "common synonym for cellular communication"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a633c276-9269-3473-a4a4-d8682e1aa548",
                    "sense_definition": "Напоминающий по строению пчелиные соты; состоящий из множества ячеек.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "honeycomb_structure",
                    "name_type": "no",
                    "examples": [

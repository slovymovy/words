    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "817addf9-9849-3909-93ce-4eba941dcf1b",
                    "sense_definition": "A person who tends, herds, feeds, or guards sheep.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tending_and_guiding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old <w>shepherd</w> sat on the hill watching over his grazing flock.",
                            "target_lang_translations": {
                                "ru": "Старый <w>пастух</w> сидел на холме, присматривая за своим пасущимся стадом."
                            }
                        },
                        {
                            "text": "A young <w>shepherd</w> boy guided the animals safely into the pen before nightfall.",
                            "target_lang_translations": {
                                "ru": "Молодой мальчик-<w>пастух</w> загнал животных в загон до наступления темноты."
                            }
                        }
                    ],
                    "synonyms": [
                        "pastor",
                        "pastor"
                    ],
                    "common_phrases": [
                        "good shepherd",
                        "shepherd boy"
                    ],
                    "target_lang_definitions": {

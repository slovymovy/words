                {
                    "sense_id": "94ad574a-6a92-3ac6-8691-f1743308c38a",
                    "sense_definition": "Overheidsdienst die belast is met de handhaving van de openbare orde en veiligheid en het opsporen van wetsovertreders.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je te hard rijdt, krijg je een bekeuring van de <w>politie</w>.",
                            "target_lang_translations": {
                                "ru": "Если ты превысишь скорость, то получишь штраф от <w>полиции</w>."
                            }
                        },
                        {
                            "text": "De <w>politie</w> stond bij hem op de stoep om vragen te stellen.",
                            "target_lang_translations": {
                                "ru": "<w>Полиция</w> стояла у него на пороге, чтобы задать вопросы."
                            }
                        }
                    ],
                    "synonyms": [
                        "popo",
                        "skotoe"
                    ],
                    "common_phrases": [
                        "de politie bellen",
                        "bij de politie",
                        "politie en justitie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государственная служба, отвечающая за поддержание общественного порядка и безопасности, а также за выявление правонарушителей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полиция",
                                "target_lang_sense_clarification": "правоохранительный орган"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe6e0805-0d2e-3ca7-aafd-90823aa93435",
                    "sense_definition": "Een ambtenaar die bij de politie werkt.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die <w>politie</w> stond op de hoek te kijken, maar hij deed verder niets.",
                            "target_lang_translations": {
                                "ru": "Тот <w>полицейский</w> стоял на углу и наблюдал, но больше ничего не делал."
                            }
                        },
                        {
                            "text": "Vraag de weg maar even aan die <w>politie</w> daar.",
                            "target_lang_translations": {
                                "ru": "Спроси-ка дорогу у того <w>полицейского</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "politieagent",
                        "diender"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "In standard Dutch, 'politieagent' is preferred for the person; 'politie' usually refers to the organization."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Должностное лицо, работающее в полиции (разговорное употребление)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полицейский",
                                "target_lang_sense_clarification": "сотрудник полиции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eeb1cea4-eb46-3adb-a749-3f8cb603aead",
                    "sense_definition": "Het bestuur van een stad of staat.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "governance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude teksten leest men soms over de goede <w>politie</w> van de stad.",
                            "target_lang_translations": {
                                "ru": "В старых текстах иногда можно прочитать о хорошем <w>управлении</w> городом."
                            }
                        },
                        {
                            "text": "De <w>politie</w> en het beheer van de openbare ruimte waren vroeger anders geregeld.",
                            "target_lang_translations": {
                                "ru": "<w>Управление</w> и контроль за общественным пространством раньше были организованы иначе."
                            }
                        }
                    ],
                    "synonyms": [
                        "bestuur",
                        "stadsbestuur"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This meaning is obsolete in modern Dutch."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Управление городом или государством (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управление",
                                "target_lang_sense_clarification": "администрация (устар.)"
                            },
                            {
                                "target_lang_word": "благочиние",
                                "target_lang_sense_clarification": "устаревший термин"
                            }
                        ]
                    }
                }
            ]

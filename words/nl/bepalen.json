    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b0f80b71-6e57-35e0-b32e-33737283ef3f",
                    "sense_definition": "Vaststellen of beslissen wat er moet gebeuren of hoe iets is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "determine_decide",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De overheid <w>bepaalt</w> de regels voor het verkeer.",
                            "target_lang_translations": {
                                "en": "The government <w>sets</w> the rules for traffic."
                            }
                        },
                        {
                            "text": "We moeten nog een datum voor het feest <w>bepalen</w>.",
                            "target_lang_translations": {
                                "en": "We still need to <w>set</w> a date for the party."
                            }
                        },
                        {
                            "text": "Je mag helemaal zelf de kleur van je nieuwe kamer <w>bepalen</w>.",
                            "target_lang_translations": {
                                "en": "You can <w>decide</w> the color of your new room all by yourself."
                            }
                        }
                    ],
                    "synonyms": [
                        "vaststellen",
                        "regelen",
                        "kiezen",
                        "beperken"
                    ],
                    "common_phrases": [
                        "de koers bepalen",
                        "een datum bepalen",
                        "de prijs bepalen",
                        "zelf bepalen"
                    ],
                    "target_lang_definitions": {
                        "en": "To establish, decide, or fix the nature or details of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "determine",
                                "target_lang_sense_clarification": "to ascertain or establish definitely"
                            },
                            {
                                "target_lang_word": "decide",
                                "target_lang_sense_clarification": "to make a choice or judgment"
                            },
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "to fix a time, price, or rule"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6b401c2-fde6-3bc2-8b1e-f1b1d7012355",
                    "sense_definition": "Zich beperken tot een specifiek onderwerp of aantal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "limit_restrict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Laten we ons tijdens de vergadering tot de feiten <w>bepalen</w>.",
                            "target_lang_translations": {
                                "en": "Let's <w>limit ourselves to</w> the facts during the meeting."
                            }
                        },
                        {
                            "text": "De spreker <w>bepaalde</w> zich tot een korte inleiding.",
                            "target_lang_translations": {
                                "en": "The speaker <w>limited</w> himself to a short introduction."
                            }
                        }
                    ],
                    "synonyms": [
                        "beperken"
                    ],
                    "common_phrases": [
                        "zich bepalen tot"
                    ],
                    "target_lang_definitions": {
                        "en": "To restrict or limit oneself to a specific subject or scope."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "limit oneself to",
                                "target_lang_sense_clarification": "to keep within a certain boundary"
                            },
                            {
                                "target_lang_word": "restrict oneself to",
                                "target_lang_sense_clarification": "to confine oneself to a specific limit"
                            },
                            {
                                "target_lang_word": "stick to",
                                "target_lang_sense_clarification": "to not deviate from a subject (informal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be95a28b-6bfd-34cd-a34d-63953544d367",
                    "sense_definition": "Een gebied afzetten of de grenzen aangeven met palen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mark_poles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer ging zijn nieuwe stuk land met houten staken <w>bepalen</w>.",
                            "target_lang_translations": {
                                "en": "The farmer went to <w>mark out</w> his new piece of land with wooden stakes."
                            }
                        },
                        {
                            "text": "Vroeger moest men de grens van het dorp nauwkeurig <w>bepalen</w>.",
                            "target_lang_translations": {
                                "en": "In the past, one had to accurately <w>demarcate</w> the village boundary."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Literal meaning related to 'paal' (pole), rarely used in modern Dutch."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To mark off an area or indicate boundaries, literally using poles."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mark out",
                                "target_lang_sense_clarification": "to define boundaries physically"
                            },
                            {
                                "target_lang_word": "demarcate",
                                "target_lang_sense_clarification": "to set the boundaries of"
                            }
                        ]
                    }
                }
            ]
        }
    ]

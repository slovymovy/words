                    "target_lang_definitions": {
                        "en": "To give a musical instrument the correct pitch and consistency of sound."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tune",
                                "target_lang_sense_clarification": "musical instruments"

                            {
                                "target_lang_word": "неприкосновенный",
                                "target_lang_sense_clarification": "защищенный законом"
                            },
                            {
                                "target_lang_word": "нечувствительный",
                                "target_lang_sense_clarification": "равнодушный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6bf6a21e-da72-32cc-8122-35aabaaa1ef0",
                    "sense_definition": "Een plotselinge, grote toeloop van mensen die allemaal tegelijk iets willen bemachtigen of bereiken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_crowd",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er ontstond gisteren een enorme <w>rush</w> op de supermarkten toen het nieuws bekend werd.",
                            "target_lang_translations": {
                                "en": "Yesterday, a huge <w>run</w> on the supermarkets started when the news broke.",
                                "ru": "Вчера, когда стало известно новость, в супермаркетах начался огромный <w>ажиотаж</w>."
                            }
                        },
                        {
                            "text": "Tijdens de uitverkoop was er een grote <w>rush</w> op de goedkope elektronica in de winkel.",
                            "target_lang_translations": {
                                "en": "During the sale, there was a big <w>rush</w> for the cheap electronics in the store.",
                                "ru": "Во время распродажи был большой <w>наплыв</w> желающих купить дешевую электронику в магазине."
                            }
                        }
                    ],
                    "synonyms": [
                        "stormloop"
                    ],
                    "common_phrases": [
                        "een rush op",
                        "de gold rush"
                    ],
                    "target_lang_definitions": {
                        "en": "A sudden, large demand or movement of people trying to obtain something simultaneously.",
                        "ru": "Внезапный большой наплыв людей, стремящихся одновременно что-то получить или достичь."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "sudden high demand (e.g., on a bank or store)"
                            },
                            {
                                "target_lang_word": "rush",
                                "target_lang_sense_clarification": "sudden movement of a crowd"
                            },
                            {

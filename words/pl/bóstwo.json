                    ],
                    "common_phrases": [
                        "bóstwo Chrystusa",
                        "dowód bóstwa",
                        "uznać bóstwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Божественная природа или сущность; свойство того, что является божественным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "божественность",

            "senses": [
                {
                    "sense_id": "b40eb2eb-0ac9-3854-81a2-1db35b03a49f",
                    "sense_definition": "Zaimek pytajny używany do zadawania pytań o tożsamość osoby.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Kto</w> to jest?",
                            "target_lang_translations": {
                                "ru": "<w>Кто</w> это?"
                            }
                        },
                        {
                            "text": "<w>Kto</w> chce iść ze mną do kina?",
                            "target_lang_translations": {
                                "ru": "<w>Кто</w> хочет пойти со мной в кино?"
                            }
                        }
                    ],
                    "antonyms": [
                        "co"
                    ],
                    "common_phrases": [
                        "kto to",
                        "kto tam",
                        "kto wie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вопросительное местоимение, используемое для вопроса о личности человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кто",
                                "target_lang_sense_clarification": "вопросительное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc4b18d7-61ad-3230-926e-81e88dea515c",
                    "sense_definition": "Zaimek względny łączący zdanie nadrzędne z podrzędnym, odnoszący się do osoby wykonującej czynność lub będącej przedmiotem opisu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wiem, <w>kto</w> to zrobił.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, <w>кто</w> это сделал."
                            }
                        },
                        {
                            "text": "Każdy, <w>kto</w> chce wejść, musi kupić bilet.",
                            "target_lang_translations": {
                                "ru": "Каждый, <w>кто</w> хочет войти, должен купить билет."
                            }
                        }
                    ],
                    "antonyms": [
                        "co"
                    ],
                    "common_phrases": [
                        "ten kto",
                        "każdy kto",
                        "wszystko jedno kto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относительное местоимение, связывающее главное предложение с придаточным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кто",
                                "target_lang_sense_clarification": "союзное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc5c0b20-0c0f-3003-ab38-baea2eaf8a09",
                    "sense_definition": "Zaimek używany w funkcjach nieokreślonych, często w konstrukcjach wyrażających brak lub istnienie osoby do wykonania czynności.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mało <w>kto</w> potrafi rozwiązać to trudne zadanie.",
                            "target_lang_translations": {
                                "ru": "Мало <w>кто</w> может решить эту трудную задачу."
                            }
                        },
                        {
                            "text": "Nawet nie ma z <w>kim</w> o tym porozmawiać.",
                            "target_lang_translations": {
                                "ru": "Даже не с <w>кем</w> об этом поговорить."
                            }
                        }
                    ],
                    "synonyms": [
                        "ktoś",
                        "ktokolwiek"
                    ],
                    "antonyms": [
                        "nikt"
                    ],
                    "common_phrases": [
                        "mało kto",
                        "nie ma kto",
                        "rzadko kto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Местоимение, используемое в неопределенных значениях или конструкциях, выражающих наличие или отсутствие субъекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кто",
                                "target_lang_sense_clarification": "в неопределенных конструкциях"
                            }
                        ]
                    }
                }
            ]
        }
    ]

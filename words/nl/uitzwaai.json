            "senses": [
                {
                    "sense_id": "f1bf06e6-8a43-3d4e-8d5e-f539462546c5",
                    "sense_definition": "Een beweging waarbij iets naar buiten zwaait, specifiek het uitzwenken van de achterkant van een voertuig in een bocht.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Houd bij het inhalen rekening met de <w>uitzwaai</w> van de bus.",
                            "target_lang_translations": {
                                "en": "When overtaking, take into account the <w>tail swing</w> of the bus.",
                                "ru": "При обгоне учитывайте <w>вынос</w> автобуса."
                            }
                        },
                        {
                            "text": "De vrachtwagen had een grote <w>uitzwaai</w> toen hij de straat inreed.",
                            "target_lang_translations": {
                                "en": "The truck had a large <w>swing-out</w> when it turned into the street.",
                                "ru": "У грузовика был большой <w>вынос</w>, когда он въезжал на улицу."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitslag"
                    ],
                    "common_phrases": [
                        "uitzwaai van de bus",
                        "grote uitzwaai"
                    ],
                    "target_lang_definitions": {
                        "en": "The outward movement of the rear part of a vehicle, such as a bus or truck, when making a turn.",
                        "ru": "Движение наружу, в частности, вынос задней части транспортного средства при повороте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tail swing",
                                "target_lang_sense_clarification": "of a vehicle"
                            },
                            {

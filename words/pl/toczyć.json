    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e1494cb3-16a1-3464-9822-dd681a661774",
                    "sense_definition": "Przesuwać okrągły przedmiot po podłożu, wprawiając go w ruch obrotowy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "rolling_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci lubią <w>toczyć</w> wielkie kule ze śniegu, żeby ulepić bałwana.",
                            "target_lang_translations": {
                                "ru": "Дети любят <w>катать</w> большие шары из снега, чтобы слепить снеговика."
                            }
                        },
                        {
                            "text": "Musieliśmy <w>toczyć</w> ciężką beczkę do piwnicy, bo była zbyt ciężka do niesienia.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <w>катить</w> тяжелую бочку в подвал, потому что она была слишком тяжелой, чтобы ее нести."
                            }
                        }
                    ],
                    "synonyms": [
                        "turlać"
                    ],
                    "common_phrases": [
                        "toczyć beczkę",
                        "toczyć kulę",
                        "toczyć kamień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать круглый предмет по поверхности, заставляя его вращаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кати́ть",
                                "target_lang_sense_clarification": "перемещать в одном направлении"
                            },
                            {
                                "target_lang_word": "ката́ть",
                                "target_lang_sense_clarification": "перемещать в разных направлениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d59b086-b1cb-30d8-8952-3e4e82a9fbca",
                    "sense_definition": "Przemieszczać się po powierzchni, wykonując obroty wokół własnej osi (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "rolling_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Piłka kopnięta przez zawodnika zaczęła <w>toczyć</w> się prosto do bramki.",
                            "target_lang_translations": {
                                "ru": "Мяч, который пнул игрок, начал <w>катиться</w> прямо в ворота."
                            }
                        },
                        {
                            "text": "Moneta upadła na podłogę i <w>toczyła</w> się przez chwilę pod stół.",
                            "target_lang_translations": {
                                "ru": "Монета упала на пол и какое-то время <w>катилась</w> под стол."
                            }
                        }
                    ],
                    "synonyms": [
                        "turlać się"
                    ],
                    "common_phrases": [
                        "toczyć się po ziemi",
                        "toczyć się w dół",
                        "koło się toczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться по поверхности, вращаясь вокруг собственной оси (о предметах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кати́ться",
                                "target_lang_sense_clarification": "двигаться вращаясь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7639b4c4-6e39-33f9-8f61-1d502a5d2e99",
                    "sense_definition": "Odbywać się, trwać lub mieć miejsce w określonym czasie i okolicznościach (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Akcja tej powieści kryminalnej <w>toczy</w> się w dziewiętnastowiecznym Londynie.",
                            "target_lang_translations": {
                                "ru": "Действие этого детективного романа <w>разворачивается</w> в Лондоне девятнадцатого века."
                            }
                        },
                        {
                            "text": "Postępowanie sądowe w tej sprawie <w>toczy</w> się już od kilku lat.",
                            "target_lang_translations": {
                                "ru": "Судебное разбирательство по этому делу <w>идет</w> уже несколько лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziać się",
                        "odbywać się",
                        "mieć miejsce"
                    ],
                    "common_phrases": [
                        "akcja się toczy",
                        "sprawa się toczy",
                        "życie się toczy",
                        "rozmowa się toczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить, длиться или иметь место в определенное время и при определенных обстоятельствах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "происходи́ть",
                                "target_lang_sense_clarification": "случаться"
                            },
                            {
                                "target_lang_word": "развора́чиваться",
                                "target_lang_sense_clarification": "о сюжете или событиях"
                            },
                            {
                                "target_lang_word": "идти́",
                                "target_lang_sense_clarification": "длиться, быть в процессе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e577db58-3833-325a-a610-31b5e98c3bb8",
                    "sense_definition": "Prowadzić działania wymagające czasu i zaangażowania, takie jak walka, spór lub rozmowa.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dwa sąsiednie kraje od wielu lat <w>toczą</w> wojnę o dostęp do morza.",
                            "target_lang_translations": {
                                "ru": "Две соседние страны уже много лет <w>ведут</w> войну за выход к морю."
                            }
                        },
                        {
                            "text": "Politycy w parlamencie często <w>toczą</w> zacięte spory o nowe ustawy.",
                            "target_lang_translations": {
                                "ru": "Политики в парламенте часто <w>ведут</w> ожесточенные споры о новых законах."
                            }
                        }
                    ],
                    "synonyms": [
                        "prowadzić",
                        "wieść",
                        "staczać"
                    ],
                    "common_phrases": [
                        "toczyć wojnę",
                        "toczyć spór",
                        "toczyć walkę",
                        "toczyć dyskusję",
                        "toczyć rozmowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести действия, требующие времени и усилий, такие как война, спор или беседа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести́",
                                "target_lang_sense_clarification": "войну, переговоры, спор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cda2d784-2d30-3b38-b8f0-83f9a2a6eba6",
                    "sense_definition": "Niszczyć coś powoli od środka lub powodować cierpienie (o chorobie, pasożytach lub negatywnych uczuciach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "destroying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nieznana choroba zaczęła <w>toczyć</w> jego organizm, powodując ogromne osłabienie.",
                            "target_lang_translations": {
                                "ru": "Неизвестная болезнь начала <w>точить</w> его организм, вызывая сильную слабость."
                            }
                        },
                        {
                            "text": "Zazdrość potrafi <w>toczyć</w> człowieka od środka i niszczyć jego relacje z bliskimi.",
                            "target_lang_translations": {
                                "ru": "Зависть может <w>глодать</w> человека изнутри и разрушать его отношения с близкими."
                            }
                        }
                    ],
                    "synonyms": [
                        "trawić"
                    ],
                    "common_phrases": [
                        "robak toczy drewno",
                        "choroba toczy organizm",
                        "smutek toczy serce"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used metaphorically regarding feelings"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Медленно разрушать изнутри или причинять страдания (о болезни, паразитах или чувствах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точи́ть",
                                "target_lang_sense_clarification": "изъедать, мучить"
                            },
                            {
                                "target_lang_word": "глода́ть",
                                "target_lang_sense_clarification": "о чувствах, совести"
                            },
                            {
                                "target_lang_word": "разъеда́ть",
                                "target_lang_sense_clarification": "разрушать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1480c3ad-83ab-3e1f-96af-68f478f576b1",
                    "sense_definition": "Poruszać się powoli, ciężko lub bezwładnie (o pojazdach lub ludziach; używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "moving_slowly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary pociąg towarowy leniwie <w>toczył</w> się po zardzewiałych szynach.",
                            "target_lang_translations": {
                                "ru": "Старый товарный поезд лениво <w>полз</w> по ржавым рельсам."
                            }
                        },
                        {
                            "text": "Zmęczony turysta powoli <w>toczył</w> się w stronę schroniska po całym dniu marszu.",
                            "target_lang_translations": {
                                "ru": "Уставший турист медленно <w>плелся</w> в сторону приюта после целого дня ходьбы."
                            }
                        }
                    ],
                    "synonyms": [
                        "człapać",
                        "telepać się"
                    ],
                    "common_phrases": [
                        "pociąg się toczy",
                        "wóz się toczy",
                        "ledwo się toczyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться медленно, тяжело или инертно (о транспорте или людях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плести́сь",
                                "target_lang_sense_clarification": "медленно идти"
                            },
                            {
                                "target_lang_word": "тащи́ться",
                                "target_lang_sense_clarification": "еле передвигаться"
                            },
                            {
                                "target_lang_word": "ползти́",
                                "target_lang_sense_clarification": "о медленном транспорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecaa65b9-ba4f-3bff-9155-cfa779634748",
                    "sense_definition": "Nadawać kształt przedmiotowi z drewna, metalu lub gliny za pomocą obracającego się narzędzia, np. tokarki.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shaping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony rzemieślnik potrafi <w>toczyć</w> piękne nogi do stołu z kawałka dębu.",
                            "target_lang_translations": {
                                "ru": "Опытный ремесленник умеет <w>вытачивать</w> красивые ножки для стола из куска дуба."
                            }
                        },
                        {
                            "text": "Garncarz usiadł przy kole, aby <w>toczyć</w> nowy dzbanek z gliny.",
                            "target_lang_translations": {
                                "ru": "Гончар сел за круг, чтобы <w>выкручивать</w> новый кувшин из глины."
                            }
                        }
                    ],
                    "synonyms": [
                        "obrabiać",
                        "wytaczać"
                    ],
                    "common_phrases": [
                        "toczyć na tokarce",
                        "toczyć w drewnie",
                        "toczyć naczynia"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать форму предмету из дерева, металла или глины с помощью вращающегося инструмента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точи́ть",
                                "target_lang_sense_clarification": "обрабатывать на станке"
                            },
                            {
                                "target_lang_word": "выта́чивать",
                                "target_lang_sense_clarification": "изготавливать точением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "653766a0-0c53-395e-b78a-45821969115b",
                    "sense_definition": "Płynąć powolnym, szerokim strumieniem (o wodzie lub rzece; używane z zaimkiem „się”).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "liquid_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wody wielkiej rzeki leniwie <w>toczyły</w> się przez nizinny krajobraz.",
                            "target_lang_translations": {
                                "ru": "Воды великой реки лениво <w>текли</w> через равнинный пейзаж."
                            }
                        },
                        {
                            "text": "Brudna woda po ulewie zaczęła <w>toczyć</w> się ulicami miasta.",
                            "target_lang_translations": {
                                "ru": "Грязная вода после ливня начала <w>течь</w> по улицам города."
                            }
                        }
                    ],
                    "synonyms": [
                        "spływać",
                        "ściekać"
                    ],
                    "common_phrases": [
                        "wody toczą się",
                        "rzeka toczy wody"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary style"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Течь медленным, широким потоком (о воде или реке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "течь",
                                "target_lang_sense_clarification": "о жидкости"
                            },
                            {
                                "target_lang_word": "кати́ть",
                                "target_lang_sense_clarification": "о волнах, водах (литературное)"
                            }
                        ]
                    }
                }
            ]
        }

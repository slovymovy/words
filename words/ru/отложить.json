    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bd5294a9-1684-3fb1-abe4-261f4c4b2651",
                    "sense_definition": "Перенести запланированное событие, дело или действие на более поздний срок.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_management",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <w>отложить</w> нашу поездку из-за плохой погоды.",
                            "target_lang_translations": {
                                "en": "We decided to <w>postpone</w> our trip due to bad weather."
                            }
                        },
                        {
                            "text": "Директор попросил <w>отложить</w> совещание на один час.",
                            "target_lang_translations": {
                                "en": "The director asked to <w>push back</w> the meeting by one hour."
                            }
                        },
                        {
                            "text": "Не стоит <w>отложить</w> на завтра то, что можно сделать сегодня.",
                            "target_lang_translations": {
                                "en": "One should not <w>put off</w> until tomorrow what can be done today."
                            }
                        }
                    ],
                    "synonyms": [
                        "перенести",
                        "отсрочить"
                    ],
                    "antonyms": [
                        "приблизить"
                    ],
                    "common_phrases": [
                        "отложить на завтра",
                        "отложить встречу",
                        "отложить решение",
                        "отложить на потом"
                    ],
                    "target_lang_definitions": {
                        "en": "To move a planned event or action to a later time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "postpone",
                                "target_lang_sense_clarification": "formal or neutral context"
                            },
                            {
                                "target_lang_word": "put off",
                                "target_lang_sense_clarification": "common phrasal verb"
                            },
                            {
                                "target_lang_word": "delay",
                                "target_lang_sense_clarification": "implies making something happen later"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d65647d0-0b95-3627-ad6c-bd99032e8859",
                    "sense_definition": "Положить предмет в сторону, перестать держать его в руках или использовать в данный момент.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она прочитала сообщение и медленно <w>отложила</w> телефон в сторону.",
                            "target_lang_translations": {
                                "en": "She read the message and slowly <w>put</w> the phone <w>aside</w>."
                            }
                        },
                        {
                            "text": "Учитель попросил всех учеников <w>отложить</w> ручки и сдать тетради.",
                            "target_lang_translations": {
                                "en": "The teacher asked all the students to <w>put down</w> their pens and hand in their notebooks."
                            }
                        }
                    ],
                    "synonyms": [
                        "убрать",
                        "отодвинуть"
                    ],
                    "common_phrases": [
                        "отложить книгу",
                        "отложить в сторону",
                        "отложить дела"
                    ],
                    "target_lang_definitions": {
                        "en": "To place an object to the side, or to stop holding or using it for the moment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put aside",
                                "target_lang_sense_clarification": "place to the side"
                            },
                            {
                                "target_lang_word": "put down",
                                "target_lang_sense_clarification": "stop holding"
                            },
                            {
                                "target_lang_word": "set aside",
                                "target_lang_sense_clarification": "place out of the way"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02cbe036-c336-3cf7-8bb7-e46f456eb3de",
                    "sense_definition": "Сберечь деньги или ресурсы, не расходуя их, для использования в будущем.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый месяц я стараюсь <w>отложить</w> немного денег на летний отпуск.",
                            "target_lang_translations": {
                                "en": "Every month I try to <w>put aside</w> a little money for summer vacation."
                            }
                        },
                        {
                            "text": "Родителям удалось <w>отложить</w> нужную сумму для покупки новой машины.",
                            "target_lang_translations": {
                                "en": "My parents managed to <w>save up</w> the necessary sum to buy a new car."
                            }
                        }
                    ],
                    "synonyms": [
                        "накопить",
                        "сберечь",
                        "приберечь"
                    ],
                    "antonyms": [
                        "потратить",
                        "истратить"
                    ],
                    "common_phrases": [
                        "отложить деньги",
                        "отложить на отпуск",
                        "отложить с зарплаты",
                        "отложить про запас"
                    ],
                    "target_lang_definitions": {
                        "en": "To keep money or resources for future use rather than spending them immediately."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "save",
                                "target_lang_sense_clarification": "accumulate money"
                            },
                            {
                                "target_lang_word": "put aside",
                                "target_lang_sense_clarification": "allocate for specific purpose"
                            },
                            {
                                "target_lang_word": "set aside",
                                "target_lang_sense_clarification": "reserve"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "071fa481-d889-39f5-ad97-9b1dd2e538fd",
                    "sense_definition": "Произвести на свет яйца или икру (о птицах, насекомых, рыбах и пресмыкающихся).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Птица выбрала безопасное место в кустах, чтобы <w>отложить</w> яйца.",
                            "target_lang_translations": {
                                "en": "The bird chose a safe place in the bushes to <w>lay</w> eggs."
                            }
                        },
                        {
                            "text": "Морские черепахи выходят на берег, чтобы <w>отложить</w> кладку в песок.",
                            "target_lang_translations": {
                                "en": "Sea turtles come out onto the shore to <w>lay</w> a clutch in the sand."
                            }

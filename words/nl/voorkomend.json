    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "065f745b-b7ad-389a-87bd-7609f6aaf1e2",
                    "sense_definition": "Wat gebeurt, bestaat of zich ergens bevindt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In <w>voorkomende</w> gevallen moet u direct contact opnemen met de huisarts.",
                            "target_lang_translations": {
                                "ru": "В <w>возникающих</w> случаях вам следует немедленно связаться с домашним врачом."
                            }
                        },
                        {
                            "text": "Griep is een veel <w>voorkomende</w> ziekte tijdens de wintermaanden.",
                            "target_lang_translations": {
                                "ru": "Грипп — это часто <w>встречающееся</w> заболевание в зимние месяцы."
                            }
                        }
                    ],
                    "synonyms": [
                        "bestaand",
                        "optredend",
                        "aanwezig"
                    ],
                    "antonyms": [
                        "ontbrekend",
                        "afwezig"
                    ],
                    "common_phrases": [
                        "in voorkomende gevallen",
                        "veel voorkomend"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что происходит, существует или находится где-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречающийся",
                                "target_lang_sense_clarification": "существующий или попадающийся где-либо"
                            },
                            {
                                "target_lang_word": "происходящий",
                                "target_lang_sense_clarification": "случающийся, имеющий место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a2116e8-7d89-32c8-a35c-3a9c0c04e7e7",
                    "sense_definition": "Hoffelijk, vriendelijk en behulpzaam in de omgang met anderen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ober was zeer <w>voorkomend</w> en hielp de gasten direct naar hun tafel.",
                            "target_lang_translations": {
                                "ru": "Официант был очень <w>предупредителен</w> и сразу проводил гостей к их столику."
                            }
                        },
                        {
                            "text": "Zij staat bekend om haar <w>voorkomende</w> karakter en helpt iedereen graag.",
                            "target_lang_translations": {
                                "ru": "Она известна своим <w>обходительным</w> характером и с радостью помогает всем."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoffelijk",
                        "attent",
                        "beleefd",
                        "vriendelijk",
                        "gedienstig"
                    ],
                    "antonyms": [
                        "onbeleefd",
                        "lomp",
                        "onvriendelijk",
                        "bot"
                    ],
                    "common_phrases": [
                        "een voorkomend mens",
                        "uiterst voorkomend"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учтивый, дружелюбный и готовый помочь в общении с другими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предупредительный",
                                "target_lang_sense_clarification": "готовый исполнить чье-либо желание, услужливый"
                            },
                            {
                                "target_lang_word": "обходительный",
                                "target_lang_sense_clarification": "вежливый и любезный в обращении"
                            },
                            {
                                "target_lang_word": "любезный",
                                "target_lang_sense_clarification": "приятный в общении, учтивый"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1928dafd-87d0-3bf6-81cf-42c4fda81826",
                    "sense_definition": "Onvoltooid deelwoord van <w>voorkomen</w> (zich voordoen, gebeuren).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De in dit gebied <w>voorkomende</w> planten zijn beschermd door de wet.",
                            "target_lang_translations": {
                                "ru": "<w>Встречающиеся</w> в этой местности растения охраняются законом."
                            }
                        },
                        {
                            "text": "Alle in de lijst <w>voorkomende</w> namen zijn alfabetisch gerangschikt.",
                            "target_lang_translations": {
                                "ru": "Все <w>встречающиеся</w> в списке имена расположены в алфавитном порядке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени от глагола \"voorkomen\" (случаться, происходить, иметься)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречающийся",
                                "target_lang_sense_clarification": "находящийся или имеющийся в наличии"
                            },
                            {
                                "target_lang_word": "фигурирующий",
                                "target_lang_sense_clarification": "присутствующий (например, в списке)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f5460be-5867-3d0f-bd3e-417e24e65f45",
                    "sense_definition": "Onvoltooid deelwoord van <w>voorkomen</w> (beletten, tegenhouden).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "prevention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij greep snel in, daarmee erger <w>voorkomend</w>.",
                            "target_lang_translations": {
                                "ru": "Он быстро вмешался, тем самым <w>предотвращая</w> худшее."
                            }
                        },
                        {
                            "text": "De politie trad op, een escalatie van het geweld <w>voorkomend</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция вмешалась, <w>предотвращая</w> эскалацию насилия."
                            }
                        }

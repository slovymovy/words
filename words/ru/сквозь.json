                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Completely, to the very end; through and through."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "through",
                                "target_lang_sense_clarification": "penetrating to the other side"
                            },
                            {
                                "target_lang_word": "through and through",

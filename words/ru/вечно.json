    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "48beb04a-eed2-3d83-af09-cff8bf5b0085",
                    "sense_definition": "В течение бесконечного времени; всегда, без конца.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "forever",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ничто в этом мире не длится <w>вечно</w>.",
                            "target_lang_translations": {
                                "en": "Nothing in this world lasts <w>forever</w>."
                            }
                        },
                        {
                            "text": "Мы будем <w>вечно</w> помнить подвиг героев.",
                            "target_lang_translations": {
                                "en": "We will <w>forever</w> remember the heroes' feat."
                            }
                        }
                    ],
                    "synonyms": [
                        "всегда",
                        "бесконечно",
                        "вовек",
                        "во веки веков"
                    ],
                    "antonyms": [
                        "временно"
                    ],
                    "common_phrases": [
                        "жить вечно",
                        "помнить вечно",
                        "вечно молодой"
                    ],
                    "target_lang_definitions": {
                        "en": "For an infinite amount of time; always, without end."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forever",
                                "target_lang_sense_clarification": "for all future time"
                            },
                            {
                                "target_lang_word": "eternally",
                                "target_lang_sense_clarification": "in a way that continues forever"
                            },
                            {
                                "target_lang_word": "always",
                                "target_lang_sense_clarification": "at all times; forever"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abe92884-205c-35bf-a9e3-9b64bed70c6e",
                    "sense_definition": "Постоянно, очень часто (обычно с оттенком недовольства или раздражения).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "constantly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты <w>вечно</w> опаздываешь на важные встречи.",
                            "target_lang_translations": {
                                "en": "You are <w>always</w> late for important meetings."
                            }
                        },
                        {
                            "text": "У него <w>вечно</w> не хватает денег до зарплаты.",
                            "target_lang_translations": {
                                "en": "He is <w>always</w> short of money before payday."
                            }
                        }
                    ],
                    "synonyms": [
                        "постоянно",
                        "то и дело"
                    ],
                    "common_phrases": [
                        "вечно ты",
                        "вечно недоволен"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Constantly, very often (usually with a tone of dissatisfaction or irritation)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "always",
                                "target_lang_sense_clarification": "very frequently (often used for complaints)"
                            },
                            {
                                "target_lang_word": "forever",
                                "target_lang_sense_clarification": "continually (often used for complaints)"
                            },
                            {
                                "target_lang_word": "perpetually",
                                "target_lang_sense_clarification": "constantly; in a way that never ends"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "00d2c453-dc44-3c0c-aa7b-3a72871fca67",
                    "sense_definition": "Краткая форма среднего рода прилагательного <w>вечный</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Искусство <w>вечно</w>, а жизнь человека коротка.",
                            "target_lang_translations": {
                                "en": "Art is <w>eternal</w>, but human life is short."
                            }
                        },
                        {
                            "text": "Это правило <w>вечно</w> и не подлежит изменению.",
                            "target_lang_translations": {
                                "en": "This rule is <w>eternal</w> and is not subject to change."
                            }
                        }
                    ],
                    "traits": [
                        {

{
    "word_family": [
        "сиюминутный"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "86059bce-2e55-3441-b5a5-fb4ef1f90738",
                    "sense_definition": "В данный момент, в то время, когда происходит речь.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_present",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>сейчас</w> очень занят и не могу говорить.",
                            "target_lang_translations": {
                                "en": "I am very busy <w>now</w> and cannot speak."
                            }
                        },
                        {
                            "text": "Где ты <w>сейчас</w> находишься?",
                            "target_lang_translations": {
                                "en": "Where are you <w>now</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "тепе́рь",
                        "в настоя́щее вре́мя"
                    ],
                    "antonyms": [
                        "тогда"
                    ],
                    "common_phrases": [
                        "прямо сейчас",
                        "именно сейчас",
                        "а сейчас"
                    ],
                    "target_lang_definitions": {
                        "en": "At the present moment, at the time of speaking."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "now",
                                "target_lang_sense_clarification": "at this moment"
                            },
                            {
                                "target_lang_word": "currently",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "at the moment",
                                "target_lang_sense_clarification": "phrase"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9f329a5-2ae0-38a2-a61f-233502af134e",
                    "sense_definition": "Очень скоро, через минуту, в ближайшем будущем.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_future",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Подожди немного, я <w>сейчас</w> приду.",
                            "target_lang_translations": {
                                "en": "Wait a bit, I'll come <w>in a moment</w>."
                            }
                        },
                        {
                            "text": "Мы <w>сейчас</w> всё исправим, не волнуйтесь.",
                            "target_lang_translations": {
                                "en": "We will fix everything <w>right away</w>, don't worry."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сейчас вернусь",
                        "сейчас приду",
                        "сейчас сделаю"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies immediate future action"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Very soon, in a minute, in the immediate future."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "right away",
                                "target_lang_sense_clarification": "immediately"
                            },
                            {
                                "target_lang_word": "in a moment",
                                "target_lang_sense_clarification": "very soon"
                            },
                            {
                                "target_lang_word": "soon",
                                "target_lang_sense_clarification": "shortly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a112c481-fef0-3771-83a3-012afc4b4b32",
                    "sense_definition": "В настоящее время, в наши дни (о длительном периоде времени).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_present",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Раньше здесь был лес, а <w>сейчас</w> построили парк.",
                            "target_lang_translations": {
                                "en": "There used to be a forest here, but <w>now</w> they have built a park."
                            }
                        },
                        {
                            "text": "<w>Сейчас</w> многие люди работают из дома.",
                            "target_lang_translations": {
                                "en": "<w>Nowadays</w> many people work from home."
                            }
                        }
                    ],
                    "synonyms": [
                        "тепе́рь",
                        "в настоя́щее вре́мя"
                    ],
                    "antonyms": [
                        "давно"
                    ],
                    "common_phrases": [
                        "до сейчас",
                        "как сейчас"
                    ],
                    "target_lang_definitions": {
                        "en": "At the present time, these days (referring to a longer period)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nowadays",
                                "target_lang_sense_clarification": "in the current era"
                            },
                            {
                                "target_lang_word": "now",
                                "target_lang_sense_clarification": "in contrast to the past"
                            },
                            {
                                "target_lang_word": "these days",
                                "target_lang_sense_clarification": "current period"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df7569a1-356a-32f2-b538-dcd467ea7f15",
                    "sense_definition": "Только что, совсем недавно (о прошлом).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_past",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>сейчас</w> встретил твоего брата на улице.",
                            "target_lang_translations": {
                                "en": "I <w>just</w> met your brother on the street."
                            }
                        },
                        {
                            "text": "Ты <w>сейчас</w> сказал что-то очень важное.",
                            "target_lang_translations": {
                                "en": "You <w>just</w> said something very important."
                            }
                        }
                    ],
                    "synonyms": [
                        "то́лько что"
                    ],
                    "antonyms": [
                        "давно"
                    ],
                    "common_phrases": [
                        "только сейчас"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Just now, a moment ago (referring to the immediate past)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "just",
                                "target_lang_sense_clarification": "very recently"
                            },
                            {
                                "target_lang_word": "just now",
                                "target_lang_sense_clarification": "a moment ago"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "062a9289-3bd5-3601-a19c-867746238c73",
                    "sense_definition": "Сразу, с первого взгляда.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "По его лицу <w>сейчас</w> видно, что он говорит правду.",
                            "target_lang_translations": {
                                "en": "It is visible <w>at once</w> from his face that he is telling the truth."
                            }
                        },
                        {
                            "text": "Это <w>сейчас</w> понятно, но тогда мы ничего не знали.",
                            "target_lang_translations": {
                                "en": "It is clear <w>now</w>, but back then we knew nothing."
                            }
                        }
                    ],
                    "synonyms": [
                        "сра́зу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Immediately, at first glance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at once",
                                "target_lang_sense_clarification": "immediately"
                            },
                            {
                                "target_lang_word": "immediately",
                                "target_lang_sense_clarification": "without delay"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "3975fda8-0e9e-3232-b610-c22d6748d3db",
                    "sense_definition": "Короткий ответ, означающий просьбу подождать или обещание сделать что-то немедленно.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "interjection_wait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама, иди сюда! — <w>Сейчас</w>, иду!",
                            "target_lang_translations": {
                                "en": "Mom, come here! — <w>Coming</w>, I'm on my way!"
                            }
                        },
                        {
                            "text": "Ты готов выходить? — <w>Сейчас</w>, ещё одну минуту.",
                            "target_lang_translations": {
                                "en": "Are you ready to go out? — <w>Just a minute</w>, one more minute."
                            }
                        }
                    ],
                    "synonyms": [
                        "погоди́",
                        "подожди́"
                    ],
                    "common_phrases": [
                        "да сейчас",
                        "ну сейчас"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A short response requesting a wait or promising immediate action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "coming",
                                "target_lang_sense_clarification": "response to a call"
                            },
                            {
                                "target_lang_word": "just a minute",
                                "target_lang_sense_clarification": "request to wait"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
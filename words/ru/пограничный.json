                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач диагностировал у пациента <w>пограничное</w> расстройство личности.",
                            "target_lang_translations": {

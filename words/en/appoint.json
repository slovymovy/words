{
    "word_family": [
        "appointment",
        "appointee",
        "reappoint"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "519b0c07-836c-322f-9bd8-c0a153956353",
                    "sense_definition": "To choose someone officially for a job or responsibility.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "assign_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The board voted to <w>appoint</w> a new director next month.",
                            "target_lang_translations": {
                                "ru": "Совет проголосовал за то, чтобы <w>назначить</w> нового директора в следующем месяце."
                            }
                        },
                        {
                            "text": "She was <w>appointed</w> as the head of the committee after the chairman resigned.",
                            "target_lang_translations": {
                                "ru": "Она была <w>назначена</w> главой комитета после отставки председателя."
                            }
                        }
                    ],
                    "synonyms": [
                        "nominate",
                        "designate",
                        "assign",
                        "choose",
                        "elect",
                        "install",
                        "tap",
                        "delegate"
                    ],
                    "antonyms": [
                        "dismiss",
                        "fire",
                        "terminate"
                    ],
                    "common_phrases": [
                        "appoint a successor",
                        "appoint a committee",
                        "appoint a guardian",
                        "appoint to a post"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально выбрать кого-то на должность или для выполнения обязанностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "назначать",
                                "target_lang_sense_clarification": "на должность"
                            },
                            {
                                "target_lang_word": "определять",
                                "target_lang_sense_clarification": "направлять на работу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a8976b3-2f0b-3093-9b10-a2998947d337",
                    "sense_definition": "To decide on a specific time or place for an event.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "fix_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>appoint</w> a date for the wedding so guests can make arrangements.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>назначить</w> дату свадьбы, чтобы гости могли подготовиться."
                            }
                        },
                        {
                            "text": "Everyone arrived promptly at the <w>appointed</w> time to discuss the contract.",
                            "target_lang_translations": {
                                "ru": "Все прибыли точно в <w>назначенное</w> время, чтобы обсудить контракт."
                            }
                        }
                    ],
                    "synonyms": [
                        "settle",
                        "decide",
                        "settle"
                    ],
                    "common_phrases": [
                        "appoint a time",
                        "appoint a day",
                        "appoint a place",
                        "at the appointed time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определить конкретное время или место для события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "назначать",
                                "target_lang_sense_clarification": "время или встречу"
                            },
                            {
                                "target_lang_word": "устанавливать",
                                "target_lang_sense_clarification": "срок или дату"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e1f2c76-36d0-30a0-b71d-77312013e04b",
                    "sense_definition": "To provide a place with necessary furniture or equipment.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "equip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hotel room was beautifully <w>appointed</w> with antique furniture and modern amenities.",
                            "target_lang_translations": {
                                "ru": "Номер в отеле был прекрасно <w>обставлен</w> антикварной мебелью и оснащен современными удобствами."
                            }
                        },
                        {
                            "text": "It costs a significant amount of money to <w>appoint</w> a house with such high-quality goods.",
                            "target_lang_translations": {
                                "ru": "Требуется значительная сумма денег, чтобы <w>обставить</w> дом такими высококачественными вещами."
                            }
                        }
                    ],
                    "synonyms": [
                        "furnish",
                        "equip"
                    ],
                    "antonyms": [
                        "unfurnish"
                    ],
                    "common_phrases": [
                        "well-appointed room",
                        "elegantly appointed",
                        "luxuriously appointed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабдить помещение необходимой мебелью или оборудованием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обставлять",
                                "target_lang_sense_clarification": "мебелью"
                            },
                            {
                                "target_lang_word": "оборудовать",
                                "target_lang_sense_clarification": "оснащать всем необходимым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78e973ea-1e4a-3ff9-9312-2aa845233250",
                    "sense_definition": "To assign property or rights to someone by a legal deed.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "assign_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The will gave her the power to <w>appoint</w> the funds to her children.",
                            "target_lang_translations": {
                                "ru": "Завещание давало ей право <w>передать</w> средства своим детям."
                            }
                        },
                        {
                            "text": "He can <w>appoint</w> the property to whomever he chooses under the terms of the trust.",
                            "target_lang_translations": {
                                "ru": "Он может <w>передать</w> имущество любому, кого выберет, согласно условиям траста."
                            }
                        }
                    ],
                    "synonyms": [
                        "assign",
                        "allot"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать имущество или права кому-либо по юридическому акту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "права или имущество"
                            },
                            {
                                "target_lang_word": "распределять",
                                "target_lang_sense_clarification": "средства согласно завещанию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38e9a417-e621-3f58-b58d-e4b4c257e14f",
                    "sense_definition": "To establish or order by authority.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "ordain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king <w>appointed</w> that the festival be held annually throughout the realm.",
                            "target_lang_translations": {
                                "ru": "Король <w>постановил</w>, чтобы фестиваль проводился ежегодно по всему королевству."
                            }
                        },
                        {
                            "text": "Fate had <w>appointed</w> him to be a leader of men during the crisis.",
                            "target_lang_translations": {
                                "ru": "Судьба <w>предопределила</w> ему стать лидером людей во время кризиса."
                            }
                        }
                    ],
                    "synonyms": [
                        "decree",
                        "ordain",
                        "command",
                        "enjoin"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устанавливать или предписывать властью; предопределять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предписывать",
                                "target_lang_sense_clarification": "устанавливать указом"
                            },
                            {
                                "target_lang_word": "предопределять",
                                "target_lang_sense_clarification": "о судьбе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "totaliteit",
        "totalitair",
        "totaliseren"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6290cb61-f650-389b-b7e7-9efb32cee4cb",
                    "sense_definition": "Omvattend alles of iedereen; zonder dat er iets ontbreekt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>totale</w> opbrengst van de actie gaat naar het goede doel.",
                            "target_lang_translations": {
                                "en": "The <w>total</w> proceeds of the campaign go to charity.",
                                "ru": "<w>Общая</w> выручка от акции пойдет на благотворительность."
                            }
                        },
                        {
                            "text": "We hebben de <w>totale</w> afstand in drie dagen afgelegd.",
                            "target_lang_translations": {
                                "en": "We covered the <w>total</w> distance in three days.",
                                "ru": "Мы преодолели <w>общее</w> расстояние за три дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "volledig",
                        "compleet"
                    ],
                    "antonyms": [
                        "gedeeltelijk"
                    ],
                    "common_phrases": [
                        "totale lengte",
                        "totale waarde"
                    ],
                    "target_lang_definitions": {
                        "en": "Encompassing everything or everyone; with nothing missing.",
                        "ru": "Охватывающий всё или всех; без исключений."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "total",
                                "target_lang_sense_clarification": "relating to the whole amount or group"
                            },
                            {
                                "target_lang_word": "entire",
                                "target_lang_sense_clarification": "whole and complete"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "общий",
                                "target_lang_sense_clarification": "относящийся ко всему количеству или группе"
                            },
                            {
                                "target_lang_word": "полный",
                                "target_lang_sense_clarification": "целый, без недостающих частей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f19b8f6e-a414-3670-b979-7f5a2e5f5f85",
                    "sense_definition": "Volkomen; in de hoogste mate (vaak gebruikt om iets negatiefs te benadrukken).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het feest was een <w>totale</w> mislukking door het slechte weer.",
                            "target_lang_translations": {
                                "en": "The party was a <w>total</w> failure due to the bad weather.",
                                "ru": "Вечеринка стала <w>полным</w> провалом из-за плохой погоды."
                            }
                        },
                        {
                            "text": "Er heerste een <w>totale</w> chaos in het centrum van de stad.",
                            "target_lang_translations": {
                                "en": "There was <w>total</w> chaos in the city center.",
                                "ru": "В центре города царил <w>полный</w> хаос."
                            }
                        }
                    ],
                    "synonyms": [
                        "volledig",
                        "compleet"
                    ],
                    "common_phrases": [
                        "totale onzin",
                        "totale chaos"
                    ],
                    "target_lang_definitions": {
                        "en": "Complete; to the highest degree (often used to emphasize something negative).",
                        "ru": "Полный; в высшей степени (часто используется для усиления негативного значения)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "total",
                                "target_lang_sense_clarification": "absolute or complete"
                            },
                            {
                                "target_lang_word": "complete",
                                "target_lang_sense_clarification": "having all necessary parts or qualities"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "полный",
                                "target_lang_sense_clarification": "абсолютный или совершенный"
                            },
                            {
                                "target_lang_word": "абсолютный",
                                "target_lang_sense_clarification": "безусловный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b023b340-1d9f-3e80-b994-d236d1a0c2c7",
                    "sense_definition": "Helemaal; in alle opzichten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben het <w>totaal</w> niet eens met jouw beslissing.",
                            "target_lang_translations": {
                                "en": "I <w>totally</w> disagree with your decision.",
                                "ru": "Я <w>совершенно</w> не согласен с твоим решением."
                            }
                        },
                        {
                            "text": "Het huis zag er <w>totaal</w> anders uit na de verbouwing.",
                            "target_lang_translations": {
                                "en": "The house looked <w>totally</w> different after the renovation.",
                                "ru": "Дом выглядел <w>совершенно</w> иначе после ремонта."
                            }
                        }
                    ],
                    "synonyms": [
                        "helemaal",
                        "volledig",
                        "compleet"
                    ],
                    "antonyms": [
                        "gedeeltelijk",
                        "enigszins"
                    ],
                    "common_phrases": [
                        "totaal niet",
                        "totaal anders"
                    ],
                    "target_lang_definitions": {
                        "en": "Completely; in all respects.",
                        "ru": "Совершенно; во всех отношениях."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "totally",
                                "target_lang_sense_clarification": "completely"
                            },
                            {
                                "target_lang_word": "completely",
                                "target_lang_sense_clarification": "in every way"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "совершенно",
                                "target_lang_sense_clarification": "полностью, совсем"
                            },
                            {
                                "target_lang_word": "полностью",
                                "target_lang_sense_clarification": "целиком"
                            },
                            {
                                "target_lang_word": "совсем",
                                "target_lang_sense_clarification": "употребляется для усиления, часто с отрицанием"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "067065d7-400f-3226-96c8-3bb8be9df956",
                    "sense_definition": "De som van alle getallen of onderdelen bij elkaar.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>totaal</w> van de rekening was hoger dan we dachten.",
                            "target_lang_translations": {
                                "en": "The <w>total</w> of the bill was higher than we thought.",
                                "ru": "<w>Сумма</w> счета оказалась выше, чем мы думали."
                            }
                        },
                        {
                            "text": "In <w>totaal</w> hebben we vijf boeken verkocht.",
                            "target_lang_translations": {
                                "en": "In <w>total</w> we sold five books.",
                                "ru": "<w>Всего</w> мы продали пять книг."
                            }
                        }
                    ],
                    "antonyms": [
                        "deel"
                    ],
                    "common_phrases": [
                        "in totaal",
                        "het totaal"
                    ],
                    "target_lang_definitions": {
                        "en": "The sum of all numbers or parts added together.",
                        "ru": "Сумма всех чисел или частей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "total",
                                "target_lang_sense_clarification": "the final amount"
                            },
                            {
                                "target_lang_word": "sum",
                                "target_lang_sense_clarification": "result of addition"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сумма",
                                "target_lang_sense_clarification": "результат сложения"
                            },
                            {
                                "target_lang_word": "итог",
                                "target_lang_sense_clarification": "общий результат"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
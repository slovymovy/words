{
    "word_family": [
        "piper",
        "piping",
        "pipeline"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e6757b04-1018-3a2e-850a-a94139813b59",
                    "sense_definition": "A long, hollow tube used to transport liquids, gases, or other substances.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "conduit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The plumber came to fix a leaking <w>pipe</w> in the kitchen.",
                            "target_lang_translations": {
                                "ru": "Сантехник пришел починить протекающую <w>трубу</w> на кухне."
                            }
                        },
                        {
                            "text": "Water flows through a large <w>pipe</w> under the street.",
                            "target_lang_translations": {
                                "ru": "Вода течет по большой <w>трубе</w> под улицей."
                            }
                        }
                    ],
                    "synonyms": [
                        "tube"
                    ],
                    "common_phrases": [
                        "water pipe",
                        "gas pipe",
                        "drain pipe",
                        "burst pipe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная полая трубка, используемая для транспортировки жидкостей, газов или других веществ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "труба",
                                "target_lang_sense_clarification": "полый предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32404c22-4dc1-3723-ae0a-198282a0ade8",
                    "sense_definition": "A device used for smoking tobacco, consisting of a hollow stem with a bowl at one end.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "smoking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old man sat on the porch to smoke a <w>pipe</w>.",
                            "target_lang_translations": {
                                "ru": "Старик сидел на крыльце, чтобы выкурить <w>трубку</w>."
                            }
                        },
                        {
                            "text": "He filled his <w>pipe</w> with fresh tobacco.",
                            "target_lang_translations": {
                                "ru": "Он набил свою <w>трубку</w> свежим табаком."
                            }
                        }
                    ],
                    "common_phrases": [
                        "smoke a pipe",
                        "tobacco pipe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приспособление для курения табака, состоящее из полого мундштука с чашечкой на конце."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трубка",
                                "target_lang_sense_clarification": "для курения"
                            },
                            {
                                "target_lang_word": "курительная трубка",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "641f16d4-24ca-3fe7-9019-6036209590ad",
                    "sense_definition": "A musical wind instrument consisting of a tube with holes that is played by blowing into it.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shepherd played a simple tune on a wooden <w>pipe</w>.",
                            "target_lang_translations": {
                                "ru": "Пастух играл простую мелодию на деревянной <w>свирели</w>."
                            }
                        },
                        {
                            "text": "You can hear the sound of the <w>pipes</w> from across the valley.",
                            "target_lang_translations": {
                                "ru": "Звуки <w>свирелей</w> слышны с другой стороны долины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "play the pipes",
                        "pan pipe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальный духовой инструмент, состоящий из трубки с отверстиями, на котором играют, дуя в него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свирель",
                                "target_lang_sense_clarification": "народный инструмент"
                            },
                            {
                                "target_lang_word": "дудка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e716f93-0a1f-3e1d-ad02-3c9c142f535f",
                    "sense_definition": "The vertical bar character (|) used in computing and mathematics.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "computing_char",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to type a <w>pipe</w> to separate the commands.",
                            "target_lang_translations": {
                                "ru": "Вам нужно ввести <w>вертикальную черту</w>, чтобы разделить команды."
                            }
                        },
                        {
                            "text": "The <w>pipe</w> symbol is located above the backslash key.",
                            "target_lang_translations": {
                                "ru": "Символ <w>вертикальной черты</w> находится над клавишей обратной косой черты."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertical bar",
                        "vertical line",
                        "bar"
                    ],
                    "common_phrases": [
                        "pipe character",
                        "pipe symbol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Символ вертикальной черты (|), используемый в информатике и математике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вертикальная черта",
                                "target_lang_sense_clarification": "символ"
                            },
                            {
                                "target_lang_word": "пайп",
                                "target_lang_sense_clarification": "компьютерный сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b3e613f-598f-378c-b917-7d4c8ffceb7c",
                    "sense_definition": "A mechanism in computing that allows the output of one program to be used as the input for another.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing_mech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Developers use a <w>pipe</w> to connect multiple processes.",
                            "target_lang_translations": {
                                "ru": "Разработчики используют <w>канал</w> для соединения нескольких процессов."
                            }
                        },
                        {
                            "text": "The data is sent through a <w>pipe</w> to the logging system.",
                            "target_lang_translations": {
                                "ru": "Данные отправляются через <w>канал</w> в систему логирования."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Механизм в информатике, позволяющий использовать вывод одной программы в качестве ввода для другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канал",
                                "target_lang_sense_clarification": "передача данных"
                            },
                            {
                                "target_lang_word": "конвейер",
                                "target_lang_sense_clarification": "последовательность процессов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af85100e-1468-3d3c-944b-0af3330ebd62",
                    "sense_definition": "A vertical channel in the Earth's crust through which magma has passed.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Diamonds are often mined from a volcanic <w>pipe</w>.",
                            "target_lang_translations": {
                                "ru": "Алмазы часто добывают из вулканической <w>трубки</w>."
                            }
                        },
                        {
                            "text": "Geologists studied the ancient <w>pipe</w> to understand the eruption.",
                            "target_lang_translations": {
                                "ru": "Геологи изучали древнюю <w>трубку</w>, чтобы понять извержение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "volcanic pipe",
                        "kimberlite pipe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикальный канал в земной коре, через который проходила магма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трубка",
                                "target_lang_sense_clarification": "геологическая"
                            },
                            {
                                "target_lang_word": "кимберлитовая трубка",
                                "target_lang_sense_clarification": "месторождение алмазов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7a007622-4eed-30a6-b175-9af9b1b62d51",
                    "sense_definition": "To transport a substance, such as water, gas, or oil, through a pipe.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conduit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city plans to <w>pipe</w> fresh water from the mountain reservoir.",
                            "target_lang_translations": {
                                "ru": "Город планирует <w>подавать</w> пресную воду из горного водохранилища по трубам."
                            }
                        },
                        {
                            "text": "Natural gas is <w>piped</w> directly into homes for heating.",
                            "target_lang_translations": {
                                "ru": "Природный газ <w>подается</w> по трубам прямо в дома для отопления."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pipe water",
                        "pipe gas",
                        "pipe in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Транспортировать вещество, такое как вода, газ или нефть, по трубе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перекачивать",
                                "target_lang_sense_clarification": "транспортировать"
                            },
                            {
                                "target_lang_word": "подавать по трубам",
                                "target_lang_sense_clarification": "снабжать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2e2faa1-15fb-3600-a131-8b3d80b2faad",
                    "sense_definition": "To decorate food, such as a cake, by squeezing a soft substance like icing through a nozzle.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "decorating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baker began to <w>pipe</w> intricate flowers onto the wedding cake.",
                            "target_lang_translations": {
                                "ru": "Пекарь начал <w>выдавливать</w> сложные цветы на свадебный торт."
                            }
                        },
                        {
                            "text": "You can <w>pipe</w> whipped cream on top of the dessert for a nice finish.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>выдавить</w> взбитые сливки сверху на десерт для красивого завершения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pipe icing",
                        "pipe cream"
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшать еду, например торт, выдавливая мягкую субстанцию, такую как глазурь, через насадку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдавливать",
                                "target_lang_sense_clarification": "кулинария"
                            },
                            {
                                "target_lang_word": "наносить узор",
                                "target_lang_sense_clarification": "украшать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d540f7c9-4696-3794-952e-5ab68f21453a",
                    "sense_definition": "To send the output of one computer program directly to another program as input.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing_mech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>pipe</w> the search results into a text file for later review.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>перенаправить</w> результаты поиска в текстовый файл для последующего просмотра."
                            }
                        },
                        {
                            "text": "The script allows you to <w>pipe</w> data between the two applications.",
                            "target_lang_translations": {
                                "ru": "Скрипт позволяет <w>передавать</w> данные между двумя приложениями."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправлять вывод одной компьютерной программы непосредственно в другую программу в качестве ввода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перенаправлять",
                                "target_lang_sense_clarification": "поток данных"
                            },
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "между программами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "caa46a74-37aa-3f7c-bae2-fcc51e808334",
                    "sense_definition": "To play music on a pipe instrument, or to make a high-pitched sound.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The birds began to <w>pipe</w> their morning songs.",
                            "target_lang_translations": {
                                "ru": "Птицы начали <w>щебетать</w> свои утренние песни."
                            }
                        },
                        {
                            "text": "He <w>piped</w> a cheerful tune while walking down the road.",
                            "target_lang_translations": {
                                "ru": "Он <w>насвистывал</w> веселую мелодию, идя по дороге."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pipe up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Играть музыку на духовом инструменте или издавать высокий звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свистеть",
                                "target_lang_sense_clarification": "издавать звук"
                            },
                            {
                                "target_lang_word": "играть на свирели",
                                "target_lang_sense_clarification": "музыка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "28532045-1b9d-30bf-a4ea-88123e98ea18",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "We invited Mr. <w>Pipe</w> to the meeting.",
                            "target_lang_translations": {
                                "ru": "Мы пригласили мистера <w>Пайпа</w> на встречу."
                            }

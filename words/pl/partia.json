            "senses": [
                {
                    "sense_id": "b2a2deac-cfe3-3000-87e2-e2e8d41ffa5d",
                    "sense_definition": "Organizacja polityczna zrzeszająca ludzi o podobnych poglądach, dążąca do zdobycia władzy w państwie lub regionie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój wujek zapisał się do nowej <w>partii</w> przed wyborami.",
                            "target_lang_translations": {
                                "ru": "Мой дядя записался в новую <w>партию</w> перед выборами."
                            }
                        },
                        {
                            "text": "Ta <w>partia</w> obiecuje obniżenie podatków dla wszystkich obywateli.",
                            "target_lang_translations": {
                                "ru": "Эта <w>партия</w> обещает снижение налогов для всех граждан."
                            }
                        }
                    ],
                    "synonyms": [
                        "ugrupowanie",
                        "stronnictwo"
                    ],
                    "common_phrases": [
                        "partia rządząca",
                        "partia opozycyjna",
                        "założyć partię",
                        "wstąpić do partii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Политическая организация, объединяющая людей с общими взглядами и стремящаяся к власти в государстве или регионе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "партия",
                                "target_lang_sense_clarification": "политическая организация"

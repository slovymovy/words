            "senses": [
                {
                    "sense_id": "3b468a11-52a2-3924-ac4c-f16e781830dd",
                    "sense_definition": "Postanowić o wspólnym spotkaniu z kimś w określonym miejscu i czasie (używane z zaimkiem „się”).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "meeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj <umówiłem> się z najlepszym przyjacielem na kawę w centrum.",
                            "target_lang_translations": {
                                "ru": "Вчера я <договорился> с лучшим другом выпить кофе в центре."
                            }
                        },
                        {
                            "text": "Musisz <umówić> się do lekarza, jeśli ten ból nie minie do jutra.",
                            "target_lang_translations": {
                                "ru": "Ты должен <записаться> к врачу, если эта боль не пройдет до завтра."
                            }
                        }
                    ],
                    "common_phrases": [
                        "umówić się na randkę",
                        "umówić się do lekarza",
                        "umówić się ze znajomymi",
                        "umówić się na telefon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Договориться о встрече с кем-либо в определенном месте и в определенное время (обычно возвратный глагол)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "договориться",
                                "target_lang_sense_clarification": "о встрече"
                            },
                            {
                                "target_lang_word": "записаться",
                                "target_lang_sense_clarification": "на прием к врачу или специалисту"
                            },
                            {
                                "target_lang_word": "условиться",
                                "target_lang_sense_clarification": "о встрече (более формально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb5ca22c-b41e-3395-a653-48798831c083",
                    "sense_definition": "Zarezerwować termin wizyty lub spotkania dla innej osoby.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "meeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sekretarka <umówiła> prezesa na ważne spotkanie z nowym klientem.",
                            "target_lang_translations": {
                                "ru": "Секретарь <записала> президента компании на важную встречу с новым клиентом."
                            }
                        },
                        {
                            "text": "Czy możesz <umówić> babcię do kardiologa na przyszły tydzień?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <записать> бабушку к кардиологу на следующую неделю?"
                            }

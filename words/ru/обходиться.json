    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "40656794-c5fa-3ce3-8ee3-b7176d491e28",
                    "sense_definition": "Жить или действовать, не имея чего-либо или довольствуясь малым",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "manage_without",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В походе нам часто приходилось <w>обходиться</w> без горячей еды.",
                            "target_lang_translations": {
                                "en": "On the hike, we often had to <w>do</w> without hot food."
                            }
                        },
                        {
                            "text": "Он научился <w>обходиться</w> малым и не требовать лишнего.",
                            "target_lang_translations": {
                                "en": "He learned to <w>make do</w> with little and not demand too much."
                            }
                        }
                    ],
                    "synonyms": [
                        "справляться",
                        "жить"
                    ],
                    "antonyms": [
                        "нуждаться"
                    ],
                    "common_phrases": [
                        "обходиться без помощи",
                        "обходиться малым",
                        "обходиться без денег"
                    ],
                    "target_lang_definitions": {
                        "en": "To live or act without having something or being content with little"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "manage",
                                "target_lang_sense_clarification": "to succeed in surviving or living, especially with limited resources"
                            },
                            {
                                "target_lang_word": "do without",
                                "target_lang_sense_clarification": "to manage without something"
                            },
                            {
                                "target_lang_word": "make do",
                                "target_lang_sense_clarification": "to manage with the limited or inadequate means available"
                            },
                            {
                                "target_lang_word": "get by",
                                "target_lang_sense_clarification": "to survive or manage, usually with few resources"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc34ebb8-f714-37db-b054-1538220ccff7",
                    "sense_definition": "Стоить определённую сумму денег",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cost",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Жизнь в большом городе <w>обходится</w> довольно дорого.",
                            "target_lang_translations": {
                                "en": "Life in a big city <w>costs</w> quite a lot."
                            }
                        },
                        {
                            "text": "Ремонт старого дома обычно <w>обходится</w> в круглую сумму.",
                            "target_lang_translations": {
                                "en": "Renovating an old house usually <w>costs</w> a pretty penny."
                            }
                        }
                    ],
                    "synonyms": [
                        "стоить"
                    ],
                    "common_phrases": [
                        "обходиться дорого",
                        "обходиться дёшево",
                        "обходиться в копеечку"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cost a certain amount of money"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cost",
                                "target_lang_sense_clarification": "to require payment of a specified price"
                            },
                            {
                                "target_lang_word": "come to",
                                "target_lang_sense_clarification": "to amount to a total sum"
                            }
                        ]
                    }
                },
                {

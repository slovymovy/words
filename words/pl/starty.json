            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a021e604-7290-387d-83fe-be1ee98bf31b",
                    "sense_definition": "Rozdrobniony na małe kawałki przy użyciu tarki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rubbing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do surówki dodałem seler <w>starty</w> na drobnych oczkach tarki.",
                            "target_lang_translations": {
                                "ru": "В салат я добавил сельдерей, <w>натёртый</w> на мелкой терке."
                            }
                        },
                        {
                            "text": "Na wierzch zapiekanki posypałem <w>starty</w> żółty ser.",
                            "target_lang_translations": {
                                "ru": "Сверху запеканку я посыпал <w>тёртым</w> сыром."
                            }
                        }
                    ],
                    "synonyms": [
                        "utarty"
                    ],
                    "common_phrases": [
                        "starty ser",
                        "starta marchewka",
                        "starty chrzan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Измельченный на мелкие кусочки с помощью терки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тёртый",
                                "target_lang_sense_clarification": "о продуктах питания"
                            },
                            {
                                "target_lang_word": "натёртый",
                                "target_lang_sense_clarification": "причастие; подвергшийся трению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e432253-d404-37b7-92e1-7401adc2d9f4",
                    "sense_definition": "Oczyszczony z czegoś (np. z kurzu, napisów) poprzez wycieranie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "rubbing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyżurny, lekcja już się zaczęła, a tablica jeszcze nie <w>starta</w>.",
                            "target_lang_translations": {
                                "ru": "Дежурный, урок уже начался, а доска еще не <w>вытерта</w>."
                            }
                        },
                        {
                            "text": "Napis na murze został <w>starty</w> przez służby porządkowe.",
                            "target_lang_translations": {
                                "ru": "Надпись на стене была <w>стёрта</w> коммунальными службами."
                            }
                        }
                    ],
                    "synonyms": [
                        "wytarty"
                    ],
                    "common_phrases": [
                        "starta tablica",
                        "starty kurz",
                        "starty naskórek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очищенный от чего-либо (например, от пыли, надписей) путем вытирания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стёртый",
                                "target_lang_sense_clarification": "удаленный трением"
                            },
                            {
                                "target_lang_word": "вытертый",
                                "target_lang_sense_clarification": "очищенный тряпкой"
                            }
                        ]
                    }
                }
            ]
        },

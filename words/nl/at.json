            "senses": [
                {
                    "sense_id": "90d3d27c-04c4-357b-bae8-edf68f11ed20",
                    "sense_definition": "De uitgesproken vorm van het symbool @, voornamelijk gebruikt bij het opnoemen van e-mailadressen.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn zakelijke e-mailadres is info <w>at</w> bedrijf punt nl.",
                            "target_lang_translations": {
                                "en": "My business email address is info <w>at</w> company dot nl.",
                                "ru": "Мой рабочий адрес электронной почты — info <w>собака</w> bedrijf точка nl."
                            }
                        },
                        {
                            "text": "Je spreekt het apenstaartje in een e-mailadres vaak uit als <w>at</w>.",
                            "target_lang_translations": {
                                "en": "You often pronounce the 'monkey tail' in an email address as <w>at</w>.",
                                "ru": "Значок «собака» в адресе электронной почты часто произносят как <w>эт</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "apenstaartje",
                        "at-teken"
                    ],
                    "common_phrases": [
                        "punt nl",
                        "punt com"
                    ],
                    "target_lang_definitions": {
                        "en": "The spoken form of the symbol @, used in email addresses.",
                        "ru": "Произносимая форма символа @, используемая в адресах электронной почты."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "symbol @"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "собака",
                                "target_lang_sense_clarification": "разговорное название символа @"
                            },
                            {
                                "target_lang_word": "эт",
                                "target_lang_sense_clarification": "транслитерация с английского"
                            }

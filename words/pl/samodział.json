{
    "word_family": [
        "samodziałowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e8ab49c-2c6a-3945-a07f-86ad780d5680",
                    "sense_definition": "Gruba tkanina z wełny lub lnu, tkana ręcznie na krośnie, często o nierównej powierzchni.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "textile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wystawie sztuki ludowej podziwialiśmy barwny <w>samodział</w> wykonany przez lokalnych artystów.",
                            "target_lang_translations": {
                                "ru": "На выставке народного искусства мы любовались красочной <w>домотканиной</w>, изготовленной местными мастерами."
                            }
                        },
                        {
                            "text": "Jej płaszcz uszyty z grubego <w>samodziału</w> doskonale chronił przed zimnym wiatrem.",
                            "target_lang_translations": {
                                "ru": "Её пальто, сшитое из толстой <w>домотканины</w>, прекрасно защищало от холодного ветра."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wełniany samodział",
                        "lniany samodział"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубая ткань из шерсти или льна, сотканная вручную на станке, часто с неровной поверхностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "домотканина",
                                "target_lang_sense_clarification": "ткань ручной работы"
                            },
                            {
                                "target_lang_word": "домотканая ткань",
                                "target_lang_sense_clarification": "ткань домашнего производства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "210eeefd-ecbd-370f-8832-0bbc07620473",
                    "sense_definition": "Alkohol wytwarzany nielegalnie w warunkach domowych.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "alcohol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz postawił na stole butelkę mocnego <w>samodziału</w> dla swoich gości.",
                            "target_lang_translations": {
                                "ru": "Хозяин поставил на стол бутылку крепкого <w>самогона</w> для своих гостей."
                            }
                        },
                        {
                            "text": "W tej okolicy pędzenie <w>samodziału</w> było dawną, choć nielegalną tradycją.",
                            "target_lang_translations": {
                                "ru": "В этой местности изготовление <w>самогона</w> было давней, хотя и незаконной традицией."
                            }
                        }
                    ],
                    "synonyms": [
                        "bimber",
                        "samogon"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Алкоголь, произведенный нелегально в домашних условиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самогон",
                                "target_lang_sense_clarification": "домашний алкоголь"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
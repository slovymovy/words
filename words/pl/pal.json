        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b0c6a9a8-4292-3ce8-8f56-574b58876f48",
                    "sense_definition": "Gruby, wydłużony kawałek drewna lub innego materiału, zwykle zaostrzony z jednej strony i wbijany w ziemię w celach konstrukcyjnych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robotnicy wbili wielki <pal> w ziemię, aby oznaczyć teren budowy.",
                            "target_lang_translations": {
                                "ru": "Рабочие вбили большой <кол> в землю, чтобы обозначить территорию стройки."
                            }
                        },
                        {
                            "text": "Cały pomost opierał się na solidnych, drewnianych <palach>.",
                            "target_lang_translations": {
                                "ru": "Весь помост держался на прочных деревянных <сваях>."
                            }
                        }
                    ],
                    "synonyms": [
                        "słup",
                        "kołek",
                        "żerdź"
                    ],
                    "common_phrases": [
                        "wbić pal",
                        "stać jak pal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Толстый, удлиненный кусок дерева или другого материала, заостренный с одной стороны и вбиваемый в землю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кол",
                                "target_lang_sense_clarification": "заостренная палка"
                            },
                            {
                                "target_lang_word": "свая",
                                "target_lang_sense_clarification": "строительная опора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8702e087-ee72-3e0c-8683-1f9eaf68c413",
                    "sense_definition": "Dawne narzędzie tortur i egzekucji w postaci zaostrzonego słupa, na który wbijano skazańca.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "torture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych wiekach zdrajców często skazywano na <pal>.",
                            "target_lang_translations": {
                                "ru": "В прошлые века предателей часто приговаривали к сажанию на <кол>."
                            }
                        },
                        {
                            "text": "Nabicie na <pal> było jedną z najokrutniejszych metod uśmiercania.",
                            "target_lang_translations": {
                                "ru": "Сажание на <кол> было одним из самых жестоких методов умерщвления."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nabić na pal"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинное орудие казни в виде заостренного столба."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кол",
                                "target_lang_sense_clarification": "орудие казни"
                            }
                        ]
                    }
                }
            ]
        },
        {

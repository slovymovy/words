    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6970d5e6-ff7b-330e-b71b-514ae889e5ef",
                    "sense_definition": "Aandacht geven aan iemand of iets om het in goede staat te houden of te helpen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "care_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verpleegkundige <w>zorgt</w> dag en nacht voor de patiënten in het ziekenhuis.",
                            "target_lang_translations": {
                                "en": "The nurse <w>cares</w> for the patients in the hospital day and night."
                            }
                        },
                        {
                            "text": "Wie <w>zorgt</w> er voor de kat als jullie op vakantie zijn?",
                            "target_lang_translations": {
                                "en": "Who <w>looks after</w> the cat when you are on vacation?"
                            }
                        }
                    ],
                    "synonyms": [
                        "verzorgen",
                        "passen op"
                    ],
                    "antonyms": [
                        "verwaarlozen"
                    ],
                    "common_phrases": [
                        "zorgen voor"
                    ],
                    "target_lang_definitions": {
                        "en": "To give attention to someone or something to keep them in good condition or to help."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take care of",
                                "target_lang_sense_clarification": "to be responsible for or attend to"
                            },
                            {
                                "target_lang_word": "care for",
                                "target_lang_sense_clarification": "to look after someone or something"
                            },
                            {
                                "target_lang_word": "look after",
                                "target_lang_sense_clarification": "to watch over or tend to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3de560d4-dc93-3f1d-8ebb-6e34d3131170",
                    "sense_definition": "Ervoor zorgen dat iets gebeurt of gedaan wordt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ensure_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal <w>zorgen</w> dat alles op tijd klaar is voor het feest.",
                            "target_lang_translations": {
                                "en": "I will <w>ensure</w> that everything is ready in time for the party."
                            }
                        },
                        {
                            "text": "Zorg jij dat de deur goed op slot zit voordat we weggaan?",
                            "target_lang_translations": {
                                "en": "Will you <w>make sure</w> the door is locked securely before we leave?"
                            }
                        }
                    ],
                    "synonyms": [
                        "regelen",
                        "garanderen",
                        "verzekeren"
                    ],
                    "common_phrases": [
                        "zorgen dat",
                        "ervoor zorgen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make sure that something happens or is done."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ensure",
                                "target_lang_sense_clarification": "to guarantee an outcome"
                            },
                            {
                                "target_lang_word": "make sure",
                                "target_lang_sense_clarification": "to verify or confirm"
                            },
                            {
                                "target_lang_word": "see to it",
                                "target_lang_sense_clarification": "to take responsibility for an action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c1f2f64-2cb8-3ce6-934e-a25d341c8362",
                    "sense_definition": "De oorzaak zijn van iets; iets teweegbrengen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cause_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zware storm heeft voor veel schade aan de huizen <w>gezorgd</w>.",
                            "target_lang_translations": {
                                "en": "The heavy storm <w>caused</w> a lot of damage to the houses."
                            }
                        },
                        {
                            "text": "Zijn onverwachte opmerking <w>zorgde</w> voor een ongemakkelijke stilte in de kamer.",
                            "target_lang_translations": {
                                "en": "His unexpected remark <w>caused</w> an awkward silence in the room."
                            }
                        }
                    ],
                    "synonyms": [
                        "veroorzaken",
                        "teweegbrengen"
                    ],
                    "common_phrases": [
                        "zorgen voor"
                    ],
                    "target_lang_definitions": {
                        "en": "To be the cause of something; to bring something about."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cause",
                                "target_lang_sense_clarification": "to make something happen"
                            },
                            {
                                "target_lang_word": "bring about",
                                "target_lang_sense_clarification": "to lead to a result"
                            },
                            {
                                "target_lang_word": "result in",
                                "target_lang_sense_clarification": "to have as an outcome"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc44ae86-ac48-306a-923f-9d4815ddba2d",
                    "sense_definition": "Meervoud van <w>zorg</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "worries_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij maakt zich grote <w>zorgen</w> over de gezondheid van haar ouders.",
                            "target_lang_translations": {
                                "en": "She has great <w>concerns</w> about her parents' health."
                            }
                        },
                        {
                            "text": "Door de financiële <w>zorgen</w> kon hij 's nachts moeilijk in slaap komen.",
                            "target_lang_translations": {
                                "en": "Because of financial <w>worries</w>, he had trouble falling asleep at night."
                            }
                        }
                    ],
                    "synonyms": [
                        "bekommernissen",
                        "problemen"
                    ],
                    "common_phrases": [
                        "zich zorgen maken",
                        "zorgen hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of 'zorg' (care/worry); feelings of anxiety or concern."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "worries",
                                "target_lang_sense_clarification": "feelings of anxiety or trouble"
                            },
                            {
                                "target_lang_word": "concerns",
                                "target_lang_sense_clarification": "matters of interest or anxiety"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
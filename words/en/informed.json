    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2266a3d9-6a03-307e-bced-788a80a29076",
                    "sense_definition": "Having a lot of knowledge or information about a specific subject or situation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "knowledgeable_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <informed> traveler always checks the weather forecast before a trip.",
                            "target_lang_translations": {
                                "ru": "<Осведомлённый> путешественник всегда проверяет прогноз погоды перед поездкой."
                            }
                        },
                        {
                            "text": "She stays <informed> about current events by reading several newspapers daily.",
                            "target_lang_translations": {
                                "ru": "Она остаётся <в курсе> текущих событий, читая несколько газет ежедневно."
                            }
                        }
                    ],
                    "synonyms": [
                        "apprised",
                        "up-to-date",
                        "abreast"
                    ],
                    "common_phrases": [
                        "well informed",
                        "keep informed",
                        "stay informed",
                        "fully informed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий обширными знаниями или информацией по определённому вопросу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осведомлённый",
                                "target_lang_sense_clarification": "обладающий сведениями"
                            },
                            {
                                "target_lang_word": "информированный",
                                "target_lang_sense_clarification": "получивший информацию"
                            },
                            {
                                "target_lang_word": "знающий",
                                "target_lang_sense_clarification": "разбирающийся в теме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c67c08a-ede4-3bba-b770-5bca9732e12e",
                    "sense_definition": "Based on a good understanding of the facts and relevant information.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "knowledge_based",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should gather all the facts to make an <informed> decision.",
                            "target_lang_translations": {
                                "ru": "Вам следует собрать все факты, чтобы принять <взвешенное> решение."
                            }
                        },
                        {
                            "text": "The committee relied on his <informed> opinion to settle the dispute.",
                            "target_lang_translations": {
                                "ru": "Комитет полагался на его <компетентное> мнение для разрешения спора."
                            }
                        }
                    ],
                    "common_phrases": [
                        "informed decision",
                        "informed choice",
                        "informed consent",
                        "informed opinion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основанный на понимании фактов и соответствующей информации (о решении или мнении)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взвешенный",
                                "target_lang_sense_clarification": "обдуманный (о решении)"
                            },
                            {
                                "target_lang_word": "осознанный",
                                "target_lang_sense_clarification": "сделанный со знанием дела (о выборе)"
                            },
                            {
                                "target_lang_word": "обоснованный",
                                "target_lang_sense_clarification": "подкреплённый фактами (о мнении)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a6c653b-5608-36ea-ac45-a2a8af644fa9",
                    "sense_definition": "Created or given a physical form.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist described the sculpture as a spirit <informed> in clay.",
                            "target_lang_translations": {
                                "ru": "Художник описал скульптуру как дух, <заключённый> в глину."
                            }
                        },
                        {
                            "text": "Ancient philosophers debated how matter was <informed> by the soul.",
                            "target_lang_translations": {
                                "ru": "Древние философы спорили о том, как материя была <одухотворена> душой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "found in older literature like Spenser"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Получивший физическую форму или наполненный содержанием (филос.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одухотворённый",
                                "target_lang_sense_clarification": "наполненный духом"
                            },
                            {
                                "target_lang_word": "облечённый в форму",
                                "target_lang_sense_clarification": "получивший вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "344bb558-1a73-3fdb-803a-a3b68955bceb",
                    "sense_definition": "Lacking a defined form or shape; deformed or shapeless.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_unformed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chaotic and <informed> mass eventually took shape under his hands.",
                            "target_lang_translations": {
                                "ru": "Хаотичная и <бесформенная> масса в конце концов обрела форму под его руками."
                            }
                        },
                        {
                            "text": "He gazed into the dark, <informed> void before the creation of the world.",
                            "target_lang_translations": {
                                "ru": "Он вглядывался в тёмную, <неоформленную> пустоту, существовавшую до сотворения мира."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rare usage where prefix in- means not"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишённый определённой формы; бесформенный (устар.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесформенный",
                                "target_lang_sense_clarification": "не имеющий формы"
                            },
                            {
                                "target_lang_word": "неоформленный",
                                "target_lang_sense_clarification": "хаотичный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5dff46c-4b0c-3575-9348-89cd0ebfc23e",
                    "sense_definition": "Not belonging to any of the recognized ancient constellations.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "astronomy_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Early astronomers struggled to map the <informed> stars scattered across the southern sky.",
                            "target_lang_translations": {
                                "ru": "Ранние астрономы с трудом наносили на карту <неоформленные> звёзды, разбросанные по южному небу."
                            }
                        },
                        {
                            "text": "These <informed> celestial bodies were later grouped into new constellations.",
                            "target_lang_translations": {
                                "ru": "Эти <не вошедшие в созвездия> небесные тела позже были сгруппированы в новые созвездия."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical astronomical term"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не входящий в состав признанных созвездий (астрон. устар.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неоформленный",
                                "target_lang_sense_clarification": "о звёздах вне созвездий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [

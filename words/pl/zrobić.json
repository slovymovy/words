            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ddf6c2df-3494-3d9c-9eec-11d12bd3c565",
                    "sense_definition": "Wykonać jakąś pracę, zadanie lub czynność; podjąć ogólne działanie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "action_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wiem, co mam teraz <zrobić> w tej trudnej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, что мне теперь <делать> в этой сложной ситуации."
                            }
                        },
                        {
                            "text": "Musisz <zrobić> to zadanie domowe przed pójściem spać.",
                            "target_lang_translations": {
                                "ru": "Ты должен <сделать> это домашнее задание перед сном."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykonać",
                        "postąpić",
                        "zachować się"
                    ],
                    "common_phrases": [
                        "zrobić wszystko",
                        "co zrobić",
                        "zrobić coś dobrego",
                        "zrobić krok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершить какую-либо работу, задачу или действие; предпринять общие меры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сделать",
                                "target_lang_sense_clarification": "общее значение совершения действия"
                            },
                            {
                                "target_lang_word": "выполнить",
                                "target_lang_sense_clarification": "о задаче или работе"
                            },
                            {
                                "target_lang_word": "совершить",
                                "target_lang_sense_clarification": "о поступке или действии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dec4dead-4c55-381d-8cfc-e73572fc2598",
                    "sense_definition": "Spowodować powstanie czegoś fizycznego lub abstrakcyjnego; przygotować lub wytworzyć coś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja mama potrafi <zrobić> najlepsze ciasto na świecie.",
                            "target_lang_translations": {
                                "ru": "Моя мама умеет <приготовить> лучший пирог в мире."
                            }
                        },
                        {
                            "text": "Chciałbym <zrobić> zdjęcie tego pięknego krajobrazu.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <сделать> фотографию этого красивого пейзажа."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykonać",
                        "urządzić",
                        "wykonać"
                    ],
                    "antonyms": [
                        "zniszczyć"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3044f46c-74aa-3766-ac3d-988722136316",
                    "sense_definition": "Коллегиальный орган, группа людей, выбранная или назначенная для руководства какой-либо областью деятельности или выполнения специальных задач.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш родительский <w>комитет</w> решил организовать экскурсию для школьников.",
                            "target_lang_translations": {
                                "en": "Our parents' <w>committee</w> decided to organize a field trip for the schoolchildren."
                            }
                        },
                        {
                            "text": "Специальный <w>комитет</w> рассматривает вопросы охраны окружающей среды.",
                            "target_lang_translations": {
                                "en": "A special <w>committee</w> is considering environmental protection issues."
                            }
                        }
                    ],
                    "synonyms": [
                        "комиссия",
                        "совет",
                        "бюро"
                    ],
                    "common_phrases": [
                        "родительский комитет",
                        "организационный комитет",
                        "центральный комитет",
                        "исполнительный комитет",
                        "следственный комитет"
                    ],
                    "target_lang_definitions": {
                        "en": "A collegial body or group of people elected or appointed to manage a specific area of activity or perform special tasks."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "committee",
                                "target_lang_sense_clarification": "administrative body"
                            },
                            {
                                "target_lang_word": "commission",
                                "target_lang_sense_clarification": "task force"
                            },
                            {
                                "target_lang_word": "board",
                                "target_lang_sense_clarification": "governing body"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cb9e060-9d52-3e0b-906d-0b441b808e8b",
                    "sense_definition": "Собрание людей (обычно родственников или знакомых), обсуждающее чьи-то дела или принимающее совместное решение (часто употребляется с иронией).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_gathering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Весь семейный <w>комитет</w> собрался на кухне, чтобы обсудить мою новую работу.",
                            "target_lang_translations": {
                                "en": "The entire family <w>committee</w> gathered in the kitchen to discuss my new job."
                            }
                        },
                        {
                            "text": "Тётушки составили целый <w>комитет</w> по спасению племянника от женитьбы.",
                            "target_lang_translations": {
                                "en": "The aunties formed a whole <w>committee</w> to save their nephew from marriage."
                            }
                        }
                    ],
                    "synonyms": [
                        "собрание",
                        "консилиум"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто используется в ироничном контексте"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A gathering of people (usually relatives or acquaintances) discussing someone's affairs or making a joint decision (often used ironically)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "committee",
                                "target_lang_sense_clarification": "ironic usage"
                            },
                            {
                                "target_lang_word": "council",
                                "target_lang_sense_clarification": "figurative gathering"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "7174d2f4-bba1-3adb-9974-3666a479a5f8",
                    "sense_definition": "Чрезмерная сентиментальность или приторная, неестественная любезность в поведении и речи.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его откровенная <w>слащавость</w> и лесть сразу вызвали у меня подозрение.",
                            "target_lang_translations": {
                                "en": "His overt <w>smarminess</w> and flattery immediately aroused my suspicion."
                            }
                        },
                        {
                            "text": "В её голосе звучала фальшивая <w>слащавость</w>, которая раздражала всех присутствующих.",
                            "target_lang_translations": {
                                "en": "There was a fake <w>sugaryness</w> in her voice that annoyed everyone present."
                            }
                        }
                    ],
                    "synonyms": [
                        "елейность",
                        "приторность",
                        "умильность"
                    ],
                    "antonyms": [
                        "грубость",
                        "сухость",
                        "резкость"
                    ],
                    "common_phrases": [
                        "приторная слащавость",
                        "фальшивая слащавость",
                        "слащавость в голосе"
                    ],
                    "target_lang_definitions": {
                        "en": "Excessive sentimentality or cloying, unnatural politeness in behavior and speech."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smarminess",
                                "target_lang_sense_clarification": "insincere or exaggerated earnestness"
                            },
                            {
                                "target_lang_word": "mawkishness",
                                "target_lang_sense_clarification": "excessive sentimentality"
                            },
                            {
                                "target_lang_word": "unctuousness",
                                "target_lang_sense_clarification": "oily, excessive flattery"
                            },
                            {
                                "target_lang_word": "saccharine quality",
                                "target_lang_sense_clarification": "excessively sweet or sentimental manner"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a19a9c0-e41d-350b-95ba-2bf73854f907",
                    "sense_definition": "Свойство того, что имеет неприятно сладкий, приторный вкус или запах.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом десерте чувствовалась химическая <w>слащавость</w>, перебивающая вкус фруктов.",
                            "target_lang_translations": {
                                "en": "A chemical <w>cloyingness</w> could be felt in this dessert, overpowering the taste of the fruit."
                            }
                        },
                        {
                            "text": "Из-за избытка сахара варенье приобрело неприятную <w>слащавость</w>.",
                            "target_lang_translations": {
                                "en": "Due to the excess sugar, the jam acquired an unpleasant <w>sickly sweetness</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "приторность"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality of having an unpleasantly sweet, cloying taste or smell."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cloyingness",

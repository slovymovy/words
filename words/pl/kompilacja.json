            "senses": [
                {
                    "sense_id": "931e91a7-99d3-3a50-9e28-c434caf9c907",
                    "sense_definition": "album muzyczny zawierający utwory zebrane z wcześniejszych wydawnictw, często różnych wykonawców",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na urodziny dostałem świetną <w>kompilację</w> z muzyką filmową.",
                            "target_lang_translations": {
                                "ru": "На день рождения я получил отличный <w>сборник</w> музыки из фильмов."
                            }
                        },
                        {
                            "text": "Ta <w>kompilacja</w> przebojów idealnie nadaje się na imprezę taneczną.",
                            "target_lang_translations": {
                                "ru": "Этот <w>сборник</w> хитов идеально подходит для танцевальной вечеринки."
                            }
                        }
                    ],
                    "synonyms": [
                        "składanka"
                    ],
                    "common_phrases": [
                        "kompilacja przebojów",
                        "kompilacja utworów",
                        "nowa kompilacja"
                    ],
                    "target_lang_definitions": {
                        "ru": "музыкальный альбом, содержащий треки, собранные из предыдущих релизов, часто разных исполнителей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сборник",
                                "target_lang_sense_clarification": "музыкальный"
                            },
                            {
                                "target_lang_word": "компиляция",
                                "target_lang_sense_clarification": "альбом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf2f6839-c850-35c1-9fc1-a8b6896b80c2",
                    "sense_definition": "proces tłumaczenia kodu źródłowego programu na kod wynikowy zrozumiały dla komputera",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_it",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas <w>kompilacji</w> kodu pojawiły się niespodziewane błędy.",
                            "target_lang_translations": {
                                "ru": "Во время <w>компиляции</w> кода возникли неожиданные ошибки."
                            }
                        },
                        {
                            "text": "Programista musi czekać na zakończenie <w>kompilacji</w>, aby przetestować zmiany.",
                            "target_lang_translations": {
                                "ru": "Программист должен дождаться завершения <w>компиляции</w>, чтобы протестировать изменения."
                            }
                        }
                    ],
                    "antonyms": [
                        "dekompilacja"
                    ],
                    "common_phrases": [
                        "błąd kompilacji",
                        "czas kompilacji",
                        "proces kompilacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "процесс перевода исходного кода программы в машинный код, понятный компьютеру"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компиляция",
                                "target_lang_sense_clarification": "процесс в IT"

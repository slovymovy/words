            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8c388c33-21f4-3312-973f-959008f4496d",
                    "sense_definition": "Бесполезно, безрезультатно, не достигая поставленной цели.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "in_vain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы целый час ждали автобус, но <зря>, он так и не приехал.",
                            "target_lang_translations": {
                                "en": "We waited for the bus for a whole hour, but <in vain>, it never came."
                            }
                        },
                        {
                            "text": "Не стоит <зря> тратить время на споры, которые ничего не изменят.",
                            "target_lang_translations": {
                                "en": "It is not worth wasting time <for nothing> on arguments that will change nothing."
                            }
                        }
                    ],
                    "synonyms": [
                        "напрасно",
                        "тщетно",
                        "безуспешно",
                        "безрезультатно",
                        "бесполезно",
                        "без толку",
                        "впустую",
                        "понапрасну",
                        "попусту",
                        "даром",
                        "бесплодно",
                        "зазря",
                        "вхолостую",
                        "бесцельно"
                    ],
                    "antonyms": [
                        "недаром",
                        "не зря"
                    ],
                    "common_phrases": [
                        "зря тратить время",
                        "зря стараться",
                        "всё зря",
                        "пропасть зря"
                    ],
                    "target_lang_definitions": {
                        "en": "Uselessly, without result, or failing to achieve the intended goal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in vain",
                                "target_lang_sense_clarification": "Without success or a positive result"
                            },
                            {
                                "target_lang_word": "for nothing",
                                "target_lang_sense_clarification": "Without any benefit or result; uselessly"
                            },
                            {
                                "target_lang_word": "to no purpose",
                                "target_lang_sense_clarification": "With no meaningful result"
                            },
                            {
                                "target_lang_word": "down the drain",
                                "target_lang_sense_clarification": "Wasted or irretrievable (idiomatic)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15e18828-c77b-3688-8573-d4cba7ddd079",
                    "sense_definition": "Ошибочно или необдуманно; используется для выражения сожаления или неодобрения по поводу совершённого действия.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mistakenly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты <зря> обидел своего друга, он хотел тебе только добра.",
                            "target_lang_translations": {
                                "en": "You offended your friend <for nothing>, he only wanted what was best for you."
                            }
                        },
                        {
                            "text": "— Я решил не идти на этот концерт. — А <зря>, там было очень весело.",
                            "target_lang_translations": {
                                "en": "— I decided not to go to this concert. — And <that was a mistake>, it was very fun there."
                            }
                        }
                    ],
                    "synonyms": [
                        "напрасно",
                        "нежелательно"
                    ],
                    "antonyms": [
                        "не зря"
                    ],
                    "common_phrases": [
                        "ты это зря",
                        "совсем зря",
                        "а зря",
                        "очень зря"
                    ],
                    "target_lang_definitions": {
                        "en": "Mistakenly, unwisely, or expressing regret that an action was taken (or not taken)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wrongly",
                                "target_lang_sense_clarification": "In a mistaken or incorrect manner"
                            },
                            {
                                "target_lang_word": "unwisely",
                                "target_lang_sense_clarification": "Showing a lack of good judgment"
                            },
                            {
                                "target_lang_word": "for nothing",
                                "target_lang_sense_clarification": "Without good cause or reason"
                            },
                            {
                                "target_lang_word": "a mistake",
                                "target_lang_sense_clarification": "Used in phrases indicating an error in judgment"
                            }
                        ]
                    }
                }
            ]
        },
        {

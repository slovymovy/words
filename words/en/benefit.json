{
    "word_family": [
        "beneficial",
        "beneficiary",
        "benefactor",
        "beneficially"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "858008d0-ca0a-39bb-af71-0840101c8517",
                    "sense_definition": "An advantage, profit, or helpful effect gained from something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "advantage_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Regular exercise has many health <w>benefits</w> for people of all ages.",
                            "target_lang_translations": {
                                "ru": "Регулярные физические упражнения приносят много <w>пользы</w> здоровью людей всех возрастов."
                            }
                        },
                        {
                            "text": "We should work together for the mutual <w>benefit</w> of our community.",
                            "target_lang_translations": {
                                "ru": "Мы должны работать вместе ради общего <w>блага</w> нашего сообщества."
                            }
                        }
                    ],
                    "synonyms": [
                        "advantage",
                        "help",
                        "aid",
                        "assistance",
                        "boon"
                    ],
                    "antonyms": [
                        "disadvantage",
                        "detriment",
                        "harm",
                        "hindrance",
                        "obstacle"
                    ],
                    "common_phrases": [
                        "for the benefit of",
                        "reap the benefits",
                        "mutual benefit",
                        "of benefit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Преимущество, прибыль или полезный эффект, полученный от чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по́льза",
                                "target_lang_sense_clarification": "положительное, благотворное влияние"
                            },
                            {
                                "target_lang_word": "вы́года",
                                "target_lang_sense_clarification": "прибыль, доход или личный интерес"
                            },
                            {
                                "target_lang_word": "преиму́щество",
                                "target_lang_sense_clarification": "качество, выгодно отличающее от других"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f10882f-09b1-33aa-9103-73d0e9ec4762",
                    "sense_definition": "Money provided by the government or an insurance company to help someone in need, such as when they are sick, unemployed, or retired.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "financial_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He lost his job last month and now claims unemployment <w>benefits</w>.",
                            "target_lang_translations": {
                                "ru": "В прошлом месяце он потерял работу и теперь получает <w>пособие</w> по безработице."
                            }
                        },
                        {
                            "text": "The insurance policy includes a substantial death <w>benefit</w> for the family.",
                            "target_lang_translations": {
                                "ru": "Страховой полис предусматривает значительную <w>выплату</w> семье в случае смерти."
                            }
                        }
                    ],
                    "synonyms": [
                        "subsidy",
                        "aid",
                        "assistance"
                    ],
                    "common_phrases": [
                        "unemployment benefit",
                        "housing benefit",
                        "claim benefits",
                        "social security benefit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деньги, предоставляемые государством или страховой компанией для помощи нуждающимся (больным, безработным и т.д.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посо́бие",
                                "target_lang_sense_clarification": "денежная помощь от государства"
                            },
                            {
                                "target_lang_word": "социа́льная вы́плата",
                                "target_lang_sense_clarification": "материальная помощь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0946c7e5-c340-34a8-8941-54195d38d74b",
                    "sense_definition": "The sake or personal interest of a specific person, often used when doing something specifically to help, inform, or impress them.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "advantage_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She spoke loudly for my <w>benefit</w> so that I would definitely hear the news.",
                            "target_lang_translations": {
                                "ru": "Она говорила громко специально для моего <w>сведения</w>, чтобы я точно услышал новости."
                            }
                        },
                        {
                            "text": "The demonstration was staged solely for the <w>benefit</w> of the new investors.",
                            "target_lang_translations": {
                                "ru": "Демонстрация была устроена исключительно в <w>интересах</w> новых инвесторов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "for someone's benefit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Личный интерес или благо конкретного человека; часто используется, когда что-то делается специально для него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интере́с",
                                "target_lang_sense_clarification": "личная заинтересованность, выгода"
                            },
                            {
                                "target_lang_word": "бла́го",
                                "target_lang_sense_clarification": "добро, счастье, благополучие"
                            },
                            {
                                "target_lang_word": "ра́ди",
                                "target_lang_sense_clarification": "предлог, указывающий на пользу для кого-то (for someone's benefit)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ccd8618e-19b3-39c1-9e12-8643c7755d89",
                    "sense_definition": "A public event, such as a concert or performance, held to raise money for a charitable cause.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "event_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band played a <w>benefit</w> to raise funds for the local hospital.",
                            "target_lang_translations": {
                                "ru": "Группа сыграла <w>благотворительный концерт</w>, чтобы собрать средства для местной больницы."
                            }
                        },
                        {
                            "text": "Tickets for the charity <w>benefit</w> were sold out within hours.",
                            "target_lang_translations": {
                                "ru": "Билеты на благотворительный <w>вечер</w> были распроданы за несколько часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "aid"
                    ],
                    "common_phrases": [
                        "benefit concert",
                        "benefit performance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публичное мероприятие (концерт, представление), проводимое для сбора средств на благотворительность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благотвори́тельный конце́рт",
                                "target_lang_sense_clarification": "концерт для сбора средств"
                            },
                            {
                                "target_lang_word": "бенефи́с",
                                "target_lang_sense_clarification": "спектакль в честь одного участника (иногда благотворительный)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "63662f0d-828a-3241-a2a3-ec364fecdd6d",
                    "sense_definition": "To receive an advantage, profit, or good result from something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "receive_good",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Students <w>benefit</w> greatly from small class sizes and individual attention.",
                            "target_lang_translations": {
                                "ru": "Студенты <w>получают</w> огромную <w>пользу</w> от небольшого размера классов и индивидуального внимания."
                            }
                        },
                        {
                            "text": "I feel that I have <w>benefited</w> from listening to your advice.",
                            "target_lang_translations": {
                                "ru": "Я чувствую, что <w>извлек пользу</w> из твоих советов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "benefit from"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать преимущество, прибыль или хороший результат от чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извлека́ть по́льзу",
                                "target_lang_sense_clarification": "получать хороший результат"
                            },
                            {
                                "target_lang_word": "выи́грывать",
                                "target_lang_sense_clarification": "получать преимущество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a571d714-c29d-32b8-87a4-cdb48e2a20db",
                    "sense_definition": "To be helpful, useful, or good to someone or something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "give_good",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new tax laws will <w>benefit</w> families with young children.",
                            "target_lang_translations": {
                                "ru": "Новые налоговые законы <w>принесут пользу</w> семьям с маленькими детьми."
                            }
                        },
                        {
                            "text": "This decision <w>benefits</w> the entire team, not just the manager.",
                            "target_lang_translations": {
                                "ru": "Это решение <w>идет на пользу</w> всей команде, а не только менеджеру."
                            }
                        }
                    ],
                    "synonyms": [
                        "help",
                        "aid",
                        "assistance"
                    ],
                    "antonyms": [
                        "harm"
                    ],
                    "common_phrases": [
                        "benefit society",
                        "benefit everyone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть полезным, приносить благо кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приноси́ть по́льзу",
                                "target_lang_sense_clarification": "быть полезным"
                            },
                            {
                                "target_lang_word": "идти́ на по́льзу",
                                "target_lang_sense_clarification": "оказывать благотворное влияние"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "330c5399-0020-3643-8f39-f03390e9c395",
                    "sense_definition": "Umieścić przedmiot z powrotem na jego miejsce lub postawić go nieco dalej",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_move",
                    "name_type": "no",

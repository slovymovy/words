    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "39fc123d-1c89-3758-8b70-108fccee566f",
                    "sense_definition": "A long, hollow object that is usually round, like a pipe.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shape_cylindrical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Water flows through the copper <w>tube</w> into the tank.",
                            "target_lang_translations": {
                                "ru": "Вода течет через медную <w>трубку</w> в бак."
                            }
                        },
                        {
                            "text": "The scientist placed the sample in a glass test <w>tube</w>.",
                            "target_lang_translations": {
                                "ru": "Ученый поместил образец в стеклянную <w>пробирку</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "test tube",
                        "inner tube",
                        "plastic tube",
                        "glass tube"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный полый предмет, обычно округлой формы, напоминающий трубу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "труба",
                                "target_lang_sense_clarification": "полый предмет"
                            },
                            {
                                "target_lang_word": "трубка",
                                "target_lang_sense_clarification": "небольшого диаметра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97de5408-cd41-3240-a1a2-ac6d4d408d88",
                    "sense_definition": "A soft container with a cap that you squeeze to release a thick liquid, such as toothpaste or cream.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to buy a new <w>tube</w> of toothpaste from the store.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новый <w>тюбик</w> зубной пасты в магазине."
                            }
                        },
                        {
                            "text": "She squeezed the last bit of paint out of the <w>tube</w>.",
                            "target_lang_translations": {
                                "ru": "Она выдавила остатки краски из <w>тюбика</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tube of toothpaste",
                        "tube of glue",
                        "squeeze the tube"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мягкий контейнер с крышкой, который сжимают для выдавливания густого содержимого, например зубной пасты или крема."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тюбик",
                                "target_lang_sense_clarification": "упаковка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4c685db-4e2e-3788-b82a-67a4e17c9ed1",
                    "sense_definition": "The underground railway system in London.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to take the <w>tube</w> to avoid the traffic in the city center.",
                            "target_lang_translations": {
                                "ru": "Мы решили поехать на <w>метро</w>, чтобы избежать пробок в центре города."
                            }
                        },
                        {
                            "text": "The <w>tube</w> station is just a few minutes walk from here.",
                            "target_lang_translations": {
                                "ru": "Станция <w>метро</w> находится всего в нескольких минутах ходьбы отсюда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on the tube",
                        "take the tube",
                        "tube station",
                        "tube map"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Система метрополитена в Лондоне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "метро",
                                "target_lang_sense_clarification": "транспорт"
                            },
                            {
                                "target_lang_word": "лондонское метро",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "424c8ca2-d023-3be0-ab23-0c76e16c4673",
                    "sense_definition": "A television.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent the whole evening sitting in front of the <w>tube</w>.",
                            "target_lang_translations": {
                                "ru": "Он провел весь вечер, сидя перед <w>теликом</w>."
                            }
                        },
                        {
                            "text": "There is nothing interesting to watch on the <w>tube</w> tonight.",
                            "target_lang_translations": {
                                "ru": "Сегодня вечером по <w>ящику</w> нечего смотреть."
                            }
                        }
                    ],
                    "synonyms": [
                        "telly",
                        "boob tube"
                    ],
                    "common_phrases": [
                        "watch the tube",
                        "glued to the tube"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Телевизор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "телевизор",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "телик",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0a77008-b1dc-3565-96f7-b63839f00f91",
                    "sense_definition": "The hollow space inside a large wave that is breaking.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surfing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The surfer managed to ride through the <w>tube</w> without falling.",
                            "target_lang_translations": {
                                "ru": "Серферу удалось проехать сквозь <w>трубу</w> и не упасть."
                            }
                        },
                        {
                            "text": "It takes great skill to navigate the <w>tube</w> of a massive wave.",
                            "target_lang_translations": {
                                "ru": "Нужно большое мастерство, чтобы пройти <w>трубу</w> огромной волны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ride the tube"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "surfing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полое пространство внутри большой обрушивающейся волны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "труба",
                                "target_lang_sense_clarification": "серфинг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5397be71-d5da-304e-b908-ff8253997a6b",
                    "sense_definition": "A stupid or foolish person.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't be such a <w>tube</w> and listen to what I am saying.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>тупицей</w> и послушай, что я говорю."
                            }
                        },
                        {
                            "text": "He made a mistake and felt like a total <w>tube</w>.",
                            "target_lang_translations": {
                                "ru": "Он совершил ошибку и чувствовал себя полным <w>идиотом</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "regional",
                            "comment": "Scottish"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Глупый или бестолковый человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болван",
                                "target_lang_sense_clarification": "ругательство"
                            },
                            {
                                "target_lang_word": "тупица",
                                "target_lang_sense_clarification": "ругательство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c57058b5-e98d-3a29-b04b-608459dcc6d8",
                    "sense_definition": "To float down a river sitting on a large inflated rubber ring.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>tubed</w> down the gentle river for three hours yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера мы три часа <w>сплавлялись на тюбах</w> по спокойной реке."
                            }
                        },
                        {
                            "text": "The kids love to <w>tube</w> on the lake during the summer holidays.",
                            "target_lang_translations": {
                                "ru": "Дети обожают <w>кататься на ватрушках</w> на озере во время летних каникул."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tube down the river"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плыть вниз по реке, сидя на большом надувном резиновом круге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сплавляться на тюбе",
                                "target_lang_sense_clarification": "активный отдых"
                            },
                            {
                                "target_lang_word": "кататься на ватрушке",
                                "target_lang_sense_clarification": "развлечение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "247e966c-8c71-39ce-9b5d-373a256c8d89",
                    "sense_definition": "To insert a tube into a person's body, especially into the windpipe to help them breathe.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The paramedics had to <w>tube</w> the patient immediately to secure his airway.",
                            "target_lang_translations": {
                                "ru": "Парамедикам пришлось немедленно <w>интубировать</w> пациента, чтобы обеспечить проходимость дыхательных путей."
                            }
                        },
                        {
                            "text": "He was <w>tubed</w> and connected to a ventilator in the emergency room.",
                            "target_lang_translations": {
                                "ru": "Его <w>интубировали</w> и подключили к аппарату ИВЛ в отделении неотложной помощи."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "medical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вводить трубку в тело человека, особенно в трахею для обеспечения дыхания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интубировать",
                                "target_lang_sense_clarification": "медицина"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "690ebe5e-c298-30d5-b733-a32b2af6b11a",
                    "sense_definition": "Rozdrobnione kawałki muru, cegieł, betonu lub kamieni, powstałe w wyniku zniszczenia lub rozbiórki budynku.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "debris_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robotnicy zamówili duży kontener, aby wywieźć <w>gruz</w> z placu budowy.",
                            "target_lang_translations": {
                                "ru": "Рабочие заказали большой контейнер, чтобы вывезти <w>строительный мусор</w> со строительной площадки."
                            }
                        },
                        {
                            "text": "Ratownicy przeszukiwali stertę <w>gruzu</w> w poszukiwaniu ocalałych.",
                            "target_lang_translations": {
                                "ru": "Спасатели обыскивали груду <w>обломков</w> в поисках выживших."
                            }
                        }
                    ],
                    "synonyms": [
                        "rumowisko",
                        "gruzowisko"
                    ],
                    "common_phrases": [
                        "wywóz gruzu",
                        "sterta gruzu",
                        "kontener na gruz",
                        "pod gruzami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раздробленные куски стен, кирпичей, бетона или камней, образовавшиеся в результате разрушения или сноса здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строительный мусор",
                                "target_lang_sense_clarification": "отходы строительства и сноса"
                            },
                            {
                                "target_lang_word": "обломки",
                                "target_lang_sense_clarification": "куски разрушенного материала"
                            },
                            {
                                "target_lang_word": "щебень",
                                "target_lang_sense_clarification": "битый камень"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8ac4894-1500-3453-adc9-b786419a16b8",
                    "sense_definition": "Resztki zniszczonych budynków lub przenośnie o czymś, co uległo całkowitemu zniszczeniu, np. plany czy marzenia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "ruins_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po trzęsieniu ziemi całe miasto zamieniło się w <w>gruzy</w>.",
                            "target_lang_translations": {
                                "ru": "После землетрясения весь город превратился в <w>руины</w>."
                            }
                        },
                        {
                            "text": "Jego ambitne plany na przyszłość legły w <w>gruzach</w> z powodu braku funduszy.",
                            "target_lang_translations": {
                                "ru": "Его амбициозные планы на будущее превратились в <w>руины</w> из-за нехватки средств."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruina"
                    ],
                    "common_phrases": [
                        "lec w gruzach",
                        "legnąć w gruzach",
                        "powstać z gruzów",
                        "miasto w gruzach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Остатки разрушенных зданий или, в переносном смысле, то, что подверглось полному уничтожению (например, планы или мечты)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "руины",
                                "target_lang_sense_clarification": "остатки зданий или переносное значение"
                            },
                            {
                                "target_lang_word": "развалины",
                                "target_lang_sense_clarification": "разрушенные строения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eceda4a5-2da1-364d-b8bc-2084450859db",
                    "sense_definition": "Stary, wysłużony samochód w bardzo złym stanie technicznym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "old_car",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wsiądę do tego auta, to kompletny <w>gruz</w> i strach nim jechać.",
                            "target_lang_translations": {
                                "ru": "Я не сяду в эту машину, это полная <w>развалюха</w>, и на ней страшно ехать."
                            }
                        },
                        {
                            "text": "Mój sąsiad kupił taniego <w>gruza</w>, żeby dojeżdżać nim tylko do pracy.",
                            "target_lang_translations": {
                                "ru": "Мой сосед купил дешевое <w>ведро</w>, чтобы ездить на нем только на работу."
                            }
                        }
                    ],
                    "synonyms": [

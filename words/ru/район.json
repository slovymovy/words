{
    "word_family": [
        "районный"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "00c208ce-bca2-3643-afe0-bd65151a1500",
                    "sense_definition": "Чётко очерченная административно-территориальная единица города или области.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "admin_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой друг переехал жить в новый спальный <w>район</w> на окраине города.",
                            "target_lang_translations": {
                                "en": "My friend moved to a new residential <w>area</w> on the outskirts of the city."
                            }
                        },
                        {
                            "text": "В нашем <w>районе</w> недавно построили большую современную школу.",
                            "target_lang_translations": {
                                "en": "A large modern school was recently built in our <w>district</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "спальный район",
                        "центр района",
                        "глава района",
                        "жилой район"
                    ],
                    "target_lang_definitions": {
                        "en": "A clearly defined administrative or territorial division of a city or region."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "district",
                                "target_lang_sense_clarification": "administrative division"
                            },
                            {
                                "target_lang_word": "neighborhood",
                                "target_lang_sense_clarification": "local community area"
                            },
                            {
                                "target_lang_word": "area",
                                "target_lang_sense_clarification": "part of a city"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91eaa007-c8d3-391c-af5f-7522e243ef31",
                    "sense_definition": "Приблизительное место расположения чего-либо или пространство вокруг какого-либо объекта.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "vicinity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы договорились встретиться с коллегами в <w>районе</w> центрального вокзала.",
                            "target_lang_translations": {
                                "en": "We agreed to meet with colleagues in the <w>vicinity</w> of the central station."
                            }
                        },
                        {
                            "text": "В этом <w>районе</w> всегда очень тихо и мало машин.",
                            "target_lang_translations": {
                                "en": "It is always very quiet and there are few cars in this <w>area</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "окрестность",
                        "округа"
                    ],
                    "common_phrases": [
                        "в районе вокзала",
                        "в этом районе",
                        "в том районе"
                    ],
                    "target_lang_definitions": {
                        "en": "An approximate location or the space surrounding a specific object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vicinity",
                                "target_lang_sense_clarification": "surrounding space"
                            },
                            {
                                "target_lang_word": "area",
                                "target_lang_sense_clarification": "general location"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc4ccd36-e4f1-37ec-b1ce-42482fd09b58",
                    "sense_definition": "Участок местности или территории, выделяемый по каким-либо общим признакам (климатическим, экономическим и т.п.).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "geo_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот горный <w>район</w> привлекает множество туристов своими пейзажами.",
                            "target_lang_translations": {
                                "en": "This mountain <w>region</w> attracts many tourists with its landscapes."
                            }
                        },
                        {
                            "text": "Северный <w>район</w> страны отличается суровым климатом и длинной зимой.",
                            "target_lang_translations": {
                                "en": "The northern <w>region</w> of the country is distinguished by a harsh climate and a long winter."
                            }
                        }
                    ],
                    "synonyms": [
                        "область"
                    ],
                    "common_phrases": [
                        "промышленный район",
                        "горный район",
                        "район бедствия"
                    ],
                    "target_lang_definitions": {
                        "en": "A geographic area or territory distinguished by specific features (climate, economy, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "region",
                                "target_lang_sense_clarification": "large geographic area"
                            },
                            {
                                "target_lang_word": "area",
                                "target_lang_sense_clarification": "geographic tract"
                            },
                            {
                                "target_lang_word": "zone",
                                "target_lang_sense_clarification": "area with specific characteristics"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0e376c07-2f04-3eb1-8ced-182526fb6cbe",
                    "sense_definition": "Niematerialny pierwiastek życia ludzkiego, stanowiący o istocie człowieka, często przeciwstawiany ciału; w wielu religiach uważana za nieśmiertelną",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "spirit_essence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu ludzi wierzy, że <w>dusza</w> wędruje do nieba po śmierci.",
                            "target_lang_translations": {
                                "ru": "Многие люди верят, что <w>душа</w> отправляется на небеса после смерти."
                            }
                        },
                        {
                            "text": "Ona jest moją bratnią <w>duszą</w>, rozumiemy się bez słów.",
                            "target_lang_translations": {
                                "ru": "Она моя родственная <w>душа</w>, мы понимаем друг друга без слов."
                            }
                        }
                    ],
                    "synonyms": [
                        "duch"
                    ],
                    "antonyms": [
                        "ciało"
                    ],
                    "common_phrases": [
                        "nieśmiertelna dusza",
                        "bratnia dusza",
                        "dusza i ciało"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нематериальное начало жизни человека, составляющее его сущность, часто противопоставляемое телу; во многих религиях считается бессмертной"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "душа",
                                "target_lang_sense_clarification": "бессмертная сущность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db75edb0-5698-31e8-b10e-5455be703653",
                    "sense_definition": "Człowiek lub osoba, zwykle w kontekście obecności lub jej braku w danym miejscu",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_metonymy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W całym budynku nie było ani jednej żywej <w>duszy</w>.",
                            "target_lang_translations": {
                                "ru": "Во всём здании не было ни одной живой <w>души</w>."
                            }
                        },
                        {
                            "text": "To dobra <w>dusza</w>, która zawsze pomaga potrzebującym sąsiadom.",
                            "target_lang_translations": {
                                "ru": "Это добрая <w>душа</w>, которая всегда помогает нуждающимся соседям."
                            }
                        }
                    ],
                    "common_phrases": [
                        "żywa dusza",
                        "dobra dusza",
                        "ani jednej duszy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или лицо, обычно в контексте присутствия или отсутствия в данном месте"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "душа",
                                "target_lang_sense_clarification": "человек (обычно в отрицаниях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "720f835c-4e62-3697-86e9-c9dbbfcdf7d8",
                    "sense_definition": "Wewnętrzna istota, charakter lub ożywiająca siła czegoś, np. grupy ludzi lub dzieła sztuki",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "spirit_essence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek to prawdziwa <w>dusza</w> towarzystwa, wszyscy go uwielbiają.",
                            "target_lang_translations": {
                                "ru": "Марек — настоящая <w>душа</w> компании, все его обожают."
                            }
                        },
                        {
                            "text": "Artysta włożył całą swoją <w>duszę</w> w namalowanie tego obrazu.",
                            "target_lang_translations": {
                                "ru": "Художник вложил всю свою <w>душу</w> в написание этой картины."
                            }
                        }
                    ],
                    "synonyms": [
                        "duch"
                    ],
                    "common_phrases": [
                        "dusza towarzystwa",
                        "włożyć w coś duszę",
                        "bez duszy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренняя сущность, характер или движущая сила чего-либо, например, группы людей или произведения искусства"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "душа",
                                "target_lang_sense_clarification": "вдохновитель, суть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8836df64-1781-31f8-82da-7f7685ffa376",
                    "sense_definition": "Metalowy wkład do starego typu żelazka, który rozgrzewano w ogniu, aby utrzymać temperaturę urządzenia",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum techniki widziałem stare żelazko z wyjmowaną <w>duszą</w>.",
                            "target_lang_translations": {
                                "ru": "В техническом музее я видел старый утюг со съемным <w>вкладышем</w>."
                            }
                        },
                        {
                            "text": "Kiedyś trzeba było włożyć rozgrzaną <w>duszę</w> do środka, żeby wyprasować ubranie.",
                            "target_lang_translations": {
                                "ru": "Раньше нужно было вложить раскаленный <w>вкладыш</w> внутрь, чтобы погладить одежду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dusza do żelazka",
                        "żelazko z duszą"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "dotyczy dawnych żelazek"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлический вкладыш для старинного утюга, который нагревали в огне для поддержания температуры"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкладыш",
                                "target_lang_sense_clarification": "нагревательный элемент утюга"
                            },
                            {
                                "target_lang_word": "плашка",
                                "target_lang_sense_clarification": "чугунная болванка для утюга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "342a6641-bbcc-363e-ab7f-5a9ef58ba2c1",
                    "sense_definition": "Mały kołeczek wewnątrz instrumentów smyczkowych łączący górną i dolną płytę pudła rezonansowego",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lutnik musiał precyzyjnie ustawić <w>duszę</w> w moich skrzypcach.",
                            "target_lang_translations": {
                                "ru": "Скрипичный мастер должен был точно установить <w>душку</w> в моей скрипке."
                            }
                        },
                        {
                            "text": "Jeśli <w>dusza</w> się przesunie, instrument traci swoje piękne brzmienie.",
                            "target_lang_translations": {
                                "ru": "Если <w>душка</w> сдвинется, инструмент теряет свое прекрасное звучание."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькая распорка внутри смычковых инструментов, соединяющая верхнюю и нижнюю деки"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "душка",
                                "target_lang_sense_clarification": "деталь скрипки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d560b51c-ab39-333c-b516-1d0e656d8dd2",
                    "sense_definition": "Wolna przestrzeń między biegami schodów",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci rzucały papierowe samoloty przez <w>duszę</w> schodów.",
                            "target_lang_translations": {
                                "ru": "Дети бросали бумажные самолетики в лестничный <w>просвет</w>."
                            }
                        },
                        {
                            "text": "Architekt zaprojektował klatkę schodową z szeroką <w>duszą</w>.",
                            "target_lang_translations": {
                                "ru": "Архитектор спроектировал лестничную клетку с широким <w>просветом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dusza schodów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободное пространство между лестничными маршами"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просвет",
                                "target_lang_sense_clarification": "между лестничными маршами"
                            },
                            {
                                "target_lang_word": "шахта",
                                "target_lang_sense_clarification": "лестничная"
                            }
                        ]
                    }
                }
            ]
        },

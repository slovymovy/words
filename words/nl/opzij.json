{
    "word_family": [
        "zijde",
        "zijkant",
        "zijdelings"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "81e1bd83-78c1-3c46-b9bc-0b3864f845d0",
                    "sense_definition": "Naar de zijkant toe; uit de weg om ruimte te maken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je even <w>opzij</w> gaan zodat ik erlangs kan?",
                            "target_lang_translations": {
                                "ru": "Ты можешь отойти <w>в сторону</w>, чтобы я мог пройти?"
                            }
                        },
                        {
                            "text": "Hij stapte snel <w>opzij</w> toen de fietser eraan kwam.",
                            "target_lang_translations": {
                                "ru": "Он быстро отступил <w>в сторону</w>, когда приближался велосипедист."
                            }
                        }
                    ],
                    "synonyms": [
                        "aan de kant"
                    ],
                    "common_phrases": [
                        "ga opzij",
                        "stap opzij",
                        "opzij gaan",
                        "opzij springen"
                    ],
                    "target_lang_definitions": {
                        "ru": "В сторону; с дороги, чтобы освободить место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в сторону",
                                "target_lang_sense_clarification": "направление движения"
                            },
                            {
                                "target_lang_word": "с дороги",
                                "target_lang_sense_clarification": "освобождая путь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47318bba-33cd-356f-be82-6e08be8a0ef9",
                    "sense_definition": "Aan de zijkant of gezien vanuit een positie aan de zijkant.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze afbeelding toont het nieuwe gebouw van <w>opzij</w>.",
                            "target_lang_translations": {
                                "ru": "Это изображение показывает новое здание <w>сбоку</w>."
                            }
                        },
                        {
                            "text": "Als je van <w>opzij</w> kijkt zie je het profiel beter.",
                            "target_lang_translations": {
                                "ru": "Если смотреть <w>сбоку</w>, профиль виден лучше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "van opzij",
                        "van opzij gezien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбоку или вид с боковой позиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбоку",
                                "target_lang_sense_clarification": "положение или вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d92dac4-74c0-3bc1-8caf-5bd86262338f",
                    "sense_definition": "Apart gehouden voor later gebruik of tijdelijk buiten beschouwing gelaten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij legde al enige tijd wat geld <w>opzij</w> voor zijn vakantie.",
                            "target_lang_translations": {
                                "ru": "Он уже некоторое время откладывал деньги <w>в сторону</w> для отпуска."
                            }
                        },
                        {
                            "text": "We moeten onze meningsverschillen even <w>opzij</w> zetten en samenwerken.",
                            "target_lang_translations": {
                                "ru": "Мы должны отбросить наши разногласия <w>в сторону</w> и работать вместе."
                            }
                        }
                    ],
                    "synonyms": [
                        "apart"
                    ],
                    "common_phrases": [
                        "geld opzij leggen",
                        "opzij zetten",
                        "alles opzij zetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отложенный для использования позже или временно оставленный без внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в сторону",
                                "target_lang_sense_clarification": "фигурально: отбросить (сомнения, разногласия)"
                            },
                            {
                                "target_lang_word": "на потом",
                                "target_lang_sense_clarification": "для будущего использования"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "a6f81fa2-76cb-3a7f-a10a-c3bc27df1874",
                    "sense_definition": "Apart van anderen of de omgeving; zonder contact met de groep.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "isolation_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij woont in een <w>afgezonderd</w> huisje diep in het bos.",
                            "target_lang_translations": {
                                "en": "He lives in a <w>secluded</w> cottage deep in the forest.",
                                "ru": "Он живет в <w>уединённом</w> домике глубоко в лесу."
                            }
                        },
                        {
                            "text": "Door de zware sneeuwstorm raakte het bergdorp volledig <w>afgezonderd</w>.",
                            "target_lang_translations": {
                                "en": "Due to the heavy snowstorm, the mountain village became completely <w>isolated</w>.",
                                "ru": "Из-за сильной метели горная деревня оказалась полностью <w>изолированной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "geïsoleerd",
                        "apart",
                        "eenzaam"
                    ],
                    "antonyms": [
                        "verbonden",
                        "samen"
                    ],
                    "common_phrases": [
                        "afgezonderd leven",
                        "afgezonderd liggen",
                        "volledig afgezonderd"
                    ],
                    "target_lang_definitions": {
                        "en": "Separate from others or the surroundings; without contact with the group.",
                        "ru": "Отдельно от других или окружения; без контакта с группой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "isolated",
                                "target_lang_sense_clarification": "general state of being alone or cut off"
                            },
                            {
                                "target_lang_word": "secluded",
                                "target_lang_sense_clarification": "private and sheltered, often regarding places"
                            },
                            {
                                "target_lang_word": "remote",
                                "target_lang_sense_clarification": "far away from populated areas"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уединённый",
                                "target_lang_sense_clarification": "скрытый, удаленный от людей (о месте или жизни)"
                            },
                            {
                                "target_lang_word": "изолированный",
                                "target_lang_sense_clarification": "отрезанный от внешнего мира"
                            }
                        ]

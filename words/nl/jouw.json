    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "cf8f5fe9-94c6-3c8f-9221-f99c1d09ca19",
                    "sense_definition": "Van jou; geeft bezit of een relatie aan met betrekking tot de aangesprokene (nadrukkelijke vorm).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possessive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Is dit <w>jouw</w> fiets of die van je broer?",
                            "target_lang_translations": {
                                "ru": "Это <w>твой</w> велосипед или твоего брата?"
                            }
                        },
                        {
                            "text": "Ik wil graag <w>jouw</w> mening horen over dit onderwerp.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы услышать <w>твоё</w> мнение по этому вопросу."
                            }
                        }
                    ],
                    "synonyms": [
                        "jouwn",
                        "jouwn"
                    ],
                    "antonyms": [
                        "mijn"
                    ],
                    "common_phrases": [
                        "jouw beurt",
                        "jouw schuld",
                        "jouw keuze",
                        "op jouw manier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий тебе; указывает на владение или отношение к собеседнику (акцентированная форма)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "твой",
                                "target_lang_sense_clarification": "притяжательное местоимение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c354fd42-df28-3b32-bd0a-8714842e50d5",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>jouwen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>jouw</w> de spelers uit als ze slecht spelen.",
                            "target_lang_translations": {
                                "ru": "Я <w>освистываю</w> игроков, когда они плохо играют."
                            }
                        },
                        {
                            "text": "Waarom <w>jouw</w> ik eigenlijk mee met de rest?",
                            "target_lang_translations": {
                                "ru": "Почему я вообще <w>улюлюкаю</w> вместе с остальными?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени от глагола <w>jouwen</w> (освистывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освистываю",
                                "target_lang_sense_clarification": "форма глагола 'освистывать'"
                            },
                            {
                                "target_lang_word": "улюлюкаю",
                                "target_lang_sense_clarification": "форма глагола 'улюлюкать'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8825c486-77a2-358d-b6ee-e54b424b1320",
                    "sense_definition": "Gebiedende wijs van <w>jouwen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Jouw</w> niet zo naar die arme man!",
                            "target_lang_translations": {
                                "ru": "Не <w>освистывай</w> так этого беднягу!"
                            }
                        },
                        {
                            "text": "<w>Jouw</w> maar raak, hij trekt zich er niets van aan.",
                            "target_lang_translations": {
                                "ru": "<w>Улюлюкай</w> сколько влезет, ему всё равно."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола <w>jouwen</w> (освистывать)."

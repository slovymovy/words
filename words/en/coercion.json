    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "103441dd-7749-3e1e-b3f3-adcbbeeccdd0",
                    "sense_definition": "The action of making someone do something by using force or threats.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The suspect claimed that he signed the confession under <w>coercion</w> from the police.",
                            "target_lang_translations": {
                                "ru": "Подозреваемый заявил, что подписал признание под <w>принуждением</w> со стороны полиции."
                            }
                        },
                        {
                            "text": "Any form of physical <w>coercion</w> is strictly prohibited during the interrogation process.",
                            "target_lang_translations": {
                                "ru": "Любая форма физического <w>принуждения</w> строго запрещена в процессе допроса."
                            }
                        }
                    ],
                    "synonyms": [
                        "compulsion",
                        "duress",
                        "force",
                        "pressure"
                    ],
                    "antonyms": [
                        "noncoercion"
                    ],
                    "common_phrases": [
                        "under coercion",
                        "physical coercion",
                        "political coercion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, вынуждающее кого-либо сделать что-то с помощью силы или угроз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принуждение",
                                "target_lang_sense_clarification": "давление, нажим"
                            },
                            {
                                "target_lang_word": "насилие",
                                "target_lang_sense_clarification": "применение физической силы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2e653c1-67f6-39a4-a656-12fd4b1a8cb7",
                    "sense_definition": "The conversion of a value from one data type to another.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In JavaScript, type <w>coercion</w> can sometimes lead to unexpected results when comparing variables.",
                            "target_lang_translations": {
                                "ru": "В JavaScript <w>приведение</w> типов иногда может привести к неожиданным результатам при сравнении переменных."
                            }
                        },
                        {
                            "text": "The compiler handles implicit <w>coercion</w> automatically, but explicit casting is often safer.",
                            "target_lang_translations": {
                                "ru": "Компилятор обрабатывает неявное <w>приведение</w> автоматически, но явное преобразование часто безопаснее."
                            }
                        }
                    ],
                    "synonyms": [
                        "type conversion"
                    ],
                    "common_phrases": [
                        "type coercion",
                        "implicit coercion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Преобразование значения из одного типа данных в другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приведение",
                                "target_lang_sense_clarification": "типов данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7b63a83-fb73-331d-8f65-eb02b6f35cb0",
                    "sense_definition": "The reinterpretation of a word's meaning to match the grammatical context.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Semantic <w>coercion</w> occurs when a mass noun is treated as a count noun in a specific sentence.",
                            "target_lang_translations": {
                                "ru": "Семантическое <w>принуждение</w> происходит, когда неисчисляемое существительное рассматривается как исчисляемое в конкретном предложении."
                            }
                        },
                        {
                            "text": "The theory of aspectual <w>coercion</w> explains how the verb adapts to the progressive form.",
                            "target_lang_translations": {
                                "ru": "Теория аспектуального <w>принуждения</w> объясняет, как глагол адаптируется к длительной форме."
                            }
                        }
                    ],
                    "common_phrases": [
                        "semantic coercion",
                        "aspectual coercion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переосмысление значения слова для соответствия грамматическому контексту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принуждение",
                                "target_lang_sense_clarification": "языковое, контекстуальное"
                            },
                            {
                                "target_lang_word": "коэрция",
                                "target_lang_sense_clarification": "лингвистический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
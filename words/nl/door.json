                },
                {
                    "sense_id": "4ef0251e-b98e-3faa-bb96-d514a049ce7e",
                    "sense_definition": "Met behulp van; geeft de manier of het middel aan waarmee iets wordt bereikt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "means",

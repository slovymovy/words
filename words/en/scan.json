{
    "word_family": [
        "scanner",
        "scannable",
        "rescan"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0f2dee30-7125-39d9-8253-5b2d838edc85",
                    "sense_definition": "To use a machine to copy a document or picture into a computer.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "digital_processing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>scan</w> this contract and email it to the office.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>отсканировать</w> этот контракт и отправить его по электронной почте в офис."
                            }
                        },
                        {
                            "text": "Old family photos can be <w>scanned</w> to preserve them digitally.",
                            "target_lang_translations": {
                                "ru": "Старые семейные фотографии можно <w>отсканировать</w>, чтобы сохранить их в цифровом виде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "scan a document",
                        "scan a photo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать устройство для копирования документа или изображения в компьютер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сканировать",
                                "target_lang_sense_clarification": "переводить в цифровую форму"
                            },
                            {
                                "target_lang_word": "отсканировать",
                                "target_lang_sense_clarification": "сделать цифровую копию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f38ccd5-de72-3ae4-bf11-abb288cad71b",
                    "sense_definition": "To read information from a barcode, ticket, or card using an electronic device.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "digital_processing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cashier <w>scanned</w> the groceries quickly at the checkout.",
                            "target_lang_translations": {
                                "ru": "Кассир быстро <w>просканировал</w> продукты на кассе."
                            }
                        },
                        {
                            "text": "You must <w>scan</w> your ID badge to open the security gate.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>отсканировать</w> свой пропуск, чтобы открыть ворота безопасности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "scan a barcode",
                        "scan a ticket"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считывать информацию со штрих-кода, билета или карты с помощью электронного устройства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сканировать",
                                "target_lang_sense_clarification": "считывать код устройством"
                            },
                            {
                                "target_lang_word": "считывать",
                                "target_lang_sense_clarification": "получать данные с носителя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "858f3e53-b194-3961-a85d-03ca00c7f8a2",
                    "sense_definition": "To look through a text or look around a place quickly to find specific information or a person.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "visual_examination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>scanned</w> the list of names to find her own.",
                            "target_lang_translations": {
                                "ru": "Она <w>пробежала глазами</w> список имен, чтобы найти свое."
                            }
                        },
                        {
                            "text": "He <w>scanned</w> the crowded room looking for an empty seat.",
                            "target_lang_translations": {
                                "ru": "Он <w>окинул взглядом</w> переполненную комнату в поисках свободного места."
                            }
                        }
                    ],
                    "synonyms": [
                        "perusal",
                        "perusal"
                    ],
                    "common_phrases": [
                        "scan the crowd",
                        "scan the headlines"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро просматривать текст или осматривать место в поисках конкретной информации или человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просматривать",
                                "target_lang_sense_clarification": "бегло читать или искать в тексте"
                            },
                            {
                                "target_lang_word": "осматривать",
                                "target_lang_sense_clarification": "искать взглядом в пространстве"
                            },
                            {
                                "target_lang_word": "пробегать глазами",
                                "target_lang_sense_clarification": "быстро читать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53cf3fa4-abbc-34eb-85e0-927e290fbce1",
                    "sense_definition": "To examine an area or system systematically to detect something hidden or dangerous.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_examination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The antivirus software <w>scans</w> the hard drive for threats every day.",
                            "target_lang_translations": {
                                "ru": "Антивирусное программное обеспечение <w>сканирует</w> жесткий диск на наличие угроз каждый день."
                            }
                        },
                        {
                            "text": "The sailor <w>scanned</w> the horizon for any sign of land.",
                            "target_lang_translations": {
                                "ru": "Моряк <w>осматривал</w> горизонт в поисках признаков земли."
                            }
                        }
                    ],
                    "synonyms": [
                        "perusal",
                        "perusal"
                    ],
                    "common_phrases": [
                        "scan for viruses",
                        "scan the horizon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Систематически проверять область или систему для обнаружения чего-либо скрытого или опасного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сканировать",
                                "target_lang_sense_clarification": "проверять программно или аппаратно"
                            },
                            {
                                "target_lang_word": "осматривать",
                                "target_lang_sense_clarification": "внимательно изучать пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96011033-8308-3d97-bae7-a44c403746b7",
                    "sense_definition": "To analyze the rhythm and meter of a line of poetry.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "prosody",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The literature students were asked to <w>scan</w> the verses for homework.",
                            "target_lang_translations": {
                                "ru": "Студентов-литературоведов попросили <w>проскандировать</w> стихи в качестве домашнего задания."
                            }
                        },
                        {
                            "text": "It can be difficult to <w>scan</w> this poem because the rhythm changes frequently.",
                            "target_lang_translations": {
                                "ru": "Может быть трудно <w>скандировать</w> это стихотворение, потому что ритм часто меняется."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Анализировать ритм и размер стихотворной строки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скандировать",
                                "target_lang_sense_clarification": "разбирать стихотворный размер"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a60ce75f-bf85-3a3f-8ecd-101d3c11e0a7",
                    "sense_definition": "A medical examination that uses a special machine to produce an image of the inside of the body, or the image itself.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical_imaging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor ordered a brain <w>scan</w> to check for any injuries.",
                            "target_lang_translations": {
                                "ru": "Врач назначил <w>сканирование</w> мозга, чтобы проверить наличие травм."
                            }
                        },
                        {
                            "text": "Fortunately, the <w>scan</w> showed that there were no broken bones.",
                            "target_lang_translations": {
                                "ru": "К счастью, <w>снимок</w> показал, что кости не сломаны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "CT scan",
                        "MRI scan",
                        "ultrasound scan"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "medical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Медицинское обследование с использованием специального аппарата для получения изображения внутренних органов, или само изображение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сканирование",
                                "target_lang_sense_clarification": "процедура обследования"
                            },
                            {
                                "target_lang_word": "снимок",
                                "target_lang_sense_clarification": "результат обследования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "492c0bfe-d187-3657-ac44-a214342c2a03",
                    "sense_definition": "An act of looking at something, often quickly.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_examination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Give the report a quick <w>scan</w> before the meeting starts.",
                            "target_lang_translations": {
                                "ru": "Быстро <w>просмотри</w> отчет перед началом собрания."
                            }
                        },
                        {
                            "text": "A brief <w>scan</w> of the room revealed nothing unusual.",
                            "target_lang_translations": {
                                "ru": "Беглый <w>осмотр</w> комнаты не выявил ничего необычного."
                            }
                        }
                    ],
                    "synonyms": [
                        "perusal"
                    ],
                    "common_phrases": [
                        "a quick scan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в быстром осмотре или просмотре чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просмотр",
                                "target_lang_sense_clarification": "беглое ознакомление"
                            },
                            {
                                "target_lang_word": "осмотр",
                                "target_lang_sense_clarification": "визуальная проверка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
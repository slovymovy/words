            "senses": [
                {
                    "sense_id": "f9ede567-cf98-3f53-b39e-51daf267aff0",
                    "sense_definition": "Вызывать интерес, любопытство или симпатию; притягивать к себе внимание.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "attract_interest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта яркая вывеска всегда <привлекает> внимание прохожих.",
                            "target_lang_translations": {
                                "en": "This bright sign always <attracts> the attention of passers-by."
                            }
                        },
                        {
                            "text": "Громкая музыка <привлекает> много людей на центральную площадь.",
                            "target_lang_translations": {
                                "en": "Loud music <draws> many people to the central square."
                            }
                        }
                    ],
                    "synonyms": [
                        "интересовать",
                        "манить",
                        "притягивать"
                    ],
                    "antonyms": [
                        "отталкивать"
                    ],
                    "common_phrases": [
                        "привлекать внимание",
                        "привлекать взгляды",
                        "привлекать клиентов"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause interest, curiosity, or sympathy; to draw attention to oneself."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attract",
                                "target_lang_sense_clarification": "interest or attention"
                            },

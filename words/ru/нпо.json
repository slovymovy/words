                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for \"non-governmental organization\"; a public entity created independently of the government to address social, political, or environmental issues."
                    },
                    "translations": {

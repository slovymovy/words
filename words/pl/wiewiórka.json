    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8cf626d4-ec20-3a40-a95b-1ffd11635aeb",
                    "sense_definition": "Nadrzewny gryzoń o puszystym, najczęściej rudym futrze oraz długiej kicie, żywiący się głównie orzechami i nasionami.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj w parku widziałem małą <w>wiewiórkę</w> skaczącą po drzewach.",
                            "target_lang_translations": {
                                "ru": "Вчера в парке я видел маленькую <w>белку</w>, прыгающую по деревьям."
                            }
                        },
                        {
                            "text": "Ruda <w>wiewiórka</w> szybko uciekła na widok psa.",
                            "target_lang_translations": {
                                "ru": "Рыжая <w>белка</w> быстро убежала при виде собаки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ruda wiewiórka",
                        "zwinna wiewiórka",
                        "wiewiórka pospolita"
                    ],
                    "target_lang_definitions": {
                        "ru": "Древесный грызун с пушистым, чаще всего рыжим мехом и длинным хвостом, питающийся в основном орехами и семенами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "белка",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a120b6d1-0372-3598-92d2-0179cc965fcc",
                    "sense_definition": "Osoba, zazwyczaj kobieta, o jaskrawo rudych włosach.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra to typowa <w>wiewiórka</w>, wszyscy zazdroszczą jej koloru włosów.",
                            "target_lang_translations": {
                                "ru": "Моя сестра — типичная <w>рыжая</w>, все завидуют цвету её волос."
                            }
                        },
                        {
                            "text": "Kim jest ta wesoła <w>wiewiórka</w> w zielonej sukience?",
                            "target_lang_translations": {
                                "ru": "Кто эта весёлая <w>рыжая</w> в зелёном платье?"
                            }
                        }
                    ],
                    "synonyms": [
                        "rudzielec",
                        "marchewka"
                    ],
                    "common_phrases": [
                        "farbowana wiewiórka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обычно женщина, с ярко-рыжими волосами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыжая",
                                "target_lang_sense_clarification": "о рыжеволосой женщине"
                            },
                            {
                                "target_lang_word": "рыжик",
                                "target_lang_sense_clarification": "ласковое прозвище"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c141b87-58e1-32b2-b577-fc32c63c372f",
                    "sense_definition": "Futro wykonane ze skór wiewiórek.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starej szafie wisiały molem nadgryzione <w>wiewiórki</w>.",
                            "target_lang_translations": {
                                "ru": "В старом шкафу висели побитые молью <w>белки</w>."
                            }
                        },
                        {
                            "text": "Niegdyś płaszcz podbity <w>wiewiórkami</w> był oznaką zamożności.",
                            "target_lang_translations": {
                                "ru": "Когда-то пальто, подбитое <w>белками</w>, было признаком богатства."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мех, выделанный из шкурок белок."

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "58b8abe8-a5d8-39cd-bb50-7f41ee4a3317",
                    "sense_definition": "Free from error; true; accurate.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "accuracy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your answer to the math problem is <w>correct</w>.",
                            "target_lang_translations": {
                                "ru": "Твой ответ на задачу по математике <w>правильный</w>."
                            }
                        },
                        {
                            "text": "We need to make sure we have the <w>correct</w> information before we leave.",
                            "target_lang_translations": {
                                "ru": "Нам нужно убедиться, что у нас <w>верная</w> информация, прежде чем мы уйдем."
                            }
                        }
                    ],
                    "synonyms": [
                        "right",
                        "faithful"
                    ],
                    "antonyms": [
                        "incorrect",
                        "inaccurate",
                        "wrong"
                    ],
                    "common_phrases": [
                        "correct answer",
                        "correct decision",
                        "correct way"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не содержащий ошибок; истинный; точный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правильный",
                                "target_lang_sense_clarification": "самый общий и употребительный перевод"
                            },
                            {
                                "target_lang_word": "верный",
                                "target_lang_sense_clarification": "подразумевает истинность или точность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a20f2c73-3c6b-34a6-9624-2800e8a39666",
                    "sense_definition": "With good manners; conforming with accepted standards of behavior.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is not considered <w>correct</w> to speak loudly in a library.",
                            "target_lang_translations": {
                                "ru": "Не считается <w>корректным</w> громко разговаривать в библиотеке."
                            }
                        },
                        {
                            "text": "He always behaves in a very <w>correct</w> and polite manner.",
                            "target_lang_translations": {
                                "ru": "Он всегда ведет себя очень <w>корректно</w> и вежливо."
                            }
                        }
                    ],
                    "synonyms": [
                        "well-mannered",
                        "well-behaved"
                    ],
                    "antonyms": [
                        "uncouth"
                    ],
                    "common_phrases": [
                        "correct behavior",
                        "correct etiquette"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий хорошими манерами; соответствующий принятым нормам поведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корректный",
                                "target_lang_sense_clarification": "вежливый, тактичный, соблюдающий приличия"
                            },
                            {
                                "target_lang_word": "воспитанный",
                                "target_lang_sense_clarification": "имеющий хорошее воспитание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ce3454ad-097b-361f-9cc0-54b379f72c90",
                    "sense_definition": "To make something that was wrong become right; to remove error from.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "rectify",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>correct</w> the spelling mistakes in your essay.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>исправь</w> орфографические ошибки в своем эссе."
                            }
                        },
                        {
                            "text": "The mechanic <w>corrected</w> the problem with the engine.",
                            "target_lang_translations": {
                                "ru": "Механик <w>исправил</w> проблему с двигателем."
                            }
                        }
                    ],
                    "synonyms": [
                        "right",
                        "faithful"
                    ],
                    "common_phrases": [
                        "correct a mistake",
                        "correct a problem",
                        "correct posture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать правильным то, что было неправильным; устранить ошибку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исправлять",
                                "target_lang_sense_clarification": "делать лучше, устранять ошибки"
                            },
                            {
                                "target_lang_word": "корректировать",
                                "target_lang_sense_clarification": "вносить поправки, уточнять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "597cf24e-d56d-36d7-8f8d-b3689d8be52a",
                    "sense_definition": "To inform someone of their error.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "inform",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>correct</w> me if I am wrong about the time.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>поправь</w> меня, если я ошибаюсь насчет времени."
                            }
                        },
                        {
                            "text": "It is rude to <w>correct</w> people in front of others.",
                            "target_lang_translations": {
                                "ru": "Невежливо <w>поправлять</w> людей при посторонних."
                            }
                        }
                    ],
                    "common_phrases": [
                        "correct me if I'm wrong"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указать кому-либо на его ошибку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поправлять",
                                "target_lang_sense_clarification": "указывать на ошибку в речи или действии"
                            },
                            {
                                "target_lang_word": "исправлять",
                                "target_lang_sense_clarification": "вносить исправления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "036b027f-8256-3e72-b957-78c8e1aba3cd",
                    "sense_definition": "To grade examination papers or assignments.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "grading",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher stayed up late to <w>correct</w> the exams.",
                            "target_lang_translations": {
                                "ru": "Учитель засиделся допоздна, чтобы <w>проверить</w> экзамены."
                            }
                        },
                        {
                            "text": "She spends her weekends <w>correcting</w> student homework.",
                            "target_lang_translations": {
                                "ru": "Она проводит выходные, <w>проверяя</w> домашние задания студентов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "correct exams",
                        "correct papers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проверять экзаменационные работы или задания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверять",
                                "target_lang_sense_clarification": "оценивать правильность выполнения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3732538-3f9d-36af-bf40-aca5f47ad4bf",
                    "sense_definition": "To discipline or punish.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "discipline",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the past, teachers were allowed to <w>correct</w> students physically.",
                            "target_lang_translations": {
                                "ru": "В прошлом учителям разрешалось <w>наказывать</w> учеников физически."
                            }
                        },
                        {
                            "text": "He was <w>corrected</w> for his disobedience.",
                            "target_lang_translations": {
                                "ru": "Он был <w>наказан</w> за свое непослушание."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly used in older contexts regarding discipline"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дисциплинировать или наказывать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наказывать",
                                "target_lang_sense_clarification": "применять меры воздействия за проступок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "720aaed8-776d-34e8-a375-8def7f999d7f",
                    "sense_definition": "Used to indicate acknowledgement or acceptance.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Correct</w>! That is the right answer.",
                            "target_lang_translations": {
                                "ru": "<w>Правильно</w>! Это верный ответ."
                            }
                        },
                        {
                            "text": "You want to go to the park? <w>Correct</w>, let's go.",
                            "target_lang_translations": {
                                "ru": "Ты хочешь пойти в парк? <w>Верно</w>, пошли."
                            }
                        }
                    ],
                    "synonyms": [

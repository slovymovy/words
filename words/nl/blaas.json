    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4cb8ce5d-b3e8-303b-b511-9b64fcea411e",
                    "sense_definition": "Een hol orgaan in het lichaam waarin urine wordt opgeslagen voordat het wordt uitgescheiden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet dringend naar het toilet omdat ik een volle <w>blaas</w> heb.",
                            "target_lang_translations": {
                                "ru": "Мне срочно нужно в туалет, потому что у меня полный <w>мочевой пузырь</w>."
                            }
                        },
                        {
                            "text": "De arts onderzocht zijn <w>blaas</w> om de oorzaak van de pijn te vinden.",
                            "target_lang_translations": {
                                "ru": "Врач осмотрел его <w>мочевой пузырь</w>, чтобы найти причину боли."
                            }
                        }
                    ],
                    "synonyms": [
                        "urineblaas"
                    ],
                    "common_phrases": [
                        "volle blaas",
                        "lege blaas",
                        "op de blaas werken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полый орган в теле, в котором накапливается моча перед выведением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мочевой пузырь",
                                "target_lang_sense_clarification": "анатомический термин"
                            },
                            {
                                "target_lang_word": "пузырь",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eea543e4-b051-334c-96d3-497739af6ad9",
                    "sense_definition": "Een met vocht of gas gevulde holte, zoals een blaar op de huid of een luchtbel in materiaal.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de hitte ontstond er een grote <w>blaas</w> in de verflaag op de deur.",
                            "target_lang_translations": {
                                "ru": "Из-за жары на слое краски на двери вздулся большой <w>пузырь</w>."
                            }
                        },
                        {
                            "text": "Na de lange wandeling had hij een pijnlijke <w>blaas</w> op zijn hiel.",
                            "target_lang_translations": {
                                "ru": "После долгих прогулок у него на пятке появился болезненный <w>волдырь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "blaar",
                        "bel",
                        "bubbel"
                    ],
                    "common_phrases": [
                        "blaasjes wijsmaken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полость, заполненная жидкостью или газом, например, волдырь на коже или пузырек воздуха в материале."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пузырь",
                                "target_lang_sense_clarification": "вздутие на поверхности или в материале"
                            },
                            {
                                "target_lang_word": "волдырь",
                                "target_lang_sense_clarification": "болезненное вздутие на коже"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4a038c03-9c3b-3746-90fd-04d159a6a46f",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>blazen</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>blaas</w> voorzichtig de stofjes van mijn brilglazen.",
                            "target_lang_translations": {
                                "ru": "Я осторожно <w>сдуваю</w> пылинки со стекол моих очков."
                            }
                        },
                        {
                            "text": "Elke keer als ik hete soep eet, <w>blaas</w> ik eerst op de lepel.",
                            "target_lang_translations": {
                                "ru": "Каждый раз, когда я ем горячий суп, я сначала <w>дую</w> на ложку."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола <w>blazen</w> (дуть)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дую",
                                "target_lang_sense_clarification": "я дую"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5be16ed5-07bb-3cfb-bcb6-494ef6eea487",
                    "sense_definition": "Gebiedende wijs van <w>blazen</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Blaas</w> de kaarsjes op de taart maar in één keer uit.",
                            "target_lang_translations": {
                                "ru": "<w>Задуй</w> свечи на торте за один раз."
                            }
                        },
                        {
                            "text": "<w>Blaas</w> niet zo hard op dat fluitje, want dat doet pijn aan mijn oren.",
                            "target_lang_translations": {
                                "ru": "Не <w>дуй</w> так сильно в этот свисток, потому что это режет мне слух."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола <w>blazen</w> (дуть)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуй",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae93a265-3411-3048-9de9-7411434b3cf0",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>blazen</w> (bij inversie).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waarom <w>blaas</w> je de ballon niet helemaal op?",
                            "target_lang_translations": {
                                "ru": "Почему ты не <w>надуваешь</w> шарик полностью?"
                            }
                        },
                        {
                            "text": "<w>Blaas</w> je even mee om het vuur aan te wakkeren?",
                            "target_lang_translations": {
                                "ru": "<w>Подуешь</w> со мной, чтобы раздуть огонь?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма второго лица единственного числа настоящего времени от глагола <w>blazen</w> (дуть)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуешь",
                                "target_lang_sense_clarification": "ты дуешь"
                            }
                        ]
                    }
                }
            ]
        }

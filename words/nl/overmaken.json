                {
                    "sense_id": "dd827596-86f4-36cc-afd5-b5e739b9a8da",
                    "sense_definition": "Geld van de ene bankrekening naar de andere sturen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb het geld gisteren naar je rekening <w>overgemaakt</w>.",
                            "target_lang_translations": {
                                "en": "I <w>transferred</w> the money to your account yesterday.",
                                "ru": "Я вчера <w>перевел</w> деньги на твой счет."
                            }
                        },
                        {
                            "text": "Je kunt het openstaande bedrag eenvoudig via internetbankieren <w>overmaken</w>.",
                            "target_lang_translations": {
                                "en": "You can easily <w>transfer</w> the outstanding amount via internet banking.",
                                "ru": "Вы можете легко <w>перевести</w> непогашенную сумму через интернет-банкинг."
                            }

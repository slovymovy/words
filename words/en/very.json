{
    "word_family": [
        "verify",
        "verity",
        "veritable",
        "verily"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "900fd3b2-ff01-3e2d-987a-466899c3ccf7",
                    "sense_definition": "To a high degree or extent; used to emphasize an adjective or adverb.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was <w>very</w> tired after the long journey.",
                            "target_lang_translations": {
                                "ru": "Она была <w>очень</w> уставшей после долгого путешествия."
                            }
                        },
                        {
                            "text": "Thank you <w>very</w> much for your help.",
                            "target_lang_translations": {
                                "ru": "<w>Огромное</w> спасибо за вашу помощь."
                            }
                        },
                        {
                            "text": "The car was moving <w>very</w> slowly down the street.",
                            "target_lang_translations": {
                                "ru": "Машина двигалась <w>очень</w> медленно по улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "extremely",
                        "really",
                        "highly",
                        "exceedingly"
                    ],
                    "antonyms": [
                        "slightly"
                    ],
                    "common_phrases": [
                        "very much",
                        "very good",
                        "very well",
                        "not very"
                    ],
                    "target_lang_definitions": {
                        "ru": "В высокой степени или мере; используется для усиления прилагательного или наречия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о́чень",
                                "target_lang_sense_clarification": "наиболее употребительное слово"
                            },
                            {
                                "target_lang_word": "весьма́",
                                "target_lang_sense_clarification": "книжное, формальное"
                            },
                            {
                                "target_lang_word": "изря́дно",
                                "target_lang_sense_clarification": "разговорное, в значительной степени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8499bea6-5414-3ebf-9fe1-59329369e983",
                    "sense_definition": "Used to emphasize a superlative adjective or the word 'own' to show that something is unique or extreme.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This is my <w>very</w> own room, and I can decorate it however I like.",
                            "target_lang_translations": {
                                "ru": "Это моя <w>собственная</w> комната, и я могу украсить её как захочу."
                            }
                        },
                        {
                            "text": "He did his <w>very</w> best to win the competition.",
                            "target_lang_translations": {
                                "ru": "Он сделал <w>всё</w> возможное, чтобы выиграть соревнование."
                            }
                        },
                        {
                            "text": "We sat in the <w>very</w> front row of the theater.",
                            "target_lang_translations": {
                                "ru": "Мы сидели в <w>самом</w> первом ряду театра."
                            }
                        }
                    ],
                    "synonyms": [
                        "absolutely"
                    ],
                    "common_phrases": [
                        "very best",
                        "very own",
                        "very first",
                        "very last"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления превосходной степени прилагательного или слова «собственный», чтобы показать уникальность или исключительность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "са́мый",
                                "target_lang_sense_clarification": "используется с превосходной степенью"
                            },
                            {
                                "target_lang_word": "со́бственный",
                                "target_lang_sense_clarification": "в значении «very own»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "80f6bf67-cb99-36ed-b4e1-0c6fea1e6e50",
                    "sense_definition": "Used to emphasize that you are referring to a specific, exact thing, place, or time, and not another.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emphasis_exactness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You are the <w>very</w> person I was looking for.",
                            "target_lang_translations": {
                                "ru": "Вы — <w>тот самый</w> человек, которого я искал."
                            }
                        },
                        {
                            "text": "We arrived at the station at the <w>very</w> end of the day.",
                            "target_lang_translations": {
                                "ru": "Мы прибыли на станцию в <w>самом</w> конце дня."
                            }
                        },
                        {
                            "text": "This is the <w>very</w> thing we need to fix the problem.",
                            "target_lang_translations": {
                                "ru": "Это <w>та самая</w> вещь, которая нам нужна, чтобы решить проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "exact",
                        "actual",
                        "precise",
                        "selfsame"
                    ],
                    "common_phrases": [
                        "the very thing",
                        "the very end",
                        "the very beginning",
                        "the very same"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для того, чтобы подчеркнуть, что вы имеете в виду конкретный, точный предмет, место или время, а не что-то иное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тот са́мый",
                                "target_lang_sense_clarification": "указание на идентичность"
                            },
                            {
                                "target_lang_word": "са́мый",
                                "target_lang_sense_clarification": "указание на точное время или место (напр. «в самом конце»)"
                            },
                            {
                                "target_lang_word": "и́менно",
                                "target_lang_sense_clarification": "уточнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c356237-12db-3d2e-9bf8-41e1de1aaca3",
                    "sense_definition": "Used to emphasize that a particular thing is enough to produce a specific result, even if it seems small or simple.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "emphasis_mere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>very</w> thought of flying makes him feel nervous.",
                            "target_lang_translations": {
                                "ru": "<w>Сама</w> мысль о полете заставляет его нервничать."
                            }
                        },
                        {
                            "text": "The <w>very</w> idea that he could steal is ridiculous.",
                            "target_lang_translations": {
                                "ru": "<w>Сама</w> идея, что он мог украсть, нелепа."
                            }
                        }
                    ],
                    "synonyms": [
                        "mere",
                        "simple"
                    ],
                    "common_phrases": [
                        "the very idea",
                        "the very thought"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для подчеркивания того, что даже упоминания или наличия чего-то одного достаточно для определенного результата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сам",
                                "target_lang_sense_clarification": "напр. «сама мысль»"
                            },
                            {
                                "target_lang_word": "оди́н",
                                "target_lang_sense_clarification": "в значении «только», «лишь»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7098aa66-bcb8-35aa-a948-245bcbe65efd",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Very</w> was known for his contributions to the local library.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Вери</w> был известен своим вкладом в местную библиотеку."
                            }
                        },
                        {
                            "text": "We met a family named <w>Very</w> during our travels.",
                            "target_lang_translations": {
                                "ru": "Мы встретили семью по фамилии <w>Вери</w> во время наших путешествий."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ве́ри",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "764bbe48-6832-30a0-a148-4c34f64a3ab0",
                    "sense_definition": "To push a finger, stick, or other pointed object quickly into or against someone or something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She used a stick to <w>poke</w> the fire and make it burn brighter.",
                            "target_lang_translations": {
                                "ru": "Она использовала палку, чтобы <w>поворошить</w> костёр и заставить его гореть ярче."
                            }
                        },
                        {
                            "text": "Stop trying to <w>poke</w> me in the ribs while I am driving.",
                            "target_lang_translations": {
                                "ru": "Перестань пытаться <w>ткнуть</w> меня в ребра, пока я за рулем."
                            }
                        }
                    ],
                    "synonyms": [
                        "prod",
                        "jab",
                        "dig",
                        "thrust"
                    ],
                    "common_phrases": [
                        "poke in the eye",
                        "poke with a stick",
                        "poke fun at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро толкать пальцем, палкой или другим острым предметом внутрь или в поверхность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тыкать",
                                "target_lang_sense_clarification": "несовершенный вид; наносить легкие удары"
                            },
                            {
                                "target_lang_word": "ткнуть",
                                "target_lang_sense_clarification": "совершенный вид; сделать быстрый тычок"
                            },
                            {
                                "target_lang_word": "шуровать",
                                "target_lang_sense_clarification": "ворошить (обычно об огне или углях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77feb726-63e1-3d99-bd50-a33be0918760",
                    "sense_definition": "To send a digital notification to get someone's attention on a social media platform.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "digital_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I decided to <w>poke</w> my old friend on Facebook to see if he would respond.",
                            "target_lang_translations": {
                                "ru": "Я решил <w>ткнуть</w> старого друга в Фейсбуке, чтобы посмотреть, ответит ли он."
                            }
                        },
                        {
                            "text": "If you <w>poke</w> him online, he might notice your message sooner.",
                            "target_lang_translations": {
                                "ru": "Если ты <w>ткнешь</w> его в сети, он может заметить твое сообщение раньше."
                            }
                        }
                    ],
                    "synonyms": [
                        "nudge",
                        "ping"
                    ],
                    "common_phrases": [
                        "poke a friend",
                        "send a poke"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправить цифровое уведомление пользователю в социальной сети, чтобы привлечь его внимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ткнуть",
                                "target_lang_sense_clarification": "сленг; использовать функцию привлечения внимания"
                            },
                            {
                                "target_lang_word": "подмигнуть",
                                "target_lang_sense_clarification": "аналог функции 'poke' в некоторых соцсетях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55fbb71d-2891-35aa-9cc8-ce71867365e6",
                    "sense_definition": "To push or stick part of something out so that it is visible; to protrude.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dog's head <w>poked</w> out of the car window to catch the breeze.",
                            "target_lang_translations": {
                                "ru": "Голова собаки <w>высунулась</w> из окна машины, чтобы поймать ветерок."
                            }
                        },
                        {
                            "text": "Be careful, there is a sharp wire <w>poking</w> through the fabric of the chair.",
                            "target_lang_translations": {
                                "ru": "Осторожно, острая проволока <w>торчит</w> сквозь ткань стула."
                            }
                        }
                    ],
                    "synonyms": [
                        "stick out",
                        "protrude",
                        "jut"
                    ],
                    "common_phrases": [
                        "poke out",
                        "poke through",
                        "poke one's head in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступать наружу или быть видимым изнутри чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высовываться",
                                "target_lang_sense_clarification": "показываться наружу (о движении)"
                            },
                            {
                                "target_lang_word": "торчать",
                                "target_lang_sense_clarification": "выступать над поверхностью (о состоянии)"
                            },
                            {
                                "target_lang_word": "выглядывать",
                                "target_lang_sense_clarification": "смотреть или виднеться из укрытия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "955ef388-1e63-35cd-9a50-5e028d9366a1",
                    "sense_definition": "To search for something by moving things around, often in a casual or unorganized way.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "search_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I spent the afternoon <w>poking</w> around the attic looking for my grandfather's old photos.",
                            "target_lang_translations": {
                                "ru": "Я провел день, <w>копаясь</w> на чердаке в поисках старых фотографий дедушки."
                            }
                        },
                        {
                            "text": "The detective began to <w>poke</w> about the crime scene for any missed clues.",
                            "target_lang_translations": {
                                "ru": "Детектив начал <w>рыскать</w> по месту преступления в поисках упущенных улик."
                            }
                        }
                    ],
                    "synonyms": [
                        "rummage",
                        "forage",
                        "dig",
                        "root"
                    ],
                    "common_phrases": [
                        "poke around",
                        "poke about",
                        "poke one's nose into"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искать что-либо, перебирая вещи, часто небрежно или без системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыться",
                                "target_lang_sense_clarification": "искать, перекладывая вещи"
                            },
                            {
                                "target_lang_word": "копаться",
                                "target_lang_sense_clarification": "медленно или тщательно искать"
                            },
                            {
                                "target_lang_word": "шуровать",
                                "target_lang_sense_clarification": "разг.; энергично искать или рыскать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ab01f8f-adc5-3de6-a7c3-436f6fbb0a02",
                    "sense_definition": "To have sexual intercourse with someone.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sexual_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teenagers were making crude jokes about who <w>poked</w> whom at the party.",
                            "target_lang_translations": {
                                "ru": "Подростки отпускали грубые шутки о том, кто кого <w>трахнул</w> на вечеринке."
                            }
                        },
                        {
                            "text": "He got into trouble for bragging that he had <w>poked</w> her.",
                            "target_lang_translations": {
                                "ru": "У него были неприятности из-за хвастовства, что он её <w>чпокнул</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "screw",
                        "bang",
                        "shag"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заниматься сексом с кем-либо (грубый сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трахать",
                                "target_lang_sense_clarification": "грубое обозначение полового акта"
                            },
                            {
                                "target_lang_word": "чпокнуть",
                                "target_lang_sense_clarification": "сленг; совершить быстрый половой акт"
                            },
                            {
                                "target_lang_word": "шуровать",
                                "target_lang_sense_clarification": "устар. сленг; заниматься сексом"
                            }
                        ]
                    }
                }
            ]
        },
        {

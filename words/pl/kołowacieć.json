    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ee795977-a75b-3c19-89a5-6f055ab758c2",
                    "sense_definition": "Tracić orientację, zdolność jasnego myślenia lub rozeznanie w sytuacji; czuć oszołomienie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "confusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Od nadmiaru hałasu i sprzecznych informacji zaczyna mi <w>kołowacieć</w> w głowie.",
                            "target_lang_translations": {
                                "ru": "От избытка шума и противоречивой информации у меня голова начинает <w>идти кругом</w>."
                            }
                        },
                        {
                            "text": "Człowiek może zupełnie <w>kołowacieć</w>, gdy znajdzie się w tak stresującej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Человек может совершенно <w>одуреть</w>, оказавшись в такой стрессовой ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "głupieć",
                        "baranieć",
                        "dezorientować się"
                    ],
                    "antonyms": [
                        "trzeźwieć"
                    ],
                    "common_phrases": [
                        "głowa kołowacieje",
                        "myśli kołowacieją"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Терять ориентацию, способность ясно мыслить или понимание ситуации; чувствовать ошеломление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуреть",
                                "target_lang_sense_clarification": "становиться глупым или безумным от чего-либо"
                            },
                            {
                                "target_lang_word": "обалдевать",
                                "target_lang_sense_clarification": "приходить в состояние крайнего удивления или непонимания"
                            },
                            {
                                "target_lang_word": "терять голову",
                                "target_lang_sense_clarification": "переставать соображать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09550c5b-d7d4-30a6-97cf-44e11199f031",
                    "sense_definition": "Stawać się sztywnym, odrętwiałym lub bezwładnym, często o ciele lub kończynach.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "numbness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego palce zaczęły <w>kołowacieć</w> na mrozie, utrudniając chwytanie przedmiotów.",
                            "target_lang_translations": {
                                "ru": "Его пальцы начали <w>коченеть</w> на морозе, затрудняя хватание предметов."
                            }
                        },
                        {
                            "text": "Czuł, jak jego nogi zaczynają <w>kołowacieć</w> po wielu godzinach siedzenia w jednej pozycji.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал, как его ноги начинают <w>деревенеть</w> после многих часов сидения в одной позе."
                            }
                        }
                    ],
                    "synonyms": [
                        "drętwieć",
                        "sztywnieć",
                        "twardnieć"
                    ],
                    "antonyms": [
                        "wiotczeć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться жестким, онемевшим или неподвижным, часто о теле или конечностях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "деревенеть",
                                "target_lang_sense_clarification": "становиться твердым и неподвижным, как дерево"
                            },
                            {
                                "target_lang_word": "коченеть",
                                "target_lang_sense_clarification": "застывать от холода"
                            },
                            {
                                "target_lang_word": "неметь",
                                "target_lang_sense_clarification": "терять чувствительность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa7e7da6-7509-33fd-b06c-20ee9a79148d",
                    "sense_definition": "Poruszać się w kółko w sposób nieskoordynowany, często z powodu niepokoju lub choroby.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chore zwierzę zaczęło <w>kołowacieć</w> i oddalać się od reszty stada.",
                            "target_lang_translations": {
                                "ru": "Больное животное начало <w>кружиться</w> и отдаляться от остального стада."
                            }
                        },
                        {
                            "text": "Zaczął <w>kołowacieć</w> po pokoju, nie mogąc znaleźć sobie miejsca ze zdenerwowania.",
                            "target_lang_translations": {
                                "ru": "Он начал <w>ходить кругами</w> по комнате, не находя себе места от волнения."
                            }
                        }
                    ],
                    "synonyms": [
                        "kręcić się",
                        "miotać się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться по кругу нескоординированным образом, часто из-за беспокойства или болезни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кружиться",
                                "target_lang_sense_clarification": "вертеться на месте"
                            },
                            {
                                "target_lang_word": "ходить кругами",
                                "target_lang_sense_clarification": "беспокойно перемещаться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
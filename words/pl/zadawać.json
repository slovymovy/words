{
    "word_family": [
        "zadać",
        "zadanie",
        "zadawanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5a46b79e-82f0-3384-934f-323cae8da021",
                    "sense_definition": "Kierować do kogoś słowa w formie pytania, aby uzyskać informację lub odpowiedź.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziennikarze lubią <w>zadawać</w> politykom trudne i niewygodne pytania podczas wywiadów.",
                            "target_lang_translations": {
                                "ru": "Журналисты любят <w>задавать</w> политикам трудные и неудобные вопросы во время интервью."
                            }
                        },
                        {
                            "text": "Nie bój się <w>zadawać</w> pytań, jeśli czegoś nie rozumiesz na lekcji.",
                            "target_lang_translations": {
                                "ru": "Не бойся <w>задавать</w> вопросы, если чего-то не понимаешь на уроке."
                            }
                        }
                    ],
                    "synonyms": [
                        "udawać"
                    ],
                    "antonyms": [
                        "odpowiadać"
                    ],
                    "common_phrases": [
                        "zadawać pytania",
                        "zadawać sobie pytanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращаться к кому-либо с вопросом, чтобы получить информацию или ответ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задавать",
                                "target_lang_sense_clarification": "вопрос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8870be3b-426a-3e64-be7a-870976f6385c",
                    "sense_definition": "Polecać komuś wykonanie jakiejś pracy, ćwiczenia lub obowiązku, najczęściej w kontekście szkolnym.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza nauczycielka matematyki uwielbia <w>zadawać</w> nam mnóstwo trudnych zadań do domu.",
                            "target_lang_translations": {
                                "ru": "Наша учительница математики обожает <w>задавать</w> нам кучу трудных задач на дом."
                            }
                        },
                        {
                            "text": "Szef nie powinien <w>zadawać</w> pracownikom dodatkowych obowiązków tuż przed końcem dnia.",
                            "target_lang_translations": {
                                "ru": "Начальник не должен <w>поручать</w> сотрудникам дополнительные обязанности прямо перед концом рабочего дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "udawać"
                    ],
                    "common_phrases": [
                        "zadawać pracę domową",
                        "zadawać lekcje",
                        "zadawać ćwiczenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать задание выполнить какую-либо работу, упражнение или обязанность (обычно в учебном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задавать",
                                "target_lang_sense_clarification": "уроки, домашнее задание"
                            },
                            {
                                "target_lang_word": "поручать",
                                "target_lang_sense_clarification": "работу, обязанности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d329187-58f0-3c21-b1f1-85f6a6e9ae25",
                    "sense_definition": "Sprawiać, że ktoś odczuwa ból fizyczny, cierpienie psychiczne lub ponosi straty; wymierzać ciosy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten rodzaj broni może <w>zadawać</w> przeciwnikowi bardzo poważne i głębokie obrażenia.",
                            "target_lang_translations": {
                                "ru": "Этот вид оружия может <w>наносить</w> противнику очень серьезные и глубокие раны."
                            }
                        },
                        {
                            "text": "Nie chciałem ci <w>zadawać</w> bólu moimi słowami, to było niechcący.",
                            "target_lang_translations": {
                                "ru": "Я не хотел <w>причинять</w> тебе боль своими словами, это вышло случайно."
                            }
                        }
                    ],
                    "synonyms": [
                        "udawać"
                    ],
                    "common_phrases": [
                        "zadawać ból",
                        "zadawać cios",
                        "zadawać rany",
                        "zadawać śmierć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять кого-либо чувствовать физическую боль, страдание или наносить повреждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "причинять",
                                "target_lang_sense_clarification": "боль, страдания"
                            },
                            {
                                "target_lang_word": "наносить",
                                "target_lang_sense_clarification": "удар, рану, обиду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a90ebee1-1773-3a80-858a-14cf926ae404",
                    "sense_definition": "Utrzymywać z kimś kontakty towarzyskie lub spotykać się, często z odcieniem negatywnym (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice ostrzegali mnie, żebym przestał <w>zadawać</w> się z tymi chuliganami z osiedla.",
                            "target_lang_translations": {
                                "ru": "Родители предупреждали меня, чтобы я перестал <w>водиться</w> с этими хулиганами из района."
                            }
                        },
                        {
                            "text": "Ona woli <w>zadawać</w> się tylko z ludźmi ze swojego środowiska artystycznego.",
                            "target_lang_translations": {
                                "ru": "Она предпочитает <w>общаться</w> только с людьми из своей творческой среды."
                            }
                        }
                    ],
                    "synonyms": [
                        "udawać"
                    ],
                    "antonyms": [
                        "unikać"
                    ],
                    "common_phrases": [
                        "zadawać się z kimś",
                        "nie zadawaj się"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used to imply associating with the wrong crowd"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поддерживать знакомство или проводить время с кем-либо (часто с негативным оттенком, используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водиться",
                                "target_lang_sense_clarification": "общаться (разг.)"
                            },
                            {
                                "target_lang_word": "связываться",
                                "target_lang_sense_clarification": "иметь дело (разг.)"
                            },
                            {
                                "target_lang_word": "общаться",
                                "target_lang_sense_clarification": "поддерживать отношения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6047c56b-0841-3195-ac09-dca64a5a99f2",
                    "sense_definition": "Dostarczać zwierzętom gospodarskim pożywienie (paszę).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik musi codziennie rano wstawać, aby <w>zadawać</w> świeżą paszę bydłu w oborze.",
                            "target_lang_translations": {
                                "ru": "Фермер должен вставать каждое утро, чтобы <w>задавать</w> свежий корм скоту в хлеву."
                            }
                        },
                        {
                            "text": "Nowoczesne systemy pozwalają automatycznie <w>zadawać</w> karmę zwierzętom o określonych godzinach.",
                            "target_lang_translations": {
                                "ru": "Современные системы позволяют автоматически <w>раздавать</w> корм животным в определенные часы."
                            }
                        }
                    ],
                    "synonyms": [
                        "udawać"
                    ],
                    "common_phrases": [
                        "zadawać paszę",
                        "zadawać karmę"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical or rural context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать корм сельскохозяйственным животным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задавать",
                                "target_lang_sense_clarification": "корм (с.-х. термин)"
                            },
                            {
                                "target_lang_word": "раздавать",
                                "target_lang_sense_clarification": "корм"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
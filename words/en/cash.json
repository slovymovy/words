        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "80bd0778-4b5f-3845-abac-96eb94497ab7",
                    "sense_definition": "To exchange a check or financial instrument for money in the form of notes or bills.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "money_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He went to the bank to <w>cash</w> his weekly paycheck.",

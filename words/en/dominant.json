                    "target_lang_definitions": {
                        "ru": "Партнер, берущий на себя контролирующую роль в садомазохистских отношениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доминант",
                                "target_lang_sense_clarification": "доминирующий партнер"
                            },
                            {
                                "target_lang_word": "верхний",
                                "target_lang_sense_clarification": "сленг БДСМ-сообщества"
                            }
                        ]
                    }
                }
            ]
        }
    ]

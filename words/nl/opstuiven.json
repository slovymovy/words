{
    "word_family": [
        "stuiven",
        "verstuiven",
        "bestuiven",
        "stuifmeel",
        "stuifzand",
        "opstuiving"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ac9918cf-087f-364d-b3ad-866d47d1c538",
                    "sense_definition": "Het door wind of luchtverplaatsing omhoogvliegen van lichte deeltjes zoals zand, stof of sneeuw.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "weather_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de harde wind zag je het zand overal <w>opstuiven</w>.",
                            "target_lang_translations": {
                                "en": "Due to the strong wind, you could see the sand <w>swirling up</w> everywhere."
                            }
                        },
                        {
                            "text": "De helikopter deed bij het landen veel stof <w>opstuiven</w>.",
                            "target_lang_translations": {
                                "en": "The helicopter made a lot of dust <w>swirl up</w> while landing."
                            }
                        }
                    ],
                    "synonyms": [
                        "opwaaien",
                        "opdwarrelen"
                    ],
                    "antonyms": [
                        "neerdalen",
                        "gaan liggen"
                    ],
                    "common_phrases": [
                        "stof doen opstuiven",
                        "zand stuift op"
                    ],
                    "target_lang_definitions": {
                        "en": "To fly upwards or swirl around due to wind or air movement (of light particles like sand, dust, or snow)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swirl up",
                                "target_lang_sense_clarification": "movement of particles like dust or sand"
                            },
                            {
                                "target_lang_word": "blow up",
                                "target_lang_sense_clarification": "to be lifted by the wind"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f944a8be-3a69-3c1d-84b6-3f7f259997b9",
                    "sense_definition": "Met grote snelheid en vaak onstuimig ergens aankomen of binnengaan.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rapid_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toen de auto het schoolplein <w>opstoof</w>, schrokken de kinderen.",
                            "target_lang_translations": {
                                "en": "When the car <w>rushed onto</w> the schoolyard, the children were startled."
                            }
                        },
                        {
                            "text": "Hij kwam woedend de kamer <w>opgestoven</w> om verhaal te halen.",
                            "target_lang_translations": {
                                "en": "He came <w>storming</w> into the room furiously to demand an explanation."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanstormen",
                        "binnenvliegen"
                    ],
                    "common_phrases": [
                        "de trap opstuiven",
                        "de weg opstuiven"
                    ],
                    "target_lang_definitions": {
                        "en": "To arrive at or enter a place with great speed, often impetuously or angrily."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rush",
                                "target_lang_sense_clarification": "to move with great speed"
                            },
                            {
                                "target_lang_word": "storm",
                                "target_lang_sense_clarification": "to move angrily or violently"
                            },
                            {
                                "target_lang_word": "dash",
                                "target_lang_sense_clarification": "to move quickly"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
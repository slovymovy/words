{
    "word_family": [
        "particularisme",
        "particulariteit"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9ab2579e-6a7f-30ea-867a-6dbebcd19ba0",
                    "sense_definition": "Niet van de overheid, maar van een persoon of een bedrijf; behorend tot de privésfeer.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "private_vs_public",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het kasteel is <w>particulier</w> bezit en is daarom niet toegankelijk voor bezoekers.",
                            "target_lang_translations": {
                                "ru": "Замок является <w>частной</w> собственностью и поэтому недоступен для посетителей."
                            }
                        },
                        {
                            "text": "In de <w>particuliere</w> sector liggen de salarissen soms hoger dan bij de overheid.",
                            "target_lang_translations": {
                                "ru": "В <w>частном</w> секторе зарплаты иногда выше, чем в государственном."
                            }
                        }
                    ],
                    "synonyms": [
                        "privaat",
                        "privé",
                        "eigen"
                    ],
                    "antonyms": [
                        "openbaar",
                        "publiek",
                        "statelijk"
                    ],
                    "common_phrases": [
                        "particulier bezit",
                        "particuliere sector",
                        "particulier initiatief",
                        "particuliere school"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не государственный, а принадлежащий отдельному лицу или компании; относящийся к частной сфере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "частный",
                                "target_lang_sense_clarification": "противоположность государственному или общественному"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "040bf4f6-79b1-3151-875b-c16fe56ebe04",
                    "sense_definition": "Wat bij één specifiek persoon hoort en niet algemeen geldt; strikt persoonlijk of subjectief.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "subjective_personal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is slechts jouw <w>particuliere</w> mening en geen vaststaand feit.",
                            "target_lang_translations": {
                                "ru": "Это лишь твое <w>частное</w> мнение, а не установленный факт."
                            }
                        },
                        {
                            "text": "De schrijver hield er nogal <w>particuliere</w> opvattingen op na die door weinigen werden gedeeld.",
                            "target_lang_translations": {
                                "ru": "Писатель придерживался довольно <w>специфических</w> взглядов, которые мало кто разделял."
                            }
                        }
                    ],
                    "synonyms": [
                        "persoonlijk",
                        "individueel",
                        "subjectief"
                    ],
                    "antonyms": [
                        "algemeen",
                        "universeel"
                    ],
                    "common_phrases": [
                        "particuliere mening",
                        "particuliere opvatting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к конкретному человеку, не общепринятый; сугубо личный или субъективный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "личный",
                                "target_lang_sense_clarification": "индивидуальный, собственный"
                            },
                            {
                                "target_lang_word": "субъективный",
                                "target_lang_sense_clarification": "основанный на личном мнении"
                            },
                            {
                                "target_lang_word": "специфический",
                                "target_lang_sense_clarification": "свойственный только данному лицу, особенный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a694de5a-7fb6-3c71-921d-b0b19acbda37",
                    "sense_definition": "Iemand die als privépersoon handelt en niet namens een bedrijf, organisatie of de overheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "private_vs_public",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze groothandel verkoopt zijn producten niet aan <w>particulieren</w>, maar alleen aan bedrijven.",
                            "target_lang_translations": {
                                "ru": "Этот оптовый торговец продает свои товары не <w>частным лицам</w>, а только компаниям."
                            }
                        },
                        {
                            "text": "Als <w>particulier</w> kun je soms subsidie krijgen voor het verduurzamen van je woning.",
                            "target_lang_translations": {
                                "ru": "Как <w>частное лицо</w>, вы иногда можете получить субсидию на повышение экологичности вашего жилья."
                            }
                        }
                    ],
                    "synonyms": [
                        "privépersoon",
                        "burger"
                    ],
                    "antonyms": [
                        "bedrijf",
                        "onderneming",
                        "overheid"
                    ],
                    "common_phrases": [
                        "voor particulieren",
                        "verkoop aan particulieren",
                        "particulieren en bedrijven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, действующий как частное лицо, а не от имени компании, организации или государства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "частное лицо",
                                "target_lang_sense_clarification": "обычный гражданин, не организация"
                            },
                            {
                                "target_lang_word": "физическое лицо",
                                "target_lang_sense_clarification": "юридический и коммерческий термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
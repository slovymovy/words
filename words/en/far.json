    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9f297eb4-fd64-3b13-9262-91a5b1bbe42d",
                    "sense_definition": "To, from, or over a great distance in space or time.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "distance_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "How <w>far</w> is the train station from your house?",
                            "target_lang_translations": {
                                "ru": "Как <w>далеко</w> находится вокзал от твоего дома?"
                            }
                        },
                        {
                            "text": "We did not go <w>far</w> because it started to rain.",
                            "target_lang_translations": {
                                "ru": "Мы не ушли <w>далеко</w>, потому что начался дождь."
                            }
                        }
                    ],
                    "antonyms": [
                        "close",
                        "near"
                    ],
                    "common_phrases": [
                        "so far",
                        "how far",
                        "go far",
                        "as far as"
                    ],
                    "target_lang_definitions": {
                        "ru": "На большом расстоянии или на большое расстояние в пространстве или времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "далеко",
                                "target_lang_sense_clarification": "наречие места или времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b85fb512-40fa-35dd-abcd-b8d5c74a1997",
                    "sense_definition": "Very much; to a great degree (used with comparatives).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "degree_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This solution is <w>far</w> better than the previous one.",
                            "target_lang_translations": {
                                "ru": "Это решение <w>гораздо</w> лучше предыдущего."
                            }
                        },
                        {
                            "text": "It is <w>far</w> too cold to go swimming today.",
                            "target_lang_translations": {
                                "ru": "Сегодня <w>слишком</w> холодно, чтобы идти плавать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "far better",
                        "far more",
                        "far too",
                        "far less"
                    ],
                    "target_lang_definitions": {
                        "ru": "В значительной степени; намного (используется с прилагательными в сравнительной степени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гораздо",
                                "target_lang_sense_clarification": "усилитель сравнительной степени"
                            },
                            {
                                "target_lang_word": "намного",
                                "target_lang_sense_clarification": "значительно"
                            },
                            {
                                "target_lang_word": "далеко",
                                "target_lang_sense_clarification": "в переносном смысле (напр. далеко не)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "08122f3a-5bd9-3c9a-860d-38fb1047f686",
                    "sense_definition": "Remote in space; located at a great distance.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "distant_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He traveled to a <w>far</w> country to find work.",
                            "target_lang_translations": {
                                "ru": "Он отправился в <w>далёкую</w> страну, чтобы найти работу."
                            }
                        },
                        {
                            "text": "The sound of thunder came from a <w>far</w> distance.",
                            "target_lang_translations": {
                                "ru": "Звук грома доносился с <w>далёкого</w> расстояния."
                            }
                        }
                    ],
                    "synonyms": [
                        "distant"
                    ],
                    "antonyms": [
                        "close",
                        "near"
                    ],
                    "common_phrases": [
                        "far away",
                        "far off",
                        "far land"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдалённый в пространстве; находящийся на большом расстоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "далёкий",
                                "target_lang_sense_clarification": "находящийся далеко"
                            },
                            {
                                "target_lang_word": "дальний",
                                "target_lang_sense_clarification": "расположенный вдали"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b0f52d0-09c1-30ff-8d0d-fd12010c4f54",
                    "sense_definition": "The more distant of two things.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relative_pos_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please sit at the <w>far</w> end of the table.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, сядьте за <w>дальний</w> конец стола."
                            }
                        },
                        {
                            "text": "The <w>far</w> side of the moon is never visible from Earth.",
                            "target_lang_translations": {
                                "ru": "<w>Обратная</w> сторона Луны никогда не видна с Земли."
                            }
                        }
                    ],
                    "common_phrases": [
                        "far side",
                        "far end",
                        "far corner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Более отдалённый из двух предметов или сторон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дальний",
                                "target_lang_sense_clarification": "противоположный ближнему"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa2a5cbe-f9e6-3cae-a0de-33aec6a1d50c",
                    "sense_definition": "Remote in time; occurring a long time ago or in the distant future.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We can only imagine what technology will exist in the <w>far</w> future.",
                            "target_lang_translations": {
                                "ru": "Мы можем только представить, какие технологии будут существовать в <w>далёком</w> будущем."
                            }
                        },
                        {
                            "text": "That ancient civilization existed in the <w>far</w> past.",
                            "target_lang_translations": {
                                "ru": "Эта древняя цивилизация существовала в <w>далёком</w> прошлом."
                            }
                        }
                    ],
                    "synonyms": [
                        "distant"
                    ],
                    "common_phrases": [
                        "far future",
                        "far past"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдалённый во времени; произошедший давно или предстоящий в далёком будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "далёкий",
                                "target_lang_sense_clarification": "о времени (прошлом или будущем)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12170426-3a6c-33d2-8fdd-849a329e8052",
                    "sense_definition": "Extreme; belonging to the extreme part of a political or social spectrum.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "political_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>far</w> right party gained significant support in the election.",
                            "target_lang_translations": {
                                "ru": "Партия <w>крайне</w> правых получила значительную поддержку на выборах."
                            }
                        },
                        {
                            "text": "His views are considered <w>far</w> left by most moderates.",
                            "target_lang_translations": {
                                "ru": "Большинство умеренных считают его взгляды <w>крайне</w> левыми."
                            }
                        }
                    ],
                    "common_phrases": [
                        "far right",
                        "far left"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крайний; относящийся к радикальной части политического спектра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крайний",
                                "target_lang_sense_clarification": "радикальный (о взглядах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cc0f7e0-f9ac-3ca6-aaeb-9b2deb2faa06",
                    "sense_definition": "Extreme in nature or quality; very different.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "difference_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The final movie was a <w>far</w> cry from the original script.",
                            "target_lang_translations": {
                                "ru": "Итоговый фильм был <w>далёк</w> от оригинального сценария."
                            }
                        },
                        {
                            "text": "Living in the city is a <w>far</w> remove from his rural childhood.",
                            "target_lang_translations": {
                                "ru": "Жизнь в городе <w>далека</w> от его деревенского детства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "far cry",
                        "far remove"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершенно иной по природе или качеству; сильно отличающийся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "далёкий",
                                "target_lang_sense_clarification": "в значении 'далёкий от' (отличающийся)"
                            },
                            {
                                "target_lang_word": "большой",
                                "target_lang_sense_clarification": "в идиомах (напр. большая разница)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc1ee395-e22c-3995-a6f6-26d43eb76749",
                    "sense_definition": "Long; covering a large extent.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "dialect_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We have walked a <w>far</w> piece to get here today.",
                            "target_lang_translations": {
                                "ru": "Мы прошли <w>длинный</w> путь, чтобы добраться сюда сегодня."
                            }
                        },
                        {
                            "text": "It is a <w>far</w> way to the nearest town.",
                            "target_lang_translations": {
                                "ru": "До ближайшего города <w>далекий</w> путь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "far piece"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "used in some regional dialects to mean 'long' or 'considerable'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный; покрывающий большое расстояние (диалектное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "длинный",
                                "target_lang_sense_clarification": "протяжённый"
                            },
                            {
                                "target_lang_word": "долгий",
                                "target_lang_sense_clarification": "длительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2af289a9-4239-3dc2-b9c0-dee5b3c4cd7b",
                    "sense_definition": "Outside the currently selected segment in a segmented memory architecture.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tech_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The programmer used a <w>far</w> pointer to access the data.",
                            "target_lang_translations": {
                                "ru": "Программист использовал <w>дальний</w> указатель для доступа к данным."
                            }
                        },
                        {
                            "text": "Accessing <w>far</w> memory is slower than accessing near memory.",
                            "target_lang_translations": {
                                "ru": "Доступ к <w>дальней</w> памяти медленнее, чем к ближней."
                            }
                        }
                    ],
                    "common_phrases": [
                        "far pointer",
                        "far memory"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical jargon in computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся за пределами текущего сегмента памяти (компьютерный термин)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дальний",
                                "target_lang_sense_clarification": "тип указателя или памяти"
                            }
                        ]
                    }
                }
            ]
        },

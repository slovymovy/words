    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc40efb2-c476-3d4e-ab5b-048217934b2d",
                    "sense_definition": "Способность человека сознательно управлять своими поступками, преодолевать трудности и контролировать себя для достижения поставленных целей.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "willpower_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этого спортсмена железная <w>воля</w> к победе, поэтому он всегда выигрывает.",
                            "target_lang_translations": {
                                "en": "This athlete has an iron <w>will</w> to win, so he always wins."
                            }
                        },
                        {
                            "text": "Ему не хватило силы <w>воли</w>, чтобы бросить курить раз и навсегда.",
                            "target_lang_translations": {
                                "en": "He lacked the strength of <w>will</w> to quit smoking once and for all."
                            }
                        },
                        {
                            "text": "Самообладание и сильная <w>воля</w> помогают человеку справляться с жизненными трудностями.",
                            "target_lang_translations": {
                                "en": "Self-control and a strong <w>will</w> help a person cope with life's difficulties."
                            }
                        }
                    ],
                    "synonyms": [
                        "целеустремлённость",
                        "самоконтроль"
                    ],
                    "antonyms": [
                        "безволие"
                    ],
                    "common_phrases": [
                        "сила воли",
                        "железная воля",
                        "воспитание воли",
                        "воля к победе"
                    ],
                    "target_lang_definitions": {
                        "en": "The ability of a person to consciously control their actions, overcome difficulties, and control themselves to achieve set goals."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "willpower",
                                "target_lang_sense_clarification": "mental strength and self-control"
                            },
                            {
                                "target_lang_word": "will",
                                "target_lang_sense_clarification": "determination or firmity of purpose"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f27b2c3a-ce4f-3ba9-9c27-632b40af763a",
                    "sense_definition": "Состояние свободы, отсутствие ограничений и зависимости от кого-либо; возможность поступать по своему усмотрению.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "freedom_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Птицу вылечили и отпустили на <w>волю</w>, где она смогла снова летать.",
                            "target_lang_translations": {
                                "en": "The bird was cured and released to <w>freedom</w>, where it could fly again."
                            }
                        },
                        {
                            "text": "Художник дал <w>волю</w> своему воображению и нарисовал удивительную картину.",
                            "target_lang_translations": {
                                "en": "The artist gave free <w>rein</w> to his imagination and painted an amazing picture."
                            }
                        },
                        {
                            "text": "Дети почувствовали <w>волю</w> и бегали по полю до самого вечера.",
                            "target_lang_translations": {
                                "en": "The children felt the <w>freedom</w> and ran around the field until the very evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "свобода"
                    ],
                    "antonyms": [
                        "неволя",
                        "плен",
                        "несвобода"
                    ],
                    "common_phrases": [
                        "дать волю",
                        "на воле",
                        "отпустить на волю",
                        "вольная птица"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of freedom, absence of restrictions and dependence on anyone; the opportunity to act at one's own discretion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "freedom",
                                "target_lang_sense_clarification": "state of being free"
                            },
                            {
                                "target_lang_word": "liberty",
                                "target_lang_sense_clarification": "absence of constraints"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46081789-d849-36b9-a39b-6362a6c77388",
                    "sense_definition": "Осознанное желание, требование или намерение осуществить что-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "desire_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Такова была его последняя <w>воля</w>, и мы обязаны её исполнить.",
                            "target_lang_translations": {
                                "en": "Such was his last <w>will</w>, and we are obliged to fulfill it."
                            }
                        },
                        {
                            "text": "Он сделал это против своей <w>воли</w>, потому что его заставили обстоятельства.",
                            "target_lang_translations": {
                                "en": "He did it against his <w>will</w> because circumstances forced him."
                            }
                        },
                        {
                            "text": "Мы пришли сюда по доброй <w>воле</w>, никто нас не принуждал.",
                            "target_lang_translations": {
                                "en": "We came here of our own free <w>will</w>, no one forced us."
                            }
                        }
                    ],
                    "synonyms": [
                        "желание",
                        "намерение",
                        "требование",
                        "пожелание"
                    ],
                    "common_phrases": [
                        "по доброй воле",
                        "против воли",
                        "последняя воля",
                        "изъявление воли"
                    ],
                    "target_lang_definitions": {
                        "en": "A conscious desire, requirement, or intention to carry out something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "will",
                                "target_lang_sense_clarification": "intent, desire, or formal wish"
                            },
                            {
                                "target_lang_word": "wish",
                                "target_lang_sense_clarification": "desire"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60301207-b776-3e15-9e45-1aaae4bcdb46",
                    "sense_definition": "Жизнь вне тюремного заключения, за пределами мест лишения свободы.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "freedom_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После пяти лет заключения он наконец вышел на <w>волю</w>.",
                            "target_lang_translations": {
                                "en": "After five years of imprisonment, he finally came out to <w>freedom</w>."
                            }
                        },
                        {
                            "text": "Заключённый мечтал о <w>воле</w> каждый день, глядя на небо через решётку.",
                            "target_lang_translations": {
                                "en": "The prisoner dreamed of <w>freedom</w> every day, looking at the sky through the bars."
                            }
                        }
                    ],
                    "synonyms": [
                        "свобода"
                    ],
                    "antonyms": [
                        "тюрьма",
                        "заключение"
                    ],
                    "common_phrases": [
                        "выйти на волю",
                        "век воли не видать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто используется в контексте тюрьмы"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Life outside of prison confinement, beyond the limits of places of deprivation of liberty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "freedom",
                                "target_lang_sense_clarification": "life outside prison"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79174e84-a1e1-3296-b5a8-c10ab593ba40",
                    "sense_definition": "Власть, право или возможность распоряжаться судьбой кого-либо или чего-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "power_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это решение уже не в моей <w>воле</w>, вам нужно обратиться к директору.",
                            "target_lang_translations": {
                                "en": "This decision is no longer in my <w>power</w>, you need to contact the director."
                            }
                        },
                        {
                            "text": "Всё в этом мире происходит по божьей <w>воле</w>, как считала моя бабушка.",
                            "target_lang_translations": {
                                "en": "Everything in this world happens according to God's <w>will</w>, as my grandmother believed."
                            }
                        }
                    ],
                    "synonyms": [
                        "власть"
                    ],
                    "common_phrases": [
                        "не в моей воле",
                        "божья воля",
                        "волею судеб"
                    ],
                    "target_lang_definitions": {
                        "en": "Power, right, or opportunity to dispose of the fate of someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "power",
                                "target_lang_sense_clarification": "authority or control"
                            },
                            {
                                "target_lang_word": "will",
                                "target_lang_sense_clarification": "divine or authoritative decision"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf24c7ac-5452-33c7-914e-464b279ebaf7",
                    "sense_definition": "Широкое открытое пространство, простор.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "space_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кони быстро скакали по широкой <w>воле</w>, наслаждаясь свежим ветром.",
                            "target_lang_translations": {
                                "en": "The horses galloped fast across the wide <w>expanse</w>, enjoying the fresh wind."
                            }
                        },
                        {
                            "text": "Здесь, в степи, настоящая <w>воля</w> и нет никаких городских стен.",
                            "target_lang_translations": {
                                "en": "Here, in the steppe, is true <w>freedom</w> and there are no city walls."
                            }
                        }
                    ],
                    "common_phrases": [
                        "в чистом поле"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "поэтическое или устаревающее значение"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Wide open space, expanse."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expanse",
                                "target_lang_sense_clarification": "wide open space"
                            },
                            {
                                "target_lang_word": "open space",
                                "target_lang_sense_clarification": "literal meaning"
                            }
                        ]
                    }
                }
            ]
        },

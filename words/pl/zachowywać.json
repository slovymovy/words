                        ]
                    }
                },
                {
                    "sense_id": "ee7f7e1b-25d7-3bcd-9b77-44d796409f01",
                    "sense_definition": "Nie ujawniać innym powierzonej tajemnicy lub poufnej informacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "secrecy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz ma prawny obowiązek <w>zachowywać</w> tajemnicę lekarską.",
                            "target_lang_translations": {

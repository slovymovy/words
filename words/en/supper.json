    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a076aef7-da5e-30a9-8f06-1d7534616e95",
                    "sense_definition": "The main meal of the day, eaten in the evening.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "evening_meal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We normally have <w>supper</w> at seven o'clock in the evening.",
                            "target_lang_translations": {
                                "ru": "Обычно у нас <w>ужин</w> в семь часов вечера."
                            }
                        },
                        {
                            "text": "She invited her friends over for a delicious roast chicken <w>supper</w>.",
                            "target_lang_translations": {
                                "ru": "Она пригласила друзей на вкусный <w>ужин</w> с жареной курицей."
                            }
                        }
                    ],
                    "synonyms": [
                        "dinner"
                    ],
                    "common_phrases": [
                        "have supper",
                        "make supper",
                        "eat supper",
                        "ready for supper"
                    ],
                    "target_lang_definitions": {
                        "ru": "Главный прием пищи, совершаемый вечером."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ужин",
                                "target_lang_sense_clarification": "вечерняя еда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ffaf249-c107-34d1-b74c-b799d6811b5f",
                    "sense_definition": "A light meal or snack consumed late in the evening before going to bed.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "evening_meal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt hungry around midnight and made himself a light <w>supper</w> of cheese and crackers.",
                            "target_lang_translations": {
                                "ru": "Около полуночи он проголодался и приготовил себе легкий <w>ужин</w> из сыра и крекеров."
                            }
                        },
                        {
                            "text": "After the theater show, they went to a cafe for a late <w>supper</w>.",
                            "target_lang_translations": {
                                "ru": "После театра они пошли в кафе на поздний <w>ужин</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "light supper",
                        "late supper"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкая еда или перекус, съедаемый поздно вечером перед сном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ужин",
                                "target_lang_sense_clarification": "поздняя или легкая вечерняя еда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a87f272f-6aad-3810-8d08-386a2a3657c2",
                    "sense_definition": "A takeaway meal from a chip shop consisting of a deep-fried food accompanied by chips.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "takeaway_meal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I stopped at the shop on my way home to buy a fish <w>supper</w>.",
                            "target_lang_translations": {
                                "ru": "По дороге домой я зашел в магазин, чтобы купить рыбу с картошкой на <w>ужин</w>."
                            }
                        },
                        {
                            "text": "He was hungry enough to eat a whole sausage <w>supper</w> by himself.",
                            "target_lang_translations": {
                                "ru": "Он был настолько голоден, что мог съесть целую порцию сосисок на <w>ужин</w> в одиночку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fish supper",
                        "sausage supper"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Scotland and Northern Ireland"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Еда на вынос из закусочной, состоящая из жареного блюда с картофелем фри."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b542473f-d741-30f6-aa51-8800fa9222c2",
                    "sense_definition": "To eat the evening meal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_eat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>suppered</w> on cold meat and fresh bread after their long journey.",
                            "target_lang_translations": {
                                "ru": "После долгого путешествия они <w>поужинали</w> холодным мясом и свежим хлебом."
                            }
                        },
                        {
                            "text": "We <w>suppered</w> late that night because the guests arrived after dark.",
                            "target_lang_translations": {
                                "ru": "В тот вечер мы <w>ужинали</w> поздно, потому что гости приехали уже затемно."
                            }
                        }
                    ],
                    "synonyms": [
                        "dine#Verb"
                    ],
                    "common_phrases": [
                        "supper on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Есть вечернюю еду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ужинать",
                                "target_lang_sense_clarification": "совершать вечернюю трапезу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2207c326-49e4-3aad-98f7-c03e65322188",
                    "sense_definition": "To provide a person or animal with their evening meal.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_feed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer went out to the barn and <w>suppered</w> the horses for the night.",
                            "target_lang_translations": {
                                "ru": "Фермер пошел в сарай и <w>покормил ужином</w> лошадей на ночь."
                            }
                        },
                        {
                            "text": "After she had <w>suppered</w> the children, she finally sat down to rest.",
                            "target_lang_translations": {
                                "ru": "После того как она <w>покормила ужином</w> детей, она наконец села отдохнуть."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "often rural or dialectal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обеспечить человека или животное вечерней едой."
                    },

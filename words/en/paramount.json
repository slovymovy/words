    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6c32bf5a-046b-38f9-94d0-93b6713320ea",
                    "sense_definition": "Of the highest importance; more important than anything else.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The safety of the passengers is <w>paramount</w>.",
                            "target_lang_translations": {
                                "ru": "Безопасность пассажиров имеет <w>первостепенное</w> значение."
                            }
                        },
                        {
                            "text": "It is of <w>paramount</w> importance that we finish the project on time.",
                            "target_lang_translations": {
                                "ru": "Своевременное завершение проекта имеет <w>первостепенное</w> значение."
                            }
                        }
                    ],
                    "synonyms": [
                        "crucial",
                        "vital",
                        "essential",
                        "critical",
                        "key",
                        "supreme"
                    ],
                    "common_phrases": [
                        "of paramount importance",
                        "paramount concern",
                        "safety is paramount"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самый важный; имеющий первостепенное значение, важнее всего остального."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "первостепенный",
                                "target_lang_sense_clarification": "имеющий высшую важность"
                            },
                            {
                                "target_lang_word": "важнейший",
                                "target_lang_sense_clarification": "очень важный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2f0bd0c-c987-3e25-92c5-85e9fca6336b",
                    "sense_definition": "Highest in rank, authority, or power; supreme.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general established himself as the <w>paramount</w> authority in the region.",
                            "target_lang_translations": {
                                "ru": "Генерал утвердился как <w>верховная</w> власть в регионе."
                            }
                        },
                        {
                            "text": "As the <w>paramount</w> leader, she made all the final decisions for the organization.",
                            "target_lang_translations": {
                                "ru": "Как <w>верховный</w> лидер, она принимала все окончательные решения в организации."
                            }
                        }
                    ],
                    "synonyms": [
                        "supreme",
                        "chief",
                        "foremost",
                        "preeminent",
                        "leading"
                    ],
                    "common_phrases": [
                        "paramount chief",
                        "paramount leader",
                        "lord paramount"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высший по рангу, власти или полномочиям; верховный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верховный",
                                "target_lang_sense_clarification": "высший по власти"
                            },
                            {
                                "target_lang_word": "высший",
                                "target_lang_sense_clarification": "находящийся на самом верху иерархии"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9a298188-dd13-3a4f-853b-f8857f2b5433",
                    "sense_definition": "A supreme chief or leader, especially one with the highest status in a specific region.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tribe gathered to hear the decision of the <w>paramount</w>.",
                            "target_lang_translations": {
                                "ru": "Племя собралось, чтобы услышать решение <w>верховного вождя</w>."
                            }
                        },
                        {
                            "text": "He was recognized as a <w>paramount</w> among the local rulers.",
                            "target_lang_translations": {
                                "ru": "Он был признан <w>верховным правителем</w> среди местных правителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "chief",
                        "overlord"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly South Africa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Верховный вождь или лидер, особенно обладающий наивысшим статусом в определенном регионе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верховный вождь",
                                "target_lang_sense_clarification": "титул лидера племени или региона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f8cd7ae-1b92-38dd-9f77-d52ab115766b",
                    "sense_definition": "A supreme ruler or landowner in the feudal system who holds land from no one else.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lord <w>paramount</w> granted land to his vassals in exchange for loyalty.",
                            "target_lang_translations": {
                                "ru": "<w>Верховный</w> лорд даровал землю своим вассалам в обмен на верность."
                            }
                        },
                        {
                            "text": "In this territory, the king acted as the <w>paramount</w> over all estates.",
                            "target_lang_translations": {
                                "ru": "На этой территории король действовал как <w>сюзерен</w> над всеми поместьями."
                            }
                        }
                    ],
                    "synonyms": [
                        "overlord",
                        "suzerain"
                    ],
                    "antonyms": [
                        "paravail"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical, feudal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Верховный правитель или землевладелец в феодальной системе, не зависящий ни от кого другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сюзерен",
                                "target_lang_sense_clarification": "феодальный правитель"
                            },
                            {
                                "target_lang_word": "верховный лорд",
                                "target_lang_sense_clarification": "титул в феодальной иерархии"
                            }
                        ]
                    }
                }
            ]
        },

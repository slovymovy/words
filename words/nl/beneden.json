                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <w>beneden</w> hem om zijn grote succes.",
                            "target_lang_translations": {
                                "ru": "Они <w>завидовали</w> ему из-за его большого успеха."
                            }
                        },
                        {
                            "text": "Wij <w>beneden</w> de buren hun nieuwe auto helemaal niet.",
                            "target_lang_translations": {
                                "ru": "Мы совсем не <w>завидовали</w> соседям из-за их новой машины."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма прошедшего времени глагола 'benijden' (завидовать), множественное число."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завидовали",
                                "target_lang_sense_clarification": "прошедшее время, мн.ч."
                            }
                        ]
                    }
                }
            ]
        }
    ]

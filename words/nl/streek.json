                {
                    "sense_id": "87ccb187-d176-3cc5-844b-1535ab71379a",
                    "sense_definition": "Een gebied met een eigen karakter, een landstreek.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze mooie <w>streek</w> is vooral bekend om zijn uitgestrekte velden met bloemen.",
                            "target_lang_translations": {
                                "ru": "Этот красивый <w>край</w> известен прежде всего своими обширными цветочными полями."
                            }
                        },
                        {
                            "text": "Wij kopen het liefst verse groenten en fruit die direct uit onze eigen <w>streek</w> komen.",
                            "target_lang_translations": {
                                "ru": "Мы предпочитаем покупать свежие овощи и фрукты непосредственно из нашего <w>региона</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gebied",
                        "regio",
                        "landstreek",
                        "gewest"
                    ],
                    "common_phrases": [
                        "uit de streek",
                        "in de streek",
                        "een bekende streek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Район или местность, обладающая собственным характером."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "местность",
                                "target_lang_sense_clarification": "территория с определенными признаками"
                            },
                            {
                                "target_lang_word": "край",
                                "target_lang_sense_clarification": "обширная область или регион"
                            },
                            {
                                "target_lang_word": "регион",
                                "target_lang_sense_clarification": "административная или географическая единица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a65cb29c-67b8-3232-811b-5911f317a98e",
                    "sense_definition": "Een handeling die handig maar niet helemaal netjes, eerlijk of beleefd is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ondeugende kinderen haalden weer een <w>streek</w> uit bij de buren.",
                            "target_lang_translations": {
                                "ru": "Озорные дети снова устроили <w>проделку</w> соседям."
                            }
                        },
                        {
                            "text": "Dat was echt een gemene <w>streek</w> om je vrienden zo voor de gek te houden.",
                            "target_lang_translations": {
                                "ru": "Это была действительно подлая <w>выходка</w> — так разыграть своих друзей."
                            }
                        }
                    ],
                    "synonyms": [
                        "gewest",
                        "oord"
                    ],
                    "common_phrases": [
                        "een gemene streek",
                        "streken uithalen",
                        "een flauwe streek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хитрый, не совсем честный поступок или шалость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проделка",
                                "target_lang_sense_clarification": "озорной поступок или шутка"

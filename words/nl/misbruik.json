                },
                {
                    "sense_id": "59d11651-a654-3ae2-9f6c-d2b6a275bd03",
                    "sense_definition": "Een slechte daad, misstap of misdrijf.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "misdaad",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude teksten wordt diefstal soms beschreven als een groot <w>misbruik</w>.",
                            "target_lang_translations": {
                                "ru": "В старых текстах кража иногда описывается как великое <w>злодеяние</w>."
                            }

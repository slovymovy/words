            "senses": [
                {
                    "sense_id": "702916e6-e76b-3b85-88b2-14e52e8377fc",
                    "sense_definition": "Способность организма защищать свою целостность и биологическую индивидуальность от внешних воздействий и инфекций.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Витамины и закаливание помогают укрепить <w>иммунитет</w>.",
                            "target_lang_translations": {
                                "en": "Vitamins and cold training help strengthen <w>immunity</w>."
                            }
                        },
                        {
                            "text": "У ребёнка выработался стойкий <w>иммунитет</w> к ветрянке.",
                            "target_lang_translations": {
                                "en": "The child developed lasting <w>immunity</w> to chickenpox."
                            }
                        }
                    ],
                    "synonyms": [
                        "невосприимчивость",
                        "сопротивляемость",
                        "резистентность"
                    ],
                    "common_phrases": [
                        "сильный иммунитет",
                        "ослабленный иммунитет",
                        "укреплять иммунитет",
                        "врождённый иммунитет"
                    ],
                    "target_lang_definitions": {
                        "en": "The body's ability to protect its integrity and biological individuality from external influences and infections."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "immunity",
                                "target_lang_sense_clarification": "biological resistance to disease"
                            },
                            {
                                "target_lang_word": "resistance",
                                "target_lang_sense_clarification": "ability to resist infection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd92bfb7-7f53-3b42-8c3b-cd8083c2db3a",
                    "sense_definition": "Исключительное право не подчиняться некоторым общим законам, предоставленное лицам, занимающим особое положение в государстве.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Посол использовал свой дипломатический <w>иммунитет</w>, чтобы избежать штрафа.",
                            "target_lang_translations": {
                                "en": "The ambassador used his diplomatic <w>immunity</w> to avoid a fine."
                            }
                        },
                        {
                            "text": "Парламент обсуждает вопрос о снятии <w>иммунитета</w> с депутата.",
                            "target_lang_translations": {
                                "en": "The parliament is discussing lifting the deputy's <w>immunity</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "дипломатический иммунитет",
                        "депутатский иммунитет",
                        "лишить иммунитета"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclusive right to be exempt from certain general laws, granted to persons holding a special position in the state."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "immunity",
                                "target_lang_sense_clarification": "legal exemption from prosecution"
                            },
                            {
                                "target_lang_word": "exemption",
                                "target_lang_sense_clarification": "freedom from an obligation or liability"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "254e34fd-bfef-3009-9307-2a9e59b219f9",
                    "sense_definition": "Совокупность прав феодала осуществлять в своих владениях некоторые государственные функции без вмешательства центральной власти.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Феодальный <w>иммунитет</w> давал право сбора налогов на подвластной территории.",
                            "target_lang_translations": {
                                "en": "Feudal <w>immunity</w> granted the right to collect taxes in the controlled territory."
                            }
                        },
                        {
                            "text": "Крупные землевладельцы часто получали <w>иммунитет</w> от королевского суда.",
                            "target_lang_translations": {
                                "en": "Large landowners often received <w>immunity</w> from the royal court."
                            }
                        }
                    ],
                    "common_phrases": [
                        "феодальный иммунитет"
                    ],
                    "target_lang_definitions": {
                        "en": "A set of rights allowing a feudal lord to exercise state functions in their territory without interference from central authority."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "immunity",
                                "target_lang_sense_clarification": "historical/feudal privilege"
                            }
                        ]
                    }
                }
            ]
        }
    ]

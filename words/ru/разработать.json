                        {
                            "text": "Если замок заедает, попробуйте смазать его и аккуратно <w>разработать</w> ключом.",
                            "target_lang_translations": {
                                "en": "If the lock sticks, try lubricating it and carefully <w>working</w> it with the key."
                            }
                        }
                    ],
                    "synonyms": [
                        "размять",
                        "натренировать"
                    ],
                    "common_phrases": [
                        "разработать руку",
                        "разработать сустав",
                        "разработать ногу",
                        "разработать механизм"

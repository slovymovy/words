    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "eb2f955c-3d1c-33aa-baa8-aeb203db8954",
                    "sense_definition": "Być skutkiem, konsekwencją lub rezultatem czegoś; mieć w czymś swoje źródło lub przyczynę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cause_and_effect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele współczesnych problemów ekologicznych <w>wynika</w> z działalności człowieka w przeszłości.",
                            "target_lang_translations": {
                                "ru": "Многие современные экологические проблемы <w>вытекают</w> из деятельности человека в прошлом."
                            }
                        },
                        {
                            "text": "Jego obecne kłopoty finansowe <w>wynikają</w> bezpośrednio z braku oszczędności.",
                            "target_lang_translations": {
                                "ru": "Его нынешние финансовые трудности <w>вытекают</w> непосредственно из отсутствия сбережений."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydobywać się"
                    ],
                    "common_phrases": [
                        "to wynika z faktu",
                        "problemy wynikają z",
                        "wynikać z założenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть следствием, последствием или результатом чего-либо; иметь в чем-то свой источник или причину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытекать",
                                "target_lang_sense_clarification": "быть следствием чего-либо"
                            },
                            {
                                "target_lang_word": "являться результатом",
                                "target_lang_sense_clarification": "быть итогом действий или событий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "728b0f9d-ce9f-39a9-b941-6e249a47d6be",
                    "sense_definition": "Stawać się jasnym, wiadomym lub możliwym do wywnioskowania na podstawie analizy faktów, dokumentów lub przesłanek.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "inference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z przedstawionego przez policję raportu jasno <w>wynika</w>, że liczba wypadków spadła.",
                            "target_lang_translations": {
                                "ru": "Из представленного полицией отчета ясно <w>следует</w>, что количество несчастных случаев снизилось."
                            }
                        },
                        {
                            "text": "Co dokładnie <w>wynika</w> dla nas z tej nowej umowy handlowej?",
                            "target_lang_translations": {
                                "ru": "Что именно <w>следует</w> для нас из этого нового торгового соглашения?"
                            }
                        }
                    ],
                    "synonyms": [
                        "okazywać się"
                    ],
                    "common_phrases": [
                        "z tego wynika",
                        "jak wynika z",
                        "z badań wynika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться ясным, известным или возможным для вывода на основе анализа фактов, документов или предпосылок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следовать",
                                "target_lang_sense_clarification": "быть логическим выводом"
                            },
                            {
                                "target_lang_word": "вытекать",
                                "target_lang_sense_clarification": "становиться очевидным из фактов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54aa74d5-b31e-3168-908f-74187424859e",
                    "sense_definition": "Wydostawać się na zewnątrz; wyłaniać się z jakiegoś wnętrza lub otoczenia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_emergence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z gęstej, porannej mgły powoli <w>wynikał</w> zarys starego zamczyska.",
                            "target_lang_translations": {
                                "ru": "Из густого утреннего тумана медленно <w>выплывало</w> очертание старого замка."
                            }
                        },
                        {
                            "text": "Nagle z cienia <w>wynikała</w> tajemnicza postać ubrana w długi płaszcz.",
                            "target_lang_translations": {
                                "ru": "Внезапно из тени <w>появилась</w> таинственная фигура, одетая в длинный плащ."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydobywać się"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie rzadko używane w znaczeniu fizycznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выходить наружу; появляться изнутри или из окружения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выплывать",
                                "target_lang_sense_clarification": "медленно появляться (из тумана, темноты)"
                            },
                            {
                                "target_lang_word": "появляться",
                                "target_lang_sense_clarification": "становиться видимым"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
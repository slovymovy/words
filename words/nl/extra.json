    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4af517ee-a950-3a0e-ae73-6cc5795473ca",
                    "sense_definition": "Toegevoegd aan wat normaal, verwacht of noodzakelijk is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "additional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "U krijgt er nu een <w>extra</w> lampje bij als u dit product koopt.",
                            "target_lang_translations": {
                                "en": "You get an <w>extra</w> light with it now if you buy this product."
                            }
                        },
                        {
                            "text": "Dat zou me veel <w>extra</w> energie kosten als ik dat ook nog moet doen.",
                            "target_lang_translations": {
                                "en": "That would cost me a lot of <w>extra</w> energy if I have to do that as well."
                            }
                        }
                    ],
                    "synonyms": [
                        "bijkomend",
                        "aanvullend",
                        "additioneel"
                    ],
                    "antonyms": [
                        "standaard",
                        "inbegrepen"
                    ],
                    "common_phrases": [
                        "extra kosten",
                        "extra tijd",
                        "extra informatie",
                        "extra geld"
                    ],
                    "target_lang_definitions": {
                        "en": "Added to what is normal, expected, or necessary."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "extra",
                                "target_lang_sense_clarification": "more than usual"
                            },
                            {
                                "target_lang_word": "additional",
                                "target_lang_sense_clarification": "added or supplementary"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "219bda7a-6c2c-3688-8b8c-2f4ba4024c78",
                    "sense_definition": "Met opzet gedaan; niet per ongeluk.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "deliberate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij deed het <w>extra</w> om mij te plagen.",
                            "target_lang_translations": {
                                "en": "He did it <w>on purpose</w> to tease me."
                            }
                        },
                        {
                            "text": "Ik heb dat echt niet <w>extra</w> gedaan, het was een ongelukje.",
                            "target_lang_translations": {
                                "en": "I really didn't do that <w>deliberately</w>, it was an accident."
                            }
                        }
                    ],
                    "synonyms": [
                        "expres",
                        "opzettelijk"
                    ],
                    "antonyms": [
                        "per ongeluk"
                    ],
                    "common_phrases": [
                        "niet extra doen",
                        "iets extra doen"
                    ],
                    "target_lang_definitions": {
                        "en": "Done on purpose; not accidentally."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on purpose",
                                "target_lang_sense_clarification": "deliberately"
                            },
                            {
                                "target_lang_word": "deliberately",
                                "target_lang_sense_clarification": "intentionally"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9d2a94c8-be0f-3591-9d12-a3b1f32155ba",

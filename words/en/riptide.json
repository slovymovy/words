    "word_family": [],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9f466778-348e-33b4-a0c6-9014f2a031b1",
                    "sense_definition": "A strong current of water flowing away from the shore that can pull swimmers out to sea.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This beach suffers from a <w>riptide</w> that can easily sweep you out to sea.",
                            "target_lang_translations": {
                                "ru": "На этом пляже часто возникает <w>отбойное течение</w>, которое может легко унести вас в открытое море."
                            }
                        },
                        {
                            "text": "If you get caught in a <w>riptide</w>, you should swim parallel to the shore to escape.",
                            "target_lang_translations": {
                                "ru": "Если вы попали в <w>тягун</w>, нужно плыть параллельно берегу, чтобы выбраться."
                            }
                        }
                    ],
                    "synonyms": [
                        "rip current"
                    ],
                    "common_phrases": [
                        "caught in a riptide",
                        "dangerous riptide"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное течение, направленное от берега в море, которое может унести пловцов на глубину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отбойное течение",
                                "target_lang_sense_clarification": "течение от берега"
                            },
                            {
                                "target_lang_word": "тягун",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a218e88d-57fa-303d-9031-00c1bb375f77",
                    "sense_definition": "A particularly strong current caused by the tide.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The small boat struggled to move forward against the powerful <w>riptide</w>.",
                            "target_lang_translations": {
                                "ru": "Маленькая лодка с трудом продвигалась вперед против мощной <w>быстрины</w>."
                            }
                        },
                        {
                            "text": "Sailors must be careful of the <w>riptide</w> when entering the narrow channel.",
                            "target_lang_translations": {
                                "ru": "Моряки должны остерегаться <w>сулоя</w> при входе в узкий пролив."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Особенно сильное или бурное течение, вызванное приливом и отливом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сулой",
                                "target_lang_sense_clarification": "морской термин"
                            },
                            {
                                "target_lang_word": "быстрина",
                                "target_lang_sense_clarification": "быстрое течение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1beb66f4-be41-34ff-a73e-5b58658a41ae",
                    "sense_definition": "An object firmly fixed in place, especially a permanent appliance or item in a house (such as a light or sink) that is sold with the property.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The price of the apartment includes all the permanent <w>fixtures</w> and fittings.",
                            "target_lang_translations": {
                                "ru": "В стоимость квартиры входит всё стационарное <w>оборудование</w> и отделка."
                            }
                        },
                        {
                            "text": "We need to hire an electrician to install a new light <w>fixture</w> in the hallway.",
                            "target_lang_translations": {
                                "ru": "Нам нужно нанять электрика, чтобы установить новый <w>светильник</w> в коридоре."
                            }
                        }
                    ],
                    "synonyms": [
                        "fitting",
                        "appliance",
                        "installation",
                        "unit"
                    ],
                    "antonyms": [
                        "furnishing",
                        "movable"
                    ],
                    "common_phrases": [
                        "light fixture",
                        "plumbing fixture",
                        "fixtures and fittings",
                        "bathroom fixture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, прочно закрепленный на месте, особенно постоянное устройство в доме (например, светильник или раковина), которое продается вместе с недвижимостью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "светильник",
                                "target_lang_sense_clarification": "осветительный прибор"
                            },
                            {
                                "target_lang_word": "арматура",
                                "target_lang_sense_clarification": "сантехническая или электрическая"
                            },
                            {
                                "target_lang_word": "оборудование",
                                "target_lang_sense_clarification": "стационарное, встроенное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02066a65-b10b-3068-bf2e-0fa5b1726771",
                    "sense_definition": "A sports match or event that has been scheduled to take place on a particular date.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team has a difficult <w>fixture</w> against the league champions next Saturday.",
                            "target_lang_translations": {
                                "ru": "В следующую субботу у команды сложный <w>матч</w> против чемпионов лиги."
                            }
                        },
                        {
                            "text": "Fans are eagerly waiting for the new season's <w>fixture</w> list to be published.",
                            "target_lang_translations": {
                                "ru": "Болельщики с нетерпением ждут, когда будет опубликован список <w>матчей</w> нового сезона."
                            }
                        }
                    ],
                    "synonyms": [
                        "match",
                        "game",
                        "event"
                    ],
                    "common_phrases": [
                        "fixture list",
                        "cup fixture",
                        "sports fixture"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивный матч или мероприятие, запланированное на определенную дату."
                    },
                    "translations": {
                        "ru": [
                            {

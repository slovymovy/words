                            },
                            {
                                "target_lang_word": "disclose",
                                "target_lang_sense_clarification": "to make secret information known"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обнародовать",
                                "target_lang_sense_clarification": "официально публиковать или объявлять"

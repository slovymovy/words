                    ],
                    "common_phrases": [
                        "расплатиться за ошибки",
                        "расплатиться здоровьем",
                        "расплатиться жизнью",
                        "дорого расплатиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To suffer punishment or lose something valuable as a consequence of one's actions or mistakes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay for",

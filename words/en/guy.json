            "senses": [
                {
                    "sense_id": "dacdf568-e29a-3668-b430-c2a99adcad25",
                    "sense_definition": "A man or a boy; a fellow.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "person_ref",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a really nice <w>guy</w> who always helps his neighbors.",
                            "target_lang_translations": {
                                "ru": "Он действительно хороший <w>парень</w>, который всегда помогает своим соседям."
                            }
                        },
                        {
                            "text": "I met a <w>guy</w> at the library who recommended this book.",
                            "target_lang_translations": {
                                "ru": "Я встретил в библиотеке одного <w>парня</w>, который порекомендовал эту книгу."
                            }
                        }
                    ],
                    "synonyms": [
                        "man",
                        "fellow",
                        "dude",
                        "chap",
                        "bloke",
                        "lad",
                        "fella"
                    ],
                    "antonyms": [
                        "girl",
                        "woman"
                    ],
                    "common_phrases": [
                        "nice guy",
                        "tough guy",
                        "bad guy",
                        "regular guy"
                    ],

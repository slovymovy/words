    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "197e37d5-b0ef-382f-a7eb-7ffe5584e158",
                    "sense_definition": "Very close to a particular state, time, quality, or action, but not quite reaching it.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>almost</w> missed the train because of the heavy traffic.",
                            "target_lang_translations": {
                                "ru": "Мы <w>почти</w> опоздали на поезд из-за сильного движения."
                            }
                        },
                        {
                            "text": "It is <w>almost</w> time for us to go home.",
                            "target_lang_translations": {
                                "ru": "Нам <w>почти</w> пора идти домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "nearly",
                        "practically",
                        "virtually",
                        "just about",
                        "not quite",
                        "pretty much",
                        "all but",
                        "close to"
                    ],
                    "common_phrases": [
                        "almost always",
                        "almost never",
                        "almost certainly",
                        "almost ready",
                        "almost done"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень близко к определенному состоянию, времени, качеству или действию, но не совсем достигая его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почти",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "почти что",
                                "target_lang_sense_clarification": "разговорный вариант"
                            },
                            {
                                "target_lang_word": "практически",
                                "target_lang_sense_clarification": "в значении «фактически»"
                            },
                            {
                                "target_lang_word": "чуть не",
                                "target_lang_sense_clarification": "о событиях, которые едва не случились"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55cffd57-7700-37fe-ad6b-4fab29491638",
                    "sense_definition": "Used in mathematics to indicate that a property holds for all elements in a set except for a negligible subset of measure zero.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The function is continuous <w>almost</w> everywhere on the interval.",
                            "target_lang_translations": {
                                "ru": "Функция непрерывна <w>почти</w> всюду на интервале."
                            }
                        },
                        {
                            "text": "In this probability model, the sequence converges <w>almost</w> surely.",
                            "target_lang_translations": {
                                "ru": "В этой вероятностной модели последовательность сходится <w>почти</w> наверное."
                            }
                        }
                    ],
                    "synonyms": [
                        "virtually"
                    ],
                    "common_phrases": [
                        "almost everywhere",
                        "almost surely"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в математике для указания того, что свойство выполняется для всех элементов множества, за исключением пренебрежимо малого подмножества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почти",
                                "target_lang_sense_clarification": "математический термин (напр. почти всюду)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d90a62f8-953c-357e-8d92-1d180887c075",
                    "sense_definition": "An attempt, event, or person that came very close to succeeding or achieving a goal but ultimately did not.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team had a few great wins and several frustrating <w>almosts</w>.",
                            "target_lang_translations": {
                                "ru": "У команды было несколько великих побед и несколько обидных <w>«почти»</w>."
                            }
                        },
                        {
                            "text": "Her career was a series of <w>almosts</w> rather than clear victories.",
                            "target_lang_translations": {
                                "ru": "Её карьера была чередой <w>«почти»</w>, а не чистых побед."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Попытка, событие или человек, которые были очень близки к успеху, но в итоге не достигли его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "«почти»",
                                "target_lang_sense_clarification": "субстантивированное наречие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

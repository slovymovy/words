    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "49d266ee-c0e5-35c8-b031-a79746b9ecb8",
                    "sense_definition": "Ogół wszystkich bóstw w danej religii politeistycznej.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_gods",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zeus zajmował najważniejsze miejsce, któremu podlegał cały grecki <w>panteon</w>.",
                            "target_lang_translations": {
                                "ru": "Зевс занимал важнейшее место, которому подчинялся весь греческий <w>пантеон</w>."
                            }
                        },
                        {
                            "text": "Badacze wciąż analizują strukturę i hierarchię słowiańskiego <w>panteonu</w>.",
                            "target_lang_translations": {
                                "ru": "Исследователи всё ещё анализируют структуру и иерархию славянского <w>пантеона</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bóstwa"
                    ],
                    "common_phrases": [
                        "panteon bogów",
                        "panteon słowiański",
                        "panteon hinduski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность всех богов какой-либо политеистической религии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пантеон",
                                "target_lang_sense_clarification": "совокупность богов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a064d1c7-940c-3145-902d-ca2631ea20df",
                    "sense_definition": "Grupa najwybitniejszych, najbardziej zasłużonych i sławnych osób w jakiejś dziedzinie lub narodzie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten wybitny reżyser na stałe wszedł do <w>panteonu</w> światowego kina.",
                            "target_lang_translations": {
                                "ru": "Этот выдающийся режиссёр навсегда вошёл в <w>пантеон</w> мирового кино."
                            }
                        },
                        {
                            "text": "W <w>panteonie</w> polskich wieszczów narodowych Adam Mickiewicz zajmuje szczególne miejsce.",
                            "target_lang_translations": {
                                "ru": "В <w>пантеоне</w> польских национальных пророков Адам Мицкевич занимает особое место."
                            }
                        }
                    ],
                    "synonyms": [
                        "elita",
                        "czołówka",
                        "śmietanka"
                    ],
                    "common_phrases": [
                        "panteon sław",
                        "panteon gwiazd",
                        "narodowy panteon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа самых выдающихся, заслуженных и известных людей в какой-либо области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пантеон",
                                "target_lang_sense_clarification": "группа выдающихся личностей"
                            },
                            {
                                "target_lang_word": "плеяда",
                                "target_lang_sense_clarification": "группа талантливых людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8d63f77-7620-34a8-a9a3-d636af19328b",
                    "sense_definition": "W starożytności: świątynia poświęcona wszystkim bogom.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starożytny <w>panteon</w> był miejscem, gdzie oddawano cześć wielu różnym bóstwom.",
                            "target_lang_translations": {
                                "ru": "Древний <w>пантеон</w> был местом, где поклонялись многим разным божествам."
                            }
                        },
                        {
                            "text": "Architekci podziwiali konstrukcję, jaką wyróżniał się ten rzymski <w>panteon</w>.",
                            "target_lang_translations": {
                                "ru": "Архитекторы восхищались конструкцией, которой отличался этот римский <w>пантеон</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "świątynia"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dotyczy starożytności"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В древности: храм, посвященный всем богам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пантеон",
                                "target_lang_sense_clarification": "античный храм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6bcfd117-fa1e-34ff-9bb5-3c0efb1a1808",
                    "sense_definition": "Reprezentacyjny budynek pełniący funkcję grobowca dla wybitnych i zasłużonych osób.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trumnę z ciałem poety uroczyście przeniesiono do narodowego <w>panteonu</w>.",
                            "target_lang_translations": {
                                "ru": "Гроб с телом поэта торжественно перенесли в национальный <w>пантеон</w>."
                            }
                        },
                        {
                            "text": "Wiele stolic posiada własny <w>panteon</w>, w którym spoczywają bohaterowie narodowi.",
                            "target_lang_translations": {
                                "ru": "Многие столицы имеют собственный <w>пантеон</w>, в котором покоятся национальные герои."
                            }
                        }
                    ],
                    "synonyms": [
                        "mauzoleum",
                        "grobowiec"
                    ],
                    "common_phrases": [
                        "panteon narodowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Монументальное здание, служащее усыпальницей для выдающихся людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пантеон",
                                "target_lang_sense_clarification": "усыпальница"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "11667872-dd32-3c91-921b-cca26be7ca3a",
                    "sense_definition": "Starożytna świątynia w Rzymie w kształcie rotundy, poświęcona wszystkim bogom, zachowana do czasów współczesnych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Podczas wycieczki do Włoch musisz koniecznie zobaczyć rzymski <w>Panteon</w>.",
                            "target_lang_translations": {
                                "ru": "Во время поездки в Италию ты должен обязательно увидеть римский <w>Пантеон</w>."
                            }
                        },
                        {
                            "text": "Słynny <w>Panteon</w> posiada jedną z największych kopuł na świecie.",
                            "target_lang_translations": {
                                "ru": "Знаменитый <w>Пантеон</w> обладает одним из самых больших куполов в мире."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Panteon w Rzymie",
                        "Panteon w Paryżu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Древний храм в Риме в форме ротонды, посвященный всем богам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пантеон",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
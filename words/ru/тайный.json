            "senses": [
                {
                    "sense_id": "af838a00-bd6e-30a0-9f93-2fb333cf230a",
                    "sense_definition": "Скрываемый от других, не подлежащий разглашению; известный только определённому кругу лиц.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "secret_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У них был <тайный> план, о котором никто не знал.",
                            "target_lang_translations": {
                                "en": "They had a <secret> plan that no one knew about."
                            }
                        },
                        {
                            "text": "В старом замке мы нашли <тайный> ход за книжным шкафом.",
                            "target_lang_translations": {
                                "en": "In the old castle, we found a <secret> passage behind the bookcase."
                            }
                        }
                    ],
                    "synonyms": [
                        "секретный",
                        "скрытый"
                    ],
                    "antonyms": [
                        "явный"
                    ],
                    "common_phrases": [
                        "тайный смысл",
                        "тайная встреча",
                        "тайное голосование",
                        "тайный ход"
                    ],
                    "target_lang_definitions": {
                        "en": "Kept hidden from others; known only to a certain circle of people."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "secret",
                                "target_lang_sense_clarification": "kept hidden from knowledge or view"
                            },
                            {
                                "target_lang_word": "hidden",
                                "target_lang_sense_clarification": "concealed from view or discovery"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42e28f7b-edf4-3680-b711-79e408b07313",
                    "sense_definition": "Не проявляемый открыто, скрытый в глубине души (о мыслях, чувствах, желаниях).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "secret_inner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <тайная> мечта стать художником так и не сбылась.",
                            "target_lang_translations": {
                                "en": "His <secret> dream of becoming an artist never came true."
                            }
                        },
                        {
                            "text": "Она хранила <тайную> надежду, что он когда-нибудь вернётся.",
                            "target_lang_translations": {
                                "en": "She harbored a <secret> hope that he would return someday."
                            }
                        }
                    ],
                    "synonyms": [
                        "сокровенный",
                        "скрытый"
                    ],
                    "antonyms": [
                        "явный"
                    ],
                    "common_phrases": [
                        "тайная мечта",
                        "тайное желание",
                        "тайная надежда",
                        "тайная любовь"
                    ],
                    "target_lang_definitions": {
                        "en": "Not expressed openly; kept deep within the mind or heart."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "secret",
                                "target_lang_sense_clarification": "regarding private thoughts or feelings"
                            },
                            {
                                "target_lang_word": "hidden",
                                "target_lang_sense_clarification": "not outwardly expressed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf97bfc4-da57-3732-8b1b-d424a795f875",
                    "sense_definition": "Связанный с деятельностью секретной организации или службы; конспиративный.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "secret_org",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В городе действовало <тайное> общество, готовившее переворот.",
                            "target_lang_translations": {
                                "en": "A <secret> society preparing a coup was operating in the city."
                            }
                        },
                        {
                            "text": "<Тайная> полиция следила за всеми подозрительными иностранцами.",
                            "target_lang_translations": {
                                "en": "The <secret> police watched all suspicious foreigners."
                            }
                        }
                    ],
                    "synonyms": [
                        "секретный"
                    ],
                    "common_phrases": [
                        "тайная полиция",
                        "тайное общество",
                        "тайный советник"

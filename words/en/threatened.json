{
    "word_family": [
        "threat",
        "threaten",
        "threatening",
        "threateningly"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3fc88dba-6993-3634-8262-6ea8780e1bde",
                    "sense_definition": "Simple past and past participle of <w>threaten</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The angry customer <w>threatened</w> to call the manager immediately.",
                            "target_lang_translations": {
                                "ru": "Сердитый клиент <w>пригрозил</w> немедленно позвонить менеджеру."
                            }
                        },
                        {
                            "text": "Dark clouds <w>threatened</w> rain for most of the afternoon.",
                            "target_lang_translations": {
                                "ru": "Тёмные тучи <w>грозили</w> дождём большую часть дня."
                            }
                        }
                    ],
                    "common_phrases": [
                        "threatened to",
                        "threatened with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола threaten (угрожать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угрожал",
                                "target_lang_sense_clarification": "выражал намерение нанести вред"
                            },
                            {
                                "target_lang_word": "грозил",
                                "target_lang_sense_clarification": "предвещал что-то плохое или опасное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "301f6424-8438-33e9-a629-bfacfde47334",
                    "sense_definition": "At risk of becoming endangered in the near future.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "risk_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We must act now to save the <w>threatened</w> species in this forest.",
                            "target_lang_translations": {
                                "ru": "Мы должны действовать сейчас, чтобы спасти <w>находящиеся под угрозой</w> виды в этом лесу."
                            }
                        },
                        {
                            "text": "Pollution has left the local river ecosystem severely <w>threatened</w>.",
                            "target_lang_translations": {
                                "ru": "Загрязнение поставило экосистему местной реки под серьёзную <w>угрозу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "endangered",
                        "at risk",
                        "vulnerable"
                    ],
                    "antonyms": [
                        "safe",
                        "secure",
                        "protected"
                    ],
                    "common_phrases": [
                        "threatened species",
                        "threatened habitat",
                        "threatened with extinction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся под риском исчезновения или уничтожения в ближайшем будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "находящийся под угрозой",
                                "target_lang_sense_clarification": "о видах животных или растений"
                            },
                            {
                                "target_lang_word": "под угрозой исчезновения",
                                "target_lang_sense_clarification": "экологический статус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e3a184d-1a06-3947-adee-5cc1f415870d",
                    "sense_definition": "Feeling insecure or vulnerable.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt <w>threatened</w> by the aggressive behavior of the stranger.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал <w>угрозу</w> из-за агрессивного поведения незнакомца."
                            }
                        },
                        {
                            "text": "You should not feel <w>threatened</w> by constructive criticism from your boss.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует чувствовать <w>угрозу</w> от конструктивной критики твоего начальника."
                            }
                        }
                    ],
                    "synonyms": [
                        "insecure",
                        "vulnerable",
                        "intimidated"
                    ],
                    "antonyms": [
                        "confident",
                        "secure",
                        "safe"
                    ],
                    "common_phrases": [
                        "feel threatened",
                        "threatened by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий опасность, незащищенность или страх перед кем-то или чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "под угрозой",
                                "target_lang_sense_clarification": "ощущающий опасность"
                            },
                            {
                                "target_lang_word": "в опасности",
                                "target_lang_sense_clarification": "чувствующий риск"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
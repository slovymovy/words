    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1f737588-38df-3971-8ac1-16a895e52b64",
                    "sense_definition": "A short letter or a brief record written down to assist the memory.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "writing_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I left a <w>note</w> on the fridge to remind him about dinner.",
                            "target_lang_translations": {
                                "ru": "Я оставил <w>записку</w> на холодильнике, чтобы напомнить ему об ужине."
                            }
                        },
                        {
                            "text": "She wrote a quick <w>note</w> to thank them for the gift.",
                            "target_lang_translations": {
                                "ru": "Она написала короткую <w>записку</w>, чтобы поблагодарить их за подарок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leave a note",
                        "write a note",
                        "thank-you note",
                        "sticky note"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткое письмо или запись, сделанная для памяти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "записка",
                                "target_lang_sense_clarification": "короткое сообщение"
                            },
                            {
                                "target_lang_word": "заметка",
                                "target_lang_sense_clarification": "краткая запись"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c71c6298-a9d0-30a3-8ea1-b4e56f4cd624",

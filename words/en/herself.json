{
    "word_family": [],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "cb48f85b-ddb6-3525-84e3-479ca66a925d",
                    "sense_definition": "The reflexive form of <w>she</w>, used when the female subject of a sentence acts upon that same person.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought <w>herself</w> a new dress for the party.",
                            "target_lang_translations": {
                                "ru": "Она купила <w>себе</w> новое платье для вечеринки."
                            }
                        },
                        {
                            "text": "After the long trip, she finally had some time to <w>herself</w>.",
                            "target_lang_translations": {
                                "ru": "После долгой поездки у неё наконец появилось время на <w>себя</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "by herself",
                        "to herself",
                        "for herself"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвратная форма местоимения «она», используемая, когда действие подлежащего направлено на само себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "возвратное местоимение"
                            },
                            {
                                "target_lang_word": "себе",
                                "target_lang_sense_clarification": "дательный падеж (сделать что-то себе)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e48c16f5-17c3-360c-98b2-a0439ae5e300",
                    "sense_definition": "Used to emphasize the female subject or object of a sentence, stressing that it is her and not anyone else.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensive_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>herself</w> admitted that the mistake was hers.",
                            "target_lang_translations": {
                                "ru": "Она <w>сама</w> признала, что ошибка была её."
                            }
                        },
                        {
                            "text": "The director <w>herself</w> came to greet us at the door.",
                            "target_lang_translations": {
                                "ru": "Директор <w>сама</w> вышла встретить нас у двери."
                            }
                        }
                    ],
                    "common_phrases": [
                        "she herself"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления подлежащего или дополнения, подчеркивая, что это именно она, а не кто-то другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сама",
                                "target_lang_sense_clarification": "усилительное местоимение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48a50ecd-b934-302c-8e07-aebeca60c011",
                    "sense_definition": "Used to refer to a specific woman, often the mistress of a household or a woman of importance; sometimes used sarcastically.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "colloquial_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Is <w>herself</w> coming down for breakfast soon?",
                            "target_lang_translations": {
                                "ru": "<w>Сама</w> скоро спустится к завтраку?"
                            }
                        },
                        {
                            "text": "We had better be quiet because <w>herself</w> is in a bad mood today.",
                            "target_lang_translations": {
                                "ru": "Нам лучше вести себя тихо, потому что <w>сама</w> сегодня не в духе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used sarcastically or to imply self-importance"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения конкретной женщины, часто хозяйки дома или важной персоны; иногда используется саркастически."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сама",
                                "target_lang_sense_clarification": "разговорное, о хозяйке или начальнице"
                            },
                            {
                                "target_lang_word": "хозяйка",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
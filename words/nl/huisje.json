                        "optrekje"
                    ],
                    "common_phrases": [
                        "leuk huisje",
                        "eigen huisje",
                        "houten huisje"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Diminutive of <w>house</w>; a small dwelling or a small building to stay in.",
                        "ru": "Уменьшительная форма от слова \"huis\" (дом); небольшое жилище или маленькое здание для проживания."

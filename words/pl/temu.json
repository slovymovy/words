{
    "word_family": [
        "ten",
        "to",
        "tamten"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "f3a02555-edd2-3753-9db4-8fc2dd465efe",
                    "sense_definition": "Forma celownika liczby pojedynczej zaimka <w>ten</w> lub <w>to</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przyglądam się <w>temu</w> budynkowi z ciekawością.",
                            "target_lang_translations": {
                                "ru": "Я с любопытством присматриваюсь к <w>этому</w> зданию."
                            }
                        },
                        {
                            "text": "Dzięki <w>temu</w> udało nam się skończyć pracę wcześniej.",
                            "target_lang_translations": {
                                "ru": "Благодаря <w>этому</w> нам удалось закончить работу раньше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dzięki temu",
                        "przeciwko temu",
                        "ku temu",
                        "wbrew temu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма дательного падежа единственного числа местоимения «ten» (этот) или «to» (это)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "этому",
                                "target_lang_sense_clarification": "местоимение (дательный падеж)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "a480c103-bace-3e25-aa5f-a03759ef0c0c",
                    "sense_definition": "Słowo używane po określeniu czasu, wskazujące, że zdarzenie miało miejsce w przeszłości przed tym okresem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_ago",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To wszystko wydarzyło się rok <w>temu</w>.",
                            "target_lang_translations": {
                                "ru": "Всё это произошло год <w>назад</w>."
                            }
                        },
                        {
                            "text": "Rozmawialiśmy o tym problemie dwie godziny <w>temu</w>.",
                            "target_lang_translations": {
                                "ru": "Мы говорили об этой проблеме два часа <w>назад</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przed"
                    ],
                    "common_phrases": [
                        "dawno temu",
                        "rok temu",
                        "chwilę temu",
                        "tydzień temu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое после указания периода времени, обозначающее, что событие произошло в прошлом (назад)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "назад",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "тому назад",
                                "target_lang_sense_clarification": "о времени (устойчивое выражение)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "1a46077f-6c91-37ac-925e-bdf69f3aa306",
                    "sense_definition": "Regionalne słowo oznaczające: dlatego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "dialect_conj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie miał czasu, <w>temu</w> nie przyszedł na spotkanie.",
                            "target_lang_translations": {
                                "ru": "У него не было времени, <w>поэтому</w> он не пришёл на встречу."
                            }
                        },
                        {
                            "text": "Było bardzo zimno, <w>temu</w> zostaliśmy w domu.",
                            "target_lang_translations": {
                                "ru": "Было очень холодно, <w>поэтому</w> мы остались дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "wcześniej"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Bukowina i Podlasie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное слово, означающее «поэтому»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поэтому",
                                "target_lang_sense_clarification": "союз"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
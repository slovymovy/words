    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "100c11b4-e2af-3b98-bc5c-62e8ac486822",
                    "sense_definition": "To meet the needs, wishes, or expectations of someone.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fulfillment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We try our best to <w>satisfy</w> our customers with high quality products.",
                            "target_lang_translations": {
                                "ru": "Мы изо всех сил стараемся <w>удовлетворить</w> наших клиентов продуктами высокого качества."
                            }
                        },
                        {
                            "text": "Nothing seems to <w>satisfy</w> him, no matter how hard I try.",
                            "target_lang_translations": {
                                "ru": "Кажется, ничто не может его <w>удовлетворить</w>, как бы я ни старался."
                            }
                        }
                    ],
                    "synonyms": [
                        "please",
                        "gratify",
                        "fulfill",
                        "content"
                    ],
                    "antonyms": [
                        "dissatisfy",
                        "disappoint"
                    ],
                    "common_phrases": [
                        "satisfy a need",
                        "satisfy a customer",
                        "hard to satisfy",
                        "satisfy a demand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исполнять чьи-либо потребности, желания или ожидания"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удовлетворять",
                                "target_lang_sense_clarification": "исполнять желания или потребности"
                            },
                            {
                                "target_lang_word": "угождать",
                                "target_lang_sense_clarification": "делать приятное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc5e37da-a24b-3551-b281-b3d7fbc1fd94",
                    "sense_definition": "To provide enough food or drink to stop the feeling of hunger or thirst.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fulfillment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He ate a large sandwich to <w>satisfy</w> his hunger after the long hike.",
                            "target_lang_translations": {
                                "ru": "Он съел большой сэндвич, чтобы <w>утолить</w> голод после долгого похода."
                            }
                        },
                        {
                            "text": "A cool glass of water will <w>satisfy</w> your thirst on a hot day.",
                            "target_lang_translations": {
                                "ru": "Прохладный стакан воды <w>утолит</w> вашу жажду в жаркий день."
                            }
                        }
                    ],
                    "synonyms": [
                        "satiate",
                        "quench",
                        "slake",
                        "fill"
                    ],
                    "common_phrases": [
                        "satisfy hunger",
                        "satisfy thirst",
                        "satisfy an appetite"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать достаточно еды или питья, чтобы прекратить чувство голода или жажды"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утолять",
                                "target_lang_sense_clarification": "голод или жажду"
                            },
                            {
                                "target_lang_word": "удовлетворять",
                                "target_lang_sense_clarification": "физическую потребность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54c0b088-d525-3444-97b0-70ef8d3d6b85",
                    "sense_definition": "To convince someone that something is true or to remove doubt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cognitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She checked the report twice to <w>satisfy</w> herself that there were no errors.",
                            "target_lang_translations": {
                                "ru": "Она дважды проверила отчет, чтобы <w>убедиться</w>, что в нем нет ошибок."
                            }
                        },
                        {
                            "text": "The suspect could not <w>satisfy</w> the police with his alibi.",
                            "target_lang_translations": {
                                "ru": "Подозреваемый не смог <w>убедить</w> полицию своим алиби."
                            }
                        }
                    ],
                    "synonyms": [
                        "convince",
                        "assure",
                        "persuade"
                    ],
                    "common_phrases": [
                        "satisfy oneself",
                        "satisfy the jury",
                        "satisfy the court"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убеждать кого-либо в истинности чего-либо или устранять сомнения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убеждать",
                                "target_lang_sense_clarification": "доказывать правду"
                            },
                            {
                                "target_lang_word": "убеждаться",
                                "target_lang_sense_clarification": "удостоверяться самому"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d08cc1e-423c-354e-bb62-19179c3c8c77",
                    "sense_definition": "To fulfill a specific requirement, condition, or legal obligation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fulfillment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The applicant must <w>satisfy</w> all the criteria to get the scholarship.",
                            "target_lang_translations": {
                                "ru": "Заявитель должен <w>соответствовать</w> всем критериям, чтобы получить стипендию."
                            }
                        },
                        {
                            "text": "They worked hard to <w>satisfy</w> the terms of the contract before the deadline.",
                            "target_lang_translations": {
                                "ru": "Они усердно работали, чтобы <w>выполнить</w> условия контракта до крайнего срока."
                            }
                        }
                    ],
                    "synonyms": [
                        "meet",
                        "fulfill",
                        "comply with"
                    ],
                    "common_phrases": [
                        "satisfy a condition",
                        "satisfy a requirement",
                        "satisfy criteria",
                        "satisfy a debt"
                    ],
                    "traits": [

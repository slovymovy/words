                    ],
                    "antonyms": [
                        "тяжёлый"
                    ],
                    "common_phrases": [
                        "лёгкий чемодан",
                        "лёгкая сумка",
                        "лёгкий вес"
                    ],
                    "target_lang_definitions": {
                        "en": "Having little weight; not heavy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "light",
                                "target_lang_sense_clarification": "having little weight"
                            },
                            {
                                "target_lang_word": "lightweight",
                                "target_lang_sense_clarification": "made of thin or light material"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "937a3f2b-6860-392f-a974-24af13c7a63e",
                    "sense_definition": "Не требующий больших усилий, труда или напряжения; простой для понимания или выполнения.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это был очень <w>лёгкий</w> экзамен, и все студенты получили отличные оценки.",
                            "target_lang_translations": {
                                "en": "It was a very <w>easy</w> exam, and all the students got excellent grades."
                            }
                        },
                        {
                            "text": "Учитель задал нам <w>лёгкий</w> вопрос, на который я сразу ответил.",
                            "target_lang_translations": {
                                "en": "The teacher asked us an <w>easy</w> question that I answered immediately."
                            }
                        }
                    ],
                    "synonyms": [
                        "простой",
                        "нетрудный",
                        "несложный"
                    ],
                    "antonyms": [
                        "трудный",
                        "сложный",
                        "тяжёлый"

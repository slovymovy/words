            "senses": [
                {
                    "sense_id": "5a9c15cf-9c99-3118-a602-d24829f381b8",
                    "sense_definition": "Iets erbij doen of samenbrengen met iets anders.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "combine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de boter en de suiker bij elkaar <w>voegen</w>.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>смешать</w> масло с сахаром."
                            }
                        },
                        {
                            "text": "De kok zal nog wat kruiden aan de soep <w>voegen</w>.",
                            "target_lang_translations": {
                                "ru": "Повар <w>добавит</w> еще немного специй в суп."
                            }
                        }
                    ],
                    "synonyms": [
                        "samenbrengen",
                        "verenigen"
                    ],
                    "antonyms": [
                        "scheiden",
                        "splitsen"
                    ],
                    "common_phrases": [
                        "bij elkaar voegen",
                        "samen voegen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавлять что-то или соединять с чем-то другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединять",
                                "target_lang_sense_clarification": "объединять вместе"
                            },
                            {
                                "target_lang_word": "добавлять",
                                "target_lang_sense_clarification": "присоединять к чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8eeb055-891d-3a8b-98a1-2146d09e3594",
                    "sense_definition": "Zich aansluiten bij een groep of persoon.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "join_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij besloot zich bij de demonstranten te <w>voegen</w>.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>присоединиться</w> к демонстрантам."
                            }
                        },
                        {
                            "text": "Mag ik mij even bij jullie aan tafel <w>voegen</w>?",
                            "target_lang_translations": {
                                "ru": "Могу я <w>присоединиться</w> к вам за столом?"
                            }
                        }
                    ],
                    "synonyms": [
                        "aansluiten"
                    ],
                    "antonyms": [
                        "verlaten"
                    ],
                    "common_phrases": [
                        "zich voegen bij"
                    ],
                    "target_lang_definitions": {
                        "ru": "Присоединяться к группе или человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присоединяться",
                                "target_lang_sense_clarification": "вступать в группу или компанию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ad29de0-f011-3495-9417-d642ccf63f72",
                    "sense_definition": "Zich aanpassen aan de wensen, regels of omstandigheden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "comply_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je zult je moeten <w>voegen</w> naar de regels van dit huis.",
                            "target_lang_translations": {
                                "ru": "Тебе придется <w>подчиниться</w> правилам этого дома."
                            }
                        },
                        {
                            "text": "Na lang protesteren <w>voegde</w> hij zich toch naar het besluit.",
                            "target_lang_translations": {
                                "ru": "После долгих протестов он все же <w>покорился</w> решению."
                            }
                        }
                    ],
                    "synonyms": [
                        "schikken",
                        "aanpassen"
                    ],
                    "antonyms": [
                        "verzetten"
                    ],
                    "common_phrases": [
                        "zich voegen naar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приспосабливаться к желаниям, правилам или обстоятельствам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подчиняться",
                                "target_lang_sense_clarification": "следовать правилам или воле"
                            },
                            {
                                "target_lang_word": "покоряться",
                                "target_lang_sense_clarification": "смиряться с обстоятельствами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "875e58e3-4113-357a-82dd-de6f42fc46d0",
                    "sense_definition": "De naden tussen bakstenen of tegels opvullen met specie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De metselaar komt morgen de buitenmuur <w>voegen</w>.",
                            "target_lang_translations": {
                                "ru": "Каменщик придет завтра <w>расшивать швы</w> на внешней стене."
                            }
                        },
                        {
                            "text": "Als je de tegels hebt geplaatst, moet je ze nog netjes <w>voegen</w>.",
                            "target_lang_translations": {
                                "ru": "Когда уложишь плитку, нужно еще аккуратно <w>затереть швы</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "opnieuw voegen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заполнять швы между кирпичами или плиткой раствором."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затирать швы",
                                "target_lang_sense_clarification": "плитка"
                            },
                            {
                                "target_lang_word": "расшивать",
                                "target_lang_sense_clarification": "кирпичная кладка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43b5cecd-3037-34b1-8fb9-eeb93e18bc2e",
                    "sense_definition": "Passen bij of geschikt zijn voor iemand.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "suit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat onbeleefde gedrag <w>voegt</w> je helemaal niet.",
                            "target_lang_translations": {
                                "ru": "Это грубое поведение тебе совсем не <w>подобает</w>."
                            }
                        },
                        {
                            "text": "Het <w>voegde</w> hem niet om zo tegen zijn meerderen te spreken.",
                            "target_lang_translations": {
                                "ru": "Ему не <w>подобало</w> так разговаривать с начальством."
                            }
                        }
                    ],
                    "synonyms": [
                        "passen",
                        "betamen"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "This usage is largely replaced by 'passen' in modern Dutch."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходить или быть уместным для кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подобать",
                                "target_lang_sense_clarification": "быть приличным или уместным"
                            }
                        ]
                    }
                }
            ]
        },

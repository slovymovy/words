            "senses": [
                {
                    "sense_id": "7353f689-9a0b-3b6d-8b1c-f662fbec2de4",
                    "sense_definition": "To take part, join in, or become involved in an activity or event.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "All students are encouraged to <w>participate</w> in the school sports day.",
                            "target_lang_translations": {
                                "ru": "Всех студентов призывают <w>участвовать</w> в школьном спортивном дне."
                            }
                        },
                        {
                            "text": "She decided to <w>participate</w> actively in the group discussion.",
                            "target_lang_translations": {
                                "ru": "Она решила активно <w>участвовать</w> в групповом обсуждении."
                            }
                        }
                    ],
                    "synonyms": [
                        "take part",
                        "join in",
                        "engage"
                    ],
                    "antonyms": [
                        "abstain",
                        "withdraw",
                        "sit out"
                    ],
                    "common_phrases": [
                        "participate in",
                        "participate actively",
                        "participate fully",
                        "decline to participate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать участие, присоединяться или вовлекаться в деятельность или событие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участвовать",
                                "target_lang_sense_clarification": "принимать участие в чём-либо, быть участником"
                            },
                            {
                                "target_lang_word": "принимать участие",
                                "target_lang_sense_clarification": "становиться участником события или деятельности"
                            }
                        ]
                    }
                },
                {

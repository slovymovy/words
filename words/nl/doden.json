{
    "word_family": [
        "dood",
        "dode",
        "dodelijk",
        "doding",
        "doods"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b2874eed-1031-30dc-ade7-0c62aebe0f28",
                    "sense_definition": "Iemand of iets van het leven beroven; zorgen dat een levend wezen sterft.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "kill_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten kregen de opdracht om de vijand te <w>doden</w>.",
                            "target_lang_translations": {
                                "en": "The soldiers were ordered to <w>kill</w> the enemy."
                            }
                        },
                        {
                            "text": "Het is verboden om beschermde diersoorten te <w>doden</w>.",
                            "target_lang_translations": {
                                "en": "It is forbidden to <w>kill</w> protected animal species."
                            }
                        }
                    ],
                    "synonyms": [
                        "afmaken",
                        "omleggen",
                        "om zeep helpen",
                        "van kant maken"
                    ],
                    "common_phrases": [
                        "iemand doden",
                        "per ongeluk doden"
                    ],
                    "target_lang_definitions": {
                        "en": "To deprive someone or something of life; to cause a living being to die."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kill",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "put to death",
                                "target_lang_sense_clarification": "formal or legal context"
                            },
                            {
                                "target_lang_word": "slay",
                                "target_lang_sense_clarification": "literary or archaic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7064b5ac-5d9a-3290-9149-79c4815b09e7",
                    "sense_definition": "Iets doen om verveling tegen te gaan terwijl men wacht.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "kill_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de wachtkamer lazen we tijdschriften om de tijd te <w>doden</w>.",
                            "target_lang_translations": {
                                "en": "In the waiting room, we read magazines to <w>kill</w> time."
                            }
                        },
                        {
                            "text": "Hij speelde een spelletje op zijn telefoon om de tijd te <w>doden</w> tijdens de lange treinreis.",
                            "target_lang_translations": {
                                "en": "He played a game on his phone to <w>kill</w> time during the long train journey."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de tijd doden"
                    ],
                    "target_lang_definitions": {
                        "en": "To do something to pass the time or prevent boredom while waiting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kill",
                                "target_lang_sense_clarification": "used with 'time'"
                            },
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "used with 'time'"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f194814e-7353-3a06-97d3-ab6fcb928bbf",
                    "sense_definition": "Meervoud van <w>dode</w>; mensen die overleden zijn.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "dead_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op 4 mei herdenken we in Nederland de <w>doden</w>.",
                            "target_lang_translations": {
                                "en": "On May 4th, we commemorate the <w>dead</w> in the Netherlands."
                            }
                        },
                        {
                            "text": "Bij de zware aardbeving zijn helaas veel <w>doden</w> gevallen.",
                            "target_lang_translations": {
                                "en": "Unfortunately, there were many <w>fatalities</w> in the severe earthquake."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de doden herdenken",
                        "rijk der doden"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "People who have died; the deceased."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the dead",
                                "target_lang_sense_clarification": "collective group"
                            },
                            {
                                "target_lang_word": "fatalities",
                                "target_lang_sense_clarification": "deaths in an accident or disaster"
                            },
                            {
                                "target_lang_word": "deaths",
                                "target_lang_sense_clarification": "instances of dying"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32663576-12fc-37fb-8b5b-334138e18b55",
                    "sense_definition": "Meervoud van <w>dood</w>; voornamelijk gebruikt in de uitdrukking 'duizend doden sterven' om intense angst uit te drukken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "idiomatic_death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij stierf duizend <w>doden</w> toen hij het podium op moest voor zijn toespraak.",
                            "target_lang_translations": {
                                "en": "He died a thousand <w>deaths</w> when he had to take the stage for his speech."
                            }
                        },
                        {
                            "text": "Tijdens de spannende film stierf ik duizend <w>doden</w> van de zenuwen.",
                            "target_lang_translations": {
                                "en": "During the suspenseful movie, I died a thousand <w>deaths</w> from nerves."
                            }
                        }
                    ],
                    "common_phrases": [
                        "duizend doden sterven"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of death; mainly used in the idiom 'to die a thousand deaths'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deaths",
                                "target_lang_sense_clarification": "plural form"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
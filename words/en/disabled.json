                    "synonyms": [
                        "incapacitated"
                    ],
                    "antonyms": [
                        "enabled"
                    ],
                    "common_phrases": [
                        "permanently disabled",
                        "temporarily disabled",
                        "disabled account"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нерабочий, выключенный или выведенный из строя (о механизмах или функциях)."

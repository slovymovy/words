{
    "word_family": [
        "geld",
        "geldig",
        "geldigheid",
        "vergelden",
        "ontgelden"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fea9ec15-f7fa-3013-bedf-b67c2d74afb4",
                    "sense_definition": "Van toepassing zijn of in werking zijn (bijvoorbeeld regels, wetten of afspraken).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_apply",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze regels <gelden> voor alle studenten op de universiteit.",
                            "target_lang_translations": {
                                "en": "These rules <apply> to all students at the university.",
                                "ru": "Эти правила <распространяются> на всех студентов университета."
                            }
                        },
                        {
                            "text": "In de bibliotheek <geldt> een verbod op hard praten.",
                            "target_lang_translations": {
                                "en": "A ban on talking loudly <is in effect> in the library.",
                                "ru": "В библиотеке <действует> запрет на громкие разговоры."
                            }
                        }
                    ],
                    "synonyms": [
                        "van kracht zijn",
                        "opgaan"
                    ],
                    "antonyms": [
                        "vervallen"
                    ],
                    "common_phrases": [
                        "regels gelden",
                        "voor iedereen gelden",
                        "wetten gelden"
                    ],
                    "target_lang_definitions": {
                        "en": "To be applicable or in effect (e.g., rules, laws, or agreements).",
                        "ru": "Быть применимым или действующим (например, о правилах, законах или соглашениях)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "apply",
                                "target_lang_sense_clarification": "to be relevant to a specific person or situation"
                            },
                            {
                                "target_lang_word": "be in effect",
                                "target_lang_sense_clarification": "to be active or enforceable (of rules/laws)"
                            },
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "to remain true or valid"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "действовать",
                                "target_lang_sense_clarification": "быть в силе"
                            },
                            {
                                "target_lang_word": "распространяться",
                                "target_lang_sense_clarification": "быть применимым к кому-либо"
                            },
                            {
                                "target_lang_word": "иметь силу",
                                "target_lang_sense_clarification": "о законе или правиле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "052ba74a-f896-3e6c-a91e-9f322799c26b",
                    "sense_definition": "Waarde hebben, geldig zijn of meetellen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit treinkaartje <geldt> alleen in het weekend.",
                            "target_lang_translations": {
                                "en": "This train ticket <is valid> only on the weekend.",
                                "ru": "Этот билет на поезд <действует> только в выходные."
                            }
                        },
                        {
                            "text": "Een doelpunt in de laatste minuut <geldt> natuurlijk ook.",
                            "target_lang_translations": {
                                "en": "A goal in the last minute naturally <counts> as well.",
                                "ru": "Гол на последней минуте, конечно, тоже <засчитывается>."
                            }
                        }
                    ],
                    "synonyms": [
                        "meetellen",
                        "tellen"
                    ],
                    "common_phrases": [
                        "niet meer gelden",
                        "zwaar gelden"
                    ],
                    "target_lang_definitions": {
                        "en": "To have value, be valid, or to count.",
                        "ru": "Иметь ценность, быть действительным (о документах) или идти в зачет."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be valid",
                                "target_lang_sense_clarification": "usable or acceptable (e.g., tickets, documents)"
                            },
                            {
                                "target_lang_word": "count",
                                "target_lang_sense_clarification": "to matter or be included in a calculation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть действительным",
                                "target_lang_sense_clarification": "о билетах или документах"
                            },
                            {
                                "target_lang_word": "засчитываться",
                                "target_lang_sense_clarification": "идти в счет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff1973c6-8b8e-3a43-9d9a-a81422440599",
                    "sense_definition": "Beschouwd worden als; de status hebben van.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_consider",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <geldt> als een van de beste experts op dit gebied.",
                            "target_lang_translations": {
                                "en": "He <is considered> one of the best experts in this field.",
                                "ru": "Он <считается> одним из лучших экспертов в этой области."
                            }
                        },
                        {
                            "text": "Dit boek <geldt> als een klassieker in de Nederlandse literatuur.",
                            "target_lang_translations": {
                                "en": "This book <is regarded> as a classic in Dutch literature.",
                                "ru": "Эта книга <считается> классикой нидерландской литературы."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorgaan voor"
                    ],
                    "common_phrases": [
                        "gelden als"
                    ],
                    "target_lang_definitions": {
                        "en": "To be considered or regarded as having a certain status.",
                        "ru": "Рассматриваться как; иметь статус или репутацию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be considered",
                                "target_lang_sense_clarification": "to be viewed as"
                            },
                            {
                                "target_lang_word": "be regarded",
                                "target_lang_sense_clarification": "to be looked upon as"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "считаться",
                                "target_lang_sense_clarification": "восприниматься как"
                            },
                            {
                                "target_lang_word": "слыть",
                                "target_lang_sense_clarification": "иметь репутацию"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c8b6f0b6-9f49-3f9d-bd21-63106051563a",
                    "sense_definition": "Meervoud van <w>geld</w>; vaak gebruikt om te verwijzen naar specifieke financiële middelen, fondsen of budgetten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente heeft extra <gelden> beschikbaar gesteld voor het nieuwe park.",
                            "target_lang_translations": {
                                "en": "The municipality has made extra <funds> available for the new park.",
                                "ru": "Муниципалитет выделил дополнительные <средства> на новый парк."
                            }
                        },
                        {
                            "text": "Er zijn geen publieke <gelden> gebruikt voor dit privéproject.",
                            "target_lang_translations": {
                                "en": "No public <funds> were used for this private project.",
                                "ru": "Для этого частного проекта не использовались государственные <средства>."
                            }
                        }
                    ],
                    "synonyms": [
                        "fondsen",
                        "middelen",
                        "financiën"
                    ],
                    "common_phrases": [
                        "publieke gelden",
                        "beschikbare gelden"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural form of 'money'; often used to refer to specific financial resources, funds, or budgets.",
                        "ru": "Финансовые средства, фонды или бюджеты (форма множественного числа)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "funds",
                                "target_lang_sense_clarification": "financial resources available for use"
                            },
                            {
                                "target_lang_word": "monies",
                                "target_lang_sense_clarification": "formal term for sums of money"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "средства",
                                "target_lang_sense_clarification": "финансы"
                            },
                            {
                                "target_lang_word": "фонды",
                                "target_lang_sense_clarification": "денежные ресурсы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
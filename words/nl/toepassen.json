{
    "word_family": [
        "toepassing",
        "toepasbaar",
        "toepasselijk"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "44f64ef9-df15-3d94-bf6b-e975b6d9ca1d",
                    "sense_definition": "Iets wat vastgelegd of geleerd is, zoals een regel, theorie of kennis, in de praktijk uitvoeren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gebruik_uitvoering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De scheidsrechter moet de spelregels tijdens de wedstrijd strikt <w>toepassen</w>.",
                            "target_lang_translations": {
                                "ru": "Судья должен строго <w>применять</w> правила игры во время матча."
                            }
                        },
                        {
                            "text": "Hij wil de kennis die hij op school heeft geleerd in zijn werk <w>toepassen</w>.",
                            "target_lang_translations": {
                                "ru": "Он хочет <w>применять</w> знания, полученные в школе, в своей работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitvoeren",
                        "hanteren",
                        "praktiseren"
                    ],
                    "antonyms": [
                        "negeren"
                    ],
                    "common_phrases": [
                        "regels toepassen",
                        "de wet toepassen",
                        "kennis toepassen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осуществлять на практике что-то установленное или изученное, например, правило, теорию или знания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "применять",
                                "target_lang_sense_clarification": "использовать на практике (правила, знания)"
                            },
                            {
                                "target_lang_word": "соблюдать",
                                "target_lang_sense_clarification": "следовать правилам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d7c0941-0a80-30fe-85a1-8281dd78f947",
                    "sense_definition": "Gebruiken of aanwenden van een bepaald middel, een techniek of een methode om een doel te bereiken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gebruik_uitvoering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts besloot een nieuwe medische behandeling <w>toe</w> te <w>passen</w>.",
                            "target_lang_translations": {
                                "ru": "Врач решил <w>применить</w> новый метод лечения."
                            }
                        },
                        {
                            "text": "Ik ga nu even deze speciale techniek <w>toepassen</w> om het probleem op te lossen.",
                            "target_lang_translations": {
                                "ru": "Я сейчас <w>применю</w> эту специальную технику, чтобы решить проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "gebruiken",
                        "aanwenden",
                        "benutten"
                    ],
                    "common_phrases": [
                        "een techniek toepassen",
                        "een methode toepassen",
                        "een strategie toepassen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать определенное средство, технику или метод для достижения цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "применять",
                                "target_lang_sense_clarification": "использовать метод или средство"
                            },
                            {
                                "target_lang_word": "использовать",
                                "target_lang_sense_clarification": "пользоваться чем-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]

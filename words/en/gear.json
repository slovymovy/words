    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2b1e27d7-f201-39b2-8c91-669c9632bda7",
                    "sense_definition": "A set of equipment, tools, or supplies needed for a particular activity or purpose.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We loaded all the camping <w>gear</w> into the back of the van.",
                            "target_lang_translations": {
                                "ru": "Мы погрузили всё туристическое <w>снаряжение</w> в кузов фургона."
                            }
                        },
                        {
                            "text": "Firefighters must wear protective <w>gear</w> before entering a burning building.",
                            "target_lang_translations": {
                                "ru": "Пожарные должны надеть защитную <w>экипировку</w> перед входом в горящее здание."
                            }
                        }
                    ],
                    "synonyms": [
                        "equipment",
                        "kit",
                        "tackle",
                        "apparatus",
                        "paraphernalia"
                    ],
                    "common_phrases": [
                        "camping gear",
                        "fishing gear",
                        "protective gear",
                        "sports gear",
                        "running gear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность предметов, инструментов или припасов, необходимых для определенной деятельности или цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снаряжение",
                                "target_lang_sense_clarification": "оборудование для походов, военных действий и т.д."
                            },
                            {
                                "target_lang_word": "экипировка",
                                "target_lang_sense_clarification": "одежда и снаряжение, часто спортивное или защитное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89ee132e-b2bd-32a6-8f69-3fc9ba9ef837",
                    "sense_definition": "A specific setting in a vehicle's transmission system that controls the relationship between engine speed and wheel speed.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to shift into a lower <w>gear</w> to drive up this steep hill.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно переключиться на более низкую <w>передачу</w>, чтобы заехать на этот крутой холм."
                            }
                        },
                        {
                            "text": "He accidentally left the car in <w>gear</w> when he parked it.",
                            "target_lang_translations": {
                                "ru": "Он случайно оставил машину на <w>передаче</w>, когда парковался."
                            }
                        }
                    ],
                    "common_phrases": [
                        "change gear",
                        "shift gear",
                        "first gear",
                        "reverse gear",
                        "in gear",
                        "out of gear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Механизм в трансмиссии транспортного средства, регулирующий соотношение между скоростью двигателя и скоростью колес."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передача",
                                "target_lang_sense_clarification": "ступень в коробке передач"
                            },
                            {
                                "target_lang_word": "скорость",
                                "target_lang_sense_clarification": "разговорное название передачи в автомобиле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9abf714-4480-3e27-8294-35bfc733b279",
                    "sense_definition": "A toothed wheel that meshes with another to transmit motion or change speed in a machine.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The clock stopped working because a small plastic <w>gear</w> inside it broke.",
                            "target_lang_translations": {
                                "ru": "Часы остановились, потому что внутри сломалась маленькая пластиковая <w>шестерёнка</w>."
                            }
                        },
                        {
                            "text": "The mechanic showed me the worn <w>gear</w> that caused the engine noise.",
                            "target_lang_translations": {
                                "ru": "Механик показал мне изношенную <w>шестерню</w>, которая была причиной шума в двигателе."
                            }
                        }
                    ],
                    "synonyms": [
                        "cog",
                        "cogwheel",
                        "sprocket"
                    ],
                    "common_phrases": [
                        "gear wheel",
                        "gear teeth",
                        "internal gear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зубчатое колесо, которое сцепляется с другим для передачи движения или изменения скорости в механизме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шестерня",
                                "target_lang_sense_clarification": "зубчатое колесо"
                            },
                            {
                                "target_lang_word": "зубчатое колесо",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a2d5c4e-1f68-3b40-b342-47f6c13ae891",
                    "sense_definition": "Clothing or garments, often referring to stylish or specific types of attire.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spends most of his salary on expensive designer <w>gear</w>.",
                            "target_lang_translations": {
                                "ru": "Он тратит большую часть зарплаты на дорогую дизайнерскую <w>одежду</w>."
                            }
                        },
                        {
                            "text": "Make sure you pack your swimming <w>gear</w> for the holiday.",
                            "target_lang_translations": {
                                "ru": "Не забудь упаковать свои плавательные <w>принадлежности</w> для отпуска."
                            }
                        }
                    ],
                    "synonyms": [
                        "clothes",
                        "clothing",
                        "attire",
                        "togs",
                        "apparel"
                    ],
                    "common_phrases": [
                        "designer gear"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одежда, часто подразумевающая стильные или специфические виды нарядов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одежда",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "вещи",
                                "target_lang_sense_clarification": "разговорное, предметы гардероба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ebf3fa5-c4e9-30de-8907-753d0f7417c0",
                    "sense_definition": "The wheels and mechanical structure used by an aircraft for takeoff and landing.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot reported a problem with the <w>gear</w> not locking into place.",
                            "target_lang_translations": {
                                "ru": "Пилот сообщил о проблеме с тем, что <w>шасси</w> не фиксируется на месте."
                            }
                        },
                        {
                            "text": "They had to perform a belly landing because the <w>gear</w> was stuck.",
                            "target_lang_translations": {
                                "ru": "Им пришлось совершить посадку на брюхо, потому что <w>шасси</w> заклинило."
                            }
                        }
                    ],
                    "synonyms": [
                        "landing gear"
                    ],
                    "common_phrases": [
                        "gear up",
                        "gear down"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "aviation context"

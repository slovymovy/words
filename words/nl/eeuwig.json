{
    "word_family": [
        "eeuw",
        "eeuwigheid"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6ec8243f-e255-3bf0-876a-2467a1edb7a6",
                    "sense_definition": "Zonder einde in de tijd; wat altijd blijft bestaan.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "literal_eternal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hun trouwgeloften beloven <w>eeuwige</w> liefde.",
                            "target_lang_translations": {
                                "en": "Their wedding vows promise <w>eternal</w> love.",
                                "ru": "Их брачные обеты обещают <w>вечную</w> любовь."
                            }
                        },
                        {
                            "text": "Niets is <w>eeuwig</w>, aan alles komt een einde.",
                            "target_lang_translations": {
                                "en": "Nothing is <w>eternal</w>, everything comes to an end.",
                                "ru": "Ничто не <w>вечно</w>, всему приходит конец."
                            }
                        }
                    ],
                    "synonyms": [
                        "oneindig",
                        "onvergankelijk",
                        "altijddurend"
                    ],
                    "antonyms": [
                        "tijdelijk",
                        "vergankelijk",
                        "eindig"
                    ],
                    "common_phrases": [
                        "het eeuwige leven",
                        "eeuwige trouw",
                        "eeuwige roem",
                        "eeuwige jeugd"
                    ],
                    "target_lang_definitions": {
                        "en": "Lasting for all time; without beginning or end.",
                        "ru": "Без конца во времени; то, что существует всегда."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eternal",
                                "target_lang_sense_clarification": "lasting forever"
                            },
                            {
                                "target_lang_word": "everlasting",
                                "target_lang_sense_clarification": "lasting through all time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вечный",
                                "target_lang_sense_clarification": "не имеющий конца во времени"
                            },
                            {
                                "target_lang_word": "бессмертный",
                                "target_lang_sense_clarification": "никогда не умирающий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c891ab20-ff99-3447-b735-aebbb993d139",
                    "sense_definition": "Wat heel lang duurt of lijkt te duren, vaak gebruikt als overdrijving.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_long",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kus in de film lijkt <w>eeuwig</w> te duren.",
                            "target_lang_translations": {
                                "en": "The kiss in the movie seems to last <w>forever</w>.",
                                "ru": "Кажется, что поцелуй в фильме длится <w>вечно</w>."
                            }
                        },
                        {
                            "text": "Het wachten op de uitslag duurde voor mijn gevoel <w>eeuwig</w>.",
                            "target_lang_translations": {
                                "en": "Waiting for the result felt like it took <w>forever</w>.",
                                "ru": "Ожидание результатов, по моим ощущениям, длилось <w>вечно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "eindeloos",
                        "langdurig"
                    ],
                    "antonyms": [
                        "kort",
                        "vluchtig"
                    ],
                    "common_phrases": [
                        "eeuwig wachten",
                        "eeuwige discussie"
                    ],
                    "target_lang_definitions": {
                        "en": "Seeming to last an extremely long time; interminable.",
                        "ru": "То, что длится или кажется, что длится очень долго; часто используется как преувеличение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "endless",
                                "target_lang_sense_clarification": "seeming to have no end"
                            },
                            {
                                "target_lang_word": "forever",
                                "target_lang_sense_clarification": "an extremely long time (informal)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бесконечный",
                                "target_lang_sense_clarification": "очень долгий, нескончаемый"
                            },
                            {
                                "target_lang_word": "вечный",
                                "target_lang_sense_clarification": "разг. очень долгий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "6258230a-1558-3b53-9a35-bdf868fe8457",
                    "sense_definition": "Voor altijd; zonder ooit te stoppen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "literal_eternal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zal <w>eeuwig</w> in onze herinneringen voortleven.",
                            "target_lang_translations": {
                                "en": "He will live on in our memories <w>forever</w>.",
                                "ru": "Он будет <w>вечно</w> жить в нашей памяти."
                            }
                        },
                        {
                            "text": "Deze bijzondere vriendschap zal <w>eeuwig</w> duren.",
                            "target_lang_translations": {
                                "en": "This special friendship will last <w>forever</w>.",
                                "ru": "Эта особенная дружба будет длиться <w>вечно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "altijd",
                        "voorgoed"
                    ],
                    "antonyms": [
                        "nooit",
                        "tijdelijk"
                    ],
                    "common_phrases": [
                        "voor eeuwig"
                    ],
                    "target_lang_definitions": {
                        "en": "For all time; permanently.",
                        "ru": "Навсегда; без остановки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forever",
                                "target_lang_sense_clarification": "for all future time"
                            },
                            {
                                "target_lang_word": "eternally",
                                "target_lang_sense_clarification": "in a way that continues forever"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вечно",
                                "target_lang_sense_clarification": "всегда"
                            },
                            {
                                "target_lang_word": "навсегда",
                                "target_lang_sense_clarification": "на все времена"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2e86261-685a-317d-ad6f-53f9050252a3",
                    "sense_definition": "Voortdurend of gedurende een zeer lange tijd, vaak gebruikt om irritatie uit te drukken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_long",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het kind bleef maar <w>eeuwig</w> doorzeuren en kreeg uiteindelijk haar zin.",
                            "target_lang_translations": {
                                "en": "The child kept whining <w>forever</w> and eventually got her way.",
                                "ru": "Ребенок продолжал <w>бесконечно</w> ныть и в конце концов добился своего."
                            }
                        },
                        {
                            "text": "Hij is <w>eeuwig</w> bezig met zijn telefoon en let niet op.",
                            "target_lang_translations": {
                                "en": "He is <w>forever</w> busy with his phone and doesn't pay attention.",
                                "ru": "Он <w>вечно</w> занят своим телефоном и не обращает внимания."
                            }
                        }
                    ],
                    "synonyms": [
                        "constant",
                        "voortdurend",
                        "eindeloos"
                    ],
                    "antonyms": [
                        "even",
                        "zelden"
                    ],
                    "common_phrases": [
                        "eeuwig en altijd",
                        "eeuwig dankbaar"
                    ],
                    "target_lang_definitions": {
                        "en": "Constantly or continually, often implying irritation.",
                        "ru": "Постоянно или в течение очень долгого времени, часто используется для выражения раздражения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forever",
                                "target_lang_sense_clarification": "continually or frequently"
                            },
                            {
                                "target_lang_word": "constantly",
                                "target_lang_sense_clarification": "happening all the time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вечно",
                                "target_lang_sense_clarification": "постоянно (с оттенком недовольства)"
                            },
                            {
                                "target_lang_word": "постоянно",
                                "target_lang_sense_clarification": "все время"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
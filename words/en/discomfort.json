    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2ddd0ad6-27cf-32fe-a8f0-77c71b0327a0",
                    "sense_definition": "A feeling of slight pain, physical irritation, or psychological unease.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_discomfort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She felt some <w>discomfort</w> in her stomach after eating the spicy food.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала некоторый <w>дискомфорт</w> в желудке после острой еды."
                            }
                        },
                        {
                            "text": "The tight shoes caused him a great deal of physical <w>discomfort</w>.",
                            "target_lang_translations": {
                                "ru": "Тесные туфли причиняли ему сильное физическое <w>неудобство</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "unease",
                        "distress",
                        "pain",
                        "irritation"
                    ],
                    "antonyms": [
                        "comfort",
                        "ease",
                        "relief"
                    ],
                    "common_phrases": [
                        "physical discomfort",
                        "abdominal discomfort",
                        "cause discomfort",
                        "feeling of discomfort"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство легкой боли, физического раздражения или психологического беспокойства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дискомфорт",
                                "target_lang_sense_clarification": "ощущение неудобства"
                            },
                            {
                                "target_lang_word": "неудобство",
                                "target_lang_sense_clarification": "физическое или душевное стеснение"
                            },
                            {
                                "target_lang_word": "неприятное ощущение",
                                "target_lang_sense_clarification": "болезненность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63a4d122-0fcb-3ed1-9387-6906efc627f7",
                    "sense_definition": "Something that causes a lack of comfort or makes one feel annoyed.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_discomfort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Camping often involves minor <w>discomforts</w> like sleeping on the hard ground.",
                            "target_lang_translations": {
                                "ru": "Походы часто сопряжены с мелкими <w>неудобствами</w>, такими как сон на жесткой земле."
                            }
                        },
                        {
                            "text": "He tried to ignore the <w>discomforts</w> of traveling in such an old bus.",
                            "target_lang_translations": {
                                "ru": "Он старался не замечать <w>неудобств</w> путешествия в таком старом автобусе."
                            }
                        }
                    ],
                    "synonyms": [
                        "annoyance",
                        "inconvenience",
                        "nuisance"
                    ],
                    "antonyms": [
                        "luxury",
                        "convenience"
                    ],
                    "common_phrases": [
                        "minor discomforts",
                        "endure discomforts"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что лишает комфорта или вызывает раздражение (обычно во множественном числе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неудобство",
                                "target_lang_sense_clarification": "отсутствие комфортных условий"
                            },
                            {
                                "target_lang_word": "лишение",
                                "target_lang_sense_clarification": "отсутствие привычных благ"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c1b202bb-2cb1-348f-bfc0-43c141e7bb79",
                    "sense_definition": "To make someone feel anxious, uneasy, or physically uncomfortable.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The intense silence in the room seemed to <w>discomfort</w> the guests.",
                            "target_lang_translations": {
                                "ru": "Напряженная тишина в комнате, казалось, <w>смущала</w> гостей."
                            }
                        },
                        {
                            "text": "I do not wish to <w>discomfort</w> you with these difficult questions.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <w>смущать</w> вас этими сложными вопросами."
                            }
                        }
                    ],
                    "synonyms": [
                        "unsettle",
                        "disturb",
                        "annoy",
                        "distress"
                    ],
                    "antonyms": [
                        "comfort",
                        "soothe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять кого-то чувствовать тревогу, неловкость или физическое неудобство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смущать",
                                "target_lang_sense_clarification": "приводить в замешательство"
                            },
                            {
                                "target_lang_word": "доставлять неудобство",
                                "target_lang_sense_clarification": "причинять беспокойство"
                            },
                            {
                                "target_lang_word": "беспокоить",
                                "target_lang_sense_clarification": "нарушать покой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3048f2f1-2179-3c41-a1a8-8325564bf87c",
                    "sense_definition": "To discourage or take away hope from someone.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden loss of their leader did <w>discomfort</w> the troops.",
                            "target_lang_translations": {
                                "ru": "Внезапная потеря командира действительно <w>обескуражила</w> войска."
                            }
                        },
                        {
                            "text": "We must not let this failure <w>discomfort</w> us in our mission.",
                            "target_lang_translations": {
                                "ru": "Мы не должны позволить этой неудаче <w>лишить</w> нас <w>мужества</w> в нашей миссии."
                            }
                        }
                    ],
                    "synonyms": [
                        "discourage",
                        "deject",
                        "dismay"
                    ],
                    "antonyms": [
                        "encourage",
                        "embolden"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This sense is rarely used in modern English."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишать кого-то надежды или мужества (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обескураживать",
                                "target_lang_sense_clarification": "лишать уверенности"
                            },
                            {
                                "target_lang_word": "удручать",
                                "target_lang_sense_clarification": "приводить в подавленное состояние"
                            }
                        ]
                    }
                }
            ]
        }

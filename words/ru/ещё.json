{
    "word_family": [],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "bc7af8f6-70d3-3993-b58f-26f43ac71382",
                    "sense_definition": "Обозначает продолжение действия или состояния, которое началось ранее; до сих пор.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы <ещё> не закончили работу над проектом.",
                            "target_lang_translations": {
                                "en": "We haven't finished working on the project <yet>."
                            }
                        },
                        {
                            "text": "Он всё <ещё> живёт в этом старом доме.",
                            "target_lang_translations": {
                                "en": "He <still> lives in this old house."
                            }
                        }
                    ],
                    "common_phrases": [
                        "всё ещё",
                        "ещё не",
                        "ещё нет"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the continuation of an action or state that began earlier; still; (in negative constructions) not yet."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "still",
                                "target_lang_sense_clarification": "continuing action or state"
                            },
                            {
                                "target_lang_word": "yet",
                                "target_lang_sense_clarification": "used in negative sentences (not yet)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66f2ec4e-ed7b-3e7a-bff9-8f0c4b3f0a37",
                    "sense_definition": "В добавление к чему-либо, дополнительно; снова, опять.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "additive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Налейте мне, пожалуйста, <ещё> немного чаю.",
                            "target_lang_translations": {
                                "en": "Please pour me some <more> tea."
                            }
                        },
                        {
                            "text": "Я хочу посмотреть этот фильм <ещё> раз.",
                            "target_lang_translations": {
                                "en": "I want to watch this movie one <more> time."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ещё раз",
                        "ещё один",
                        "что ещё",
                        "кто ещё"
                    ],
                    "target_lang_definitions": {
                        "en": "In addition to something; additional; more; else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "more",
                                "target_lang_sense_clarification": "additional quantity or degree"
                            },
                            {
                                "target_lang_word": "else",
                                "target_lang_sense_clarification": "different or additional (used with pronouns/adverbs)"
                            },
                            {
                                "target_lang_word": "another",
                                "target_lang_sense_clarification": "one more"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b875370-06fe-350f-b403-e00072ed233e",
                    "sense_definition": "Употребляется со сравнительной степенью прилагательных и наречий для усиления качества.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сегодня погода стала <ещё> лучше, чем вчера.",
                            "target_lang_translations": {
                                "en": "Today the weather has become <even> better than yesterday."
                            }
                        },
                        {
                            "text": "Эта задача оказалась <ещё> сложнее, чем мы думали.",
                            "target_lang_translations": {
                                "en": "This task turned out to be <even> harder than we thought."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ещё лучше",
                        "ещё больше",
                        "ещё хуже"
                    ],
                    "target_lang_definitions": {
                        "en": "Used with the comparative degree of adjectives and adverbs to intensify the quality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "even",
                                "target_lang_sense_clarification": "intensifier for comparatives"
                            },
                            {
                                "target_lang_word": "still",
                                "target_lang_sense_clarification": "intensifier for comparatives (still better)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "feff447c-ba69-3ea4-afb7-f2574f14bd81",
                    "sense_definition": "По крайней мере, в некоторой степени (при сравнении с чем-то худшим или другим).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "modal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тебе <ещё> повезло, что ты не опоздал на поезд.",
                            "target_lang_translations": {
                                "en": "You were <still> lucky that you didn't miss the train."
                            }
                        },
                        {
                            "text": "Этот вариант <ещё> ничего, остальные гораздо хуже.",
                            "target_lang_translations": {
                                "en": "This option is <still> alright; the others are much worse."
                            }
                        }
                    ],
                    "synonyms": [
                        "относительно",
                        "хотя бы"
                    ],
                    "target_lang_definitions": {
                        "en": "At least; to some extent; implies a concession or comparison with a worse alternative."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "still",
                                "target_lang_sense_clarification": "nevertheless; despite that"
                            },
                            {
                                "target_lang_word": "at least",
                                "target_lang_sense_clarification": "minimum degree"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "666d3f0f-53ba-3f03-bd32-3ecbb9961b56",
                    "sense_definition": "Употребляется для выражения удивления, возмущения или досады.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "emphatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "И он <ещё> спорит со мной после всего этого!",
                            "target_lang_translations": {
                                "en": "And he <still> argues with me after all that!"
                            }
                        },
                        {
                            "text": "Куда <ещё> нам идти в такую ужасную погоду?",
                            "target_lang_translations": {
                                "en": "Where <else> can we go in such terrible weather?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to express surprise, indignation, or annoyance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "still",
                                "target_lang_sense_clarification": "emphatic usage"
                            },
                            {
                                "target_lang_word": "else",
                                "target_lang_sense_clarification": "in questions (where else, what else)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
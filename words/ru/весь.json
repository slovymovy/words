                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "totality_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Всё</w> будет хорошо, не волнуйся.",
                            "target_lang_translations": {

                            },
                            {
                                "target_lang_word": "заинтересовать",
                                "target_lang_sense_clarification": "вызвать интерес к чему-либо"
                            },
                            {
                                "target_lang_word": "привлечь",
                                "target_lang_sense_clarification": "приманить (например, клиентов)"
                            },
                            {
                                "target_lang_word": "вдохновить",
                                "target_lang_sense_clarification": "воодушевить на поступок"
                            }
                        ]

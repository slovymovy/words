            "pos": "noun",
            "senses": [
                {
                    "sense_id": "31e6032b-888c-3611-9cae-65e925b0426f",
                    "sense_definition": "Ogół osób zgromadzonych w celu obejrzenia przedstawienia, filmu lub innego widowiska.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten mało ambitny film jest skierowany do niezbyt wymagającej <w>widowni</w>.",
                            "target_lang_translations": {
                                "ru": "Этот малоамбициозный фильм рассчитан на не слишком требовательную <w>аудиторию</w>."
                            }
                        },
                        {
                            "text": "Cała <w>widownia</w> wstała, aby nagrodzić artystów gromkimi brawami.",
                            "target_lang_translations": {
                                "ru": "Вся <w>публика</w> встала, чтобы наградить артистов громкими аплодисментами."
                            }
                        }
                    ],
                    "synonyms": [
                        "publiczność",
                        "publika",
                        "audytorium"
                    ],
                    "common_phrases": [
                        "liczna widownia",
                        "reakcja widowni",
                        "porwać widownię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность людей, собравшихся для просмотра представления, фильма или другого зрелища."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публика",
                                "target_lang_sense_clarification": "люди, зрители (собирательное)"
                            },
                            {
                                "target_lang_word": "зрители",
                                "target_lang_sense_clarification": "множественное число"
                            },
                            {
                                "target_lang_word": "аудитория",
                                "target_lang_sense_clarification": "целевая группа, слушатели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9beec405-de35-34c2-80c2-f1cd75a7e1e8",
                    "sense_definition": "Część sali teatralnej, kinowej lub koncertowej przeznaczona dla publiczności, wyposażona w miejsca siedzące.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Najlepsze miejsca na <w>widowni</w> znajdują się zazwyczaj w środkowych rzędach.",
                            "target_lang_translations": {
                                "ru": "Лучшие места в <w>зрительном зале</w> обычно находятся в средних рядах."
                            }
                        },
                        {
                            "text": "Gdy zgasły światła na <w>widowni</w>, kurtyna powoli poszła w górę.",
                            "target_lang_translations": {
                                "ru": "Когда погас свет в <w>зрительном зале</w>, занавес медленно поднялся."
                            }
                        }
                    ],
                    "synonyms": [
                        "audytorium"
                    ],
                    "common_phrases": [
                        "wejść na widownię",
                        "siedzieć na widowni",
                        "światła na widowni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть театрального, кино- или концертного зала, предназначенная для размещения публики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зрительный зал",
                                "target_lang_sense_clarification": "помещение"
                            },
                            {
                                "target_lang_word": "зал",
                                "target_lang_sense_clarification": "краткая форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce6db30e-1486-3b0d-b5cf-a17bd8afc240",
                    "sense_definition": "Miejsce lub tło, na którym rozgrywają się ważne wydarzenia publiczne, polityczne lub historyczne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po przegranych wyborach znany polityk zniknął nagle z <w>widowni</w> politycznej.",
                            "target_lang_translations": {
                                "ru": "После проигранных выборов известный политик внезапно исчез с политической <w>арены</w>."
                            }
                        },
                        {
                            "text": "Nowe mocarstwo wkroczyło pewnie na światową <w>widownię</w> dziejową.",
                            "target_lang_translations": {
                                "ru": "Новая держава уверенно вышла на мировую историческую <w>арену</w>."
                            }
                        }
                    ],
                    "synonyms": [

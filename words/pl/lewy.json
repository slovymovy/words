    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d47d9ead-f1e1-3239-85bf-b81b4a73e40f",
                    "sense_definition": "Znajdujący się po tej stronie ciała, po której zazwyczaj bije serce człowieka (przeciwieństwo strony prawej).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_orientation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W Polsce fotel kierowcy znajduje się zwykle po <lewej> stronie samochodu.",
                            "target_lang_translations": {
                                "ru": "В Польше водительское сиденье обычно находится с <левой> стороны автомобиля."
                            }
                        },
                        {
                            "text": "Boli mnie <lewa> ręka, ponieważ wczoraj dźwigałem ciężkie zakupy.",
                            "target_lang_translations": {
                                "ru": "У меня болит <левая> рука, потому что вчера я носил тяжелые покупки."
                            }
                        }
                    ],
                    "antonyms": [
                        "prawy"
                    ],
                    "common_phrases": [
                        "lewa strona",
                        "lewa ręka",
                        "lewy pas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся с той стороны тела, где обычно бьется сердце (противоположность правой стороне)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "левый",
                                "target_lang_sense_clarification": "о стороне или направлении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "daae666f-771c-3c0f-be9c-c1e7b330f98e",
                    "sense_definition": "O dokumentach, interesach lub pieniądzach: nielegalny, sfałszowany lub zdobyty w nieuczciwy sposób.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "illegality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam nadzieję, że policja nie zauważy, że mam <lewe> prawo jazdy.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, полиция не заметит, что у меня <левые> права."
                            }
                        },
                        {
                            "text": "Nie chcę brać udziału w żadnych <lewych> interesach, to zbyt ryzykowne.",
                            "target_lang_translations": {
                                "ru": "Я не хочу участвовать ни в каких <левых> делах, это слишком рискованно."
                            }
                        }
                    ],
                    "synonyms": [
                        "nielegalny",
                        "fałszywy"
                    ],
                    "common_phrases": [
                        "lewe interesy",
                        "lewe papiery",
                        "lewa kasa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О документах, делах или деньгах: незаконный, поддельный или добытый нечестным путем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "левый",
                                "target_lang_sense_clarification": "разг. незаконный или сомнительный"
                            },
                            {
                                "target_lang_word": "поддельный",
                                "target_lang_sense_clarification": "фальшивый"
                            },
                            {
                                "target_lang_word": "незаконный",
                                "target_lang_sense_clarification": "противоправный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6cfea13-ce33-3473-a81d-518e7f14e354",
                    "sense_definition": "Związany z lewicą, mający poglądy lewicowe (polityczne).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego poglądy są wyraźnie <lewe>, zawsze popiera programy socjalne.",
                            "target_lang_translations": {
                                "ru": "Его взгляды явно <левые>, он всегда поддерживает социальные программы."
                            }
                        },
                        {
                            "text": "To czasopismo reprezentuje <lewy> nurt w debacie publicznej.",
                            "target_lang_translations": {
                                "ru": "Этот журнал представляет <левое> течение в общественных дебатах."
                            }
                        }
                    ],
                    "synonyms": [
                        "lewicowy"
                    ],
                    "common_phrases": [
                        "lewe skrzydło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с левыми политическими взглядами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "левый",
                                "target_lang_sense_clarification": "политический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18af27d2-cf73-31b7-9a05-456fe43331bc",
                    "sense_definition": "Wewnętrzna, niewidoczna podczas noszenia strona tkaniny lub ubrania.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "spatial_orientation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez pośpiech założyłem koszulkę na <lewą> stronę i wszyscy się śmiali.",
                            "target_lang_translations": {
                                "ru": "В спешке я надел футболку на <изнаночную> сторону, и все смеялись."
                            }
                        },
                        {
                            "text": "Materiał na <lewej> stronie jest matowy i mniej przyjemny w dotyku.",
                            "target_lang_translations": {
                                "ru": "Материал с <изнаночной> стороны матовый и менее приятный на ощупь."
                            }
                        }
                    ],
                    "antonyms": [
                        "prawy"
                    ],
                    "common_phrases": [
                        "na lewą stronę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренняя, невидимая при носке сторона ткани или одежды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изнаночный",
                                "target_lang_sense_clarification": "о стороне ткани"
                            },
                            {
                                "target_lang_word": "левый",
                                "target_lang_sense_clarification": "разг. изнаночный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3fd5d99-a1a6-3d7d-848c-dff1b62e5624",
                    "sense_definition": "O człowieku: niepotrafiący niczego dobrze zrobić, niezdarny lub niekompetentny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "competence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie proś go o naprawę kranu, on jest kompletnie <lewy> do prac technicznych.",
                            "target_lang_translations": {
                                "ru": "Не проси его чинить кран, он совершенно <неумелый> в технических работах."
                            }
                        },
                        {
                            "text": "Jestem <lewy> z matematyki, więc potrzebuję korepetycji.",
                            "target_lang_translations": {
                                "ru": "Я <слаб> в математике, поэтому мне нужны репетиторы."
                            }
                        }
                    ],
                    "synonyms": [
                        "niezdarny"
                    ],
                    "common_phrases": [
                        "lewy do roboty"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке: не умеющий ничего делать хорошо, неуклюжий или некомпетентный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неумелый",
                                "target_lang_sense_clarification": "некомпетентный"
                            },
                            {
                                "target_lang_word": "безрукий",
                                "target_lang_sense_clarification": "разг. неуклюжий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7dccaa9c-fa31-3bfd-bf20-4f04e0016fe4",
                    "sense_definition": "Homoseksualny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sexuality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tamtych czasach środowisko <lewe> musiało się ukrywać przed władzą.",
                            "target_lang_translations": {
                                "ru": "В те времена <голубое> сообщество должно было скрываться от властей."
                            }
                        },
                        {
                            "text": "Użyli tego słowa w znaczeniu <lewym>, co obraziło mojego kolegę.",
                            "target_lang_translations": {
                                "ru": "Они использовали это слово в значении <голубой>, что обидело моего коллегу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often offensive or dated slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гомосексуальный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голубой",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "00271027-fec2-3a10-9911-46c92cb59a83",
                    "sense_definition": "Forma mianownika, biernika lub wołacza liczby mnogiej rzeczownika <w>lew</w> (waluta Bułgarii).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "currency_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W kantorze wymieniłem euro na <lewy>, aby mieć gotówkę na wakacje.",
                            "target_lang_translations": {
                                "ru": "В обменнике я поменял евро на <левы>, чтобы были наличные на отпуск."
                            }
                        },
                        {
                            "text": "Za ten obiad zapłaciliśmy tylko cztery <lewy>, co było bardzo tanio.",
                            "target_lang_translations": {
                                "ru": "За этот обед мы заплатили всего четыре <лева>, что было очень дешево."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural form of currency"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма множественного числа существительного «лев» (валюта Болгарии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "левы",
                                "target_lang_sense_clarification": "валюта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30e57f24-5ba2-34da-aada-0c9026de6a7a",
                    "sense_definition": "Forma dopełniacza liczby pojedynczej rzeczownika <w>lewa</w> (termin w grach karcianych oznaczający wzięcie kart).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "card_game_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Brakowało nam jednej <lewy>, aby wygrać całą partię brydża.",
                            "target_lang_translations": {
                                "ru": "Нам не хватало одной <взятки>, чтобы выиграть всю партию в бридж."
                            }
                        },
                        {
                            "text": "Do zdobycia tej <lewy> potrzebujesz asa lub króla.",
                            "target_lang_translations": {
                                "ru": "Для получения этой <взятки> тебе нужен туз или король."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection of noun 'lewa'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа единственного числа существительного «lewa» (термин в карточных играх, означающий взятку)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взятки",
                                "target_lang_sense_clarification": "в картах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
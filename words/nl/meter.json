            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ba72736a-1d3d-38a7-bd64-16ca1db51b2d",
                    "sense_definition": "De standaardeenheid voor lengte in het metrieke stelsel, gelijk aan honderd centimeter.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tafel in de keuken is precies twee <w>meter</w> lang.",
                            "target_lang_translations": {
                                "en": "The table in the kitchen is exactly two <w>meters</w> long.",
                                "ru": "Стол на кухне ровно два <w>метра</w> в длину."
                            }
                        },
                        {
                            "text": "Wij wonen op slechts honderd <w>meter</w> afstand van de school.",
                            "target_lang_translations": {
                                "en": "We live just a hundred <w>meters</w> away from the school.",
                                "ru": "Мы живем всего в ста <w>метрах</w> от школы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vierkante meter",
                        "kubieke meter",
                        "meter boven zeeniveau"
                    ],
                    "target_lang_definitions": {
                        "en": "The standard unit of length in the metric system, equal to one hundred centimeters.",
                        "ru": "Стандартная единица длины в метрической системе, равная ста сантиметрам."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "meter",
                                "target_lang_sense_clarification": "unit of length (US spelling)"
                            },
                            {
                                "target_lang_word": "metre",
                                "target_lang_sense_clarification": "unit of length (UK spelling)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "метр",
                                "target_lang_sense_clarification": "единица измерения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a258c290-b5fe-35c1-a884-2ed60157a862",
                    "sense_definition": "Een apparaat of instrument dat een hoeveelheid meet of registreert, zoals gas, water of elektriciteit.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De monteur kijkt naar de <w>meter</w> om het stroomverbruik te controleren.",
                            "target_lang_translations": {
                                "en": "The technician looks at the <w>meter</w> to check the power consumption.",
                                "ru": "Монтёр смотрит на <w>счётчик</w>, чтобы проверить потребление электроэнергии."
                            }
                        },
                        {
                            "text": "We hebben thuis een nieuwe slimme <w>meter</w> laten installeren.",
                            "target_lang_translations": {
                                "en": "We had a new smart <w>meter</w> installed at home.",
                                "ru": "Мы установили дома новый умный <w>счётчик</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "teller"
                    ],
                    "common_phrases": [
                        "slimme meter",
                        "op de meter kijken"
                    ],
                    "target_lang_definitions": {
                        "en": "A device or instrument that measures or records a quantity, such as gas, water, or electricity.",
                        "ru": "Прибор или инструмент, который измеряет или регистрирует количество чего-либо, например, газа, воды или электричества."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "meter",
                                "target_lang_sense_clarification": "measuring device"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "счётчик",
                                "target_lang_sense_clarification": "прибор"
                            },
                            {
                                "target_lang_word": "измеритель",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d093f45e-a723-331a-ad86-eb13a28bf12d",
                    "sense_definition": "Een vrouw die bij de doop van een kind belooft te helpen bij de opvoeding; peetmoeder.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is gevraagd om <w>meter</w> te zijn van haar pasgeboren nichtje.",
                            "target_lang_translations": {
                                "en": "She has been asked to be the <w>godmother</w> of her newborn niece.",
                                "ru": "Её попросили стать <w>крёстной</w> её новорожденной племянницы."
                            }
                        },
                        {
                            "text": "De <w>meter</w> en peter gaven het kind een prachtig geschenk bij de doop.",
                            "target_lang_translations": {
                                "en": "The <w>godmother</w> and godfather gave the child a beautiful gift at the baptism.",
                                "ru": "<w>Крёстная</w> и крёстный подарили ребенку прекрасный подарок на крестины."
                            }
                        }
                    ],
                    "synonyms": [
                        "peettante"
                    ],
                    "common_phrases": [
                        "meter en peter"
                    ],
                    "target_lang_definitions": {
                        "en": "A woman who promises at a child's baptism to help with the upbringing; godmother.",
                        "ru": "Женщина, которая при крещении ребенка обещает помогать в его воспитании; крестная мать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "godmother",
                                "target_lang_sense_clarification": "female godparent"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "крёстная",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "крёстная мать",
                                "target_lang_sense_clarification": "формальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f4b4c8e-fffc-3186-98b1-22fc1e4c9653",
                    "sense_definition": "Iemand die beroepsmatig metingen verricht, bijvoorbeeld van de waterstand of elektriciteit.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "person_job",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>meter</w> komt morgen langs om de standen op te nemen.",
                            "target_lang_translations": {
                                "en": "The <w>meter reader</w> is coming by tomorrow to record the readings.",
                                "ru": "<w>Контролёр</w> придет завтра, чтобы снять показания."
                            }
                        },
                        {
                            "text": "Vroeger ging de <w>meter</w> bij alle huizen langs de deuren.",
                            "target_lang_translations": {
                                "en": "In the past, the <w>meter reader</w> went door to door at all the houses.",
                                "ru": "Раньше <w>контролёр</w> обходил все дома."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who performs measurements professionally, for example of water levels or electricity.",
                        "ru": "Человек, профессионально занимающийся измерениями или снятием показаний приборов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "meter reader",
                                "target_lang_sense_clarification": "person who records utility usage"
                            },
                            {
                                "target_lang_word": "measurer",
                                "target_lang_sense_clarification": "person who measures"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "контролёр",
                                "target_lang_sense_clarification": "снимающий показания"
                            },
                            {
                                "target_lang_word": "учётчик",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                }
            ]

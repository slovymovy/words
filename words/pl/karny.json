        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "56790629-84c3-3257-bec4-40e0e5fb01fe",
                    "sense_definition": "Związany z prawem karnym, dotyczący sądzenia przestępstw i wymierzania za nie kar.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "law_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Adwokat specjalizuje się w prawie <w>karnym</w> i broni oskarżonych.",
                            "target_lang_translations": {
                                "ru": "Адвокат специализируется на <w>уголовном</w> праве и защищает обвиняемых."
                            }
                        },
                        {
                            "text": "Kodeks <w>karny</w> dokładnie określa wymiar kary za to przestępstwo.",
                            "target_lang_translations": {
                                "ru": "<w>Уголовный</w> кодекс точно определяет меру наказания за это преступление."
                            }
                        }
                    ],
                    "synonyms": [
                        "karnoprawny",
                        "prawnokarny"
                    ],
                    "common_phrases": [
                        "kodeks karny",
                        "prawo karne",
                        "proces karny",
                        "sąd karny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с уголовным правом, касающийся судебного преследования преступлений и назначения наказаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уголовный",
                                "target_lang_sense_clarification": "относящийся к преступлениям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97fd0269-a82d-3fcb-80cc-eda3d2c911d8",
                    "sense_definition": "Służący do karania, będący karą lub mający charakter kary.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "punishment_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze zostali wysłani do kompanii <w>karnej</w> za nieposłuszeństwo.",
                            "target_lang_translations": {
                                "ru": "Солдат отправили в <w>штрафную</w> роту за неповиновение."
                            }
                        },
                        {
                            "text": "Władze wysłały ekspedycję <w>karną</w>, aby stłumić bunt w regionie.",
                            "target_lang_translations": {
                                "ru": "Власти отправили <w>карательную</w> экспедицию, чтобы подавить бунт в регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "karzący",
                        "dyscyplinarny",
                        "penitencjarny"
                    ],
                    "common_phrases": [
                        "obóz karny",
                        "ekspedycja karna",
                        "kompania karna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служащий для наказания, являющийся наказанием или имеющий характер кары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карательный",
                                "target_lang_sense_clarification": "имеющий целью наказание"
                            },
                            {
                                "target_lang_word": "штрафной",
                                "target_lang_sense_clarification": "дисциплинарный (воен.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28a63656-468a-3b54-a73f-0f2e34b49549",
                    "sense_definition": "Przestrzegający dyscypliny, wykonujący polecenia, posłuszny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "obedience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To był bardzo <w>karny</w> oddział, który wykonywał rozkazy bez wahania.",
                            "target_lang_translations": {
                                "ru": "Это был очень <w>дисциплинированный</w> отряд, который выполнял приказы без колебаний."
                            }
                        },
                        {
                            "text": "Jego pies jest niezwykle <w>karny</w> i reaguje na każdą komendę.",
                            "target_lang_translations": {
                                "ru": "Его пёс чрезвычайно <w>послушный</w> и реагирует на каждую команду."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdyscyplinowany",
                        "posłuszny",
                        "uległy",
                        "subordynowany",
                        "pokorny",
                        "potulny"
                    ],
                    "antonyms": [
                        "niekarny",
                        "nieposłuszny",
                        "niezdyscyplinowany"
                    ],
                    "common_phrases": [
                        "karny żołnierz",
                        "karny zespół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соблюдающий дисциплину, выполняющий приказы, послушный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дисциплинированный",

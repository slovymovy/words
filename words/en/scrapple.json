                {
                    "sense_id": "befdaa22-9b6b-3c06-9f57-f20a99620348",
                    "sense_definition": "A tool used for scraping surfaces.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The worker used an iron <w>scrapple</w> to clean the mud off his boots.",

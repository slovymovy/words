    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2f850e8e-ab58-3236-bcb0-6dbe38839bd5",
                    "sense_definition": "The absolute or relative size, extent, or importance of something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We did not realize the true <w>magnitude</w> of the problem until it was too late to fix it."
                        },
                        {
                            "text": "The project is of such great <w>magnitude</w> that it will require funding from multiple countries."
                        }
                    ],
                    "synonyms": [
                        "size",
                        "extent",
                        "scale",
                        "importance",
                        "significance"
                    ],
                    "antonyms": [
                        "insignificance",
                        "smallness"
                    ],
                    "common_phrases": [
                        "order of magnitude",
                        "of great magnitude",
                        "first magnitude"
                    ]
                },
                {
                    "sense_id": "e666b6fe-1442-3227-a39a-05439376e26e",
                    "sense_definition": "A measure of the energy released by an earthquake.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The earthquake had a <w>magnitude</w> of seven on the Richter scale and caused widespread damage."
                        },
                        {
                            "text": "Scientists warned that aftershocks of a lower <w>magnitude</w> could continue for several days."
                        }
                    ],
                    "common_phrases": [
                        "Richter magnitude",
                        "moment magnitude",
                        "high magnitude"
                    ]
                },
                {
                    "sense_id": "c9819323-8765-37b0-ba74-f82ea5267988",
                    "sense_definition": "A logarithmic scale of brightness used to measure the apparent or absolute brightness of a celestial body.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sirius is the star with the brightest apparent <w>magnitude</w> visible from Earth."
                        },
                        {
                            "text": "Astronomers use absolute <w>magnitude</w> to compare the intrinsic brightness of stars at a standard distance."
                        }
                    ],
                    "common_phrases": [
                        "apparent magnitude",
                        "absolute magnitude"
                    ]
                },
                {
                    "sense_id": "e1387be0-8c1f-397e-9a46-d84588783230",
                    "sense_definition": "The length or numerical value of a vector or quantity, distinct from its direction.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In physics, velocity is a vector quantity that has both <w>magnitude</w> and direction."
                        },
                        {
                            "text": "You must calculate the <w>magnitude</w> of the force before determining its effect on the object."
                        }
                    ],
                    "synonyms": [
                        "norm",
                        "length"
                    ]
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4eea389f-5f10-34f1-a1ac-1407bf78993c",
                    "sense_definition": "W sposób bardzo ładny, kolorowy i estetyczny, przypominający wyglądem słodycze lub bajkowy świat, często z przewagą pastelowych barw.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "visual_aesthetic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej sypialnia została urządzona bardzo <w>cukierkowo</w>, z dużą ilością różu i miękkich dodatków.",
                            "target_lang_translations": {
                                "ru": "Её спальня была оформлена очень <w>конфетно</w>, с большим количеством розового и мягких аксессуаров."
                            }
                        },
                        {
                            "text": "Na zdjęciach z wakacji wszystko wyglądało <w>cukierkowo</w> i wręcz nierealnie pięknie.",
                            "target_lang_translations": {
                                "ru": "На фотографиях с отпуска всё выглядело <w>конфетно</w> и просто нереально красиво."
                            }
                        }
                    ],
                    "synonyms": [
                        "landrynkowo",
                        "bajkowo",
                        "słodko"
                    ],
                    "antonyms": [
                        "mrocznie",
                        "ponuro",
                        "surowo"
                    ],
                    "common_phrases": [
                        "wyglądać cukierkowo",
                        "urządzone cukierkowo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень мило, красочно и эстетично, напоминая видом сладости или сказочный мир, часто с преобладанием пастельных тонов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конфетно",
                                "target_lang_sense_clarification": "о стиле или внешнем виде: ярко, мило, как конфета"
                            },
                            {
                                "target_lang_word": "кукольно",
                                "target_lang_sense_clarification": "неестественно красиво, как в кукольном домике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "633ab017-faf1-35b7-a6b6-d5e139c325ab",
                    "sense_definition": "W sposób przesadnie miły, sentymentalny lub idealistyczny, często odbierany jako sztuczny lub kiczowaty.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_sentiment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Film zakończył się tak <w>cukierkowo</w>, że wielu widzów uznało to za banalne rozwiązanie.",
                            "target_lang_translations": {
                                "ru": "Фильм закончился так <w>слащаво</w>, что многие зрители сочли это банальным решением."
                            }
                        },
                        {
                            "text": "Nie lubię, gdy w powieściach jest zbyt <w>cukierkowo</w> i wszyscy bohaterowie są bez skazy.",
                            "target_lang_translations": {
                                "ru": "Я не люблю, когда в романах всё слишком <w>приторно</w>, а все герои безупречны."
                            }
                        }
                    ],
                    "synonyms": [
                        "ckliwie",
                        "kiczowato",
                        "sielankowo",
                        "przesłodzenie"
                    ],
                    "antonyms": [
                        "realistycznie",
                        "dramatycznie",
                        "szorstko"
                    ],
                    "common_phrases": [
                        "zbyt cukierkowo",
                        "zrobiło się cukierkowo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезмерно мило, сентиментально или идеалистично, часто воспринимается как искусственно или китчево."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слащаво",
                                "target_lang_sense_clarification": "излишне сентиментально, приторно-ласково"
                            },
                            {
                                "target_lang_word": "приторно",
                                "target_lang_sense_clarification": "чрезмерно сладко, до неприятного ощущения"
                            },
                            {
                                "target_lang_word": "ванильно",
                                "target_lang_sense_clarification": "наивно, романтично и банально (сленг)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
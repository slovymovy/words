    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e4e0d5f0-11c6-38dd-b409-d1abfd5c1db5",
                    "sense_definition": "Possessing great physical strength or power.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "strength_power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>mighty</w> warrior defeated all his enemies in battle.",
                            "target_lang_translations": {
                                "ru": "<w>Могучий</w> воин победил всех своих врагов в битве."
                            }
                        },
                        {
                            "text": "A <w>mighty</w> storm caused significant damage to the coastal town.",
                            "target_lang_translations": {
                                "ru": "<w>Мощный</w> шторм нанес значительный ущерб прибрежному городу."
                            }
                        }
                    ],
                    "synonyms": [
                        "strong",
                        "powerful",
                        "robust",
                        "potent"
                    ],
                    "antonyms": [
                        "weak",
                        "feeble",
                        "powerless"
                    ],
                    "common_phrases": [
                        "mighty warrior",
                        "mighty army",
                        "mighty king",
                        "mighty hand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий огромной физической силой или властью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "могучий",
                                "target_lang_sense_clarification": "обладающий большой физической силой (о людях, природе)"
                            },
                            {
                                "target_lang_word": "могущественный",
                                "target_lang_sense_clarification": "обладающий большой властью или влиянием"
                            },
                            {
                                "target_lang_word": "мощный",
                                "target_lang_sense_clarification": "обладающий большой силой или энергией"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7432a7ed-aa05-39e7-95cc-a6fd6dca4bee",
                    "sense_definition": "Of great size, extent, or importance; imposing.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "size_magnitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We stood in silence looking at the <w>mighty</w> mountain range ahead of us.",
                            "target_lang_translations": {
                                "ru": "Мы стояли молча, глядя на <w>величественную</w> горную цепь впереди нас."
                            }
                        },
                        {
                            "text": "The <w>mighty</w> river flows through three different countries before reaching the sea.",
                            "target_lang_translations": {
                                "ru": "<w>Могучая</w> река протекает через три разные страны, прежде чем достичь моря."
                            }
                        }
                    ],
                    "synonyms": [
                        "huge",
                        "enormous",
                        "vast",
                        "immense",
                        "colossal"
                    ],
                    "antonyms": [
                        "tiny",
                        "small",
                        "insignificant"
                    ],

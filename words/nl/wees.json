                            {
                                "target_lang_word": "будьте",
                                "target_lang_sense_clarification": "повелительное наклонение (мн.ч. или вежливая форма)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a446ccf-14cb-38cc-87cb-a283e0c1c2c8",
                    "sense_definition": "Enkelvoud verleden tijd van <w>wijzen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "past_wijzen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar wees de leerling op zijn fouten in de tekst.",
                            "target_lang_translations": {
                                "en": "The teacher <w>pointed</w> out the student's errors in the text.",
                                "ru": "Учитель <w>указал</w> ученику на его ошибки в тексте."
                            }
                        },
                        {
                            "text": "Ze wees naar de prachtige zonsondergang aan de horizon.",
                            "target_lang_translations": {
                                "en": "She <w>pointed</w> at the beautiful sunset on the horizon.",
                                "ru": "Она <w>показала</w> на красивый закат на горизонте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wees de weg",
                        "wees naar",
                        "wees af"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Singular past tense of <w>to point</w>.",
                        "ru": "Форма прошедшего времени единственного числа глагола <w>wijzen</w> (указывать)."
                    },
                    "translations": {
                        "en": [

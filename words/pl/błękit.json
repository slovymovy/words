{
    "word_family": [
        "błękitny",
        "błękitnieć",
        "błękitnawy",
        "błękitność"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eeafe531-3081-303f-a9ec-c59b3e217004",
                    "sense_definition": "Jasny, intensywny odcień koloru niebieskiego, przypominający barwę czystego nieba w słoneczny dzień.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "color_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nad naszymi głowami rozciągał się nieskończony <w>błękit</w>.",
                            "target_lang_translations": {
                                "ru": "Над нашими головами простиралась бесконечная <w>лазурь</w>."
                            }
                        },
                        {
                            "text": "Jej oczy mają niesamowity, głęboki <w>błękit</w>.",
                            "target_lang_translations": {
                                "ru": "У её глаз невероятная, глубокая <w>голубизна</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lazur",
                        "błękitność"
                    ],
                    "antonyms": [
                        "pomarańcz"
                    ],
                    "common_phrases": [
                        "błękit nieba",
                        "morski błękit",
                        "głęboki błękit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Светлый, насыщенный оттенок синего цвета, напоминающий цвет чистого неба в солнечный день."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голубизна",
                                "target_lang_sense_clarification": "свойство цвета"
                            },
                            {
                                "target_lang_word": "лазурь",
                                "target_lang_sense_clarification": "оттенок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95123689-81c6-38a3-a72f-573ef6f5028f",
                    "sense_definition": "Bezchmurne niebo lub przestrzeń powietrzna nad ziemią.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sky_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ptak wzbił się wysoko w <w>błękit</w> i zniknął nam z oczu.",
                            "target_lang_translations": {
                                "ru": "Птица взмыла высоко в <w>синеву</w> и исчезла из виду."
                            }
                        },
                        {
                            "text": "Leżeliśmy na trawie, patrząc w pogodny <w>błękit</w>.",
                            "target_lang_translations": {
                                "ru": "Мы лежали на траве, глядя в ясную <w>лазурь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "niebo"
                    ],
                    "common_phrases": [
                        "wpatrzony w błękit",
                        "lecieć w błękit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Безоблачное небо или воздушное пространство над землей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "синева",
                                "target_lang_sense_clarification": "небосвод"
                            },
                            {
                                "target_lang_word": "лазурь",
                                "target_lang_sense_clarification": "поэтически о небе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c54eedad-651d-3758-9ed1-f2d06af45b40",
                    "sense_definition": "Substancja barwiąca, pigment lub farba o niebieskim kolorze.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "pigment_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Malarz zmieszał <w>błękit</w> z bielą, aby uzyskać jaśniejszy kolor.",
                            "target_lang_translations": {
                                "ru": "Художник смешал <w>лазурь</w> с белилами, чтобы получить более светлый цвет."
                            }
                        },
                        {
                            "text": "Ten chemiczny <w>błękit</w> jest bardzo trwały i odporny na światło.",
                            "target_lang_translations": {
                                "ru": "Этот химический <w>синий пигмент</w> очень стоек и устойчив к свету."
                            }
                        }
                    ],
                    "synonyms": [
                        "lazur"
                    ],
                    "common_phrases": [
                        "błękit pruski",
                        "błękit paryski",
                        "błękit metylenowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Красящее вещество, пигмент или краска синего цвета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лазурь",
                                "target_lang_sense_clarification": "пигмент"
                            },
                            {
                                "target_lang_word": "синька",
                                "target_lang_sense_clarification": "красящее вещество"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
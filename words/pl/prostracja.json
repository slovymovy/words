    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1087a0a3-5179-3a60-94c3-5ec2b7c119de",
                    "sense_definition": "Stan skrajnego wyczerpania psychicznego lub fizycznego, charakteryzujący się brakiem sił, bezwładem i obojętnością na bodźce.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielu tygodniach intensywnego stresu pacjent popadł w stan całkowitej <w>prostracji</w>.",
                            "target_lang_translations": {
                                "ru": "После многих недель сильного стресса пациент впал в состояние полной <w>прострации</w>."
                            }
                        },
                        {
                            "text": "Lekarz stwierdził u niej nerwową <w>prostrację</w>, zalecając długi odpoczynek i terapię.",
                            "target_lang_translations": {
                                "ru": "Врач диагностировал у неё нервную <w>прострацию</w>, порекомендовав длительный отдых и терапию."
                            }
                        }
                    ],
                    "synonyms": [
                        "wycieńczenie",
                        "wyczerpanie psychiczne"
                    ],
                    "common_phrases": [
                        "stan prostracji",
                        "nerwowa prostracja",
                        "głęboka prostracja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние крайнего психического или физического истощения, характеризующееся упадком сил, безволием и безразличием к внешним раздражителям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прострация",
                                "target_lang_sense_clarification": "состояние полной безучастности"
                            },
                            {
                                "target_lang_word": "изнеможение",
                                "target_lang_sense_clarification": "крайняя усталость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f691ec3-1d21-3755-941c-f405ff7058fb",
                    "sense_definition": "Leżenie krzyżem lub padanie na twarz jako gest uniżenia, pokuty lub głębokiej czci religijnej.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas uroczystości święceń kandydaci trwali w <w>prostracji</w> przed ołtarzem.",
                            "target_lang_translations": {
                                "ru": "Во время церемонии рукоположения кандидаты находились в <w>прострации</w> перед алтарем."
                            }
                        },
                        {
                            "text": "Wierni wykonali gest <w>prostracji</w>, aby wyrazić swoją pokorę wobec Boga.",
                            "target_lang_translations": {
                                "ru": "Верующие совершили жест <w>прострации</w>, чтобы выразить свое смирение перед Богом."
                            }
                        }
                    ],
                    "synonyms": [
                        "leżenie krzyżem",
                        "padanie na twarz"
                    ],
                    "common_phrases": [
                        "gest prostracji",
                        "trwać w prostracji",
                        "akt prostracji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лежание ниц (распростершись на земле) как жест смирения, покаяния или глубокого религиозного почитания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прострация",

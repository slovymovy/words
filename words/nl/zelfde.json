{
    "word_family": [
        "zelf",
        "hetzelfde",
        "dezelfde",
        "diezelfde",
        "datzelfde",
        "eenzelfde"
    ],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "153fd1ed-b7c6-3238-9d8f-506a44186c5e",
                    "sense_definition": "Identiek; niet verschillend van iets of iemand anders; precies dit of dat exemplaar.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij wonen al jaren in de <w>zelfde</w> straat.",
                            "target_lang_translations": {
                                "en": "We have lived in the <w>same</w> street for years.",
                                "ru": "Мы уже много лет живем на <w>одной и той же</w> улице."
                            }
                        },
                        {
                            "text": "Zij dragen vandaag precies de <w>zelfde</w> kleren.",
                            "target_lang_translations": {
                                "en": "They are wearing exactly the <w>same</w> clothes today.",
                                "ru": "Сегодня на них точно <w>такая же</w> одежда."
                            }
                        }
                    ],
                    "synonyms": [
                        "identiek",
                        "eender"
                    ],
                    "antonyms": [
                        "ander",
                        "verschillend"
                    ],
                    "common_phrases": [
                        "dezelfde dag",
                        "op hetzelfde moment",
                        "in dezelfde situatie",
                        "hetzelfde probleem"
                    ],
                    "target_lang_definitions": {
                        "en": "Identical; not different from something or someone else; exactly this or that instance.",
                        "ru": "Идентичный; не отличающийся от чего-либо или кого-либо другого; именно этот экземпляр."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "same",
                                "target_lang_sense_clarification": "identical, not different"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тот же самый",
                                "target_lang_sense_clarification": "идентичный, именно этот"
                            },
                            {
                                "target_lang_word": "один и тот же",
                                "target_lang_sense_clarification": "без различий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17884c4b-6e76-36db-a250-d6539029c6da",
                    "sense_definition": "Van een gelijke soort of aard; vergelijkbaar maar niet noodzakelijk één en hetzelfde exemplaar.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "similarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben op zoek naar een <w>zelfde</w> soort tafel voor mijn woonkamer.",
                            "target_lang_translations": {
                                "en": "I am looking for a <w>similar</w> kind of table for my living room.",
                                "ru": "Я ищу стол <w>такого же</w> типа для своей гостиной."
                            }
                        },
                        {
                            "text": "Hij vertoonde een <w>zelfde</w> reactie toen hij het nieuws hoorde.",
                            "target_lang_translations": {
                                "en": "He showed a <w>similar</w> reaction when he heard the news.",
                                "ru": "Он продемонстрировал <w>такую же</w> реакцию, когда услышал новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "soortgelijk",
                        "dergelijk"
                    ],
                    "common_phrases": [
                        "een zelfde soort",
                        "een zelfde type"
                    ],
                    "target_lang_definitions": {
                        "en": "Of a similar kind or nature; comparable but not necessarily one and the same instance.",
                        "ru": "Того же рода или типа; похожий, но не обязательно тот же самый экземпляр."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "similar",
                                "target_lang_sense_clarification": "alike in kind or nature"
                            },
                            {
                                "target_lang_word": "same",
                                "target_lang_sense_clarification": "of the identical type"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "такой же",
                                "target_lang_sense_clarification": "одинаковый по свойствам"
                            },
                            {
                                "target_lang_word": "подобный",
                                "target_lang_sense_clarification": "схожий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "75314be4-6680-32ce-8835-66203d36eb52",
                    "sense_definition": "Verwijst nadrukkelijk naar een eerder genoemd persoon, zaak of eigenschap om identiteit aan te duiden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil graag zo'n <w>zelfde</w> fiets als mijn broer heeft.",
                            "target_lang_translations": {
                                "en": "I would like the <w>same</w> bike as my brother has.",
                                "ru": "Я хочу <w>такой же</w> велосипед, как у моего брата."
                            }
                        },
                        {
                            "text": "Deze <w>zelfde</w> woorden werden vorig jaar ook al uitgesproken.",
                            "target_lang_translations": {
                                "en": "These <w>same</w> words were spoken last year as well.",
                                "ru": "Эти <w>же самые</w> слова были произнесены и в прошлом году."
                            }
                        }
                    ],
                    "common_phrases": [
                        "die zelfde",
                        "dat zelfde"
                    ],
                    "target_lang_definitions": {
                        "en": "Refers emphatically to a previously mentioned person, thing, or property to indicate identity.",
                        "ru": "Ссылается на ранее упомянутое лицо, предмет или свойство для указания тождества."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "same",
                                "target_lang_sense_clarification": "referring to the specific one mentioned"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "такой же",
                                "target_lang_sense_clarification": "аналогичный упомянутому"
                            },
                            {
                                "target_lang_word": "тот же",
                                "target_lang_sense_clarification": "упомянутый ранее"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
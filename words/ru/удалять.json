    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f62d5a85-a566-3c7a-aadc-eeefa6a90953",
                    "sense_definition": "Уничтожать или стирать информацию, файлы или записи (обычно в цифровом контексте).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "elimination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я всегда <удаляю> старые сообщения, чтобы освободить память телефона.",
                            "target_lang_translations": {
                                "en": "I always <delete> old messages to free up phone memory."
                            }
                        },
                        {
                            "text": "Системный администратор <удаляет> подозрительные файлы с сервера.",
                            "target_lang_translations": {
                                "en": "The system administrator <deletes> suspicious files from the server."
                            }
                        }
                    ],
                    "synonyms": [
                        "стирать",
                        "уничтожать"
                    ],
                    "antonyms": [
                        "добавлять"
                    ],
                    "common_phrases": [
                        "удалять файл",
                        "удалять сообщение",
                        "удалять аккаунт",
                        "удалять программу"
                    ],
                    "target_lang_definitions": {
                        "en": "To destroy or erase information, files, or records (usually in a digital context)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "delete",
                                "target_lang_sense_clarification": "files, data, accounts"
                            },
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "erase",
                                "target_lang_sense_clarification": "data, memory"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0612092b-ecad-321a-9612-779335bac8d8",
                    "sense_definition": "Изымать, вынимать или устранять физический объект, пятно или часть чего-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "elimination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Стоматолог сказал, что придётся <удалять> больной зуб.",
                            "target_lang_translations": {
                                "en": "The dentist said that the bad tooth would have to be <extracted>."
                            }
                        },
                        {
                            "text": "Это химическое средство эффективно <удаляет> жирные пятна с одежды.",
                            "target_lang_translations": {
                                "en": "This chemical agent effectively <removes> grease stains from clothes."
                            }
                        }
                    ],
                    "synonyms": [
                        "устранять",
                        "вынимать",
                        "изымать"
                    ],
                    "antonyms": [
                        "добавлять"
                    ],
                    "common_phrases": [
                        "удалять зуб",
                        "удалять пятно",
                        "удалять макияж",
                        "удалять сорняки"
                    ],
                    "target_lang_definitions": {
                        "en": "To take out, extract, or eliminate a physical object, stain, or part of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "general physical removal"
                            },
                            {
                                "target_lang_word": "extract",
                                "target_lang_sense_clarification": "teeth, splinters"
                            },
                            {
                                "target_lang_word": "eliminate",
                                "target_lang_sense_clarification": "stains, defects"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cac0de4-7013-33b8-b47e-4a7de341c642",
                    "sense_definition": "Заставлять кого-либо покинуть место или отстранять от участия в чём-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "exclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Судья часто <удаляет> игроков с поля за грубые нарушения правил.",
                            "target_lang_translations": {
                                "en": "The referee often <sends> players <off> the field for gross violations of the rules."
                            }
                        },
                        {
                            "text": "Охрана вежливо <удаляет> шумных посетителей из концертного зала.",
                            "target_lang_translations": {
                                "en": "Security politely <removes> noisy visitors from the concert hall."
                            }
                        }
                    ],
                    "synonyms": [
                        "выдворять",
                        "уводить"
                    ],
                    "antonyms": [
                        "приглашать"
                    ],
                    "common_phrases": [
                        "удалять с поля",
                        "удалять из зала",
                        "удалять из друзей"
                    ],
                    "target_lang_definitions": {
                        "en": "To force someone to leave a place or suspend them from participation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "from a place or group"
                            },
                            {
                                "target_lang_word": "send off",
                                "target_lang_sense_clarification": "sports context"
                            },
                            {
                                "target_lang_word": "eject",
                                "target_lang_sense_clarification": "forceful removal"
                            },
                            {
                                "target_lang_word": "send away",
                                "target_lang_sense_clarification": "dismiss someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0917c5dd-380c-3906-b839-1a58802b930d",
                    "sense_definition": "Перемещать на большее расстояние, делать более далёким.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "distance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Быстрое течение реки <удаляет> лодку от берега.",
                            "target_lang_translations": {
                                "en": "The river's rapid current <moves> the boat <away> from the shore."
                            }
                        },
                        {
                            "text": "Художник намеренно <удаляет> задний план, используя холодные тона.",
                            "target_lang_translations": {
                                "en": "The artist intentionally <distances> the background using cold tones."
                            }
                        }
                    ],
                    "synonyms": [
                        "отдалять"
                    ],
                    "antonyms": [
                        "приближать"
                    ],
                    "target_lang_definitions": {
                        "en": "To move something to a greater distance or make it appear further away."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move away",
                                "target_lang_sense_clarification": "increase distance"
                            },
                            {
                                "target_lang_word": "distance",
                                "target_lang_sense_clarification": "make distant"
                            }
                        ]
                    }
                }
            ]
        }

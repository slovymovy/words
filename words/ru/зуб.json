    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f218753c-78f1-3e05-ae6d-0c31a749bac0",
                    "sense_definition": "Костное образование в ротовой полости, служащее для откусывания и разжёвывания пищи",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждое утро и вечер я иду в ванную чистить <w>зубы</w>.",
                            "target_lang_translations": {
                                "en": "Every morning and evening I go to the bathroom to brush my <w>teeth</w>."
                            }
                        },
                        {
                            "text": "У маленького ребёнка вчера выпал первый молочный <w>зуб</w>.",
                            "target_lang_translations": {
                                "en": "The small child's first baby <w>tooth</w> fell out yesterday."
                            }
                        }
                    ],
                    "common_phrases": [
                        "чистить зубы",
                        "лечить зубы",
                        "болит зуб",
                        "молочный зуб",
                        "коренной зуб"
                    ],
                    "target_lang_definitions": {
                        "en": "A hard, bony structure in the mouth used for biting and chewing food"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tooth",
                                "target_lang_sense_clarification": "anatomical"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f911f77e-82ff-339e-8c3f-1d440b939fbc",
                    "sense_definition": "Острый выступ на инструменте, орудии или детали машины",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старая пила затупилась, и её <w>зубья</w> больше не режут дерево.",
                            "target_lang_translations": {
                                "en": "The old saw has become blunt, and its <w>teeth</w> no longer cut wood."
                            }
                        },
                        {
                            "text": "На большом колесе механизма сломался один железный <w>зуб</w>.",
                            "target_lang_translations": {
                                "en": "One iron <w>tooth</w> broke on the mechanism's large wheel."
                            }
                        }
                    ],
                    "synonyms": [
                        "зубец"
                    ],
                    "common_phrases": [
                        "зубья пилы",
                        "зубья шестерни",
                        "зубья вилки",
                        "зубья расчёски"
                    ],
                    "target_lang_definitions": {
                        "en": "A sharp projection on a tool, instrument, or machine part"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tooth",
                                "target_lang_sense_clarification": "of a saw, comb, or gear"
                            },
                            {
                                "target_lang_word": "cog",
                                "target_lang_sense_clarification": "of a gearwheel"
                            },
                            {
                                "target_lang_word": "tine",
                                "target_lang_sense_clarification": "of a fork"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ff9fe22-b7af-3f57-ba54-163b5d819b88",
                    "sense_definition": "Чувство неприязни, затаённая обида или желание причинить вред",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой коллега давно имеет на меня <w>зуб</w> из-за прошлого конфликта.",
                            "target_lang_translations": {
                                "en": "My colleague has held a <w>grudge</w> against me for a long time because of a past conflict."
                            }
                        },
                        {
                            "text": "Не стоит точить <w>зуб</w> на старых друзей из-за мелких ссор.",
                            "target_lang_translations": {
                                "en": "It is not worth bearing a <w>grudge</w> against old friends because of petty quarrels."
                            }
                        }
                    ],
                    "synonyms": [
                        "неприязнь"
                    ],
                    "antonyms": [
                        "симпатия"
                    ],
                    "common_phrases": [
                        "иметь зуб",
                        "точить зуб"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in idioms"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A feeling of resentment or ill will resulting from a past insult or injury"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grudge",
                                "target_lang_sense_clarification": "resentment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8ca48e3-87ae-3ffe-a270-23479f37117f",
                    "sense_definition": "Колючее образование или шип на кожных покровах некоторых животных и рыб",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На спине этой редкой рыбы виден острый защитный <w>зуб</w>.",
                            "target_lang_translations": {
                                "en": "A sharp protective <w>spine</w> is visible on the back of this rare fish."
                            }
                        },
                        {
                            "text": "Твёрдый <w>нарост</w> на панцире животного напоминает шип.",
                            "target_lang_translations": {
                                "en": "The hard <w>growth</w> on the animal's shell resembles a spike."
                            }
                        }
                    ],
                    "synonyms": [
                        "нарост"
                    ],
                    "target_lang_definitions": {
                        "en": "A sharp, tooth-like projection or spine on the skin of certain animals"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spine",
                                "target_lang_sense_clarification": "sharp projection on an animal"
                            },
                            {
                                "target_lang_word": "denticle",
                                "target_lang_sense_clarification": "small tooth-like scale"
                            }
                        ]
                    }
                }
            ]
        }
    ]

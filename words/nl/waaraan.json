    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "58a4868f-04d2-3bee-a653-69b0b13ea7f5",
                    "sense_definition": "Vragend woord dat 'aan wat' of 'aan welk ding' betekent.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "interrogative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Waaraan</w> heb ik dit mooie cadeau te danken?",
                            "target_lang_translations": {
                                "ru": "<w>Чему</w> я обязан этим прекрасным подарком?"
                            }
                        },
                        {
                            "text": "Ik vraag me af <w>waaraan</w> hij nu zit te denken.",
                            "target_lang_translations": {
                                "ru": "Интересно, <w>о чём</w> он сейчас думает."
                            }
                        }
                    ],
                    "synonyms": [
                        "aan wat"
                    ],
                    "common_phrases": [
                        "waaraan denken",
                        "waaraan herkennen",
                        "waaraan liggen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вопросительное слово, заменяющее сочетание предлога 'aan' (к, о, над, у) и местоимения 'wat' (что)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чему",
                                "target_lang_sense_clarification": "в вопросах о причине или соответствии (напр. быть обязанным чему-то)"
                            },
                            {
                                "target_lang_word": "о чём",
                                "target_lang_sense_clarification": "в вопросах о мыслях (напр. думать о чём-то)"
                            },
                            {
                                "target_lang_word": "над чем",
                                "target_lang_sense_clarification": "в вопросах о работе или действии"
                            },
                            {
                                "target_lang_word": "по чему",
                                "target_lang_sense_clarification": "в вопросах об узнавании (напр. узнать по чему-то)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77591b2e-35e6-3664-ac90-9381a586109a",
                    "sense_definition": "Verwijst terug naar een eerder genoemd ding of situatie in combinatie met het voorzetsel 'aan'.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is het project <w>waaraan</w> we de afgelopen maanden hebben gewerkt.",
                            "target_lang_translations": {
                                "ru": "Это проект, <w>над которым</w> мы работали последние месяцы."
                            }
                        },
                        {
                            "text": "Er zijn strenge eisen <w>waaraan</w> het nieuwe gebouw moet voldoen.",
                            "target_lang_translations": {
                                "ru": "Существуют строгие требования, <w>которым</w> должно соответствовать новое здание."
                            }
                        }
                    ],
                    "synonyms": [
                        "aan hetwelk"
                    ],
                    "common_phrases": [
                        "de eisen waaraan",
                        "het doel waaraan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союзное слово, которое связывает придаточное предложение с существительным, заменяя предлог 'aan' + 'die/dat'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "над которым",
                                "target_lang_sense_clarification": "ссылка на объект работы или действия"
                            },
                            {
                                "target_lang_word": "которому",
                                "target_lang_sense_clarification": "ссылка на объект соответствия или подчинения"
                            },
                            {
                                "target_lang_word": "о котором",
                                "target_lang_sense_clarification": "ссылка на объект мысли"
                            }
                        ]
                    }
                }
            ]
        }
    ]

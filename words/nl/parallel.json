{
    "word_family": [
        "parallellisme",
        "parallellogram"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "14cbd800-757b-3ce7-82ed-f7ba31f5a3d7",
                    "sense_definition": "Zich in dezelfde richting uitstrekkend en overal op gelijke afstand van elkaar blijvend, zonder elkaar te snijden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De spoorrails lopen <w>parallel</w> aan de snelweg.",
                            "target_lang_translations": {
                                "ru": "Железнодорожные пути идут <w>параллельно</w> шоссе."
                            }
                        },
                        {
                            "text": "Teken een lijn die <w>parallel</w> is aan de onderkant van het papier.",
                            "target_lang_translations": {
                                "ru": "Нарисуй линию, <w>параллельную</w> нижнему краю бумаги."
                            }
                        }
                    ],
                    "synonyms": [
                        "evenwijdig"
                    ],
                    "common_phrases": [
                        "parallel lopen",
                        "parallel aan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простирающийся в том же направлении и остающийся на равном расстоянии, не пересекаясь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "параллельный",
                                "target_lang_sense_clarification": "геометрический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31ab9359-1ac4-3f75-9583-bcd20a4c2d7d",
                    "sense_definition": "Tegelijkertijd plaatsvindend of naast elkaar bestaand zonder elkaar direct te beïnvloeden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "simultaneous",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er vinden twee <w>parallelle</w> sessies plaats tijdens de conferentie.",
                            "target_lang_translations": {
                                "ru": "Во время конференции проходят две <w>параллельные</w> сессии."
                            }
                        },
                        {
                            "text": "De computer voert meerdere <w>parallelle</w> processen uit om tijd te besparen.",
                            "target_lang_translations": {
                                "ru": "Компьютер выполняет несколько <w>параллельных</w> процессов для экономии времени."
                            }
                        }
                    ],
                    "common_phrases": [
                        "parallelle import",
                        "parallelle samenleving"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий одновременно или существующий рядом, не влияя друг на друга напрямую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "параллельный",
                                "target_lang_sense_clarification": "одновременный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "017e9f46-9e42-3cd4-9b3e-cd8c8619c130",
                    "sense_definition": "Grote gelijkenis of overeenkomst vertonend met iets anders.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "comparable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We zien een <w>parallelle</w> ontwikkeling in de economie van beide landen.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдаем <w>параллельное</w> развитие в экономике обеих стран."
                            }
                        },
                        {
                            "text": "Deze situatie is <w>parallel</w> aan de gebeurtenissen van vorig jaar.",
                            "target_lang_translations": {
                                "ru": "Эта ситуация <w>аналогична</w> событиям прошлого года."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий большое сходство или соответствие с чем-то другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аналогичный",
                                "target_lang_sense_clarification": "схожий"
                            },
                            {
                                "target_lang_word": "параллельный",
                                "target_lang_sense_clarification": "сопутствующий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {

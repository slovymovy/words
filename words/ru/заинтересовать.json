    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "96e4b66d-4403-34f4-abb4-f10dbbc1fe1d",
                    "sense_definition": "Вызвать у кого-либо чувство интереса, любопытства или желание узнать что-то новое.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта необычная история может <заинтересовать> даже самого скучающего читателя.",
                            "target_lang_translations": {
                                "en": "This unusual story can <interest> even the most bored reader."
                            }
                        },
                        {
                            "text": "Учитель старался <заинтересовать> учеников своим предметом с помощью игр.",
                            "target_lang_translations": {
                                "en": "The teacher tried to <interest> the students in his subject with the help of games."
                            }
                        }
                    ],
                    "synonyms": [
                        "заинтриговать",
                        "увлечь",
                        "привлечь"
                    ],
                    "common_phrases": [
                        "заинтересовать слушателей",
                        "заинтересовать идеей",
                        "сильно заинтересовать"
                    ],
                    "target_lang_definitions": {
                        "en": "To arouse interest, curiosity, or a desire to learn something new in someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "interest",
                                "target_lang_sense_clarification": "to attract someone's attention or curiosity"
                            },
                            {
                                "target_lang_word": "intrigue",
                                "target_lang_sense_clarification": "to arouse strong curiosity or fascination"
                            },
                            {
                                "target_lang_word": "engage",
                                "target_lang_sense_clarification": "to capture attention or involve someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8420ccf9-9550-3660-a2ca-522010abf655",
                    "sense_definition": "Склонить кого-либо к участию в чём-либо, предложив материальную выгоду или иное вознаграждение.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Руководство решило материально <заинтересовать> сотрудников, чтобы повысить продажи.",
                            "target_lang_translations": {
                                "en": "Management decided to financially <incentivize> the employees in order to increase sales."
                            }
                        },
                        {
                            "text": "Нам нужно <заинтересовать> инвесторов, чтобы получить деньги на развитие проекта.",
                            "target_lang_translations": {
                                "en": "We need to <interest> investors to get money for the project development."
                            }
                        }
                    ],
                    "synonyms": [
                        "мотивировать",
                        "стимулировать"
                    ],
                    "common_phrases": [
                        "материально заинтересовать",
                        "заинтересовать клиента",
                        "лично заинтересовать"
                    ],
                    "target_lang_definitions": {
                        "en": "To persuade someone to participate in something by offering material gain or other rewards."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "incentivize",
                                "target_lang_sense_clarification": "to provide a motivation or reward"
                            },
                            {
                                "target_lang_word": "motivate",
                                "target_lang_sense_clarification": "to give a reason or incentive to do something"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "upper",
        "uppermost",
        "upward",
        "upwards"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "aa9d5909-2cd6-38a3-b2d0-51c657a2195e",
                    "sense_definition": "Towards a higher place or position; away from the ground.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please look <w>up</w> at the screen.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, посмотрите <w>вверх</w> на экран."
                            }
                        },
                        {
                            "text": "The balloon flew <w>up</w> into the sky.",
                            "target_lang_translations": {
                                "ru": "Воздушный шар улетел <w>вверх</w> в небо."
                            }
                        }
                    ],
                    "antonyms": [
                        "down"
                    ],
                    "common_phrases": [
                        "look up",
                        "go up",
                        "come up"
                    ],
                    "target_lang_definitions": {
                        "ru": "В более высокое место или положение; прочь от земли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вверх",
                                "target_lang_sense_clarification": "направление движения"
                            },
                            {
                                "target_lang_word": "наверх",
                                "target_lang_sense_clarification": "направление или место назначения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f666972f-5da1-30bf-8a56-c0c0d6e992b9",
                    "sense_definition": "Into an upright or vertical position.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "posture_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should stand <w>up</w> when the anthem plays.",
                            "target_lang_translations": {
                                "ru": "Тебе следует <w>встать</w>, когда играет гимн."
                            }
                        },
                        {
                            "text": "The baby can sit <w>up</w> by herself now.",
                            "target_lang_translations": {
                                "ru": "Ребенок теперь может <w>сидеть</w> самостоятельно."
                            }
                        }
                    ],
                    "antonyms": [
                        "down"
                    ],
                    "common_phrases": [
                        "stand up",
                        "sit up",
                        "get up"
                    ],
                    "target_lang_definitions": {
                        "ru": "В вертикальное положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встать",
                                "target_lang_sense_clarification": "принять вертикальное положение (обычно в составе глагола)"
                            },
                            {
                                "target_lang_word": "вертикально",
                                "target_lang_sense_clarification": "положение тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d019bf47-57fe-3e07-8e79-6edcc57a0380",
                    "sense_definition": "To a higher level of quantity, volume, value, or intensity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please turn the music <w>up</w> so we can dance.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, сделай музыку <w>погромче</w>, чтобы мы могли потанцевать."
                            }
                        },
                        {
                            "text": "The price of food has gone <w>up</w> recently.",
                            "target_lang_translations": {
                                "ru": "Цены на еду недавно пошли <w>вверх</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "down"
                    ],
                    "common_phrases": [
                        "go up",
                        "turn up",
                        "speak up"
                    ],
                    "target_lang_definitions": {
                        "ru": "К более высокому уровню количества, громкости, стоимости или интенсивности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "громче",
                                "target_lang_sense_clarification": "о звуке"
                            },
                            {
                                "target_lang_word": "выше",
                                "target_lang_sense_clarification": "о цене или уровне"
                            },
                            {
                                "target_lang_word": "вверх",
                                "target_lang_sense_clarification": "о росте показателей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01534970-793b-3659-bf62-5c09ab803950",
                    "sense_definition": "Towards a person or thing; approaching close to something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "approach_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A stranger walked <w>up</w> to me and asked for the time.",
                            "target_lang_translations": {
                                "ru": "Незнакомец <w>подошёл</w> ко мне и спросил время."
                            }
                        },
                        {
                            "text": "She ran <w>up</w> to her father and hugged him.",
                            "target_lang_translations": {
                                "ru": "Она <w>подбежала</w> к отцу и обняла его."
                            }
                        }
                    ],
                    "common_phrases": [
                        "walk up to",
                        "come up to",
                        "run up to"
                    ],
                    "target_lang_definitions": {
                        "ru": "По направлению к кому-либо или чему-либо; приближаясь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подо-",
                                "target_lang_sense_clarification": "приставка, обозначающая приближение (подойти, подбежать)"
                            },
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "предлог направления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70aff23b-024e-3a3e-a5dc-26c90d977467",
                    "sense_definition": "Completely or thoroughly; used to emphasize the completion of an action.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "completeness_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to clean <w>up</w> your room before you go out.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>убрать</w> в своей комнате, прежде чем идти гулять."
                            }
                        },
                        {
                            "text": "Drink <w>up</w> your juice so we can leave.",
                            "target_lang_translations": {
                                "ru": "<w>Допивай</w> свой сок, чтобы мы могли уйти."
                            }
                        }
                    ],
                    "common_phrases": [
                        "give up",
                        "clean up",
                        "eat up",
                        "use up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью или тщательно; используется для подчеркивания завершенности действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полностью",
                                "target_lang_sense_clarification": "наречие завершенности"
                            },
                            {
                                "target_lang_word": "до конца",
                                "target_lang_sense_clarification": "полное завершение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "da6d950d-cf26-37eb-b69d-85113e7dc588",
                    "sense_definition": "From a lower to a higher point on something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction_prep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cat climbed <w>up</w> the tree to catch a bird.",
                            "target_lang_translations": {
                                "ru": "Кошка залезла <w>на</w> дерево, чтобы поймать птицу."
                            }
                        },
                        {
                            "text": "We walked <w>up</w> the stairs to the second floor.",
                            "target_lang_translations": {
                                "ru": "Мы поднялись <w>вверх по</w> лестнице на второй этаж."
                            }
                        }
                    ],
                    "antonyms": [
                        "down"
                    ],
                    "common_phrases": [
                        "up the stairs",
                        "up the hill",
                        "up the tree"
                    ],
                    "target_lang_definitions": {
                        "ru": "С более низкой точки на более высокую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вверх по",
                                "target_lang_sense_clarification": "движение вдоль чего-либо вверх"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "на поверхность чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "476a8c2e-592c-3d71-b0c7-b44f3860360a",
                    "sense_definition": "Along or further along a street, road, or path.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "location_prep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They live just <w>up</w> the road from the school.",
                            "target_lang_translations": {
                                "ru": "Они живут чуть <w>дальше по</w> дороге от школы."
                            }
                        },
                        {
                            "text": "Go <w>up</w> the street and turn left at the bank.",
                            "target_lang_translations": {
                                "ru": "Идите <w>по</w> улице и поверните налево у банка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "up the road",
                        "up the street"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вдоль или дальше по улице, дороге или пути."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "движение вдоль"
                            },
                            {
                                "target_lang_word": "дальше по",
                                "target_lang_sense_clarification": "продолжение движения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "767a824f-6efa-3984-aec6-9f2e69934dfd",
                    "sense_definition": "Awake and out of bed.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "state_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was <w>up</w> very early this morning to catch the train.",
                            "target_lang_translations": {
                                "ru": "Я <w>встал</w> очень рано сегодня утром, чтобы успеть на поезд."
                            }
                        },
                        {
                            "text": "Are the children <w>up</w> yet, or are they still sleeping?",
                            "target_lang_translations": {
                                "ru": "Дети уже <w>встали</w> или всё ещё спят?"
                            }
                        }
                    ],
                    "synonyms": [
                        "alley oop"
                    ],
                    "antonyms": [
                        "down"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проснувшийся и вставший с постели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на ногах",
                                "target_lang_sense_clarification": "разговорное выражение"
                            },
                            {
                                "target_lang_word": "встал",
                                "target_lang_sense_clarification": "глагол прошедшего времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37444f74-b730-3c39-812c-2db53020fa58",
                    "sense_definition": "Finished or ended, especially regarding a period of time.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your time is <w>up</w>, so please put your pens down.",
                            "target_lang_translations": {
                                "ru": "Ваше время <w>истекло</w>, пожалуйста, положите ручки."
                            }
                        },
                        {
                            "text": "The contract is <w>up</w> next month, so we need to renew it.",
                            "target_lang_translations": {
                                "ru": "Срок контракта <w>истекает</w> в следующем месяце, поэтому нам нужно его продлить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "time is up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Законченный или истекший (о времени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "истекло",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "закончилось",
                                "target_lang_sense_clarification": "о событии или периоде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4f332f3-e2f6-3bf4-a81e-ca922a8321f1",
                    "sense_definition": "Happening, new, or of concern; commonly used in greetings.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "event_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hey, what is <w>up</w> with you today?",
                            "target_lang_translations": {
                                "ru": "Эй, что с тобой сегодня <w>происходит</w>?"
                            }
                        },
                        {
                            "text": "I knew something was <w>up</w> when I saw the police cars.",
                            "target_lang_translations": {
                                "ru": "Я понял, что что-то <w>случилось</w>, когда увидел полицейские машины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "what's up"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "very common in informal greetings"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий, новый или вызывающий беспокойство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "случилось",
                                "target_lang_sense_clarification": "о событии"
                            },
                            {
                                "target_lang_word": "происходит",
                                "target_lang_sense_clarification": "о текущей ситуации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a81962f5-8e2a-34f6-9422-9e5bfcf01b88",
                    "sense_definition": "Higher in value, volume, quantity, or degree.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sales are <w>up</w> by twenty percent compared to last year.",
                            "target_lang_translations": {
                                "ru": "Продажи <w>выросли</w> на двадцать процентов по сравнению с прошлым годом."
                            }
                        },
                        {
                            "text": "The temperature is <w>up</w> today, so it feels quite warm.",
                            "target_lang_translations": {
                                "ru": "Температура сегодня <w>выше</w>, поэтому довольно тепло."
                            }
                        }
                    ],
                    "antonyms": [
                        "down"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выше по значению, объему, количеству или степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выше",
                                "target_lang_sense_clarification": "сравнение уровня"
                            },
                            {
                                "target_lang_word": "вырос",
                                "target_lang_sense_clarification": "об увеличении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "642427a3-208e-3446-9c52-e97482963f41",
                    "sense_definition": "Functional, working, or in operation (especially of machines or systems).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The website is finally <w>up</w> after the maintenance.",
                            "target_lang_translations": {
                                "ru": "Сайт наконец <w>работает</w> после технического обслуживания."
                            }
                        },
                        {
                            "text": "Is the computer network <w>up</w> or is it still broken?",
                            "target_lang_translations": {
                                "ru": "Компьютерная сеть <w>работает</w> или всё ещё сломана?"
                            }
                        }
                    ],
                    "antonyms": [
                        "down"
                    ],
                    "target_lang_definitions": {
                        "ru": "Работающий, функционирующий (о машинах или системах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работает",
                                "target_lang_sense_clarification": "в рабочем состоянии"
                            },
                            {
                                "target_lang_word": "в строю",
                                "target_lang_sense_clarification": "функционирует"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0b4a963-a81f-3251-9c04-65db750c870c",
                    "sense_definition": "Willing, ready, or in the mood for something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>up</w> for going to the cinema if you are.",
                            "target_lang_translations": {
                                "ru": "Я <w>за</w> то, чтобы пойти в кино, если ты тоже."
                            }
                        },
                        {
                            "text": "Who is <w>up</w> for a game of football in the park?",
                            "target_lang_translations": {
                                "ru": "Кто <w>хочет</w> сыграть в футбол в парке?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "up for"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовый, желающий чего-либо или находящийся в настроении для чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "выражение согласия"
                            },
                            {
                                "target_lang_word": "готов",
                                "target_lang_sense_clarification": "согласие на действие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b36c7b66-e755-3d14-b8d8-a111e6b0ff4d",
                    "sense_definition": "To increase the level, amount, or price of something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "increase_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The store <w>upped</w> the price of milk yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера магазин <w>поднял</w> цену на молоко."
                            }
                        },
                        {
                            "text": "They have <w>upped</w> their offer to buy the house.",
                            "target_lang_translations": {
                                "ru": "Они <w>повысили</w> своё предложение о покупке дома."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повышать уровень, количество или цену чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повысить",
                                "target_lang_sense_clarification": "увеличить значение"
                            },
                            {
                                "target_lang_word": "поднять",
                                "target_lang_sense_clarification": "увеличить цену или ставку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca232207-74e0-36c0-bab7-5d63dc155a83",
                    "sense_definition": "To do something suddenly or unexpectedly (usually followed by 'and').",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She just <w>upped</w> and left without saying goodbye.",
                            "target_lang_translations": {
                                "ru": "Она просто <w>взяла и</w> ушла, не попрощавшись."
                            }
                        },
                        {
                            "text": "He <w>upped</w> and quit his job in the middle of the week.",
                            "target_lang_translations": {
                                "ru": "Он <w>взял и</w> уволился с работы посреди недели."
                            }
                        }
                    ],
                    "common_phrases": [
                        "up and leave",
                        "up and quit"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать что-то внезапно или неожиданно (обычно с 'and')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взять и",
                                "target_lang_sense_clarification": "идиома для внезапного действия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "690b83dc-06ce-376c-a4fb-d588f15496a0",
                    "sense_definition": "A positive period, aspect, or situation; good fortune.",
                    "learner_level": "C1",

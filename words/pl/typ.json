                    "frequency": "Middle",
                    "semantic_group_id": "computer_science",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Programista musi zadeklarować odpowiedni <w>typ</w> zmiennej przed jej użyciem w kodzie.",
                            "target_lang_translations": {
                                "ru": "Программист должен объявить соответствующий <w>тип</w> переменной перед ее использованием в коде."
                            }

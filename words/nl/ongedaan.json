{
    "word_family": [
        "doen",
        "daad",
        "dader",
        "gedaan",
        "wandaad",
        "weldaad"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1f6bbdb9-6c34-3edd-8c6d-57055d14c95b",
                    "sense_definition": "Teruggedraaid of tenietgedaan, zodat de gevolgen van een actie niet meer bestaan.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "reversal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter besloot de maatregel onmiddellijk <w>ongedaan</w> te maken.",
                            "target_lang_translations": {
                                "en": "The judge decided to immediately <w>reverse</w> the measure.",
                                "ru": "Судья решил немедленно <w>отменить</w> эту меру."
                            }
                        },
                        {
                            "text": "Gedane zaken nemen geen keer, je kunt ze niet meer <w>ongedaan</w> maken.",
                            "target_lang_translations": {
                                "en": "What is done cannot be turned back, you cannot <w>undo</w> it anymore.",
                                "ru": "Сделанного не воротишь, это уже нельзя <w>изменить</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tenietgedaan",
                        "teruggedraaid"
                    ],
                    "antonyms": [
                        "gedaan"
                    ],
                    "common_phrases": [
                        "ongedaan maken",
                        "schade ongedaan maken",
                        "iets ongedaan maken"
                    ],
                    "target_lang_definitions": {
                        "en": "Reversed or annulled, so that the consequences of an action no longer exist.",
                        "ru": "Отменённый или аннулированный, возвращённый в исходное состояние, так что последствия действия больше не существуют."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "undone",
                                "target_lang_sense_clarification": "reversed or cancelled out (usually with 'maken')"
                            },
                            {
                                "target_lang_word": "reversed",
                                "target_lang_sense_clarification": "turned back to a previous state"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "аннулированный",
                                "target_lang_sense_clarification": "признанный недействительным или отменённый"
                            },
                            {
                                "target_lang_word": "отменённый",
                                "target_lang_sense_clarification": "возвращённый назад, ликвидированный (о действии или решении)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b07e5930-83ef-31b5-8183-abeab2f6e991",
                    "sense_definition": "Niet verricht of niet uitgevoerd; wat men nagelaten heeft te doen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "negation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij liet geen middel <w>ongedaan</w> om zijn doel te bereiken.",
                            "target_lang_translations": {
                                "en": "He left no means <w>untried</w> to achieve his goal.",
                                "ru": "Он не оставил <w>неиспробованным</w> ни одного средства, чтобы достичь своей цели."
                            }
                        },
                        {
                            "text": "Door tijdgebrek bleef een groot deel van het werk <w>ongedaan</w>.",
                            "target_lang_translations": {
                                "en": "Due to a lack of time, a large part of the work remained <w>undone</w>.",
                                "ru": "Из-за нехватки времени большая часть работы осталась <w>невыполненной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "onafgedaan"
                    ],
                    "antonyms": [
                        "gedaan"
                    ],
                    "common_phrases": [
                        "ongedaan laten"
                    ],
                    "target_lang_definitions": {
                        "en": "Not performed or executed; what one has neglected to do.",
                        "ru": "Невыполненный, неосуществлённый; то, что было упущено или не сделано."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "undone",
                                "target_lang_sense_clarification": "not finished or completed"
                            },
                            {
                                "target_lang_word": "untried",
                                "target_lang_sense_clarification": "not attempted or tested"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "невыполненный",
                                "target_lang_sense_clarification": "не завершённый или не исполненный"
                            },
                            {
                                "target_lang_word": "несделанный",
                                "target_lang_sense_clarification": "оставленный без исполнения"
                            },
                            {
                                "target_lang_word": "неиспробованный",
                                "target_lang_sense_clarification": "в контексте средств или методов (оставить что-то неиспробованным)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
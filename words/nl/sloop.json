    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9648cae0-9d1d-31d8-84b3-3f7ca816676d",
                    "sense_definition": "Een stoffen hoes die om een hoofdkussen wordt gedaan ter bescherming en decoratie",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "bedding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal even een <w>sloopje</w> om dat kussen doen.",
                            "target_lang_translations": {
                                "ru": "Я быстро надену <w>наволочку</w> на эту подушку.",
                                "en": "I'll just put a <w>pillowcase</w> on that pillow."
                            }
                        },
                        {
                            "text": "Vergeet niet om de <w>sloop</w> wekelijks te wassen voor een fris bed.",
                            "target_lang_translations": {
                                "ru": "Не забывай еженедельно стирать <w>наволочку</w> для свежести постели.",
                                "en": "Don't forget to wash the <w>pillowcase</w> weekly for a fresh bed."
                            }
                        }
                    ],
                    "synonyms": [
                        "kussensloop"
                    ],
                    "common_phrases": [
                        "schone sloop",
                        "sloop verschonen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тканевый чехол, надеваемый на подушку для защиты и украшения",
                        "en": "A cloth cover placed around a pillow for protection and decoration"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наволочка",
                                "target_lang_sense_clarification": "чехол для подушки"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "pillowcase",
                                "target_lang_sense_clarification": "cover for a pillow"
                            },
                            {
                                "target_lang_word": "pillow slip",
                                "target_lang_sense_clarification": "cover for a pillow"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a92784d-dd23-3af0-99f3-f5b130dc2e9e",
                    "sense_definition": "Het proces van het afbreken van gebouwen, schepen of voertuigen, of de plaats waar dit gebeurt",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat schip is rijp voor de <w>sloop</w>.",
                            "target_lang_translations": {
                                "ru": "Этот корабль пора отправить на <w>слом</w>.",
                                "en": "That ship is ready for <w>scrapping</w>."
                            }
                        },
                        {
                            "text": "We brengen de kapotte auto morgen naar de <w>sloop</w> voor onderdelen.",
                            "target_lang_translations": {
                                "ru": "Завтра мы отвезем сломанную машину на <w>авторазборку</w> за запчастями.",
                                "en": "We are taking the broken car to the <w>scrapyard</w> tomorrow for parts."
                            }
                        }
                    ],
                    "synonyms": [
                        "afbraak",
                        "destructie"
                    ],
                    "antonyms": [
                        "bouw",
                        "constructie"
                    ],
                    "common_phrases": [
                        "voor de sloop",
                        "naar de sloop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс сноса зданий, судов или транспортных средств, или место, где это происходит",
                        "en": "The process of demolishing buildings, ships, or vehicles, or the place where this occurs"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снос",
                                "target_lang_sense_clarification": "разрушение зданий"
                            },
                            {
                                "target_lang_word": "разборка",
                                "target_lang_sense_clarification": "место разбора техники на запчасти"
                            },
                            {
                                "target_lang_word": "утилизация",
                                "target_lang_sense_clarification": "переработка"
                            },
                            {
                                "target_lang_word": "слом",
                                "target_lang_sense_clarification": "разрушение, сдача в утиль"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "demolition",
                                "target_lang_sense_clarification": "destruction of buildings"
                            },
                            {
                                "target_lang_word": "scrapping",
                                "target_lang_sense_clarification": "dismantling of vehicles or ships"
                            },
                            {
                                "target_lang_word": "scrapyard",
                                "target_lang_sense_clarification": "place where vehicles are dismantled"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1a361ba5-717c-3fed-8073-8b1e11ee5e70",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>slopen</w>",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>sloop</w> de oude schuur in de achtertuin om plaats te maken voor een terras.",
                            "target_lang_translations": {
                                "ru": "Я <w>сношу</w> старый сарай на заднем дворе, чтобы освободить место для террасы.",
                                "en": "I <w>am demolishing</w> the old shed in the backyard to make room for a terrace."
                            }
                        },
                        {
                            "text": "<w>Sloop</w> dat muurtje voorzichtig, anders beschadig je de leidingen.",
                            "target_lang_translations": {
                                "ru": "<w>Ломай</w> эту стенку осторожно, иначе повредишь трубы.",
                                "en": "<w>Demolish</w> that wall carefully, otherwise you'll damage the pipes."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола \"slopen\" (сносить, ломать)",
                        "en": "First person singular present tense of the verb \"slopen\" (to demolish, to wreck)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сношу",
                                "target_lang_sense_clarification": "разрушаю здание"
                            },
                            {
                                "target_lang_word": "ломаю",
                                "target_lang_sense_clarification": "разбиваю, порчу"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "demolish",
                                "target_lang_sense_clarification": "to pull down a building"
                            },
                            {
                                "target_lang_word": "wreck",
                                "target_lang_sense_clarification": "to destroy or ruin"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8df79b03-b513-39a1-8e96-869c5219aae6",
                    "sense_definition": "Enkelvoud verleden tijd van <w>sluipen</w>",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kat <w>sloop</w> zachtjes door het hoge gras op zoek naar een muis.",
                            "target_lang_translations": {
                                "ru": "Кот тихо <w>крался</w> в высокой траве в поисках мыши.",
                                "en": "The cat <w>crept</w> softly through the tall grass looking for a mouse."
                            }
                        },
                        {
                            "text": "Hij <w>sloop</w> de kamer uit zonder iemand wakker te maken.",
                            "target_lang_translations": {
                                "ru": "Он <w>выкрался</w> из комнаты, никого не разбудив.",
                                "en": "He <w>sneaked</w> out of the room without waking anyone."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма прошедшего времени единственного числа глагола \"sluipen\" (красться)",
                        "en": "Singular past tense of the verb \"sluipen\" (to sneak, to creep)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крался",
                                "target_lang_sense_clarification": "двигался тихо и незаметно"
                            },
                            {
                                "target_lang_word": "прокрался",
                                "target_lang_sense_clarification": "проник незаметно"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "crept",
                                "target_lang_sense_clarification": "moved quietly and slowly"
                            },
                            {
                                "target_lang_word": "sneaked",
                                "target_lang_sense_clarification": "moved furtively"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "krzywda",
        "krzywdzić",
        "krzywdzący",
        "pokrzywdzony",
        "krzywdziciel"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6163b78c-711f-3cd2-a6aa-694c901268e0",
                    "sense_definition": "Wyrządzić komuś krzywdę, sprawić przykrość, ból lub potraktować kogoś niesprawiedliwie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "harm_others",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nigdy nie chciałem cię <skrzywdzić>, moje słowa były nieprzemyślane i głupie.",
                            "target_lang_translations": {
                                "ru": "Я никогда не хотел тебя <обидеть>, мои слова были необдуманными и глупыми."
                            }
                        },
                        {
                            "text": "Musisz uważać na to, co mówisz, aby niechcący nie <skrzywdzić> kogoś bliskiego.",
                            "target_lang_translations": {
                                "ru": "Ты должен следить за тем, что говоришь, чтобы случайно не <обидеть> кого-то из близких."
                            }
                        },
                        {
                            "text": "To bardzo łagodny człowiek, który w życiu nie skrzywdziłby nawet muchy.",
                            "target_lang_translations": {
                                "ru": "Это очень мягкий человек, который в жизни и мухи не <обидел бы>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zranić",
                        "urazić",
                        "zaszkodzić"
                    ],
                    "antonyms": [
                        "uszczęśliwić",
                        "ochronić",
                        "pomóc"
                    ],
                    "common_phrases": [
                        "nie chcieć skrzywdzić",
                        "skrzywdzić kogoś słowem",
                        "niesłusznie skrzywdzić",
                        "skrzywdzić dziecko",
                        "bardzo skrzywdzić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причинить кому-то вред, доставить неприятности, боль или поступить с кем-то несправедливо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обидеть",
                                "target_lang_sense_clarification": "причинить душевную боль или поступить несправедливо"
                            },
                            {
                                "target_lang_word": "причинить вред",
                                "target_lang_sense_clarification": "нанести ущерб или физическую боль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "897762bf-3a16-3dae-877e-6dd02a5717ce",
                    "sense_definition": "Wyrządzić szkodę samemu sobie lub (w liczbie mnogiej) sprawić przykrość sobie nawzajem.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "harm_self_reciprocal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas kłótni małżonkowie mogą się dotkliwie <skrzywdzić> przykrymi oskarżeniami.",
                            "target_lang_translations": {
                                "ru": "Во время ссоры супруги могут сильно <ранить друг друга> обидными обвинениями."
                            }
                        },
                        {
                            "text": "Podejmując tak ryzykowne decyzje finansowe, możesz się tylko niepotrzebnie <skrzywdzić>.",
                            "target_lang_translations": {
                                "ru": "Принимая такие рискованные финансовые решения, ты можешь только зря <навредить себе>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zranić"
                    ],
                    "common_phrases": [
                        "skrzywdzić się nawzajem",
                        "dotkliwie się skrzywdzić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нанести вред самому себе или (во множественном числе) причинить боль друг другу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навредить себе",
                                "target_lang_sense_clarification": "сделать хуже самому себе"
                            },
                            {
                                "target_lang_word": "ранить друг друга",
                                "target_lang_sense_clarification": "причинить взаимную боль (обычно эмоциональную)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d3c11107-b731-3d26-970e-c30c46ae4808",
                    "sense_definition": "Требование или просьба прийти, приехать куда-либо (обычно официальное или к специалисту).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "request_signal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы оформили срочный <w>вызов</w> врача на дом из-за высокой температуры.",
                            "target_lang_translations": {
                                "en": "We arranged an urgent house <w>call</w> from a doctor due to high fever."
                            }
                        },
                        {
                            "text": "Ему пришёл официальный <w>вызов</w> в суд в качестве свидетеля.",
                            "target_lang_translations": {
                                "en": "He received an official <w>summons</w> to court as a witness."
                            }
                        }
                    ],
                    "synonyms": [
                        "приглашение",
                        "требование"
                    ],
                    "common_phrases": [
                        "вызов врача",
                        "ложный вызов",
                        "экстренный вызов",
                        "вызов такси"
                    ],
                    "target_lang_definitions": {
                        "en": "A request or order for someone to appear or attend, often used for doctors or court proceedings."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "call",
                                "target_lang_sense_clarification": "request for a professional visit (e.g., doctor)"
                            },
                            {
                                "target_lang_word": "summons",
                                "target_lang_sense_clarification": "official order to appear in court"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4e02fd7-912c-3c94-8b06-878e2e302e3e",
                    "sense_definition": "Звуковой или световой сигнал (обычно телефонный), сообщающий о попытке установить связь.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "request_signal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На экране телефона высветился пропущенный <w>вызов</w> от мамы.",
                            "target_lang_translations": {
                                "en": "A missed <w>call</w> from Mom appeared on the phone screen."
                            }
                        },
                        {
                            "text": "Я не слышал <w>вызова</w>, потому что телефон был на беззвучном режиме.",
                            "target_lang_translations": {
                                "en": "I didn't hear the <w>call</w> because the phone was on silent mode."
                            }
                        }
                    ],
                    "synonyms": [
                        "звонок",
                        "сигнал"
                    ],
                    "common_phrases": [
                        "входящий вызов",
                        "пропущенный вызов",
                        "ждать вызова",
                        "исходящий вызов"
                    ],
                    "target_lang_definitions": {
                        "en": "A sound or signal, usually from a telephone, indicating an incoming connection attempt."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "call",
                                "target_lang_sense_clarification": "act of telephoning"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19f0295e-3eb8-364d-9072-045a32b4e360",
                    "sense_definition": "Предложение вступить в борьбу, спор или соревнование.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "confrontation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спортсмен принял <w>вызов</w> и согласился на матч-реванш.",
                            "target_lang_translations": {
                                "en": "The athlete accepted the <w>challenge</w> and agreed to a rematch."
                            }
                        },
                        {
                            "text": "В его голосе звучал откровенный <w>вызов</w> и готовность спорить.",
                            "target_lang_translations": {
                                "en": "There was an open <w>challenge</w> in his voice and a readiness to argue."
                            }
                        }
                    ],
                    "common_phrases": [
                        "бросить вызов",
                        "принять вызов"
                    ],
                    "target_lang_definitions": {
                        "en": "An invitation or provocation to enter into a fight, contest, or argument."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "challenge",
                                "target_lang_sense_clarification": "invitation to compete"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8508a5d-0b9f-3e0b-8f63-2e18fb51e53f",
                    "sense_definition": "Сложная задача или проблема, требующая больших усилий и смелости для её решения.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "confrontation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Изменение климата — это главный глобальный <w>вызов</w> современности.",
                            "target_lang_translations": {
                                "en": "Climate change is the main global <w>challenge</w> of our time."
                            }
                        },
                        {
                            "text": "Новая работа стала для меня настоящим профессиональным <w>вызовом</w>.",
                            "target_lang_translations": {
                                "en": "The new job became a real professional <w>challenge</w> for me."
                            }
                        }
                    ],
                    "synonyms": [
                        "испытание",
                        "проблема",
                        "задача"
                    ],
                    "common_phrases": [
                        "ответ на вызов",
                        "серьёзный вызов",
                        "стоять перед вызовом"
                    ],
                    "target_lang_definitions": {
                        "en": "A difficult task or situation that tests one's abilities and requires effort to overcome."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "challenge",
                                "target_lang_sense_clarification": "difficult task"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f124d543-2926-31f6-80b3-91c4c51e137d",
                    "sense_definition": "Обращение к подпрограмме или функции в программировании.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "request_signal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Программа завершилась с ошибкой из-за неправильного <w>вызова</w> функции.",
                            "target_lang_translations": {
                                "en": "The program crashed due to an incorrect function <w>call</w>."
                            }
                        },
                        {
                            "text": "Этот <w>вызов</w> метода возвращает значение истины или лжи.",
                            "target_lang_translations": {
                                "en": "This method <w>call</w> returns a true or false value."
                            }
                        }
                    ],
                    "synonyms": [
                        "обращение"
                    ],
                    "common_phrases": [
                        "вызов функции",
                        "вызов процедуры",
                        "системный вызов"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The execution of a specific function or routine in a computer program."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "call",
                                "target_lang_sense_clarification": "computing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dab4dd63-d8de-367e-b896-c3d23ec89f05",
                    "sense_definition": "Поступок или поведение, демонстративно нарушающие общепринятые нормы.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "confrontation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её яркая и необычная одежда была своего рода <w>вызовом</w> серой толпе.",
                            "target_lang_translations": {
                                "en": "Her bright and unusual clothes were a kind of <w>challenge</w> to the grey crowd."
                            }
                        },
                        {
                            "text": "В этом дерзком поступке сквозил <w>вызов</w> общественной морали.",
                            "target_lang_translations": {
                                "en": "A <w>challenge</w> to public morality was evident in this daring act."
                            }
                        }
                    ],
                    "synonyms": [
                        "провокация",
                        "дерзость"
                    ],
                    "antonyms": [
                        "покорность"
                    ],
                    "common_phrases": [
                        "дерзкий вызов",
                        "вызов обществу"
                    ],
                    "target_lang_definitions": {
                        "en": "An act or behavior that openly disregards or opposes accepted norms."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "challenge",
                                "target_lang_sense_clarification": "act of opposition or provocation"
                            },
                            {
                                "target_lang_word": "defiance",
                                "target_lang_sense_clarification": "open resistance"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
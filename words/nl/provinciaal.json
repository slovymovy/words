{
    "word_family": [
        "provincie",
        "provincialisme"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fa4ea35a-67d4-3b15-9f26-3f993058d5c2",
                    "sense_definition": "Met betrekking tot of behorend bij een provincie (een deelgebied van een land).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>provinciale</w> verkiezingen bepalen wie er in het bestuur komt.",
                            "target_lang_translations": {
                                "ru": "<w>Провинциальные</w> выборы определяют, кто войдет в состав правления."
                            }
                        },
                        {
                            "text": "We reden over een rustige <w>provinciale</w> weg naar het dorp.",
                            "target_lang_translations": {
                                "ru": "Мы ехали в деревню по тихой <w>провинциальной</w> дороге."
                            }
                        }
                    ],
                    "synonyms": [
                        "gewestelijk"
                    ],
                    "antonyms": [
                        "landelijk",
                        "nationaal"
                    ],
                    "common_phrases": [
                        "provinciale staten",
                        "provinciale weg",
                        "provinciaal bestuur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к провинции (административной единице) или принадлежащий ей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провинциальный",
                                "target_lang_sense_clarification": "относящийся к провинции"
                            },
                            {
                                "target_lang_word": "областной",
                                "target_lang_sense_clarification": "относящийся к области (административный)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "508db2d8-49e9-36da-88c4-0233a54e7fcf",
                    "sense_definition": "Bekrompen in opvattingen of gedrag; niet werelds of stedelijk.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft een nogal <w>provinciale</w> kijk op kunst en cultuur.",
                            "target_lang_translations": {
                                "ru": "У него довольно <w>провинциальный</w> взгляд на искусство и культуру."
                            }
                        },
                        {
                            "text": "Haar ouders vonden zijn manieren wat <w>provinciaal</w> en onbehouwen.",
                            "target_lang_translations": {
                                "ru": "Её родители считали его манеры несколько <w>провинциальными</w> и неотесанными."
                            }
                        }
                    ],
                    "synonyms": [
                        "bekrompen",
                        "kleingeestig",
                        "dorps"
                    ],
                    "antonyms": [
                        "werelds",
                        "mondain",
                        "stedelijk"
                    ],
                    "common_phrases": [
                        "een provinciale mentaliteit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограниченный во взглядах или поведении; не светский, старомодный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провинциальный",
                                "target_lang_sense_clarification": "перен. отсталый, наивный"
                            },
                            {
                                "target_lang_word": "местечковый",
                                "target_lang_sense_clarification": "узкий, ограниченный интересами своего места"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ad9ca861-5a20-37a9-b7ec-7812ec7693bf",
                    "sense_definition": "Iemand die op het platteland woont of vandaan komt (soms met de bijbetekenis van niet-stedelijk gedrag).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als eenvoudige <w>provinciaal</w> keek hij zijn ogen uit in de grote stad.",
                            "target_lang_translations": {
                                "ru": "Будучи простым <w>провинциалом</w>, он с изумлением смотрел на большой город."
                            }
                        },
                        {
                            "text": "De stedelingen maakten grapjes over de kleding van de <w>provinciaal</w>.",
                            "target_lang_translations": {
                                "ru": "Горожане шутили над одеждой <w>провинциала</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "plattelander",
                        "dorpeling"
                    ],
                    "antonyms": [
                        "stedeling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, живущий в провинции или приехавший оттуда (иногда с оттенком простоватости)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провинциал",
                                "target_lang_sense_clarification": "житель провинции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbc7afb5-8f11-38ed-bf9c-221b311a3a42",
                    "sense_definition": "Het hoofd van een kloosterprovincie binnen een religieuze orde.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>provinciaal</w> bezocht vorige week ons klooster voor een inspectie.",
                            "target_lang_translations": {
                                "ru": "На прошлой неделе <w>провинциал</w> посетил наш монастырь с инспекцией."
                            }
                        },
                        {
                            "text": "Hij werd door het kapittel gekozen tot nieuwe <w>provinciaal</w> van de orde.",
                            "target_lang_translations": {
                                "ru": "Капитул избрал его новым <w>провинциалом</w> ордена."
                            }
                        }
                    ],
                    "synonyms": [
                        "overste"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глава церковной провинции в монашеском ордене."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провинциал",
                                "target_lang_sense_clarification": "церковный титул"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "bezwzględność",
        "bezwzględnie"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "20329944-9bd1-3d78-8af6-df8a8d67fb0e",
                    "sense_definition": "Okrutny, pozbawiony litości i skrupułów wobec innych; bardzo surowy w ocenie lub działaniu.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>bezwzględny</w> przestępca nie okazał żadnej skruchy podczas procesu sądowego.",
                            "target_lang_translations": {
                                "ru": "Этот <w>безжалостный</w> преступник не выказал никакого раскаяния во время судебного процесса."
                            }
                        },
                        {
                            "text": "W świecie wielkiego biznesu często toczy się <w>bezwzględna</w> walka o klienta.",
                            "target_lang_translations": {
                                "ru": "В мире большого бизнеса часто идет <w>беспощадная</w> борьба за клиента."
                            }
                        }
                    ],
                    "synonyms": [
                        "okrutny",
                        "surowy",
                        "srogi",
                        "niepobłażliwy",
                        "niewyrozumiały"
                    ],
                    "antonyms": [
                        "łagodny",
                        "wyrozumiały",
                        "pobłażliwy"
                    ],
                    "common_phrases": [
                        "bezwzględna walka",
                        "bezwzględny morderca",
                        "bezwzględna krytyka",
                        "bezwzględne traktowanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жестокий, лишенный жалости и сострадания к другим; очень суровый в оценках или действиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беспощадный",
                                "target_lang_sense_clarification": "не знающий пощады"
                            },
                            {
                                "target_lang_word": "безжалостный",
                                "target_lang_sense_clarification": "не имеющий жалости"
                            },
                            {
                                "target_lang_word": "жестокий",
                                "target_lang_sense_clarification": "крайне суровый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d7a8bc0-7695-3af0-981b-d850066df11e",
                    "sense_definition": "Całkowity, zupełny, niepodlegający żadnym warunkom; niedopuszczający wyjątków (często o zakazach lub nakazach).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na terenie całego szpitala obowiązuje <w>bezwzględny</w> zakaz palenia papierosów.",
                            "target_lang_translations": {
                                "ru": "На всей территории больницы действует <w>категорический</w> запрет на курение."
                            }
                        },
                        {
                            "text": "Wymagane jest <w>bezwzględne</w> posłuszeństwo wobec rozkazów przełożonego.",
                            "target_lang_translations": {
                                "ru": "Требуется <w>беспрекословное</w> послушание приказам начальника."
                            }
                        }
                    ],
                    "synonyms": [
                        "absolutny",
                        "bezwarunkowy"
                    ],
                    "antonyms": [
                        "względny"
                    ],
                    "common_phrases": [
                        "bezwzględny zakaz",
                        "bezwzględna cisza",
                        "bezwzględne posłuszeństwo",
                        "bezwzględna większość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный, совершенный, не зависящий от каких-либо условий; не допускающий исключений (часто о запретах или приказах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "абсолютный",
                                "target_lang_sense_clarification": "полный, совершенный"
                            },
                            {
                                "target_lang_word": "безусловный",
                                "target_lang_sense_clarification": "не ограниченный условиями"
                            },
                            {
                                "target_lang_word": "категорический",
                                "target_lang_sense_clarification": "решительный, не допускающий возражений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8919eeb7-2bc3-399e-8a27-60630f83874f",
                    "sense_definition": "W nauce (matematyce, fizyce): niezależny od znaku, kierunku lub układu odniesienia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wartość <w>bezwzględna</w> z liczby ujemnej jest zawsze liczbą dodatnią.",
                            "target_lang_translations": {
                                "ru": "<w>Абсолютная</w> величина отрицательного числа всегда является положительным числом."
                            }
                        },
                        {
                            "text": "Błąd <w>bezwzględny</w> pomiaru to różnica między wynikiem zmierzonym a wartością rzeczywistą.",
                            "target_lang_translations": {
                                "ru": "<w>Абсолютная</w> погрешность измерения — это разность между измеренным результатом и истинным значением."
                            }
                        }
                    ],
                    "synonyms": [
                        "absolutny"
                    ],
                    "antonyms": [
                        "względny"
                    ],
                    "common_phrases": [
                        "wartość bezwzględna",
                        "błąd bezwzględny",
                        "zero bezwzględne",
                        "wilgotność bezwzględna"
                    ],
                    "target_lang_definitions": {
                        "ru": "В науке (математике, физике): независимый от знака, направления или системы отсчета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "абсолютный",
                                "target_lang_sense_clarification": "научный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
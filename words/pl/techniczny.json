            "senses": [
                {
                    "sense_id": "24e4d61e-50bc-3c02-9bfd-5efeb9d10f83",
                    "sense_definition": "Związany z techniką, inżynierią, maszynami lub produkcją przemysłową.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "technology_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat studiuje na uniwersytecie <w>technicznym</w> w Gdańsku.",
                            "target_lang_translations": {
                                "ru": "Мой брат учится в <w>техническом</w> университете в Гданьске."
                            }
                        },
                        {
                            "text": "Samochód musi przejść coroczny przegląd <w>techniczny</w>, aby mógł poruszać się po drogach.",
                            "target_lang_translations": {
                                "ru": "Автомобиль должен пройти ежегодный <w>технический</w> осмотр, чтобы ездить по дорогам."
                            }
                        }
                    ],
                    "synonyms": [
                        "inżynieryjny",
                        "technologiczny",
                        "przemysłowy"
                    ],
                    "antonyms": [
                        "humanistyczny",
                        "nietechniczny"
                    ],
                    "common_phrases": [
                        "stan techniczny",
                        "postęp techniczny",
                        "przegląd techniczny",
                        "uniwersytet techniczny",
                        "dane techniczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с техникой, инженерией, машинами или промышленным производством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "технический",
                                "target_lang_sense_clarification": "относящийся к технике и производству"
                            },
                            {
                                "target_lang_word": "промышленный",
                                "target_lang_sense_clarification": "связанный с индустрией"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "811ad8ba-9abd-381b-a33d-03025c97304f",
                    "sense_definition": "Dotyczący sposobu wykonywania danej czynności, wymagający wprawy lub specjalnych umiejętności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "skill_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pianista popełnił drobny błąd <w>techniczny</w> podczas koncertu, ale publiczność tego nie zauważyła.",
                            "target_lang_translations": {
                                "ru": "Пианист допустил небольшую <w>техническую</w> ошибку во время концерта, но публика этого не заметила."
                            }
                        },
                        {
                            "text": "Ten piłkarz ma świetne umiejętności <w>techniczne</w>, ale brakuje mu kondycji fizycznej.",
                            "target_lang_translations": {
                                "ru": "У этого футболиста отличные <w>технические</w> навыки, но ему не хватает физической подготовки."
                            }
                        }
                    ],
                    "synonyms": [
                        "warsztatowy",
                        "wykonawczy"
                    ],
                    "common_phrases": [
                        "błąd techniczny",
                        "nokaut techniczny",
                        "strona techniczna",
                        "umiejętności techniczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к способу выполнения действия, требующий сноровки или специальных навыков."

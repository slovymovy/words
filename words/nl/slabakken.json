    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c64a5690-9945-3360-ad91-2d627bcb4865",
                    "sense_definition": "Lui zijn, traag werken of taken laten versloffen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "behavior_lazy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nou aan je werk, je hebt genoeg <w>geslabakt</w>!",
                            "target_lang_translations": {
                                "ru": "Ну, за работу, ты достаточно <w>полодырничал</w>!"
                            }
                        },
                        {
                            "text": "Als we nu gaan <w>slabakken</w>, krijgen we het project nooit op tijd af.",
                            "target_lang_translations": {
                                "ru": "Если мы сейчас будем <w>лениться</w>, мы никогда не закончим проект вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "luieren",
                        "lanterfanten",
                        "treuzelen",
                        "verslappen"
                    ],
                    "antonyms": [
                        "doorwerken",
                        "aanpakken",
                        "opschieten"
                    ],
                    "common_phrases": [
                        "niet slabakken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть ленивым, работать медленно или пренебрегать обязанностями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лодырничать",
                                "target_lang_sense_clarification": "разг. уклоняться от работы"
                            },
                            {
                                "target_lang_word": "бездельничать",
                                "target_lang_sense_clarification": "ничего не делать"
                            },
                            {
                                "target_lang_word": "лениться",
                                "target_lang_sense_clarification": "проявлять лень"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d347e3c4-00b4-3544-980b-e63a486221f5",
                    "sense_definition": "Minder actief worden, stagneren of slecht lopen (vaak gezegd van de economie of handel).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "process_stagnate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De economie <w>slabakt</w> al enige tijd door de wereldwijde crisis.",
                            "target_lang_translations": {
                                "ru": "Экономика уже некоторое время <w>застаивается</w> из-за мирового кризиса."
                            }
                        },
                        {
                            "text": "De verkoop van nieuwe woningen is de laatste maanden flink <w>geslabakt</w>.",
                            "target_lang_translations": {
                                "ru": "Продажи новых домов в последние месяцы сильно <w>замедлились</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stagneren",
                        "inzakken",
                        "teruglopen",
                        "verminderen"
                    ],
                    "antonyms": [
                        "floreren",
                        "groeien",
                        "aantrekken"
                    ],
                    "common_phrases": [
                        "de economie slabakt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться менее активным, застаиваться или идти на спад (об экономике или торговле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застаиваться",
                                "target_lang_sense_clarification": "останавливаться в развитии"
                            },
                            {
                                "target_lang_word": "идти на спад",
                                "target_lang_sense_clarification": "снижаться, уменьшаться"
                            },
                            {
                                "target_lang_word": "замедляться",
                                "target_lang_sense_clarification": "снижать темп"
                            }
                        ]
                    }
                }
            ]
        }
    ]

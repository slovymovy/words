                {
                    "sense_id": "2b77c861-b183-399d-8ea7-b5923b0c6ef1",
                    "sense_definition": "De eigenschap van het blond zijn; het hebben van een lichte haarkleur.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "state_of_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is erg trots op haar natuurlijke <w>blondheid</w>.",
                            "target_lang_translations": {
                                "en": "She is very proud of her natural <w>blondness</w>.",
                                "ru": "Она очень гордится своей естественной <w>светловолосостью</w>."
                            }
                        },
                        {
                            "text": "De <w>blondheid</w> van de kinderen viel direct op in de menigte met donkerharige mensen.",
                            "target_lang_translations": {
                                "en": "The <w>blondness</w> of the children stood out immediately in the crowd of dark-haired people.",
                                "ru": "<w>Светловолосость</w> детей сразу бросалась в глаза в толпе темноволосых людей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "natuurlijke blondheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality of being blond; having a light hair color.",
                        "ru": "Свойство иметь светлые волосы; светловолосость."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "333eee04-a0aa-385c-b747-7bb5f1402ff1",
                    "sense_definition": "At serious risk of extinction or harm; often used to describe species that might disappear soon.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "risk_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government passed new laws to protect <w>endangered</w> species from hunting.",
                            "target_lang_translations": {
                                "ru": "Правительство приняло новые законы для защиты <w>исчезающих</w> видов от охоты."
                            }
                        },
                        {
                            "text": "Many <w>endangered</w> animals are kept in zoos to help their population recover.",
                            "target_lang_translations": {
                                "ru": "Многие <w>вымирающие</w> животные содержатся в зоопарках, чтобы помочь восстановлению их популяции."
                            }
                        }
                    ],
                    "synonyms": [
                        "threatened",
                        "at risk",
                        "imperiled"
                    ],
                    "antonyms": [
                        "safe",
                        "secure",
                        "protected"
                    ],
                    "common_phrases": [
                        "endangered species",
                        "critically endangered",
                        "endangered list"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся под угрозой исчезновения или в серьезной опасности; часто используется для описания видов, которые могут скоро исчезнуть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "находящийся под угрозой исчезновения",
                                "target_lang_sense_clarification": "официальный статус видов"
                            },
                            {
                                "target_lang_word": "вымирающий",
                                "target_lang_sense_clarification": "о животных или растениях"
                            },
                            {
                                "target_lang_word": "исчезающий",
                                "target_lang_sense_clarification": "становящийся редким"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c257c0f6-5066-3dbf-87db-be4c1aa531ed",
                    "sense_definition": "Simple past and past participle of <w>endanger</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden storm <w>endangered</w> the lives of the sailors.",
                            "target_lang_translations": {
                                "ru": "Внезапный шторм <w>поставил под угрозу</w> жизни моряков."
                            }
                        },
                        {
                            "text": "His reckless driving <w>endangered</w> everyone on the road.",
                            "target_lang_translations": {
                                "ru": "Его неосторожное вождение <w>подвергло опасности</w> всех на дороге."
                            }
                        }
                    ],
                    "common_phrases": [
                        "endangered lives",
                        "endangered the public"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола endanger (подвергать опасности)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подверг опасности",
                                "target_lang_sense_clarification": "глагол в прошедшем времени"
                            },
                            {
                                "target_lang_word": "поставил под угрозу",
                                "target_lang_sense_clarification": "синоним, прошедшее время"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2e60932e-cb68-339d-8385-465276725372",
                    "sense_definition": "A system of transport using trains on tracks.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transport_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to travel to the seaside by <w>rail</w> to avoid the traffic.",
                            "target_lang_translations": {
                                "ru": "Мы решили поехать к морю по <w>железной дороге</w>, чтобы избежать пробок."
                            }
                        },
                        {
                            "text": "The government plans to invest more money in the national <w>rail</w> network.",
                            "target_lang_translations": {
                                "ru": "Правительство планирует вкладывать больше денег в национальную <w>железнодорожную</w> сеть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "by rail",
                        "rail transport",
                        "rail network",
                        "high-speed rail"
                    ],
                    "target_lang_definitions": {
                        "ru": "Железная дорога как система транспорта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железная дорога",
                                "target_lang_sense_clarification": "система путей сообщения"
                            },
                            {
                                "target_lang_word": "железнодорожный путь",
                                "target_lang_sense_clarification": "пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83276002-e0d0-34f4-8b3f-7762961d6c22",
                    "sense_definition": "One of the two parallel metal bars laid on the ground for trains to move on.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy train wheels ground loudly against the steel <w>rails</w>.",
                            "target_lang_translations": {
                                "ru": "Тяжелые колеса поезда громко скрежетали о стальные <w>рельсы</w>."
                            }
                        },
                        {
                            "text": "It is very dangerous to walk on the <w>rails</w> because a train could come at any time.",
                            "target_lang_translations": {
                                "ru": "Очень опасно ходить по <w>рельсам</w>, потому что в любой момент может появиться поезд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "third rail",
                        "off the rails"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стальная балка, укладываемая на шпалы, по которой движутся поезда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рельс",
                                "target_lang_sense_clarification": "стальная балка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4889419a-581c-37f3-b37b-74b4c7154504",
                    "sense_definition": "A horizontal bar fixed to posts or walls, used for support, hanging things, or as a barrier.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "barrier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please hold the <w>rail</w> firmly while walking down the steep stairs.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, крепко держитесь за <w>перила</w>, спускаясь по крутой лестнице."
                            }
                        },
                        {
                            "text": "She hung the wet towel on the heated <w>rail</w> in the bathroom.",
                            "target_lang_translations": {
                                "ru": "Она повесила мокрое полотенце на нагретую <w>перекладину</w> в ванной."
                            }
                        }
                    ],
                    "synonyms": [
                        "rail out",
                        "rail out"
                    ],
                    "common_phrases": [
                        "hand rail",
                        "towel rail",
                        "curtain rail",
                        "grab the rail"
                    ],
                    "target_lang_definitions": {
                        "ru": "Горизонтальная перекладина или ограждение для опоры или подвешивания вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перила",
                                "target_lang_sense_clarification": "ограждение лестницы"
                            },
                            {
                                "target_lang_word": "поручень",
                                "target_lang_sense_clarification": "опора для рук"
                            },
                            {
                                "target_lang_word": "перекладина",
                                "target_lang_sense_clarification": "брус для подвешивания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9201f276-933e-3686-99f1-b0252a54dd09",
                    "sense_definition": "A small bird with short wings that lives in marshes.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The elusive <w>rail</w> hid silently among the tall reeds in the marsh.",
                            "target_lang_translations": {
                                "ru": "Неуловимый <w>пастушок</w> тихо прятался среди высокого камыша на болоте."
                            }
                        },
                        {
                            "text": "Birdwatchers were excited to spot a rare species of <w>rail</w> near the lake.",
                            "target_lang_translations": {
                                "ru": "Орнитологи были в восторге, заметив редкий вид <w>пастушка</w> у озера."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая птица семейства пастушковых, обитающая на болотах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пастушок",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "83ecbc33-d5e1-3923-9845-1ec8520a542b",
                    "sense_definition": "To complain or protest strongly and angrily about something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "complain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The citizens began to <w>rail</w> against the corruption in the local government.",
                            "target_lang_translations": {
                                "ru": "Граждане начали громко <w>выступать</w> против коррупции в местном правительстве."
                            }
                        },
                        {
                            "text": "He would often <w>rail</w> at the television when the news made him angry.",
                            "target_lang_translations": {
                                "ru": "Он часто <w>ругался</w> на телевизор, когда новости его злили."
                            }
                        }
                    ],
                    "synonyms": [
                        "fulminate",
                        "inveigh"
                    ],
                    "common_phrases": [
                        "rail against",
                        "rail at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громко и гневно жаловаться или протестовать против чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ругать",
                                "target_lang_sense_clarification": "бранить"
                            },
                            {
                                "target_lang_word": "негодовать",
                                "target_lang_sense_clarification": "возмущаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be7f984b-f655-34d3-8a5b-38b49345fdc6",
                    "sense_definition": "To enclose or separate an area with a fence or barrier.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "enclose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The construction crew decided to <w>rail</w> off the dangerous section of the sidewalk.",
                            "target_lang_translations": {
                                "ru": "Строительная бригада решила <w>огородить</w> опасный участок тротуара."
                            }
                        },
                        {
                            "text": "The garden was <w>railed</w> in to prevent the wild animals from eating the vegetables.",
                            "target_lang_translations": {
                                "ru": "Сад был <w>обнесен оградой</w>, чтобы дикие животные не ели овощи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rail off",
                        "rail in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Огораживать территорию забором или барьером."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "огораживать",
                                "target_lang_sense_clarification": "обносить оградой"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
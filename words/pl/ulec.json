    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d343921d-4434-3018-8d05-15bf62bdb8a9",
                    "sense_definition": "Przestać stawiać opór wobec kogoś lub czegoś; poddać się oddziaływaniu jakiejś siły, uczucia, prośby lub nacisku.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "submission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musiałem w końcu <ulec> jego namowom i zgodzić się na wyjazd.",
                            "target_lang_translations": {
                                "ru": "В конце концов мне пришлось <поддаться> его уговорам и согласиться на поездку."
                            }
                        },
                        {
                            "text": "Nie możesz <ulec> panice, musisz zachować spokój i działać racjonalnie.",
                            "target_lang_translations": {
                                "ru": "Ты не должен <поддаваться> панике, нужно сохранять спокойствие и действовать рационально."
                            }
                        },
                        {
                            "text": "Wielu ludzi potrafi <ulec> pokusie łatwego zarobku, ignorując ryzyko.",
                            "target_lang_translations": {
                                "ru": "Многие люди способны <поддаться> искушению легкого заработка, игнорируя риск."
                            }
                        }
                    ],
                    "synonyms": [
                        "poddać się",
                        "ustąpić"
                    ],
                    "antonyms": [
                        "oprzeć się"
                    ],
                    "common_phrases": [
                        "ulec pokusie",
                        "ulec namowom",
                        "ulec presji",
                        "ulec złudzeniu",
                        "ulec emocjom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать сопротивляться; подчиниться воздействию какой-либо силы, чувства, просьбы или давления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддаться",
                                "target_lang_sense_clarification": "эмоциям, панике, искушению"
                            },
                            {
                                "target_lang_word": "уступить",
                                "target_lang_sense_clarification": "просьбам, давлению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea85f017-2a89-35e9-afdd-6ce32311c390",
                    "sense_definition": "Zostać poddanym jakiemuś procesowi lub działaniu, w wyniku którego następuje zmiana stanu, często o charakterze negatywnym (np. zniszczenie, uszkodzenie).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "transformation_accident",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas gwałtownej burzy stary dach stodoły <uległ> całkowitemu zniszczeniu.",
                            "target_lang_translations": {
                                "ru": "Во время сильной бури старая крыша сарая <подверглась> полному разрушению."
                            }
                        },
                        {
                            "text": "Niestety, plany naszej wycieczki <uległy> zmianie z powodu choroby przewodnika.",
                            "target_lang_translations": {
                                "ru": "К сожалению, планы нашей поездки <претерпели> изменения из-за болезни гида."
                            }
                        },
                        {
                            "text": "Kierowca stracił panowanie nad pojazdem i <uległ> groźnemu wypadkowi.",
                            "target_lang_translations": {
                                "ru": "Водитель потерял управление автомобилем и <попал> в серьезную аварию."
                            }
                        }
                    ],
                    "synonyms": [
                        "doznać"
                    ],
                    "common_phrases": [
                        "ulec wypadkowi",
                        "ulec zmianie",
                        "ulec zniszczeniu",
                        "ulec awarii",
                        "ulec pogorszeniu",
                        "ulec przedawnieniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергнуться какому-либо процессу или действию, в результате которого происходит изменение состояния (часто негативное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвергнуться",
                                "target_lang_sense_clarification": "разрушению, воздействию"
                            },
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "в аварию"
                            },
                            {
                                "target_lang_word": "претерпеть",
                                "target_lang_sense_clarification": "изменения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
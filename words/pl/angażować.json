{
    "word_family": [
        "angaż",
        "zaangażowanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "18f77f8d-17b0-341d-af91-b255f0304325",
                    "sense_definition": "Brać aktywny udział w jakimś działaniu, sprawie lub przedsięwzięciu, poświęcając temu swój czas i energię.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "involvement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek lubi <angażować> się w różne akcje charytatywne.",
                            "target_lang_translations": {
                                "ru": "Марек любит <участвовать> в различных благотворительных акциях."
                            }
                        },
                        {
                            "text": "Nie powinieneś zbytnio <angażować> się w ten konflikt sąsiedzki.",
                            "target_lang_translations": {
                                "ru": "Тебе не стоит слишком сильно <вовлекаться> в этот соседский конфликт."
                            }
                        }
                    ],
                    "synonyms": [
                        "włączać się",
                        "poświęcać się",
                        "wciągać się",
                        "zajmować się"
                    ],
                    "common_phrases": [
                        "angażować się w pomoc",
                        "angażować się politycznie",
                        "angażować się emocjonalnie",
                        "angażować się w dyskusję"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать активное участие в каком-либо деле, посвящая этому свое время и энергию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участвовать",
                                "target_lang_sense_clarification": "принимать участие в деятельности"
                            },
                            {
                                "target_lang_word": "вовлекаться",
                                "target_lang_sense_clarification": "становиться участником чего-либо, часто эмоционально"
                            },
                            {
                                "target_lang_word": "ввязываться",
                                "target_lang_sense_clarification": "вмешиваться (часто с негативным оттенком)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d49c4994-479f-3a79-bed2-6c23d18ea198",
                    "sense_definition": "Włączać kogoś lub coś do udziału w jakiejś sprawie; także zajmować czyjąś uwagę lub wykorzystywać dostępne środki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "involvement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten duży projekt będzie <angażować> wielu specjalistów z różnych dziedzin.",
                            "target_lang_translations": {
                                "ru": "Этот крупный проект будет <задействовать> многих специалистов из разных областей."
                            }
                        },
                        {
                            "text": "Nauczyciel starał się <angażować> całą klasę w prowadzoną lekcję.",
                            "target_lang_translations": {
                                "ru": "Учитель старался <вовлекать> весь класс в урок."
                            }
                        }
                    ],
                    "synonyms": [
                        "włączać",
                        "wciągać",
                        "absorbować",
                        "pochłaniać"
                    ],
                    "common_phrases": [
                        "angażować środki",
                        "angażować autorytet",
                        "angażować wyobraźnię",
                        "angażować uwagę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привлекать кого-либо к участию в деле или использовать доступные ресурсы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задействовать",
                                "target_lang_sense_clarification": "использовать средства или людей"
                            },
                            {
                                "target_lang_word": "привлекать",
                                "target_lang_sense_clarification": "приглашать к участию"
                            },
                            {
                                "target_lang_word": "вовлекать",
                                "target_lang_sense_clarification": "побуждать кого-то принять участие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45a50887-26b8-39db-8ebc-24eaae3c832a",
                    "sense_definition": "Przyjmować kogoś do pracy, zawierać z kimś umowę o pracę (często używane w kontekście artystycznym, np. w teatrze).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "employment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor teatru postanowił <angażować> tylko doświadczonych aktorów.",
                            "target_lang_translations": {
                                "ru": "Директор театра решил <нанимать> только опытных актеров."
                            }
                        },
                        {
                            "text": "Firma planuje <angażować> nowych pracowników do działu sprzedaży.",
                            "target_lang_translations": {
                                "ru": "Компания планирует <нанимать> новых сотрудников в отдел продаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "zatrudniać"
                    ],
                    "common_phrases": [
                        "angażować aktora",
                        "angażować zespół",
                        "angażować personel",
                        "angażować do filmu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать кого-либо на работу, заключать трудовой договор (часто в творческой сфере)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нанимать",
                                "target_lang_sense_clarification": "брать на работу"
                            },
                            {
                                "target_lang_word": "приглашать",
                                "target_lang_sense_clarification": "звать на работу (об актерах, специалистах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec4fdffb-8796-3413-8914-c9d8e81eaf1c",
                    "sense_definition": "Podejmować pracę w jakiejś instytucji lub przy jakimś projekcie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "employment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody aktor nie chciał <angażować> się na stałe w jednym teatrze.",
                            "target_lang_translations": {
                                "ru": "Молодой актер не хотел <устраиваться> на постоянную работу в одном театре."
                            }
                        },
                        {
                            "text": "Ona woli <angażować> się tylko do konkretnych, krótkich zleceń.",
                            "target_lang_translations": {
                                "ru": "Она предпочитает <наниматься> только на конкретные, краткосрочные заказы."
                            }
                        }
                    ],
                    "synonyms": [
                        "zatrudniać się"
                    ],
                    "antonyms": [
                        "zwalniać się"
                    ],
                    "common_phrases": [
                        "angażować się do teatru",
                        "angażować się do pracy"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устраиваться на работу в учреждение или на проект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наниматься",
                                "target_lang_sense_clarification": "поступать на службу"
                            },
                            {
                                "target_lang_word": "устраиваться",
                                "target_lang_sense_clarification": "оформляться на работу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
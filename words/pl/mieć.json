{
    "word_family": [
        "mienie",
        "majątek",
        "majętny",
        "zamożny",
        "niezamożny"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9b769ad2-c51e-3b0e-bdc3-7d824c3a7178",
                    "sense_definition": "Być właścicielem czegoś, dysponować czymś lub posiadać określone cechy fizyczne.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi rodzice <w>mają</w> duży dom na wsi.",
                            "target_lang_translations": {
                                "ru": "У моих родителей <w>есть</w> большой дом в деревне."
                            }
                        },
                        {
                            "text": "Niestety nie <w>mam</w> przy sobie żadnych pieniędzy.",
                            "target_lang_translations": {
                                "ru": "К сожалению, у меня с собой <w>нет</w> денег."
                            }
                        },
                        {
                            "text": "Ona <w>ma</w> piękne, długie włosy i niebieskie oczy.",
                            "target_lang_translations": {
                                "ru": "У нее <w>есть</w> красивые длинные волосы и голубые глаза."
                            }
                        }
                    ],
                    "synonyms": [
                        "posiadać",
                        "dysponować"
                    ],
                    "antonyms": [
                        "nie mieć"
                    ],
                    "common_phrases": [
                        "mieć czas",
                        "mieć pieniądze",
                        "mieć prawo",
                        "mieć na imię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть владельцем чего-либо, располагать чем-либо или обладать определенными физическими чертами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иметь",
                                "target_lang_sense_clarification": "владеть, обладать (в русском языке чаще используется конструкция «у меня есть»)"
                            },
                            {
                                "target_lang_word": "быть у",
                                "target_lang_sense_clarification": "конструкция принадлежности (у кого-либо есть)"
                            },
                            {
                                "target_lang_word": "обладать",
                                "target_lang_sense_clarification": "иметь качество или свойство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bc1af94-439d-3055-b3fc-b5d8ab1e6663",
                    "sense_definition": "Określać wiek osoby, zwierzęcia lub rzeczy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "age",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój młodszy brat <w>ma</w> dopiero pięć lat.",
                            "target_lang_translations": {
                                "ru": "Моему младшему брату всего пять лет."
                            }
                        },
                        {
                            "text": "Ile <w>masz</w> lat?",
                            "target_lang_translations": {
                                "ru": "Сколько тебе лет?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieć lat",
                        "mieć rok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначать возраст человека, животного или предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "в конструкции указания возраста (кому-либо ... лет)"
                            },
                            {
                                "target_lang_word": "исполниться",
                                "target_lang_sense_clarification": "о достижении определенного возраста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "618b68d7-16e9-3d99-a31a-8c2d902ef0dd",
                    "sense_definition": "Odczuwać pewien stan fizyczny, psychiczny lub znajdować się w określonej sytuacji (używane w licznych zwrotach frazeologicznych).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wydaje mi się, że w tej sprawie <w>masz</w> całkowitą rację.",
                            "target_lang_translations": {
                                "ru": "Мне кажется, что в этом деле ты абсолютно <w>прав</w>."
                            }
                        },
                        {
                            "text": "<w>Mam</w> nadzieję, że jutro będzie ładna pogoda.",
                            "target_lang_translations": {
                                "ru": "Я <w>надеюсь</w>, что завтра будет хорошая погода."
                            }
                        },
                        {
                            "text": "Dzieci <w>mają</w> ochotę na lody czekoladowe.",
                            "target_lang_translations": {
                                "ru": "Дети <w>хотят</w> шоколадное мороженое."
                            }
                        }
                    ],
                    "synonyms": [
                        "odczuwać",
                        "czuć"
                    ],
                    "common_phrases": [
                        "mieć rację",
                        "mieć nadzieję",
                        "mieć ochotę",
                        "mieć dość",
                        "mieć znaczenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать физическое или психическое состояние, находиться в определенной ситуации (используется во фразеологизмах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствовать",
                                "target_lang_sense_clarification": "испытывать состояние"
                            },
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "находиться в состоянии (напр., быть правым)"
                            },
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "иметь желание (mieć ochotę)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16be7fb3-b34c-34c0-be4b-16ea62e15e0f",
                    "sense_definition": "Wyraża konieczność, obowiązek lub plan zrobienia czegoś (zastępuje czasownik 'musieć').",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Mamy</w> posprzątać pokój, zanim wrócą rodzice.",
                            "target_lang_translations": {
                                "ru": "Мы <w>должны</w> убрать комнату до возвращения родителей."
                            }
                        },
                        {
                            "text": "Co <w>masz</w> dzisiaj do zrobienia w pracy?",
                            "target_lang_translations": {
                                "ru": "Что ты сегодня <w>должен</w> сделать на работе?"
                            }
                        }
                    ],
                    "synonyms": [
                        "musieć"
                    ],
                    "common_phrases": [
                        "mieć do zrobienia",
                        "mieć iść",
                        "mieć zamiar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать необходимость, обязанность или план действий (заменяет глагол 'musieć')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть должным",
                                "target_lang_sense_clarification": "выражение обязанности"
                            },
                            {
                                "target_lang_word": "предстоять",
                                "target_lang_sense_clarification": "о планах или будущем действии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2260306a-fb54-3aa2-9080-e375cdc2f487",
                    "sense_definition": "Jako „mieć się”: czuć się, znajdować się w określonym stanie zdrowia lub nastroju.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jak się <w>masz</w> po powrocie z wakacji?",
                            "target_lang_translations": {
                                "ru": "Как ты <w>поживаешь</w> после возвращения из отпуска?"
                            }
                        },
                        {
                            "text": "Dziękuję, <w>mam się</w> całkiem dobrze.",
                            "target_lang_translations": {
                                "ru": "Спасибо, я <w>поживаю</w> совсем неплохо."
                            }
                        }
                    ],
                    "synonyms": [
                        "czuć się"
                    ],
                    "common_phrases": [
                        "mieć się dobrze",
                        "mieć się źle",
                        "jak się masz"
                    ],
                    "target_lang_definitions": {
                        "ru": "В возвратной форме «mieć się»: чувствовать себя, находиться в определенном состоянии здоровья или духа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поживать",
                                "target_lang_sense_clarification": "о делах и жизни"
                            },
                            {
                                "target_lang_word": "чувствовать себя",
                                "target_lang_sense_clarification": "о здоровье и самочувствии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb9cb858-0ffb-3f54-ac31-b1417551ac72",
                    "sense_definition": "Posiadać coś w stanie zakończonym lub wykonanym (konstrukcja z imiesłowem biernym).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Mam</w> już napisane zadanie domowe.",
                            "target_lang_translations": {
                                "ru": "У меня уже <w>написано</w> домашнее задание."
                            }
                        },
                        {
                            "text": "Czy <w>macie</w> zaplanowany wyjazd na weekend?",
                            "target_lang_translations": {
                                "ru": "У вас <w>запланирована</w> поездка на выходные?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieć zrobione",
                        "mieć napisane",
                        "mieć zaplanowane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь что-либо в завершенном состоянии (конструкция с пассивным причастием)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иметь",
                                "target_lang_sense_clarification": "в конструкции с причастием (результат действия)"
                            },
                            {
                                "target_lang_word": "быть сделанным",
                                "target_lang_sense_clarification": "пассивная конструкция (у меня сделано)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "989f0a73-0385-365a-808f-bad025716188",
                    "sense_definition": "Uważać kogoś za kogoś innego, przypisywać komuś określoną rolę lub cechy.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy w biurze <w>mieli</w> go za eksperta.",
                            "target_lang_translations": {
                                "ru": "Все в офисе <w>считали</w> его экспертом."
                            }
                        },
                        {
                            "text": "Za kogo ty mnie <w>masz</w>, że tak mnie traktujesz?",
                            "target_lang_translations": {
                                "ru": "За кого ты меня <w>принимаешь</w>, что так со мной обращаешься?"
                            }
                        }
                    ],
                    "synonyms": [
                        "uważać"
                    ],
                    "common_phrases": [
                        "mieć za złe",
                        "mieć za nic",
                        "mieć za głupca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считать кого-либо кем-либо, приписывать кому-либо определенную роль или качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "иметь мнение о ком-то"
                            },
                            {
                                "target_lang_word": "принимать за",
                                "target_lang_sense_clarification": "ошибочно или условно считать кем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4061a4a-9f81-3a9f-a8b6-9daecd18f4b5",
                    "sense_definition": "Służy do przytaczania niesprawdzonych informacji, plotek lub zdarzeń, które rzekomo miały miejsce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reported",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten polityk <w>miał</w> przyjąć dużą łapówkę.",
                            "target_lang_translations": {
                                "ru": "Этот политик, <w>якобы</w>, взял крупную взятку."
                            }
                        },
                        {
                            "text": "Podobno w tym starym zamku <w>miało</w> straszyć.",
                            "target_lang_translations": {
                                "ru": "Говорят, в этом старом замке <w>якобы</w> водились привидения."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Служит для передачи непроверенной информации, слухов или событий, которые якобы произошли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "якобы",
                                "target_lang_sense_clarification": "частица, указывающая на недостоверность"
                            },
                            {
                                "target_lang_word": "говорить",
                                "target_lang_sense_clarification": "передача слухов (говорят, что...)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fecb3146-4aa9-3e70-b791-646cec5db822",
                    "sense_definition": "Jako „mieć się”: odnosić się do czegoś, pozostawać w związku lub relacji z czymś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reflexive_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To, co mówisz, nijak <w>ma się</w> do rzeczywistości.",
                            "target_lang_translations": {
                                "ru": "То, что ты говоришь, никак не <w>относится</w> к действительности."
                            }
                        },
                        {
                            "text": "Jak to rozwiązanie <w>ma się</w> do naszych wcześniejszych ustaleń?",
                            "target_lang_translations": {
                                "ru": "Как это решение <w>соотносится</w> с нашими предыдущими договоренностями?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieć się do",
                        "mieć się nijak"
                    ],
                    "target_lang_definitions": {
                        "ru": "В форме «mieć się»: относиться к чему-либо, иметь связь с чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "относиться",
                                "target_lang_sense_clarification": "иметь отношение, касаться"
                            },
                            {
                                "target_lang_word": "соотноситься",
                                "target_lang_sense_clarification": "быть связанным, соответствовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09c5b74f-f5d5-3d72-8068-f84d8dc57b9b",
                    "sense_definition": "Odbywać stosunek seksualny.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sexual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chwalił się kolegom, że już ją <w>miał</w>.",
                            "target_lang_translations": {
                                "ru": "Он хвастался коллегам, что уже <w>имел</w> ее."
                            }
                        },
                        {
                            "text": "Nigdy nie <w>miał</w> kobiety w ten sposób.",
                            "target_lang_translations": {
                                "ru": "Он никогда не <w>имел</w> женщину таким образом."
                            }
                        }
                    ],
                    "synonyms": [
                        "spać",
                        "kochać się"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "euphemism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершать половой акт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иметь",
                                "target_lang_sense_clarification": "груб., эвфемизм для полового акта"
                            },
                            {
                                "target_lang_word": "спать с",
                                "target_lang_sense_clarification": "разговорное выражение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
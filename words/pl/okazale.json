    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "277f70b3-ca8e-3cee-8184-6e680500feba",
                    "sense_definition": "W sposób charakteryzujący się imponującym rozmiarem, zasięgiem lub imponującą wielkością; wzbudzając podziw swoim wyglądem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trzeba przyznać, że z sumiastym siwym wąsem, wyprostowany i postawny, wyglądał <w>okazale</w>.",
                            "target_lang_translations": {
                                "ru": "Надо признать, что с густыми седыми усами, прямой и статный, он выглядел <w>внушительно</w>."
                            }
                        },
                        {
                            "text": "Zabytkowy ratusz na rynku prezentuje się bardzo <w>okazale</w> po niedawnym remoncie elewacji.",
                            "target_lang_translations": {
                                "ru": "Старинная ратуша на рыночной площади смотрится очень <w>величественно</w> после недавнего ремонта фасада."
                            }
                        }
                    ],
                    "synonyms": [
                        "imponująco",
                        "majestatycznie",
                        "efektownie",
                        "reprezentacyjnie",
                        "wspaniale",
                        "pokaźnie"
                    ],
                    "antonyms": [
                        "skromnie"
                    ],
                    "common_phrases": [
                        "wyglądać okazale",
                        "prezentować się okazale"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, характеризующимся внушительным размером или величием; вызывая восхищение своим внешним видом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внушительно",
                                "target_lang_sense_clarification": "производя сильное впечатление видом или размерами"
                            },
                            {
                                "target_lang_word": "величественно",
                                "target_lang_sense_clarification": "торжественно, исполненный достоинства"
                            },
                            {
                                "target_lang_word": "представительно",
                                "target_lang_sense_clarification": "о солидном, видном внешнем облике человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1dc6bb5c-d78c-35c1-84f5-2adcf75f22e6",
                    "sense_definition": "W sposób wystawny, bogaty, z dużym przepychem.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hrabia żył <w>okazale</w>, ale zawsze z rachubą, co często podkreślali jego współcześni.",
                            "target_lang_translations": {
                                "ru": "Граф жил <w>роскошно</w>, но всегда расчетливо, что часто подчеркивали его современники."
                            }
                        },
                        {
                            "text": "Wesele córki prezydenta zorganizowano niezwykle <w>okazale</w>, zapraszając setki gości z całego świata.",
                            "target_lang_translations": {
                                "ru": "Свадьбу дочери президента организовали необычайно <w>пышно</w>, пригласив сотни гостей со всего мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "wystawnie",
                        "z pompą",
                        "z rozmachem",
                        "królewsko",
                        "przepysznie",
                        "bizantyjsko"
                    ],
                    "antonyms": [
                        "skromnie",
                        "oszczędnie",
                        "umiarkowanie"
                    ],
                    "common_phrases": [
                        "żyć okazale",
                        "świętować okazale",
                        "mieszkać okazale"
                    ],
                    "target_lang_definitions": {
                        "ru": "Роскошно, богато, с большим размахом и пышностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пышно",
                                "target_lang_sense_clarification": "богато, торжественно, с блеском"
                            },
                            {
                                "target_lang_word": "роскошно",
                                "target_lang_sense_clarification": "с большой роскошью"
                            },
                            {
                                "target_lang_word": "с размахом",
                                "target_lang_sense_clarification": "широко, масштабно"
                            }
                        ]
                    }
                }
            ]
        }
    ]

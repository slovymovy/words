            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d4a16eed-1306-32c0-93c4-7034d9e3c7cc",
                    "sense_definition": "Cecha charakteru objawiająca się chęcią sprzeciwiania się, robienia na złość lub postrzegania spraw inaczej niż otoczenie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "character_traits",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego wrodzona <w>przekorność</w> nie pozwalała mu zgodzić się z większością grupy.",
                            "target_lang_translations": {
                                "ru": "Его врожденная <w>строптивость</w> не позволяла ему согласиться с большинством группы."
                            }
                        },
                        {
                            "text": "Mimo próśb rodziców, mała Zosia okazywała swoją <w>przekorność</w> przy każdym posiłku.",
                            "target_lang_translations": {
                                "ru": "Несмотря на просьбы родителей, маленькая Зося проявляла свое <w>своенравие</w> за каждым приемом пищи."
                            }
                        }
                    ],
                    "synonyms": [
                        "przekora"
                    ],
                    "antonyms": [
                        "potulność"
                    ],
                    "common_phrases": [
                        "wrodzona przekorność",
                        "dziecięca przekorność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Черта характера, проявляющаяся в желании противоречить, делать назло или воспринимать вещи иначе, чем окружающие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строптивость",
                                "target_lang_sense_clarification": "непокорность, упрямство"
                            },
                            {
                                "target_lang_word": "своенравие",
                                "target_lang_sense_clarification": "склонность поступать по-своему, капризность"
                            },
                            {
                                "target_lang_word": "дух противоречия",
                                "target_lang_sense_clarification": "стремление спорить и делать наоборот"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68769043-256a-37d4-bcfb-2b11e6b1acde",
                    "sense_definition": "Cecha zdarzeń lub przedmiotów, które układają się niepomyślnie, wbrew ludzkim planom i oczekiwaniom, jakby celowo stawiały opór.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fate_and_luck",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To była typowa <w>przekorność</w> losu, że deszcz spadł dokładnie w dniu naszego pikniku.",
                            "target_lang_translations": {
                                "ru": "Это была типичная <w>ирония</w> судьбы, что дождь пошел именно в день нашего пикника."
                            }
                        },
                        {
                            "text": "Czasami mam wrażenie, że <w>przekorność</w> rzeczy martwych to nie tylko przesąd, ale fakt.",
                            "target_lang_translations": {
                                "ru": "Иногда мне кажется, что <w>коварство</w> неодушевленных предметов — это не просто суеверие, а факт."
                            }
                        }
                    ],
                    "synonyms": [
                        "przekora"
                    ],
                    "common_phrases": [
                        "przekorność losu",
                        "przekorność rzeczy martwych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство событий или предметов складываться неблагоприятно, вопреки планам, словно оказывая сопротивление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ирония",
                                "target_lang_sense_clarification": "в контексте судьбы (ирония судьбы)"
                            },
                            {
                                "target_lang_word": "коварство",
                                "target_lang_sense_clarification": "скрытая зловредность обстоятельств или предметов"
                            },
                            {
                                "target_lang_word": "превратность",
                                "target_lang_sense_clarification": "изменчивость и непредсказуемость судьбы"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                                "target_lang_word": "двигаю",
                                "target_lang_sense_clarification": "перемещаю что-либо"
                            },
                            {
                                "target_lang_word": "пододвигаю",
                                "target_lang_sense_clarification": "придвигаю ближе"
                            },
                            {
                                "target_lang_word": "задергиваю",
                                "target_lang_sense_clarification": "о шторах"

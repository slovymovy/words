    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "35ccd192-b669-36e4-a839-676cfa55ca08",
                    "sense_definition": "The standard of something as measured against other things of a similar kind; the degree of excellence of something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "excellence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our company is famous for the high <w>quality</w> of its products.",
                            "target_lang_translations": {
                                "ru": "Наша компания славится высоким <w>качеством</w> своей продукции."
                            }
                        },
                        {
                            "text": "Air <w>quality</w> in the city has improved significantly this year.",
                            "target_lang_translations": {
                                "ru": "<w>Качество</w> воздуха в городе значительно улучшилось в этом году."
                            }
                        }
                    ],
                    "synonyms": [
                        "standard",
                        "grade",
                        "class",
                        "caliber",
                        "excellence"
                    ],
                    "antonyms": [
                        "inferiority"
                    ],
                    "common_phrases": [
                        "high quality",
                        "poor quality",
                        "top quality",
                        "air quality",
                        "quality of life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Степень совершенства или ценности чего-либо; соответствие определенным стандартам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ка́чество",
                                "target_lang_sense_clarification": "степень совершенства, сорт"
                            },
                            {
                                "target_lang_word": "у́ровень",
                                "target_lang_sense_clarification": "в контексте степени развития (напр. уровень жизни)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cee43b82-36c7-38e9-a558-e5872a1f7ee6",
                    "sense_definition": "A distinctive attribute, characteristic, or feature possessed by someone or something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attribute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Patience is a necessary <w>quality</w> for a good teacher.",
                            "target_lang_translations": {
                                "ru": "Терпение — необходимое <w>качество</w> для хорошего учителя."
                            }
                        },
                        {
                            "text": "One of his best <w>qualities</w> is his willingness to help others.",
                            "target_lang_translations": {
                                "ru": "Одно из его лучших <w>качеств</w> — готовность помочь другим."
                            }
                        }
                    ],
                    "synonyms": [
                        "characteristic",
                        "feature",
                        "trait",
                        "attribute",
                        "property"
                    ],
                    "common_phrases": [
                        "leadership qualities",
                        "personal qualities",
                        "redeeming quality",
                        "star quality"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличительная черта, свойство или особенность, присущая кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ка́чество",
                                "target_lang_sense_clarification": "черта характера, особенность"
                            },
                            {
                                "target_lang_word": "сво́йство",
                                "target_lang_sense_clarification": "характеристика, атрибут"
                            },
                            {
                                "target_lang_word": "черта́",
                                "target_lang_sense_clarification": "отличительная особенность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cbcc246-1cba-3bb9-a397-d6aba5d2b28c",
                    "sense_definition": "A newspaper with a serious tone and high standards of journalism.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "newspaper",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He prefers reading the <w>qualities</w> rather than the tabloids on Sunday.",
                            "target_lang_translations": {
                                "ru": "По воскресеньям он предпочитает читать <w>серьезные газеты</w>, а не таблоиды."
                            }
                        },
                        {
                            "text": "The gap between the popular press and the <w>qualities</w> has narrowed.",
                            "target_lang_translations": {
                                "ru": "Разрыв между массовой прессой и <w>качественными изданиями</w> сократился."
                            }
                        }
                    ],
                    "synonyms": [
                        "broadsheet"
                    ],
                    "antonyms": [
                        "tabloid"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in the plural form 'the qualities'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Газета с серьезным тоном и высокими стандартами журналистики (в отличие от таблоидов)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серьёзная газе́та",
                                "target_lang_sense_clarification": "качественное печатное издание"
                            },
                            {
                                "target_lang_word": "ка́чественная пре́сса",
                                "target_lang_sense_clarification": "собирательное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b915214-0f16-3ab8-b348-2f42c073cf66",
                    "sense_definition": "High social position or people of high social standing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "social_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the past, a person of <w>quality</w> was expected to behave with great dignity.",
                            "target_lang_translations": {
                                "ru": "В прошлом от человека <w>знатного происхождения</w> ожидалось поведение с большим достоинством."
                            }
                        },
                        {
                            "text": "The club was originally exclusive to men of <w>quality</w> and wealth.",
                            "target_lang_translations": {
                                "ru": "Клуб изначально был доступен только людям <w>знатным</w> и богатым."
                            }
                        }
                    ],
                    "synonyms": [
                        "nobility",
                        "aristocracy"
                    ],
                    "antonyms": [
                        "commoners"
                    ],
                    "common_phrases": [
                        "lady of quality",
                        "man of quality",
                        "people of quality"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical or literary context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокое общественное положение или принадлежность к знати (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зна́тность",
                                "target_lang_sense_clarification": "принадлежность к знатному роду"
                            },
                            {
                                "target_lang_word": "высо́кое положе́ние",
                                "target_lang_sense_clarification": "социальный статус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d58eda45-40ab-391b-a260-fb477a460563",
                    "sense_definition": "A specific role, capacity, or official status.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He attended the conference in his <w>quality</w> as the chairman.",
                            "target_lang_translations": {
                                "ru": "Он присутствовал на конференции в <w>качестве</w> председателя."
                            }
                        },
                        {
                            "text": "She signed the document in the <w>quality</w> of a witness.",
                            "target_lang_translations": {
                                "ru": "Она подписала документ в <w>качестве</w> свидетеля."
                            }
                        }
                    ],
                    "synonyms": [
                        "capacity",
                        "role",
                        "function",
                        "status"
                    ],
                    "common_phrases": [
                        "in the quality of"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly replaced by 'capacity' or 'role' in modern English"
                        },
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфическая роль, должность или официальный статус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ка́чество",
                                "target_lang_sense_clarification": "в значении 'роль' (обычно в фразе 'в качестве')"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "04f2bb25-ff79-377a-82c7-12a9327fbc5d",
                    "sense_definition": "Of good worth, well made, or excellent; fit for a specific purpose.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "excellence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We spent some <w>quality</w> time together over the weekend.",
                            "target_lang_translations": {
                                "ru": "Мы <w>отлично</w> провели время вместе в выходные."
                            }
                        },
                        {
                            "text": "The shop sells <w>quality</w> furniture at reasonable prices.",
                            "target_lang_translations": {
                                "ru": "Магазин продает <w>качественную</w> мебель по разумным ценам."
                            }
                        }
                    ],
                    "synonyms": [
                        "excellent",
                        "superior",
                        "fine",
                        "premium",
                        "high-class"
                    ],
                    "antonyms": [
                        "poor",
                        "inferior",
                        "cheap"
                    ],
                    "common_phrases": [
                        "quality time",
                        "quality control",
                        "quality assurance",
                        "quality product"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокого качества, добротный, отличный; подходящий для определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ка́чественный",
                                "target_lang_sense_clarification": "добротный, хороший"
                            },
                            {
                                "target_lang_word": "высокока́чественный",
                                "target_lang_sense_clarification": "высшего сорта"
                            },
                            {
                                "target_lang_word": "первокла́ссный",
                                "target_lang_sense_clarification": "отличный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

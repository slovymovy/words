                    ],
                    "antonyms": [
                        "formeel"
                    ],
                    "common_phrases": [
                        "informele sfeer",
                        "informele sector",
                        "informele zorg"
                    ],
                    "target_lang_definitions": {
                        "en": "Not official; without strict rules or ceremonies.",
                        "ru": "Неофициальный; без строгих правил или церемоний."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "informal",
                                "target_lang_sense_clarification": "not official or ceremonial"

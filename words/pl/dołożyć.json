            "senses": [
                {
                    "sense_id": "d223828b-ea37-3d6a-9006-ed9bb076045c",
                    "sense_definition": "Dodać coś do czegoś; umieścić dodatkową ilość czegoś w jakimś miejscu lub zbiorze.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_add",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy mogę ci jeszcze <dołożyć> ziemniaków na talerz?",
                            "target_lang_translations": {
                                "ru": "Могу я тебе еще <положить> картошки на тарелку?"
                            }
                        },
                        {
                            "text": "Musisz <dołożyć> drewna do ogniska, żeby ogień nie zgasł.",
                            "target_lang_translations": {
                                "ru": "Ты должен <подбросить> дров в костер, чтобы огонь не погас."
                            }
                        }
                    ],
                    "synonyms": [
                        "dodać",
                        "dorzucić",
                        "uzupełnić",
                        "dokładać"
                    ],
                    "antonyms": [
                        "odjąć",
                        "ująć",
                        "zabrać"
                    ],
                    "common_phrases": [
                        "dołożyć do pieca",
                        "dołożyć jedzenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавить что-то к чему-то; поместить дополнительное количество чего-либо в какое-то место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добавить",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "подложить",
                                "target_lang_sense_clarification": "дрова, вещи"
                            },
                            {
                                "target_lang_word": "положить",
                                "target_lang_sense_clarification": "еду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d909151-9bfa-3961-9ef3-97bd83c31ba9",
                    "sense_definition": "Wpłacić dodatkowe pieniądze lub pokryć część kosztów w ramach składki na wspólny cel.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_add",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musieliśmy <dołożyć> brakującą kwotę z własnej kieszeni.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <доплатить> недостающую сумму из собственного кармана."
                            }
                        },
                        {
                            "text": "Chcę się <dołożyć> do prezentu urodzinowego dla szefa.",
                            "target_lang_translations": {
                                "ru": "Я хочу <скинуться> на подарок ко дню рождения начальника."
                            }
                        }
                    ],
                    "synonyms": [
                        "dopłacić",
                        "dorzucić się"
                    ],
                    "common_phrases": [
                        "dołożyć się do prezentu",
                        "dołożyć z własnej kieszeni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внести дополнительные деньги или покрыть часть расходов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добавить",
                                "target_lang_sense_clarification": "деньги"
                            },
                            {
                                "target_lang_word": "доплатить",
                                "target_lang_sense_clarification": "недостающую сумму"
                            },
                            {
                                "target_lang_word": "внести",
                                "target_lang_sense_clarification": "вклад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a288afee-719e-31dd-9a73-b0d1a19341a2",
                    "sense_definition": "Wykonać duży wysiłek w celu osiągnięcia czegoś (używane niemal wyłącznie w połączeniu z rzeczownikiem „starań”).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_effort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obiecuję <dołożyć> wszelkich starań, aby projekt zakończył się sukcesem.",
                            "target_lang_translations": {
                                "ru": "Обещаю <приложить> все усилия, чтобы проект завершился успешно."
                            }
                        },
                        {
                            "text": "Lekarze musieli <dołożyć> starań, by uratować pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врачам пришлось <приложить> усилия, чтобы спасти пациента."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dołożyć wszelkich starań",
                        "dołożyć starań"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приложить большие усилия для достижения чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приложить",
                                "target_lang_sense_clarification": "усилия, старания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cca4ded-c335-3ca8-b5fb-d9db222f983f",
                    "sense_definition": "Uderzyć kogoś mocno lub pobić; wyrządzić komuś krzywdę fizyczną.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bał się wracać nocą, żeby chuligani nie chcieli mu <dołożyć>.",
                            "target_lang_translations": {
                                "ru": "Он боялся возвращаться ночью, чтобы хулиганы не захотели ему <навалять>."
                            }
                        },
                        {
                            "text": "Jeśli nie przestaniesz mnie obrażać, mogę ci <dołożyć>.",
                            "target_lang_translations": {
                                "ru": "Если не перестанешь меня оскорблять, я могу тебе <врезать>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyłożyć",
                        "dowalić",
                        "uderzyć",
                        "pobić",
                        "walnąć",
                        "grzmotnąć"

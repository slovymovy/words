                },
                {
                    "sense_id": "9ac2afc8-78fc-3fd8-8cab-3121ae204dff",
                    "sense_definition": "A colloquial or dialectal form of <w>one</w>, referring to a specific thing previously mentioned or implied.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "referring_to_one",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That joke was a good <w>'un</w>, and it made everyone in the room laugh.",
                            "target_lang_translations": {
                                "ru": "Эта шутка была отличной <w>штукой</w>, и она рассмешила всех в комнате."
                            }
                        },
                        {
                            "text": "I prefer the red apples, so please pass me a ripe <w>'un</w> from the bowl.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю красные яблоки, поэтому передай мне, пожалуйста, спелое <w>яблоко</w> из вазы."
                            }
                        }
                    ],
                    "synonyms": [
                        "one"
                    ],
                    "common_phrases": [

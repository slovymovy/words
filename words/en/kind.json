{
    "word_family": [
        "kindness",
        "kindly",
        "unkind",
        "mankind",
        "humankind",
        "kindred"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "629d5f4f-4af0-3c01-871d-3890e22792bf",
                    "sense_definition": "Having a friendly, generous, and considerate nature; showing concern for the well-being of others.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "benevolent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a very <w>kind</w> person who always helps her neighbors.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>добрый</w> человек, который всегда помогает своим соседям."
                            }
                        },
                        {
                            "text": "It was very <w>kind</w> of you to invite us to your home.",
                            "target_lang_translations": {
                                "ru": "Было очень <w>любезно</w> с вашей стороны пригласить нас к себе домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "child",
                        "generation",
                        "offspring",
                        "derivative",
                        "genre",
                        "sort",
                        "type"
                    ],
                    "antonyms": [
                        "cruel",
                        "mean",
                        "unkind"
                    ],
                    "common_phrases": [
                        "be kind to",
                        "kind of you",
                        "kind regards",
                        "kind heart"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий дружелюбный, щедрый и внимательный характер; проявляющий заботу о благополучии других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добрый",
                                "target_lang_sense_clarification": "отзывчивый, хороший"
                            },
                            {
                                "target_lang_word": "любезный",
                                "target_lang_sense_clarification": "вежливый, учтивый"
                            },
                            {
                                "target_lang_word": "доброжелательный",
                                "target_lang_sense_clarification": "желающий добра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ea03505-8125-343f-a07a-2b9154b09968",
                    "sense_definition": "Mild, gentle, or not harsh in effect or action.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "benevolent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The weather has been <w>kind</w> to us, with plenty of sunshine all week.",
                            "target_lang_translations": {
                                "ru": "Погода была к нам <w>благосклонна</w>, и всю неделю светило солнце."
                            }
                        },
                        {
                            "text": "You should use a soap that is <w>kind</w> to sensitive skin.",
                            "target_lang_translations": {
                                "ru": "Вам следует использовать мыло, <w>мягкое</w> для чувствительной кожи."
                            }
                        }
                    ],
                    "synonyms": [
                        "child",
                        "generation",
                        "offspring",
                        "derivative",
                        "genre",
                        "sort",
                        "type"
                    ],
                    "antonyms": [
                        "harsh",
                        "severe"
                    ],
                    "common_phrases": [
                        "kind to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мягкий, щадящий или не резкий по воздействию или действию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мягкий",
                                "target_lang_sense_clarification": "не суровый, щадящий"
                            },
                            {
                                "target_lang_word": "благоприятный",
                                "target_lang_sense_clarification": "способствующий успеху (о погоде, условиях)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "892cfb9f-3f8f-3887-873d-c1db039128b1",
                    "sense_definition": "A category of things or people that share common characteristics.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "category",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "What <w>kind</w> of music do you like to listen to?",
                            "target_lang_translations": {
                                "ru": "Какой <w>тип</w> музыки тебе нравится слушать?"
                            }
                        },
                        {
                            "text": "There are many different <w>kinds</w> of apples in the supermarket.",
                            "target_lang_translations": {
                                "ru": "В супермаркете много разных <w>сортов</w> яблок."
                            }
                        }
                    ],
                    "synonyms": [
                        "type",
                        "sort",
                        "genre"
                    ],
                    "common_phrases": [
                        "kind of",
                        "all kinds of",
                        "one of a kind",
                        "what kind of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Категория вещей или людей, обладающих общими характеристиками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "разновидность, тип"
                            },
                            {
                                "target_lang_word": "тип",
                                "target_lang_sense_clarification": "образец, модель"
                            },
                            {
                                "target_lang_word": "сорт",
                                "target_lang_sense_clarification": "категория качества (о товарах, продуктах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9115ecf8-ced3-3f25-8be8-d90c07bd105a",
                    "sense_definition": "Used to describe something that is roughly or partly true, or to refer to a vague or imprecise example of something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "category",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a <w>kind</w> of blue color, but much darker than the sky.",
                            "target_lang_translations": {
                                "ru": "Это был <w>своего рода</w> синий цвет, но гораздо темнее неба."
                            }
                        },
                        {
                            "text": "He felt a <w>kind</w> of sadness when he watched the movie.",
                            "target_lang_translations": {
                                "ru": "Он почувствовал <w>некую</w> грусть, когда смотрел фильм."
                            }
                        }
                    ],
                    "synonyms": [
                        "sort",
                        "type"
                    ],
                    "common_phrases": [
                        "a kind of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для описания чего-то, что верно лишь отчасти, или для обозначения неточного примера чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "своего рода",
                                "target_lang_sense_clarification": "некая разновидность"
                            },
                            {
                                "target_lang_word": "нечто вроде",
                                "target_lang_sense_clarification": "подобие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f331301-f753-3101-8c14-5edfc140d740",
                    "sense_definition": "Goods or services used as payment instead of money.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "in_kind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The villagers had no cash, so they paid their taxes in <w>kind</w> with grain and livestock.",
                            "target_lang_translations": {
                                "ru": "У жителей деревни не было наличных, поэтому они заплатили налоги <w>натурой</w> — зерном и скотом."
                            }
                        },
                        {
                            "text": "The company offered benefits in <w>kind</w>, such as a company car and free meals.",
                            "target_lang_translations": {
                                "ru": "Компания предложила льготы в <w>натуральной форме</w>, такие как служебный автомобиль и бесплатное питание."
                            }
                        }
                    ],
                    "common_phrases": [
                        "payment in kind",
                        "benefit in kind"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "idiomatic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Товары или услуги, используемые в качестве оплаты вместо денег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "натура",
                                "target_lang_sense_clarification": "товары/продукты как средство оплаты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9e40abf-de76-3e29-8cde-4cef4d3fcbf1",
                    "sense_definition": "The same manner or way; an equivalent action given in return.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "in_kind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She insulted him publicly, and he decided to respond in <w>kind</w>.",
                            "target_lang_translations": {
                                "ru": "Она публично оскорбила его, и он решил ответить <w>тем же</w>."
                            }
                        },
                        {
                            "text": "If you are polite to customers, they will usually treat you in <w>kind</w>.",
                            "target_lang_translations": {
                                "ru": "Если вы вежливы с клиентами, они обычно отвечают вам <w>тем же</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "respond in kind"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "idiomatic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот же способ или манера; равноценное действие, совершенное в ответ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тем же",
                                "target_lang_sense_clarification": "таким же образом"
                            },
                            {
                                "target_lang_word": "той же монетой",
                                "target_lang_sense_clarification": "идиома: равноценным ответом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19f463c6-b445-3885-b0a4-972649cd66c1",
                    "sense_definition": "The inherent nature or essential character of something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two problems differ in degree, but not in <w>kind</w>.",
                            "target_lang_translations": {
                                "ru": "Эти две проблемы различаются по степени, но не по <w>сути</w>."
                            }
                        },
                        {
                            "text": "Human beings are distinct in <w>kind</w> from other animals due to their ability to reason.",
                            "target_lang_translations": {
                                "ru": "Люди отличаются по своей <w>природе</w> от других животных благодаря способности мыслить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "difference in kind"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренняя природа или сущностный характер чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "природа",
                                "target_lang_sense_clarification": "естество, сущность"
                            },
                            {
                                "target_lang_word": "сущность",
                                "target_lang_sense_clarification": "внутреннее содержание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "abonneren",
        "abonnee"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dfc5e82a-6fdb-37fa-976c-30f8f2296c70",
                    "sense_definition": "Een overeenkomst waarbij een persoon op geregelde tijden een tijdschrift, krant of digitale dienst ontvangt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij hebben al jaren een <w>abonnement</w> op de krant.",
                            "target_lang_translations": {
                                "ru": "У нас уже много лет <w>подписка</w> на газету."
                            }
                        },
                        {
                            "text": "Zij nam een <w>abonnement</w> op een tijdschrift over tuinieren.",
                            "target_lang_translations": {
                                "ru": "Она оформила <w>подписку</w> на журнал о садоводстве."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een abonnement nemen",
                        "een abonnement opzeggen",
                        "abonnement op een tijdschrift"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соглашение, по которому человек регулярно получает журнал, газету или доступ к цифровой услуге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подписка",
                                "target_lang_sense_clarification": "на газеты, журналы, онлайн-сервисы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edc1e311-2983-3c01-8209-b1388b40ebb2",
                    "sense_definition": "Een regeling waarbij een periodieke betaling recht geeft op onbeperkt gebruik van een dienst, zoals openbaar vervoer of een sportfaciliteit.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft een <w>abonnement</w> op het zwembad om elke ochtend te kunnen zwemmen.",
                            "target_lang_translations": {
                                "ru": "У него есть <w>абонемент</w> в бассейн, чтобы плавать каждое утро."
                            }
                        },
                        {
                            "text": "Met dit <w>abonnement</w> kun je in het weekend gratis met de trein reizen.",
                            "target_lang_translations": {
                                "ru": "С этим <w>абонементом</w> можно бесплатно ездить на поезде по выходным."
                            }
                        }
                    ],
                    "synonyms": [
                        "jaarkaart",
                        "seizoenkaart"
                    ],
                    "common_phrases": [
                        "abonnement verlengen",
                        "abonnement voor de trein"
                    ],
                    "target_lang_definitions": {
                        "ru": "Право на пользование услугой (например, транспортом или спортивным сооружением) в течение определенного срока за плату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "абонемент",
                                "target_lang_sense_clarification": "в спортзал, бассейн, театр"
                            },
                            {
                                "target_lang_word": "проездной",
                                "target_lang_sense_clarification": "на общественный транспорт"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
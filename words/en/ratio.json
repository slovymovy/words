    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7959778c-1893-39fc-be72-0f2d702fe1d4",
                    "sense_definition": "A comparison between two numbers or amounts that shows how much bigger one is than the other, or the relationship between them.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "math_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>ratio</w> of men to women in the department is two to one.",
                            "target_lang_translations": {
                                "ru": "<w>Соотношение</w> мужчин и женщин в отделе — два к одному."
                            }
                        },
                        {
                            "text": "They want to improve the teacher-student <w>ratio</w> by hiring more staff.",
                            "target_lang_translations": {
                                "ru": "Они хотят улучшить <w>соотношение</w> учителей и учеников, наняв больше сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "proportion",
                        "correlation",
                        "relationship"
                    ],
                    "common_phrases": [
                        "sex ratio",
                        "aspect ratio",
                        "ratio of",
                        "in direct ratio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сравнение двух чисел или величин, показывающее, во сколько раз одно больше другого, или взаимосвязь между ними."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соотношение",
                                "target_lang_sense_clarification": "взаимная связь величин"
                            },
                            {
                                "target_lang_word": "отношение",
                                "target_lang_sense_clarification": "математическое частное"
                            },
                            {
                                "target_lang_word": "пропорция",
                                "target_lang_sense_clarification": "соразмерность частей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0af86a62-b7ca-3394-9de8-21ac37d4c882",
                    "sense_definition": "A situation on social media where a post receives significantly more replies or negative comments than likes, usually indicating strong disagreement.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_media_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The controversial tweet had a terrible <w>ratio</w>, showing that most people disliked it.",
                            "target_lang_translations": {
                                "ru": "У этого скандального твита было ужасное <w>соотношение</w> реакций, показывающее, что большинству он не понравился."
                            }
                        },
                        {
                            "text": "He deleted his comment to avoid a bad <w>ratio</w> from the community.",
                            "target_lang_translations": {
                                "ru": "Он удалил свой комментарий, чтобы избежать плохого <w>рейшио</w> со стороны сообщества."
                            }
                        }
                    ],
                    "common_phrases": [
                        "have a ratio"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация в соцсетях, когда пост получает значительно больше ответов или негативных комментариев, чем лайков, что обычно указывает на несогласие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дисбаланс реакций",
                                "target_lang_sense_clarification": "описательный перевод"
                            },
                            {
                                "target_lang_word": "рейшио",
                                "target_lang_sense_clarification": "интернет-сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4dd0a470-da31-3fb6-bb1b-5889c1c3f7c7",
                    "sense_definition": "To react to a social media post with a large number of negative comments or replies, often outnumbering the likes, in order to mock or disagree with the poster.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_media_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The politician got <w>ratioed</w> immediately after posting his unpopular opinion.",
                            "target_lang_translations": {
                                "ru": "Политика сразу же <w>задизлайкали</w> после того, как он опубликовал свое непопулярное мнение."
                            }
                        },
                        {
                            "text": "Users decided to <w>ratio</w> the company's insensitive advertisement.",
                            "target_lang_translations": {
                                "ru": "Пользователи решили <w>завалить</w> неуместную рекламу компании <w>критикой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get ratioed"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отреагировать на пост в соцсетях большим количеством негативных комментариев, число которых превышает количество лайков, с целью насмешки или выражения несогласия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задизлайкать",
                                "target_lang_sense_clarification": "сленг: выразить массовое неодобрение"
                            },
                            {
                                "target_lang_word": "завалить критикой",
                                "target_lang_sense_clarification": "массово критиковать в комментариях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "501eeeb5-c0c0-3e0e-bccb-2cd2c5392a75",
                    "sense_definition": "Used to express mockery or disagreement with a social media post, implying the post is bad or invalid.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "social_media_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Someone simply commented \"<w>ratio</w>\" under his bad take instead of arguing.",
                            "target_lang_translations": {
                                "ru": "Кто-то просто написал «<w>слит</w>» под его глупым мнением вместо того, чтобы спорить."
                            }
                        },
                        {
                            "text": "He replied with \"L + <w>ratio</w>\" to show his complete lack of respect for the statement.",
                            "target_lang_translations": {
                                "ru": "Он ответил «L + <w>рейшио</w>», чтобы показать полное неуважение к заявлению."
                            }
                        }
                    ],
                    "common_phrases": [
                        "L + ratio"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения насмешки или несогласия с постом в соцсетях, подразумевая, что пост плохой или неверный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слит",
                                "target_lang_sense_clarification": "сленг: ты проиграл спор"
                            },
                            {
                                "target_lang_word": "рейшио",
                                "target_lang_sense_clarification": "интернет-сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]

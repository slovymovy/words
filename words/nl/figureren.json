    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "de4cdcc7-4131-3f23-9958-82870747afe2",
                    "sense_definition": "Optreden als figurant in een film, toneelstuk of televisieprogramma, meestal op de achtergrond en zonder tekst.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "acting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij mocht <w>figureren</w> in de nieuwe film van haar favoriete regisseur.",
                            "target_lang_translations": {
                                "en": "She was allowed to <w>appear as an extra</w> in her favorite director's new film.",
                                "ru": "Ей разрешили <w>сняться в массовке</w> в новом фильме её любимого режиссёра."
                            }
                        },
                        {
                            "text": "Veel studenten <w>figureren</w> voor een kleine vergoeding in reclamespotjes.",
                            "target_lang_translations": {
                                "en": "Many students <w>act as extras</w> in commercials for a small fee.",
                                "ru": "Многие студенты за небольшую плату <w>снимаются в массовке</w> в рекламных роликах."
                            }
                        }
                    ],
                    "synonyms": [
                        "meespelen"
                    ],
                    "antonyms": [
                        "de hoofdrol spelen"
                    ],
                    "common_phrases": [
                        "in een film figureren"
                    ],
                    "target_lang_definitions": {
                        "en": "To act as an extra in a film, play, or television program, usually in the background and without lines.",
                        "ru": "Исполнять роль статиста в фильме, пьесе или телепрограмме, обычно на заднем плане и без слов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appear as an extra",
                                "target_lang_sense_clarification": "perform a background role"
                            },
                            {
                                "target_lang_word": "act as an extra",
                                "target_lang_sense_clarification": "perform a non-speaking role"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сниматься в массовке",
                                "target_lang_sense_clarification": "участвовать в массовых сценах"
                            },
                            {
                                "target_lang_word": "быть статистом",
                                "target_lang_sense_clarification": "играть роль без слов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdf1d7e0-5588-3a02-b517-c02d927c22c8",
                    "sense_definition": "Genoemd worden of aanwezig zijn in een document, lijst of verhaal; een rol spelen in een groter geheel.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "appearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn naam <w>figureert</w> op de lijst van verdachten in deze zaak.",
                            "target_lang_translations": {
                                "en": "His name <w>appears</w> on the list of suspects in this case.",
                                "ru": "Его имя <w>фигурирует</w> в списке подозреваемых по этому делу."
                            }
                        },
                        {
                            "text": "In dit historische boek <w>figureren</w> verschillende bekende politici.",
                            "target_lang_translations": {
                                "en": "Various well-known politicians <w>feature</w> in this historical book.",
                                "ru": "В этой исторической книге <w>фигурируют</w> различные известные политики."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorkomen",
                        "verschijnen",
                        "vermeld staan"
                    ],
                    "antonyms": [
                        "ontbreken"
                    ],
                    "common_phrases": [
                        "op de lijst figureren",
                        "in het dossier figureren"
                    ],
                    "target_lang_definitions": {
                        "en": "To be mentioned or present in a document, list, or story; to play a part in a larger context.",
                        "ru": "Упоминаться или присутствовать в документе, списке или истории; играть роль в каком-либо деле."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "be present in a list or document"
                            },
                            {
                                "target_lang_word": "feature",
                                "target_lang_sense_clarification": "have a prominent part"
                            },
                            {
                                "target_lang_word": "figure",
                                "target_lang_sense_clarification": "be mentioned or involved"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "фигурировать",
                                "target_lang_sense_clarification": "упоминаться или присутствовать"
                            },
                            {
                                "target_lang_word": "значиться",
                                "target_lang_sense_clarification": "быть указанным в списке"
                            },
                            {
                                "target_lang_word": "упоминаться",
                                "target_lang_sense_clarification": "быть названным"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "18e48596-63ea-366b-86ea-7b7332e2ee22",
                    "sense_definition": "A dish or foodstuff where at least one piece, but typically two or more pieces, of bread serves as the wrapper or container of some other food.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I packed a cheese <w>sandwich</w> for my lunch.",
                            "target_lang_translations": {
                                "ru": "Я взял с собой <w>бутерброд</w> с сыром на обед."
                            }
                        },
                        {
                            "text": "She ordered a chicken <w>sandwich</w> and a cup of coffee.",
                            "target_lang_translations": {
                                "ru": "Она заказала <w>сэндвич</w> с курицей и чашку кофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "sarnie",
                        "butty",
                        "grinder",
                        "sammich"
                    ],
                    "common_phrases": [
                        "ham sandwich",
                        "cheese sandwich",
                        "make a sandwich",
                        "sandwich shop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Блюдо, состоящее из двух или более ломтиков хлеба с начинкой (мясом, сыром и т.д.) между ними."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сэндвич",
                                "target_lang_sense_clarification": "закрытый бутерброд"
                            },
                            {
                                "target_lang_word": "бутерброд",
                                "target_lang_sense_clarification": "закуска на хлебе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c08025a9-bd41-3d33-b1e3-1e4cd7ba1f0b",
                    "sense_definition": "Any combination formed by layering one type of material between two layers of some other material.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "layer_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wall is a <w>sandwich</w> of foam insulation between two concrete slabs.",
                            "target_lang_translations": {
                                "ru": "Стена представляет собой <w>сэндвич</w> из пеноизоляции между двумя бетонными плитами."
                            }
                        },
                        {
                            "text": "This material is a strong <w>sandwich</w> structure used in aircraft.",
                            "target_lang_translations": {
                                "ru": "Этот материал представляет собой прочную <w>сэндвич</w>-структуру, используемую в авиации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sandwich structure",
                        "sandwich material"
                    ],
                    "target_lang_definitions": {
                        "ru": "Многослойная конструкция, в которой один материал помещен между двумя слоями другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сэндвич",
                                "target_lang_sense_clarification": "многослойная структура"
                            },
                            {
                                "target_lang_word": "многослойная панель",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8614a97-6369-3947-a398-c7ca38f03455",
                    "sense_definition": "A layer cake or sandwich cake.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "food_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She baked a delicious Victoria <w>sandwich</w> for the tea party.",
                            "target_lang_translations": {
                                "ru": "Она испекла вкусный Викторианский <w>бисквит</w> к чаепитию."
                            }
                        },
                        {
                            "text": "The sponge <w>sandwich</w> was filled with strawberry jam and cream.",
                            "target_lang_translations": {
                                "ru": "Бисквитный <w>торт</w> был с начинкой из клубничного джема и крема."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Victoria sandwich"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность бисквитного торта, состоящего из двух коржей с прослойкой джема или крема."

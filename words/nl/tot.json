                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "telephone_intro",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hallo, u spreekt <tot> Jan.",
                            "target_lang_translations": {
                                "en": "Hello, you are speaking <w>to</w> Jan.",
                                "ru": "Алло, вы говорите <с> Яном."

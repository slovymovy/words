                },
                {
                    "sense_id": "dcc5efb1-2732-3b5e-ba21-83bdd8f7ec4e",
                    "sense_definition": "Designed to use very little computer memory or processing power.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This <w>lightweight</w> browser loads web pages much faster than the old one.",
                            "target_lang_translations": {
                                "ru": "Этот <w>легковесный</w> браузер загружает веб-страницы намного быстрее старого."
                            }
                        },
                        {
                            "text": "Developers prefer <w>lightweight</w> code for mobile applications to save battery life.",
                            "target_lang_translations": {
                                "ru": "Разработчики предпочитают <w>легковесный</w> код для мобильных приложений, чтобы экономить заряд батареи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lightweight app",
                        "lightweight software"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разработанный для использования малого объема памяти или вычислительной мощности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "легковесный",
                                "target_lang_sense_clarification": "о программном обеспечении"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "20e7e9bc-4d18-3289-87ef-9e3e01faa715",
                    "sense_definition": "Временное место размещения людей вне населённых пунктов для отдыха, спорта или военных учений.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "place_temporary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждое лето родители отправляли меня в детский оздоровительный <w>лагерь</w> на берегу моря.",
                            "target_lang_translations": {
                                "en": "Every summer my parents sent me to a children's health <w>camp</w> on the seashore."
                            }
                        },
                        {
                            "text": "Туристы решили разбить <w>лагерь</w> на большой зелёной поляне возле горной реки.",
                            "target_lang_translations": {
                                "en": "The tourists decided to set up <w>camp</w> in a large green clearing near a mountain river."
                            }
                        }
                    ],
                    "synonyms": [
                        "стоя́нка"
                    ],
                    "common_phrases": [
                        "летний лагерь",
                        "разбить лагерь",
                        "детский лагерь",
                        "спортивный лагерь"
                    ],
                    "target_lang_definitions": {
                        "en": "A temporary place for people to stay outdoors, or an organized facility for recreation and sports."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "camp",
                                "target_lang_sense_clarification": "temporary accommodation or organized event"
                            },
                            {
                                "target_lang_word": "campsite",
                                "target_lang_sense_clarification": "place for setting up tents"
                            },
                            {
                                "target_lang_word": "summer camp",
                                "target_lang_sense_clarification": "recreational institution for children"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "739f8579-8110-34d4-868f-2e3c4397ab21",
                    "sense_definition": "Охраняемое место для принудительного содержания заключённых или военнопленных.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "place_detention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В исторических книгах часто описывают тяжёлую жизнь людей, попавших в трудовой <w>лагерь</w>.",
                            "target_lang_translations": {
                                "en": "History books often describe the hard life of people who ended up in a labor <w>camp</w>."
                            }
                        },
                        {
                            "text": "Заключённый пытался сбежать из <w>лагеря</w>, но охрана вовремя заметила его отсутствие.",
                            "target_lang_translations": {
                                "en": "The prisoner tried to escape from the <w>camp</w>, but the guards noticed his absence in time."
                            }
                        }
                    ],
                    "synonyms": [
                        "ка́торга"
                    ],
                    "common_phrases": [
                        "концентрационный лагерь",
                        "исправительный лагерь",
                        "лагерь для военнопленных"
                    ],
                    "target_lang_definitions": {
                        "en": "A guarded place for the confinement of prisoners, forced laborers, or prisoners of war."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "camp",
                                "target_lang_sense_clarification": "place of detention"
                            },
                            {
                                "target_lang_word": "labor camp",
                                "target_lang_sense_clarification": "facility for forced labor"
                            },
                            {
                                "target_lang_word": "prison camp",
                                "target_lang_sense_clarification": "facility for prisoners"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aaff3543-fe99-390b-8ad5-3c2f2e360d8a",
                    "sense_definition": "Группа людей, организаций или стран, объединённых общими идеями, интересами или политическими взглядами.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "political_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спор о новых налогах разделил парламент на два непримиримых <w>лагеря</w>.",
                            "target_lang_translations": {
                                "en": "The dispute over new taxes divided the parliament into two irreconcilable <w>camps</w>."
                            }
                        },
                        {
                            "text": "Страны социалистического <w>лагеря</w> поддерживали тесные экономические связи друг с другом.",
                            "target_lang_translations": {
                                "en": "The countries of the socialist <w>camp</w> maintained close economic ties with each other."
                            }
                        }
                    ],
                    "synonyms": [
                        "сою́з",
                        "коали́ция"
                    ],
                    "common_phrases": [
                        "политический лагерь",
                        "два лагеря",
                        "враждебный лагерь"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of people or organizations united by shared ideas or political views."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "camp",
                                "target_lang_sense_clarification": "group with shared ideals"
                            },
                            {
                                "target_lang_word": "faction",
                                "target_lang_sense_clarification": "political group"
                            }
                        ]
                    }
                }
            ]
        }
    ]

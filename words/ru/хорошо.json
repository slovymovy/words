{
    "word_family": [
        "хороший",
        "хорошеть",
        "хорошенький",
        "хорошист",
        "по-хорошему"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "6c755008-1e5c-337e-b9ce-8e82c8146a6c",
                    "sense_definition": "Так, как надо, как желательно, как считается красивым или правильным; качественно",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой друг очень <w>хорошо</w> говорит по-русски.",
                            "target_lang_translations": {
                                "en": "My friend speaks Russian very <w>well</w>."
                            }
                        },
                        {
                            "text": "Ты <w>хорошо</w> выполнил это задание.",
                            "target_lang_translations": {
                                "en": "You did this task <w>well</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "удачно"
                    ],
                    "antonyms": [
                        "неудачно"
                    ],
                    "common_phrases": [
                        "очень хорошо",
                        "всё хорошо"
                    ],
                    "target_lang_definitions": {
                        "en": "In a good, correct, or satisfactory manner; skillfully"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "well",
                                "target_lang_sense_clarification": "adverb indicating manner or quality of action"
                            },
                            {
                                "target_lang_word": "nicely",
                                "target_lang_sense_clarification": "in a pleasant or satisfactory way"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90e687be-c18d-333d-be0a-159ef3bd5b4d",
                    "sense_definition": "О чувстве удовлетворения, удовольствия, радости или физическом комфорте (в значении сказуемого)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом в деревне очень <w>хорошо</w> и спокойно.",
                            "target_lang_translations": {
                                "en": "It is very <w>nice</w> and peaceful in the village in summer."
                            }
                        },
                        {
                            "text": "Мне <w>хорошо</w> рядом с тобой.",
                            "target_lang_translations": {
                                "en": "I feel <w>good</w> when I'm with you."
                            }
                        }
                    ],
                    "common_phrases": [
                        "мне хорошо",
                        "как хорошо"
                    ],
                    "target_lang_definitions": {
                        "en": "Describes a state of comfort, happiness, pleasure, or well-being"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "good",
                                "target_lang_sense_clarification": "feeling physical or emotional well-being"
                            },
                            {
                                "target_lang_word": "nice",
                                "target_lang_sense_clarification": "pleasant or agreeable (of a situation or atmosphere)"
                            },
                            {
                                "target_lang_word": "well",
                                "target_lang_sense_clarification": "in good health or a satisfactory state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "099920af-91cf-3209-a4ef-bbdd6f210385",
                    "sense_definition": "Выражает согласие или подтверждение; то же, что «да», «согласен»",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Мы встретимся завтра? — <w>Хорошо</w>, я приду.",
                            "target_lang_translations": {
                                "en": "— Will we meet tomorrow? — <w>Okay</w>, I will come."
                            }
                        },
                        {
                            "text": "— Купи хлеба по дороге. — <w>Хорошо</w>.",
                            "target_lang_translations": {
                                "en": "— Buy some bread on the way. — <w>All right</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "да",
                        "согласен"
                    ],
                    "antonyms": [
                        "напротив"
                    ],
                    "common_phrases": [
                        "ну хорошо",
                        "ладно хорошо"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to express agreement, consent, or confirmation"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "okay",
                                "target_lang_sense_clarification": "expressing agreement or acceptance"
                            },
                            {
                                "target_lang_word": "all right",
                                "target_lang_sense_clarification": "indicating consent or agreement"
                            },
                            {
                                "target_lang_word": "fine",
                                "target_lang_sense_clarification": "expressing acceptance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ecc8da7-453f-31a5-b98f-daf89c408829",
                    "sense_definition": "Положительная оценка успеваемости в пятибалльной системе (соответствует 4 баллам)",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "grade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За контрольную работу ученик получил <w>хорошо</w>.",
                            "target_lang_translations": {
                                "en": "The student received a <w>good</w> mark for the test."
                            }
                        },
                        {
                            "text": "Он учится на <w>хорошо</w> и отлично.",
                            "target_lang_translations": {
                                "en": "He gets <w>Bs</w> and As."
                            }
                        }
                    ],
                    "synonyms": [
                        "четвёрка"
                    ],
                    "common_phrases": [
                        "получить хорошо",
                        "сдать на хорошо"
                    ],
                    "target_lang_definitions": {
                        "en": "A school grade indicating good performance (equivalent to a B or 4 out of 5)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "B",
                                "target_lang_sense_clarification": "academic grade equivalent"
                            },
                            {
                                "target_lang_word": "good",
                                "target_lang_sense_clarification": "literal translation of the grade name"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f43df1c6-f3bf-34fc-8591-d9b047669183",
                    "sense_definition": "Выражает угрозу или предостережение; то же, что «ну, смотри», «ну, погоди»",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "threat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Хорошо</w> же, я тебе это ещё припомню!",
                            "target_lang_translations": {
                                "en": "<w>All right</w> then, I'll remind you of this later!"
                            }
                        },
                        {
                            "text": "Ну <w>хорошо</w>, ты сам напросился на неприятности.",
                            "target_lang_translations": {
                                "en": "Well <w>okay</w>, you asked for trouble yourself."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ну хорошо же"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in spoken language to express a threat"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to express a threat or warning, implying consequences will follow"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "all right",
                                "target_lang_sense_clarification": "used in a threatening or defiant manner"
                            },
                            {
                                "target_lang_word": "very well",
                                "target_lang_sense_clarification": "formal acceptance of a conflict or challenge"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c06c229a-709a-331a-9063-ce450277b5b1",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>хороший</w>",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "adj_short",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это решение было <w>хорошо</w> обдумано всеми участниками.",
                            "target_lang_translations": {
                                "en": "This decision was <w>well</w> thought out by all participants."
                            }
                        },
                        {
                            "text": "Утреннее море сегодня особенно <w>хорошо</w> и спокойно.",
                            "target_lang_translations": {
                                "en": "The morning sea is especially <w>good</w> and calm today."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short form of the adjective meaning having positive qualities; beautiful or suitable"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "good",
                                "target_lang_sense_clarification": "having desirable qualities"
                            },
                            {
                                "target_lang_word": "well",
                                "target_lang_sense_clarification": "used in compound adjectives (e.g., well thought out)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
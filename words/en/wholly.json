    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "864699c7-a0c5-348e-b9fd-31978b90b396",
                    "sense_definition": "Completely and entirely; to the fullest extent.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new subsidiary is <w>wholly</w> owned by the parent company.",
                            "target_lang_translations": {
                                "ru": "Новая дочерняя компания <w>полностью</w> принадлежит материнской компании."
                            }
                        },
                        {
                            "text": "I am not <w>wholly</w> convinced that this is the best solution.",
                            "target_lang_translations": {
                                "ru": "Я не <w>совсем</w> убежден, что это лучшее решение."
                            }
                        }
                    ],
                    "synonyms": [
                        "completely",
                        "totally",
                        "entirely"
                    ],
                    "antonyms": [
                        "partly"
                    ],
                    "common_phrases": [
                        "wholly owned",
                        "wholly different",
                        "wholly appropriate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью и целиком; в полной мере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полностью",
                                "target_lang_sense_clarification": "на 100%, без остатка"
                            },
                            {
                                "target_lang_word": "целиком",
                                "target_lang_sense_clarification": "как единое целое"
                            },
                            {
                                "target_lang_word": "совершенно",
                                "target_lang_sense_clarification": "абсолютно (часто с прилагательными)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7cd2bb8-dd74-356d-81e8-fae419ff733a",
                    "sense_definition": "Exclusively and solely; for one specific purpose or reason only.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The success of the project depends <w>wholly</w> on your efforts.",
                            "target_lang_translations": {
                                "ru": "Успех проекта <w>всецело</w> зависит от ваших усилий."
                            }
                        },
                        {
                            "text": "This fund is used <w>wholly</w> for charitable purposes.",
                            "target_lang_translations": {
                                "ru": "Этот фонд используется <w>исключительно</w> в благотворительных целях."
                            }
                        }
                    ],
                    "synonyms": [
                        "solely"
                    ],
                    "common_phrases": [
                        "wholly responsible"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исключительно; только для определенной цели или причины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исключительно",
                                "target_lang_sense_clarification": "только, единственно"
                            },
                            {
                                "target_lang_word": "всецело",
                                "target_lang_sense_clarification": "полностью посвящая чему-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]

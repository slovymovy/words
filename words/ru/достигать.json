            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2ed989cb-cb44-304c-8566-ca375e32ee09",
                    "sense_definition": "Передвигаясь, доходить или доезжать до определённого места.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы планируют <достигать> вершины горы к полудню.",
                            "target_lang_translations": {
                                "en": "The tourists plan to <w>reach</w> the mountain peak by noon."
                            }
                        },
                        {
                            "text": "Корабль должен <достигать> берега через несколько часов.",
                            "target_lang_translations": {
                                "en": "The ship should <w>reach</w> the shore in a few hours."
                            }
                        }
                    ],
                    "synonyms": [
                        "добираться"
                    ],
                    "common_phrases": [
                        "достигать берега",
                        "достигать вершины",
                        "достигать пункта назначения"
                    ],
                    "target_lang_definitions": {
                        "en": "To walk or drive to a certain place; to arrive at a destination."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reach",
                                "target_lang_sense_clarification": "arrive at a destination"
                            },
                            {
                                "target_lang_word": "arrive at",
                                "target_lang_sense_clarification": "come to a place"
                            },
                            {
                                "target_lang_word": "get to",
                                "target_lang_sense_clarification": "reach a location"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f176c410-308f-3c47-bdcf-8dc4a060a747",
                    "sense_definition": "Получать желаемое или добиваться поставленной цели, прилагая усилия.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_achievement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Упорным трудом можно <достигать> любых поставленных целей.",
                            "target_lang_translations": {
                                "en": "With hard work, one can <w>achieve</w> any set goals."
                            }
                        },
                        {
                            "text": "Компания продолжает <достигать> успеха на международном рынке.",
                            "target_lang_translations": {
                                "en": "The company continues to <w>achieve</w> success in the international market."
                            }
                        }
                    ],
                    "synonyms": [
                        "добиваться"
                    ],
                    "common_phrases": [
                        "достигать цели",
                        "достигать успеха",
                        "достигать результата",
                        "достигать соглашения"
                    ],
                    "target_lang_definitions": {
                        "en": "To obtain what is desired or achieve a set goal by applying effort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "achieve",
                                "target_lang_sense_clarification": "successfully bring about a desired result"
                            },
                            {
                                "target_lang_word": "attain",
                                "target_lang_sense_clarification": "succeed in achieving"
                            },
                            {
                                "target_lang_word": "reach",
                                "target_lang_sense_clarification": "succeed in achieving a goal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f87dbaf-a602-37ed-a2fe-17ca9025ab4c",
                    "sense_definition": "Доходить до определённого уровня, размера, веса или количества.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantitative_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Скорость ветра может <достигать> двадцати метров в секунду.",
                            "target_lang_translations": {
                                "en": "Wind speed can <w>reach</w> twenty meters per second."
                            }
                        },
                        {
                            "text": "Температура воздуха летом будет <достигать> тридцати градусов.",
                            "target_lang_translations": {
                                "en": "The air temperature in summer will <w>reach</w> thirty degrees."
                            }
                        }
                    ],
                    "common_phrases": [
                        "достигать уровня",
                        "достигать максимума",
                        "достигать размеров",
                        "достигать скорости"
                    ],
                    "target_lang_definitions": {
                        "en": "To reach a certain level, size, weight, or quantity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reach",
                                "target_lang_sense_clarification": "amount to or get as high as"
                            },
                            {
                                "target_lang_word": "hit",
                                "target_lang_sense_clarification": "reach a particular level or point"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13772cf0-6e32-3ee5-a0f8-bc5cf60c9a72",
                    "sense_definition": "Доживать до определённого возраста.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantitative_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодые люди начинают <достигать> совершеннолетия в восемнадцать лет.",
                            "target_lang_translations": {
                                "en": "Young people start to <w>reach</w> the age of majority at eighteen."
                            }
                        },
                        {
                            "text": "Некоторые виды черепах могут <достигать> возраста ста лет.",
                            "target_lang_translations": {
                                "en": "Some species of turtles can <w>reach</w> the age of one hundred years."
                            }
                        }
                    ],
                    "synonyms": [
                        "доживать"
                    ],
                    "common_phrases": [
                        "достигать совершеннолетия",
                        "достигать возраста",
                        "достигать старости"
                    ],
                    "target_lang_definitions": {
                        "en": "To live to a certain age."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reach",
                                "target_lang_sense_clarification": "live until a certain age"
                            },
                            {
                                "target_lang_word": "attain",
                                "target_lang_sense_clarification": "reach a specific age (formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2246929-8536-3894-ae74-cc2c0c47d05d",
                    "sense_definition": "Догонять или настигать кого-либо во время движения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бегун начал постепенно <достигать> лидера гонки на последнем круге.",
                            "target_lang_translations": {
                                "en": "The runner began to gradually <w>gain on</w> the race leader on the last lap."
                            }
                        },
                        {
                            "text": "Охотник старался <достигать> зверя, идя по свежим следам.",
                            "target_lang_translations": {
                                "en": "The hunter tried to <w>catch up with</w> the beast by following fresh tracks."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To catch up with or overtake someone while moving."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "catch up with",
                                "target_lang_sense_clarification": "succeed in reaching a person ahead"
                            },
                            {
                                "target_lang_word": "gain on",
                                "target_lang_sense_clarification": "get closer to someone being chased"
                            }
                        ]
                    }
                }
            ]
        }

                {
                    "sense_id": "81815c1a-6383-324f-a574-fdbcc0f7af52",
                    "sense_definition": "Een plastisch mengsel van een bindmiddel (zoals cement of kalk), zand en water dat hard wordt na droging, gebruikt om stenen aan elkaar te metselen of muren te bepleisteren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "construction_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bouwvakker mengde de <w>mortel</w> in een grote kuip.",
                            "target_lang_translations": {
                                "en": "The construction worker mixed the <w>mortar</w> in a large tub."
                            }
                        },
                        {
                            "text": "Zonder stevige <w>mortel</w> blijft de muur niet lang staan.",
                            "target_lang_translations": {
                                "en": "Without strong <w>mortar</w>, the wall won't stand for long."
                            }
                        }
                    ],
                    "synonyms": [
                        "metselspecie",
                        "specie"
                    ],
                    "common_phrases": [
                        "mortel aanmaken",
                        "natte mortel",
                        "laag mortel"
                    ],
                    "target_lang_definitions": {
                        "en": "A moldable mixture of a binder (such as cement or lime), sand, and water that hardens after drying, used to bond stones together or plaster walls."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mortar",
                                "target_lang_sense_clarification": "building material"
                            }
                        ]

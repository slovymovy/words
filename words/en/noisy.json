{
    "word_family": [
        "noise",
        "noisily",
        "noisiness",
        "noiseless"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "77d8f1b9-874f-319a-b762-f1271eafd4a3",
                    "sense_definition": "Making a lot of loud or unpleasant noise.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "auditory_noise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>noisy</w> crowd cheered loudly for their favorite team.",
                            "target_lang_translations": {
                                "ru": "<w>Шумная</w> толпа громко болела за свою любимую команду."
                            }
                        },
                        {
                            "text": "We could not sleep because of the <w>noisy</w> neighbors upstairs.",
                            "target_lang_translations": {
                                "ru": "Мы не могли спать из-за <w>шумных</w> соседей сверху."
                            }
                        }
                    ],
                    "synonyms": [
                        "loud",
                        "boisterous",
                        "rowdy",
                        "clamorous",
                        "vociferous",
                        "raucous"
                    ],
                    "antonyms": [
                        "quiet",
                        "silent"
                    ],
                    "common_phrases": [
                        "noisy neighbors",
                        "noisy crowd",
                        "noisy children",
                        "noisy eater"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издающий много громкого или неприятного шума."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шумный",
                                "target_lang_sense_clarification": "издающий шум"
                            },
                            {
                                "target_lang_word": "шумливый",
                                "target_lang_sense_clarification": "склонный шуметь (о людях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "057a6cbe-f79d-34c7-b272-e0209464f39c",
                    "sense_definition": "Filled with or characterized by a lot of noise.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "auditory_noise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is hard to concentrate on work in such a <w>noisy</w> room.",
                            "target_lang_translations": {
                                "ru": "Трудно сосредоточиться на работе в такой <w>шумной</w> комнате."
                            }
                        },
                        {
                            "text": "They decided to leave the <w>noisy</w> bar and go somewhere quieter.",
                            "target_lang_translations": {
                                "ru": "Они решили уйти из <w>шумного</w> бара и пойти куда-нибудь потише."
                            }
                        }
                    ],
                    "synonyms": [
                        "loud",
                        "tumultuous",
                        "deafening",
                        "rowdy"
                    ],
                    "antonyms": [
                        "quiet",
                        "silent"
                    ],
                    "common_phrases": [
                        "noisy street",
                        "noisy environment",
                        "noisy city",
                        "noisy traffic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наполненный шумом или характеризующийся высоким уровнем шума."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шумный",
                                "target_lang_sense_clarification": "полный шума"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3709d33b-1e1b-3a7f-9087-d0dbdc210ac7",
                    "sense_definition": "Very bright, colorful, or patterned in a way that attracts unwanted attention.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "visual_noise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He wore a <w>noisy</w> shirt with clashing colors to the party.",
                            "target_lang_translations": {
                                "ru": "Он пришел на вечеринку в <w>кричащей</w> рубашке с несочетающимися цветами."
                            }
                        },
                        {
                            "text": "The wallpaper design was too <w>noisy</w> for a relaxing bedroom.",
                            "target_lang_translations": {
                                "ru": "Рисунок обоев был слишком <w>пестрым</w> для спокойной спальни."
                            }
                        }
                    ],
                    "synonyms": [
                        "loud"
                    ],
                    "common_phrases": [
                        "noisy pattern",
                        "noisy colors",
                        "noisy background"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слишком яркий, пестрый или привлекающий ненужное внимание (о цветах или узорах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кричащий",
                                "target_lang_sense_clarification": "о цвете или одежде"
                            },
                            {
                                "target_lang_word": "пестрый",
                                "target_lang_sense_clarification": "об узоре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f105ebd4-bf82-3ca5-bda4-470f7d9d0e78",
                    "sense_definition": "Containing random fluctuations or irrelevant data that obscure the clear signal or information.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "technical_noise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The radio transmission was <w>noisy</w> and difficult to understand.",
                            "target_lang_translations": {
                                "ru": "Радиопередача была <w>с помехами</w>, и её было трудно понять."
                            }
                        },
                        {
                            "text": "Scientists had to filter the <w>noisy</w> data to find the correct trend.",
                            "target_lang_translations": {
                                "ru": "Ученым пришлось отфильтровать <w>зашумленные</w> данные, чтобы найти правильную тенденцию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "noisy data",
                        "noisy signal",
                        "noisy channel",
                        "noisy image"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий случайные колебания или искажения, скрывающие полезный сигнал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зашумленный",
                                "target_lang_sense_clarification": "о данных или изображении"
                            },
                            {
                                "target_lang_word": "с помехами",
                                "target_lang_sense_clarification": "о сигнале"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
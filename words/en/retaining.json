{
    "word_family": [
        "retention",
        "retainer",
        "retentive",
        "retainment"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "41221ab7-760c-3b78-a584-22f49cab21ac",
                    "sense_definition": "Present participle and gerund of <w>retain</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company is focusing on <w>retaining</w> its most loyal customers.",
                            "target_lang_translations": {
                                "ru": "Компания сосредоточена на <w>удержании</w> своих самых лояльных клиентов."
                            }
                        },
                        {
                            "text": "This thick fabric is excellent for <w>retaining</w> heat during the winter.",
                            "target_lang_translations": {
                                "ru": "Эта плотная ткань отлично подходит для <w>сохранения</w> тепла зимой."
                            }
                        }
                    ],
                    "synonyms": [
                        "keeping",
                        "holding",
                        "maintaining",
                        "preserving"
                    ],
                    "antonyms": [
                        "losing",
                        "releasing",
                        "discarding"
                    ],
                    "common_phrases": [
                        "retaining control",
                        "retaining ownership",
                        "retaining moisture",
                        "retaining heat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола retain (сохранять, удерживать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сохранение",
                                "target_lang_sense_clarification": "процесс сбережения чего-либо (тепла, информации)"
                            },
                            {
                                "target_lang_word": "удержание",
                                "target_lang_sense_clarification": "сохранение контроля над чем-либо или кем-либо (клиентов, власти)"
                            },
                            {
                                "target_lang_word": "удерживающий",
                                "target_lang_sense_clarification": "причастие: тот, кто удерживает или сохраняет"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "855c3ff6-fcb0-3c47-9571-20af69e6c011",
                    "sense_definition": "Serving to hold something back, keep it in place, or support a structure.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They built a large <w>retaining</w> wall to prevent the soil from sliding down the hill.",
                            "target_lang_translations": {
                                "ru": "Они построили большую <w>подпорную</w> стену, чтобы предотвратить сползание грунта со склона."
                            }
                        },
                        {
                            "text": "The mechanic replaced the broken <w>retaining</w> clip on the engine part.",
                            "target_lang_translations": {
                                "ru": "Механик заменил сломанный <w>стопорный</w> зажим на детали двигателя."
                            }
                        }
                    ],
                    "synonyms": [
                        "supporting",
                        "holding"
                    ],
                    "common_phrases": [
                        "retaining wall",
                        "retaining ring",
                        "retaining clip",
                        "retaining screw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служащий для сдерживания, фиксации на месте или поддержки конструкции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подпорный",
                                "target_lang_sense_clarification": "используется в строительстве (например, стена)"
                            },
                            {
                                "target_lang_word": "стопорный",
                                "target_lang_sense_clarification": "используется в механике (например, кольцо, зажим)"
                            },
                            {
                                "target_lang_word": "удерживающий",
                                "target_lang_sense_clarification": "общая функция фиксации"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a5303915-480a-354e-8150-b12f082c1fd6",
                    "sense_definition": "The act of keeping someone or something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>retaining</w> of these old legal documents is required by the new regulations.",
                            "target_lang_translations": {
                                "ru": "<w>Хранение</w> этих старых юридических документов требуется новыми правилами."
                            }
                        },
                        {
                            "text": "Successful <w>retaining</w> of talented staff depends on providing good working conditions.",
                            "target_lang_translations": {
                                "ru": "Успешное <w>удержание</w> талантливых сотрудников зависит от обеспечения хороших условий труда."
                            }
                        }
                    ],
                    "synonyms": [
                        "retention",
                        "keeping",
                        "preservation"
                    ],
                    "antonyms": [
                        "disposal",
                        "loss"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по сохранению или удержанию кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удержание",
                                "target_lang_sense_clarification": "акт сохранения (персонала, доли)"
                            },
                            {
                                "target_lang_word": "сохранение",
                                "target_lang_sense_clarification": "акт сбережения"
                            },
                            {
                                "target_lang_word": "хранение",
                                "target_lang_sense_clarification": "процесс содержания документов или вещей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
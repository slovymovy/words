                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The patient is starting to <w>respond</w> well to the new treatment.",
                            "target_lang_translations": {
                                "ru": "Пациент начинает хорошо <w>реагировать</w> на новое лечение."
                            }

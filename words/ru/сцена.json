    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "48bf7e41-6d2e-390a-93c1-28dbd401b552",
                    "sense_definition": "Специальная площадка в театре или концертном зале, на которой выступают артисты.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "theater_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Актёры готовились выйти на <сцену> под громкие аплодисменты зрителей.",
                            "target_lang_translations": {
                                "en": "The actors were preparing to go on <stage> to the loud applause of the audience."
                            }
                        },
                        {
                            "text": "Яркий свет прожекторов освещал пустую <сцену> перед началом спектакля.",
                            "target_lang_translations": {
                                "en": "Bright spotlights illuminated the empty <stage> before the start of the performance."
                            }
                        }
                    ],
                    "synonyms": [
                        "подмостки"
                    ],
                    "common_phrases": [
                        "выйти на сцену",
                        "стоять на сцене",
                        "большая сцена"
                    ],
                    "target_lang_definitions": {
                        "en": "A raised platform in a theater or concert hall where artists perform."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stage",
                                "target_lang_sense_clarification": "platform for performance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e39bfff5-ae98-3232-adef-d01178bbf2e0",
                    "sense_definition": "Отдельная часть действия в театральной пьесе, фильме или книге.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "narrative_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта финальная <сцена> фильма заставила всех зрителей в зале плакать.",
                            "target_lang_translations": {
                                "en": "This final <scene> of the movie made all the viewers in the hall cry."
                            }
                        },
                        {
                            "text": "Режиссёр попросил актёров повторить <сцену> встречи главных героев.",
                            "target_lang_translations": {
                                "en": "The director asked the actors to repeat the meeting <scene> of the main characters."
                            }
                        }
                    ],
                    "synonyms": [
                        "эпизод"
                    ],
                    "common_phrases": [
                        "финальная сцена",
                        "сцена из фильма",
                        "любовная сцена"
                    ],
                    "target_lang_definitions": {
                        "en": "A subdivision of a play, movie, or book where the action takes place in one location or is continuous."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scene",
                                "target_lang_sense_clarification": "part of a movie or play"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1bb26df-e2e9-36db-9a33-79c2bca44228",
                    "sense_definition": "Конфликтный разговор, ссора или эмоциональное выяснение отношений.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, не нужно устраивать <сцену> из-за такой маленькой ошибки.",
                            "target_lang_translations": {
                                "en": "Please, there is no need to make a <scene> over such a small mistake."
                            }
                        },
                        {
                            "text": "Она закатила мужу громкую <сцену> ревности прямо в ресторане.",
                            "target_lang_translations": {
                                "en": "She made a loud jealous <scene> to her husband right in the restaurant."
                            }
                        }
                    ],
                    "common_phrases": [
                        "устроить сцену",
                        "сцена ревности",
                        "закатить сцену"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies a dramatic or public display of emotion"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A public display of anger, emotion, or a quarrel."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scene",
                                "target_lang_sense_clarification": "public display of emotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e783d601-060e-3da5-8d72-4ae536f0a910",
                    "sense_definition": "Происшествие, событие или картина, представшая взору.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "event_view",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция внимательно осмотрела <сцену> преступления в поисках улик.",
                            "target_lang_translations": {
                                "en": "The police carefully examined the crime <scene> in search of evidence."
                            }
                        },
                        {
                            "text": "Когда учитель вошёл в класс, возникла немая <сцена> удивления.",
                            "target_lang_translations": {
                                "en": "When the teacher entered the classroom, a silent <scene> of surprise arose."
                            }
                        }
                    ],
                    "common_phrases": [
                        "немая сцена",
                        "сцена преступления"
                    ],
                    "target_lang_definitions": {
                        "en": "The place where an incident in real life or fiction occurs, or a view presented to the eye."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scene",
                                "target_lang_sense_clarification": "location of an event"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10739235-101f-388e-a81d-b12fbc57eee2",
                    "sense_definition": "Театр как вид деятельности и искусства.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "theater_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Известный оперный певец объявил, что навсегда уходит со <сцены>.",
                            "target_lang_translations": {
                                "en": "The famous opera singer announced that he is leaving the <stage> forever."
                            }
                        },
                        {
                            "text": "Моя бабушка всю свою жизнь мечтала служить <сцене>.",
                            "target_lang_translations": {
                                "en": "My grandmother dreamed of serving the <stage> all her life."
                            }
                        }
                    ],
                    "synonyms": [
                        "театр"
                    ],
                    "common_phrases": [
                        "уйти со сцены",
                        "служить сцене",
                        "звёзды сцены"
                    ],
                    "target_lang_definitions": {
                        "en": "The acting profession or the world of theater in general."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stage",
                                "target_lang_sense_clarification": "the acting profession"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "052baba1-15d3-30bc-ac41-02ae552f4e53",
                    "sense_definition": "Виртуальное пространство моделирования с расположенными в нём трёхмерными объектами.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "computer_graphics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дизайнеру потребовалось много времени, чтобы создать эту сложную <сцену>.",
                            "target_lang_translations": {
                                "en": "It took the designer a long time to create this complex <scene>."
                            }
                        },
                        {
                            "text": "Компьютер начал обрабатывать трёхмерную <сцену> для новой игры.",
                            "target_lang_translations": {
                                "en": "The computer started processing the three-dimensional <scene> for the new game."
                            }
                        }
                    ],
                    "synonyms": [
                        "трёхмерная сцена",
                        "3D-сцена"
                    ],
                    "common_phrases": [
                        "рендеринг сцены",
                        "загрузка сцены"
                    ],
                    "target_lang_definitions": {
                        "en": "A virtual environment in computer graphics containing 3D objects."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scene",
                                "target_lang_sense_clarification": "3D computer graphics"
                            }
                        ]
                    }
                }
            ]
        }
    ]

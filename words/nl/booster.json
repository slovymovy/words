{
    "word_family": [
        "boosteren",
        "boost"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "811de488-3df9-3b2f-b32d-6e7d75968b73",
                    "sense_definition": "Een herhalingsvaccinatie om de afweer tegen een ziekte weer op peil te brengen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn grootouders hebben gisteren hun <w>booster</w> gehaald bij de huisarts.",
                            "target_lang_translations": {
                                "ru": "Мои бабушка и дедушка вчера сделали <w>бустер</w> у домашнего врача."
                            }
                        },
                        {
                            "text": "De <w>booster</w> zorgt voor extra bescherming tegen het virus.",
                            "target_lang_translations": {
                                "ru": "<w>Бустер</w> обеспечивает дополнительную защиту от вируса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "booster halen",
                        "booster prikken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повторная вакцинация для восстановления уровня защиты от болезни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бустер",
                                "target_lang_sense_clarification": "дополнительная доза вакцины"
                            },
                            {
                                "target_lang_word": "ревакцинация",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f76b1e87-81b5-31e0-8c94-490b831e89b3",
                    "sense_definition": "Een elektrisch aangedreven voertuig voor mensen die slecht ter been zijn.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij gaat elke ochtend met haar <w>booster</w> naar de supermarkt.",
                            "target_lang_translations": {
                                "ru": "Каждое утро она ездит в супермаркет на своем <w>электроскутере</w>."
                            }
                        },
                        {
                            "text": "Omdat hij niet goed kan lopen, gebruikt hij een <w>booster</w> voor uitstapjes.",
                            "target_lang_translations": {
                                "ru": "Так как он плохо ходит, он использует <w>электроскутер</w> для прогулок."
                            }
                        }
                    ],
                    "synonyms": [
                        "scootmobiel"
                    ],
                    "common_phrases": [
                        "op de booster",
                        "elektrische booster"
                    ],
                    "target_lang_definitions": {
                        "ru": "Электрическое транспортное средство для людей с ограниченной подвижностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "электроскутер",
                                "target_lang_sense_clarification": "для маломобильных граждан"
                            },
                            {
                                "target_lang_word": "скутер",
                                "target_lang_sense_clarification": "транспортное средство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "790d7e25-f4b1-3e1d-9356-06e87135def5",
                    "sense_definition": "Een apparaat dat een elektrisch signaal of geluid versterkt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>booster</w> verbetert het zwakke signaal van de antenne aanzienlijk.",
                            "target_lang_translations": {
                                "ru": "<w>Усилитель</w> значительно улучшает слабый сигнал антенны."
                            }
                        },
                        {
                            "text": "Hij installeerde een <w>booster</w> in zijn auto om de radio beter te kunnen horen.",
                            "target_lang_translations": {
                                "ru": "Он установил в машину <w>усилитель</w>, чтобы лучше слышать радио."
                            }
                        }
                    ],
                    "synonyms": [
                        "versterker"
                    ],
                    "common_phrases": [
                        "signaal versterken",
                        "booster aansluiten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство, которое усиливает электрический сигнал или звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усилитель",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "бустер",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "874886a3-cf1a-37d0-b4e9-06b755c2868d",
                    "sense_definition": "Een hulpraket die tijdelijk extra stuwkracht levert bij de lancering van een ruimtevaartuig.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de lancering viel de lege <w>booster</w> terug in de oceaan.",
                            "target_lang_translations": {
                                "ru": "После запуска пустой <w>ускоритель</w> упал обратно в океан."
                            }
                        },
                        {
                            "text": "De raket heeft een krachtige <w>booster</w> nodig om de ruimte te bereiken.",
                            "target_lang_translations": {
                                "ru": "Ракете нужен мощный <w>ускоритель</w>, чтобы достичь космоса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "raket met booster",
                        "booster afwerpen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вспомогательная ракета, обеспечивающая дополнительную тягу при запуске космического корабля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ускоритель",
                                "target_lang_sense_clarification": "ракетный двигатель"
                            },
                            {
                                "target_lang_word": "стартовый ускоритель",
                                "target_lang_sense_clarification": "космонавтика"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4021485d-4072-395f-9185-4435856b3a98",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>boosteren</w>.",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>booster</w> mijn immuunsysteem door gezond te eten en te sporten.",
                            "target_lang_translations": {
                                "ru": "Я <w>укрепляю</w> свой иммунитет, правильно питаясь и занимаясь спортом."
                            }
                        },
                        {
                            "text": "Ik <w>booster</w> het wifi-signaal zodat we overal bereik hebben.",
                            "target_lang_translations": {
                                "ru": "Я <w>усиливаю</w> сигнал Wi-Fi, чтобы связь была везде."
                            }
                        }
                    ],
                    "traits": [

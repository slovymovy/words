{
    "word_family": [
        "nagrywać",
        "nagranie",
        "nagrywarka",
        "nagrywający"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ce6c9c35-9b88-3de6-9017-afa9b64237ab",
                    "sense_definition": "Zarejestrować obraz, dźwięk lub informację na jakimś nośniku (np. dysku, taśmie) w celu ich utrwalenia i późniejszego odtworzenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "recording",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <nagrać> ten program telewizyjny, ponieważ nie będę mógł go obejrzeć na żywo.",
                            "target_lang_translations": {
                                "ru": "Я должен <записать> эту телепередачу, так как не смогу посмотреть её в прямом эфире."
                            }
                        },
                        {
                            "text": "Zespół wszedł do profesjonalnego studia, aby <nagrać> swój debiutancki album.",
                            "target_lang_translations": {
                                "ru": "Группа вошла в профессиональную студию, чтобы <записать> свой дебютный альбом."
                            }
                        }
                    ],
                    "synonyms": [
                        "zarejestrować",
                        "utrwalić"
                    ],
                    "common_phrases": [
                        "nagrać płytę",
                        "nagrać film",
                        "nagrać wiadomość",
                        "nagrać na dyktafon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Записать изображение, звук или информацию на носитель (диск, ленту) для сохранения и воспроизведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "записать",
                                "target_lang_sense_clarification": "общий термин для аудио, видео и данных"
                            },
                            {
                                "target_lang_word": "заснять",
                                "target_lang_sense_clarification": "касательно видео или фильма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e4ba95a-4985-3d50-b8da-4fe12b76eaf3",
                    "sense_definition": "Utrwalić swój własny głos lub wizerunek na urządzeniu rejestrującym (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "recording",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli nie odbiorę telefonu, proszę <nagrać> się na moją pocztę głosową.",
                            "target_lang_translations": {
                                "ru": "Если я не возьму трубку, прошу <оставить сообщение> на моей голосовой почте."
                            }
                        },
                        {
                            "text": "Postanowiliśmy <nagrać> się kamerą podczas próby, aby przeanalizować nasze błędy.",
                            "target_lang_translations": {
                                "ru": "Мы решили <заснять> себя на камеру во время репетиции, чтобы проанализировать наши ошибки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nagrać się na pocztę",
                        "nagrać się na wideo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зафиксировать свой голос или изображение на записывающем устройстве (часто используется в контексте автоответчика)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставить сообщение",
                                "target_lang_sense_clarification": "на автоответчик/голосовую почту"
                            },
                            {
                                "target_lang_word": "записать",
                                "target_lang_sense_clarification": "себя на видео или аудио"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97f23a53-26a6-34a8-9fd0-bf5e42b857ba",
                    "sense_definition": "Załatwić coś dla kogoś, zorganizować coś lub skojarzyć kogoś z kimś, zazwyczaj dzięki sprytowi lub znajomościom.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "arranging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój znajomy obiecał <nagrać> mi dorywczą pracę na okres wakacyjny.",
                            "target_lang_translations": {
                                "ru": "Мой знакомый обещал <устроить> мне подработку на лето."
                            }
                        },
                        {
                            "text": "Czy mógłbyś <nagrać> nam bilety na ten koncert, skoro znasz organizatora?",
                            "target_lang_translations": {
                                "ru": "Ты не мог бы <достать> нам билеты на этот концерт, раз уж ты знаешь организатора?"
                            }
                        }
                    ],
                    "synonyms": [
                        "załatwić"
                    ],
                    "common_phrases": [
                        "nagrać robotę",
                        "nagrać sprawę",
                        "nagrać kogoś"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устроить, организовать или достать что-то для кого-то, используя связи или хитрость (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устроить",
                                "target_lang_sense_clarification": "работу, встречу"
                            },
                            {
                                "target_lang_word": "достать",
                                "target_lang_sense_clarification": "билеты, товары"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88111ca1-a823-3e28-ae99-a51f8f772954",
                    "sense_definition": "Spędzić dużo czasu na graniu w gry lub na instrumencie, aż do momentu nasycenia lub zmęczenia (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "playing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ten weekend zamierzam <nagrać> się na konsoli za wszystkie czasy.",
                            "target_lang_translations": {
                                "ru": "В эти выходные я собираюсь <наиграться> на приставке за всё время."
                            }
                        },
                        {
                            "text": "Dzieci mogły wreszcie <nagrać> się w piłkę na nowym boisku szkolnym.",
                            "target_lang_translations": {
                                "ru": "Дети могли наконец <наиграться> в футбол на новой школьной площадке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nagrać się do woli",
                        "nagrać się za wszystkie czasy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Провести много времени за игрой (на инструменте или в видеоигры) до полного насыщения или усталости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наиграться",
                                "target_lang_sense_clarification": "вдоволь поиграть"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
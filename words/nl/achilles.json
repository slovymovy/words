    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "cb741678-2981-3800-b458-88df3f1e3fbe",
                    "sense_definition": "Een figuur uit de Griekse mythologie, zoon van Peleus en Thetis, en de belangrijkste Griekse held in de Trojaanse Oorlog.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mythology",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "De woede van <w>Achilles</w> is het centrale thema van de Ilias van Homerus.",
                            "target_lang_translations": {
                                "ru": "Гнев <w>Ахилла</w> — центральная тема «Илиады» Гомера."
                            }
                        },
                        {
                            "text": "Omdat zijn moeder hem bij zijn hiel vasthield, bleef dat de enige zwakke plek van <w>Achilles</w>.",
                            "target_lang_translations": {
                                "ru": "Так как мать держала его за пятку, это осталось единственным слабым местом <w>Ахилла</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hiel van Achilles"
                    ],
                    "target_lang_definitions": {
                        "ru": "Персонаж греческой мифологии, сын Пелея и Фетиды, главный греческий герой Троянской войны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ахилл",
                                "target_lang_sense_clarification": "герой древнегреческой мифологии"
                            },
                            {
                                "target_lang_word": "Ахиллес",
                                "target_lang_sense_clarification": "вариант имени героя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1be9797d-576c-3732-8b9e-526d299e0795",
                    "sense_definition": "Een jongensnaam.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Onze buurman heet <w>Achilles</w> en woont al jaren in deze straat.",
                            "target_lang_translations": {
                                "ru": "Нашего соседа зовут <w>Ахиллес</w>, и он живет на этой улице уже много лет."
                            }
                        },
                        {
                            "text": "De naam <w>Achilles</w> komt tegenwoordig niet zo vaak meer voor in Nederland.",
                            "target_lang_translations": {
                                "ru": "Имя <w>Ахиллес</w> в наши дни встречается в Нидерландах не так часто."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ахиллес",
                                "target_lang_sense_clarification": "мужское имя"
                            },
                            {
                                "target_lang_word": "Ахилл",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]

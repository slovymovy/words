                    "sense_id": "79674ee2-c720-316f-a3ef-918516d47063",
                    "sense_definition": "Zespół cech dźwiękowych danego instrumentu, sprzętu lub głosu; charakterystyczny dźwięk.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "audio_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twoja gitara ma rzeczywiście fajne <w>brzmienie</w>.",
                            "target_lang_translations": {
                                "ru": "У твоей гитары действительно классное <w>звучание</w>."
                            }
                        },
                        {
                            "text": "To stare radio ma bardzo ciepłe i przyjemne <w>brzmienie</w>.",
                            "target_lang_translations": {
                                "ru": "У этого старого радио очень теплое и приятное <w>звучание</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "barwa",
                        "ton",
                        "zabarwienie",
                        "dźwięk"
                    ],
                    "common_phrases": [

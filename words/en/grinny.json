    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "98808c7b-f195-325d-8fa6-93074af5d05a",
                    "sense_definition": "Having or resembling a grin; characterized by a broad smile.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The clown had a strange, <w>grinny</w> look painted on his face.",
                            "target_lang_translations": {
                                "ru": "У клоуна на лице было нарисовано странное, <w>ухмыляющееся</w> выражение."
                            }
                        },
                        {
                            "text": "She gave me a <w>grinny</w> smile that suggested she was up to something.",
                            "target_lang_translations": {
                                "ru": "Она одарила меня <w>ехидной</w> улыбкой, которая намекала, что она что-то задумала."
                            }
                        }
                    ],
                    "synonyms": [
                        "smiling",
                        "beaming",
                        "grinning"
                    ],
                    "antonyms": [
                        "frowning",
                        "scowling"
                    ],
                    "common_phrases": [
                        "grinny face",
                        "grinny smile"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий ухмылку или напоминающий её; характеризующийся широкой улыбкой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухмыляющийся",
                                "target_lang_sense_clarification": "выражающий ухмылку"
                            },
                            {
                                "target_lang_word": "ехидный",
                                "target_lang_sense_clarification": "насмешливый, с ухмылкой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a1b79b11-7db3-3d28-b4cd-2da0ccca8b86",
                    "sense_definition": "A colloquial term for the eastern chipmunk.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old farmer pointed out a <w>grinny</w> running along the fence rail.",
                            "target_lang_translations": {
                                "ru": "Старый фермер указал на <w>бурундука</w>, бегущего по изгороди."
                            }
                        },
                        {
                            "text": "A small <w>grinny</w> scurried across the dry leaves to hide under a log.",
                            "target_lang_translations": {
                                "ru": "Маленький <w>бурундук</w> промчался по сухим листьям, чтобы спрятаться под бревном."
                            }
                        }
                    ],
                    "synonyms": [

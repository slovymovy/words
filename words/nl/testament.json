    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0794c574-f9e9-3853-91a1-5682b8c62ea6",
                    "sense_definition": "Een officieel document waarin iemand vastlegt wat er na zijn of haar overlijden met de bezittingen moet gebeuren.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het <w>testament</w> staat dat de kinderen uit beide huwelijken voor gelijke delen erven.",
                            "target_lang_translations": {
                                "ru": "В <w>завещании</w> сказано, что дети от обоих браков наследуют в равных долях."
                            }
                        },
                        {
                            "text": "Mijn grootvader heeft gisteren zijn <w>testament</w> bij de notaris laten wijzigen.",
                            "target_lang_translations": {
                                "ru": "Вчера мой дедушка изменил своё <w>завещание</w> у нотариуса."
                            }
                        }
                    ],
                    "synonyms": [
                        "uiterste wil",
                        "laatste wilsbeschikking"
                    ],
                    "common_phrases": [
                        "een testament opmaken",
                        "in het testament",
                        "testament herroepen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный документ, в котором человек указывает, как распорядиться его имуществом после смерти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завещание",
                                "target_lang_sense_clarification": "документ о наследстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eed1bee8-a1ed-3136-a797-292070a403b6",
                    "sense_definition": "Elk van de twee hoofdgedeelten waaruit de Bijbel bestaat.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De Bijbel kent het Oude en het Nieuwe <w>Testament</w>.",
                            "target_lang_translations": {
                                "ru": "Библия состоит из Ветхого и Нового <w>Заветов</w>."
                            }
                        },
                        {
                            "text": "Tijdens de kerkdienst werd er een stuk voorgelezen uit het Nieuwe <w>Testament</w>.",
                            "target_lang_translations": {
                                "ru": "Во время церковной службы читали отрывок из Нового <w>Завета</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Oude Testament",
                        "Nieuwe Testament"
                    ],
                    "target_lang_definitions": {
                        "ru": "Каждая из двух основных частей, составляющих Библию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Завет",
                                "target_lang_sense_clarification": "часть Библии"
                            }
                        ]
                    }
                }
            ]
        }
    ]

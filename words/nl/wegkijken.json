    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9a09364c-30be-390c-b848-2498d52b3d9b",
                    "sense_definition": "De blik afwenden; een andere kant op kijken, vaak om oogcontact te vermijden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "literal_look",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij knikt verlegen naar haar en <w>kijkt</w> direct weer <w>weg</w>.",
                            "target_lang_translations": {
                                "en": "He nods shyly at her and immediately <w>looks away</w> again.",
                                "ru": "Он застенчиво кивает ей и сразу же <w>отводит взгляд</w>."
                            }
                        },
                        {
                            "text": "Toen onze blikken elkaar kruisten, <w>keek</w> de man snel <w>weg</w>.",
                            "target_lang_translations": {
                                "en": "When our eyes met, the man quickly <w>looked away</w>.",
                                "ru": "Когда наши взгляды встретились, мужчина быстро <w>отвел взгляд</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wegdraaien",
                        "afwenden"
                    ],
                    "antonyms": [
                        "aankijken",
                        "staren"
                    ],
                    "common_phrases": [
                        "snel wegkijken",
                        "beschaamd wegkijken",
                        "van iemand wegkijken"
                    ],
                    "target_lang_definitions": {
                        "en": "To avert one's gaze; to look in a different direction, often to avoid eye contact.",
                        "ru": "Отводить взгляд или смотреть в другую сторону, часто чтобы избежать зрительного контакта."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look away",
                                "target_lang_sense_clarification": "general action of averting gaze"
                            },
                            {
                                "target_lang_word": "avert one's gaze",
                                "target_lang_sense_clarification": "more formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отводить взгляд",
                                "target_lang_sense_clarification": "избегать зрительного контакта"
                            },
                            {
                                "target_lang_word": "отворачиваться",
                                "target_lang_sense_clarification": "физическое действие"
                            },
                            {
                                "target_lang_word": "смотреть в сторону",
                                "target_lang_sense_clarification": "направление взгляда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71fd95ef-21ab-3a9e-beb8-975640297020",
                    "sense_definition": "Doen alsof een probleem of ongewenste situatie niet bestaat; weigeren de realiteit onder ogen te zien.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_ignore",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het geweld in die regio werd zo hevig, dat de wereld niet meer kon <w>wegkijken</w>.",
                            "target_lang_translations": {
                                "en": "The violence in that region became so intense that the world could no longer <w>look away</w>.",
                                "ru": "Насилие в этом регионе стало настолько сильным, что мир больше не мог <w>закрывать глаза</w>."
                            }
                        },
                        {
                            "text": "De overheid mag niet langer <w>wegkijken</w> voor de armoede in de grote steden.",
                            "target_lang_translations": {
                                "en": "The government can no longer <w>turn a blind eye</w> to the poverty in the big cities.",
                                "ru": "Правительство больше не может <w>закрывать глаза</w> на бедность в больших городах."
                            }
                        }
                    ],
                    "synonyms": [
                        "negeren",
                        "ontkennen",
                        "veronachtzamen"
                    ],
                    "antonyms": [
                        "erkennen",
                        "aanpakken"
                    ],
                    "common_phrases": [
                        "niet mogen wegkijken",
                        "wegkijken voor problemen",
                        "de andere kant opkijken"
                    ],
                    "target_lang_definitions": {
                        "en": "To pretend a problem or unwanted situation does not exist; to refuse to face reality.",
                        "ru": "Игнорировать проблему или делать вид, что неприятной ситуации не существует; отказываться видеть реальность."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn a blind eye",
                                "target_lang_sense_clarification": "idiomatic, to deliberately ignore"
                            },
                            {
                                "target_lang_word": "look away",
                                "target_lang_sense_clarification": "figurative, to ignore moral responsibility"
                            },
                            {
                                "target_lang_word": "ignore",
                                "target_lang_sense_clarification": "direct translation of the effect"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "закрывать глаза",
                                "target_lang_sense_clarification": "идиома: игнорировать что-то"
                            },
                            {
                                "target_lang_word": "игнорировать",
                                "target_lang_sense_clarification": "не обращать внимания"
                            },
                            {
                                "target_lang_word": "отворачиваться",
                                "target_lang_sense_clarification": "переносное значение: уходить от проблем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59fd5a9a-8cc6-31c9-8652-75a8f2927536",
                    "sense_definition": "Iemand zo indringend of afkeurend aankijken dat deze zich ongemakkelijk voelt en vertrekt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "transitive_hostile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het gezin met jonge kinderen werd nog net niet <w>weggekeken</w> uit de chique winkel.",
                            "target_lang_translations": {
                                "en": "The family with young children was all but <w>stared out</w> of the chic shop.",
                                "ru": "Семью с маленькими детьми чуть ли не <w>выжили взглядами</w> из шикарного магазина."
                            }
                        },
                        {
                            "text": "De stamgasten probeerden de luidruchtige toeristen <w>weg</w> te <w>kijken</w>.",
                            "target_lang_translations": {
                                "en": "The regulars tried to <w>stare</w> the noisy tourists <w>away</w>.",
                                "ru": "Постоянные посетители пытались <w>выжить</w> шумных туристов <w>взглядами</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wegstaren"
                    ],
                    "antonyms": [
                        "verwelkomen"
                    ],
                    "common_phrases": [
                        "iemand wegkijken"
                    ],
                    "target_lang_definitions": {
                        "en": "To look at someone so intensely or disapprovingly that they feel uncomfortable and leave.",
                        "ru": "Заставлять кого-то уйти, пристально и неодобрительно глядя на него."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stare out",
                                "target_lang_sense_clarification": "to make someone leave by staring at them"
                            },
                            {
                                "target_lang_word": "glare away",
                                "target_lang_sense_clarification": "to drive someone away with angry looks"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выживать взглядом",
                                "target_lang_sense_clarification": "заставлять уйти давлением"
                            },
                            {
                                "target_lang_word": "сверлить взглядом",
                                "target_lang_sense_clarification": "пристально, враждебно смотреть"
                            }
                        ]
                    }
                }
            ]
        }

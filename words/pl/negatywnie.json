            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4a1c632a-4e5d-3b5a-a26f-cde4dc372423",
                    "sense_definition": "W sposób niekorzystny, szkodliwy lub wyrażający krytykę i dezaprobatę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego decyzja wpłynęła <w>negatywnie</w> na atmosferę w całym zespole.",
                            "target_lang_translations": {
                                "ru": "Его решение <w>негативно</w> повлияло на атмосферу во всем коллективе."
                            }
                        },
                        {
                            "text": "Komisja rekrutacyjna <w>negatywnie</w> oceniła jego podanie o pracę.",
                            "target_lang_translations": {
                                "ru": "Приемная комиссия <w>отрицательно</w> оценила его заявление о приеме на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "niekorzystnie",
                        "ujemnie",
                        "pejoratywnie"
                    ],
                    "antonyms": [
                        "pozytywnie"
                    ],
                    "common_phrases": [
                        "wpływać negatywnie",
                        "oceniać negatywnie",
                        "wypowiadać się negatywnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неблагоприятно, вредно или выражая критику и неодобрение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "негативно",
                                "target_lang_sense_clarification": "о влиянии или воздействии"
                            },
                            {
                                "target_lang_word": "отрицательно",
                                "target_lang_sense_clarification": "об оценке или суждении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28a2ff24-4422-397d-b958-dfb85ad72233",
                    "sense_definition": "W sposób pesymistyczny, przewidujący złe zakończenie lub porażkę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dlaczego jesteś tak <w>negatywnie</w> nastawiony do tego pomysłu?",
                            "target_lang_translations": {
                                "ru": "Почему ты так <w>негативно</w> настроен по отношению к этой идее?"
                            }
                        },
                        {
                            "text": "Nie myśl ciągle <w>negatywnie</w>, bo to tylko pogarsza twoje samopoczucie.",
                            "target_lang_translations": {
                                "ru": "Не думай постоянно <w>негативно</w>, так как это только ухудшает твое самочувствие."
                            }
                        }
                    ],
                    "synonyms": [
                        "ujemnie"
                    ],
                    "antonyms": [
                        "pozytywnie"
                    ],
                    "common_phrases": [
                        "negatywnie nastawiony",
                        "myśleć negatywnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пессимистично, предвидя плохой исход или неудачу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "негативно",
                                "target_lang_sense_clarification": "об отношении или настрое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98a9481e-6e28-3e8f-b75b-db4fbdd41313",
                    "sense_definition": "W sposób wskazujący na brak poszukiwanej cechy, substancji lub zjawiska (zwykle o wynikach badań medycznych lub naukowych).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Możesz być spokojna, ponieważ test ciążowy wyszedł <w>negatywnie</w>.",
                            "target_lang_translations": {
                                "ru": "Можешь быть спокойна, так как тест на беременность оказался <w>отрицательным</w>."
                            }
                        },
                        {
                            "text": "Badanie próbki na obecność wirusa zakończyło się <w>negatywnie</w>.",
                            "target_lang_translations": {
                                "ru": "Анализ образца на наличие вируса дал <w>отрицательный</w> результат."
                            }
                        }
                    ],
                    "synonyms": [
                        "ujemnie"
                    ],
                    "antonyms": [
                        "pozytywnie"
                    ],
                    "common_phrases": [
                        "test wyszedł negatywnie",
                        "zweryfikować negatywnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывая на отсутствие искомого признака, вещества или явления (обычно о результатах медицинских или научных исследований)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрицательно",
                                "target_lang_sense_clarification": "о результатах тестов или анализов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

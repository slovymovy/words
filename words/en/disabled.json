                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отклю́ченный",
                                "target_lang_sense_clarification": "switched off (e.g., settings)"
                            },
                            {
                                "target_lang_word": "неиспра́вный",
                                "target_lang_sense_clarification": "broken, not functioning (e.g., vehicle)"
                            },
                            {
                                "target_lang_word": "деактиви́рованный",
                                "target_lang_sense_clarification": "deactivated"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bafb7479-3d31-3ba1-838f-09983872d8b0",
                    "sense_definition": "Legally disqualified or deprived of a right.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "legal_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The court declared him legally <disabled> from managing the family estate.",
                            "target_lang_translations": {
                                "ru": "Суд признал его юридически <недееспособным> в отношении управления семейным имуществом."
                            }
                        },
                        {
                            "text": "Certain convictions may render a person <disabled> from holding public office.",
                            "target_lang_translations": {
                                "ru": "Определенные судимости могут сделать человека <лишенным права> занимать государственные должности."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридически лишенный права или возможности совершать определенные действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недееспосо́бный",
                                "target_lang_sense_clarification": "legally incapacitated"
                            },
                            {
                                "target_lang_word": "лишенный прав",
                                "target_lang_sense_clarification": "deprived of rights"
                            }
                        ]
                    }
                }
            ]
        },
        {

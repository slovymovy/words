                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stop <w>mommying</w> me, because I can make my own decisions.",
                            "target_lang_translations": {
                                "ru": "Хватит со мной <w>нянчиться</w>, я могу сам принимать решения."

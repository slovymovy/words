    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0a0c59da-157c-3f7b-b70c-26264eb10278",
                    "sense_definition": "Украшение в виде кольца из металла, кожи, кости или других материалов, которое обычно носят на запястье.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "jewelry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На день рождения муж подарил ей красивый золотой <w>браслет</w>.",
                            "target_lang_translations": {
                                "en": "For her birthday, her husband gave her a beautiful gold <w>bracelet</w>."
                            }
                        },
                        {
                            "text": "Она любит носить широкий серебряный <w>браслет</w> на правой руке.",
                            "target_lang_translations": {
                                "en": "She likes to wear a wide silver <w>bracelet</w> on her right wrist."
                            }
                        }
                    ],
                    "common_phrases": [
                        "золотой браслет",
                        "серебряный браслет",
                        "носить браслет",
                        "браслет на руку",
                        "браслет с камнями"
                    ],
                    "target_lang_definitions": {
                        "en": "A piece of jewelry worn around the wrist, made of metal, leather, or other materials."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bracelet",
                                "target_lang_sense_clarification": "jewelry"
                            },
                            {
                                "target_lang_word": "bangle",
                                "target_lang_sense_clarification": "rigid ring-shaped bracelet"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c5e46b1-fdb6-3d50-a601-ffc8765d81c5",
                    "sense_definition": "Металлическое изделие, служащее ремешком для ношения наручных часов.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "jewelry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У моих часов сломался замок на <w>браслете</w>, и я отнёс их в ремонт.",
                            "target_lang_translations": {
                                "en": "The clasp on my watch <w>bracelet</w> broke, and I took it in for repair."
                            }
                        },
                        {
                            "text": "Мужчина расстегнул <w>браслет</w> часов и положил их на тумбочку.",
                            "target_lang_translations": {
                                "en": "The man unfastened his watch <w>bracelet</w> and put it on the nightstand."
                            }
                        }
                    ],
                    "synonyms": [
                        "ремешок"
                    ],
                    "common_phrases": [
                        "браслет для часов",
                        "стальной браслет",
                        "металлический браслет",
                        "укоротить браслет"
                    ],
                    "target_lang_definitions": {
                        "en": "A metal band or strap used to secure a wristwatch to the wrist."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "watch bracelet",
                                "target_lang_sense_clarification": "metal band for a watch"
                            },
                            {
                                "target_lang_word": "watchband",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "strap",
                                "target_lang_sense_clarification": "watch strap"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7bffce4-0170-3214-965e-cf6f542f8572",
                    "sense_definition": "Наручники (используется в разговорной речи, полицейском или уголовном жаргоне).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полицейский быстро надел на задержанного <w>браслеты</w> и повёл его к машине.",
                            "target_lang_translations": {
                                "en": "The police officer quickly put the <w>bracelets</w> on the detainee and led him to the car."
                            }
                        },
                        {
                            "text": "Преступник понял, что сопротивление бесполезно, когда на его запястьях защёлкнулись <w>браслеты</w>.",
                            "target_lang_translations": {
                                "en": "The criminal realized that resistance was futile when the <w>bracelets</w> clicked shut on his wrists."
                            }
                        }
                    ],
                    "synonyms": [
                        "наручники"
                    ],
                    "common_phrases": [
                        "надеть браслеты",
                        "стальные браслеты"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "police or criminal jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Slang term for handcuffs used by police or criminals."
                    },

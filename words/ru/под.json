    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "86a6af6f-0310-3996-8fd1-b064655f1b70",
                    "sense_definition": "Указывает на нахождение ниже предмета или движение в пространство ниже чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кошка любит спать <под> столом.",
                            "target_lang_translations": {
                                "en": "The cat likes to sleep <w>under</w> the table."
                            }
                        },
                        {
                            "text": "Мы спрятались от дождя <под> дерево.",
                            "target_lang_translations": {
                                "en": "We hid from the rain <w>under</w> a tree."
                            }
                        },
                        {
                            "text": "Положи свои вещи <под> кровать.",
                            "target_lang_translations": {
                                "en": "Put your things <w>under</w> the bed."
                            }
                        }
                    ],
                    "synonyms": [
                        "подо"
                    ],
                    "antonyms": [
                        "над"
                    ],
                    "common_phrases": [
                        "под столом",
                        "под водой",
                        "под землёй",
                        "под кровать"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates location below an object or movement into the space below something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "under",
                                "target_lang_sense_clarification": "physical location or movement below"
                            },
                            {
                                "target_lang_word": "beneath",
                                "target_lang_sense_clarification": "formal or literary synonym for under"
                            },
                            {
                                "target_lang_word": "below",
                                "target_lang_sense_clarification": "lower in position than"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b10e6d4c-12e6-3c8f-ab8c-5bf27ebf26fb",
                    "sense_definition": "Указывает на нахождение вблизи, недалеко от какого-либо места (обычно города).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "location_vicinity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У моих родителей есть дача <под> Москвой.",
                            "target_lang_translations": {
                                "en": "My parents have a dacha <w>near</w> Moscow."
                            }
                        },
                        {
                            "text": "Решающая битва произошла <под> Полтавой.",
                            "target_lang_translations": {
                                "en": "The decisive battle took place <w>near</w> Poltava."
                            }

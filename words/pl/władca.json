            "senses": [
                {
                    "sense_id": "c67358f6-58ea-3d12-a32d-f2309e862d25",
                    "sense_definition": "Osoba sprawująca najwyższą władzę w państwie o ustroju innym niż republika, na przykład król lub cesarz.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kazimierz był mądrym i sprawiedliwym <w>władcą</w>.",
                            "target_lang_translations": {
                                "ru": "Казимир был мудрым и справедливым <w>правителем</w>."
                            }
                        },
                        {
                            "text": "Nowy <w>władca</w> obiecał poprawę losu swoich poddanych.",
                            "target_lang_translations": {
                                "ru": "Новый <w>правитель</w> обещал улучшить участь своих подданных."
                            }
                        }
                    ],
                    "synonyms": [
                        "monarcha",
                        "król",
                        "cesarz",
                        "książę"
                    ],
                    "antonyms": [
                        "poddany",
                        "wasal",
                        "prezydent"
                    ],
                    "common_phrases": [
                        "władca absolutny",
                        "prawowity władca",
                        "mądry władca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, обладающее верховной властью в государстве с нереспубликанской формой правления, например король или император."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правитель",
                                "target_lang_sense_clarification": "глава государства"
                            },
                            {
                                "target_lang_word": "властелин",
                                "target_lang_sense_clarification": "верховный правитель, обладающий могуществом"
                            },
                            {
                                "target_lang_word": "монарх",
                                "target_lang_sense_clarification": "единоличный правитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65efbc14-db30-33a7-b03c-931c3cb71892",
                    "sense_definition": "Osoba mająca nad czymś lub nad kimś pełną kontrolę lub dominującą pozycję.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten miliarder to prawdziwy <w>władca</w> lokalnego rynku nieruchomości.",
                            "target_lang_translations": {
                                "ru": "Этот миллиардер — настоящий <w>властелин</w> местного рынка недвижимости."
                            }
                        },
                        {
                            "text": "Jako kapitan statku czuł się niepodzielnym <w>władcą</w> na morzu.",
                            "target_lang_translations": {
                                "ru": "Как капитан корабля, он чувствовал себя безраздельным <w>владыкой</w> на море."
                            }
                        }
                    ],
                    "synonyms": [
                        "mocarz"
                    ],
                    "antonyms": [
                        "poddany"
                    ],
                    "common_phrases": [
                        "władca mórz",
                        "władca umysłów",
                        "niepodzielny władca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, имеющий полный контроль или доминирующее положение над чем-либо или кем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "властелин",
                                "target_lang_sense_clarification": "тот, кто имеет полную власть над чем-либо"
                            },
                            {
                                "target_lang_word": "повелитель",
                                "target_lang_sense_clarification": "тот, кто повелевает, господин"
                            },
                            {
                                "target_lang_word": "владыка",
                                "target_lang_sense_clarification": "полновластный хозяин (книжн.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

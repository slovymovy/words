            "senses": [
                {
                    "sense_id": "5f617eba-ffe3-3964-bebc-527267c93d4b",
                    "sense_definition": "Słowo służące do wyrażenia mocnego przekonania mówiącego o prawdziwości wypowiedzi; potwierdzenie, że coś jest pewne.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pewnosc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten młody pianista z Japonii to <w>niewątpliwie</w> wielki talent.",
                            "target_lang_translations": {
                                "ru": "Этот молодой пианист из Японии — <w>несомненно</w> большой талант."
                            }
                        },
                        {
                            "text": "Najbardziej znanym mieszkańcem Asyżu jest <w>niewątpliwie</w> św. Franciszek.",
                            "target_lang_translations": {
                                "ru": "Самым известным жителем Ассизи является, <w>несомненно</w>, святой Франциск."
                            }
                        },
                        {
                            "text": "To była <w>niewątpliwie</w> najtrudniejsza decyzja w moim życiu.",
                            "target_lang_translations": {
                                "ru": "Это было, <w>безусловно</w>, самое трудное решение в моей жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "bez wątpienia",
                        "na pewno",
                        "oczywiście",
                        "ani chybi",
                        "zapewne"
                    ],
                    "common_phrases": [
                        "niewątpliwie tak",
                        "to niewątpliwie prawda",
                        "jest niewątpliwie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое для выражения твердой уверенности говорящего в истинности высказывания; подтверждение того, что нечто является несомненным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несомненно",
                                "target_lang_sense_clarification": "выражение полной уверенности"

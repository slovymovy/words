                            {
                                "target_lang_word": "вырывать",
                                "target_lang_sense_clarification": "удалять рывком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7c67d65-d398-370f-8ddb-c4bb622bebd2",
                    "sense_definition": "Zabierać coś komuś siłą, gwałtownie lub zdobywać coś z wielkim trudem i wysiłkiem.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [

                    }
                },
                {
                    "sense_id": "69aa02ad-4667-3702-8217-fd36948cba04",
                    "sense_definition": "A town in Louisiana or a community in Ohio, United States.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They drove through the small town of <w>Welsh</w> on their road trip.",
                            "target_lang_translations": {
                                "ru": "Во время поездки они проехали через маленький городок <w>Уэлш</w>."
                            }

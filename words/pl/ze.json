{
    "word_family": [
        "z"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "032bef85-4b9f-3e56-af18-521e31076c6d",
                    "sense_definition": "Wariant fonetyczny przyimka <w>z</w>, używany dla ułatwienia wymowy przed wyrazami rozpoczynającymi się od określonych grup spółgłoskowych (np. s, z, ś, ź, ż, rz + inna spółgłoska) lub przed zaimkiem \"mnie\".",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "variant_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wróciłam wcześniej <w>ze</w> szkoły, bo źle się poczułam.",
                            "target_lang_translations": {
                                "ru": "Я вернулась <w>из</w> школы пораньше, потому что плохо себя почувствовала."
                            }
                        },
                        {
                            "text": "Czy pójdziesz <w>ze</w> mną na spacer do parku?",
                            "target_lang_translations": {
                                "ru": "Ты пойдешь <w>со</w> мной на прогулку в парк?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "ze szkoły",
                        "ze mną",
                        "ze względu na",
                        "jeden ze",
                        "ze strachu"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wariant pozycyjny przyimka z"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фонетический вариант предлога «z», используемый для облегчения произношения перед словами, начинающимися с определенных групп согласных, или перед местоимением «mnie»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "со",
                                "target_lang_sense_clarification": "предлог: вместе с кем-то / с поверхности (фонетический вариант «с»)"
                            },
                            {
                                "target_lang_word": "из",
                                "target_lang_sense_clarification": "предлог: изнутри, происхождение"
                            },
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "предлог: вместе с / с поверхности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2ee4dede-c300-3a4c-8317-3a91060d107b",
                    "sense_definition": "Partykuła stająca przed liczebnikiem, informująca, że podana liczba jest przybliżona; około.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "approximation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Daj mi jeszcze <w>ze</w> dwadzieścia minut na dokończenie pracy.",
                            "target_lang_translations": {
                                "ru": "Дай мне еще <w>около</w> двадцати минут, чтобы закончить работу."
                            }
                        },
                        {
                            "text": "Na placu zebrało się <w>ze</w> sto osób.",
                            "target_lang_translations": {
                                "ru": "На площади собралось <w>около</w> ста человек."
                            }
                        }
                    ],
                    "synonyms": [
                        "około",
                        "mniej więcej"
                    ],
                    "common_phrases": [
                        "ze dwa razy",
                        "ze trzy dni",
                        "ze sto złotych"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Частица, стоящая перед числительным и указывающая на то, что число является приблизительным; около."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "около",
                                "target_lang_sense_clarification": "приблизительно"
                            },
                            {
                                "target_lang_word": "примерно",
                                "target_lang_sense_clarification": "приблизительно"
                            },
                            {
                                "target_lang_word": "где-то",
                                "target_lang_sense_clarification": "разговорное: приблизительно"
                            }
                        ]
                    }
                }
            ]
        },

                            {
                                "target_lang_word": "уговор",
                                "target_lang_sense_clarification": "устное или неформальное соглашение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e615c644-ba64-3fc5-89fb-590e2ad1318c",
                    "sense_definition": "A formal treaty or agreement between two or more nations or political organizations.",

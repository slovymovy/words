            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "7b8d921a-59fa-3b24-ab05-d8e7ee48831e",
                    "sense_definition": "Uitroep van opluchting, tevredenheid of vermoeidheid nadat een taak is afgerond of een situatie voorbij is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hèhè</w>, we zijn eindelijk thuis na die lange reis.",
                            "target_lang_translations": {
                                "en": "<w>Phew</w>, we are finally home after that long journey.",
                                "ru": "<w>Фух</w>, мы наконец-то дома после долгого путешествия."
                            }
                        },
                        {
                            "text": "<w>Hèhè</w>, even zitten na een dag hard werken.",
                            "target_lang_translations": {
                                "en": "<w>Ah</w>, sitting down for a moment after a hard day's work.",
                                "ru": "<w>Уф</w>, наконец-то можно присесть после тяжелого рабочего дня."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hèhè, eindelijk",
                        "zo, hèhè",
                        "hèhè, even zitten"
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamation of relief, satisfaction, or fatigue after a task is completed or a situation is over.",
                        "ru": "Возглас облегчения, удовлетворения или усталости после завершения дела или окончания ситуации."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "phew",
                                "target_lang_sense_clarification": "expression of relief or fatigue"
                            },
                            {
                                "target_lang_word": "ah",
                                "target_lang_sense_clarification": "expression of satisfaction or relief"
                            },
                            {
                                "target_lang_word": "finally",
                                "target_lang_sense_clarification": "expression indicating a long-awaited conclusion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "фух",
                                "target_lang_sense_clarification": "выражение облегчения"
                            },
                            {
                                "target_lang_word": "уф",
                                "target_lang_sense_clarification": "выражение усталости"
                            },
                            {
                                "target_lang_word": "ну вот",
                                "target_lang_sense_clarification": "удовлетворение от завершения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbe3dd74-4b10-3042-b7bb-e532b87ff5ca",
                    "sense_definition": "Uitroep om op sarcastische wijze aan te geven dat een opmerking overduidelijk of logisch is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sarcasm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ja <w>hèhè</w>, het is niet dat ik dat niet snap.",
                            "target_lang_translations": {
                                "en": "Yeah <w>duh</w>, it's not like I don't get that.",
                                "ru": "Ну <w>конечно</w>, не то чтобы я этого не понимаю."
                            }
                        },
                        {
                            "text": "Je moet de stekker er wel in doen als je wilt stofzuigen. – Ja <w>hèhè</w>!",
                            "target_lang_translations": {
                                "en": "You have to plug it in if you want to vacuum. – Yeah <w>duh</w>!",
                                "ru": "Нужно вставить вилку в розетку, если хочешь пылесосить. – Ну <w>естественно</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "duh"
                    ],
                    "common_phrases": [
                        "ja hèhè"
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamation used sarcastically to indicate that a remark is obvious or logical.",
                        "ru": "Саркастическое восклицание, указывающее на то, что замечание является очевидным или логичным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "duh",
                                "target_lang_sense_clarification": "indicating something is obvious"
                            },
                            {
                                "target_lang_word": "no kidding",
                                "target_lang_sense_clarification": "sarcastic response to the obvious"
                            },
                            {
                                "target_lang_word": "obviously",
                                "target_lang_sense_clarification": "sarcastic confirmation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ну ещё бы",
                                "target_lang_sense_clarification": "указание на очевидность"
                            },
                            {
                                "target_lang_word": "ну конечно",
                                "target_lang_sense_clarification": "саркастическое согласие"
                            },
                            {
                                "target_lang_word": "да неужели",
                                "target_lang_sense_clarification": "притворное удивление очевидному"
                            }
                        ]
                    }
                }
            ]
        }
    ]

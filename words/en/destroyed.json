    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5a741aff-89eb-37ba-a77c-7c831bb451dc",
                    "sense_definition": "Simple past and past participle of <w>destroy</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "damage_ruin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The massive fire <w>destroyed</w> the old factory completely.",
                            "target_lang_translations": {
                                "ru": "Сильный пожар полностью <w>уничтожил</w> старую фабрику."
                            }
                        },
                        {
                            "text": "He felt that he had <w>destroyed</w> his chances of getting the job.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал, что <w>разрушил</w> свои шансы получить эту работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "completely destroyed",
                        "utterly destroyed",
                        "virtually destroyed"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past and past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола destroy (разрушать, уничтожать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уничтожил",
                                "target_lang_sense_clarification": "глагол (прошедшее время)"
                            },
                            {
                                "target_lang_word": "разрушил",
                                "target_lang_sense_clarification": "глагол (прошедшее время)"
                            }

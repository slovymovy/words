                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>gekte</w> maar wat toen hij dat vreemde verhaal vertelde.",
                            "target_lang_translations": {
                                "en": "He was just <w>joking</w> when he told that strange story.",
                                "ru": "Он просто <w>дурачился</w>, когда рассказывал ту странную историю."
                            }

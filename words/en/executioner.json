    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "99edf03a-cfc8-39f6-b4dc-de721f0fb323",
                    "sense_definition": "An official person who carries out the capital punishment of a criminal.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "killer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>executioner</w> wore a black mask to conceal his identity from the crowd.",
                            "target_lang_translations": {
                                "ru": "<w>Палач</w> надел черную маску, чтобы скрыть свою личность от толпы."
                            }
                        },
                        {
                            "text": "In medieval times, the public <w>executioner</w> was a feared figure in society.",
                            "target_lang_translations": {
                                "ru": "В средневековье публичный <w>палач</w> был фигурой, внушающей страх в обществе."
                            }
                        }
                    ],
                    "synonyms": [
                        "deathsman",
                        "Jack Ketch"
                    ],
                    "common_phrases": [
                        "public executioner",
                        "royal executioner",
                        "chief executioner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Должностное лицо, приводящее в исполнение смертный приговор преступнику."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "палач",
                                "target_lang_sense_clarification": "смертная казнь"
                            },
                            {
                                "target_lang_word": "экзекутор",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78849fa8-7aba-38cf-b093-213fcafbdcd6",
                    "sense_definition": "A hit man, especially one working for a criminal organization.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "killer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police identified him as a notorious <w>executioner</w> for the local cartel.",
                            "target_lang_translations": {
                                "ru": "Полиция опознала его как печально известного <w>палача</w> местного картеля."
                            }
                        },
                        {
                            "text": "He acted as a gang <w>executioner</w> to eliminate rivals.",
                            "target_lang_translations": {
                                "ru": "Он действовал как <w>ликвидатор</w> банды для устранения соперников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cartel executioner",
                        "gang executioner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наемный убийца, особенно работающий на преступную организацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наемный убийца",
                                "target_lang_sense_clarification": "киллер"
                            },
                            {
                                "target_lang_word": "ликвидатор",
                                "target_lang_sense_clarification": "бандитская группировка"
                            },
                            {
                                "target_lang_word": "палач",
                                "target_lang_sense_clarification": "каратель мафии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f70cd692-83f7-3dc8-aff1-bee478fb91b7",
                    "sense_definition": "A person who performs or carries out a specific task or duty.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was the efficient <w>executioner</w> of the director's complex vision.",
                            "target_lang_translations": {
                                "ru": "Она была эффективным <w>исполнителем</w> сложного замысла режиссера."
                            }
                        },
                        {
                            "text": "He served as the faithful <w>executioner</w> of the committee's final decision.",
                            "target_lang_translations": {
                                "ru": "Он служил верным <w>исполнителем</w> окончательного решения комитета."
                            }
                        }
                    ],
                    "synonyms": [
                        "executor"
                    ],
                    "common_phrases": [
                        "executioner of the plan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который выполняет или осуществляет определенную задачу или обязанность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исполнитель",
                                "target_lang_sense_clarification": "реализация задач"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
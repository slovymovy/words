    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "947d5a48-88a2-322e-9ee6-4abf606f5836",
                    "sense_definition": "Having more than a trace amount of alcohol in its contents.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He ordered an <w>alcoholic</w> beverage with his dinner.",
                            "target_lang_translations": {
                                "ru": "Он заказал <w>алкогольный</w> напиток к ужину."
                            }
                        },
                        {
                            "text": "This punch is <w>alcoholic</w>, so it is not suitable for children.",
                            "target_lang_translations": {
                                "ru": "Этот пунш <w>алкогольный</w>, поэтому он не подходит для детей."
                            }
                        }
                    ],
                    "synonyms": [

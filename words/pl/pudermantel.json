            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa2c5c16-5f8a-3be6-9287-64fee43042c0",
                    "sense_definition": "Lekki płaszcz podróżny, dawniej używany do ochrony ubrania przed kurzem podczas jazdy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "coat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podróżny zdjął zakurzony <w>pudermantel</w> i powiesił go w przedpokoju gospody.",
                            "target_lang_translations": {
                                "ru": "Путешественник снял запыленный <w>пыльник</w> и повесил его в прихожей постоялого двора."
                            }
                        },
                        {
                            "text": "W dawnych czasach damy zakładały <w>pudermantel</w> przed wejściem do otwartego powozu.",
                            "target_lang_translations": {
                                "ru": "В былые времена дамы надевали <w>пыльник</w> перед тем, как сесть в открытый экипаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "puderman"
                    ],
                    "common_phrases": [
                        "zakurzony pudermantel",
                        "zdjąć pudermantel"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкий дорожный плащ, который раньше использовали для защиты одежды от пыли во время поездок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пыльник",
                                "target_lang_sense_clarification": "легкое летнее пальто для защиты от пыли"
                            },
                            {
                                "target_lang_word": "дорожный плащ",
                                "target_lang_sense_clarification": "верхняя одежда для путешествий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78c0c7f9-f09d-3df7-8a29-a99b4036cc95",
                    "sense_definition": "Damskie okrycie domowe, narzucane na ramiona podczas porannej toalety, czesania lub pudrowania.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "gown",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hrabina siedziała przed lustrem w jedwabnym <w>pudermantlu</w>, podczas gdy pokojówka układała jej włosy.",
                            "target_lang_translations": {
                                "ru": "Графиня сидела перед зеркалом в шелковом <w>пеньюаре</w>, пока горничная укладывала ей волосы."
                            }
                        },
                        {
                            "text": "Kobieta narzuciła na siebie <w>pudermantel</w>, aby nie pobrudzić sukni pudrem.",
                            "target_lang_translations": {
                                "ru": "Женщина набросила на себя <w>накидку</w>, чтобы не испачкать платье пудрой."
                            }
                        }
                    ],
                    "synonyms": [
                        "peniuar",
                        "podwłośnik"
                    ],
                    "common_phrases": [
                        "jedwabny pudermantel",
                        "siedzieć w pudermantlu"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женская домашняя накидка, набрасываемая на плечи во время утреннего туалета, причесывания или пудривания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пеньюар",
                                "target_lang_sense_clarification": "утренняя женская домашняя одежда"
                            },
                            {
                                "target_lang_word": "накидка",
                                "target_lang_sense_clarification": "одежда, набрасываемая на плечи"
                            },
                            {
                                "target_lang_word": "пудермантель",
                                "target_lang_sense_clarification": "историческое название накидки для пудривания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
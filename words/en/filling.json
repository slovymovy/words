                    "common_phrases": [
                        "cushion filling",
                        "synthetic filling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой материал, используемый для набивки или наполнения предмета, например, подушки или игрушки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набивка",
                                "target_lang_sense_clarification": "материал для заполнения мягких предметов"
                            },
                            {
                                "target_lang_word": "наполнитель",
                                "target_lang_sense_clarification": "вещество или материал, заполняющий что-либо"
                            }
                        ]

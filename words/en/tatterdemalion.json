            "senses": [
                {
                    "sense_id": "999a8b5e-b07d-30d0-94f7-c4aa4a3d7eb4",
                    "sense_definition": "Wearing ragged clothing or appearing ruined and dilapidated.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "appearance_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>tatterdemalion</w> army marched wearily without proper uniforms or shoes.",
                            "target_lang_translations": {
                                "ru": "<w>Оборванная</w> армия устало маршировала без надлежащей формы и обуви."
                            }
                        },
                        {
                            "text": "He cut a <w>tatterdemalion</w> figure in his torn coat and worn-out boots.",
                            "target_lang_translations": {
                                "ru": "В своем рваном пальто и изношенных ботинках он представлял собой <w>потрепанную</w> фигуру."
                            }
                        }
                    ],
                    "synonyms": [
                        "shabby",
                        "dilapidated",
                        "bedraggled",
                        "run-down",
                        "ramshackle"
                    ],
                    "antonyms": [
                        "smart",
                        "neat",
                        "well-dressed"
                    ],
                    "common_phrases": [
                        "tatterdemalion army",
                        "tatterdemalion figure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одетый в лохмотья или выглядящий разрушенным и ветхим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оборванный",
                                "target_lang_sense_clarification": "об одежде или внешнем виде"
                            },
                            {
                                "target_lang_word": "ветхий",
                                "target_lang_sense_clarification": "о состоянии или внешнем виде"
                            },
                            {
                                "target_lang_word": "потрепанный",
                                "target_lang_sense_clarification": "изношенный, жалкий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3914d8b5-f506-3506-8f18-d1a08226c063",
                    "sense_definition": "A person wearing ragged or torn clothing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wealthy merchant ignored the <w>tatterdemalion</w> begging at the gate.",
                            "target_lang_translations": {
                                "ru": "Богатый купец проигнорировал <w>оборванца</w>, просящего милостыню у ворот."
                            }
                        },
                        {
                            "text": "A band of <w>tatterdemalions</w> gathered near the fire to keep warm.",
                            "target_lang_translations": {
                                "ru": "Группа <w>оборванцев</w> собралась у костра, чтобы согреться."
                            }
                        }
                    ],
                    "synonyms": [
                        "ragamuffin"
                    ],
                    "common_phrases": [

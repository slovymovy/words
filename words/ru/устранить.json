{
    "word_family": [
        "устранение",
        "неустранимый"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0dee2f82-20c9-36ec-89de-09c1b326224c",
                    "sense_definition": "Удалить, убрать что-либо мешающее, ненужное или опасное.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "remove_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам необходимо срочно <устранить> причину возгорания.",
                            "target_lang_translations": {
                                "en": "We need to urgently <eliminate> the cause of the fire."
                            }
                        },
                        {
                            "text": "Врачам удалось <устранить> угрозу для жизни пациента.",
                            "target_lang_translations": {
                                "en": "The doctors managed to <eliminate> the threat to the patient's life."
                            }
                        }
                    ],
                    "synonyms": [
                        "удалить",
                        "исключить"
                    ],
                    "common_phrases": [
                        "устранить причину",
                        "устранить препятствие",
                        "устранить угрозу",
                        "устранить противоречия"
                    ],
                    "target_lang_definitions": {
                        "en": "To completely remove or get rid of something that is unwanted, dangerous, or an obstacle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eliminate",
                                "target_lang_sense_clarification": "a threat, cause, or possibility"
                            },
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "an obstacle or barrier"
                            },
                            {
                                "target_lang_word": "get rid of",
                                "target_lang_sense_clarification": "something annoying or unnecessary"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "380fc62e-ceb1-38ca-a1f4-cd67cacd13fc",
                    "sense_definition": "Исправить, починить или ликвидировать недостатки, ошибки или поломки.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "remove_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Механик обещал <устранить> неисправность в двигателе к вечеру.",
                            "target_lang_translations": {
                                "en": "The mechanic promised to <fix> the engine malfunction by the evening."
                            }
                        },
                        {
                            "text": "Программисты пытаются <устранить> ошибку в новой версии приложения.",
                            "target_lang_translations": {
                                "en": "Programmers are trying to <fix> the bug in the new version of the app."
                            }
                        }
                    ],
                    "synonyms": [
                        "ликвидировать"
                    ],
                    "common_phrases": [
                        "устранить неисправность",
                        "устранить ошибку",
                        "устранить недостатки",
                        "устранить последствия"
                    ],
                    "target_lang_definitions": {
                        "en": "To correct a mistake, repair a defect, or resolve a technical issue."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fix",
                                "target_lang_sense_clarification": "a problem, bug, or malfunction"
                            },
                            {
                                "target_lang_word": "rectify",
                                "target_lang_sense_clarification": "an error or shortcoming (formal)"
                            },
                            {
                                "target_lang_word": "repair",
                                "target_lang_sense_clarification": "damage or breakage"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d434c3dc-b079-30a1-888e-ad2ef86f94d6",
                    "sense_definition": "Лишить кого-либо возможности действовать, отстранить от должности или власти.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "remove_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Совет директоров решил <устранить> его от управления компанией.",
                            "target_lang_translations": {
                                "en": "The board of directors decided to <remove> him from managing the company."
                            }
                        },
                        {
                            "text": "Политика пытались <устранить> от участия в предстоящих выборах.",
                            "target_lang_translations": {
                                "en": "They tried to <bar> the politician from participating in the upcoming elections."
                            }
                        }
                    ],
                    "synonyms": [
                        "отстранить"
                    ],
                    "common_phrases": [
                        "устранить от власти",
                        "устранить конкурента",
                        "устранить свидетеля",
                        "устранить от дел"
                    ],
                    "target_lang_definitions": {
                        "en": "To force someone out of a position of power, office, or participation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "from office or a position"
                            },
                            {
                                "target_lang_word": "oust",
                                "target_lang_sense_clarification": "from power (often forcefully)"
                            },
                            {
                                "target_lang_word": "eliminate",
                                "target_lang_sense_clarification": "a competitor or rival"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
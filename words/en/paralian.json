    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "29215a7f-3973-3eed-bc30-3fff4cd63807",
                    "sense_definition": "Someone who lives by the sea.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old <w>paralian</w> gazed out at the ocean every morning, unable to imagine life inland.",
                            "target_lang_translations": {
                                "ru": "Старый <w>житель побережья</w> каждое утро смотрел на океан, не в силах представить себе жизнь вдали от моря."
                            }
                        },
                        {
                            "text": "As a true <w>paralian</w>, she felt a deep connection to the tides and the salt air.",
                            "target_lang_translations": {
                                "ru": "Как истинная <w>жительница побережья</w>, она чувствовала глубокую связь с приливами и солёным воздухом."
                            }
                        }
                    ],
                    "antonyms": [
                        "inlander"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто живёт у моря."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "житель побережья",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "приморец",
                                "target_lang_sense_clarification": "житель приморской местности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18f62e3e-2c79-365b-ade0-9aa63286b079",
                    "sense_definition": "A member of an ancient Greek people who lived by the Athenian coast in the 6th century B.C.E.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Each <w>paralian</w> supported the political interests of the coastal region in ancient Athens.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>паралий</w> поддерживал политические интересы прибрежного региона в древних Афинах."
                            }
                        },
                        {
                            "text": "Historians analyze the conflict between the <w>paralian</w> faction and the men of the plain.",
                            "target_lang_translations": {
                                "ru": "Историки анализируют конфликт между фракцией <w>паралиев</w> и жителями равнин."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Specific to ancient Attica."
                        },
                        {
                            "trait_type": "dated",
                            "comment": "Refers to a historical group."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель древнегреческой политической группировки, населявшей побережье Аттики в VI веке до н.э."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паралий",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                }
            ]
        }

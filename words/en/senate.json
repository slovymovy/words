{
    "word_family": [
        "senator",
        "senatorial",
        "senatorship"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1d265ee5-dc1d-3d6a-9a32-a2dd45e789ea",
                    "sense_definition": "The upper house or chamber in a government with two legislative sections.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legislative_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bill was passed by the <w>senate</w> after a long debate.",
                            "target_lang_translations": {
                                "ru": "Законопроект был принят <w>сенатом</w> после долгих дебатов."
                            }
                        },
                        {
                            "text": "She decided to run for a seat in the state <w>senate</w> next year.",
                            "target_lang_translations": {
                                "ru": "Она решила баллотироваться в <w>сенат</w> штата в следующем году."
                            }
                        }
                    ],
                    "synonyms": [
                        "upper house",
                        "upper chamber"
                    ],
                    "antonyms": [
                        "lower house"
                    ],
                    "common_phrases": [
                        "state senate",
                        "senate floor",
                        "senate committee"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя палата парламента в государствах с двухпалатной системой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сенат",
                                "target_lang_sense_clarification": "верхняя палата парламента"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "098c7e5f-80cb-3e80-8307-f66b76654256",
                    "sense_definition": "A governing body of experienced individuals, often found in universities to decide on academic matters.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "governing_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The university <w>senate</w> voted to change the grading policy for all students.",
                            "target_lang_translations": {
                                "ru": "Университетский <w>сенат</w> проголосовал за изменение системы оценок для всех студентов."
                            }
                        },
                        {
                            "text": "Faculty members elected representatives to the academic <w>senate</w>.",
                            "target_lang_translations": {
                                "ru": "Преподаватели избрали представителей в академический <w>сенат</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "council",
                        "governing body"
                    ],
                    "common_phrases": [
                        "university senate",
                        "academic senate",
                        "faculty senate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Управляющий орган в университетах, решающий академические вопросы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "учёный совет",
                                "target_lang_sense_clarification": "в университете"
                            },
                            {
                                "target_lang_word": "сенат",
                                "target_lang_sense_clarification": "в западных вузах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "554af9aa-fc6b-36b4-a482-c5fb9f6c03b9",
                    "sense_definition": "The upper house of the United States Congress.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "legislative_body_proper",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "The <w>Senate</w> confirmed the president's nominee for the Supreme Court.",
                            "target_lang_translations": {
                                "ru": "<w>Сенат</w> утвердил кандидата президента в Верховный суд."
                            }
                        },
                        {
                            "text": "Elections for the US <w>Senate</w> are held every six years.",
                            "target_lang_translations": {
                                "ru": "Выборы в <w>Сенат</w> США проводятся каждые шесть лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "US Senate",
                        "the Senate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя палата Конгресса США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сенат",
                                "target_lang_sense_clarification": "США"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7a4e880-2846-3981-bea6-5b4585493b79",
                    "sense_definition": "The upper legislative body in various countries, often the more senior of two houses.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legislative_body_proper",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "The Canadian <w>Senate</w> reviews legislation passed by the House of Commons.",
                            "target_lang_translations": {
                                "ru": "Канадский <w>Сенат</w> рассматривает законы, принятые Палатой общин."
                            }
                        },
                        {
                            "text": "Members of the <w>Senate</w> are appointed rather than elected in some countries.",
                            "target_lang_translations": {
                                "ru": "В некоторых странах члены <w>Сената</w> назначаются, а не избираются."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя законодательная палата в различных странах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сенат",
                                "target_lang_sense_clarification": "государственный орган"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7e964a8-07b2-3296-bd1d-d406e2655bed",
                    "sense_definition": "The governing council of the ancient Roman Republic.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_body",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "Caesar was assassinated at a meeting of the <w>Senate</w>.",
                            "target_lang_translations": {
                                "ru": "Цезарь был убит на заседании <w>сената</w>."
                            }
                        },
                        {
                            "text": "The power of the Roman <w>Senate</w> declined significantly under the emperors.",
                            "target_lang_translations": {
                                "ru": "Власть римского <w>сената</w> значительно ослабла при императорах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Roman Senate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Правящий совет в Древнем Риме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сенат",
                                "target_lang_sense_clarification": "в Древнем Риме"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
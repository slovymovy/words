    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b198d3b6-3003-3110-b902-3fbdf1be90ba",
                    "sense_definition": "Zonder geluid of met heel weinig geluid.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de bibliotheek moet iedereen <w>stil</w> zijn zodat anderen kunnen lezen.",
                            "target_lang_translations": {
                                "en": "In the library, everyone must be <w>quiet</w> so that others can read.",
                                "ru": "В библиотеке все должны вести себя <w>тихо</w>, чтобы другие могли читать."
                            }
                        },
                        {
                            "text": "De nieuwe elektrische auto rijdt erg <w>stil</w> door de straat.",
                            "target_lang_translations": {
                                "en": "The new electric car drives very <w>quietly</w> through the street.",
                                "ru": "Новый электромобиль едет по улице очень <w>тихо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "geluidloos",
                        "geruisloos"
                    ],
                    "antonyms": [
                        "luid"
                    ],
                    "common_phrases": [
                        "muisstil zijn",
                        "doodstil",
                        "wees stil"
                    ],
                    "target_lang_definitions": {
                        "en": "Without sound or with very little sound.",
                        "ru": "Без звука или с очень тихим звуком."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quiet",
                                "target_lang_sense_clarification": "making little or no noise"
                            },
                            {
                                "target_lang_word": "silent",
                                "target_lang_sense_clarification": "completely without sound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тихий",
                                "target_lang_sense_clarification": "издающий мало шума"
                            },
                            {
                                "target_lang_word": "бесшумный",
                                "target_lang_sense_clarification": "совсем без звука"
                            },
                            {
                                "target_lang_word": "молчаливый",
                                "target_lang_sense_clarification": "неразговорчивый или безмолвный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e947c2e-5da4-37de-b108-999380052e05",
                    "sense_definition": "Zonder drukte, verstoring of veel activiteit; vredig.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "calmness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is heerlijk om in een <w>stille</w> omgeving te wonen zonder verkeer.",
                            "target_lang_translations": {
                                "en": "It is wonderful to live in a <w>quiet</w> environment without traffic.",
                                "ru": "Замечательно жить в <w>тихой</w> обстановке без движения транспорта."
                            }
                        },
                        {
                            "text": "Op zondagochtend zijn de straten in het dorp vaak <w>stil</w> en leeg.",
                            "target_lang_translations": {
                                "en": "On Sunday mornings, the streets in the village are often <w>quiet</w> and empty.",
                                "ru": "Воскресным утром улицы в деревне часто <w>тихие</w> и пустые."
                            }
                        }
                    ],
                    "synonyms": [
                        "kalm",
                        "rustig"
                    ],
                    "antonyms": [
                        "druk",
                        "bruisend"
                    ],
                    "common_phrases": [
                        "stille wateren",
                        "een stille tocht",
                        "de stille week"
                    ],
                    "target_lang_definitions": {
                        "en": "Without bustle, disturbance, or much activity; peaceful.",
                        "ru": "Без суеты, беспокойства или большой активности; мирный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quiet",
                                "target_lang_sense_clarification": "free from activity or disturbance"
                            },
                            {
                                "target_lang_word": "peaceful",
                                "target_lang_sense_clarification": "calm and tranquil"
                            },
                            {
                                "target_lang_word": "calm",
                                "target_lang_sense_clarification": "not excited or agitated"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "спокойный",
                                "target_lang_sense_clarification": "без волнения или суеты"
                            },
                            {
                                "target_lang_word": "тихий",
                                "target_lang_sense_clarification": "свободный от активности"
                            },
                            {
                                "target_lang_word": "мирный",
                                "target_lang_sense_clarification": "безмятежный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5492189-919d-370d-b9a9-53349f0fd4fa",
                    "sense_definition": "Zonder beweging; niet bewegend.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het dier bleef <w>stil</w> staan toen het ons plotseling zag.",
                            "target_lang_translations": {
                                "en": "The animal stood <w>still</w> when it suddenly saw us.",
                                "ru": "Животное стояло <w>неподвижно</w>, когда внезапно увидело нас."
                            }
                        },
                        {
                            "text": "Voor de foto moet je even heel <w>stil</w> blijven zitten.",
                            "target_lang_translations": {
                                "en": "For the photo, you have to sit very <w>still</w> for a moment.",
                                "ru": "Для фотографии тебе нужно немного посидеть совершенно <w>неподвижно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "onbeweeglijk"
                    ],
                    "common_phrases": [
                        "stil zitten",
                        "stil staan",
                        "houd je stil"
                    ],
                    "target_lang_definitions": {
                        "en": "Without movement; not moving.",
                        "ru": "Без движения; не двигаясь."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "still",
                                "target_lang_sense_clarification": "motionless"
                            },
                            {
                                "target_lang_word": "motionless",
                                "target_lang_sense_clarification": "without movement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "неподвижный",
                                "target_lang_sense_clarification": "без движения"

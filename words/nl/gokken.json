                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "играть в азартные игры",
                                "target_lang_sense_clarification": "участвовать в играх на деньги"
                            },
                            {
                                "target_lang_word": "делать ставки",
                                "target_lang_sense_clarification": "ставить деньги на результат"

                {
                    "sense_id": "c71ff634-dc37-3060-bf41-deef09ca6bcb",
                    "sense_definition": "Kobieta, często postrzegana jako nieprzyjemna, kłótliwa, zaniedbana lub w podeszłym wieku.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "woman_pejorative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jakaś wścibska <baba> ciągle zagląda nam przez płot do ogrodu.",
                            "target_lang_translations": {
                                "ru": "Какая-то любопытная <баба> постоянно заглядывает к нам в сад через забор."
                            }
                        },
                        {
                            "text": "W autobusie strasznie krzyczała na kierowcę jakaś stara <baba>.",
                            "target_lang_translations": {
                                "ru": "В автобусе на водителя страшно кричала какая-то старая <баба>."
                            }
                        }
                    ],
                    "synonyms": [
                        "babsztyl",
                        "jędza",
                        "wiedźma",
                        "pudło"
                    ],
                    "common_phrases": [
                        "wredna baba",
                        "stara baba",
                        "głupia baba"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często o wydźwięku lekceważącym lub obraźliwym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина (часто грубо: о сварливой, неопрятной или старой женщине)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баба",
                                "target_lang_sense_clarification": "грубое или просторечное"
                            },
                            {
                                "target_lang_word": "тётка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8534716b-65ef-3b66-80a9-99479ee575dc",
                    "sense_definition": "Potoczne określenie na żonę lub stałą partnerkę życiową.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "woman_partner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja <baba> zrobiła dzisiaj na obiad pyszne pierogi z mięsem.",
                            "target_lang_translations": {
                                "ru": "Моя <баба> приготовила сегодня на обед вкусные вареники с мясом."
                            }
                        },
                        {
                            "text": "Muszę zapytać mojej <baby>, czy mamy wolny weekend w przyszłym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Мне нужно спросить у моей <бабы>, свободны ли у нас выходные в следующем месяце."
                            }
                        }
                    ],
                    "synonyms": [
                        "żona",
                        "partnerka",
                        "małżonka",
                        "stara"
                    ],
                    "antonyms": [

                {
                    "sense_id": "12cb97fc-d5fe-3fc5-8b91-a73261d0c553",
                    "sense_definition": "Urządzenie optyczne służące do wykonywania zdjęć fotograficznych lub nagrywania filmów.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem nowy <aparat>, żeby robić zdjęcia podczas wakacji.",
                            "target_lang_translations": {
                                "ru": "Я купил новый <фотоаппарат>, чтобы делать снимки во время отпуска."
                            }
                        },
                        {
                            "text": "Ten <aparat> cyfrowy ma bardzo duży i wyraźny wyświetlacz.",
                            "target_lang_translations": {
                                "ru": "У этого цифрового <фотоаппарата> очень большой и четкий дисплей."
                            }
                        }
                    ],
                    "synonyms": [
                        "aparat fotograficzny",
                        "cyfrak"
                    ],
                    "common_phrases": [
                        "aparat cyfrowy",
                        "aparat fotograficzny",
                        "lampa błyskowa aparatu",
                        "obiektyw aparatu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оптическое устройство для съемки фотографий или записи видео."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фотоаппарат",
                                "target_lang_sense_clarification": "устройство для фотосъемки"
                            },
                            {
                                "target_lang_word": "камера",
                                "target_lang_sense_clarification": "разговорное, цифровая камера"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bc1e388-9c38-3f42-b502-212fdee5a003",
                    "sense_definition": "Urządzenie techniczne służące do przekazywania dźwięku na odległość, zazwyczaj w odniesieniu do telefonu stacjonarnego.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę chwilę poczekać przy <aparacie>, zaraz zawołam dyrektora.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подождите минуту у <аппарата>, я сейчас позову директора."
                            }
                        },
                        {
                            "text": "W starym biurze stał jeszcze tradycyjny <aparat> telefoniczny z kablem.",
                            "target_lang_translations": {
                                "ru": "В старом офисе стоял еще традиционный телефонный <аппарат> с проводом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aparat telefoniczny",
                        "przy aparacie",
                        "odłożyć słuchawkę aparatu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Техническое устройство для передачи звука на расстояние, обычно стационарный телефон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "телефонный аппарат",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "аппарат",
                                "target_lang_sense_clarification": "в выражениях: у аппарата"

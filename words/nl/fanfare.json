    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ee1d7259-1ab6-3e81-aaa9-870b72b76ace",
                    "sense_definition": "Een orkest dat bestaat uit koperblazers, saxofoons en slagwerk.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn broer speelt al jaren trompet in de lokale <w>fanfare</w>.",
                            "target_lang_translations": {
                                "ru": "Мой брат уже много лет играет на трубе в местном <w>духовом оркестре</w>."
                            }
                        },
                        {
                            "text": "Tijdens de optocht liep de <w>fanfare</w> voorop en speelde vrolijke muziek.",
                            "target_lang_translations": {
                                "ru": "Во время шествия <w>духовой оркестр</w> шел впереди и играл веселую музыку."
                            }
                        }
                    ],
                    "synonyms": [
                        "fanfarekorps",
                        "blaasorkest"
                    ],
                    "common_phrases": [
                        "bij de fanfare",
                        "de lokale fanfare"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оркестр, состоящий из медных духовых инструментов, саксофонов и ударных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "духовой оркестр",
                                "target_lang_sense_clarification": "музыкальный коллектив"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de799bae-82d0-3962-84de-3230f9f65106",
                    "sense_definition": "Een kort, feestelijk muziekstuk gespeeld door koperblazers.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koning werd met een luide <w>fanfare</w> verwelkomd in de stad.",
                            "target_lang_translations": {
                                "ru": "Короля приветствовали в городе громкими <w>фанфарами</w>."
                            }
                        },
                        {
                            "text": "Er klonk een <w>fanfare</w> toen de winnaars het podium betraden.",
                            "target_lang_translations": {
                                "ru": "Зазвучали <w>фанфары</w>, когда победители вышли на сцену."
                            }
                        }
                    ],
                    "synonyms": [
                        "trompetgeschal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткая торжественная музыкальная пьеса, исполняемая на медных духовых инструментах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фанфары",
                                "target_lang_sense_clarification": "музыкальный сигнал"
                            },
                            {
                                "target_lang_word": "туш",
                                "target_lang_sense_clarification": "короткое музыкальное приветствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2f3fc0b-8f7c-3231-af2d-30927804d2b5",
                    "sense_definition": "Grote drukte, ophef of vertoon, vaak overdreven.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij vertrok uiteindelijk zonder veel <w>fanfare</w> uit het bedrijf.",
                            "target_lang_translations": {
                                "ru": "В конце концов, он ушел из компании без особой <w>помпы</w>."
                            }
                        },
                        {
                            "text": "De nieuwe maatregel werd met veel <w>fanfare</w> aangekondigd, maar werkte niet.",
                            "target_lang_translations": {
                                "ru": "О новой мере объявили с большой <w>помпой</w>, но она не сработала."
                            }
                        }
                    ],
                    "synonyms": [
                        "ophef",
                        "bombarie",
                        "poeha"
                    ],
                    "common_phrases": [
                        "zonder veel fanfare",
                        "met veel fanfare"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая суета, шумиха или показуха, часто преувеличенная."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помпа",
                                "target_lang_sense_clarification": "пышность, торжественность"
                            },
                            {
                                "target_lang_word": "шумиха",
                                "target_lang_sense_clarification": "громкая огласка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
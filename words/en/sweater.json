    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ad6cc5a-d01b-38dd-92af-e9a8cb2e30ac",
                    "sense_definition": "A knitted garment, typically with long sleeves, worn on the upper body for warmth.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "garment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is cold outside, so you should put on a warm <w>sweater</w>.",
                            "target_lang_translations": {
                                "ru": "На улице холодно, поэтому тебе стоит надеть теплый <w>свитер</w>."
                            }
                        },
                        {
                            "text": "She knitted a beautiful wool <w>sweater</w> for her brother.",
                            "target_lang_translations": {
                                "ru": "Она связала красивый шерстяной <w>свитер</w> для своего брата."
                            }
                        }
                    ],
                    "synonyms": [
                        "jumper",
                        "pullover",
                        "jersey"
                    ],
                    "common_phrases": [
                        "wear a sweater",
                        "wool sweater",
                        "knitted sweater",
                        "warm sweater"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вязаная одежда, обычно с длинными рукавами, которую носят на верхней части тела для тепла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свитер",
                                "target_lang_sense_clarification": "с высоким воротником"
                            },
                            {
                                "target_lang_word": "джемпер",
                                "target_lang_sense_clarification": "без воротника, обычно с круглым вырезом"
                            },
                            {
                                "target_lang_word": "пуловер",
                                "target_lang_sense_clarification": "с V-образным вырезом"
                            },
                            {
                                "target_lang_word": "кофта",
                                "target_lang_sense_clarification": "общее название вязаной одежды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c0a9206-a59f-32eb-8b33-eea9c391cf3c",
                    "sense_definition": "A person who sweats profusely.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_agent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am a heavy <w>sweater</w>, so I always bring a towel to the gym.",
                            "target_lang_translations": {
                                "ru": "Я очень <w>потливый человек</w>, поэтому всегда беру с собой полотенце в спортзал."
                            }
                        },
                        {
                            "text": "The doctor noted that the patient was a profuse <w>sweater</w> during the stress test.",
                            "target_lang_translations": {
                                "ru": "Врач отметил, что пациент был обильно <w>потеющим</w> во время стресс-теста."
                            }
                        }
                    ],
                    "synonyms": [
                        "perspirer"
                    ],
                    "common_phrases": [
                        "heavy sweater",
                        "profuse sweater"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который сильно и часто потеет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потливый человек",
                                "target_lang_sense_clarification": "описание человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75aaa74a-d514-30e5-8f77-98ae45cb3718",
                    "sense_definition": "An employer or middleman who underpays workers and forces them to work in poor conditions.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person_agent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>sweater</w> made a profit by exploiting the labor of the tailors.",
                            "target_lang_translations": {
                                "ru": "<w>Эксплуататор</w> получал прибыль, используя труд портных."
                            }
                        },
                        {
                            "text": "Unions were formed to protect workers from the greed of the <w>sweater</w>.",
                            "target_lang_translations": {
                                "ru": "Профсоюзы создавались для защиты рабочих от жадности <w>эксплуататора</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "exploiter"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical labor context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Работодатель или посредник, который недоплачивает работникам и заставляет их работать в тяжелых условиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эксплуататор",
                                "target_lang_sense_clarification": "исторический или социальный контекст"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f240ae1e-762d-348f-b8d0-3a76105b6f44",
                    "sense_definition": "To dress someone in a sweater.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mother <w>sweatered</w> the child before sending him out into the snow.",
                            "target_lang_translations": {
                                "ru": "Мать <w>надела свитер</w> на ребенка перед тем, как отправить его на снег."
                            }
                        },
                        {
                            "text": "Once fully <w>sweatered</w>, the small dog looked quite ridiculous but warm.",
                            "target_lang_translations": {
                                "ru": "Будучи полностью <w>одетой в свитер</w>, маленькая собачка выглядела довольно нелепо, но ей было тепло."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Надеть на кого-то свитер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надевать свитер",
                                "target_lang_sense_clarification": "на кого-либо"
                            },
                            {
                                "target_lang_word": "одевать в свитер",
                                "target_lang_sense_clarification": "кого-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
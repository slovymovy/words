    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "376d9061-133d-35b3-a472-6d08b7c7f19a",
                    "sense_definition": "В левую сторону; в направлении, противоположном правому.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На следующем перекрёстке вам нужно повернуть <w>влево</w>.",
                            "target_lang_translations": {
                                "en": "At the next intersection you need to turn <w>left</w>."
                            }
                        },
                        {
                            "text": "Он посмотрел <w>влево</w> и увидел своего старого друга.",
                            "target_lang_translations": {
                                "en": "He looked <w>to the left</w> and saw his old friend."
                            }
                        }
                    ],
                    "synonyms": [
                        "налево",
                        "по левую сторону"
                    ],
                    "antonyms": [
                        "вправо",
                        "впра́во"
                    ],
                    "common_phrases": [
                        "повернуть влево",
                        "посмотреть влево",
                        "шаг влево",
                        "сдвинуть влево"
                    ],
                    "target_lang_definitions": {
                        "en": "To the left side; in the direction opposite to the right."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to the left",
                                "target_lang_sense_clarification": "direction"
                            },
                            {
                                "target_lang_word": "left",
                                "target_lang_sense_clarification": "direction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18c87fe7-941e-37ed-9613-497914be72b9",
                    "sense_definition": "В сторону радикальных, социалистических или демократических убеждений (о политических взглядах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Политика новой партии резко качнулась <w>влево</w> после выборов.",
                            "target_lang_translations": {
                                "en": "The new party's policy swung sharply <w>to the left</w> after the elections."
                            }
                        },
                        {
                            "text": "Его взгляды с годами изменились, и он ушёл <w>влево</w>.",
                            "target_lang_translations": {
                                "en": "His views changed over the years, and he moved <w>to the left</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "налево"
                    ],
                    "antonyms": [
                        "вправо"
                    ],
                    "common_phrases": [
                        "сдвиг влево",
                        "поворот влево",
                        "уклон влево"
                    ],
                    "target_lang_definitions": {
                        "en": "Towards radical, socialist, or democratic beliefs (regarding political views)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to the left",
                                "target_lang_sense_clarification": "political spectrum"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
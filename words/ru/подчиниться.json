    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b1fa95b2-72e4-3205-bf9e-98c604e1e70f",
                    "sense_definition": "Согласиться выполнить чью-либо волю, приказ или требование; начать действовать по установленным правилам.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Все сотрудники обязаны безоговорочно <w>подчиниться</w> новым правилам безопасности.",
                            "target_lang_translations": {
                                "en": "All employees are required to unconditionally <w>comply with</w> the new safety rules."
                            }
                        },
                        {
                            "text": "Он решил <w>подчиниться</w> приказу командира, несмотря на свои сомнения.",
                            "target_lang_translations": {
                                "en": "He decided to <w>obey</w> the commander's order, despite his doubts."
                            }
                        }
                    ],
                    "synonyms": [
                        "послушаться",
                        "повиноваться"
                    ],
                    "antonyms": [
                        "ослушаться",
                        "воспротивиться"
                    ],
                    "common_phrases": [
                        "подчиниться приказу",
                        "подчиниться правилам",
                        "подчиниться закону",
                        "подчиниться решению"
                    ],
                    "target_lang_definitions": {
                        "en": "To agree to fulfill someone's will, order, or requirement; to start acting according to established rules."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "obey",
                                "target_lang_sense_clarification": "general term for following orders or people"
                            },
                            {
                                "target_lang_word": "comply with",
                                "target_lang_sense_clarification": "formal, usually regarding rules or regulations"
                            },
                            {
                                "target_lang_word": "submit to",
                                "target_lang_sense_clarification": "to accept authority or control"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e48c7d0-13af-31cd-8d8e-71f95bbfe4d5",
                    "sense_definition": "Перестать оказывать сопротивление и признать чью-либо власть или силу; смириться с чем-либо неизбежным.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Город отказался <w>подчиниться</w> вражеской армии и продолжил оборону.",
                            "target_lang_translations": {
                                "en": "The city refused to <w>submit</w> to the enemy army and continued its defense."
                            }
                        },
                        {
                            "text": "Иногда разумнее <w>подчиниться</w> обстоятельствам и подождать лучшего момента для действий.",
                            "target_lang_translations": {
                                "en": "Sometimes it is wiser to <w>yield</w> to circumstances and wait for a better moment to act."
                            }
                        }
                    ],
                    "synonyms": [
                        "покориться",
                        "сдаться",
                        "уступить"
                    ],
                    "antonyms": [
                        "сопротивляться",
                        "бороться"
                    ],
                    "common_phrases": [
                        "подчиниться силе",
                        "подчиниться судьбе",
                        "подчиниться обстоятельствам"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop resisting and acknowledge someone's power or force; to resign oneself to something inevitable."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "submit",
                                "target_lang_sense_clarification": "to stop resisting power or force"
                            },
                            {
                                "target_lang_word": "yield",
                                "target_lang_sense_clarification": "to give way to pressure or circumstances"
                            },
                            {
                                "target_lang_word": "bow to",
                                "target_lang_sense_clarification": "figurative, to accept fate or the inevitable"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
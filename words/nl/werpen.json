                {
                    "sense_id": "c242fc17-988f-3003-956d-ca0c96edd4fb",
                    "sense_definition": "Iets met een zwaai van de arm door de lucht laten vliegen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>wierp</w> de bal naar de andere kant van het veld.",
                            "target_lang_translations": {
                                "en": "He <w>threw</w> the ball to the other side of the field.",
                                "ru": "Он <w>бросил</w> мяч на другую сторону поля."
                            }
                        },
                        {
                            "text": "Zij <w>wierp</w> een snelle blik op de klok om te zien hoe laat het was.",
                            "target_lang_translations": {
                                "en": "She <w>cast</w> a quick glance at the clock to see what time it was.",
                                "ru": "Она <w>бросила</w> быстрый взгляд на часы, чтобы посмотреть, который час."
                            }
                        }
                    ],
                    "synonyms": [
                        "gooien"
                    ],
                    "common_phrases": [
                        "een blik werpen",
                        "een schaduw werpen",
                        "het anker werpen"
                    ],
                    "target_lang_definitions": {
                        "en": "To propel something through the air with a swing of the arm or hand.",
                        "ru": "Заставлять что-либо лететь по воздуху взмахом руки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "throw",
                                "target_lang_sense_clarification": "to propel through the air"
                            },

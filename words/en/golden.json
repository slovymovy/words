                },
                {
                    "sense_id": "b05b9d4d-b23a-339a-aea4-21354866dcb9",
                    "sense_definition": "Fine, without problems.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Just follow the instructions carefully and you will be <w>golden</w>.",

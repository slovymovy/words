    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6192499d-f638-3f17-ac36-4d70a043f7b5",
                    "sense_definition": "Iemand in een slecht humeur brengen of ergeren.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn arrogante houding zal de andere collega's zeker <w>ontstemmen</w>.",
                            "target_lang_translations": {
                                "en": "His arrogant attitude will certainly <w>displease</w> the other colleagues."
                            }
                        },
                        {
                            "text": "Het slechte nieuws <w>ontstemde</w> de directeur zichtbaar.",
                            "target_lang_translations": {
                                "en": "The bad news visibly <w>upset</w> the director."
                            }
                        }
                    ],
                    "synonyms": [
                        "ergeren",
                        "irriteren",
                        "misnoegen"
                    ],
                    "antonyms": [
                        "plezieren",
                        "verblijden"
                    ],
                    "common_phrases": [
                        "iemand hevig ontstemmen",
                        "de kiezers ontstemmen"
                    ],
                    "target_lang_definitions": {
                        "en": "To put someone in a bad mood or annoy them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "displease",
                                "target_lang_sense_clarification": "cause annoyance or unhappiness"
                            },
                            {
                                "target_lang_word": "upset",
                                "target_lang_sense_clarification": "make unhappy or disappointed"
                            },
                            {
                                "target_lang_word": "annoy",
                                "target_lang_sense_clarification": "irritate or bother"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42ff2d37-109c-342f-b621-e899d3f11882",
                    "sense_definition": "De zuivere stemming van een muziekinstrument verliezen of verstoren.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de plotselinge temperatuurwisseling kan de piano snel <w>ontstemmen</w>.",
                            "target_lang_translations": {
                                "en": "Due to the sudden temperature change, the piano can quickly <w>go out of tune</w>."
                            }
                        },
                        {
                            "text": "Als je te veel aan de mechanieken draait, <w>ontstem</w> je de gitaar.",
                            "target_lang_translations": {
                                "en": "If you turn the tuning pegs too much, you <w>detune</w> the guitar."
                            }
                        }
                    ],
                    "antonyms": [
                        "stemmen"
                    ],
                    "common_phrases": [
                        "een instrument ontstemmen",
                        "de piano ontstemmen"
                    ],
                    "target_lang_definitions": {
                        "en": "To lose or disturb the tuning of a musical instrument."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "detune",
                                "target_lang_sense_clarification": "cause to be out of tune"
                            },
                            {
                                "target_lang_word": "go out of tune",
                                "target_lang_sense_clarification": "lose proper pitch"
                            },
                            {
                                "target_lang_word": "put out of tune",
                                "target_lang_sense_clarification": "disturb the pitch of an instrument"
                            }
                        ]
                    }
                }
            ]
        }
    ]

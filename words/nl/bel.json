                {
                    "sense_id": "a42ea24e-2cb2-39e7-9a81-08c26eef1988",
                    "sense_definition": "Een voorwerp of apparaat dat een rinkelend of luidend geluid maakt, vaak gebruikt als signaal om de aandacht te trekken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sound_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De postbode drukte op de <w>bel</w> omdat het pakketje niet door de brievenbus paste.",
                            "target_lang_translations": {
                                "en": "The mail carrier pressed the <w>bell</w> because the package didn't fit through the letterbox.",
                                "ru": "Почтальон нажал на <w>звонок</w>, так как посылка не пролезала в почтовый ящик."
                            }
                        },
                        {
                            "text": "Aan de halsband van de kat hangt een klein <w>belletje</w>.",
                            "target_lang_translations": {
                                "en": "A small <w>bell</w> hangs from the cat's collar.",
                                "ru": "На ошейнике кота висит маленький <w>колокольчик</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan de bel trekken",
                        "de bel gaat",
                        "op de bel drukken"
                    ],
                    "target_lang_definitions": {
                        "en": "An object or device that makes a ringing or tolling sound, often used as a signal to attract attention.",
                        "ru": "Предмет или устройство, издающее звон или громкий звук, часто используемое как сигнал для привлечения внимания."
                    },

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "aa4b98dc-1ec9-3aa3-8334-cf9fc6521533",
                    "sense_definition": "Nawiązać z kimś łączność, rozmowę lub relację, zazwyczaj używając telefonu, internetu lub spotykając się osobiście.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Próbowałem się z tobą bezskutecznie <skontaktować> przez cały wczorajszy dzień.",
                            "target_lang_translations": {
                                "ru": "Я безуспешно пытался <связаться> с тобой весь вчерашний день."
                            }
                        },
                        {
                            "text": "Jeśli masz jakiekolwiek pytania, proszę <skontaktować> się z naszym biurem obsługi.",
                            "target_lang_translations": {
                                "ru": "Если у вас есть какие-либо вопросы, пожалуйста, <свяжитесь> с нашим отделом обслуживания."
                            }
                        }
                    ],
                    "synonyms": [
                        "skomunikować się",
                        "spiknąć"
                    ],
                    "common_phrases": [
                        "skontaktować się telefonicznie",
                        "skontaktować się mailowo",
                        "skontaktować się z obsługą",
                        "proszę się skontaktować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Установить связь, начать разговор или отношения с кем-либо, обычно по телефону, интернету или при личной встрече."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связаться",
                                "target_lang_sense_clarification": "установить контакт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e94f08a1-06ae-30ec-87b2-b79c09a50dda",
                    "sense_definition": "Umożliwić dwóm lub więcej osobom (lub podmiotom) nawiązanie relacji, rozmowy lub spotkanie się ze sobą.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "communication_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój znajomy może cię <skontaktować> z dobrym prawnikiem, który pomoże w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "Мой знакомый может <свести> тебя с хорошим юристом, который поможет в этом деле."
                            }
                        },
                        {
                            "text": "Organizatorzy wydarzenia postanowili <skontaktować> ze sobą wszystkich uczestników projektu.",
                            "target_lang_translations": {
                                "ru": "Организаторы мероприятия решили <связать> друг с другом всех участников проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "spiknąć",
                        "spiknąć"
                    ],
                    "common_phrases": [
                        "skontaktować kogoś z kimś",
                        "skontaktować dwie strony",
                        "skontaktować ze sobą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помочь двум или более людям (или организациям) установить связь, начать разговор или встретиться друг с другом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свести",
                                "target_lang_sense_clarification": "помочь встретиться или наладить контакт"
                            },
                            {
                                "target_lang_word": "связать",
                                "target_lang_sense_clarification": "соединить кого-то с кем-то"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
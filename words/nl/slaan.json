            "senses": [
                {
                    "sense_id": "ae9352ac-9929-38b7-bb44-053777f07414",
                    "sense_definition": "Iemand of iets met kracht raken, meestal met de hand of een voorwerp.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>sloeg</w> met zijn vuist hard op de tafel.",
                            "target_lang_translations": {
                                "ru": "Он сильно <w>ударил</w> кулаком по столу.",
                                "en": "He <w>hit</w> the table hard with his fist."
                            }
                        },
                        {
                            "text": "Je mag je kleine broertje nooit zomaar <w>slaan</w>.",
                            "target_lang_translations": {
                                "ru": "Нельзя просто так <w>бить</w> своего младшего братика.",
                                "en": "You must never just <w>hit</w> your little brother."
                            }
                        }
                    ],
                    "synonyms": [
                        "meppen"
                    ],
                    "common_phrases": [
                        "iemand slaan",
                        "hard slaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ударять кого-то или что-то с силой, обычно рукой или предметом.",
                        "en": "To touch someone or something with force, usually with the hand or an object."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "наносить удары"
                            },
                            {
                                "target_lang_word": "ударять",
                                "target_lang_sense_clarification": "совершать удар"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "hit",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "strike",
                                "target_lang_sense_clarification": "more formal"
                            },
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "repeatedly or severely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ea9ae2c-1e73-3094-85f5-1e9b72ace3b7",
                    "sense_definition": "Een geluid maken om de tijd aan te geven (van een klok).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude klok in de gang <w>slaat</w> elk heel uur.",
                            "target_lang_translations": {
                                "ru": "Старые часы в коридоре <w>бьют</w> каждый полный час.",
                                "en": "The old clock in the hall <w>strikes</w> every full hour."
                            }
                        },
                        {
                            "text": "We wisten precies hoe laat het was toen de klok <w>sloeg</w>.",
                            "target_lang_translations": {
                                "ru": "Мы точно знали, который час, когда часы <w>пробили</w>.",
                                "en": "We knew exactly what time it was when the clock <w>struck</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de klok slaat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать звук, указывая время (о часах).",
                        "en": "To make a sound to indicate the time (of a clock)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "о часах"
                            },
                            {
                                "target_lang_word": "пробить",
                                "target_lang_sense_clarification": "о времени"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "strike",
                                "target_lang_sense_clarification": "of a clock"
                            },
                            {
                                "target_lang_word": "chime",
                                "target_lang_sense_clarification": "make a musical sound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f2fa405-eecd-37ef-bb2c-db817668a47e",
                    "sense_definition": "Ritmisch bewegen en pompen van bloed (van het hart).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn hart <w>sloeg</w> een slag over van schrik.",
                            "target_lang_translations": {
                                "ru": "Моё сердце <w>пропустило</w> удар от испуга.",
                                "en": "My heart <w>skipped</w> a beat from fright."
                            }
                        },
                        {
                            "text": "Na het rennen <w>sloeg</w> zijn hart erg snel.",
                            "target_lang_translations": {
                                "ru": "После бега его сердце <w>билось</w> очень быстро.",
                                "en": "After running, his heart <w>beat</w> very fast."
                            }
                        }
                    ],
                    "synonyms": [
                        "meppen"
                    ],
                    "common_phrases": [
                        "het hart slaat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ритмично двигаться и качать кровь (о сердце).",
                        "en": "To move rhythmically and pump blood (of the heart)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "биться",
                                "target_lang_sense_clarification": "о сердце"
                            },
                            {
                                "target_lang_word": "стучать",
                                "target_lang_sense_clarification": "пульсировать"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "pulsate"
                            },
                            {
                                "target_lang_word": "throb",
                                "target_lang_sense_clarification": "beat strongly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c62d129-2e55-3935-a470-3a6bfea8b906",
                    "sense_definition": "Een andere richting kiezen tijdens het rijden of lopen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij het volgende stoplicht moeten we rechtsaf <w>slaan</w>.",
                            "target_lang_translations": {
                                "ru": "На следующем светофоре нам нужно <w>повернуть</w> направо.",
                                "en": "At the next traffic light we have to <w>turn</w> right."
                            }
                        },
                        {
                            "text": "De auto <w>sloeg</w> onverwachts een smal zijstraatje in.",
                            "target_lang_translations": {
                                "ru": "Машина неожиданно <w>свернула</w> в узкий переулок.",
                                "en": "The car unexpectedly <w>turned</w> into a narrow side street."
                            }
                        }
                    ],
                    "synonyms": [
                        "meppen"
                    ],
                    "common_phrases": [
                        "linksaf slaan",
                        "rechtsaf slaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выбирать другое направление во время езды или ходьбы.",
                        "en": "To choose a different direction while driving or walking."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поворачивать",
                                "target_lang_sense_clarification": "менять направление"
                            },
                            {
                                "target_lang_word": "сворачивать",
                                "target_lang_sense_clarification": "уходить в сторону"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "change direction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72df0d4f-a3d0-35e2-8d0c-543888cfc8cb",
                    "sense_definition": "Winnen van een tegenstander in een wedstrijd of gevecht.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het team <w>sloeg</w> de tegenstander met grote cijfers.",
                            "target_lang_translations": {
                                "ru": "Команда <w>разгромила</w> противника с крупным счётом.",
                                "en": "The team <w>beat</w> the opponent by a large margin."
                            }
                        },
                        {
                            "text": "Ik denk dat ik jou wel kan <w>slaan</w> met schaken.",
                            "target_lang_translations": {
                                "ru": "Думаю, я смогу <w>обыграть</w> тебя в шахматы.",
                                "en": "I think I can <w>beat</w> you at chess."
                            }
                        }
                    ],
                    "synonyms": [
                        "meppen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выигрывать у противника в соревновании или драке.",
                        "en": "To win against an opponent in a match or fight."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побеждать",
                                "target_lang_sense_clarification": "одерживать верх"
                            },
                            {
                                "target_lang_word": "обыгрывать",
                                "target_lang_sense_clarification": "в игре или спорте"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "defeat someone"
                            },
                            {
                                "target_lang_word": "defeat",
                                "target_lang_sense_clarification": "win against"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc7e33fe-ac4d-3ffa-8bc4-1f764740c801",
                    "sense_definition": "Iets om iemand of iets heen vouwen of leggen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>sloeg</w> troostend een arm om haar schouders.",
                            "target_lang_translations": {
                                "ru": "Он утешающе <w>обнял</w> её за плечи.",
                                "en": "He comfortingly <w>put</w> an arm around her shoulders."
                            }
                        },
                        {
                            "text": "Als het koud is, kun je deze sjaal om je heen <w>slaan</w>.",
                            "target_lang_translations": {
                                "ru": "Если холодно, ты можешь <w>накинуть</w> этот шарф на себя.",
                                "en": "If it is cold, you can <w>wrap</w> this scarf around you."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een arm om iemand slaan",
                        "een doek om slaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оборачивать или класть что-то вокруг кого-то или чего-то.",
                        "en": "To fold or place something around someone or something."

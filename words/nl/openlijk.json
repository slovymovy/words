    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b9077f3d-7865-334b-8f21-4a6bdf851796",
                    "sense_definition": "Niet geheim of verborgen, maar voor iedereen duidelijk zichtbaar en waarneembaar.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "public_visible",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>openlijke</w> ruzie tussen de twee broers zorgde voor een ongemakkelijke sfeer op het feest.",
                            "target_lang_translations": {
                                "en": "The <w>public</w> quarrel between the two brothers caused an awkward atmosphere at the party.",
                                "ru": "<w>Открытая</w> ссора между двумя братьями создала неловкую атмосферу на вечеринке."
                            }
                        },
                        {
                            "text": "Zij kreeg veel <w>openlijke</w> steun van haar collega's tijdens de moeilijke periode.",
                            "target_lang_translations": {
                                "en": "She received a lot of <w>open</w> support from her colleagues during the difficult period.",
                                "ru": "Она получила большую <w>открытую</w> поддержку от своих коллег в этот трудный период."
                            }
                        }
                    ],
                    "synonyms": [
                        "publiekelijk",
                        "onverholen",
                        "openbaar",
                        "manifest"
                    ],
                    "antonyms": [
                        "heimelijk",
                        "verborgen",
                        "stiekem"
                    ],
                    "common_phrases": [
                        "openlijke steun",
                        "openlijke kritiek",
                        "openlijke geweldpleging",
                        "openlijke vijandigheid"
                    ],
                    "target_lang_definitions": {
                        "en": "Not secret or hidden, but clearly visible and perceptible to everyone.",
                        "ru": "Не тайный и не скрытый, а ясно видимый и заметный для всех."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overt",
                                "target_lang_sense_clarification": "Done or shown openly; plainly apparent."
                            },
                            {
                                "target_lang_word": "open",
                                "target_lang_sense_clarification": "Not concealed; generally known."
                            },
                            {
                                "target_lang_word": "public",
                                "target_lang_sense_clarification": "Done in plain view of others."
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "не скрываемый, явный"
                            },
                            {
                                "target_lang_word": "публичный",
                                "target_lang_sense_clarification": "совершаемый в присутствии общества"
                            },
                            {
                                "target_lang_word": "явный",
                                "target_lang_sense_clarification": "очевидный, нескрываемый"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9e9c6b59-33c9-3f58-85a3-c86029b8f99d",
                    "sense_definition": "Op een manier die voor iedereen zichtbaar is; zonder iets achter te houden of te verbergen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "public_visible",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politicus durfde <w>openlijk</w> te twijfelen aan de beslissingen van zijn eigen partij.",
                            "target_lang_translations": {
                                "en": "The politician dared to <w>openly</w> question the decisions of his own party.",
                                "ru": "Политик осмелился <w>открыто</w> усомниться в решениях своей собственной партии."
                            }
                        },
                        {
                            "text": "Hij heeft <w>openlijk</w> toegegeven dat hij fout zat tijdens de vergadering.",
                            "target_lang_translations": {
                                "en": "He <w>openly</w> admitted that he was wrong during the meeting.",
                                "ru": "Он <w>открыто</w> признал во время собрания, что был неправ."
                            }
                        }
                    ],
                    "synonyms": [
                        "ronduit",
                        "vrijuit",
                        "publiekelijk"
                    ],
                    "antonyms": [
                        "heimelijk",
                        "in het geheim"
                    ],
                    "common_phrases": [
                        "openlijk toegeven",
                        "openlijk twijfelen",
                        "openlijk bespreken",
                        "zich openlijk uitspreken"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that is visible to everyone; without holding anything back or hiding.",
                        "ru": "Таким образом, что это видно всем; ничего не утаивая и не скрывая."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "openly",
                                "target_lang_sense_clarification": "Without concealment; frankly."
                            },
                            {
                                "target_lang_word": "publicly",
                                "target_lang_sense_clarification": "In an open, public manner."
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "открыто",
                                "target_lang_sense_clarification": "прямо, не таясь"
                            },
                            {
                                "target_lang_word": "публично",
                                "target_lang_sense_clarification": "при всех, официально"
                            }
                        ]
                    }
                }
            ]
        }

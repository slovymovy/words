                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сине-белый",
                                "target_lang_sense_clarification": "о символике или команде"
                            },
                            {
                                "target_lang_word": "бело-синий",
                                "target_lang_sense_clarification": "о символике или команде"
                            }

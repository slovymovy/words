{
    "word_family": [
        "misgauge",
        "gauger"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7dc937c9-6e11-3e9e-8665-43a3129830b6",
                    "sense_definition": "An instrument used to measure and display the amount, level, or dimensions of something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "measurement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fuel <w>gauge</w> indicated that the tank was almost empty.",
                            "target_lang_translations": {
                                "ru": "Топливный <w>датчик</w> показывал, что бак почти пуст."
                            }
                        },
                        {
                            "text": "You should use a pressure <w>gauge</w> to check the air in your tires regularly.",
                            "target_lang_translations": {
                                "ru": "Вам следует регулярно использовать <w>манометр</w> для проверки давления в шинах."
                            }
                        }
                    ],
                    "synonyms": [
                        "ear gauge"
                    ],
                    "common_phrases": [
                        "pressure gauge",
                        "fuel gauge",
                        "temperature gauge",
                        "rain gauge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибор для измерения и отображения количества, уровня или размеров чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "датчик",
                                "target_lang_sense_clarification": "устройство для измерения уровня (топлива, температуры и т.д.)"
                            },
                            {
                                "target_lang_word": "манометр",
                                "target_lang_sense_clarification": "прибор для измерения давления"
                            },
                            {
                                "target_lang_word": "измерительный прибор",
                                "target_lang_sense_clarification": "общее название инструментов измерения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae127d12-4618-384b-97c8-5dbd5007de16",
                    "sense_definition": "A fact, event, or feeling that can be used to estimate or judge a situation.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "estimation_judgment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voter turnout is often a good <w>gauge</w> of public interest in the election.",
                            "target_lang_translations": {
                                "ru": "Явка избирателей часто является хорошим <w>показателем</w> общественного интереса к выборам."
                            }
                        },
                        {
                            "text": "The stock market serves as a <w>gauge</w> of the country's economic health.",
                            "target_lang_translations": {
                                "ru": "Фондовый рынок служит <w>индикатором</w> экономического здоровья страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "ear gauge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Факт, событие или чувство, которые могут быть использованы для оценки или суждения о ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показатель",
                                "target_lang_sense_clarification": "данные, по которым можно судить о состоянии чего-либо"
                            },
                            {
                                "target_lang_word": "критерий",
                                "target_lang_sense_clarification": "мерило оценки"
                            },
                            {
                                "target_lang_word": "индикатор",
                                "target_lang_sense_clarification": "признак, указывающий на состояние ситуации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db3f8d94-7a44-3bbc-a8f9-95a87ead6f86",
                    "sense_definition": "A standard unit of measurement for the thickness of sheet metal or wire, or the diameter of a gun barrel.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The electrician decided to use a heavier <w>gauge</w> of wire for the circuit.",
                            "target_lang_translations": {
                                "ru": "Электрик решил использовать провод большего <w>сечения</w> для цепи."
                            }
                        },
                        {
                            "text": "He went hunting with a 12-<w>gauge</w> shotgun.",
                            "target_lang_translations": {
                                "ru": "Он пошел на охоту с ружьем 12-го <w>калибра</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ear gauge"
                    ],
                    "common_phrases": [
                        "12-gauge",
                        "heavy gauge",
                        "light gauge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стандартная единица измерения толщины листового металла или проволоки, либо диаметра ствола оружия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "калибр",
                                "target_lang_sense_clarification": "диаметр канала ствола оружия"
                            },
                            {
                                "target_lang_word": "толщина",
                                "target_lang_sense_clarification": "размер сечения провода или листа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29201902-859f-3316-a4fd-84083087c22a",
                    "sense_definition": "A type of jewelry worn in a stretched earlobe hole, or the hole itself.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wears a large black <w>gauge</w> in her left ear.",
                            "target_lang_translations": {
                                "ru": "Она носит большой черный <w>тоннель</w> в левом ухе."
                            }
                        },
                        {
                            "text": "It takes a lot of time and patience to stretch your ears for a larger <w>gauge</w>.",
                            "target_lang_translations": {
                                "ru": "Требуется много времени и терпения, чтобы растянуть уши под больший <w>диаметр</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ear gauge"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in youth culture and body modification contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид украшения, которое носят в растянутом отверстии мочки уха, или само это отверстие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тоннель",
                                "target_lang_sense_clarification": "украшение в виде кольца, вставляемое в прокол уха"
                            },
                            {
                                "target_lang_word": "плаг",
                                "target_lang_sense_clarification": "сплошное украшение для растянутого прокола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3f6933e-d6e1-331a-b771-1dccffbd8ba9",
                    "sense_definition": "The distance between the two rails of a railway track.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train cannot travel on this line because the <w>gauge</w> is different.",
                            "target_lang_translations": {
                                "ru": "Поезд не может ходить по этой линии, потому что <w>колея</w> другая."
                            }
                        },
                        {
                            "text": "Most countries in Europe use the standard <w>gauge</w> for their railway networks.",
                            "target_lang_translations": {
                                "ru": "Большинство стран Европы используют стандартную <w>ширину колеи</w> для своих железнодорожных сетей."
                            }
                        }
                    ],
                    "synonyms": [
                        "ear gauge"
                    ],
                    "common_phrases": [
                        "narrow gauge",
                        "standard gauge",
                        "broad gauge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расстояние между двумя рельсами железнодорожного пути."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колея",
                                "target_lang_sense_clarification": "железнодорожный путь"
                            },
                            {
                                "target_lang_word": "ширина колеи",
                                "target_lang_sense_clarification": "расстояние между рельсами"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9162732e-b136-32a2-ab7e-934bd85a79de",
                    "sense_definition": "To make a judgment about a situation, a person's character, or their feelings.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "estimation_judgment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I tried to <w>gauge</w> his reaction to the news, but he remained expressionless.",
                            "target_lang_translations": {
                                "ru": "Я пытался <w>оценить</w> его реакцию на новости, но он оставался бесстрастным."
                            }
                        },
                        {
                            "text": "It is difficult to <w>gauge</w> the mood of the voters before the election.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>определить</w> настроение избирателей перед выборами."
                            }
                        }
                    ],
                    "synonyms": [
                        "ear gauge"
                    ],
                    "common_phrases": [
                        "gauge the reaction",
                        "gauge the mood",
                        "gauge the situation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Составить суждение о ситуации, характере человека или его чувствах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оценивать",
                                "target_lang_sense_clarification": "составлять мнение о чем-либо"
                            },
                            {
                                "target_lang_word": "определять",
                                "target_lang_sense_clarification": "выявлять, устанавливать (настроение, реакцию)"
                            },
                            {
                                "target_lang_word": "прикидывать",
                                "target_lang_sense_clarification": "разг. оценивать приблизительно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d651d06a-c523-3bd3-b24a-7062abef86df",
                    "sense_definition": "To measure the exact size, amount, or capacity of something, often using a tool.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "measurement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The carpenter carefully <w>gauged</w> the width of the door frame.",
                            "target_lang_translations": {
                                "ru": "Плотник тщательно <w>измерил</w> ширину дверной коробки."
                            }
                        },
                        {
                            "text": "Scientists are trying to <w>gauge</w> the effect of the new drug on patients.",
                            "target_lang_translations": {
                                "ru": "Ученые пытаются <w>измерить</w> эффект нового лекарства на пациентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "ear gauge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Измерить точный размер, количество или вместимость чего-либо, часто с помощью инструмента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "измерять",
                                "target_lang_sense_clarification": "определять величину"
                            },
                            {
                                "target_lang_word": "замерять",
                                "target_lang_sense_clarification": "производить замер"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "cac36ff5-7a58-36a4-b126-37c2a83346c1",
                    "sense_definition": "A male given name.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My uncle <w>Gauge</w> lives in the countryside.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>Гейдж</w> живет в сельской местности."
                            }
                        },
                        {
                            "text": "We decided to name our newborn son <w>Gauge</w>.",
                            "target_lang_translations": {
                                "ru": "Мы решили назвать нашего новорожденного сына <w>Гейдж</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ear gauge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское личное имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Гейдж",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
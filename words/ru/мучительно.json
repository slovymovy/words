    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "66555bac-64af-3246-9133-d669505c95d7",
                    "sense_definition": "В очень сильной, крайней степени (обычно о неприятных чувствах, ощущениях или желаниях).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему стало <w>мучительно</w> стыдно за свои грубые слова.",
                            "target_lang_translations": {
                                "en": "He felt <w>excruciatingly</w> ashamed of his rude words."
                            }
                        },
                        {
                            "text": "Мне <w>мучительно</w> хотелось узнать правду, но я боялся спросить.",
                            "target_lang_translations": {
                                "en": "I <w>painfully</w> wanted to know the truth, but I was afraid to ask."
                            }
                        }
                    ],
                    "synonyms": [
                        "невыносимо",
                        "нестерпимо"
                    ],
                    "common_phrases": [
                        "мучительно стыдно",
                        "мучительно больно",
                        "мучительно хочется"
                    ],
                    "target_lang_definitions": {
                        "en": "To an extreme, often unbearable degree (regarding feelings or desires)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "excruciatingly",
                                "target_lang_sense_clarification": "used to emphasize an extreme degree of something painful or shameful"
                            },
                            {
                                "target_lang_word": "unbearably",
                                "target_lang_sense_clarification": "to a degree that is hard to endure"
                            },
                            {
                                "target_lang_word": "painfully",
                                "target_lang_sense_clarification": "intensely or distressingly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "860bfd6a-267c-3781-ab8e-985d6cc71745",
                    "sense_definition": "С чувством муки, боли или страдания; причиняя страдания.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Время в ожидании поезда тянулось <w>мучительно</w> долго.",
                            "target_lang_translations": {
                                "en": "The time waiting for the train dragged on <w>agonizingly</w> slowly."
                            }
                        },
                        {
                            "text": "Раненый солдат <w>мучительно</w> стонал во сне.",
                            "target_lang_translations": {
                                "en": "The wounded soldier groaned <w>painfully</w> in his sleep."
                            }
                        }
                    ],
                    "synonyms": [
                        "невыносимо",
                        "нестерпимо"
                    ],
                    "antonyms": [
                        "блаженно"
                    ],
                    "common_phrases": [
                        "мучительно долго",
                        "мучительно трудно"
                    ],
                    "target_lang_definitions": {
                        "en": "In a manner characterized by suffering, pain, or difficulty"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "agonizingly",
                                "target_lang_sense_clarification": "in a way that causes great physical or mental pain"
                            },
                            {
                                "target_lang_word": "painfully",
                                "target_lang_sense_clarification": "with pain or difficulty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28b08ae0-c837-3dca-b2ce-b788ae922584",
                    "sense_definition": "Оценка ситуации или состояния как тяжёлых, вызывающих физические или нравственные страдания.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Смотреть на страдания больного ребёнка было <w>мучительно</w>.",
                            "target_lang_translations": {
                                "en": "It was <w>painful</w> to watch the sick child suffering."
                            }
                        },
                        {
                            "text": "Нам всем <w>мучительно</w> осознавать, что мы ничего не можем изменить.",
                            "target_lang_translations": {
                                "en": "It is <w>agonizing</w> for us all to realize that we cannot change anything."
                            }
                        }
                    ],
                    "common_phrases": [
                        "было мучительно",
                        "стало мучительно"
                    ],
                    "target_lang_definitions": {
                        "en": "Causing mental or physical suffering (used as a state or assessment)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "painful",
                                "target_lang_sense_clarification": "causing distress or suffering"
                            },
                            {
                                "target_lang_word": "agonizing",
                                "target_lang_sense_clarification": "causing great physical or mental pain"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "afdd1b54-0101-3479-88f6-51e0e754f8dd",
                    "sense_definition": "Краткая форма среднего рода прилагательного <w>мучительный</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это долгое ожидание ответа совершенно <w>мучительно</w>.",
                            "target_lang_translations": {
                                "en": "This long wait for an answer is absolutely <w>agonizing</w>."
                            }
                        },
                        {
                            "text": "Принимать такое сложное решение всегда <w>мучительно</w> для совести.",
                            "target_lang_translations": {
                                "en": "Making such a difficult decision is always <w>torturous</w> for the conscience."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short neuter singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short form of the adjective 'agonizing' or 'torturous'"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "agonizing",
                                "target_lang_sense_clarification": "extremely painful or difficult"
                            },
                            {
                                "target_lang_word": "torturous",
                                "target_lang_sense_clarification": "involving or causing torture or suffering"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "13107962-1048-3679-bfa3-f66399ddf33b",
                    "sense_definition": "Populacja drobnoustrojów (np. bakterii lub wirusów) w obrębie jednego gatunku, wyróżniająca się specyficznymi cechami genetycznymi lub fizjologicznymi.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "biology_strain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naukowcy zidentyfikowali nowy <w>szczep</w> wirusa grypy.",
                            "target_lang_translations": {
                                "ru": "Ученые идентифицировали новый <w>штамм</w> вируса гриппа."
                            }
                        },
                        {
                            "text": "Ten <w>szczep</w> bakterii jest odporny na większość antybiotyków.",
                            "target_lang_translations": {
                                "ru": "Этот <w>штамм</w> бактерий устойчив к большинству антибиотиков."
                            }
                        }
                    ],
                    "synonyms": [
                        "plemię",
                        "plemię"
                    ],
                    "common_phrases": [
                        "szczep bakterii",
                        "szczep wirusa",
                        "odporny szczep",
                        "nowy szczep"
                    ],
                    "target_lang_definitions": {
                        "ru": "Популяция микроорганизмов (например, бактерий или вирусов) в пределах одного вида, отличающаяся специфическими генетическими или физиологическими признаками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штамм",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68284c97-c7e8-3927-b5e8-7110a068ac9c",
                    "sense_definition": "Grupa ludzi połączona wspólnym pochodzeniem, kulturą i terytorium, składająca się z rodów lub klanów; forma organizacji społecznej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "anthropology_tribe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wódz zebrał cały <w>szczep</w> na wieczorną naradę.",
                            "target_lang_translations": {
                                "ru": "Вождь собрал все <w>племя</w> на вечернее совещание."
                            }
                        },
                        {
                            "text": "Ten starożytny <w>szczep</w> zamieszkiwał tereny dzisiejszej Amazonii.",
                            "target_lang_translations": {
                                "ru": "Это древнее <w>племя</w> населяло территории современной Амазонии."
                            }
                        }
                    ],
                    "synonyms": [
                        "plemię"
                    ],
                    "common_phrases": [
                        "szczep indiański",
                        "wódz szczepu",
                        "członek szczepu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, объединенная общим происхождением, культурой и территорией, состоящая из родов или кланов; форма социальной организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "племя",
                                "target_lang_sense_clarification": "антропология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "914df88f-3e93-3d23-800d-96e22a542091",
                    "sense_definition": "Jednostka organizacyjna w harcerstwie, skupiająca kilka drużyn działających zazwyczaj w jednym środowisku (np. w jednej szkole).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "scouting_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz <w>szczep</w> organizuje w tym roku obóz letni w górach.",
                            "target_lang_translations": {
                                "ru": "Наша <w>дружина</w> организует в этом году летний лагерь в горах."
                            }
                        },
                        {
                            "text": "Komendantem tego <w>szczepu</w> został doświadczony instruktor.",
                            "target_lang_translations": {
                                "ru": "Комендантом этой <w>дружины</w> стал опытный инструктор."
                            }
                        }
                    ],
                    "common_phrases": [
                        "komenda szczepu",
                        "rada szczepu",
                        "szczep harcerski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организационная единица в скаутском движении, объединяющая несколько отрядов, действующих обычно в одной среде (например, в одной школе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дружина",
                                "target_lang_sense_clarification": "скаутская организация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5427bb3-8d9b-3c69-aaa2-a8d9fef0c56d",
                    "sense_definition": "Pęd lub pąk rośliny szlachetnej przeniesiony na inną roślinę (podkładkę) w celu jej uszlachetnienia; także roślina uzyskana w ten sposób.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "botany_graft",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ogrodnik ostrożnie przymocował <w>szczep</w> do gałęzi jabłoni.",
                            "target_lang_translations": {
                                "ru": "Садовник осторожно прикрепил <w>привой</w> к ветке яблони."
                            }
                        },
                        {
                            "text": "Młody <w>szczep</w> winorośli wymaga dużo słońca i odpowiedniej gleby.",
                            "target_lang_translations": {
                                "ru": "Молодой <w>черенок</w> винограда требует много солнца и подходящей почвы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szczep winorośli",
                        "szlachetny szczep"
                    ],
                    "target_lang_definitions": {
                        "ru": "Побег или почка культурного растения, перенесенные на другое растение (подвой) для его облагораживания; также растение, полученное таким способом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привой",
                                "target_lang_sense_clarification": "ботаника"
                            },
                            {
                                "target_lang_word": "черенок",
                                "target_lang_sense_clarification": "садоводство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e07b2e05-edc7-353c-98ce-945df0265d15",
                    "sense_definition": "Druga osoba liczby pojedynczej trybu rozkazującego czasownika <w>szczepić</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie czekaj dłużej i <w>szczep</w> psa przeciwko wściekliźnie.",
                            "target_lang_translations": {
                                "ru": "Не жди больше и <w>прививай</w> собаку от бешенства."
                            }
                        },
                        {
                            "text": "<w>Szczep</w> te drzewka wiosną, aby lepiej się przyjęły.",
                            "target_lang_translations": {
                                "ru": "<w>Прививай</w> эти деревца весной, чтобы они лучше прижились."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма повелительного наклонения второго лица единственного числа глагола \"szczepić\" (прививать, вакцинировать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прививай",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            },
                            {
                                "target_lang_word": "вакцинируй",
                                "target_lang_sense_clarification": "медицина"
                            }
                        ]
                    }
                }
            ]
        }
    ]

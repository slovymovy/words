            "senses": [
                {
                    "sense_id": "5cca8458-98fb-3249-b1d6-f7fe8d7d6a8e",
                    "sense_definition": "Lacking a hem; having an edge that has not been folded back and sewn.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>unhemmed</w> skirt would soon unravel, but it would last long enough for the play.",
                            "target_lang_translations": {
                                "ru": "<w>Неподшитая</w> юбка скоро начала бы осыпаться, но для спектакля её бы хватило."
                            }
                        },
                        {
                            "text": "He bought a pair of <w>unhemmed</w> trousers so he could tailor them to his exact height.",
                            "target_lang_translations": {
                                "ru": "Он купил <w>неподшитые</w> брюки, чтобы подогнать их точно под свой рост."
                            }
                        }
                    ],
                    "synonyms": [
                        "unfinished",
                        "raw"
                    ],
                    "antonyms": [
                        "hemmed",
                        "finished"
                    ],
                    "common_phrases": [
                        "unhemmed edge",
                        "unhemmed trousers",
                        "unhemmed pants"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий подшитого края; с краем, который не был подогнут и прошит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неподшитый",
                                "target_lang_sense_clarification": "о ткани или одежде"
                            },
                            {
                                "target_lang_word": "с необработанным краем",
                                "target_lang_sense_clarification": "описание состояния среза"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "116c3efa-53dd-309b-a274-9a63b80a2914",
                    "sense_definition": "Simple past and past participle of <w>unhem</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She carefully <w>unhemmed</w> the vintage dress to release the extra fabric at the bottom.",
                            "target_lang_translations": {
                                "ru": "Она аккуратно <w>распорола подшивку</w> винтажного платья, чтобы выпустить лишнюю ткань снизу."
                            }
                        },
                        {
                            "text": "Once the curtains were <w>unhemmed</w>, they were long enough to cover the entire window.",
                            "target_lang_translations": {

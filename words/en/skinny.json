    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "984da532-4b05-343b-9f4f-7dd6635ac825",
                    "sense_definition": "Very thin, especially in a way that is unattractive or shows a lack of strength.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her recent weight loss has made her look <w>skinny</w> rather than slender.",
                            "target_lang_translations": {
                                "ru": "Из-за недавней потери веса она стала выглядеть <w>тощей</w>, а не стройной."
                            }
                        },
                        {
                            "text": "He was a <w>skinny</w> boy who looked like he needed a good meal.",
                            "target_lang_translations": {
                                "ru": "Он был <w>худым</w> мальчиком, который выглядел так, будто ему нужно хорошо поесть."
                            }
                        }
                    ],
                    "synonyms": [
                        "thin"
                    ],
                    "antonyms": [
                        "fat"
                    ],
                    "common_phrases": [
                        "skinny legs",
                        "skinny kid",
                        "tall and skinny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень худой, особенно если это выглядит непривлекательно или указывает на слабость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тощий",
                                "target_lang_sense_clarification": "очень худой, часто с негативным оттенком"
                            },
                            {
                                "target_lang_word": "худой",
                                "target_lang_sense_clarification": "имеющий мало жира или мышц"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8cf9250-0528-3747-9b50-ef2e6ad8fa40",
                    "sense_definition": "Designed to fit very closely to the body, especially of clothing.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a pair of <w>skinny</w> jeans to wear with her new boots.",
                            "target_lang_translations": {
                                "ru": "Она купила <w>обтягивающие</w> джинсы, чтобы носить их с новыми ботинками."
                            }
                        },
                        {
                            "text": "I do not find <w>skinny</w> trousers very comfortable to wear all day.",
                            "target_lang_translations": {
                                "ru": "Я не нахожу <w>узкие</w> брюки очень удобными для ношения весь день."
                            }

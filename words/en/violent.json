                },
                {
                    "sense_id": "c4e9625e-8a29-34b0-8f8e-0ffaa97f9e78",
                    "sense_definition": "Likely to use physical force to hurt others.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_harm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police warned the public that the escaped prisoner is armed and <w>violent</w>.",

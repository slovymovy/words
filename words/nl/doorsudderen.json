                    ],
                    "synonyms": [
                        "doorstoven"
                    ],
                    "common_phrases": [
                        "zachtjes laten doorsudderen",
                        "even laten doorsudderen"
                    ],
                    "target_lang_definitions": {
                        "en": "To let something continue cooking slowly over a low heat.",
                        "ru": "Медленно доваривать или тушить блюдо на слабом огне."
                    },
                    "translations": {

                },
                {
                    "sense_id": "6068d2b9-40a5-3189-81ff-9241aea1debd",
                    "sense_definition": "A man or person, often described with sympathy, affection, or envy rather than insult.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Look at that lucky <w>bastard</w> driving a brand new sports car.",
                            "target_lang_translations": {
                                "ru": "Посмотри на этого <w>везунчика</w> за рулем новенького спорткара."
                            }
                        },
                        {
                            "text": "The poor <w>bastard</w> has to work a double shift on Christmas Day.",
                            "target_lang_translations": {
                                "ru": "<w>Бедняге</w> приходится работать в две смены на Рождество."
                            }

                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>note</w> that the library will be closed tomorrow.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>обратите внимание</w>, что библиотека завтра будет закрыта."
                            }
                        },
                        {
                            "text": "She <w>noted</w> a slight change in the color of the solution.",
                            "target_lang_translations": {
                                "ru": "Она <w>заметила</w> небольшое изменение в цвете раствора."
                            }
                        }
                    ],
                    "common_phrases": [
                        "please note",
                        "duly noted"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращать внимание на что-либо или замечать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замечать",
                                "target_lang_sense_clarification": "видеть, обнаруживать"
                            },
                            {
                                "target_lang_word": "обращать внимание",
                                "target_lang_sense_clarification": "уделять внимание"
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cdd75cde-0a9c-342a-833d-7c4a0e442b89",
                    "sense_definition": "Sankcja, środek wychowawczy lub dolegliwość stosowana wobec kogoś, kto popełnił przestępstwo, wykroczenie lub zrobił coś niewłaściwego",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "punishment_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd skazał oskarżonego na <karę> dwóch lat więzienia.",
                            "target_lang_translations": {
                                "ru": "Суд назначил обвиняемому <наказание> в виде двух лет лишения свободы."
                            }
                        },
                        {
                            "text": "Za <karę> ojciec odebrał jej kluczyki od samochodu.",
                            "target_lang_translations": {
                                "ru": "В <наказание> отец забрал у нее ключи от машины."
                            }
                        },
                        {
                            "text": "Nauczyciel wymierzył uczniowi <karę> za nieodrobienie pracy domowej.",
                            "target_lang_translations": {
                                "ru": "Учитель назначил ученику <наказание> за невыполненное домашнее задание."
                            }
                        }
                    ],
                    "antonyms": [
                        "nagroda"
                    ],
                    "common_phrases": [
                        "kara śmierci",
                        "kara pozbawienia wolności",
                        "kara grzywny",
                        "za karę",
                        "wymierzyć karę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Санкция, воспитательная мера или неприятность, применяемая к кому-либо, кто совершил преступление, проступок или сделал что-то неправильное"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наказание",
                                "target_lang_sense_clarification": "мера воздействия за проступок"
                            },
                            {
                                "target_lang_word": "кара",
                                "target_lang_sense_clarification": "возмездие, суровое наказание (книжн.)"
                            }
                        ]
                    }
                },
                {

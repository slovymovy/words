{
    "word_family": [
        "daarom",
        "erom",
        "waarom",
        "hier",
        "om"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2d6087fe-8ffc-36ef-9659-0ce28cc0cad2",
                    "sense_definition": "Om deze reden; geeft een oorzaak of verklaring aan die direct verband houdt met het voorgaande of het huidige.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reason",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het regende hard en <w>hierom</w> deed ik het raam dicht.",
                            "target_lang_translations": {
                                "en": "It was raining hard and <w>because of this</w> I closed the window."
                            }
                        },
                        {
                            "text": "Hij heeft jarenlang getraind en <w>hierom</w> won hij de wedstrijd.",
                            "target_lang_translations": {
                                "en": "He trained for years and <w>for this reason</w> he won the competition."
                            }
                        }
                    ],
                    "synonyms": [
                        "daarom"
                    ],
                    "common_phrases": [
                        "precies hierom",
                        "juist hierom"
                    ],
                    "target_lang_definitions": {
                        "en": "For this reason; indicates a cause or explanation directly related to the preceding or current situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "because of this",
                                "target_lang_sense_clarification": "indicating a direct cause"
                            },
                            {
                                "target_lang_word": "for this reason",
                                "target_lang_sense_clarification": "formal explanation"
                            },
                            {
                                "target_lang_word": "therefore",
                                "target_lang_sense_clarification": "consequence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba9a00d3-0a93-3c89-a6df-62e29350fe41",
                    "sense_definition": "Vervanging voor de combinatie van het voorzetsel <w>om</w> en het aanwijzend voornaamwoord <w>dit</w> of <w>deze</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het gaat mij niet om het geld, maar <w>hierom</w>.",
                            "target_lang_translations": {
                                "en": "It is not about the money for me, but <w>about this</w>."
                            }
                        },
                        {
                            "text": "Alles in dit verhaal draait uiteindelijk <w>hierom</w>.",
                            "target_lang_translations": {
                                "en": "Everything in this story ultimately revolves <w>around this</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het gaat hierom"
                    ],
                    "target_lang_definitions": {
                        "en": "Pronominal adverb replacing the combination of the preposition 'om' (about/for) and the demonstrative 'dit' (this)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "about this",
                                "target_lang_sense_clarification": "concerning this matter"
                            },
                            {
                                "target_lang_word": "for this",
                                "target_lang_sense_clarification": "regarding this purpose"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
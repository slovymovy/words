                    "sense_id": "0635db72-a50b-3cbe-b0e9-db272f13222a",
                    "sense_definition": "Iets in omloop brengen of over een groter oppervlak verdelen",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>verspreid</w> het diagram onder mijn collega's.",
                            "target_lang_translations": {
                                "en": "I <w>distribute</w> the diagram among my colleagues."
                            }
                        },
                        {
                            "text": "De wind <w>verspreidt</w> de zaden over het hele veld.",
                            "target_lang_translations": {
                                "en": "The wind <w>scatters</w> the seeds over the entire field."
                            }
                        },
                        {
                            "text": "Deze ziekte wordt door ratten en hun vlooien <w>verspreid</w>.",
                            "target_lang_translations": {
                                "en": "This disease is <w>spread</w> by rats and their fleas."
                            }
                        }
                    ],
                    "synonyms": [

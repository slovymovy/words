                        "referentie"
                    ],
                    "common_phrases": [
                        "onder kenmerk",
                        "uw kenmerk",
                        "ons kenmerk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ссылочный номер или код на документе или в административной записи.",
                        "en": "A reference number or code on a document or in an administration."
                    },
                    "translations": {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f8e67e7a-5f20-33d3-9e9b-4c289cc6c927",
                    "sense_definition": "Proces niszczenia środowiska naturalnego i wyczerpywania jego zasobów.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Postępująca <w>degradacja</w> środowiska naturalnego wymaga natychmiastowych działań.",
                            "target_lang_translations": {
                                "ru": "Прогрессирующая <w>деградация</w> окружающей среды требует немедленных действий."
                            }
                        },
                        {
                            "text": "Wycinka lasów powoduje szybką <w>degradację</w> gleby w tym regionie.",
                            "target_lang_translations": {
                                "ru": "Вырубка лесов приводит к быстрой <w>деградации</w> почвы в этом регионе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "degradacja środowiska",
                        "degradacja gleby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс разрушения природной среды и истощения её ресурсов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "деградация",
                                "target_lang_sense_clarification": "экология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "caba6fac-9ebc-387b-868c-7e9cacf57d0d",
                    "sense_definition": "Zmiana czyjegoś stanowiska zawodowego lub stopnia wojskowego na niższy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego nagła <w>degradacja</w> była szokiem dla całego zespołu.",
                            "target_lang_translations": {
                                "ru": "Его внезапное <w>понижение в должности</w> стало шоком для всей команды."
                            }
                        },
                        {
                            "text": "Za poważne naruszenie regulaminu grozi <w>degradacja</w> do niższego stopnia.",
                            "target_lang_translations": {
                                "ru": "За серьезное нарушение устава грозит <w>разжалование</w> в звании."
                            }
                        }
                    ],
                    "synonyms": [
                        "deklasacja"
                    ],
                    "antonyms": [
                        "awans"
                    ],
                    "common_phrases": [
                        "degradacja zawodowa",
                        "degradacja społeczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перевод на более низкую должность или понижение в воинском звании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понижение в должности",
                                "target_lang_sense_clarification": "работа"
                            },
                            {
                                "target_lang_word": "разжалование",
                                "target_lang_sense_clarification": "армия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abfe8f57-48ff-38b7-b594-f08794efc3bb",
                    "sense_definition": "Obniżenie wartości, znaczenia lub poziomu czegoś; pogorszenie się stanu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obserwujemy powolną <w>degradację</w> zabytkowej dzielnicy miasta.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдаем медленную <w>деградацию</w> исторического района города."
                            }
                        },
                        {
                            "text": "Kryzys gospodarczy spowodował <w>degradację</w> wielu grup społecznych.",
                            "target_lang_translations": {
                                "ru": "Экономический кризис привел к <w>деградации</w> многих социальных групп."
                            }
                        }
                    ],
                    "synonyms": [
                        "spadek"
                    ],
                    "common_phrases": [
                        "degradacja moralna",
                        "degradacja kultury"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снижение ценности, значимости или уровня чего-либо; ухудшение состояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "деградация",
                                "target_lang_sense_clarification": "ухудшение свойств или личности"
                            },
                            {
                                "target_lang_word": "упадок",
                                "target_lang_sense_clarification": "состояние разрухи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36701ef3-be7f-3b54-a974-d19902ec5744",
                    "sense_definition": "Rozkład związku chemicznego na prostsze związki.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten plastik ulega całkowitej <w>degradacji</w> w ciągu kilku miesięcy.",
                            "target_lang_translations": {
                                "ru": "Этот пластик подвергается полному <w>разложению</w> в течение нескольких месяцев."
                            }
                        },
                        {
                            "text": "Badamy proces <w>degradacji</w> tego polimeru pod wpływem światła.",
                            "target_lang_translations": {
                                "ru": "Мы изучаем процесс <w>деградации</w> этого полимера под воздействием света."
                            }
                        }

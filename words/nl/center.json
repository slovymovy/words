                        "ru": [
                            {
                                "target_lang_word": "центровой",
                                "target_lang_sense_clarification": "позиция в спорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ec3b53a-af04-3eac-b230-db96dfb83b13",

                    }
                },
                {
                    "sense_id": "1ae914ff-f332-3fa8-bd1e-cf4d276167de",
                    "sense_definition": "Situated immediately before, after, or facing something else in a sequence or arrangement.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_proximity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The picture is on the <w>adjacent</w> page.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "63d8e1d6-4455-38f7-a5f8-26af8cff3313",
                    "sense_definition": "Zdolność organizmu do obrony przed chorobami, wirusami i bakteriami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą moja <w>odporność</w> jest słabsza, więc staram się brać witaminy.",
                            "target_lang_translations": {
                                "ru": "Зимой мой <w>иммунитет</w> слабее, поэтому я стараюсь принимать витамины."
                            }
                        },
                        {
                            "text": "Regularny sport i zdrowa dieta pomagają budować naturalną <w>odporność</w> organizmu.",
                            "target_lang_translations": {
                                "ru": "Регулярный спорт и здоровая диета помогают укреплять естественный <w>иммунитет</w> организма."
                            }
                        }
                    ],
                    "synonyms": [
                        "uodpornienie",
                        "immunizacja"
                    ],
                    "antonyms": [
                        "podatność"
                    ],
                    "common_phrases": [
                        "odporność organizmu",
                        "spadek odporności",
                        "budować odporność",
                        "niska odporność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность организма защищаться от болезней, вирусов и бактерий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иммунитет",
                                "target_lang_sense_clarification": "биологическая защита организма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34177b97-e398-32cf-a92f-ac291e474245",
                    "sense_definition": "Cecha materiału lub przedmiotu polegająca na niewrażliwości na działanie czynników zewnętrznych, uszkodzenia lub zniszczenie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_property",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowoczesny telefon charakteryzuje się dużą <w>odpornością</w> na wodę i kurz.",
                            "target_lang_translations": {
                                "ru": "Этот современный телефон отличается высокой <w>устойчивостью</w> к воде и пыли."
                            }
                        },
                        {
                            "text": "Inżynierowie musieli sprawdzić <w>odporność</w> mostu na bardzo silny wiatr.",
                            "target_lang_translations": {
                                "ru": "Инженеры должны были проверить <w>устойчивость</w> моста к очень сильному ветру."
                            }
                        }
                    ],
                    "synonyms": [
                        "wytrzymałość",
                        "twardość"
                    ],
                    "antonyms": [
                        "wrażliwość",
                        "podatność"
                    ],
                    "common_phrases": [
                        "odporność na wodę",
                        "odporność na wstrząsy",
                        "odporność materiału",
                        "duża odporność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство материала или предмета не поддаваться воздействию внешних факторов, повреждениям или разрушению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устойчивость",
                                "target_lang_sense_clarification": "сопротивляемость воздействию"
                            },
                            {
                                "target_lang_word": "стойкость",
                                "target_lang_sense_clarification": "прочность, выносливость материала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b77b200-07e7-3de1-b3c0-3e85b8bf19d4",
                    "sense_definition": "Umiejętność psychiczna radzenia sobie z trudnymi sytuacjami, stresem lub presją.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_trait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Praca na stanowisku kierowniczym wymaga ogromnej <w>odporności</w> na stres i krytykę.",
                            "target_lang_translations": {
                                "ru": "Работа на руководящей должности требует огромной <w>устойчивости</w> к стрессу и критике."
                            }
                        },
                        {
                            "text": "Jej <w>odporność</w> psychiczna pozwoliła jej przetrwać ten trudny okres w życiu.",
                            "target_lang_translations": {
                                "ru": "Её психологическая <w>устойчивость</w> позволила ей пережить этот трудный период в жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "wytrzymałość"
                    ],
                    "antonyms": [
                        "wrażliwość",
                        "czułość"
                    ],
                    "common_phrases": [
                        "odporność na stres",
                        "odporność psychiczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Психологическая способность справляться с трудными ситуациями, стрессом или давлением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устойчивость",
                                "target_lang_sense_clarification": "психологическая"
                            },
                            {
                                "target_lang_word": "стойкость",
                                "target_lang_sense_clarification": "сила духа"
                            }
                        ]
                    }
                }
            ]
        }
    ]

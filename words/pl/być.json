                },
                {
                    "sense_id": "422302aa-fc86-396c-8e6f-c63fc9be305e",
                    "sense_definition": "Czasownik posiłkowy służący do tworzenia strony biernej w połączeniu z imiesłowem biernym.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "auxiliary_passive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drzwi wejściowe do budynku <są> zamykane codziennie o godzinie dwudziestej.",
                            "target_lang_translations": {
                                "ru": "Входные двери в здание <закрываются> ежедневно в двадцать часов."
                            }
                        },
                        {
                            "text": "Ten piękny obraz <był> namalowany przez bardzo znanego artystę.",
                            "target_lang_translations": {
                                "ru": "Эта прекрасная картина <была> написана очень известным художником."
                            }
                        }
                    ],
                    "common_phrases": [
                        "być zrobionym",
                        "być otwartym",
                        "być kochanym",
                        "być przygotowanym"
                    ],
                    "traits": [

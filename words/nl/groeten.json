    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1725da1c-8d86-3480-8fcf-937e85aef026",
                    "sense_definition": "Iemand welkom heten of gedag zeggen met woorden of een gebaar.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <groette> mij direct toen ik zijn huis binnenkwam.",
                            "target_lang_translations": {
                                "en": "He <greeted> me immediately when I entered his house.",
                                "ru": "Он сразу <поприветствовал> меня, когда я вошел в его дом."
                            }
                        },
                        {
                            "text": "Ik <groet> de buren altijd vriendelijk als ik ze op straat zie.",
                            "target_lang_translations": {
                                "en": "I always <greet> the neighbors kindly when I see them on the street.",
                                "ru": "Я всегда дружелюбно <здороваюсь> с соседями, когда вижу их на улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "begroeten"
                    ],
                    "antonyms": [
                        "negeren"
                    ],
                    "common_phrases": [
                        "vriendelijk groeten",
                        "iemand groeten"
                    ],
                    "target_lang_definitions": {
                        "en": "To welcome someone or say hello with words or a gesture.",
                        "ru": "Приветствовать кого-либо или прощаться словами или жестом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "greet",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "say hello to",
                                "target_lang_sense_clarification": "informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "здороваться",
                                "target_lang_sense_clarification": "произносить слова приветствия"
                            },
                            {
                                "target_lang_word": "приветствовать",
                                "target_lang_sense_clarification": "встречать или обращаться с приветствием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2612a96d-ea50-3163-955f-7dccc8aa3b84",
                    "sense_definition": "Een militair eerbewijs brengen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "greeting_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaat <groette> zijn officier strak en formeel.",
                            "target_lang_translations": {
                                "en": "The soldier <saluted> his officer stiffly and formally.",
                                "ru": "Солдат четко и формально <отдал честь> своему офицеру."
                            }
                        },
                        {
                            "text": "Tijdens de ceremonie <groetten> alle militairen tegelijk.",
                            "target_lang_translations": {
                                "en": "During the ceremony, all soldiers <saluted> at the same time.",
                                "ru": "Во время церемонии все военные одновременно <отдали честь>."
                            }
                        }
                    ],
                    "synonyms": [
                        "salueren"
                    ],
                    "common_phrases": [
                        "militair groeten"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform a military salute.",
                        "ru": "Отдавать воинское приветствие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "salute",
                                "target_lang_sense_clarification": "military context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отдавать честь",
                                "target_lang_sense_clarification": "военный термин"
                            },
                            {
                                "target_lang_word": "салютовать",
                                "target_lang_sense_clarification": "торжественно приветствовать"
                            }
                        ]
                    }
                }
            ]
        },
        {

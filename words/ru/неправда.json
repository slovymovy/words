            "senses": [
                {
                    "sense_id": "5f4ea144-1cfd-3ac2-b7d5-e963800bf6f5",
                    "sense_definition": "То, что не соответствует действительности, противоречит истине; ложь, вымысел.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я сразу понял, что всё это <w>неправда</w>.",
                            "target_lang_translations": {
                                "en": "I immediately realized that all this was a <w>lie</w>."
                            }
                        },
                        {
                            "text": "Никогда не говори <w>неправду</w> своим близким друзьям.",
                            "target_lang_translations": {
                                "en": "Never tell a <w>lie</w> to your close friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "ложь",
                        "враньё",
                        "обман"
                    ],
                    "antonyms": [
                        "правда",
                        "истина"
                    ],
                    "common_phrases": [
                        "сказать неправду",
                        "это неправда",
                        "чистая неправда"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that does not correspond to reality or contradicts the truth; a lie or fabrication."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lie",
                                "target_lang_sense_clarification": "An intentionally false statement"
                            },
                            {
                                "target_lang_word": "untruth",
                                "target_lang_sense_clarification": "A statement that is not true; often softer or more formal than 'lie'"
                            },
                            {
                                "target_lang_word": "falsehood",
                                "target_lang_sense_clarification": "The quality of being untrue or a false statement (formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65c2ffc7-9dbf-3a44-a44b-4903d2111e61",
                    "sense_definition": "Отсутствие справедливости; несправедливый поступок, нарушение моральных норм.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Герой романа не мог мириться с окружающей его <w>неправдой</w>.",
                            "target_lang_translations": {
                                "en": "The hero of the novel could not reconcile himself with the <w>injustice</w> surrounding him."
                            }
                        },
                        {
                            "text": "В те времена много <w>неправды</w> творилось на земле.",
                            "target_lang_translations": {
                                "en": "In those times, much <w>injustice</w> was committed on earth."
                            }
                        }
                    ],
                    "synonyms": [
                        "несправедливость",
                        "кривда"
                    ],
                    "antonyms": [
                        "справедливость"
                    ],
                    "common_phrases": [
                        "жить по неправде"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "в современном языке в этом значении чаще используется слово «несправедливость»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A lack of justice; an unjust act or violation of moral norms."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "injustice",
                                "target_lang_sense_clarification": "Lack of fairness or justice"
                            },
                            {
                                "target_lang_word": "falsehood",
                                "target_lang_sense_clarification": "Lack of moral rectitude or truth (archaic/literary)"
                            }
                        ]
                    }
                }
            ]

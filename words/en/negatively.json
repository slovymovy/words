    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4ce2b919-2104-3895-8519-cc6fd728e308",
                    "sense_definition": "In a way that is harmful, unpleasant, or not hopeful.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scandal <w>negatively</w> affected the company's reputation for years.",
                            "target_lang_translations": {
                                "ru": "Скандал <w>негативно</w> сказывался на репутации компании в течение многих лет."
                            }
                        },
                        {
                            "text": "He tends to react <w>negatively</w> to any criticism of his work.",
                            "target_lang_translations": {
                                "ru": "Он склонен <w>негативно</w> реагировать на любую критику своей работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "adversely",
                        "unfavorably",
                        "badly"
                    ],
                    "antonyms": [
                        "positively"
                    ],
                    "common_phrases": [
                        "negatively affect",
                        "react negatively",
                        "impact negatively",
                        "view negatively"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вредным, неприятным или лишенным надежды образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "негативно",
                                "target_lang_sense_clarification": "плохо, вредно"
                            },
                            {
                                "target_lang_word": "отрицательно",
                                "target_lang_sense_clarification": "неблагоприятно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72e8d284-b1f8-3a47-aa54-1047d42c386b",
                    "sense_definition": "In a way that indicates refusal, denial, or disagreement; meaning \"no\".",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When asked if he was involved in the incident, the witness answered <w>negatively</w>.",
                            "target_lang_translations": {
                                "ru": "Когда свидетеля спросили, причастен ли он к инциденту, он ответил <w>отрицательно</w>."
                            }
                        },
                        {
                            "text": "The committee voted <w>negatively</w> on the proposal to extend the deadline.",
                            "target_lang_translations": {
                                "ru": "Комитет проголосовал <w>отрицательно</w> по предложению о продлении срока."
                            }
                        }
                    ],
                    "antonyms": [
                        "affirmatively"
                    ],
                    "common_phrases": [
                        "answer negatively",
                        "respond negatively",
                        "vote negatively"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, выражающим отказ, отрицание или несогласие; в значении «нет»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрицательно",
                                "target_lang_sense_clarification": "выражая отказ или несогласие"
                            }
                        ]
                    }
                }
            ]
        }

        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "478eeca9-7fea-39fc-bb7b-28505f9ee7cd",
                    "sense_definition": "Op dit moment; in de huidige tijd.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb <nou> echt geen tijd om naar je verhaal te luisteren.",
                            "target_lang_translations": {
                                "en": "I really don't have time <now> to listen to your story.",
                                "ru": "У меня <сейчас> правда нет времени слушать твою историю."
                            }
                        },
                        {
                            "text": "Wat is er <nou> eigenlijk precies aan de hand in die straat?",
                            "target_lang_translations": {
                                "en": "What is <now> actually going on in that street?",
                                "ru": "Что <сейчас> на самом деле происходит на этой улице?"
                            }
                        }
                    ],
                    "synonyms": [
                        "nu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informele variant van 'nu'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "At this moment; in the current time.",
                        "ru": "В данный момент; сейчас (разговорный вариант)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "now",
                                "target_lang_sense_clarification": "at the present time"
                            },
                            {
                                "target_lang_word": "currently",
                                "target_lang_sense_clarification": "at this time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сейчас",
                                "target_lang_sense_clarification": "в настоящее время"
                            },
                            {
                                "target_lang_word": "теперь",
                                "target_lang_sense_clarification": "в данный момент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4424a888-f892-3948-9e9d-c8ebdc4a3962",
                    "sense_definition": "Drukt ongeduld of een dringende aansporing uit.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Schiet <nou> toch eens op, we komen nog te laat!",
                            "target_lang_translations": {
                                "en": "Hurry up <now>, we're going to be late!",
                                "ru": "Поторапливайся <же>, мы опоздаем!"
                            }
                        },
                        {
                            "text": "Doe <nou> niet zo vervelend tegen je kleine broertje.",
                            "target_lang_translations": {
                                "en": "Don't be so annoying <now> to your little brother.",
                                "ru": "<Ну> не будь таким вредным по отношению к младшему брату."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kom nou",
                        "ga nou",
                        "hou nou op"
                    ],
                    "target_lang_definitions": {
                        "en": "Expresses impatience or urgent encouragement.",
                        "ru": "Выражает нетерпение или настоятельную просьбу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "now",
                                "target_lang_sense_clarification": "indicating urgency or command"
                            },
                            {
                                "target_lang_word": "come on",
                                "target_lang_sense_clarification": "encouragement or impatience"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "же",
                                "target_lang_sense_clarification": "усилительная частица"
                            },
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "побуждение к действию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3de97a67-cfd0-3014-881c-8d18af6ee001",
                    "sense_definition": "Geeft een relativering, berusting of nadruk aan in een bewering.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is <nou> eenmaal hoe de regels hier werken.",
                            "target_lang_translations": {
                                "en": "That is <just> how the rules work here.",
                                "ru": "Так <уж> здесь работают правила."
                            }
                        },
                        {
                            "text": "Het is <nou> ook weer niet zo erg als je denkt.",
                            "target_lang_translations": {
                                "en": "It is not <really> as bad as you think.",
                                "ru": "Это <же> не так плохо, как ты думаешь."
                            }
                        }
                    ],
                    "common_phrases": [

{
    "word_family": [
        "podchodzić",
        "podejście"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4856f88d-00ca-3f3f-ae95-8bf502cd985d",
                    "sense_definition": "Zbliżyć się do kogoś lub czegoś, idąc w tamtym kierunku.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_approach",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna <podszedł> do recepcji, aby odebrać klucze do pokoju.",
                            "target_lang_translations": {
                                "ru": "Мужчина <подошёл> к стойке регистрации, чтобы забрать ключи от номера."
                            }
                        },
                        {
                            "text": "Czy możesz <podejść> tu na chwilę i mi pomóc?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <подойти> сюда на минутку и помочь мне?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zbliżyć się"
                    ],
                    "antonyms": [
                        "odejść",
                        "oddalić się"
                    ],
                    "common_phrases": [
                        "podejść do okna",
                        "podejść bliżej",
                        "podejść do lady"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приблизиться к кому-либо или чему-либо, идя в этом направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подойти",
                                "target_lang_sense_clarification": "приблизиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a92d859-343c-3822-8790-e470caf38eac",
                    "sense_definition": "Przystąpić do egzaminu lub sprawdzianu w celu uzyskania oceny lub kwalifikacji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy uczniowie w naszej szkole <podejdą> do matury w maju.",
                            "target_lang_translations": {
                                "ru": "Все ученики нашей школы будут <сдавать> выпускные экзамены в мае."
                            }
                        },
                        {
                            "text": "Niestety nie udało mi się <podejść> do tego testu w pierwszym terminie.",
                            "target_lang_translations": {
                                "ru": "К сожалению, мне не удалось <сдать> этот тест в первый срок."
                            }
                        }
                    ],
                    "synonyms": [
                        "przystąpić",
                        "zdawać"
                    ],
                    "common_phrases": [
                        "podejść do egzaminu",
                        "podejść do matury",
                        "podejść do poprawki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приступить к экзамену или тесту для получения оценки или квалификации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "экзамен"
                            },
                            {
                                "target_lang_word": "приступить",
                                "target_lang_sense_clarification": "начать выполнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7109515c-6b39-38bf-b201-f65969cac435",
                    "sense_definition": "Wejść na wzniesienie lub górę; pokonać pewien odcinek drogi pod górę.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "movement_climb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Turyści musieli <podejść> kawałek pod stromą górę, aby zobaczyć widok.",
                            "target_lang_translations": {
                                "ru": "Туристам пришлось немного <подняться> по крутой горе, чтобы увидеть вид."
                            }
                        },
                        {
                            "text": "Udało nam się <podejść> pod schronisko tuż przed zachodem słońca.",
                            "target_lang_translations": {
                                "ru": "Нам удалось <подняться> к приюту как раз перед закатом."
                            }
                        }
                    ],
                    "synonyms": [
                        "wejść",
                        "wspiąć się"
                    ],
                    "antonyms": [
                        "zejść"
                    ],
                    "common_phrases": [
                        "podejść pod górę",
                        "podejść pod szczyt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подняться на возвышенность или гору; преодолеть определенный участок пути вверх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подняться",
                                "target_lang_sense_clarification": "вверх, в гору"
                            },
                            {
                                "target_lang_word": "взойти",
                                "target_lang_sense_clarification": "на вершину"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3765c04f-6f3f-315b-bc91-600f0efbac8b",
                    "sense_definition": "Zbliżyć się do kogoś lub czegoś ostrożnie i niepostrzeżenie, często w celu schwytania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement_stalk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kot cicho <podszedł> ptaka siedzącego na niskim płocie.",
                            "target_lang_translations": {
                                "ru": "Кот тихо <подкрался> к птице, сидящей на низком заборе."
                            }
                        },
                        {
                            "text": "Myśliwy starał się <podejść> zwierzynę pod wiatr, aby nie zostać wyczutym.",
                            "target_lang_translations": {
                                "ru": "Охотник старался <подойти> к зверю против ветра, чтобы его не учуяли."
                            }
                        }
                    ],
                    "synonyms": [
                        "podkraść się",
                        "zakraść się"
                    ],
                    "common_phrases": [
                        "podejść zwierzynę",
                        "podejść wroga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осторожно и незаметно приблизиться к кому-либо или чему-либо, часто с целью поимки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подкрасться",
                                "target_lang_sense_clarification": "незаметно"
                            },
                            {
                                "target_lang_word": "подойти",
                                "target_lang_sense_clarification": "охотн. к зверю"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75722544-92ba-3d92-b064-46b5aaedf11c",
                    "sense_definition": "Napełnić się lub nabiec cieczą, taką jak krew, ropa lub woda (często o częściach ciała).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej oczy <podeszły> łzami, gdy usłyszała tę smutną wiadomość.",
                            "target_lang_translations": {
                                "ru": "Её глаза <наполнились> слезами, когда она услышала эту печальную новость."
                            }
                        },
                        {
                            "text": "Rana na nodze była głęboka i szybko <podeszła> krwią.",
                            "target_lang_translations": {
                                "ru": "Рана на ноге была глубокой и быстро <налилась> кровью."
                            }
                        }
                    ],
                    "synonyms": [
                        "nabiec"
                    ],
                    "common_phrases": [
                        "podejść krwią",
                        "podejść łzami",
                        "podejść ropą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наполниться жидкостью, такой как кровь, гной или вода (часто о частях тела)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "налиться",
                                "target_lang_sense_clarification": "кровью"
                            },
                            {
                                "target_lang_word": "наполниться",
                                "target_lang_sense_clarification": "слезами"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d67d3139-2ee5-31cb-94a1-7f6e15fca8ad",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>podejście</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mamy kilka różnych <podejść> do rozwiązania tego trudnego problemu.",
                            "target_lang_translations": {
                                "ru": "У нас есть несколько разных <подходов> к решению этой сложной проблемы."
                            }
                        },
                        {
                            "text": "Analiza tych dwóch <podejść> zajmie nam trochę czasu.",
                            "target_lang_translations": {
                                "ru": "Анализ этих двух <подходов> займет у нас некоторое время."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного 'podejście' (подход)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подходов",
                                "target_lang_sense_clarification": "родительный падеж мн. ч."
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "09c43134-0abe-3e2d-ad3f-017ec92e841f",
                    "sense_definition": "Het onzijdig enkelvoud van <w>deze</w>; verwijst zelfstandig naar iets dat dichtbij is, naar een situatie of naar iets dat zojuist genoemd is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "demonstrative_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat is <w>dit</w> voor een vreemd geluid?",
                            "target_lang_translations": {
                                "en": "What kind of strange sound is <w>this</w>?",
                                "ru": "Что <w>это</w> за странный звук?"
                            }
                        },
                        {
                            "text": "<w>Dit</w> is precies wat ik zocht.",
                            "target_lang_translations": {
                                "en": "<w>This</w> is exactly what I was looking for.",
                                "ru": "<w>Это</w> именно то, что я искал."
                            }
                        }
                    ],
                    "antonyms": [
                        "dat"
                    ],
                    "common_phrases": [
                        "wat is dit",
                        "dit is",
                        "dit en dat",
                        "dit betekent"
                    ],
                    "target_lang_definitions": {
                        "en": "The neuter singular of <w>deze</w>; refers independently to something nearby, a situation, or something just mentioned.",
                        "ru": "Средний род единственного числа от <w>deze</w>; самостоятельно указывает на что-то, находящееся близко, на ситуацию или на что-то, только что упомянутое."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "this",
                                "target_lang_sense_clarification": "pronoun referring to a specific thing or situation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "это",
                                "target_lang_sense_clarification": "указательное местоимение (самостоятельное употребление)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "f46a2940-bd96-32e8-98ea-6cec06ac3f9a",
                    "sense_definition": "Het onzijdig enkelvoud van <w>deze</w>; gebruikt vóór een onzijdig zelfstandig naamwoord om iets aan te wijzen dat zich dichtbij bevindt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "demonstrative_determiner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil <w>dit</w> boek graag kopen.",
                            "target_lang_translations": {
                                "en": "I would like to buy <w>this</w> book.",
                                "ru": "Я хочу купить <w>эту</w> книгу."
                            }
                        },
                        {
                            "text": "<w>Dit</w> huis staat al jaren leeg.",
                            "target_lang_translations": {
                                "en": "<w>This</w> house has been empty for years.",
                                "ru": "<w>Этот</w> дом пустует уже много лет."
                            }
                        }
                    ],
                    "antonyms": [
                        "dat"
                    ],
                    "common_phrases": [
                        "dit jaar",
                        "op dit moment",
                        "in dit geval",
                        "dit keer"
                    ],
                    "target_lang_definitions": {
                        "en": "The neuter singular of <w>deze</w>; used before a neuter noun to indicate something nearby.",
                        "ru": "Средний род единственного числа от <w>deze</w>; используется перед существительным среднего рода для указания на объект, находящийся поблизости."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "this",
                                "target_lang_sense_clarification": "determiner used before a noun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "этот",
                                "target_lang_sense_clarification": "указательное местоимение (изменяется по родам: этот, эта, это)"
                            },
                            {
                                "target_lang_word": "это",
                                "target_lang_sense_clarification": "форма среднего рода"
                            }
                        ]
                    }
                }
            ]
        },

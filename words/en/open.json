{
    "word_family": [
        "opener",
        "opening",
        "openly",
        "openness",
        "reopen"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "11f8403e-cf14-3380-8b6f-9db301ad04ae",
                    "sense_definition": "Not closed, fastened, or sealed; allowing access, passage, or a view of the inside.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please leave the door <w>open</w> so the cat can come in.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, оставь дверь <w>открытой</w>, чтобы кошка могла войти."
                            }
                        },
                        {
                            "text": "The box was <w>open</w> and empty when we found it.",
                            "target_lang_translations": {
                                "ru": "Коробка была <w>открыта</w> и пуста, когда мы ее нашли."
                            }
                        }
                    ],
                    "antonyms": [
                        "closed",
                        "shut"
                    ],
                    "common_phrases": [
                        "wide open",
                        "leave open"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не закрытый, не запертый или не запечатанный; позволяющий доступ, проход или обзор внутренней части."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "не закрытый, доступный"
                            },
                            {
                                "target_lang_word": "незапертый",
                                "target_lang_sense_clarification": "без замка"
                            },
                            {
                                "target_lang_word": "распахнутый",
                                "target_lang_sense_clarification": "широко открытый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd77a8e4-964e-337c-8c45-ccbb04c2251d",
                    "sense_definition": "Ready to provide service or conduct business.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "business_availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The supermarket is <w>open</w> until midnight every day.",
                            "target_lang_translations": {
                                "ru": "Супермаркет <w>открыт</w> до полуночи каждый день."
                            }
                        },
                        {
                            "text": "Is the bank <w>open</w> on Sundays?",
                            "target_lang_translations": {
                                "ru": "Банк <w>открыт</w> по воскресеньям?"
                            }
                        }
                    ],
                    "synonyms": [
                        "accessible"
                    ],
                    "antonyms": [
                        "closed"
                    ],
                    "common_phrases": [
                        "open for business",
                        "now open"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовый к обслуживанию клиентов или ведению бизнеса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "работающий, готовый к обслуживанию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "772f24e5-0782-3d57-ab69-82e1ce7417a5",
                    "sense_definition": "Spread out or unfolded; not closed up or contracted.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She sat on the sofa with an <w>open</w> book on her lap.",
                            "target_lang_translations": {
                                "ru": "Она сидела на диване с <w>раскрытой</w> книгой на коленях."
                            }
                        },
                        {
                            "text": "He greeted his old friend with <w>open</w> arms.",
                            "target_lang_translations": {
                                "ru": "Он встретил своего старого друга с <w>распростертыми</w> объятиями."
                            }
                        }
                    ],
                    "antonyms": [
                        "closed"
                    ],
                    "common_phrases": [
                        "open arms",
                        "open book"
                    ],
                    "target_lang_definitions": {
                        "ru": "Развернутый или раскрытый; не сложенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрытый",
                                "target_lang_sense_clarification": "о книге, объятиях"
                            },
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce633607-6978-3768-91e9-30e6f01b4fe3",
                    "sense_definition": "Willing to listen to and consider new ideas, suggestions, or arguments.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>open</w> to any suggestions you might have regarding the project.",
                            "target_lang_translations": {
                                "ru": "Я <w>открыт</w> для любых предложений, которые могут у вас возникнуть по поводу проекта."
                            }
                        },
                        {
                            "text": "You should keep an <w>open</w> mind about the changes.",
                            "target_lang_translations": {
                                "ru": "Тебе следует сохранять <w>непредвзятое</w> отношение к переменам."
                            }
                        }
                    ],
                    "synonyms": [
                        "accessible"
                    ],
                    "antonyms": [
                        "closed"
                    ],
                    "common_phrases": [
                        "open to ideas",
                        "open mind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовый выслушать и рассмотреть новые идеи, предложения или аргументы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "готовый принять новые идеи"
                            },
                            {
                                "target_lang_word": "восприимчивый",
                                "target_lang_sense_clarification": "легко воспринимающий новое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2edac722-9628-31cd-8aa5-90d7718f4b4c",
                    "sense_definition": "Of a space or area, not enclosed by boundaries or obstructed by objects.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We enjoy walking in the <w>open</w> countryside on weekends.",
                            "target_lang_translations": {
                                "ru": "Нам нравится гулять по <w>открытой</w> местности по выходным."
                            }
                        },
                        {
                            "text": "It was a large <w>open</w> room with big windows.",
                            "target_lang_translations": {
                                "ru": "Это была большая <w>открытая</w> комната с большими окнами."
                            }
                        }
                    ],
                    "synonyms": [
                        "unimpeded",
                        "free"
                    ],
                    "common_phrases": [
                        "open space",
                        "open air"
                    ],
                    "target_lang_definitions": {
                        "ru": "О пространстве: не ограниченное стенами или препятствиями; просторное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "не огороженный"
                            },
                            {
                                "target_lang_word": "просторный",
                                "target_lang_sense_clarification": "свободный, без препятствий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4932cf15-b4c5-36a4-b374-fa53b565b549",
                    "sense_definition": "Currently active, running, or displayed on a computer screen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have too many tabs <w>open</w> in my browser right now.",
                            "target_lang_translations": {
                                "ru": "У меня сейчас <w>открыто</w> слишком много вкладок в браузере."
                            }
                        },
                        {
                            "text": "Keep the file <w>open</w> while you work on the report.",
                            "target_lang_translations": {
                                "ru": "Держи файл <w>открытым</w>, пока работаешь над отчетом."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Активный, запущенный или отображаемый на экране компьютера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "о файле или вкладке"
                            },
                            {
                                "target_lang_word": "запущенный",
                                "target_lang_sense_clarification": "о программе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a0a0c66-0a70-31e9-a045-828c8b7890cc",
                    "sense_definition": "Not yet settled, decided, or finalized.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It remains an <w>open</w> question whether the plan will succeed.",
                            "target_lang_translations": {
                                "ru": "Остается <w>открытым</w> вопросом, удастся ли план."
                            }
                        },
                        {
                            "text": "We have an <w>open</w> invitation to visit them whenever we want.",
                            "target_lang_translations": {
                                "ru": "У нас есть <w>открытое</w> приглашение навещать их, когда захотим."
                            }
                        }
                    ],
                    "antonyms": [
                        "closed"
                    ],
                    "common_phrases": [
                        "open question"
                    ],
                    "target_lang_definitions": {
                        "ru": "Еще не решенный, не урегулированный или не завершенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "нерешённый (вопрос)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bedf18f6-ec8a-3a58-83ac-7e10c6e8fe43",
                    "sense_definition": "Public and not secret; known to everyone.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "publicity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He wrote an <w>open</w> letter to the newspaper criticizing the policy.",
                            "target_lang_translations": {
                                "ru": "Он написал <w>открытое</w> письмо в газету с критикой этой политики."
                            }
                        },
                        {
                            "text": "Their hostility towards each other was an <w>open</w> secret in the office.",
                            "target_lang_translations": {
                                "ru": "Их враждебность друг к другу была <w>открытым</w> секретом в офисе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "open letter",
                        "open secret"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публичный, не секретный; известный всем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "публичный, не тайный"
                            },
                            {
                                "target_lang_word": "явный",
                                "target_lang_sense_clarification": "очевидный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9d5bf5f-52e7-33ba-acf2-dc8a7541631a",
                    "sense_definition": "Made public and usable with a free license, without proprietary restrictions.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "licensing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The software is <w>open</w> source, so anyone can modify the code.",
                            "target_lang_translations": {
                                "ru": "Это программное обеспечение имеет <w>открытый</w> исходный код, поэтому любой может изменить его."
                            }
                        },
                        {
                            "text": "We support <w>open</w> access to scientific research.",
                            "target_lang_translations": {
                                "ru": "Мы поддерживаем <w>открытый</w> доступ к научным исследованиям."
                            }
                        }
                    ],
                    "synonyms": [
                        "free"
                    ],
                    "antonyms": [
                        "closed-source",
                        "proprietary"
                    ],
                    "common_phrases": [
                        "open source",
                        "open access"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доступный для свободного использования и модификации, без проприетарных ограничений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "свободный для использования (о лицензии)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b2690dc2-858d-3967-a709-b37a6e456241",
                    "sense_definition": "To move a door, lid, or cover so that a space is no longer closed.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you <w>open</w> the window to let some fresh air in?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>открыть</w> окно, чтобы впустить свежий воздух?"
                            }
                        },
                        {
                            "text": "He <w>opened</w> the box to see what was inside.",
                            "target_lang_translations": {
                                "ru": "Он <w>открыл</w> коробку, чтобы посмотреть, что внутри."
                            }
                        }
                    ],
                    "synonyms": [
                        "unseal"
                    ],
                    "antonyms": [
                        "shut"
                    ],
                    "common_phrases": [
                        "open up",
                        "open the door"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сдвинуть дверь, крышку или покрытие так, чтобы пространство перестало быть закрытым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "делать не закрытым (несов. вид)"
                            },
                            {
                                "target_lang_word": "открыть",
                                "target_lang_sense_clarification": "сделать не закрытым (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ce224ff-dc7f-3bc0-bca0-cc1f69c500d7",
                    "sense_definition": "To start business for the day or to establish a new business.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "business_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "What time does the library <w>open</w> in the morning?",
                            "target_lang_translations": {
                                "ru": "Во сколько библиотека <w>открывается</w> утром?"
                            }
                        },
                        {
                            "text": "They plan to <w>open</w> a new restaurant in the city center.",
                            "target_lang_translations": {
                                "ru": "Они планируют <w>открыть</w> новый ресторан в центре города."
                            }
                        }
                    ],
                    "antonyms": [
                        "shut"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать работу (о заведении) или основать новый бизнес."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открываться",
                                "target_lang_sense_clarification": "начинать работу (о магазине)"
                            },
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "основывать новое заведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "279f5110-d588-3d22-b6a4-ea8e000fa4fd",
                    "sense_definition": "To spread something out so that it is no longer folded or closed.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>open</w> your books to page ten.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>откройте</w> свои книги на десятой странице."
                            }
                        },
                        {
                            "text": "The flower <w>opens</w> its petals in the morning sun.",
                            "target_lang_translations": {
                                "ru": "Цветок <w>раскрывает</w> свои лепестки на утреннем солнце."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Развернуть или раскрыть что-то сложенное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрывать",
                                "target_lang_sense_clarification": "разворачивать сложенное (книгу, зонт)"
                            },
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "делать доступным содержимое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "491835ef-cdca-399e-a087-185ff3656401",
                    "sense_definition": "To make a computer file or program ready to use.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "computing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Click the icon to <w>open</w> the program.",
                            "target_lang_translations": {
                                "ru": "Нажмите на значок, чтобы <w>открыть</w> программу."
                            }
                        },
                        {
                            "text": "I cannot <w>open</w> this attachment on my phone.",
                            "target_lang_translations": {
                                "ru": "Я не могу <w>открыть</w> это вложение на своем телефоне."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подготовить компьютерный файл или программу к использованию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "запускать файл или программу"
                            },
                            {
                                "target_lang_word": "запускать",
                                "target_lang_sense_clarification": "начинать выполнение программы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea0c26b3-772d-32bb-b9fa-78790cab4eb3",
                    "sense_definition": "To begin or start an event, activity, or process.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "start_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police <w>opened</w> fire on the fleeing suspect.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>открыла</w> огонь по убегающему подозреваемому."
                            }
                        },
                        {
                            "text": "She <w>opened</w> the meeting with a short speech.",
                            "target_lang_translations": {
                                "ru": "Она <w>открыла</w> собрание короткой речью."
                            }
                        }
                    ],
                    "synonyms": [
                        "start",
                        "begin"
                    ],
                    "common_phrases": [
                        "open fire",
                        "open a discussion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать событие, деятельность или процесс."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "начинать (собрание, огонь)"
                            },
                            {
                                "target_lang_word": "начинать",
                                "target_lang_sense_clarification": "приступать к действию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c825474b-106d-3326-aa9e-5fbe03f2c63f",
                    "sense_definition": "To disclose, reveal, or make something known.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "reveal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>opened</w> his heart to her and told her everything.",
                            "target_lang_translations": {
                                "ru": "Он <w>открыл</w> ей свое сердце и рассказал все."
                            }
                        },
                        {
                            "text": "The investigation <w>opened</w> a new line of inquiry.",
                            "target_lang_translations": {
                                "ru": "Расследование <w>открыло</w> новое направление."
                            }
                        }
                    ],
                    "synonyms": [
                        "reveal",
                        "bare"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскрыть, обнаружить или сделать что-то известным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "делать тайное явным"
                            },
                            {
                                "target_lang_word": "раскрывать",
                                "target_lang_sense_clarification": "обнажать, показывать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8e647114-99f3-31cd-9569-ac9aea3b3c1e",
                    "sense_definition": "An area outdoors that is not enclosed or covered.",
                    "learner_level": "B2",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f4553b6c-da52-3ab9-90f0-4c2862a7abbd",
                    "sense_definition": "A man-made machine placed in orbit around the Earth or another celestial body to collect information or for communication.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "space_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many telecommunication <w>satellites</w> orbit at 36000km above the equator.",
                            "target_lang_translations": {
                                "ru": "Многие телекоммуникационные <w>спутники</w> вращаются на орбите в 36000 км над экватором."
                            }
                        },
                        {
                            "text": "The weather <w>satellite</w> sends updated images of the storm every hour.",
                            "target_lang_translations": {
                                "ru": "Метеорологический <w>спутник</w> каждый час присылает обновленные снимки шторма."
                            }
                        }
                    ],
                    "synonyms": [
                        "sat"
                    ],
                    "common_phrases": [
                        "weather satellite",
                        "spy satellite",
                        "communications satellite",
                        "launch a satellite"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственный аппарат, выведенный на орбиту вокруг Земли или другого небесного тела для сбора информации или обеспечения связи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спутник",
                                "target_lang_sense_clarification": "космический аппарат"
                            },
                            {
                                "target_lang_word": "искусственный спутник",
                                "target_lang_sense_clarification": "полный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e483c7c-3a6e-3cc8-9566-7378ddad4edf",
                    "sense_definition": "Television programming or signals transmitted from space, or the equipment used to receive them.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you have <w>satellite</w> at your house or do you use cable?",
                            "target_lang_translations": {
                                "ru": "У вас дома <w>спутниковое телевидение</w> или кабельное?"
                            }
                        },
                        {
                            "text": "We watched the football match live via <w>satellite</w>.",
                            "target_lang_translations": {
                                "ru": "Мы смотрели футбольный матч в прямом эфире через <w>спутник</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "satellite TV",
                        "satellite dish",
                        "via satellite"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used to refer to the service or system"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Телевизионные программы или сигналы, передаваемые из космоса, а также оборудование для их приема."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спутниковое телевидение",
                                "target_lang_sense_clarification": "услуга/система"
                            },
                            {
                                "target_lang_word": "спутник",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "спутниковая антенна",
                                "target_lang_sense_clarification": "оборудование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2aabe6ed-e485-35c3-bf53-b3333343389d",
                    "sense_definition": "A natural object, such as a moon, that moves in an orbit around a larger planet or star.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "space_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Moon is the only natural <w>satellite</w> of the Earth.",
                            "target_lang_translations": {
                                "ru": "Луна — единственный естественный <w>спутник</w> Земли."
                            }
                        },
                        {
                            "text": "Jupiter has many icy <w>satellites</w> that orbit it at different distances.",
                            "target_lang_translations": {
                                "ru": "У Юпитера много ледяных <w>спутников</w>, вращающихся вокруг него на разных расстояниях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "natural satellite",
                        "Earth's satellite"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественное небесное тело, например Луна, движущееся по орбите вокруг более крупной планеты или звезды."
                    },
                    "translations": {
                        "ru": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ab287365-c164-3062-90aa-5af00c7ee3f2",
                    "sense_definition": "Umierając, sprawiać, że dziecko traci rodziców i staje się sierotą.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każda kolejna bitwa tej okrutnej wojny <w>osieroca</w> setki niewinnych dzieci.",
                            "target_lang_translations": {
                                "ru": "Каждая новая битва этой жестокой войны <w>оставляет сиротами</w> сотни невинных детей."
                            }
                        },
                        {
                            "text": "Wypadki drogowe każdego roku <w>osierocają</w> wiele rodzin w naszym kraju.",
                            "target_lang_translations": {
                                "ru": "Дорожные аварии каждый год <w>оставляют сиротами</w> многие семьи в нашей стране."
                            }
                        }
                    ],
                    "common_phrases": [
                        "osierocać dzieci",
                        "osierocać rodzinę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умирая, лишать ребенка родителей, делая его сиротой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставлять сиротой",
                                "target_lang_sense_clarification": "делать сиротой"
                            },
                            {
                                "target_lang_word": "сиротить",
                                "target_lang_sense_clarification": "лишать родителей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3389e01e-a51f-3684-8413-ecebc83be5bc",
                    "sense_definition": "Opuszczać kogoś lub coś, pozostawiając w osamotnieniu lub bez opieki (często w kontekście śmierci ważnej osoby).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Odejście tak wybitnego mistrza <w>osieroca</w> całe środowisko artystyczne.",
                            "target_lang_translations": {
                                "ru": "Уход такого выдающегося мастера <w>оставляет сиротой</w> всю художественную среду."
                            }
                        },
                        {
                            "text": "Emigracja młodych ludzi powoli <w>osieroca</w> te piękne, wiejskie tereny.",
                            "target_lang_translations": {
                                "ru": "Эмиграция молодых людей медленно <w>оставляет покинутыми</w> эти красивые сельские районы."
                            }
                        }
                    ],
                    "synonyms": [
                        "opuszczać",
                        "porzucać",
                        "zostawiać"
                    ],
                    "antonyms": [
                        "przygarniać"
                    ],
                    "common_phrases": [
                        "osierocać kraj",
                        "osierocać tron"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покидать кого-либо или что-либо, оставляя в одиночестве или без попечения (часто в контексте смерти важного лица)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покидать",
                                "target_lang_sense_clarification": "оставлять без поддержки"
                            },
                            {
                                "target_lang_word": "оставлять сиротой",
                                "target_lang_sense_clarification": "в переносном смысле"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_id": "01a64f1a-55a1-358f-877a-d12cf4ae39f8",
                    "sense_definition": "Urządzenie elektryczne służące do uzupełniania energii w akumulatorach lub bateriach, np. w telefonach, laptopach czy samochodach.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "electronics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy masz może pożyczyć <w>ładowarkę</w> do telefonu, bo mój się rozładował?",
                            "target_lang_translations": {
                                "ru": "Можешь одолжить <w>зарядку</w> для телефона, а то мой разрядился?"
                            }
                        },
                        {
                            "text": "Nowa <w>ładowarka</w> samochodowa pozwala na szybkie ładowanie dwóch urządzeń jednocześnie.",
                            "target_lang_translations": {
                                "ru": "Новое автомобильное <w>зарядное устройство</w> позволяет быстро заряжать два устройства одновременно."
                            }
                        }
                    ],
                    "synonyms": [
                        "zasilacz",
                        "prostownik"
                    ],
                    "common_phrases": [
                        "ładowarka do telefonu",
                        "ładowarka samochodowa",
                        "ładowarka sieciowa",
                        "podłączyć ładowarkę",
                        "ładowarka indukcyjna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Электрическое устройство для зарядки аккумуляторов или батарей, например, в телефонах, ноутбуках или автомобилях."
                    },
                    "translations": {

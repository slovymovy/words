                            {
                                "target_lang_word": "смочь",
                                "target_lang_sense_clarification": "быть в состоянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ddb5132-a242-31ed-beb4-ec76df4b0911",
                    "sense_definition": "Iets ontdekken; achter informatie komen die men nog niet had.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "kennis_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We proberen te <w>weten</w> te komen wie verantwoordelijk is voor de fout.",
                            "target_lang_translations": {
                                "en": "We are trying to <w>find out</w> who is responsible for the mistake.",
                                "ru": "Мы пытаемся <w>выяснить</w>, кто несёт ответственность за ошибку."
                            }
                        },
                        {
                            "text": "Tijdens het onderzoek kwam de politie veel nieuwe feiten te <w>weten</w>.",
                            "target_lang_translations": {
                                "en": "During the investigation, the police <w>found out</w> many new facts.",
                                "ru": "В ходе расследования полиция <w>узнала</w> много новых фактов."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontdekken",
                        "achterhalen",
                        "vernemen"
                    ],
                    "common_phrases": [
                        "te weten komen"

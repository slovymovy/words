            "pos": "verb",
            "senses": [
                {
                    "sense_id": "80b1769e-b301-3980-a5a7-4ddb198cf6c6",
                    "sense_definition": "Uderzać w coś lub kogoś z dużą siłą, często wielokrotnie; wydawać głośne dźwięki poprzez uderzanie",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ktoś mocno <walił> pięścią w stół, domagając się ciszy na sali.",
                            "target_lang_translations": {
                                "ru": "Кто-то сильно <стучал> кулаком по столу, требуя тишины в зале."
                            }
                        },
                        {
                            "text": "Moje serce zaczęło <walić> jak młot, gdy tylko go zobaczyłam.",
                            "target_lang_translations": {
                                "ru": "Мое сердце начало <колотиться> как молот, как только я его увидела."
                            }
                        }
                    ],
                    "synonyms": [
                        "uderzać",
                        "bić",
                        "tłuc",
                        "łomotać"
                    ],
                    "antonyms": [
                        "dotykać",
                        "mówić delikatnie"
                    ],
                    "common_phrases": [
                        "walić w drzwi",
                        "serce wali",
                        "walić głową w mur"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бить или ударять с большой силой; издавать громкие звуки ударами"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "наносить удары"
                            },
                            {
                                "target_lang_word": "колотить",
                                "target_lang_sense_clarification": "сильно или часто ударять"
                            },
                            {
                                "target_lang_word": "стучать",
                                "target_lang_sense_clarification": "издавать звук удара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d52a19da-8312-3f91-b471-ef3a56b06f02",
                    "sense_definition": "Wydzielać bardzo nieprzyjemny, intensywny zapach",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W całym mieszkaniu strasznie <waliło> dymem papierosowym i stęchlizną.",
                            "target_lang_translations": {
                                "ru": "Во всей квартире ужасно <воняло> сигаретным дымом и затхлостью."
                            }
                        },
                        {
                            "text": "Nie wchodź do tego pomieszczenia, bo <wali> tam wilgocią.",
                            "target_lang_translations": {
                                "ru": "Не заходи в это помещение, потому что там <несет> сыростью."
                            }
                        }
                    ],
                    "synonyms": [
                        "śmierdzieć",
                        "cuchnąć"
                    ],
                    "common_phrases": [
                        "walić z ust",
                        "strasznie walić"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать очень неприятный, сильный запах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вонять",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "нести",
                                "target_lang_sense_clarification": "о запахе, безличное"
                            },
                            {
                                "target_lang_word": "разить",
                                "target_lang_sense_clarification": "очень сильно пахнуть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e3098f3-f56a-3b65-94e6-f7c864bc542b",
                    "sense_definition": "Całkowicie coś lekceważyć, nie przejmować się czymś",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szczerze mówiąc, <wali> mnie to, co oni o mnie myślą.",
                            "target_lang_translations": {
                                "ru": "Честно говоря, мне <плевать> на то, что они обо мне думают."
                            }
                        },
                        {
                            "text": "Powiedział szefowi, że go to <wali> i po prostu wyszedł z biura.",
                            "target_lang_translations": {
                                "ru": "Он сказал начальнику, что ему на это <наплевать>, и просто вышел из офиса."
                            }
                        }
                    ],
                    "synonyms": [
                        "olewać",
                        "lekceważyć"
                    ],
                    "common_phrases": [
                        "wali mnie to",
                        "walić to"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью игнорировать что-либо, быть безразличным"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плевать",
                                "target_lang_sense_clarification": "быть безразличным"
                            },
                            {
                                "target_lang_word": "быть пофиг",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5641b520-f8f7-3eee-88d7-e6ec1a0e2e38",
                    "sense_definition": "Padać bardzo intensywnie (o opadach atmosferycznych) lub przewracać się z hukiem",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za oknem od samego rana gęsto <wali> mokry śnieg.",
                            "target_lang_translations": {
                                "ru": "За окном с самого утра густо <валит> мокрый снег."
                            }
                        },
                        {
                            "text": "Stary budynek gospodarczy <wali> się i grozi całkowitym zawaleniem.",
                            "target_lang_translations": {
                                "ru": "Старая хозяйственная постройка <рушится> и грозит полным обвалом."
                            }
                        }
                    ],
                    "synonyms": [
                        "sypać",
                        "runąć",
                        "upadać"
                    ],
                    "common_phrases": [

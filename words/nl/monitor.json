{
    "word_family": [
        "monitoren",
        "monitoring"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c141cf5c-5ec4-3ab6-9c62-66e2f2059b50",
                    "sense_definition": "Een beeldscherm waarop informatie van een computer of ander elektronisch apparaat zichtbaar wordt gemaakt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "screen_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb een tweede <w>monitor</w> gekocht om efficiënter te kunnen werken.",
                            "target_lang_translations": {
                                "ru": "Я купил второй <w>монитор</w>, чтобы работать эффективнее."
                            }
                        },
                        {
                            "text": "De tekst op de <w>monitor</w> was haarscherp en goed leesbaar.",
                            "target_lang_translations": {
                                "ru": "Текст на <w>мониторе</w> был очень четким и легко читаемым."
                            }
                        }
                    ],
                    "synonyms": [
                        "beeldscherm"
                    ],
                    "common_phrases": [
                        "op de monitor",
                        "tweede monitor",
                        "externe monitor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Экран, на котором отображается информация с компьютера или другого электронного устройства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монитор",
                                "target_lang_sense_clarification": "экран компьютера"
                            },
                            {
                                "target_lang_word": "дисплей",
                                "target_lang_sense_clarification": "экран устройства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d513033-f09d-362e-b19e-7424c2ffe23d",
                    "sense_definition": "Iemand die toezicht houdt op en leiding geeft aan een groep, vaak bij activiteiten voor kinderen of jongeren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_supervisor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>monitor</w> zorgde ervoor dat alle kinderen veilig de straat overstaken.",
                            "target_lang_translations": {
                                "ru": "<w>Вожатый</w> следил за тем, чтобы все дети безопасно перешли улицу."
                            }
                        },
                        {
                            "text": "Tijdens het zomerkamp was hij actief als <w>monitor</w> voor de jongste groep.",
                            "target_lang_translations": {
                                "ru": "В летнем лагере он работал <w>вожатым</w> младшей группы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "monitor worden",
                        "ervaren monitor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который присматривает за группой и руководит ею, часто на мероприятиях для детей или молодежи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вожатый",
                                "target_lang_sense_clarification": "в лагере"
                            },
                            {
                                "target_lang_word": "инструктор",
                                "target_lang_sense_clarification": "руководитель группы"
                            },
                            {
                                "target_lang_word": "куратор",
                                "target_lang_sense_clarification": "наблюдатель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ff0fb6e-41ad-31f9-9d66-1e3925a876b4",
                    "sense_definition": "Een luidspreker die op een podium of in een studio wordt gebruikt zodat muzikanten of technici het geluid kunnen controleren.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "audio_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zanger vroeg de geluidsman om zijn stem luider op de <w>monitor</w> te zetten.",
                            "target_lang_translations": {
                                "ru": "Певец попросил звукорежиссера сделать его голос громче в <w>мониторе</w>."
                            }
                        },
                        {
                            "text": "Zonder een goede <w>monitor</w> kan de band zichzelf niet goed horen spelen.",
                            "target_lang_translations": {
                                "ru": "Без хорошего <w>монитора</w> группа не может хорошо слышать свою игру."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in-ear monitor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Динамик, используемый на сцене или в студии, чтобы музыканты или техники могли контролировать звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монитор",
                                "target_lang_sense_clarification": "сценический динамик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d73cfd15-ce36-3c7c-9ee4-ed512843fdcc",
                    "sense_definition": "Een rapportage of systeem waarmee ontwikkelingen op een specifiek maatschappelijk of economisch gebied worden gevolgd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "report_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De overheid publiceerde een nieuwe <w>monitor</w> over de armoede in de grote steden.",
                            "target_lang_translations": {
                                "ru": "Правительство опубликовало новый <w>отчёт</w> о бедности в крупных городах."
                            }
                        },
                        {
                            "text": "Uit de jaarlijkse <w>monitor</w> blijkt dat de werkloosheid licht is gedaald.",
                            "target_lang_translations": {
                                "ru": "Ежегодный <w>мониторинг</w> показывает, что безработица немного снизилась."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sociale monitor",
                        "armoedemonitor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отчет или система, с помощью которой отслеживаются события в определенной социальной или экономической сфере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мониторинг",
                                "target_lang_sense_clarification": "система наблюдения"
                            },
                            {
                                "target_lang_word": "отчёт",
                                "target_lang_sense_clarification": "документ"
                            },
                            {
                                "target_lang_word": "обзор",
                                "target_lang_sense_clarification": "сводка данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa407d48-df37-3537-a20a-cc3a52969323",
                    "sense_definition": "Een begeleider die studenten ondersteunt bij hun studieproces.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "person_mentor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je moeite hebt met de stof, kun je hulp vragen aan je <w>monitor</w>.",
                            "target_lang_translations": {
                                "ru": "Если у тебя трудности с материалом, ты можешь попросить помощи у своего <w>куратора</w>."
                            }
                        },
                        {
                            "text": "De <w>monitor</w> hielp de student bij het maken van een realistische studieplanning.",
                            "target_lang_translations": {
                                "ru": "<w>Куратор</w> помог студенту составить реалистичный учебный план."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наставник, который поддерживает студентов в процессе обучения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куратор",
                                "target_lang_sense_clarification": "наставник студентов"
                            },
                            {
                                "target_lang_word": "наставник",
                                "target_lang_sense_clarification": "ментор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6011305b-e3e4-34bc-b7d1-5eee6b55161f",
                    "sense_definition": "Een type zwaar bewapend oorlogsschip met een geringe diepgang, historisch gebruikt voor kustverdediging.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "warship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het maritiem museum kun je een model van een oude <w>monitor</w> bekijken.",
                            "target_lang_translations": {
                                "ru": "В морском музее можно увидеть модель старого <w>монитора</w>."
                            }
                        },
                        {
                            "text": "De <w>monitor</w> werd ingezet om de vijandelijke kustlijn te beschieten.",
                            "target_lang_translations": {
                                "ru": "<w>Монитор</w> использовался для обстрела вражеской береговой линии."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип тяжело вооруженного военного корабля с малой осадкой, исторически использовавшийся для береговой обороны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монитор",
                                "target_lang_sense_clarification": "тип корабля"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d2d855dd-4b93-3695-b5ff-9ba74ad2adae",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>monitoren</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>monitor</w> de voortgang van het project nauwgezet om vertragingen te voorkomen.",
                            "target_lang_translations": {
                                "ru": "Я тщательно <w>отслеживаю</w> ход проекта, чтобы предотвратить задержки."
                            }
                        },
                        {
                            "text": "Als systeembeheerder <w>monitor</w> ik dagelijks de veiligheid van het netwerk.",
                            "target_lang_translations": {
                                "ru": "Как системный администратор, я ежедневно <w>контролирую</w> безопасность сети."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени от глагола 'monitoren' (отслеживать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отслеживаю",
                                "target_lang_sense_clarification": "наблюдаю"
                            },
                            {
                                "target_lang_word": "контролирую",
                                "target_lang_sense_clarification": "проверяю"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a497be36-e35d-3e9d-b53c-278c6b5964ea",
                    "sense_definition": "Gebiedende wijs van <w>monitoren</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Monitor</w> de patiënt goed en waarschuw direct een arts als de toestand verslechtert.",
                            "target_lang_translations": {
                                "ru": "Внимательно <w>наблюдайте</w> за пациентом и немедленно сообщите врачу, если состояние ухудшится."
                            }
                        },
                        {
                            "text": "<w>Monitor</w> je uitgaven als je aan het einde van de maand geld wilt overhouden.",
                            "target_lang_translations": {
                                "ru": "<w>Контролируй</w> свои расходы, если хочешь, чтобы в конце месяца остались деньги."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение от глагола 'monitoren' (отслеживать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отслеживай",
                                "target_lang_sense_clarification": "наблюдай"
                            },
                            {
                                "target_lang_word": "контролируй",
                                "target_lang_sense_clarification": "проверяй"
                            },
                            {
                                "target_lang_word": "следи",
                                "target_lang_sense_clarification": "присматривай"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d510953-fb1c-3a8e-ba3c-43bbf9f98b41",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>monitoren</w> (bij inversie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Monitor</w> je de resultaten zelf, of laat je dat door een collega doen?",
                            "target_lang_translations": {
                                "ru": "Ты <w>отслеживаешь</w> результаты сам или поручаешь это коллеге?"
                            }
                        },
                        {
                            "text": "Hoe vaak <w>monitor</w> je de prestaties van de servers?",
                            "target_lang_translations": {
                                "ru": "Как часто ты <w>проверяешь</w> производительность серверов?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Второе лицо единственного числа настоящего времени от глагола 'monitoren' (при инверсии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отслеживаешь",
                                "target_lang_sense_clarification": "наблюдаешь"
                            },
                            {
                                "target_lang_word": "контролируешь",
                                "target_lang_sense_clarification": "проверяешь"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
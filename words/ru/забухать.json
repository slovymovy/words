    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "535be1db-a95c-3b37-a39d-8287daf53e12",
                    "sense_definition": "Начать пьянствовать, чрезмерно употреблять спиртные напитки (обычно в течение длительного времени).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "alcohol_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сосед потерял работу и с горя <w>забухал</w> на целую неделю.",
                            "target_lang_translations": {
                                "en": "The neighbor lost his job and, out of grief, <w>went on a bender</w> for a whole week."
                            }
                        },
                        {
                            "text": "Они решили <w>забухать</w> на даче, забыв обо всех делах.",
                            "target_lang_translations": {
                                "en": "They decided to <w>go on a binge</w> at the dacha, forgetting about all their chores."
                            }
                        }
                    ],
                    "synonyms": [
                        "запить"
                    ],
                    "common_phrases": [
                        "конкретно забухать",
                        "забухать на неделю"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To start drinking alcohol excessively, often for an extended period."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go on a bender",
                                "target_lang_sense_clarification": "slang, implies a prolonged period of drinking"
                            },
                            {
                                "target_lang_word": "go on a binge",
                                "target_lang_sense_clarification": "slang, implies excessive consumption"
                            },
                            {
                                "target_lang_word": "start drinking heavily",
                                "target_lang_sense_clarification": "general description of the action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6bb3ddbd-7175-33db-97f6-e9d5b4302fb0",
                    "sense_definition": "Начать издавать громкие, глухие, отрывистые звуки; начать стучать или ударять чем-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_production",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Где-то вдалеке <w>забухали</w> тяжелые орудия, возвещая о начале боя.",
                            "target_lang_translations": {
                                "en": "Somewhere in the distance, heavy guns <w>started booming</w>, announcing the start of the battle."
                            }
                        },
                        {
                            "text": "Он в отчаянии <w>забухал</w> кулаками в запертую дверь.",
                            "target_lang_translations": {
                                "en": "In despair, he <w>started pounding</w> his fists on the locked door."
                            }
                        }
                    ],
                    "synonyms": [
                        "загрохотать",
                        "забабахать"
                    ],
                    "antonyms": [
                        "стихнуть"
                    ],
                    "common_phrases": [
                        "забухали пушки",
                        "забухали молоты"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To start making loud, dull, abrupt sounds; to start knocking or hitting something heavily."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start booming",
                                "target_lang_sense_clarification": "regarding loud, deep sounds like cannons"
                            },
                            {
                                "target_lang_word": "start pounding",
                                "target_lang_sense_clarification": "regarding hitting something repeatedly and heavily"
                            },
                            {
                                "target_lang_word": "start banging",
                                "target_lang_sense_clarification": "regarding loud impact sounds"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c034894-d906-3c26-a84c-d9a8b9e13be0",
                    "sense_definition": "Послышаться, раздаться (о низких, глухих, ритмичных звуках, например, басах или взрывах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_production",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Внезапно <w>забухали</w> барабаны, и оркестр заиграл военный марш.",
                            "target_lang_translations": {
                                "en": "Suddenly the drums <w>started thumping</w>, and the orchestra began playing a military march."
                            }
                        },
                        {
                            "text": "На дискотеке громко <w>забухали</w> басы, и молодежь пошла танцевать.",
                            "target_lang_translations": {
                                "en": "At the disco, the bass <w>started booming</w> loudly, and the young people went to dance."
                            }
                        }
                    ],
                    "common_phrases": [
                        "забухали басы"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To begin to sound or resound (referring to low, dull, rhythmic sounds like bass or explosions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start thumping",
                                "target_lang_sense_clarification": "regarding rhythmic, low-frequency sounds like drums or bass"
                            },
                            {
                                "target_lang_word": "start booming",
                                "target_lang_sense_clarification": "regarding deep, resonant sounds"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fe9efa8-e37e-3e66-b6ec-c6296079e3a9",
                    "sense_definition": "Не выйти на работу, учёбу или пропустить важное мероприятие по причине сильного опьянения.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "alcohol_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бригадир пригрозил увольнением каждому, кто <w>забухает</w> в понедельник.",
                            "target_lang_translations": {
                                "en": "The foreman threatened to fire anyone who <w>goes on a bender</w> on Monday."
                            }
                        },
                        {
                            "text": "Студент <w>забухал</w> и пропустил важный экзамен в университете.",
                            "target_lang_translations": {
                                "en": "The student <w>got wasted</w> and missed an important exam at the university."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To miss work, study, or an important event due to severe intoxication."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go on a bender",
                                "target_lang_sense_clarification": "implies missing obligations due to drinking"
                            },
                            {
                                "target_lang_word": "get wasted",
                                "target_lang_sense_clarification": "slang, implies becoming incapacitated by alcohol"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
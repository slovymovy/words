            "senses": [
                {
                    "sense_id": "995d1517-a1f5-365f-a46e-8ff9b89ca0f9",
                    "sense_definition": "Meervoud verleden tijd van <w>zien</w>",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>zagen</w> gisteren een prachtige film in de bioscoop.",
                            "target_lang_translations": {
                                "en": "We <w>saw</w> a beautiful movie in the cinema yesterday.",
                                "ru": "Вчера мы <w>смотрели</w> прекрасный фильм в кинотеатре."
                            }
                        },
                        {
                            "text": "Toen ze uit het raam keken, <w>zagen</w> ze dat het begon te regenen.",
                            "target_lang_translations": {
                                "en": "When they looked out the window, they <w>saw</w> that it started raining.",
                                "ru": "Когда они выглянули в окно, то <w>увидели</w>, что начинается дождь."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural past tense of <w>see</w>",
                        "ru": "Множественное число прошедшего времени глагола <w>zien</w>"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "saw",
                                "target_lang_sense_clarification": "past tense of 'to see'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "видели",
                                "target_lang_sense_clarification": "прошедшее время, мн. ч."
                            },
                            {
                                "target_lang_word": "смотрели",
                                "target_lang_sense_clarification": "в контексте просмотра (фильма и т.п.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "500df550-0211-3a4b-8e31-a0db499057f6",
                    "sense_definition": "Materiaal, meestal hout, in stukken verdelen met behulp van een zaag",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De timmerman moest de planken precies op maat <w>zagen</w>.",
                            "target_lang_translations": {
                                "en": "The carpenter had to <w>saw</w> the planks exactly to size.",
                                "ru": "Плотник должен был <w>отпилить</w> доски точно по размеру."
                            }
                        },
                        {
                            "text": "Ik ga vanmiddag hout <w>zagen</w> voor de open haard.",
                            "target_lang_translations": {
                                "en": "I am going to <w>saw</w> wood for the fireplace this afternoon.",
                                "ru": "Сегодня днём я буду <w>пилить</w> дрова для камина."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hout zagen",
                        "in stukken zagen",
                        "recht zagen"
                    ],
                    "target_lang_definitions": {
                        "en": "To divide material, usually wood, into pieces using a saw",
                        "ru": "Разрезать материал, обычно дерево, на части с помощью пилы"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to saw",
                                "target_lang_sense_clarification": "cut with a tool"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пилить",
                                "target_lang_sense_clarification": "резать пилой"
                            },
                            {
                                "target_lang_word": "распиливать",
                                "target_lang_sense_clarification": "разделять на части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e8b3b89-a060-3c2c-b6da-773902d6b2e1",
                    "sense_definition": "Op een vervelende, eentonige manier blijven praten of klagen over onbelangrijke zaken",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij loopt de hele dag te <w>zagen</w> over het slechte weer.",
                            "target_lang_translations": {
                                "en": "He has been <w>nagging</w> about the bad weather all day.",
                                "ru": "Он весь день <w>ноет</w> из-за плохой погоды."
                            }
                        },
                        {
                            "text": "Hou nu eens op met <w>zagen</w> en ga iets nuttigs doen.",
                            "target_lang_translations": {
                                "en": "Stop <w>whining</w> for once and go do something useful.",
                                "ru": "Хватит уже <w>ныть</w> и займись чем-нибудь полезным."
                            }
                        }
                    ],
                    "synonyms": [
                        "zeuren",
                        "zeiken",
                        "neuten",
                        "zemelen"
                    ],
                    "common_phrases": [
                        "zagen en klagen",
                        "lopen te zagen",
                        "niet zagen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To keep talking or complaining about unimportant matters in an annoying, monotonous way",
                        "ru": "Надоедливо и монотонно говорить или жаловаться на неважные вещи"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to nag",
                                "target_lang_sense_clarification": "complain persistently"
                            },
                            {
                                "target_lang_word": "to whine",
                                "target_lang_sense_clarification": "complain annoyingly"
                            },
                            {
                                "target_lang_word": "to drone on",
                                "target_lang_sense_clarification": "talk monotonously"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ныть",
                                "target_lang_sense_clarification": "жаловаться"
                            },
                            {
                                "target_lang_word": "нудить",
                                "target_lang_sense_clarification": "занудствовать"
                            },
                            {
                                "target_lang_word": "пилить",
                                "target_lang_sense_clarification": "разг., донимать упреками"
                            }
                        ]
                    }
                }
            ]
        },
        {

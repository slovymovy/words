    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "110eb63d-d341-3ba2-8b27-8e4fee75c0a2",
                    "sense_definition": "Polskie nazwisko, noszone zarówno przez kobiety, jak i mężczyzn.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "surname_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Jerzy <w>Pilch</w> był jednym z najbardziej cenionych współczesnych pisarzy polskich.",
                            "target_lang_translations": {
                                "ru": "Ежи <w>Пильх</w> был одним из самых ценимых современных польских писателей."
                            }
                        },
                        {
                            "text": "Na liście obecności nie znalazłem nikogo o nazwisku <w>Pilch</w>.",
                            "target_lang_translations": {
                                "ru": "В списке присутствующих я не нашел никого по фамилии <w>Пильх</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pan Pilch",
                        "Jerzy Pilch"
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "common Polish surname"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия, которую носят как мужчины, так и женщины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пильх",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6f83c19a-fd4f-3a2c-bccd-8fedcd81d787",
                    "sense_definition": "Niewielki, nocny gryzoń nadrzewny o puszystym ogonie, przypominający wiewiórkę; to samo co <w>popielica</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_rodent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dziupli starego dębu zamieszkał mały <w>pilch</w>, który spał przez cały dzień.",
                            "target_lang_translations": {
                                "ru": "В дупле старого дуба поселился маленький <w>полчок</w>, который спал весь день."
                            }
                        },
                        {
                            "text": "Zwinny <w>pilch</w> przeskoczył bezszelestnie na sąsiednią gałąź.",
                            "target_lang_translations": {
                                "ru": "Проворный <w>полчок</w> бесшумно перепрыгнул на соседнюю ветку."
                            }
                        }
                    ],
                    "synonyms": [
                        "popielica"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой ночной древесный грызун с пушистым хвостом, напоминающий белку; то же, что соня-полчок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соня-полчок",
                                "target_lang_sense_clarification": "зоология"
                            },
                            {
                                "target_lang_word": "полчок",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "соня",
                                "target_lang_sense_clarification": "семейство грызунов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5f9ed65-8649-3ca4-a0c9-a324d54be042",
                    "sense_definition": "Pogardliwe określenie osoby chciwej, która niechętnie wydaje pieniądze; to samo co <w>skąpiec</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary <w>pilch</w> nigdy nie dałby ani grosza na cel charytatywny.",
                            "target_lang_translations": {
                                "ru": "Этот старый <w>скряга</w> никогда не дал бы ни гроша на благотворительность."
                            }
                        },
                        {
                            "text": "Nie bądź takim <w>pilchem</w> i podziel się z nami wygraną.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>жмотом</w> и поделись с нами выигрышем."
                            }
                        }
                    ],
                    "synonyms": [
                        "skąpiec"
                    ],
                    "common_phrases": [
                        "stary pilch"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Презрительное название жадного человека, который неохотно тратит деньги; то же, что скряга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скряга",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "скупердяй",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "жмот",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76a1c90a-4141-3416-9295-5e0d916e2d4f",
                    "sense_definition": "Regionalna nazwa małego ssaka ryjącego podziemne korytarze; to samo co <w>kret</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_mole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik narzekał, że jakiś <w>pilch</w> zniszczył mu grządki w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Фермер жаловался, что какой-то <w>крот</w> уничтожил его грядки в огороде."
                            }
                        },
                        {
                            "text": "Na łące pojawiły się świeże kopce usypane przez <w>pilcha</w>.",
                            "target_lang_translations": {
                                "ru": "На лугу появились свежие холмики, насыпанные <w>кротом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kret"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "dialectal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное название крота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крот",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "21b8b0cf-06fd-32fe-976c-a84ee2616ee2",
                    "sense_definition": "Een hekwerk of afscheiding die is opgebouwd uit palen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "enclosure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer controleerde of de houten <w>ompaling</w> nog stevig genoeg was.",
                            "target_lang_translations": {
                                "en": "The farmer checked if the wooden <w>palisade</w> was still sturdy enough.",
                                "ru": "Фермер проверял, достаточно ли еще крепок деревянный <w>частокол</w>."
                            }
                        },
                        {
                            "text": "Langs de weg stond een oude <w>ompaling</w> die nodig vervangen moest worden.",
                            "target_lang_translations": {
                                "en": "Along the road stood an old <w>stockade</w> that urgently needed replacing.",
                                "ru": "Вдоль дороги стояла старая <w>ограда</w>, которую необходимо было заменить."
                            }
                        }
                    ],
                    "synonyms": [
                        "omheining"
                    ],
                    "common_phrases": [
                        "houten ompaling",
                        "stevige ompaling"
                    ],
                    "target_lang_definitions": {
                        "en": "A fence or barrier constructed from posts.",
                        "ru": "Ограждение или забор, сооруженный из столбов или кольев."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "palisade",
                                "target_lang_sense_clarification": "fence made of stakes"
                            },
                            {
                                "target_lang_word": "stockade",
                                "target_lang_sense_clarification": "barrier of upright posts"
                            },
                            {
                                "target_lang_word": "fencing",
                                "target_lang_sense_clarification": "general barrier made of posts"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "частокол",
                                "target_lang_sense_clarification": "забор из тесно вбитых кольев"
                            },
                            {
                                "target_lang_word": "ограда",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "изгородь",
                                "target_lang_sense_clarification": "легкий забор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b776e8c7-c2a2-35fd-8df8-304e1af2f739",
                    "sense_definition": "Een stuk grond of ruimte dat door palen is omgeven.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "enclosure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schapen liepen rustig te grazen binnen de veilige <w>ompaling</w>.",
                            "target_lang_translations": {
                                "en": "The sheep were grazing quietly within the safe <w>enclosure</w>.",
                                "ru": "Овцы спокойно паслись внутри безопасного <w>загона</w>."
                            }
                        },
                        {
                            "text": "Niemand mocht zich zonder toestemming binnen de <w>ompaling</w> begeven.",
                            "target_lang_translations": {
                                "en": "No one was allowed to enter the <w>enclosure</w> without permission.",
                                "ru": "Никому не разрешалось без разрешения заходить на <w>огороженную территорию</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "binnen de ompaling"
                    ],
                    "target_lang_definitions": {
                        "en": "A piece of land or space surrounded by posts.",
                        "ru": "Участок земли или пространство, огороженное столбами."
                    },
                    "translations": {

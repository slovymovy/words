            "senses": [
                {
                    "sense_id": "a0a5b0b2-7a1e-3a03-9ce3-ffdb3538020c",
                    "sense_definition": "Совершенно, буквально, самым настоящим образом; используется для усиления значения, часто с оттенком удивления или возмущения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом доме творится <w>форменно</w> что-то невообразимое и странное.",
                            "target_lang_translations": {
                                "en": "Something <w>positively</w> unimaginable and strange is going on in this house."
                            }
                        },
                        {
                            "text": "Он <w>форменно</w> издевается над нами, заставляя ждать ответа уже целый месяц.",
                            "target_lang_translations": {
                                "en": "He is <w>downright</w> mocking us by making us wait a whole month for an answer."
                            }
                        }
                    ],
                    "synonyms": [
                        "буквально",
                        "совершенно",
                        "абсолютно",
                        "прямо-таки",
                        "просто"
                    ],
                    "common_phrases": [
                        "форменно издеваться",
                        "форменно сходить с ума",
                        "форменно невозможно"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "употребляется в разговорной речи для эмоционального усиления"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Completely, literally, in the truest sense; used to intensify meaning, often with a nuance of surprise or indignation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "positively",
                                "target_lang_sense_clarification": "used to emphasize the truth of a statement"
                            },
                            {
                                "target_lang_word": "literally",
                                "target_lang_sense_clarification": "used for emphasis, virtually"
                            },
                            {
                                "target_lang_word": "downright",
                                "target_lang_sense_clarification": "thoroughly; often used with negative qualities"
                            },
                            {
                                "target_lang_word": "absolutely",
                                "target_lang_sense_clarification": "completely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "984736b7-9d1a-3903-9c87-28db6fbf8731",
                    "sense_definition": "Полностью подобно кому-либо или чему-либо; точно так же, как кто-то другой.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "similarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ситуация повторилась <w>форменно</w> так же, как это было в прошлом году.",
                            "target_lang_translations": {
                                "en": "The situation repeated itself <w>exactly</w> the same way as it was last year."
                            }
                        },
                        {
                            "text": "Они поступили <w>форменно</w> одинаково, не сговариваясь друг с другом.",
                            "target_lang_translations": {
                                "en": "They acted <w>identically</w>, without conspiring with each other."
                            }
                        }
                    ],
                    "synonyms": [
                        "точь-в-точь",
                        "одинаково",
                        "идентично"
                    ],
                    "antonyms": [
                        "иначе",
                        "по-другому"
                    ],
                    "target_lang_definitions": {
                        "en": "Completely similar to someone or something; exactly the same way as someone else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exactly",
                                "target_lang_sense_clarification": "precisely the same"
                            },
                            {
                                "target_lang_word": "identically",
                                "target_lang_sense_clarification": "in the same way"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "particularity",
        "particularize",
        "particularly"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9fb00233-3321-35a3-8016-01e077742756",
                    "sense_definition": "Used to refer to one specific person, thing, or time and not others.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "specific_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Is there a <w>particular</w> type of food you would like to eat tonight?",
                            "target_lang_translations": {
                                "ru": "Есть ли какой-то <w>конкретный</w> вид еды, который вы хотели бы съесть сегодня вечером?"
                            }
                        },
                        {
                            "text": "I remember that <w>particular</w> afternoon very clearly because it was snowing.",
                            "target_lang_translations": {
                                "ru": "Я очень ясно помню тот <w>конкретный</w> день, потому что шёл снег."
                            }
                        }
                    ],
                    "synonyms": [
                        "specific",
                        "individual",
                        "single",
                        "concrete"
                    ],
                    "antonyms": [
                        "general"
                    ],
                    "common_phrases": [
                        "in particular",
                        "this particular",
                        "any particular"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на одного конкретного человека, предмет или время, в отличие от других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конкретный",
                                "target_lang_sense_clarification": "точно обозначенный, определенный"
                            },
                            {
                                "target_lang_word": "определённый",
                                "target_lang_sense_clarification": "известный, тот или иной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c284f57-fd58-3086-b90e-48424a2535e1",
                    "sense_definition": "Greater than usual; special or distinguished.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "special_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He took <w>particular</w> care to ensure the fragile items were packed safely.",
                            "target_lang_translations": {
                                "ru": "Он проявил <w>особую</w> тщательность, чтобы убедиться, что хрупкие предметы упакованы надежно."
                            }
                        },
                        {
                            "text": "I have no <w>particular</w> interest in watching that movie.",
                            "target_lang_translations": {
                                "ru": "У меня нет <w>особого</w> интереса смотреть этот фильм."
                            }
                        }
                    ],
                    "synonyms": [
                        "special",
                        "notable",
                        "singular"
                    ],
                    "common_phrases": [
                        "no particular reason",
                        "of particular interest",
                        "in no particular order"
                    ],
                    "target_lang_definitions": {
                        "ru": "Больше обычного; особенный или выдающийся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "особый",
                                "target_lang_sense_clarification": "не такой, как все; значительный"
                            },
                            {
                                "target_lang_word": "особенный",
                                "target_lang_sense_clarification": "отличающийся от других, необычный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96f132e7-0f6e-3a82-8444-a7265dc276f1",
                    "sense_definition": "Very careful about choosing exactly what one likes and not easily satisfied.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fastidious_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is very <w>particular</w> about the quality of coffee she drinks.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>разборчива</w> в качестве кофе, который пьёт."
                            }
                        },
                        {
                            "text": "My boss is <w>particular</w> about punctuality, so do not be late.",
                            "target_lang_translations": {
                                "ru": "Мой начальник очень <w>щепетилен</w> в вопросах пунктуальности, поэтому не опаздывайте."
                            }
                        }
                    ],
                    "synonyms": [
                        "fastidious",
                        "fussy",
                        "picky",
                        "choosy",
                        "exacting",
                        "meticulous"
                    ],
                    "common_phrases": [
                        "particular about",
                        "very particular"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень тщательный в выборе того, что нравится; трудно угодить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разборчивый",
                                "target_lang_sense_clarification": "строгий в выборе, требовательный"
                            },
                            {
                                "target_lang_word": "привередливый",
                                "target_lang_sense_clarification": "слишком требовательный, капризный"
                            },
                            {
                                "target_lang_word": "щепетильный",
                                "target_lang_sense_clarification": "строго принципиальный в мелочах, педантичный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7458ea47-f0a9-32f3-ba0c-75c2169db0e8",
                    "sense_definition": "A specific detail, fact, or piece of information.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_detail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police refused to release the <w>particulars</w> of the accident to the press.",
                            "target_lang_translations": {
                                "ru": "Полиция отказалась сообщить прессе <w>подробности</w> происшествия."
                            }
                        },
                        {
                            "text": "You must fill in your personal <w>particulars</w> on the application form.",
                            "target_lang_translations": {
                                "ru": "Вы должны внести свои личные <w>данные</w> в анкету."
                            }
                        }
                    ],
                    "synonyms": [
                        "detail",
                        "fact",
                        "specific"
                    ],
                    "common_phrases": [
                        "full particulars",
                        "personal particulars",
                        "go into particulars"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная деталь, факт или часть информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подробность",
                                "target_lang_sense_clarification": "мелкое обстоятельство, деталь"
                            },
                            {
                                "target_lang_word": "деталь",
                                "target_lang_sense_clarification": "частность, подробность"
                            },
                            {
                                "target_lang_word": "данные",
                                "target_lang_sense_clarification": "сведения, информация (обычно во мн. ч.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
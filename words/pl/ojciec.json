            "senses": [
                {
                    "sense_id": "d58e035b-abc0-38be-bf47-9c1f43eba65a",
                    "sense_definition": "Mężczyzna, który ma własne dziecko (w relacji do tego dziecka).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "family_biological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>ojciec</w> pracuje w dużym biurze w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Мой <w>отец</w> работает в большом офисе в центре города."
                            }
                        },
                        {
                            "text": "Każdy dobry <w>ojciec</w> dba o bezpieczeństwo swojej rodziny.",
                            "target_lang_translations": {
                                "ru": "Каждый хороший <w>отец</w> заботится о безопасности своей семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "tata",
                        "rodzic"
                    ],
                    "antonyms": [
                        "matka"
                    ],
                    "common_phrases": [
                        "dobry ojciec",
                        "zostać ojcem",
                        "ojciec i matka",
                        "podobny do ojca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, у которого есть собственный ребенок (по отношению к этому ребенку)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отец",
                                "target_lang_sense_clarification": "родитель"
                            },
                            {
                                "target_lang_word": "папа",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b78580e-f0a2-36e0-bc98-3ad89e16799f",
                    "sense_definition": "Tytuł grzecznościowy używany wobec zakonników oraz niektórych księży.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj <w>ojciec</w> proboszcz ogłosił termin parafialnego festynu.",
                            "target_lang_translations": {
                                "ru": "Вчера <w>отец</w> настоятель объявил дату приходского праздника."
                            }
                        },
                        {
                            "text": "Młody <w>ojciec</w> z zakonu dominikanów wygłosił piękne kazanie.",
                            "target_lang_translations": {
                                "ru": "Молодой <w>отец</w> из ордена доминиканцев произнёс прекрасную проповедь."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakonnik",
                        "duchowny",
                        "ksiądz"
                    ],
                    "common_phrases": [
                        "ojciec dyrektor",
                        "ojciec przełożony",
                        "ojciec duchowny"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "religious context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул вежливости, используемый по отношению к монахам и некоторым священникам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отец",
                                "target_lang_sense_clarification": "обращение к священнику"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db7a0e49-9875-3bde-95b0-7f7a60b4bbac",
                    "sense_definition": "Samiec zwierzęcia, który ma potomstwo.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "family_biological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten piękny ogier jest <w>ojcem</w> wielu koni wyścigowych.",
                            "target_lang_translations": {
                                "ru": "Этот красивый жеребец является <w>отцом</w> многих скаковых лошадей."
                            }
                        },
                        {
                            "text": "Nie wiemy, który pies jest <w>ojcem</w> tych szczeniaków.",
                            "target_lang_translations": {
                                "ru": "Мы не знаем, какой пёс является <w>отцом</w> этих щенков."
                            }
                        }
                    ],
                    "synonyms": [
                        "samiec"
                    ],
                    "antonyms": [
                        "matka"
                    ],
                    "common_phrases": [
                        "nieznany ojciec",
                        "ojciec miotu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самец животного, имеющий потомство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отец",
                                "target_lang_sense_clarification": "самец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32f20b8f-acd1-3cd5-9cf4-fb2fe2987b38",
                    "sense_definition": "Mężczyzna, który coś zapoczątkował, stworzył lub jest założycielem jakiejś idei, nurtu lub organizacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_creator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hipokrates jest powszechnie uważany za <w>ojca</w> medycyny.",
                            "target_lang_translations": {
                                "ru": "Гиппократ считается <w>отцом</w> медицины."
                            }
                        },
                        {
                            "text": "Ten polityk jest nazywany <w>ojcem</w> nowoczesnej demokracji w naszym kraju.",
                            "target_lang_translations": {
                                "ru": "Этого политика называют <w>отцом</w> современной демократии в нашей стране."
                            }
                        }
                    ],
                    "synonyms": [
                        "twórca",
                        "założyciel",
                        "inicjator"
                    ],
                    "common_phrases": [
                        "ojciec sukcesu",
                        "ojciec niepodległości",
                        "ojciec założyciel"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "metaphorical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, который положил начало чему-либо, создал что-то или является основателем идеи, течения или организации."

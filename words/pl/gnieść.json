                    ],
                    "target_lang_definitions": {
                        "ru": "Давить на что-то с силой, изменяя его форму или разрушая структуру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мять",
                                "target_lang_sense_clarification": "деформировать руками"
                            },
                            {
                                "target_lang_word": "сминать",
                                "target_lang_sense_clarification": "сдавливать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32377e30-f722-3b4d-a09c-a260cd31bfa6",
                    "sense_definition": "Sprawiać komuś cierpienie, przytłaczać psychicznie lub ograniczać czyjąś swobodę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "s_oppress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poczucie winy zaczęło go <w>gnieść</w> coraz bardziej z każdym dniem.",
                            "target_lang_translations": {
                                "ru": "Чувство вины начало <w>гнести</w> его всё сильнее с каждым днём."
                            }
                        },
                        {
                            "text": "Władza nie powinna <w>gnieść</w> obywateli nadmiernymi podatkami i zakazami.",
                            "target_lang_translations": {
                                "ru": "Власть не должна <w>угнетать</w> граждан чрезмерными налогами и запретами."
                            }
                        }
                    ],
                    "synonyms": [

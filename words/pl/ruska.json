                    "sense_id": "07dafed9-f770-3409-91cb-bbf4fa296867",
                    "sense_definition": "forma żeńska mianownika i wołacza liczby pojedynczej przymiotnika <w>ruski</w>",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3f54c62d-c0ed-302e-99e0-78ed309fd877",
                    "sense_definition": "Воспринимать что-либо органами чувств (зрением, слухом, обонянием); видеть или слышать отдельные детали.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sensory_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В густом тумане водитель с трудом <w>различал</w> дорогу и встречные машины.",
                            "target_lang_translations": {
                                "en": "In the thick fog, the driver could hardly <w>make out</w> the road and oncoming cars."
                            }
                        },
                        {
                            "text": "Сквозь шум дождя я всё же <w>различаю</w> тихие шаги в коридоре.",
                            "target_lang_translations": {
                                "en": "Through the noise of the rain, I still <w>distinguish</w> quiet footsteps in the corridor."
                            }
                        }
                    ],
                    "synonyms": [
                        "распознавать",
                        "видеть",
                        "слышать",
                        "улавливать",
                        "замечать"
                    ],
                    "common_phrases": [
                        "с трудом различать",
                        "различать в темноте",
                        "различать голоса",
                        "различать силуэты",
                        "ясно различать"
                    ],
                    "target_lang_definitions": {
                        "en": "To perceive something with the senses (sight, hearing, smell); to see or hear specific details."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make out",
                                "target_lang_sense_clarification": "to see, hear, or understand something with difficulty"
                            },
                            {
                                "target_lang_word": "distinguish",
                                "target_lang_sense_clarification": "to manage to see or hear something clearly"
                            },
                            {
                                "target_lang_word": "discern",
                                "target_lang_sense_clarification": "to perceive or recognize something (often formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43d7c12e-c193-3b56-929b-0421ab8400f9",
                    "sense_definition": "Устанавливать различие между кем-либо или чем-либо; понимать разницу.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intellectual_distinction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленький ребёнок постепенно учится <w>различать</w> цвета и формы предметов.",
                            "target_lang_translations": {
                                "en": "A small child gradually learns to <w>distinguish</w> colors and object shapes."
                            }
                        },
                        {
                            "text": "Важно уметь <w>различать</w> правду и ложь в потоке информации.",
                            "target_lang_translations": {
                                "en": "It is important to be able to <w>distinguish</w> truth from lies in the stream of information."
                            }
                        }
                    ],
                    "synonyms": [
                        "отличать"
                    ],
                    "antonyms": [
                        "путать",
                        "смешивать"
                    ],
                    "common_phrases": [
                        "уметь различать",
                        "различать добро и зло",
                        "различать оттенки",
                        "перестать различать"
                    ],
                    "target_lang_definitions": {
                        "en": "To establish a difference between people or things; to understand the difference."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "distinguish",
                                "target_lang_sense_clarification": "to recognize the difference between things"
                            },
                            {
                                "target_lang_word": "differentiate",
                                "target_lang_sense_clarification": "to recognize or ascertain what makes things different"
                            },
                            {
                                "target_lang_word": "tell apart",
                                "target_lang_sense_clarification": "to distinguish between two or more people or things"
                            }
                        ]
                    }
                }
            ]
        }
    ]

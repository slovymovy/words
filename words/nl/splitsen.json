                    "common_phrases": [
                        "de rekening splitsen",
                        "in groepen splitsen",
                        "atomen splitsen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разделить или разъединить что-либо на две или более частей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разделить",

                },
                {
                    "sense_id": "c9aac15d-bf43-3f08-bbec-3c9517bd7016",
                    "sense_definition": "Otwór na szczycie kanalika w osłonce zalążka rośliny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biological_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez <w>okienko</w> do wnętrza zalążka wnika łagiewka pyłkowa.",
                            "target_lang_translations": {
                                "ru": "Через <w>микропиле</w> внутрь семяпочки проникает пыльцевая трубка."
                            }
                        },
                        {
                            "text": "Botanicy obserwowali pod mikroskopem strukturę, którą tworzy <w>okienko</w> mikropylarne.",
                            "target_lang_translations": {
                                "ru": "Ботаники наблюдали под микроскопом структуру, которую образует микропилярное <w>отверстие</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mikropyle",
                        "okienko mikropylarne"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "botany"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отверстие на вершине канала в оболочке семяпочки растения."
                    },
                    "translations": {

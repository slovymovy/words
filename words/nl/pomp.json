                    "sense_id": "6bc92d8f-e125-3489-a8a7-965c9049e4df",
                    "sense_definition": "Een werktuig dat vloeistoffen of gassen verplaatst door middel van drukverschil.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "device",
                    "name_type": "no",

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1d4c2065-65f4-386d-9c05-ade2c4b416cc",
                    "sense_definition": "Niet te geloven; zo vreemd of onwaarschijnlijk dat het onmogelijk waar lijkt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is <w>ongelooflijk</w> dat hij de wedstrijd toch nog heeft gewonnen.",
                            "target_lang_translations": {
                                "en": "It is <w>incredible</w> that he still won the match.",
                                "ru": "<w>Невероятно</w>, что он всё-таки выиграл матч."
                            }
                        },
                        {
                            "text": "Ik ben die <w>ongelooflijke</w> verhalen van hem meer dan zat.",
                            "target_lang_translations": {
                                "en": "I am more than tired of those <w>unbelievable</w> stories of his.",
                                "ru": "Я сыт по горло этими его <w>невероятными</w> историями."
                            }
                        }
                    ],
                    "synonyms": [
                        "ongelofelijk"
                    ],
                    "common_phrases": [
                        "ongelooflijk maar waar",
                        "klinkt ongelooflijk"
                    ],
                    "target_lang_definitions": {
                        "en": "Impossible to believe; so strange or improbable that it seems impossible to be true.",
                        "ru": "Невозможно поверить; настолько странно или маловероятно, что кажется невозможным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unbelievable",
                                "target_lang_sense_clarification": "not to be believed; implausible"
                            },
                            {
                                "target_lang_word": "incredible",
                                "target_lang_sense_clarification": "too extraordinary and improbable to be believed"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "невероятный",
                                "target_lang_sense_clarification": "в который трудно поверить"
                            },
                            {
                                "target_lang_word": "неправдоподобный",
                                "target_lang_sense_clarification": "кажущийся ложным или выдуманным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3653eeb-0563-30bd-9ae1-efadc62737b2",
                    "sense_definition": "In zeer hoge mate; buitengewoon of uitzonderlijk.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de lange wandeling had ik een <w>ongelooflijke</w> honger.",
                            "target_lang_translations": {
                                "en": "After the long walk, I had an <w>incredible</w> hunger.",
                                "ru": "После долгой прогулки у меня был <w>невероятный</w> голод."
                            }
                        },
                        {
                            "text": "Zij kan <w>ongelooflijk</w> goed zingen voor haar leeftijd.",
                            "target_lang_translations": {
                                "en": "She can sing <w>incredibly</w> well for her age.",
                                "ru": "Она <w>невероятно</w> хорошо поёт для своего возраста."
                            }
                        }
                    ],
                    "synonyms": [
                        "ongelofelijk"
                    ],
                    "common_phrases": [
                        "ongelooflijk veel",
                        "ongelooflijk mooi",
                        "ongelooflijk snel"
                    ],
                    "target_lang_definitions": {
                        "en": "To a very high degree; extraordinary or exceptional.",
                        "ru": "В очень высокой степени; необычайно или исключительно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "incredible",
                                "target_lang_sense_clarification": "extraordinary or amazing"
                            },
                            {
                                "target_lang_word": "unbelievable",
                                "target_lang_sense_clarification": "extreme or astonishing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "невероятный",
                                "target_lang_sense_clarification": "чрезвычайный, огромный"
                            },
                            {
                                "target_lang_word": "потрясающий",
                                "target_lang_sense_clarification": "восхитительный, впечатляющий"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
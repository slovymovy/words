    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3f6c8950-e9c4-3358-86f9-c48cae9c4ec1",
                    "sense_definition": "The act of assigning a value to something that is lower than its true worth.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government was criticized for the <w>undervaluation</w> of state assets during the privatization process.",
                            "target_lang_translations": {
                                "ru": "Правительство критиковали за <w>занижение стоимости</w> государственных активов в процессе приватизации."
                            }
                        },
                        {
                            "text": "Economists argued that the <w>undervaluation</w> of the currency helped boost the country's exports.",
                            "target_lang_translations": {
                                "ru": "Экономисты утверждали, что <w>недооценка</w> валюты помогла увеличить экспорт страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "undervalue#Noun",
                        "underweening"
                    ],
                    "antonyms": [
                        "overvaluation"
                    ],
                    "common_phrases": [
                        "currency undervaluation",
                        "asset undervaluation",
                        "market undervaluation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по установлению стоимости чего-либо ниже его реальной ценности"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недооценка",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "занижение стоимости",
                                "target_lang_sense_clarification": "активов, имущества"
                            },
                            {
                                "target_lang_word": "занижение курса",
                                "target_lang_sense_clarification": "валюты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cd064d1-6035-3002-b248-728071d1f0b2",
                    "sense_definition": "An estimated value that is lower than the actual worth of something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The audit revealed a significant <w>undervaluation</w> of the company's property holdings.",
                            "target_lang_translations": {
                                "ru": "Аудит выявил значительную <w>недооценку</w> имущественных активов компании."
                            }
                        },
                        {
                            "text": "Due to a calculation error, there was a slight <w>undervaluation</w> in the final report.",
                            "target_lang_translations": {
                                "ru": "Из-за ошибки в расчетах в итоговом отчете была небольшая <w>недооценка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "underweening"
                    ],
                    "antonyms": [
                        "overvaluation"
                    ],
                    "common_phrases": [
                        "significant undervaluation",
                        "gross undervaluation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценочная стоимость, которая оказывается ниже фактической ценности"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заниженная оценка",
                                "target_lang_sense_clarification": "результат оценки"
                            },
                            {
                                "target_lang_word": "недооценка",
                                "target_lang_sense_clarification": "факт несоответствия цены"
                            }
                        ]
                    }
                }
            ]
        }

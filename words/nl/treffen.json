    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6033dda2-5791-3a35-b18a-204bb6e7efb4",
                    "sense_definition": "Iemand tegenkomen of bij elkaar komen op een afgesproken plek.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "meeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>treffen</w> elkaar morgenmiddag in het centrum van de stad.",
                            "target_lang_translations": {
                                "en": "We will <w>meet</w> each other tomorrow afternoon in the city center.",
                                "ru": "Мы <w>встретимся</w> завтра днем в центре города."
                            }
                        },
                        {
                            "text": "Ik hoop dat ik je thuis <w>tref</w> als ik straks even langskom.",
                            "target_lang_translations": {
                                "en": "I hope to <w>find</w> you at home when I drop by later.",
                                "ru": "Я надеюсь <w>застать</w> тебя дома, когда загляну попозже."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontmoeten"
                    ],
                    "common_phrases": [
                        "elkaar treffen",
                        "iemand thuis treffen"
                    ],
                    "target_lang_definitions": {
                        "en": "To encounter someone or come together at an agreed place.",
                        "ru": "Встретить кого-то или собраться вместе в условленном месте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "meet",
                                "target_lang_sense_clarification": "to come together with someone"
                            },
                            {
                                "target_lang_word": "encounter",
                                "target_lang_sense_clarification": "to meet unexpectedly"
                            },
                            {
                                "target_lang_word": "find",
                                "target_lang_sense_clarification": "to discover someone at a location"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "встречаться",
                                "target_lang_sense_clarification": "собираться вместе с кем-то"
                            },
                            {
                                "target_lang_word": "встретить",
                                "target_lang_sense_clarification": "увидеть или натолкнуться на кого-то"
                            },
                            {
                                "target_lang_word": "застать",
                                "target_lang_sense_clarification": "обнаружить кого-то в определенном месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ac181e2-8c2e-34f4-810b-4ee8ca42ede5",
                    "sense_definition": "Iets of iemand fysiek raken, of emotioneel diep raken (vaak door onheil).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De pijl heeft het midden van het bord precies <w>getroffen</w>.",
                            "target_lang_translations": {
                                "en": "The arrow <w>hit</w> the center of the board precisely.",
                                "ru": "Стрела точно <w>попала</w> в центр мишени."
                            }
                        },
                        {
                            "text": "Het hele land werd diep <w>getroffen</w> door de verschrikkelijke natuurramp.",
                            "target_lang_translations": {
                                "en": "The whole country was deeply <w>affected</w> by the terrible natural disaster.",
                                "ru": "Вся страна была глубоко <w>поражена</w> ужасным стихийным бедствием."
                            }
                        }
                    ],
                    "synonyms": [
                        "raken"
                    ],
                    "antonyms": [
                        "missen"
                    ],
                    "common_phrases": [
                        "doel treffen",
                        "zwaar getroffen",
                        "diep treffen"
                    ],
                    "target_lang_definitions": {
                        "en": "To physically hit something or someone, or to affect deeply emotionally (often by misfortune).",
                        "ru": "Физически попасть в цель или глубоко задеть эмоционально (часто о несчастье)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hit",
                                "target_lang_sense_clarification": "to physically strike a target"
                            },
                            {
                                "target_lang_word": "strike",
                                "target_lang_sense_clarification": "to hit forcefully or affect emotionally"
                            },
                            {
                                "target_lang_word": "affect",
                                "target_lang_sense_clarification": "to have an emotional impact on"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "физически ударить в цель"
                            },
                            {
                                "target_lang_word": "поразить",
                                "target_lang_sense_clarification": "произвести сильное впечатление или нанести удар"
                            },
                            {
                                "target_lang_word": "задеть",
                                "target_lang_sense_clarification": "коснуться или обидеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1111e05-94a0-37c0-9518-fd08d954e0fc",
                    "sense_definition": "Op het juiste moment gebeuren of goed uitkomen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "opportune",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat <w>treft</w>, want ik was net naar jou op zoek.",
                            "target_lang_translations": {
                                "en": "That <w>works out well</w>, because I was just looking for you.",
                                "ru": "Это <w>удачно совпало</w>, так как я как раз искал тебя."
                            }
                        },
                        {
                            "text": "Het <w>trof</w> bijzonder goed dat we allebei een dag vrij waren.",
                            "target_lang_translations": {
                                "en": "It <w>worked out</w> particularly well that we both had a day off.",
                                "ru": "<w>Сложилось</w> особенно удачно, что у нас у обоих был выходной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dat treft",
                        "het treft dat"
                    ],
                    "target_lang_definitions": {
                        "en": "To happen at the right moment or turn out well.",
                        "ru": "Случиться в нужный момент или удачно сложиться."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "work out",
                                "target_lang_sense_clarification": "to happen opportunely"
                            },
                            {
                                "target_lang_word": "be convenient",
                                "target_lang_sense_clarification": "to suit the timing or situation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "удачно сложиться",
                                "target_lang_sense_clarification": "произойти в подходящий момент"
                            },
                            {
                                "target_lang_word": "быть кстати",
                                "target_lang_sense_clarification": "подходить к моменту или ситуации"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a456f03b-f443-39f1-8ec3-6079386bd775",
                    "sense_definition": "Strong alcoholic drink derived from fermentation and distillation; more broadly, any alcoholic drink.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "alcohol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He went to the store to buy a bottle of <w>liquor</w> for the party.",
                            "target_lang_translations": {
                                "ru": "Он пошел в магазин, чтобы купить бутылку <w>спиртного</w> для вечеринки."
                            }
                        },
                        {
                            "text": "The bar serves beer, wine, and various types of hard <w>liquor</w>.",
                            "target_lang_translations": {
                                "ru": "В баре подают пиво, вино и различные виды <w>крепкого алкоголя</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "spirits",
                        "hard liquor",
                        "hard drink",
                        "strong drink",
                        "aqua vitae",
                        "water of life"
                    ],
                    "common_phrases": [
                        "hard liquor",
                        "liquor store",
                        "malt liquor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крепкий алкогольный напиток, полученный путем дистилляции; в более широком смысле — любой алкогольный напиток."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крепкий алкоголь",
                                "target_lang_sense_clarification": "напитки с высоким содержанием спирта (виски, водка, ром)"
                            },
                            {
                                "target_lang_word": "спиртное",
                                "target_lang_sense_clarification": "общее название для алкогольных напитков"
                            },
                            {
                                "target_lang_word": "алкоголь",
                                "target_lang_sense_clarification": "спиртные напитки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fec8cba-9e59-3637-a2c2-89c0290ca61f",
                    "sense_definition": "A liquid obtained by cooking meat or vegetables.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "cooking_liquid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chef used the chicken <w>liquor</w> to create a rich and flavorful soup.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар использовал куриный <w>бульон</w>, чтобы приготовить наваристый и ароматный суп."
                            }
                        },
                        {
                            "text": "You should save the pot <w>liquor</w> from the greens as it contains many vitamins.",
                            "target_lang_translations": {
                                "ru": "Вам следует сохранить <w>отвар</w> от зелени, так как в нем содержится много витаминов."
                            }
                        }
                    ],
                    "synonyms": [
                        "stock",
                        "broth",
                        "pot liquor",
                        "bouillon"
                    ],
                    "common_phrases": [
                        "pot liquor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкость, полученная в результате варки мяса или овощей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бульон",
                                "target_lang_sense_clarification": "жидкий навар из мяса"
                            },
                            {
                                "target_lang_word": "отвар",
                                "target_lang_sense_clarification": "жидкость после варки овощей или грибов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd1206fd-38d5-3ced-8a89-6e3efd1026d6",
                    "sense_definition": "A liquid solution containing chemicals, used in industrial processes such as pulping or tanning.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "industrial_liquid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory treats the waste <w>liquor</w> before releasing it into the environment.",
                            "target_lang_translations": {
                                "ru": "Завод очищает отработанный <w>раствор</w> перед сбросом в окружающую среду."
                            }
                        },
                        {
                            "text": "Workers must wear protective gear when handling the corrosive <w>liquor</w> in the tanks.",
                            "target_lang_translations": {
                                "ru": "Рабочие должны носить защитное снаряжение при работе с едким <w>раствором</w> в резервуарах."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкий раствор, содержащий химические вещества, используемый в промышленных процессах (например, при производстве бумаги или дублении)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раствор",
                                "target_lang_sense_clarification": "техническая жидкость с химикатами"
                            },
                            {
                                "target_lang_word": "щёлок",
                                "target_lang_sense_clarification": "специфический термин в целлюлозной промышленности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "364165e0-aff3-3b1f-83ba-803e5d4fcdc7",
                    "sense_definition": "A parsley sauce commonly served with traditional pies and mash.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sauce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He ordered a traditional plate of pie and mash covered in green <w>liquor</w>.",
                            "target_lang_translations": {
                                "ru": "Он заказал традиционную порцию пирога с пюре, политую зеленым <w>соусом из петрушки</w>."
                            }
                        },
                        {
                            "text": "The shop is famous for its authentic eel pie served with hot <w>liquor</w>.",
                            "target_lang_translations": {
                                "ru": "Магазин славится своим настоящим пирогом с угрем, который подают с горячим <w>соусом</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK, specifically London/Cockney"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Соус из петрушки, который традиционно подают к пирогам с картофельным пюре (британское блюдо)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соус из петрушки",
                                "target_lang_sense_clarification": "традиционная подливка к английским пирогам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c9ff4c5-d754-30e5-9a1c-7c97b5a66f1d",
                    "sense_definition": "A liquid substance or fluid.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "general_liquid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The alchemist poured the mysterious glowing <w>liquor</w> into a glass vial.",
                            "target_lang_translations": {
                                "ru": "Алхимик налил таинственную светящуюся <w>жидкость</w> в стеклянный флакон."
                            }
                        },
                        {
                            "text": "In the old text, blood is described as the vital <w>liquor</w> of life.",
                            "target_lang_translations": {
                                "ru": "В старинном тексте кровь описывается как жизненная <w>влага</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкая субстанция или жидкость (устаревшее или литературное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жидкость",
                                "target_lang_sense_clarification": "любая жидкая субстанция"
                            },
                            {
                                "target_lang_word": "влага",
                                "target_lang_sense_clarification": "поэтическое или устаревшее обозначение жидкости"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "09967ab9-f3a4-3a9f-80c7-e7e8e3dfdc91",
                    "sense_definition": "To drink alcoholic beverages, usually to excess, or to cause someone to do so.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "drink_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to <w>liquor</w> up before heading out to the club for the night.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>напиться</w> перед тем, как отправиться в клуб на всю ночь."
                            }
                        },
                        {
                            "text": "He was fully <w>liquored</w> up by the time the guests arrived at the party.",
                            "target_lang_translations": {
                                "ru": "Он уже успел <w>напиться</w> к тому времени, как гости прибыли на вечеринку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "liquor up"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used with 'up'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пить алкогольные напитки, обычно в избытке, или спаивать кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напиваться",
                                "target_lang_sense_clarification": "употреблять много алкоголя"
                            },
                            {
                                "target_lang_word": "пьянствовать",
                                "target_lang_sense_clarification": "предаваться пьянству"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d709109-b899-3eda-bfa1-97cc29f4cfd1",
                    "sense_definition": "To apply grease or oil to something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grease_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer <w>liquored</w> his leather boots to keep the water out.",
                            "target_lang_translations": {
                                "ru": "Фермер <w>смазал жиром</w> свои кожаные сапоги, чтобы они не пропускали воду."
                            }
                        },
                        {
                            "text": "You should <w>liquor</w> the cart wheels to stop them from squeaking.",
                            "target_lang_translations": {
                                "ru": "Тебе следует <w>смазать</w> колеса телеги, чтобы они не скрипели."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": "dialectal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить жир или масло на что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смазывать",
                                "target_lang_sense_clarification": "покрывать смазкой или жиром"
                            },
                            {
                                "target_lang_word": "жировать",
                                "target_lang_sense_clarification": "пропитывать кожу жиром"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
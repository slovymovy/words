    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2760910f-d017-30d3-964d-2c1cd87154dc",
                    "sense_definition": "Taki, który lubi przebywać w towarzystwie innych ludzi i łatwo nawiązuje kontakty.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój wujek jest otwartą i <w>towarzyską</w> osobą.",
                            "target_lang_translations": {
                                "ru": "Мой дядя — открытый и <w>общительный</w> человек."
                            }
                        },
                        {
                            "text": "Anna jest bardzo <w>towarzyska</w> i szybko zdobywa nowych przyjaciół.",
                            "target_lang_translations": {
                                "ru": "Анна очень <w>общительна</w> и быстро находит новых друзей."
                            }
                        }
                    ],
                    "synonyms": [
                        "otwarty",
                        "koleżeński",
                        "ekstrawertyczny",
                        "jowialny"
                    ],
                    "antonyms": [
                        "nietowarzyski",
                        "introwertyczny"
                    ],
                    "common_phrases": [
                        "osoba towarzyska",
                        "być towarzyskim",
                        "człowiek towarzyski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто любит находиться в обществе других людей и легко устанавливает контакты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общительный",
                                "target_lang_sense_clarification": "легко вступающий в контакт с людьми"
                            },
                            {
                                "target_lang_word": "коммуникабельный",
                                "target_lang_sense_clarification": "обладающий хорошими навыками общения (часто в профессиональном контексте)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ede92b5-8d98-38d6-8d45-c77ff1f32f26",
                    "sense_definition": "Dotyczący wspólnego spędzania czasu, rozrywki lub kontaktów między znajomymi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_life",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zapisałem się na kurs tańca <w>towarzyskiego</w>.",
                            "target_lang_translations": {
                                "ru": "Я записался на курс <w>бальных</w> танцев."
                            }
                        },
                        {
                            "text": "W sobotę planujemy małe spotkanie <w>towarzyskie</w> w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "В субботу мы планируем небольшую <w>дружескую</w> встречу в саду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "taniec towarzyski",
                        "gry towarzyskie",
                        "życie towarzyskie",
                        "spotkanie towarzyskie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к совместному времяпрепровождению, развлечениям или контактам между знакомыми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "светский",
                                "target_lang_sense_clarification": "связанный с жизнью общества, развлечениями (напр. жизнь, беседа)"
                            },
                            {
                                "target_lang_word": "бальный",
                                "target_lang_sense_clarification": "относящийся к танцам (напр. танец)"
                            },
                            {
                                "target_lang_word": "товарищеский",
                                "target_lang_sense_clarification": "дружеский, неофициальный (напр. матч, встреча)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7105960-ae9f-3271-9a05-5c835bf2f6f7",
                    "sense_definition": "Związany z płatnymi usługami seksualnymi (określenie eufemistyczne).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "euphemism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz portal randkowy nie przyjmuje anonsów <w>towarzyskich</w>.",
                            "target_lang_translations": {
                                "ru": "Наш сайт знакомств не принимает объявлений об <w>эскорт-услугах</w>."
                            }
                        },
                        {
                            "text": "W tej dzielnicy zlikwidowano nielegalną agencję <w>towarzyską</w>.",
                            "target_lang_translations": {
                                "ru": "В этом районе ликвидировали нелегальное <w>эскорт</w>-агентство."
                            }
                        }
                    ],
                    "common_phrases": [
                        "agencja towarzyska",
                        "anonsy towarzyskie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с платными сексуальными услугами (эвфемизм)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эскорт-",
                                "target_lang_sense_clarification": "в составе сложных слов (напр. эскорт-услуги, эскорт-агентство)"
                            },
                            {
                                "target_lang_word": "интимный",
                                "target_lang_sense_clarification": "связанный с сексуальной сферой (напр. услуги)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
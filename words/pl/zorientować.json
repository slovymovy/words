                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сориентировать",
                                "target_lang_sense_clarification": "направить в определенную сторону"
                            },
                            {
                                "target_lang_word": "нацелить",
                                "target_lang_sense_clarification": "сфокусировать на задаче"
                            },
                            {

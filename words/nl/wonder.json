    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a82985fc-4956-355f-ba04-b2c20d3c8439",
                    "sense_definition": "Een gebeurtenis die niet logisch verklaard kan worden en vaak aan een bovennatuurlijke kracht wordt toegeschreven.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was echt een <w>wonder</w> dat hij dat zware ongeluk heeft overleefd.",
                            "target_lang_translations": {
                                "ru": "Это было настоящим <w>чудом</w>, что он выжил в той тяжелой аварии."
                            }
                        },
                        {
                            "text": "De genezing van de zieke werd door velen beschouwd als een groot <w>wonder</w>.",
                            "target_lang_translations": {
                                "ru": "Исцеление больного многие считали великим <w>чудом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mirakel"
                    ],
                    "common_phrases": [
                        "geen wonder",
                        "een wonder verrichten",
                        "in wonderen geloven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Событие, которое невозможно объяснить логически и которое часто приписывается сверхъестественным силам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чудо",
                                "target_lang_sense_clarification": "сверхъестественное явление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ca91a34-9f70-32c0-9c9c-a7cd2371c205",
                    "sense_definition": "Een natuurlijke gebeurtenis of verschijnsel dat zo bijzonder of mooi is dat het diepe bewondering oproept.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik geloof in de kracht van de natuur en het <w>wonder</w> van de seizoenen.",
                            "target_lang_translations": {
                                "ru": "Я верю в силу природы и <w>чудо</w> смены времен года."
                            }
                        },
                        {
                            "text": "De geboorte van hun eerste kind was voor de ouders een klein <w>wonder</w>.",
                            "target_lang_translations": {
                                "ru": "Рождение первенца стало для родителей маленьким <w>чудом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wonder der natuur",
                        "wereldwonder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Природное событие или явление, настолько особенное или красивое, что вызывает глубокое восхищение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чудо",
                                "target_lang_sense_clarification": "нечто восхитительное"
                            },
                            {
                                "target_lang_word": "диво",
                                "target_lang_sense_clarification": "удивительное явление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33698d03-6054-3e56-aa21-ca6d52b08b66",
                    "sense_definition": "Iets dat een probleem op een verrassend effectieve manier oplost.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een goede nachtrust en minder stress kunnen <w>wonderen</w> doen voor je gezondheid.",
                            "target_lang_translations": {
                                "ru": "Хороший ночной сон и снижение стресса могут творить <w>чудеса</w> для вашего здоровья."
                            }
                        },
                        {
                            "text": "Het nieuwe medicijn verrichtte <w>wonderen</w> bij de patiënten.",
                            "target_lang_translations": {

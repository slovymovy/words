    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0f78e55f-614c-330c-836e-ad224ea285a7",
                    "sense_definition": "Iemand of iets waar je op kunt rekenen en die je niet in de steek laat",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "loyalty_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is al jaren een <w>trouwe</w> werknemer voor zijn baas.",
                            "target_lang_translations": {
                                "ru": "Он уже много лет <w>верный</w> сотрудник своего начальника."
                            }
                        },
                        {
                            "text": "De hond bleef zijn baasje altijd <w>trouw</w>, wat er ook gebeurde.",
                            "target_lang_translations": {
                                "ru": "Собака всегда оставалась <w>преданной</w> своему хозяину, что бы ни случилось."
                            }
                        }
                    ],
                    "synonyms": [
                        "betrouwbaar",
                        "loyaal",
                        "standvastig"
                    ],
                    "antonyms": [
                        "onbetrouwbaar",
                        "ontrouw",
                        "verraderlijk"
                    ],
                    "common_phrases": [
                        "trouwe dienst",
                        "trouw blijven",
                        "trouwe vriend"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, на кого можно положиться и кто не подведет"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верный",
                                "target_lang_sense_clarification": "надежный, постоянный в чувствах"
                            },
                            {
                                "target_lang_word": "преданный",
                                "target_lang_sense_clarification": "исполненный любви и верности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b7a0991e-f599-38d4-8cc3-158412425c85",
                    "sense_definition": "De eigenschap dat men zich aan een verbintenis, belofte of persoon houdt",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "loyalty_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaat toonde grote <w>trouw</w> aan zijn land tijdens de oorlog.",
                            "target_lang_translations": {
                                "ru": "Солдат проявил большую <w>верность</w> своей стране во время войны."
                            }
                        },
                        {
                            "text": "Zij beloofden elkaar eeuwige <w>trouw</w> tijdens de ceremonie.",
                            "target_lang_translations": {
                                "ru": "Они обещали друг другу вечную <w>верность</w> во время церемонии."
                            }
                        }
                    ],
                    "antonyms": [
                        "ontrouw",
                        "verraad"
                    ],
                    "common_phrases": [
                        "eeuwige trouw",
                        "trouw zweren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество, заключающееся в соблюдении обязательств, обещаний или верности человеку"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верность",
                                "target_lang_sense_clarification": "стойкость и неизменность в чувствах или обязанностях"
                            },
                            {
                                "target_lang_word": "преданность",
                                "target_lang_sense_clarification": "глубокая приверженность кому-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e21e095a-1324-3eb7-8e45-9470b2b4e5d3",
                    "sense_definition": "Het huwelijk of de huwelijksceremonie",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "marriage_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de <w>trouw</w> gingen ze direct op huwelijksreis naar Italië.",
                            "target_lang_translations": {
                                "ru": "После <w>свадьбы</w> они сразу отправились в свадебное путешествие в Италию."
                            }
                        },
                        {
                            "text": "De hele familie was aanwezig om de <w>trouw</w> te vieren.",
                            "target_lang_translations": {
                                "ru": "Вся семья присутствовала, чтобы отпраздновать <w>свадьбу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "huwelijk",
                        "echt"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België gebruikt"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Свадьба или брачная церемония"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свадьба",
                                "target_lang_sense_clarification": "празднование вступления в брак"
                            },
                            {
                                "target_lang_word": "бракосочетание",
                                "target_lang_sense_clarification": "официальная церемония"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0184c62e-5cd3-391a-9ef5-0ca75b72b34c",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>trouwen</w>",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>trouw</w> volgende maand met mijn grote liefde.",
                            "target_lang_translations": {
                                "ru": "В следующем месяце я <w>выхожу замуж</w> за свою большую любовь."
                            }
                        },
                        {
                            "text": "Vandaag <w>trouw</w> ik eindelijk met mijn beste vriendin.",
                            "target_lang_translations": {
                                "ru": "Сегодня я наконец <w>женюсь</w> на своей лучшей подруге."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола «жениться» или «выходить замуж»"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "женюсь",
                                "target_lang_sense_clarification": "о мужчине"
                            },
                            {
                                "target_lang_word": "выхожу замуж",
                                "target_lang_sense_clarification": "о женщине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f579d15f-740b-3708-9e5b-e6200fc351dd",
                    "sense_definition": "Gebiedende wijs van <w>trouwen</w>",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Trouw</w> niet met hem als je nog twijfelt over je gevoelens.",
                            "target_lang_translations": {
                                "ru": "Не <w>выходи замуж</w> за него, если всё ещё сомневаешься в своих чувствах."
                            }
                        },
                        {
                            "text": "<w>Trouw</w> alleen uit liefde en niet om andere redenen.",
                            "target_lang_translations": {
                                "ru": "<w>Женись</w> только по любви, а не по другим причинам."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола «жениться» или «выходить замуж»"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "женись",
                                "target_lang_sense_clarification": "обращение к мужчине"
                            },
                            {
                                "target_lang_word": "выходи замуж",
                                "target_lang_sense_clarification": "обращение к женщине"
                            }
                        ]
                    }
                }
            ]
        }
    ]

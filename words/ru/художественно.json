    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4c17245f-8d33-3173-8f04-5f6ce62be83a",
                    "sense_definition": "Наречие к художественный; в соответствии с требованиями и принципами искусства, творчески.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "art_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автор очень <w>художественно</w> описал осенний лес в своём новом романе.",
                            "target_lang_translations": {
                                "en": "The author described the autumn forest very <w>artistically</w> in his new novel."
                            }
                        },
                        {
                            "text": "Этот фильм снят <w>художественно</w> и с большим вниманием к деталям.",
                            "target_lang_translations": {
                                "en": "This film was shot <w>artistically</w> and with great attention to detail."
                            }
                        }
                    ],
                    "synonyms": [
                        "творчески",
                        "мастерски",
                        "искусно"
                    ],
                    "antonyms": [
                        "бездарно",
                        "ремесленно"
                    ],
                    "common_phrases": [
                        "художественно оформить",
                        "художественно ценный",
                        "художественно обработать"
                    ],
                    "target_lang_definitions": {
                        "en": "In an artistic manner; in accordance with the requirements and principles of art, creatively."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "artistically",
                                "target_lang_sense_clarification": "relating to art or literature"
                            },
                            {
                                "target_lang_word": "creatively",
                                "target_lang_sense_clarification": "with imagination or skill"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "158db12f-71f8-3c66-b3e9-b3796645d1a5",
                    "sense_definition": "Красиво, изящно, со вкусом; эстетично.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "aesthetic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она <w>художественно</w> разложила фрукты на большой тарелке для гостей.",
                            "target_lang_translations": {
                                "en": "She <w>artistically</w> arranged the fruit on a large plate for the guests."
                            }
                        },
                        {
                            "text": "Шарф был повязан небрежно, но очень <w>художественно</w>.",
                            "target_lang_translations": {
                                "en": "The scarf was tied carelessly, but very <w>artistically</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "красиво",
                        "изящно",
                        "эстетично",
                        "живописно"
                    ],
                    "antonyms": [
                        "безвкусно",
                        "безобразно"
                    ],
                    "common_phrases": [
                        "выглядеть художественно",
                        "художественно разбросать"
                    ],
                    "target_lang_definitions": {
                        "en": "Beautifully, gracefully, with taste; aesthetically."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "artistically",
                                "target_lang_sense_clarification": "in an aesthetically pleasing way"
                            },
                            {
                                "target_lang_word": "tastefully",
                                "target_lang_sense_clarification": "with good taste or style"
                            }
                        ]
                    }
                }
            ]
        },

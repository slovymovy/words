            "senses": [
                {
                    "sense_id": "72f1fa4d-1036-36be-9e1d-85b881f99069",
                    "sense_definition": "Het bovenste deel van het menselijk lichaam, waar de hersenen, ogen, oren, neus en mond zitten.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij stootte zijn <w>hoofd</w> hard tegen de lage deuropening.",
                            "target_lang_translations": {
                                "ru": "Он сильно ударился <w>головой</w> о низкий дверной проем.",
                                "en": "He bumped his <w>head</w> hard against the low doorway."
                            }
                        },
                        {
                            "text": "Ik moet dit gedicht voor morgen helemaal uit mijn <w>hoofd</w> leren.",
                            "target_lang_translations": {
                                "ru": "Я должен выучить это стихотворение <w>наизусть</w> к завтрашнему дню.",
                                "en": "I have to learn this poem entirely by <w>heart</w> for tomorrow."
                            }
                        }
                    ],
                    "synonyms": [
                        "harses",
                        "kanis",
                        "knar"
                    ],
                    "common_phrases": [
                        "uit het hoofd",
                        "hoofd en schouders",
                        "het hoofd bieden",
                        "het hoofd koel houden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя часть человеческого тела, где находятся мозг, глаза, уши, нос и рот.",
                        "en": "The upper part of the human body, where the brain, eyes, ears, nose, and mouth are located."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голова",
                                "target_lang_sense_clarification": "часть тела"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "head",
                                "target_lang_sense_clarification": "body part"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53083448-bf04-33ba-b98b-a1a5ed5e2e5a",
                    "sense_definition": "De persoon die de leiding heeft over een organisatie, afdeling of groep.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "leader_top",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als <w>hoofd</w> van de afdeling is zij verantwoordelijk voor het budget.",
                            "target_lang_translations": {
                                "ru": "Как <w>начальник</w> отдела, она отвечает за бюджет.",
                                "en": "As <w>head</w> of the department, she is responsible for the budget."
                            }
                        },
                        {
                            "text": "De koning is in naam het <w>hoofd</w> van de regering.",
                            "target_lang_translations": {
                                "ru": "Король номинально является <w>главой</w> правительства.",
                                "en": "The king is nominally the <w>head</w> of the government."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan het hoofd staan",
                        "gekroonde hoofden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, возглавляющее организацию, отдел или группу.",
                        "en": "The person who is in charge of an organization, department, or group."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глава",
                                "target_lang_sense_clarification": "руководитель организации или группы"
                            },
                            {
                                "target_lang_word": "начальник",
                                "target_lang_sense_clarification": "руководитель отдела"
                            },
                            {
                                "target_lang_word": "руководитель",
                                "target_lang_sense_clarification": "лидер"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "head",
                                "target_lang_sense_clarification": "leader of a group"
                            },
                            {
                                "target_lang_word": "chief",
                                "target_lang_sense_clarification": "person in charge"
                            },
                            {
                                "target_lang_word": "boss",
                                "target_lang_sense_clarification": "manager"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "febc59f6-1467-380f-bf2d-1bdbdcf14d23",
                    "sense_definition": "Het bovenste, voorste of belangrijkste uiteinde van een voorwerp.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "leader_top",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het diner zat vader plechtig aan het <w>hoofd</w> van de tafel.",
                            "target_lang_translations": {
                                "ru": "Во время ужина отец торжественно сидел во <w>главе</w> стола.",
                                "en": "During dinner, father sat solemnly at the <w>head</w> of the table."
                            }
                        },
                        {
                            "text": "Het <w>hoofd</w> van de hamer vloog eraf tijdens het klussen.",
                            "target_lang_translations": {
                                "ru": "Во время работы <w>головка</w> молотка слетела.",
                                "en": "The <w>head</w> of the hammer flew off while working."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hoofd van de tafel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя, передняя или главная часть предмета.",
                        "en": "The top, front, or most important end of an object."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "головка",
                                "target_lang_sense_clarification": "часть инструмента или механизма"
                            },
                            {
                                "target_lang_word": "глава",
                                "target_lang_sense_clarification": "почетное место (во главе стола)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "head",
                                "target_lang_sense_clarification": "top or front part"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "184ea98d-b248-3d7c-a3d3-24dcd50f6888",
                    "sense_definition": "Een dam, pier of steiger die haaks op de oever of kust in het water steekt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wandelaars liepen tot het einde van het <w>hoofd</w> om de zee te zien.",
                            "target_lang_translations": {
                                "ru": "Прогуливающиеся дошли до конца <w>мола</w>, чтобы посмотреть на море.",
                                "en": "The walkers walked to the end of the <w>jetty</w> to see the sea."
                            }
                        },
                        {
                            "text": "Grote schepen varen voorzichtig langs het <w>hoofd</w> de haven binnen.",
                            "target_lang_translations": {
                                "ru": "Большие корабли осторожно заходят в гавань мимо <w>мола</w>.",
                                "en": "Large ships sail carefully past the <w>jetty</w> into the harbor."
                            }
                        }
                    ],
                    "synonyms": [

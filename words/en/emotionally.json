    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "45f8d4cb-1edb-30c7-a9a3-994b73a345be",
                    "sense_definition": "Regarding emotions or one's psychological state.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The long recovery process was physically easy but <w>emotionally</w> draining.",
                            "target_lang_translations": {
                                "ru": "Длительный процесс восстановления был физически легким, но <w>эмоционально</w> изматывающим."
                            }
                        },
                        {
                            "text": "She felt <w>emotionally</w> attached to the house where she grew up.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала себя <w>эмоционально</w> привязанной к дому, где выросла."
                            }
                        }
                    ],
                    "synonyms": [
                        "psychologically",
                        "mentally",
                        "inwardly"
                    ],
                    "antonyms": [
                        "physically"
                    ],
                    "common_phrases": [
                        "emotionally stable",
                        "emotionally draining",
                        "emotionally attached",
                        "emotionally unavailable"
                    ],
                    "target_lang_definitions": {
                        "ru": "Касательно эмоций или психологического состояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эмоционально",
                                "target_lang_sense_clarification": "в эмоциональном плане, с точки зрения чувств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db7a0263-5198-3403-b59a-074459341021",
                    "sense_definition": "In a manner that displays or expresses emotion.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spoke <w>emotionally</w> about the sacrifices his parents made for him.",
                            "target_lang_translations": {
                                "ru": "Он <w>эмоционально</w> говорил о жертвах, на которые пошли ради него родители."
                            }
                        },
                        {
                            "text": "Try not to react <w>emotionally</w> when you hear the criticism.",
                            "target_lang_translations": {
                                "ru": "Постарайся не реагировать <w>эмоционально</w>, когда услышишь критику."
                            }
                        }
                    ],
                    "synonyms": [
                        "passionately",
                        "feelingly",
                        "intensely"
                    ],
                    "antonyms": [
                        "unemotionally",
                        "coldly",
                        "indifferently",
                        "calmly"
                    ],
                    "common_phrases": [
                        "react emotionally",
                        "speak emotionally",
                        "respond emotionally"
                    ],
                    "target_lang_definitions": {
                        "ru": "Таким образом, который демонстрирует или выражает эмоции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эмоционально",
                                "target_lang_sense_clarification": "с чувством, выразительно"
                            }
                        ]
                    }
                }
            ]
        }
    ]

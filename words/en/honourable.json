    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bfde2a6f-ac25-38bf-a81a-5d13500baafc",
                    "sense_definition": "Deserving of honor and respect; having or showing honesty and good moral character.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "moral_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is an <w>honourable</w> man who always keeps his promises to his family.",
                            "target_lang_translations": {
                                "ru": "Он <w>благородный</w> человек, который всегда выполняет свои обещания перед семьей."
                            }
                        },
                        {
                            "text": "The soldiers were given an <w>honourable</w> discharge after their years of service.",
                            "target_lang_translations": {
                                "ru": "Солдаты получили <w>почётную</w> отставку после многих лет службы."
                            }
                        }
                    ],
                    "synonyms": [
                        "respectable",
                        "upright",
                        "noble",
                        "ethical",
                        "principled"
                    ],
                    "antonyms": [
                        "dishonourable",
                        "shameful",
                        "unethical",
                        "corrupt"
                    ],
                    "common_phrases": [
                        "honourable man",
                        "honourable intentions",
                        "do the honourable thing",
                        "honourable discharge"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заслуживающий чести и уважения; обладающий честностью и высокими моральными качествами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благородный",
                                "target_lang_sense_clarification": "обладающий высокими моральными качествами"
                            },
                            {
                                "target_lang_word": "почётный",
                                "target_lang_sense_clarification": "выражающий почёт, уважение (напр. об отставке)"
                            },
                            {
                                "target_lang_word": "порядочный",
                                "target_lang_sense_clarification": "честный, достойный уважения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e23090ec-f651-30fa-b1f8-390ca5753843",
                    "sense_definition": "A title of respect used for certain government officials, judges, or children of the nobility.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "title",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "The <w>Honourable</w> Jane Smith arrived at the ceremony to represent the committee.",
                            "target_lang_translations": {
                                "ru": "<w>Достопочтенная</w> Джейн Смит прибыла на церемонию, чтобы представить комитет."
                            }
                        },
                        {
                            "text": "Several <w>honourables</w> from the parliament gathered to discuss the new policy.",
                            "target_lang_translations": {
                                "ru": "Несколько <w>достопочтенных</w> членов парламента собрались, чтобы обсудить новую политику."
                            }
                        }
                    ],
                    "common_phrases": [
                        "The Honourable",
                        "Right Honourable"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул уважения, используемый для определенных государственных чиновников, судей или детей знати."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достопочтенный",
                                "target_lang_sense_clarification": "титул или обращение к высокопоставленному лицу"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "2714bae6-9ac3-396c-ab44-d6d1a9103c40",
                    "sense_definition": "Twardy kawałek skały lub minerału, który został oddzielony od całości.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "material_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten duży <kamień> jest zbyt ciężki, żeby go podnieść.",
                            "target_lang_translations": {
                                "ru": "Этот большой <камень> слишком тяжёлый, чтобы его поднять."
                            }
                        },
                        {
                            "text": "Chłopiec rzucił małym <kamieniem> w taflę jeziora.",
                            "target_lang_translations": {
                                "ru": "Мальчик бросил маленький <камень> в гладь озера."
                            }
                        }
                    ],
                    "synonyms": [
                        "głaz",
                        "skała"
                    ],
                    "common_phrases": [
                        "rzucić kamieniem",
                        "kamień węgielny",
                        "serce z kamienia",
                        "kamień z serca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твёрдый кусок горной породы или минерала, отделившийся от основного массива."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "камень",
                                "target_lang_sense_clarification": "кусок породы"

                        {
                            "text": "В этом болоте очень легко <завязать> и не выбраться.",
                            "target_lang_translations": {
                                "en": "It is very easy to <get bogged down> in this swamp and not get out."
                            }
                        }
                    ],
                    "synonyms": [
                        "застревать",
                        "увязать"
                    ],
                    "common_phrases": [
                        "завязать в болоте",
                        "завязать в грязи"
                    ],
                    "target_lang_definitions": {

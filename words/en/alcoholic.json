                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The patient was treated for severe <w>alcoholic</w> poisoning.",
                            "target_lang_translations": {
                                "ru": "Пациента лечили от тяжелого <w>алкогольного</w> отравления."
                            }
                        },
                        {
                            "text": "His <w>alcoholic</w> tendencies caused many problems within the family.",
                            "target_lang_translations": {
                                "ru": "Его <w>алкогольные</w> наклонности создавали много проблем в семье."
                            }
                        }
                    ],
                    "common_phrases": [
                        "alcoholic liver disease",
                        "alcoholic stupor"
                    ],
                    "target_lang_definitions": {

            "senses": [
                {
                    "sense_id": "2f0bd55f-3c81-3e3b-8191-e77b035fbf3e",
                    "sense_definition": "Всегда, обычно, регулярно; повторяясь через промежутки времени.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

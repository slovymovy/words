    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4d0d63e5-e0a4-378b-b557-d2b2c1051bdb",
                    "sense_definition": "Dodać truciznę do czegoś lub podać ją komuś, powodując chorobę lub śmierć.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "poison_active",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wróg planował <zatruć> studnię w wiosce, aby osłabić mieszkańców.",
                            "target_lang_translations": {
                                "ru": "Враг планировал <отравить> колодец в деревне, чтобы ослабить жителей."
                            }
                        },
                        {
                            "text": "Nieświeże mięso może <zatruć> każdego, kto je zje.",
                            "target_lang_translations": {
                                "ru": "Несвежее мясо может <отравить> каждого, кто его съест."
                            }
                        }
                    ],
                    "synonyms": [
                        "otruć"
                    ],
                    "antonyms": [
                        "odtruć"
                    ],
                    "common_phrases": [
                        "zatruć wodę",
                        "zatruć studnię",
                        "zatruć organizm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавить яд во что-то или дать его кому-то, вызывая болезнь или смерть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отравить",
                                "target_lang_sense_clarification": "дать яд, убить ядом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44d8cbe5-9c8a-30f4-92c6-63fc4af071d0",
                    "sense_definition": "Wywołać u siebie objawy chorobowe poprzez spożycie czegoś szkodliwego, nieświeżego lub trującego (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "poison_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Można się poważnie <zatruć> grzybami, jeśli nie zna się na ich zbieraniu.",
                            "target_lang_translations": {
                                "ru": "Можно серьезно <отравиться> грибами, если не разбираешься в их сборе."
                            }
                        },
                        {
                            "text": "Uważaj, żeby nie <zatruć> się spalinami w zamkniętym garażu.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, чтобы не <отравиться> выхлопными газами в закрытом гараже."
                            }
                        }
                    ],
                    "synonyms": [
                        "struć"
                    ],
                    "common_phrases": [
                        "zatruć się grzybami",
                        "zatruć się alkoholem",
                        "zatruć się czadem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызвать у себя симптомы болезни из-за употребления чего-то вредного, несвежего или ядовитого (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отравиться",
                                "target_lang_sense_clarification": "пострадать от яда или плохой пищи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ce269b3-3cf2-3f6d-85a5-396a1e3a5ee3",
                    "sense_definition": "Sprawić, że coś staje się szkodliwe, zepsute lub nieprzyjemne (często w odniesieniu do środowiska lub w sensie przenośnym o relacjach międzyludzkich).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "pollute_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wylewanie chemikaliów do rzeki może trwale <zatruć> środowisko naturalne.",
                            "target_lang_translations": {
                                "ru": "Сброс химикатов в реку может навсегда <отравить> окружающую среду."
                            }
                        },
                        {
                            "text": "Jego ciągłe narzekanie potrafi skutecznie <zatruć> atmosferę w pracy.",
                            "target_lang_translations": {
                                "ru": "Его постоянное нытье может эффективно <отравить> атмосферу на работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "skazić",
                        "zanieczyścić"
                    ],
                    "antonyms": [
                        "oczyścić"
                    ],
                    "common_phrases": [
                        "zatruć życie",
                        "zatruć atmosferę",
                        "zatruć środowisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать что-то вредным, испорченным или неприятным (часто в отношении окружающей среды или в переносном смысле о человеческих отношениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отравить",
                                "target_lang_sense_clarification": "испортить (жизнь, атмосферу)"
                            },
                            {
                                "target_lang_word": "загрязнить",
                                "target_lang_sense_clarification": "окружающую среду"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bb97efdc-c83e-3e2e-bf0e-cc0a33992298",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <zatrucie>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W okresie letnim szpitale notują wzrost liczby <zatruć> pokarmowych.",
                            "target_lang_translations": {
                                "ru": "В летний период больницы фиксируют рост числа пищевых <отравлений>."
                            }
                        },
                        {
                            "text": "Większości przypadkowych <zatruć> można uniknąć dzięki ostrożności.",
                            "target_lang_translations": {
                                "ru": "Большинства случайных <отравлений> можно избежать благодаря осторожности."
                            }
                        }
                    ],
                    "traits": [

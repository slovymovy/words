    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "737e98af-4b50-341e-8214-15b93cdaaa45",
                    "sense_definition": "Выясниться, стать известным как обладающий определёнными свойствами, качествами или статусом.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "turn_out",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот новый фильм <оказался> очень интересным и смешным."
                        },
                        {
                            "text": "В итоге мой друг <оказался> прав в этом споре."
                        }
                    ],
                    "synonyms": [
                        "обнаружиться"
                    ],
                    "common_phrases": [
                        "оказаться правым",
                        "оказаться полезным",
                        "оказаться сложнее",
                        "оказаться правдой"
                    ]
                },
                {
                    "sense_id": "a04c18bf-ff2f-33b4-9576-2869a43eed4f",
                    "sense_definition": "Неожиданно попасть в какое-либо место или ситуацию; очутиться.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "end_up",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы случайно <оказались> в центре незнакомого города."
                        },
                        {
                            "text": "Я не хочу снова <оказаться> в такой сложной ситуации."
                        }
                    ],
                    "synonyms": [
                        "очутиться"
                    ],
                    "common_phrases": [
                        "оказаться дома",
                        "оказаться в ловушке",
                        "оказаться на улице",
                        "оказаться рядом"
                    ]
                },
                {
                    "sense_id": "1ac78120-4dee-3dd1-aa75-f1aa504399ea",
                    "sense_definition": "Быть в наличии, найтись где-либо (часто при поиске или необходимости).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "be_found",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В кармане куртки не <оказалось> ключей от квартиры."
                        },
                        {
                            "text": "У меня с собой случайно <оказалась> интересная книга."
                        }
                    ],
                    "synonyms": [
                        "найтись"
                    ],
                    "common_phrases": [
                        "оказаться под рукой",
                        "не оказалось денег",
                        "оказаться на месте"
                    ]
                },
                {
                    "sense_id": "49163c48-034a-35d6-b915-2a4e3f0259c6",
                    "sense_definition": "Стать заметным, проявиться (обычно о способностях или талантах).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "manifest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его музыкальный талант <оказался> ещё в раннем детстве."
                        },
                        {
                            "text": "В её характере <оказалась> неожиданная для всех твёрдость."
                        }
                    ],
                    "synonyms": [
                        "проявиться"
                    ],

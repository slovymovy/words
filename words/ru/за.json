{
    "word_family": [
        "задний",
        "сзади",
        "позади"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "e8802ec2-071c-3970-ab31-13064084f3fb",
                    "sense_definition": "Указывает на местонахождение позади кого-либо или чего-либо, а также на перемещение в это место.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_behind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солнце медленно садилось <w>за</w> горизонт.",
                            "target_lang_translations": {
                                "en": "The sun was slowly setting <w>behind</w> the horizon."
                            }
                        },
                        {
                            "text": "Мы решили спрятаться <w>за</w> большим деревом.",
                            "target_lang_translations": {
                                "en": "We decided to hide <w>behind</w> a large tree."
                            }
                        }
                    ],
                    "synonyms": [
                        "позади"
                    ],
                    "antonyms": [
                        "перед"
                    ],
                    "common_phrases": [
                        "стоять за дверью",
                        "спрятаться за угол",
                        "за спиной"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates a location behind someone or something, or movement to that place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "behind",
                                "target_lang_sense_clarification": "spatial position"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae7922aa-3ebd-3d3f-b5ff-0c86e64a5f41",
                    "sense_definition": "Выражает поддержку, согласие или действие в пользу кого-либо или чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вся наша семья проголосовала <w>за</w> этого кандидата.",
                            "target_lang_translations": {
                                "en": "Our whole family voted <w>for</w> this candidate."
                            }
                        },
                        {
                            "text": "Мы готовы бороться <w>за</w> свои права до конца.",
                            "target_lang_translations": {
                                "en": "We are ready to fight <w>for</w> our rights to the end."
                            }
                        }
                    ],
                    "synonyms": [
                        "в поддержку",
                        "в пользу",
                        "ради"
                    ],
                    "antonyms": [
                        "против"
                    ],
                    "common_phrases": [
                        "голосовать за",
                        "я только за",
                        "бороться за мир"
                    ],
                    "target_lang_definitions": {
                        "en": "Expresses support, agreement, or action in favor of someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for",
                                "target_lang_sense_clarification": "in support of"
                            },
                            {
                                "target_lang_word": "in favor of",
                                "target_lang_sense_clarification": "supporting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14c02259-df9c-3169-b35f-d55a084d0697",
                    "sense_definition": "Указывает на цену, возмездность или обмен одного предмета на другой, а также на причину благодарности.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "exchange",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я купил эту красивую куртку <w>за</w> пять тысяч рублей.",
                            "target_lang_translations": {
                                "en": "I bought this beautiful jacket <w>for</w> five thousand rubles."
                            }
                        },
                        {
                            "text": "Большое спасибо вам <w>за</w> помощь и поддержку.",
                            "target_lang_translations": {
                                "en": "Thank you very much <w>for</w> your help and support."
                            }
                        }
                    ],
                    "synonyms": [
                        "в обмен на"
                    ],
                    "common_phrases": [
                        "спасибо за всё",
                        "купить за деньги",
                        "глаз за глаз"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates price, exchange of one item for another, or reason for gratitude."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for",
                                "target_lang_sense_clarification": "exchange or reason"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35389e52-f37a-3735-b8ea-c5ef1dc1c651",
                    "sense_definition": "Указывает на нахождение рядом с предметом для совершения действия (еды, работы) или занятие чем-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вся большая семья собралась вечером <w>за</w> ужином.",
                            "target_lang_translations": {
                                "en": "The whole big family gathered in the evening <w>at</w> dinner."
                            }
                        },
                        {
                            "text": "Студент провёл весь день <w>за</w> учебниками.",
                            "target_lang_translations": {
                                "en": "The student spent the whole day <w>over</w> textbooks."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сидеть за столом",
                        "быть за рулём",
                        "сесть за работу"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates being near an object to perform an action (eating, working) or being occupied with something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "location for activity"
                            },
                            {
                                "target_lang_word": "over",
                                "target_lang_sense_clarification": "occupied with"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0d433d4-bedd-3dd1-8cfb-87923d003d89",
                    "sense_definition": "Указывает на промежуток времени, в течение которого полностью совершается какое-либо действие.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Художник нарисовал этот портрет всего <w>за</w> один час.",
                            "target_lang_translations": {
                                "en": "The artist painted this portrait <w>in</w> just one hour."
                            }
                        },
                        {
                            "text": "Мы успели добраться до вокзала <w>за</w> двадцать минут.",
                            "target_lang_translations": {
                                "en": "We managed to get to the station <w>in</w> twenty minutes."
                            }
                        }
                    ],
                    "synonyms": [
                        "в течение"
                    ],
                    "common_phrases": [
                        "сделать за час",
                        "выучить за ночь",
                        "за короткое время"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates a period of time during which an action is fully completed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in",
                                "target_lang_sense_clarification": "duration to complete"
                            },
                            {
                                "target_lang_word": "within",
                                "target_lang_sense_clarification": "inside a time limit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b7e7d4d-2dcb-3cc2-957a-8df6b825e4ae",
                    "sense_definition": "Указывает на цель движения при желании достать, принести что-либо или позвать кого-либо.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "goal_fetching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама попросила меня сходить в магазин <w>за</w> хлебом.",
                            "target_lang_translations": {
                                "en": "Mom asked me to go to the store <w>for</w> bread."
                            }
                        },
                        {
                            "text": "Вам нужно срочно послать <w>за</w> доктором.",
                            "target_lang_translations": {
                                "en": "You need to send <w>for</w> a doctor urgently."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пойти за хлебом",
                        "послать за врачом",
                        "вернуться за вещами"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the goal of movement when fetching something or calling someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for",
                                "target_lang_sense_clarification": "purpose of movement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6975c4c-8972-35d2-a274-ce2709fc44c2",
                    "sense_definition": "Указывает на часть предмета или тела, за которую берутся, хватаются или держат.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "grasping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец крепко держал маленького сына <w>за</w> руку.",
                            "target_lang_translations": {
                                "en": "The father held his little son tightly <w>by</w> the hand."
                            }
                        },
                        {
                            "text": "Он схватился <w>за</w> голову от удивления.",
                            "target_lang_translations": {
                                "en": "He clutched his head <w>in</w> surprise."
                            }
                        }
                    ],
                    "common_phrases": [
                        "держать за руку",
                        "взять за ручку",
                        "тянуть за верёвку"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates a part of an object or body that is taken, grabbed, or held."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "part held"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7e0da72-011d-34da-9012-adf1e59827fc",
                    "sense_definition": "Указывает на отрезок времени, отделяющий одно событие от другого, предшествующего ему.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_before",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы пришли в театр <w>за</w> десять минут до начала спектакля.",
                            "target_lang_translations": {
                                "en": "We arrived at the theater ten minutes <w>before</w> the start of the play."
                            }
                        },
                        {
                            "text": "Он позвонил мне <w>за</w> день до своего отъезда.",
                            "target_lang_translations": {
                                "en": "He called me a day <w>before</w> his departure."
                            }
                        }
                    ],
                    "common_phrases": [
                        "за час до",
                        "за день до",
                        "за минуту до"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates a period of time separating one event from another preceding it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "before",
                                "target_lang_sense_clarification": "time prior to event"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5234d41c-e8c1-3f85-973c-7dd8cc7801b0",
                    "sense_definition": "Указывает на нахождение вне пределов чего-либо или по ту сторону чего-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_beyond",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом мы предпочитаем жить <w>за</w> городом на даче.",
                            "target_lang_translations": {
                                "en": "In summer we prefer to live <w>out of</w> town at the dacha."
                            }
                        },
                        {
                            "text": "Эта деревня находится <w>за</w> рекой.",
                            "target_lang_translations": {
                                "en": "This village is located <w>beyond</w> the river."
                            }
                        }
                    ],
                    "synonyms": [
                        "за пределами",
                        "вне"
                    ],
                    "antonyms": [
                        "внутри"
                    ],
                    "common_phrases": [
                        "ехать за город",
                        "жить за городом",
                        "выйти за дверь"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates being outside the limits of something or on the other side of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beyond",
                                "target_lang_sense_clarification": "further away than"
                            },
                            {
                                "target_lang_word": "outside",
                                "target_lang_sense_clarification": "external to"
                            },
                            {
                                "target_lang_word": "out of",
                                "target_lang_sense_clarification": "outside limits"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ce4aae2-c578-3e3f-a07c-2093204d9146",
                    "sense_definition": "Указывает на причину переживания, ответственности или лицо, вызывающее беспокойство.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "responsibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители всегда очень волнуются <w>за</w> своих детей.",
                            "target_lang_translations": {
                                "en": "Parents always worry a lot <w>about</w> their children."
                            }
                        },
                        {
                            "text": "Кто будет отвечать <w>за</w> организацию праздника?",
                            "target_lang_translations": {
                                "en": "Who will be responsible <w>for</w> organizing the party?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "бояться за кого-то",
                        "отвечать за поступки",
                        "радоваться за друга"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the cause of worry, responsibility, or the person causing concern."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for",
                                "target_lang_sense_clarification": "responsibility"
                            },
                            {
                                "target_lang_word": "about",
                                "target_lang_sense_clarification": "concern/worry"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4da03df-c588-3f34-a97d-8fa654e60887",
                    "sense_definition": "Вместо кого-либо, в качестве замены другого лица или предмета.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "substitution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сегодня мне пришлось работать <w>за</w> заболевшего коллегу.",
                            "target_lang_translations": {
                                "en": "Today I had to work <w>for</w> a sick colleague."
                            }
                        },
                        {
                            "text": "Он заплатил в ресторане <w>за</w> всех присутствующих.",
                            "target_lang_translations": {
                                "en": "He paid in the restaurant <w>for</w> everyone present."
                            }
                        }
                    ],
                    "synonyms": [
                        "вместо"
                    ],
                    "common_phrases": [
                        "работать за двоих",
                        "платить за всех",
                        "принять за другого"
                    ],
                    "target_lang_definitions": {
                        "en": "Instead of someone, as a replacement for another person or object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for",
                                "target_lang_sense_clarification": "on behalf of"
                            },
                            {
                                "target_lang_word": "in place of",
                                "target_lang_sense_clarification": "substitution"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f5af4e1-1a3e-3c68-a26f-f2812a73ae56",
                    "sense_definition": "По причине, вследствие чего-либо (обычно при указании на препятствие или формальное основание).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дело было закрыто <w>за</w> отсутствием состава преступления.",
                            "target_lang_translations": {
                                "en": "The case was closed <w>for</w> lack of evidence."
                            }
                        },
                        {
                            "text": "Мы не пошли гулять <w>за</w> неимением свободного времени.",
                            "target_lang_translations": {
                                "en": "We didn't go for a walk <w>for</w> lack of free time."
                            }
                        }
                    ],
                    "synonyms": [
                        "из-за",
                        "по причине",
                        "вследствие"
                    ],
                    "common_phrases": [
                        "за неимением",
                        "за отсутствием",
                        "за ненадобностью"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Due to, as a consequence of something (usually indicating an obstacle or formal ground)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for",
                                "target_lang_sense_clarification": "reason/cause"
                            },
                            {
                                "target_lang_word": "due to",
                                "target_lang_sense_clarification": "formal cause"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acaa903c-9f3c-3aee-9574-0ac85923112a",
                    "sense_definition": "Относительно, касательно кого-либо или чего-либо (соответствует предлогам «о», «про»).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "topic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы сидели на кухне и говорили <w>за</w> жизнь.",
                            "target_lang_translations": {
                                "en": "We sat in the kitchen and talked <w>about</w> life."
                            }
                        },
                        {
                            "text": "Что вы можете сказать <w>за</w> этого человека?",
                            "target_lang_translations": {
                                "en": "What can you say <w>about</w> this person?"
                            }
                        }
                    ],
                    "synonyms": [
                        "о",
                        "про"
                    ],
                    "common_phrases": [
                        "говорить за жизнь",
                        "пояснить за",
                        "знать за"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "regional",
                            "comment": "South Russian, Odessa"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Regarding or concerning someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "about",
                                "target_lang_sense_clarification": "topic"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комната",
                                "target_lang_sense_clarification": "виртуальная, чат"
                            },
                            {
                                "target_lang_word": "чат",
                                "target_lang_sense_clarification": "место для общения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd08f56c-27f0-3ba4-b8c9-448d27afae49",
                    "sense_definition": "The opportunity or possibility to do something or for something to happen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager said there is always <w>room</w> for improvement in our sales figures.",
                            "target_lang_translations": {
                                "ru": "Менеджер сказал, что в наших показателях продаж всегда есть <w>возможность</w> для улучшения."
                            }
                        },
                        {
                            "text": "The clear evidence left no <w>room</w> for doubt regarding the suspect's guilt.",
                            "target_lang_translations": {
                                "ru": "Ясные улики не оставили <w>места</w> для сомнений в виновности подозреваемого."
                            }
                        }
                    ],
                    "common_phrases": [
                        "room for improvement",
                        "room for doubt",
                        "room for error",
                        "room to maneuver"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возможность или предпосылка для того, чтобы что-то сделать или чтобы что-то произошло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возможность",
                                "target_lang_sense_clarification": "шанс, перспектива"
                            },
                            {
                                "target_lang_word": "место",
                                "target_lang_sense_clarification": "основание для чего-либо"

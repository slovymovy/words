    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bae6f27d-7313-373a-9591-4a4bb8b4ea7e",
                    "sense_definition": "Поддерживать общение или находить общий язык с кем-либо.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он очень замкнутый человек и ни с кем не <w>контачит</w>.",
                            "target_lang_translations": {
                                "en": "He is a very withdrawn person and doesn't <w>interact</w> with anyone."
                            }
                        },
                        {
                            "text": "Мы с коллегами нормально <w>контачим</w> по рабочим вопросам.",
                            "target_lang_translations": {
                                "en": "My colleagues and I <w>get along</w> fine regarding work issues."
                            }
                        }
                    ],
                    "synonyms": [
                        "общаться",
                        "ладить",
                        "взаимодействовать"
                    ],
                    "antonyms": [
                        "игнорировать",
                        "враждовать"
                    ],
                    "common_phrases": [
                        "контачить с людьми",
                        "перестать контачить"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech instead of 'контактировать' or 'общаться'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To maintain communication or find common ground with someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get along",
                                "target_lang_sense_clarification": "to have a harmonious relationship"
                            },
                            {
                                "target_lang_word": "interact",
                                "target_lang_sense_clarification": "to communicate or engage with others"
                            },
                            {
                                "target_lang_word": "connect",
                                "target_lang_sense_clarification": "to establish a rapport or communication"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd5f31ca-0503-398d-a62c-b8e0c20e277d",
                    "sense_definition": "Иметь или обеспечивать электрическое соединение (о проводах, деталях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лампочка постоянно мигает, потому что патрон плохо <w>контачит</w>.",
                            "target_lang_translations": {
                                "en": "The light bulb keeps blinking because the socket <w>makes poor contact</w>."
                            }
                        },
                        {
                            "text": "В старом радиоприёмнике что-то не <w>контачит</w>, и звук пропадает.",
                            "target_lang_translations": {
                                "en": "Something in the old radio isn't <w>making contact</w>, and the sound cuts out."
                            }
                        }
                    ],
                    "synonyms": [
                        "соединяться",
                        "соприкасаться"
                    ],
                    "antonyms": [
                        "отходить"
                    ],
                    "common_phrases": [
                        "плохо контачит",
                        "где-то не контачит"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in informal technical contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To have or ensure an electrical connection (regarding wires or parts)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make contact",
                                "target_lang_sense_clarification": "to form an electrical connection"
                            },
                            {
                                "target_lang_word": "connect",
                                "target_lang_sense_clarification": "to join or touch electrically"
                            }
                        ]
                    }
                }
            ]
        }
    ]

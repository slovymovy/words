    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "4f65bed9-0b31-3a60-9bc3-58652064c4e6",
                    "sense_definition": "Used to express weak amusement, mild laughter, cynicism, or sometimes boredom.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "laughter_sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That was actually a pretty good joke, <w>heh</w>.",
                            "target_lang_translations": {
                                "ru": "Это была на самом деле неплохая шутка, <w>хе</w>."
                            }
                        },
                        {
                            "text": "<w>Heh</w>, I guess I forgot to bring my wallet again.",
                            "target_lang_translations": {
                                "ru": "<w>Хе</w>, похоже, я снова забыл взять кошелек."
                            }
                        }
                    ],
                    "synonyms": [
                        "chuckle",
                        "snicker"
                    ],
                    "common_phrases": [
                        "heh heh"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech and writing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения легкого веселья, усмешки, цинизма или иногда скуки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хе",
                                "target_lang_sense_clarification": "звук усмешки или легкого смеха"
                            },
                            {
                                "target_lang_word": "хе-хе",
                                "target_lang_sense_clarification": "короткий смешок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0821c79-5e4d-3cfc-9bd9-ba53aa3a6c1c",
                    "sense_definition": "An alternative form of <w>eh</w>, used as a tag question to invite agreement or confirmation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tag_questions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You didn't expect to see me here so soon, <w>heh</w>?",
                            "target_lang_translations": {
                                "ru": "Ты не ожидал увидеть меня здесь так скоро, <w>а</w>?"
                            }
                        },
                        {
                            "text": "It is quite a cold day for a swim, <w>heh</w>?",
                            "target_lang_translations": {
                                "ru": "Довольно холодный день для купания, <w>да</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "eh",
                        "huh",
                        "right"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "alternative form of eh"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорная частица в конце фразы, используемая для получения подтверждения или согласия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "вопросительная частица"
                            },
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "запрос подтверждения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "623533db-f729-3e3f-944e-37cfb8fdeaeb",
                    "sense_definition": "The fifth letter of many Semitic alphabets, such as Hebrew and Arabic.",

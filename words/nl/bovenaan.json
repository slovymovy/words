                    "sense_id": "b543d3b9-8627-32b9-aea1-f076e4ae1e97",
                    "sense_definition": "Op de hoogste plaats of positie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De belangrijkste informatie staat meestal <w>bovenaan</w>.",
                            "target_lang_translations": {

                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer draagt ze graag een luchtig <w>topje</w>.",
                            "target_lang_translations": {
                                "ru": "Летом она любит носить легкий <w>топ</w>."
                            }
                        },
                        {
                            "text": "Ze heeft een nieuw rood <w>topje</w> gekocht voor het feest.",
                            "target_lang_translations": {
                                "ru": "Она купила новый красный <w>топ</w> для вечеринки."
                            }
                        }
                    ],
                    "synonyms": [
                        "hemdje",
                        "truitje"
                    ],
                    "common_phrases": [

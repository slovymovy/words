{
    "word_family": [
        "против",
        "противный",
        "противоположный",
        "противиться",
        "противник",
        "сопротивление"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "9ed677fd-60e4-36b5-a1a0-801e66ac4392",
                    "sense_definition": "На другой, противоположной стороне от кого-либо или чего-либо; лицом к кому-либо или чему-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Аптека находится прямо <напротив> моего дома.",
                            "target_lang_translations": {
                                "en": "The pharmacy is located right <opposite> my house."
                            }
                        },
                        {
                            "text": "Мы сели за столик <напротив> друг друга.",
                            "target_lang_translations": {
                                "en": "We sat at a table <opposite> each other."
                            }
                        }
                    ],
                    "synonyms": [
                        "насупротив",
                        "против"
                    ],
                    "antonyms": [
                        "сообразно",
                        "бок о бок"
                    ],
                    "common_phrases": [
                        "прямо напротив",
                        "сидеть напротив",
                        "жить напротив"
                    ],
                    "target_lang_definitions": {
                        "en": "On the other, opposite side of someone or something; facing someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opposite",
                                "target_lang_sense_clarification": "facing something or someone"
                            },
                            {
                                "target_lang_word": "across from",
                                "target_lang_sense_clarification": "on the other side of"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8824efaf-0d50-324a-986c-940fea66c532",
                    "sense_definition": "На противоположной стороне (улицы, помещения и т. п.).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я живу в высоком доме <напротив>.",
                            "target_lang_translations": {
                                "en": "I live in the tall building <across the way>."
                            }
                        },
                        {
                            "text": "Окна нашей спальни выходят на стену <напротив>.",
                            "target_lang_translations": {
                                "en": "Our bedroom windows face the wall <opposite>."
                            }
                        }
                    ],
                    "synonyms": [
                        "насупротив"
                    ],
                    "common_phrases": [
                        "дом напротив",
                        "сосед напротив"
                    ],
                    "target_lang_definitions": {
                        "en": "On the opposite side (of the street, room, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opposite",
                                "target_lang_sense_clarification": "located across"
                            },
                            {
                                "target_lang_word": "across the way",
                                "target_lang_sense_clarification": "on the other side of the street or area"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "808d49ed-c7d6-3a49-a196-1bc3e792beb4",
                    "sense_definition": "Наоборот; слово, выражающее противопоставление сказанному ранее.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "logical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он не рассердился на шутку, а, <напротив>, громко рассмеялся.",
                            "target_lang_translations": {
                                "en": "He didn't get angry at the joke, but, <on the contrary>, laughed loudly."
                            }
                        },
                        {
                            "text": "Погода не улучшилась, <напротив>, пошёл сильный дождь.",
                            "target_lang_translations": {
                                "en": "The weather didn't improve; <on the contrary>, it started raining heavily."
                            }
                        }
                    ],
                    "synonyms": [
                        "наоборот",
                        "с друго́й стороны́"
                    ],
                    "common_phrases": [
                        "как раз напротив",
                        "совсем напротив",
                        "даже напротив"
                    ],
                    "target_lang_definitions": {
                        "en": "Conversely; a word expressing opposition to what was said earlier."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on the contrary",
                                "target_lang_sense_clarification": "indicating the opposite is true"
                            },
                            {
                                "target_lang_word": "conversely",
                                "target_lang_sense_clarification": "introducing a reversed statement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75e126e3-a540-34f8-9d14-1d492f4467aa",
                    "sense_definition": "Вовсе нет; используется как ответ на вопрос или утверждение для вежливого отрицания или опровержения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "logical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Я вам не мешаю? — <Напротив>, я очень рад вас видеть.",
                            "target_lang_translations": {
                                "en": "— Am I disturbing you? — <Not at all>, I am very glad to see you."
                            }
                        },
                        {
                            "text": "Это вовсе не скучно, <напротив>, очень интересно.",
                            "target_lang_translations": {
                                "en": "It's not boring at all, <on the contrary>, it's very interesting."
                            }
                        }
                    ],
                    "synonyms": [
                        "наоборот"
                    ],
                    "target_lang_definitions": {
                        "en": "Not at all; used as a response to a question or statement for polite denial or refutation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "not at all",
                                "target_lang_sense_clarification": "polite denial"
                            },
                            {
                                "target_lang_word": "on the contrary",
                                "target_lang_sense_clarification": "refuting a negative assumption"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ef9fc9a-5616-3772-b963-80eb6d94babd",
                    "sense_definition": "Наперекор, назло; несогласно с кем-либо или чем-либо.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Что бы я ни сказал, он всё делает мне <напротив>.",
                            "target_lang_translations": {
                                "en": "Whatever I say, he does everything <to spite> me."
                            }
                        },
                        {
                            "text": "Он специально говорил <напротив>, чтобы позлить собеседника.",
                            "target_lang_translations": {
                                "en": "He spoke <contrarily> on purpose to annoy the interlocutor."
                            }
                        }
                    ],
                    "synonyms": [
                        "напереко́р",
                        "назло́",
                        "противоре́ча",
                        "ина́че"
                    ],
                    "antonyms": [
                        "сообразно"
                    ],
                    "common_phrases": [
                        "делать напротив",
                        "говорить напротив"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe acting spitefully or contrary to expectations"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Defiantly, out of spite; in disagreement with someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to spite",
                                "target_lang_sense_clarification": "doing something to annoy or defy"
                            },
                            {
                                "target_lang_word": "contrarily",
                                "target_lang_sense_clarification": "acting against expectations"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
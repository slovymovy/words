    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "34b16b21-c849-3eaa-96a3-313e4fbef195",
                    "sense_definition": "Переместить кого-либо или что-либо изнутри наружу, неся в руках или на себе.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, помоги мне <вынести> этот тяжёлый стол в сад.",
                            "target_lang_translations": {
                                "en": "Please help me <carry out> this heavy table into the garden."
                            }
                        },
                        {
                            "text": "Студенты должны <вынести> всё лишнее из аудитории перед началом экзамена.",
                            "target_lang_translations": {
                                "en": "Students must <take out> everything unnecessary from the classroom before the exam starts."
                            }
                        }
                    ],
                    "antonyms": [
                        "сохранить",
                        "сохранить"
                    ],
                    "common_phrases": [
                        "вынести мусор",
                        "вынести вещи",
                        "вынести из дома"
                    ],
                    "target_lang_definitions": {
                        "en": "To move someone or something from inside to outside by carrying them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carry out",
                                "target_lang_sense_clarification": "physically moving something out"
                            },
                            {
                                "target_lang_word": "take out",
                                "target_lang_sense_clarification": "general removal from a place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93e0a707-1169-3e17-b8a1-dbcf70b0ff0a",
                    "sense_definition": "Вытерпеть, пережить что-либо тяжелое, неприятное или трудное (испытания, боль, разлуку).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "endurance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он не смог <вынести> такой сильной боли и потерял сознание.",
                            "target_lang_translations": {
                                "en": "He could not <bear> such severe pain and lost consciousness."
                            }
                        },
                        {
                            "text": "Наш народ сумел <вынести> все тяготы войны и победить.",
                            "target_lang_translations": {
                                "en": "Our people managed to <endure> all the hardships of war and win."
                            }
                        }
                    ],
                    "synonyms": [
                        "вытерпеть",
                        "пережить"
                    ],
                    "common_phrases": [
                        "вынести испытания",
                        "не вынести боли",
                        "вынести разлуку"
                    ],
                    "target_lang_definitions": {
                        "en": "To tolerate, survive, or withstand something difficult, painful, or unpleasant."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "endure",
                                "target_lang_sense_clarification": "suffering through hardships or trials"
                            },
                            {
                                "target_lang_word": "bear",
                                "target_lang_sense_clarification": "tolerating pain or heavy burdens"
                            },
                            {
                                "target_lang_word": "stand",
                                "target_lang_sense_clarification": "tolerating something negative"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d1b96e7-1c2e-3a1c-bb1d-e699368813ed",
                    "sense_definition": "Официально объявить, постановить (о решении, приговоре, вердикте).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Судья должен <вынести> справедливый приговор по этому сложному делу.",
                            "target_lang_translations": {
                                "en": "The judge must <pass> a just sentence in this complex case."
                            }
                        },
                        {
                            "text": "Комиссия обещала <вынести> окончательное решение завтра утром.",
                            "target_lang_translations": {
                                "en": "The commission promised to <render> a final decision tomorrow morning."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вынести приговор",
                        "вынести решение",
                        "вынести вердикт"

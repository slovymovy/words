    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0771de4-2476-3ab3-aa40-52d48943ca9a",
                    "sense_definition": "Een verzoek om informatie, uitleg of een antwoord.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij stelde zijn leerkracht een <w>vraag</w>.",
                            "target_lang_translations": {
                                "en": "He asked his teacher a <w>question</w>.",
                                "ru": "Он задал своему учителю <w>вопрос</w>."
                            }
                        },
                        {
                            "text": "Heb je nog een <w>vraag</w> over het huiswerk?",
                            "target_lang_translations": {
                                "en": "Do you have another <w>question</w> about the homework?",
                                "ru": "У тебя есть ещё <w>вопрос</w> по домашнему заданию?"
                            }
                        }
                    ],
                    "antonyms": [
                        "antwoord"
                    ],
                    "common_phrases": [
                        "een vraag stellen",
                        "een vraag beantwoorden"
                    ],
                    "target_lang_definitions": {
                        "en": "A request for information, explanation, or an answer.",
                        "ru": "Запрос информации, объяснения или ответа."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "question",
                                "target_lang_sense_clarification": "inquiry for information"
                            },
                            {
                                "target_lang_word": "inquiry",
                                "target_lang_sense_clarification": "formal request for information"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вопрос",
                                "target_lang_sense_clarification": "запрос информации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0148b301-3391-372c-9d29-6010f24e3417",
                    "sense_definition": "Een probleem, kwestie of onderwerp dat bespreking of een oplossing vereist.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is nog maar de <w>vraag</w> of het plan gaat werken.",
                            "target_lang_translations": {
                                "en": "It is still the <w>question</w> whether the plan will work.",
                                "ru": "Ещё <w>вопрос</w>, сработает ли план."
                            }
                        },
                        {
                            "text": "Dit is een belangrijke politieke <w>vraag</w> die we moeten oplossen.",
                            "target_lang_translations": {
                                "en": "This is an important political <w>question</w> that we need to solve.",
                                "ru": "Это важный политический <w>вопрос</w>, который мы должны решить."
                            }
                        }
                    ],
                    "synonyms": [
                        "kwestie",
                        "vraagstuk",
                        "zaak"
                    ],
                    "common_phrases": [
                        "de vraag is",
                        "buiten kijf"
                    ],
                    "target_lang_definitions": {
                        "en": "A problem, issue, or subject requiring discussion or a solution.",
                        "ru": "Проблема, дело или тема, требующие обсуждения или решения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "question",
                                "target_lang_sense_clarification": "matter or issue to be discussed"
                            },
                            {
                                "target_lang_word": "issue",
                                "target_lang_sense_clarification": "topic of debate"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вопрос",
                                "target_lang_sense_clarification": "проблема, дело"
                            },
                            {
                                "target_lang_word": "проблема",
                                "target_lang_sense_clarification": "сложная ситуация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "487e3f2e-704c-365e-be82-bdfde4cad314",
                    "sense_definition": "De behoefte aan goederen of diensten in de economie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In Nederland is er veel <w>vraag</w> naar brandstof.",
                            "target_lang_translations": {
                                "en": "In the Netherlands, there is a high <w>demand</w> for fuel.",
                                "ru": "В Нидерландах большой <w>спрос</w> на топливо."
                            }
                        },
                        {
                            "text": "Als de <w>vraag</w> stijgt, gaat de prijs vaak omhoog.",
                            "target_lang_translations": {
                                "en": "When <w>demand</w> rises, the price often goes up.",
                                "ru": "Когда <w>спрос</w> растёт, цена часто поднимается."
                            }
                        }
                    ],
                    "antonyms": [
                        "aanbod"
                    ],
                    "common_phrases": [
                        "vraag en aanbod",
                        "vraag naar"
                    ],
                    "target_lang_definitions": {
                        "en": "The need for goods or services in the economy.",
                        "ru": "Потребность в товарах или услугах в экономике."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demand",
                                "target_lang_sense_clarification": "economic desire for goods"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "спрос",
                                "target_lang_sense_clarification": "экономический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2ee8665a-b641-3730-87bd-45833d984d30",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>vragen</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>vraag</w> me af hoe laat de trein vertrekt.",
                            "target_lang_translations": {
                                "en": "I <w>wonder</w> what time the train leaves.",
                                "ru": "Я <w>спрашиваю</w> себя (интересуюсь), во сколько отправляется поезд."
                            }
                        },
                        {
                            "text": "Ik <w>vraag</w> om hulp omdat ik het niet alleen kan.",
                            "target_lang_translations": {
                                "en": "I <w>ask</w> for help because I cannot do it alone.",
                                "ru": "Я <w>прошу</w> о помощи, потому что не могу справиться один."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'vragen' (to ask).",
                        "ru": "Форма 1-го лица единственного числа настоящего времени глагола <w>vragen</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "to request information or action"
                            },
                            {
                                "target_lang_word": "wonder",
                                "target_lang_sense_clarification": "reflexive use (ask oneself)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "спрашиваю",
                                "target_lang_sense_clarification": "задаю вопрос"
                            },
                            {
                                "target_lang_word": "прошу",
                                "target_lang_sense_clarification": "выражаю просьбу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4aa9790-f679-3c65-a9d3-db6bbc36d249",
                    "sense_definition": "Gebiedende wijs van <w>vragen</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Vraag</w> het even aan je vader als je hem ziet.",
                            "target_lang_translations": {
                                "en": "<w>Ask</w> your father for a moment when you see him.",
                                "ru": "<w>Спроси</w> это у папы, когда увидишь его."
                            }
                        },
                        {
                            "text": "<w>Vraag</w> gerust om advies als je twijfelt.",
                            "target_lang_translations": {
                                "en": "Feel free to <w>ask</w> for advice if you are in doubt.",
                                "ru": "Не стесняйся, <w>проси</w> совета, если сомневаешься."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative mood of 'vragen' (to ask).",
                        "ru": "Форма повелительного наклонения глагола <w>vragen</w>."
                    },
                    "translations": {

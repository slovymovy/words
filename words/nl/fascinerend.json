            "senses": [
                {
                    "sense_id": "9fc54d27-f5e5-3f9b-84ec-42707bdb07d8",
                    "sense_definition": "De aandacht sterk trekkend omdat het heel interessant of mooi is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was een <w>fascinerend</w> boek over de geschiedenis van het oude Egypte.",
                            "target_lang_translations": {
                                "ru": "Это была <w>увлекательная</w> книга об истории Древнего Египта.",
                                "en": "It was a <w>fascinating</w> book about the history of ancient Egypt."
                            }
                        },
                        {
                            "text": "Ik vind het <w>fascinerend</w> om te zien hoe snel de technologie zich ontwikkelt.",
                            "target_lang_translations": {
                                "ru": "Я нахожу <w>захватывающим</w> наблюдать, как быстро развиваются технологии.",
                                "en": "I find it <w>fascinating</w> to see how quickly technology develops."
                            }
                        }
                    ],
                    "synonyms": [
                        "boeiend",
                        "interessant",
                        "pakkend",
                        "meeslepend"
                    ],
                    "antonyms": [
                        "saai",
                        "vervelend",
                        "oninteressant"
                    ],
                    "common_phrases": [
                        "een fascinerend verhaal",
                        "fascinerend schouwspel",
                        "buitengewoon fascinerend"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно привлекающий внимание, потому что очень интересен или красив.",
                        "en": "Strongly attracting attention because it is very interesting or beautiful."
                    },
                    "translations": {

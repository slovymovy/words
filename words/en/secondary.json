    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cf1d9ece-4f80-343d-9c32-a65ca697e15d",
                    "sense_definition": "Of less than primary importance or value; minor.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cost of the project is a <w>secondary</w> consideration compared to the safety of the workers.",
                            "target_lang_translations": {
                                "ru": "Стоимость проекта — это <w>второстепенный</w> вопрос по сравнению с безопасностью рабочих."
                            }
                        },
                        {
                            "text": "Winning the game was <w>secondary</w> to having fun with friends.",
                            "target_lang_translations": {
                                "ru": "Победа в игре была <w>второстепенной</w> по сравнению с весельем в кругу друзей."
                            }
                        }
                    ],
                    "synonyms": [
                        "minor",
                        "subordinate",
                        "subsidiary"
                    ],
                    "antonyms": [
                        "idiopathic",
                        "idiopathic"
                    ],
                    "common_phrases": [
                        "secondary importance",
                        "secondary issue",
                        "secondary role"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий меньшую важность или ценность по сравнению с чем-то основным; второстепенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "второстепенный",
                                "target_lang_sense_clarification": "не основной"
                            },
                            {
                                "target_lang_word": "побочный",
                                "target_lang_sense_clarification": "сопутствующий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65ebf22e-fcf8-30f4-9c4d-173a0790545e",
                    "sense_definition": "Related to the education of children between the ages of approximately 11 and 18, following primary education.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She teaches mathematics at a large <w>secondary</w> school in the city.",
                            "target_lang_translations": {
                                "ru": "Она преподает математику в большой <w>средней</w> школе в городе."
                            }
                        },
                        {
                            "text": "Students usually begin their <w>secondary</w> education at the age of eleven.",
                            "target_lang_translations": {
                                "ru": "Ученики обычно начинают <w>среднее</w> образование в возрасте одиннадцати лет."
                            }

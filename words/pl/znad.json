            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "3e7aba6a-ebaa-3730-a99d-db320e258879",
                    "sense_definition": "Z obszaru znajdującego się w pobliżu zbiornika wodnego, takiego jak morze, jezioro lub rzeka.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj moi rodzice wrócili wypoczęci <znad> morza.",
                            "target_lang_translations": {
                                "ru": "Вчера мои родители вернулись отдохнувшими <с> моря."
                            }
                        },
                        {
                            "text": "Przywieźliśmy piękne muszle prosto <znad> oceanu.",
                            "target_lang_translations": {
                                "ru": "Мы привезли красивые ракушки прямо <с> океана."
                            }
                        }
                    ],
                    "common_phrases": [
                        "znad morza",
                        "znad jeziora",
                        "znad rzeki",
                        "wiatr znad zatoki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Со стороны водного пространства (моря, озера, реки); указывает на возвращение или прибытие из местности у воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "предлог, используемый при указании движения со стороны водоема (с моря, с реки)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f6d87c2-3b90-359f-b485-0d58323b4ac0",
                    "sense_definition": "Z miejsca znajdującego się wyżej niż coś innego lub bezpośrednio nad czymś; wskazuje na ruch z góry lub zza przeszkody.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel surowo spojrzał na ucznia <znad> okularów.",
                            "target_lang_translations": {
                                "ru": "Учитель сурово посмотрел на ученика <поверх> очков."
                            }
                        },
                        {
                            "text": "Słońce wreszcie wyjrzało <znad> ciemnych chmur.",
                            "target_lang_translations": {
                                "ru": "Солнце наконец выглянуло <из-за> темных туч."
                            }
                        }
                    ],
                    "synonyms": [
                        "sponad"
                    ],
                    "antonyms": [
                        "spod"
                    ],
                    "common_phrases": [
                        "znad granicy",
                        "znad stołu",
                        "znad chmur",
                        "głos znad grobu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Из пространства, находящегося выше чего-либо, или из-за верхней кромки препятствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поверх",
                                "target_lang_sense_clarification": "указывает на действие, направленное над верхним краем чего-либо (например, очков)"
                            },
                            {
                                "target_lang_word": "из-за",
                                "target_lang_sense_clarification": "указывает на движение из пространства за чем-либо или над чем-либо (например, туч)"
                            },
                            {
                                "target_lang_word": "сверху",
                                "target_lang_sense_clarification": "общее указание на направление с более высокого уровня"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "0605c939-a35c-35e8-8bfd-75bf8ba37c94",
                    "sense_definition": "To wish to apprehend or find someone, typically by the police.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police <w>want</w> him for questioning regarding the theft.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>разыскивает</w> его для допроса по поводу кражи."
                            }
                        },
                        {
                            "text": "He is <w>wanted</w> for robbery in three states.",
                            "target_lang_translations": {
                                "ru": "Он <w>разыскивается</w> за ограбление в трех штатах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wanted for",
                        "wanted by police"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искать кого-либо с целью задержания (обычно полицией)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разыскивать",
                                "target_lang_sense_clarification": "искать (о полиции)"
                            },
                            {
                                "target_lang_word": "искать",
                                "target_lang_sense_clarification": "пытаться найти"
                            }
                        ]
                    }
                },
                {

{
    "word_family": [
        "powód",
        "spowodować"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e17160fc-c7d3-3cc8-b957-2cad18dce480",
                    "sense_definition": "Być przyczyną jakiegoś zdarzenia, stanu rzeczy lub sytuacji; sprawiać, że coś powstaje.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gwałtowna burza może <w>powodować</w> poważne zniszczenia w infrastrukturze miasta.",
                            "target_lang_translations": {
                                "ru": "Сильная буря может <w>причинять</w> серьезные разрушения в инфраструктуре города."
                            }
                        },
                        {
                            "text": "Jego nieodpowiedzialne zachowanie zaczęło <w>powodować</w> konflikty w zespole.",
                            "target_lang_translations": {
                                "ru": "Его безответственное поведение начало <w>создавать</w> конфликты в коллективе."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprawiać",
                        "wywoływać",
                        "rodzić",
                        "prowokować",
                        "wzniecać"
                    ],
                    "antonyms": [
                        "zapobiegać",
                        "przeciwdziałać"
                    ],
                    "common_phrases": [
                        "powodować wypadek",
                        "powodować straty",
                        "powodować ból",
                        "powodować problemy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть причиной какого-либо события, положения вещей или ситуации; приводить к возникновению чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "приводить к появлению чего-либо (реакции, явления)"
                            },
                            {
                                "target_lang_word": "причинять",
                                "target_lang_sense_clarification": "служить причиной чего-то негативного (боли, ущерба)"
                            },
                            {
                                "target_lang_word": "создавать",
                                "target_lang_sense_clarification": "творить, делать существующим (проблемы, ситуации)"
                            },
                            {
                                "target_lang_word": "порождать",
                                "target_lang_sense_clarification": "давать начало чему-либо, быть источником"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e05ea66-4e63-3f38-8460-6b24c20e33fa",
                    "sense_definition": "Być czynnikiem wpływającym na czyjeś decyzje lub zachowanie; kierować kimś (zwykle o uczuciach lub motywach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wiem, co nim <w>powodowało</w>, gdy podejmował tak ryzykowną decyzję.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, что им <w>двигало</w>, когда он принимал такое рискованное решение."
                            }
                        },
                        {
                            "text": "Często <w>powoduje</w> nią chęć pomocy innym, nawet kosztem własnego czasu.",
                            "target_lang_translations": {
                                "ru": "Часто ею <w>движет</w> желание помочь другим, даже в ущерб собственному времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierować",
                        "wpływać"
                    ],
                    "common_phrases": [
                        "co tobą powoduje",
                        "powodować kimś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть фактором, влияющим на чьи-либо решения или поведение; управлять кем-либо (обычно о чувствах или мотивах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "движить",
                                "target_lang_sense_clarification": "побуждать к действию (о чувствах, страстях)"
                            },
                            {
                                "target_lang_word": "руководить",
                                "target_lang_sense_clarification": "направлять чьи-либо действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a61906e-de3c-36d2-b587-eec1501294b6",
                    "sense_definition": "Podejmować decyzje lub działać pod wpływem określonych czynników, uczuć lub zasad (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W życiu zawodowym należy <w>powodować</w> się uczciwością i profesjonalizmem.",
                            "target_lang_translations": {
                                "ru": "В профессиональной жизни следует <w>руководствоваться</w> честностью и профессионализмом."
                            }
                        },
                        {
                            "text": "Kupując ten stary dom, <w>powodował</w> się głównie sentymentem do okolicy.",
                            "target_lang_translations": {
                                "ru": "Покупая этот старый дом, он <w>руководствовался</w> главным образом сентиментальной привязанностью к этому району."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierować się"
                    ],
                    "common_phrases": [
                        "powodować się emocjami",
                        "powodować się rozsądkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать решения или действовать под влиянием определенных факторов, чувств или принципов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "руководствоваться",
                                "target_lang_sense_clarification": "следовать чему-либо как правилу или принципу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09fd785d-f816-3b2f-a799-190c644b326e",
                    "sense_definition": "Kierować ruchem zwierzęcia pociągowego lub pojazdu; prowadzić.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "direct",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony woźnica potrafił sprawnie <w>powodować</w> zaprzęgiem konnym.",
                            "target_lang_translations": {
                                "ru": "Опытный кучер умел ловко <w>править</w> конной упряжкой."
                            }
                        },
                        {
                            "text": "Młody jeździec uczył się, jak <w>powodować</w> koniem za pomocą lejców.",
                            "target_lang_translations": {
                                "ru": "Молодой наездник учился, как <w>управлять</w> лошадью с помощью поводьев."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierować",
                        "prowadzić"
                    ],
                    "common_phrases": [
                        "powodować końmi"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie rzadko używane, częściej: kierować, powozić"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Управлять движением упряжного животного или транспортного средства; править."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "править",
                                "target_lang_sense_clarification": "направлять движение (лошадьми, повозкой)"
                            },
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "руководить ходом, движением"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "173f8b98-a769-3bb5-9450-f3908cdab949",
                    "sense_definition": "Włókno konopne lub lniane, będące odpadem przy obróbce tych roślin, używane głównie do uszczelniania połączeń hydraulicznych oraz jako materiał izolacyjny lub wyściółka.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hydraulik nawinął <w>pakuły</w> na gwint rury, aby zapobiec wyciekowi wody.",
                            "target_lang_translations": {
                                "ru": "Сантехник намотал <w>паклю</w> на резьбу трубы, чтобы предотвратить утечку воды."
                            }
                        },
                        {
                            "text": "Dawniej do ocieplania drewnianych domów powszechnie stosowano <w>pakuły</w> wciśnięte między belki.",
                            "target_lang_translations": {
                                "ru": "Раньше для утепления деревянных домов повсеместно использовали <w>паклю</w>, забитую между бревнами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uszczelniać pakułami",
                        "pakuły lniane",
                        "pakuły konopne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубое конопляное или льняное волокно, являющееся отходом обработки этих растений; используется в основном для уплотнения гидравлических соединений, а также как изоляционный материал или набивка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пакля",
                                "target_lang_sense_clarification": "волокнистый материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff0b776a-4c40-3946-89eb-4023519e4d5f",
                    "sense_definition": "forma słowa <w>pakuła</w> (mianownik, biernik, wołacz liczby mnogiej)",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te dwie <w>pakuły</w> znowu zapomniały przynieść potrzebne dokumenty.",
                            "target_lang_translations": {
                                "ru": "Эти две <w>клуши</w> снова забыли принести нужные документы."
                            }
                        },
                        {
                            "text": "Na podłodze leżały rozrzucone brudne <w>pakuły</w>.",
                            "target_lang_translations": {
                                "ru": "На полу валялась разбросанная грязная <w>пакля</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма слова «pakuła» (именительный, винительный, звательный падежи множественного числа)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пакля",

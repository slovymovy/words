{
    "word_family": [
        "boei",
        "boeiend"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "12556513-1d28-3938-8a34-e0824e1cca6d",
                    "sense_definition": "Iemands aandacht sterk vasthouden of erg interesseren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fascineren",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar wist de kinderen tijdens de les maar niet te <w>boeien</w>.",
                            "target_lang_translations": {
                                "en": "The teacher just couldn't <w>captivate</w> the children during the lesson.",
                                "ru": "Учителю никак не удавалось <w>увлечь</w> детей во время урока."
                            }
                        },
                        {
                            "text": "Het boeit mij niet wat anderen van mijn kleding vinden.",
                            "target_lang_translations": {
                                "en": "It doesn't <w>interest</w> me what others think of my clothes.",
                                "ru": "Меня не <w>волнует</w>, что другие думают о моей одежде."
                            }
                        }
                    ],
                    "synonyms": [
                        "fascineren"
                    ],
                    "common_phrases": [
                        "het boeit niet",
                        "niet kunnen boeien"
                    ],
                    "target_lang_definitions": {
                        "en": "To hold someone's attention strongly or interest them greatly.",
                        "ru": "Сильно удерживать чье-то внимание или очень интересовать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "captivate",
                                "target_lang_sense_clarification": "hold attention"
                            },
                            {
                                "target_lang_word": "fascinate",
                                "target_lang_sense_clarification": "attract strongly"
                            },
                            {
                                "target_lang_word": "interest",
                                "target_lang_sense_clarification": "engage attention"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "интересовать",
                                "target_lang_sense_clarification": "вызывать интерес"
                            },
                            {
                                "target_lang_word": "увлекать",
                                "target_lang_sense_clarification": "захватывать внимание"
                            },
                            {
                                "target_lang_word": "волновать",
                                "target_lang_sense_clarification": "беспокоить (часто в отрицании: мне все равно)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a10f490a-3083-3d9b-a720-8fe1a29b0db9",
                    "sense_definition": "Iemands bewegingsvrijheid beperken door hem vast te binden, vaak met handboeien.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "vastbinden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie wist de verdachte snel te <w>boeien</w> na de achtervolging.",
                            "target_lang_translations": {
                                "en": "The police managed to quickly <w>handcuff</w> the suspect after the chase.",
                                "ru": "Полиции удалось быстро <w>заковать</w> подозреваемого после погони."
                            }
                        },
                        {
                            "text": "De gevangene werd <w>geboeid</w> naar de verhoorkamer gebracht.",
                            "target_lang_translations": {
                                "en": "The prisoner was brought to the interrogation room <w>shackled</w>.",
                                "ru": "Заключенного <w>в наручниках</w> привели в комнату для допросов."
                            }
                        }
                    ],
                    "synonyms": [
                        "kluisteren"
                    ],
                    "common_phrases": [
                        "in de boeien slaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To restrict someone's freedom of movement by tying them up, often with handcuffs.",
                        "ru": "Ограничивать свободу движений, связывая кого-то, часто наручниками."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "handcuff",
                                "target_lang_sense_clarification": "restrain wrists"
                            },
                            {
                                "target_lang_word": "shackle",
                                "target_lang_sense_clarification": "restrain with chains"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заковывать",
                                "target_lang_sense_clarification": "надевать оковы или наручники"
                            },
                            {
                                "target_lang_word": "надевать наручники",
                                "target_lang_sense_clarification": "полицейское задержание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "d0c253c4-ad4d-33e2-9434-e4173d303f48",
                    "sense_definition": "Uitroep van onverschilligheid om aan te geven dat men iets totaal niet interessant vindt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je bent je jas vergeten, maar ja <w>boeien</w>, ik heb het toch niet koud.",
                            "target_lang_translations": {
                                "en": "You forgot your coat, but yeah <w>who cares</w>, I'm not cold anyway.",
                                "ru": "Ты забыл куртку, но да <w>пофиг</w>, мне все равно не холодно."
                            }
                        },
                        {
                            "text": "<w>Boeien</w>, we zien wel wat er morgen gebeurt.",
                            "target_lang_translations": {
                                "en": "<w>Whatever</w>, we'll see what happens tomorrow.",
                                "ru": "<w>Плевать</w>, посмотрим, что будет завтра."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lekker boeien",
                        "ja boeien"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "spreektaal"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "jongerentaal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamation of indifference to indicate that one finds something totally uninteresting.",
                        "ru": "Возглас безразличия, показывающий, что кому-то это совершенно не интересно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "who cares",
                                "target_lang_sense_clarification": "expression of indifference"
                            },
                            {
                                "target_lang_word": "whatever",
                                "target_lang_sense_clarification": "dismissive exclamation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пофиг",
                                "target_lang_sense_clarification": "сленговое выражение безразличия"
                            },
                            {
                                "target_lang_word": "плевать",
                                "target_lang_sense_clarification": "грубое выражение безразличия"
                            },
                            {
                                "target_lang_word": "какая разница",
                                "target_lang_sense_clarification": "риторический вопрос"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "62f93b92-4b2c-38ab-bd99-6dbf4f6e1977",
                    "sense_definition": "Meervoud van <w>boei</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zwemmers mochten in de zee niet voorbij de <w>boeien</w> komen.",
                            "target_lang_translations": {
                                "en": "The swimmers were not allowed to go past the <w>buoys</w> in the sea.",
                                "ru": "Пловцам в море не разрешалось заплывать за <w>буи</w>."
                            }
                        },
                        {
                            "text": "De arrestant werd stevig in de <w>boeien</w> geslagen.",
                            "target_lang_translations": {
                                "en": "The detainee was firmly put in <w>shackles</w>.",
                                "ru": "Арестованного крепко заковали в <w>наручники</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of 'boei' (buoy or shackle).",
                        "ru": "Множественное число от слова \"boei\" (буй или оковы)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "buoys",
                                "target_lang_sense_clarification": "floating markers"
                            },
                            {
                                "target_lang_word": "shackles",
                                "target_lang_sense_clarification": "restraints"
                            },
                            {
                                "target_lang_word": "handcuffs",
                                "target_lang_sense_clarification": "wrist restraints"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "буи",
                                "target_lang_sense_clarification": "плавучие знаки"
                            },
                            {
                                "target_lang_word": "наручники",
                                "target_lang_sense_clarification": "средство ограничения свободы"
                            },
                            {
                                "target_lang_word": "оковы",
                                "target_lang_sense_clarification": "цепи"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
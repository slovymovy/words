            "senses": [
                {
                    "sense_id": "615e33c7-65f6-3154-9faa-1e6d0d69e7e2",
                    "sense_definition": "Initialism of <w>body</w> <w>odor</w>; an unpleasant smell caused by sweat and bacteria on the skin.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "bo_smell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After working out at the gym, he realized he had terrible <w>BO</w>.",
                            "target_lang_translations": {
                                "ru": "После тренировки в спортзале он понял, что от него исходит ужасный <w>запах пота</w>."
                            }
                        },
                        {
                            "text": "Please use deodorant so you do not have <w>BO</w> in the office.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, пользуйся дезодорантом, чтобы от тебя не было <w>запаха пота</w> в офисе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "have bad BO",
                        "strong BO"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common abbreviation in spoken English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неприятный запах тела, вызванный потом и бактериями (сокращение от body odor)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запах пота",
                                "target_lang_sense_clarification": "неприятный запах от тела"
                            },
                            {
                                "target_lang_word": "неприятный запах",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa3e0a14-074a-378f-96b7-c7a807b0f66a",
                    "sense_definition": "Initialism of <w>box</w> <w>office</w>; the place where tickets are sold or the total revenue of a movie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bo_cinema",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new superhero movie was a massive hit at the domestic <w>BO</w>.",

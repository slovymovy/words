                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "comprehension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you repeat what you just said because I did not <w>understand</w>?"
                        },
                        {
                            "text": "In reading this book, be very certain you never go past a word you do not fully <w>understand</w>."
                        }
                    ],
                    "synonyms": [
                        "comprehend",
                        "grasp",
                        "get",
                        "apprehend",
                        "perceive"
                    ],
                    "antonyms": [

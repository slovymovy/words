                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Kap</w> nou eens met dat vervelende lawaai!",
                            "target_lang_translations": {

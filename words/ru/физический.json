            "senses": [
                {
                    "sense_id": "bd4f656e-e7a5-3775-9c9b-9d14377d1cdc",
                    "sense_definition": "Относящийся к физике как науке, основанный на её законах.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "science_physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы проводили сложный <w>физический</w> опыт в школьной лаборатории."
                        },
                        {
                            "text": "Все природные явления подчиняются строгим <w>физическим</w> законам."
                        }
                    ],
                    "common_phrases": [
                        "физический закон",
                        "физическое явление",
                        "физическая лаборатория",
                        "физическая величина"
                    ]
                },
                {
                    "sense_id": "7f689b01-f44d-35f6-b78c-b9e959e5888b",
                    "sense_definition": "Относящийся к телу человека, телесный (в отличие от духовного, психического).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "human_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В школе большое внимание уделяется <w>физическому</w> воспитанию детей."
                        },
                        {
                            "text": "Ему требовался полный покой и отдых для восстановления <w>физического</w> здоровья."
                        }
                    ],
                    "synonyms": [
                        "телесный"
                    ],
                    "antonyms": [
                        "духовный",
                        "умственный",
                        "интеллектуальный"
                    ],
                    "common_phrases": [
                        "физическое воспитание",
                        "физическое здоровье",
                        "физическое развитие",
                        "физическое лицо",
                        "физический недостаток"
                    ]
                },
                {
                    "sense_id": "11122c17-55c7-324a-88e0-2659427a41e5",
                    "sense_definition": "Связанный с деятельностью мышц, требующий применения мускульной силы.",

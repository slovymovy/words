                },
                {
                    "sense_id": "367e9ab7-7754-35ff-a87b-c7be1efab4c5",
                    "sense_definition": "A container or place where waste material is put.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "waste_container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He crumpled the paper and threw it into the <w>garbage</w>.",

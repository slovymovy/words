    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cf812ecd-ff49-34a6-8539-2fd4ae92802e",
                    "sense_definition": "Sytuacja w grze w szachy, w której król jest bezpośrednio zagrożony biciem przez bierkę przeciwnika.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "chess_game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz przesunąć króla, ponieważ przeciwnik dał ci <w>szacha</w>.",
                            "target_lang_translations": {
                                "ru": "Ты должен увести короля, так как противник поставил тебе <w>шах</w>."
                            }
                        },
                        {
                            "text": "To był sprytny ruch, który zakończył się <w>szachem</w> dla białych.",
                            "target_lang_translations": {
                                "ru": "Это был хитрый ход, который закончился <w>шахом</w> белым."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaszachowanie"
                    ],
                    "antonyms": [
                        "mat",
                        "pat"
                    ],
                    "common_phrases": [
                        "dać szacha",
                        "trzymać w szachu",
                        "wieczny szach",
                        "szach z odsłony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация в шахматной партии, при которой король находится под ударом фигуры соперника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шах",
                                "target_lang_sense_clarification": "ситуация в игре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "face6d6c-af13-3aa3-8658-eeeb7c89c3d4",
                    "sense_definition": "Tytuł monarchy używany dawniej w Persji, a następnie w Iranie; także osoba nosząca ten tytuł.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "title",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "Ostatni <w>szach</w> Iranu został obalony w wyniku rewolucji.",
                            "target_lang_translations": {
                                "ru": "Последний <w>шах</w> Ирана был свергнут в результате революции."
                            }
                        },
                        {
                            "text": "Podróżnik został przyjęty na dworze przez samego <w>szacha</w>.",
                            "target_lang_translations": {
                                "ru": "Путешественник был принят при дворе самим <w>шахом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szach perski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул монарха в Персии (Иране) и некоторых других странах Востока, а также лицо, носящее этот титул."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шах",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "94850e16-fc64-3f91-8c1a-6879d2f2dceb",
                    "sense_definition": "Słowo wypowiadane przez gracza w momencie, gdy atakuje on króla przeciwnika.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "chess_game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krzyknął głośno „<w>szach</w>”, stawiając wieżę na otwartej linii.",
                            "target_lang_translations": {
                                "ru": "Он громко крикнул «<w>шах</w>», ставя ладью на открытую линию."
                            }
                        },
                        {
                            "text": "Kiedy usłyszałem „<w>szach</w>”, wiedziałem, że moja sytuacja jest trudna.",
                            "target_lang_translations": {
                                "ru": "Когда я услышал «<w>шах</w>», я понял, что моё положение трудное."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szach i mat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас игрока в шахматы, предупреждающий соперника о нападении на его короля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шах",
                                "target_lang_sense_clarification": "возглас"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
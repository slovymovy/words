        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b821e414-829a-3d46-9950-6dd2896cbef4",
                    "sense_definition": "Dotyczący Frygii (starożytnej krainy w Azji Mniejszej), wywodzący się z niej lub związany z jej mieszkańcami i kulturą.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "history_geo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na słynnym obrazie rewolucjonistka ma na głowie czerwoną czapkę <w>frygijską</w>.",
                            "target_lang_translations": {
                                "ru": "На знаменитой картине у революционерки на голове красный <w>фригийский</w> колпак."
                            }
                        },
                        {
                            "text": "Legendarny król <w>frygijski</w> Midas zamieniał wszystko, czego dotknął, w złoto.",
                            "target_lang_translations": {
                                "ru": "Легендарный <w>фригийский</w> царь Мидас превращал всё, к чему прикасался, в золото."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czapka frygijska",
                        "skala frygijska"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "dotyczy starożytnej Frygii"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Фригии (древней области в Малой Азии), происходящий из неё или связанный с её жителями и культурой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фригийский",
                                "target_lang_sense_clarification": "относящийся к Фригии"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5c0834b0-2a43-3bb6-86c8-37fbc8a6f5ae",
                    "sense_definition": "Wymarły język indoeuropejski, którym posługiwali się starożytni mieszkańcy Frygii.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "language",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Niewiele zachowało się inskrypcji, które pozwalają badać <w>frygijski</w>.",
                            "target_lang_translations": {
                                "ru": "Сохранилось немного надписей, позволяющих изучать <w>фригийский</w>."
                            }
                        },
                        {
                            "text": "Niektórzy lingwiści uważają, że <w>frygijski</w> był blisko spokrewniony z greką.",
                            "target_lang_translations": {
                                "ru": "Некоторые лингвисты считают, что <w>фригийский</w> был близко родственен греческому."
                            }
                        }
                    ],
                    "traits": [

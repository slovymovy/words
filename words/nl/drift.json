            "senses": [
                {
                    "sense_id": "4a3fe740-0165-3857-b0fe-d783840d62b2",
                    "sense_definition": "Een plotselinge, hevige opwelling van woede.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gooide van <w>drift</w> zijn telefoon tegen de muur.",

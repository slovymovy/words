    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3495934e-bdad-35de-825d-67bd7a341ebb",
                    "sense_definition": "Het hemellichaam dat als natuurlijke satelliet in een baan om de aarde draait.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "moon_earth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kijk naar boven, de <w>maan</w> schijnt helder vannacht.",
                            "target_lang_translations": {
                                "ru": "Посмотри наверх, <w>луна</w> ярко светит сегодня ночью."
                            }
                        },
                        {
                            "text": "Bij volle <w>maan</w> is het buiten licht genoeg om te wandelen.",
                            "target_lang_translations": {
                                "ru": "При полной <w>луне</w> на улице достаточно светло, чтобы гулять."
                            }
                        }
                    ],
                    "synonyms": [
                        "Luna"
                    ],
                    "common_phrases": [
                        "volle maan",
                        "halve maan",
                        "nieuwe maan",
                        "naar de maan",
                        "de man in de maan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небесное тело, которое вращается вокруг Земли как естественный спутник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "луна",
                                "target_lang_sense_clarification": "естественный спутник Земли"
                            },
                            {
                                "target_lang_word": "месяц",
                                "target_lang_sense_clarification": "серп луны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2814d559-2028-3aa2-a483-87328f66bdec",
                    "sense_definition": "Een hemellichaam dat in een baan rond een andere planeet draait.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "moon_generic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De planeet Jupiter heeft tientallen <w>manen</w> in zijn zwaartekrachtveld.",
                            "target_lang_translations": {
                                "ru": "Планета Юпитер удерживает десятки <w>лун</w> в своем гравитационном поле."
                            }
                        },
                        {
                            "text": "Titan is de grootste <w>maan</w> van Saturnus.",
                            "target_lang_translations": {
                                "ru": "Титан — самая большая <w>луна</w> Сатурна."
                            }
                        }
                    ],
                    "synonyms": [
                        "satelliet",
                        "wachter"
                    ],
                    "common_phrases": [
                        "de manen van Jupiter",
                        "natuurlijke maan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небесное тело, вращающееся по орбите вокруг другой планеты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спутник",
                                "target_lang_sense_clarification": "небесное тело"
                            },
                            {
                                "target_lang_word": "луна",
                                "target_lang_sense_clarification": "спутник планеты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3647a031-3d3b-3e17-9284-1cdfd6d2a5b8",
                    "sense_definition": "De lange haren die op de nek van bepaalde dieren groeien, zoals bij paarden en leeuwen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mane_hair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het paard had een prachtig gevlochten <w>maan</w> voor de wedstrijd.",
                            "target_lang_translations": {
                                "ru": "У лошади была красиво заплетенная <w>грива</w> перед соревнованиями."
                            }
                        },
                        {
                            "text": "De leeuw schudde zijn kop en zijn wilde <w>manen</w> bewogen mee.",
                            "target_lang_translations": {
                                "ru": "Лев тряхнул головой, и его дикая <w>грива</w> пришла в движение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wapperende manen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинные волосы, растущие на шее некоторых животных, например, лошадей и львов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грива",
                                "target_lang_sense_clarification": "шерсть на шее животного"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5ecd5ceb-7802-3296-812c-90e8528db89f",
                    "sense_definition": "*Eerste persoon enkelvoud tegenwoordige tijd van* <w>manen</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>maan</w> je om voorzichtig te zijn met dat breekbare glas.",
                            "target_lang_translations": {
                                "ru": "Я <w>призываю</w> тебя быть осторожным с этим хрупким стеклом."
                            }
                        },
                        {
                            "text": "Als leraar <w>maan</w> ik de leerlingen regelmatig tot stilte.",
                            "target_lang_translations": {
                                "ru": "Как учитель, я регулярно <w>призываю</w> учеников к тишине."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "*Первое лицо единственного числа настоящего времени от* <w>manen</w> (призывать, увещевать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "призываю",
                                "target_lang_sense_clarification": "форма глагола"
                            },
                            {
                                "target_lang_word": "увещеваю",
                                "target_lang_sense_clarification": "форма глагола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a559ad3f-f5bd-36af-8730-889a5f1ee948",
                    "sense_definition": "*Gebiedende wijs van* <w>manen</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Maan</w> je vrienden tot kalmte voordat de ruzie escaleert!",
                            "target_lang_translations": {
                                "ru": "<w>Призови</w> друзей к спокойствию, пока ссора не обострилась!"
                            }
                        },
                        {
                            "text": "<w>Maan</w> hem om zijn openstaande schulden eindelijk te betalen.",
                            "target_lang_translations": {
                                "ru": "<w>Потребуй</w> от него наконец оплатить непогашенные долги."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "*Повелительное наклонение от* <w>manen</w> (призывать, требовать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "призови",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            },
                            {
                                "target_lang_word": "потребуй",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a0a21398-6084-3c14-ad22-e9ba0234ccf1",
                    "sense_definition": "De verpersoonlijking van de maan, vaak beschouwd als een godheid in mythologische context.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "moon_deity",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "In dit oude scheppingsverhaal spreekt Grootmoeder <w>Maan</w> tot de mensen.",
                            "target_lang_translations": {
                                "ru": "В этом древнем мифе о сотворении мира Бабушка <w>Луна</w> говорит с людьми."
                            }
                        },
                        {
                            "text": "De priesters brachten offers aan de <w>Maan</w> om een goede oogst te vragen.",
                            "target_lang_translations": {
                                "ru": "Жрецы приносили жертвы <w>Луне</w>, чтобы попросить о хорошем урожае."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mythological context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Олицетворение луны, часто почитаемое как божество в мифологическом контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Луна",
                                "target_lang_sense_clarification": "божество"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
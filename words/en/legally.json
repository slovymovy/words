    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "14a43cf9-4e80-3a09-ac10-12c4ab1bf6e6",
                    "sense_definition": "In a way that is permitted by law; not contrary to law.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>legally</w> park in this lot on weekends without paying a fee.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>законно</w> припарковаться на этой стоянке в выходные, не оплачивая сбор."
                            }
                        },
                        {
                            "text": "The signed contract is <w>legally</w> binding for both parties involved.",
                            "target_lang_translations": {
                                "ru": "Подписанный договор является <w>юридически</w> обязательным для обеих участвующих сторон."
                            }
                        }
                    ],
                    "synonyms": [
                        "lawfully"
                    ],
                    "antonyms": [
                        "illegally"
                    ],
                    "common_phrases": [
                        "legally binding",
                        "legally authorized",
                        "legally responsible"
                    ],
                    "target_lang_definitions": {
                        "ru": "В соответствии с законом; законным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "законно",
                                "target_lang_sense_clarification": "в соответствии с законом"
                            },
                            {
                                "target_lang_word": "легально",
                                "target_lang_sense_clarification": "официально разрешено"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5db5b56-f137-3227-a261-a55b500ad8c7",
                    "sense_definition": "From a legal perspective or according to the rules of the law.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Legally</w>, I think you are covered by the insurance policy.",
                            "target_lang_translations": {
                                "ru": "<w>Юридически</w>, я думаю, действие страхового полиса на вас распространяется."
                            }
                        },
                        {
                            "text": "He is considered <w>legally</w> blind because his vision is so poor.",
                            "target_lang_translations": {
                                "ru": "Он считается <w>юридически</w> слепым, так как его зрение очень слабое."
                            }
                        }
                    ],
                    "common_phrases": [
                        "legally speaking",
                        "legally blind",
                        "legally dead"
                    ],
                    "target_lang_definitions": {
                        "ru": "С юридической точки зрения или согласно нормам права."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юридически",
                                "target_lang_sense_clarification": "с правовой точки зрения"
                            },
                            {
                                "target_lang_word": "по закону",
                                "target_lang_sense_clarification": "согласно законодательству"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
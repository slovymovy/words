    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6809872e-935a-3fe9-a83b-fa1293f13bce",
                    "sense_definition": "A well-known short statement that expresses a general truth, piece of advice, or rule of conduct.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "proverbs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As the old <w>saying</w> goes, you should never judge a book by its cover.",
                            "target_lang_translations": {
                                "ru": "Как гласит старая <w>поговорка</w>, никогда не суди о книге по обложке."
                            }
                        },
                        {
                            "text": "My grandmother always had a wise <w>saying</w> ready for any difficult situation.",
                            "target_lang_translations": {
                                "ru": "У моей бабушки всегда была наготове мудрая <w>поговорка</w> для любой сложной ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "proverb",
                        "maxim",
                        "adage",
                        "expression"
                    ],
                    "common_phrases": [
                        "as the saying goes",
                        "old saying",
                        "common saying",
                        "famous saying"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошо известное краткое утверждение, выражающее общую истину, совет или правило поведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поговорка",
                                "target_lang_sense_clarification": "образное выражение, метко определяющее какое-либо явление"
                            },
                            {
                                "target_lang_word": "пословица",
                                "target_lang_sense_clarification": "краткое изречение с назидательным смыслом"
                            },
                            {
                                "target_lang_word": "изречение",
                                "target_lang_sense_clarification": "глубокая мысль, выраженная кратко"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28595bc8-8098-3211-bce8-cccd3efc9768",
                    "sense_definition": "Something that is said; a specific utterance or statement.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "utterances",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient text is full of the hard <w>sayings</w> of the prophets.",
                            "target_lang_translations": {
                                "ru": "Древний текст полон суровых <w>изречений</w> пророков."
                            }
                        },
                        {
                            "text": "He paid close attention to the <w>saying</w> of the witness during the trial.",
                            "target_lang_translations": {
                                "ru": "Он внимательно прислушивался к <w>словам</w> свидетеля во время суда."
                            }
                        }
                    ],
                    "synonyms": [
                        "dictum",
                        "expression"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage typically prefers 'statement' or 'utterance'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что сказано; конкретное высказывание или утверждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высказывание",
                                "target_lang_sense_clarification": "выраженная словами мысль"
                            },
                            {
                                "target_lang_word": "слова",
                                "target_lang_sense_clarification": "сказанное кем-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "743e6e6d-a5d6-32f7-81ec-7f33e2db9772",
                    "sense_definition": "Present participle and gerund of <w>say</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>saying</w> that she wants to go to the park now.",
                            "target_lang_translations": {
                                "ru": "Она <w>говорит</w>, что хочет пойти в парк прямо сейчас."
                            }
                        },
                        {
                            "text": "It goes without <w>saying</w> that you must study hard to pass the exam.",
                            "target_lang_translations": {
                                "ru": "Понятно без <w>слов</w>, что ты должен усердно учиться, чтобы сдать экзамен."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go without saying",
                        "keep saying",
                        "start saying"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола say (говорить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говорить",
                                "target_lang_sense_clarification": "произносить слова, сообщать мысли"
                            },
                            {
                                "target_lang_word": "сказать",
                                "target_lang_sense_clarification": "совершенный вид глагола говорить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
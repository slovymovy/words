    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bc9b1099-9d96-3a0a-8970-978550d17736",
                    "sense_definition": "Firmly fixed or established; not likely to change, move, deteriorate, or fail.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "adj_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is looking for a <w>stable</w> job with a regular income.",
                            "target_lang_translations": {
                                "ru": "Он ищет <w>стабильную</w> работу с регулярным доходом."
                            }
                        },
                        {
                            "text": "The patient is now in a <w>stable</w> condition after the surgery.",
                            "target_lang_translations": {
                                "ru": "Пациент сейчас находится в <w>стабильном</w> состоянии после операции."
                            }
                        }
                    ],
                    "synonyms": [
                        "steady",
                        "fixed",
                        "unvarying"
                    ],
                    "antonyms": [
                        "unstable",
                        "varying",
                        "mobile"
                    ],
                    "common_phrases": [
                        "stable relationship",
                        "stable condition",
                        "stable economy",
                        "stable government"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердо установленный; не склонный к изменениям, перемещениям или ухудшению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стабильный",
                                "target_lang_sense_clarification": "постоянный, неизменный"
                            },
                            {
                                "target_lang_word": "устойчивый",
                                "target_lang_sense_clarification": "твердый, непоколебимый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7674dfc1-71d0-3140-ab25-59ddec9159d6",
                    "sense_definition": "Of software: reliable and free from serious errors, bugs, or crashes.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should download the latest <w>stable</w> version of the application.",
                            "target_lang_translations": {
                                "ru": "Вам следует скачать последнюю <w>стабильную</w> версию приложения."
                            }
                        },
                        {
                            "text": "The beta release is not yet <w>stable</w> enough for daily use.",
                            "target_lang_translations": {
                                "ru": "Бета-версия еще недостаточно <w>стабильна</w> для повседневного использования."
                            }
                        }
                    ],
                    "antonyms": [
                        "unstable"
                    ],
                    "common_phrases": [
                        "stable version",
                        "stable release"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О программном обеспечении: надежный и без серьезных ошибок или сбоев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стабильный",
                                "target_lang_sense_clarification": "о работе программы"
                            }
                        ]

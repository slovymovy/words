                    "synonyms": [
                        "shell-like",
                        "shell-like"
                    ],
                    "common_phrases": [
                        "egg shell",
                        "nut shell",
                        "peanut shell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердый внешний слой яйца, ореха или семени, защищающий содержимое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скорлупа",
                                "target_lang_sense_clarification": "яйца, ореха"

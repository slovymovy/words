                },
                {
                    "sense_id": "48a45df7-4227-3b6f-b994-2097d500927e",
                    "sense_definition": "Een grote auto van Amerikaans fabricaat.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij rijdt het liefst in een oude <amerikaan> met veel chroom.",

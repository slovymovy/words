                {
                    "sense_id": "fdb2d472-eb44-3d2c-a319-374d1dd67e5e",
                    "sense_definition": "Urząd sprawowany przez rabina lub instytucja kierująca życiem religijnym gminy żydowskiej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szkoła zwróciła się z oficjalną prośbą do miejscowego <w>rabinatu</w>.",
                            "target_lang_translations": {
                                "ru": "Школа обратилась с официальной просьбой в местный <w>раввинат</w>."
                            }
                        },
                        {
                            "text": "Naczelny <w>rabinat</w> wydał oświadczenie w sprawie nadchodzących świąt.",
                            "target_lang_translations": {
                                "ru": "Главный <w>раввинат</w> выпустил заявление по поводу предстоящих праздников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naczelny rabinat",
                        "rabinat polowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Должность раввина или учреждение, руководящее религиозной жизнью еврейской общины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раввинат",
                                "target_lang_sense_clarification": "учреждение или должность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b55b340-3f9a-3165-877d-f969dd29af45",
                    "sense_definition": "Budynek lub lokal będący siedzibą rabina i jego urzędu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na końcu ulicy stał odnowiony <w>rabinat</w> oraz synagoga.",
                            "target_lang_translations": {
                                "ru": "В конце улицы стоял отреставрированный <w>раввинат</w> и синагога."
                            }
                        },
                        {
                            "text": "Spotkanie odbyło się w budynku, gdzie mieści się <w>rabinat</w>.",
                            "target_lang_translations": {

{
    "word_family": [
        "miner",
        "mining",
        "minefield",
        "mineworker",
        "unmined"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "cdde2227-9a24-3a34-9c41-eb48d0690461",
                    "sense_definition": "That or those belonging to me; the possessive form of I.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This blue umbrella is <w>mine</w>, but the red one is yours.",
                            "target_lang_translations": {
                                "ru": "Этот синий зонт — <w>мой</w>, а красный — твой."
                            }
                        },
                        {
                            "text": "A good friend of <w>mine</w> helped me fix the car yesterday.",
                            "target_lang_translations": {
                                "ru": "Один <w>мой</w> хороший друг помог мне починить машину вчера."
                            }
                        }
                    ],
                    "antonyms": [
                        "yours",
                        "theirs",
                        "his",
                        "hers"
                    ],
                    "common_phrases": [
                        "friend of mine",
                        "of mine",
                        "yours and mine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот или те, что принадлежат мне; притяжательная форма местоимения «я»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мой",
                                "target_lang_sense_clarification": "притяжательное местоимение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17b6c521-2c15-351b-bb2a-18cb3123ddd0",
                    "sense_definition": "My house or home.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We can go back to <w>mine</w> for coffee after the movie.",
                            "target_lang_translations": {
                                "ru": "Мы можем пойти <w>ко мне</w> на кофе после кино."
                            }
                        },
                        {
                            "text": "The party is at <w>mine</w> this weekend.",
                            "target_lang_translations": {
                                "ru": "Вечеринка в эти выходные <w>у меня</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "my place",
                        "my home"
                    ],
                    "common_phrases": [
                        "back to mine",
                        "at mine"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мой дом или жилище."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ко мне",
                                "target_lang_sense_clarification": "направление движения (домой)"
                            },
                            {
                                "target_lang_word": "у меня",
                                "target_lang_sense_clarification": "местонахождение (дома)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "69197c0d-feac-391e-b624-53cb8a8cf5f5",
                    "sense_definition": "An excavation in the earth from which ore, coal, or other minerals are extracted.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "excavation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandfather worked in a coal <w>mine</w> for forty years.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка проработал в угольной <w>шахте</w> сорок лет."
                            }
                        },
                        {
                            "text": "The company closed the gold <w>mine</w> because it was no longer profitable.",
                            "target_lang_translations": {
                                "ru": "Компания закрыла золотой <w>рудник</w>, так как он перестал быть прибыльным."
                            }
                        }
                    ],
                    "synonyms": [
                        "pit",
                        "colliery",
                        "quarry",
                        "excavation"
                    ],
                    "common_phrases": [
                        "coal mine",
                        "gold mine",
                        "salt mine",
                        "open-pit mine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место добычи полезных ископаемых, таких как руда или уголь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шахта",
                                "target_lang_sense_clarification": "подземная выработка (обычно угольная)"
                            },
                            {
                                "target_lang_word": "рудник",
                                "target_lang_sense_clarification": "предприятие по добыче руды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8fa9aca-9af4-3819-ad89-534e7b23099c",
                    "sense_definition": "An explosive device placed in the ground or water, designed to explode when touched or approached.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "explosive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldier carefully marked the location of the buried <w>mine</w>.",
                            "target_lang_translations": {
                                "ru": "Солдат осторожно отметил местоположение закопанной <w>мины</w>."
                            }
                        },
                        {
                            "text": "The ship was damaged by a floating naval <w>mine</w> during the war.",
                            "target_lang_translations": {
                                "ru": "Корабль был поврежден плавучей морской <w>миной</w> во время войны."
                            }
                        }
                    ],
                    "synonyms": [
                        "landmine",
                        "explosive"
                    ],
                    "common_phrases": [
                        "land mine",
                        "naval mine",
                        "step on a mine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взрывное устройство, закладываемое в землю или воду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мина",
                                "target_lang_sense_clarification": "боеприпас"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f4a18b3-e146-3a6f-aa2d-9f2cccf2e3d0",
                    "sense_definition": "A rich source of something, such as information or wealth.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor is a <w>mine</w> of information about ancient history.",
                            "target_lang_translations": {
                                "ru": "Профессор — настоящий <w>кладезь</w> информации о древней истории."
                            }
                        },
                        {
                            "text": "This old library is a <w>mine</w> of forgotten stories.",
                            "target_lang_translations": {
                                "ru": "Эта старая библиотека — <w>кладезь</w> забытых историй."
                            }
                        }
                    ],
                    "synonyms": [
                        "treasure trove",
                        "wealth",
                        "fund",
                        "storehouse"
                    ],
                    "common_phrases": [
                        "mine of information"
                    ],
                    "target_lang_definitions": {
                        "ru": "Богатый источник чего-либо, например информации или знаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кладезь",
                                "target_lang_sense_clarification": "неиссякаемый источник (перен.)"
                            },
                            {
                                "target_lang_word": "сокровищница",
                                "target_lang_sense_clarification": "место хранения ценностей (перен.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10e46b08-b81f-39ab-b14b-4780f43e809e",
                    "sense_definition": "A computer or network of computers used to generate units of a cryptocurrency.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "crypto",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He set up a Bitcoin <w>mine</w> in his basement using powerful servers.",
                            "target_lang_translations": {
                                "ru": "Он устроил биткоин-<w>ферму</w> в подвале, используя мощные серверы."
                            }
                        },
                        {
                            "text": "The electricity consumption of a large crypto <w>mine</w> can be very high.",
                            "target_lang_translations": {
                                "ru": "Потребление электричества крупной крипто<w>фермой</w> может быть очень высоким."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "computing context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютер или сеть компьютеров для генерации единиц криптовалюты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ферма",
                                "target_lang_sense_clarification": "компьютерное оборудование для майнинга"
                            },
                            {
                                "target_lang_word": "майнинг-ферма",
                                "target_lang_sense_clarification": "специализированное оборудование"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ccef5870-d142-3f8d-89ba-a9d897cba1d0",
                    "sense_definition": "To extract ores, coal, or other minerals from the earth.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "excavation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They plan to <w>mine</w> for diamonds in this remote region.",
                            "target_lang_translations": {
                                "ru": "Они планируют <w>добывать</w> алмазы в этом отдаленном регионе."
                            }
                        },
                        {
                            "text": "Large machines are used to <w>mine</w> coal from the ground.",
                            "target_lang_translations": {
                                "ru": "Большие машины используются, чтобы <w>добывать</w> уголь из-под земли."
                            }
                        }
                    ],
                    "synonyms": [
                        "excavate",
                        "extract",
                        "dig",
                        "quarry"
                    ],
                    "common_phrases": [
                        "mine for gold",
                        "mine coal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Извлекать руду, уголь или другие полезные ископаемые из земли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добывать",
                                "target_lang_sense_clarification": "извлекать ископаемые"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "379edb41-3379-3eca-9012-40d9150ec3cb",
                    "sense_definition": "To earn new units of cryptocurrency by performing complex mathematical calculations with a computer.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "crypto",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It has become much harder to <w>mine</w> Bitcoin on a personal computer.",
                            "target_lang_translations": {
                                "ru": "Стало гораздо сложнее <w>майнить</w> биткоины на персональном компьютере."
                            }
                        },
                        {
                            "text": "Many people try to <w>mine</w> cryptocurrency to make extra money.",
                            "target_lang_translations": {
                                "ru": "Многие люди пытаются <w>майнить</w> криптовалюту, чтобы заработать дополнительные деньги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mine Bitcoin",
                        "mine crypto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зарабатывать новые единицы криптовалюты, выполняя сложные вычисления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "майнить",
                                "target_lang_sense_clarification": "добывать криптовалюту (слэнг)"
                            },
                            {
                                "target_lang_word": "добывать",
                                "target_lang_sense_clarification": "получать криптовалюту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8f39399-66d3-3734-8584-40f726a8f0f4",
                    "sense_definition": "To place explosive mines in an area of land or water.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "explosive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The retreating army <w>mined</w> the bridge to stop the enemy from crossing.",
                            "target_lang_translations": {
                                "ru": "Отступающая армия <w>заминировала</w> мост, чтобы остановить переправу врага."
                            }
                        },
                        {
                            "text": "It is illegal to <w>mine</w> civilian areas during a conflict.",
                            "target_lang_translations": {
                                "ru": "Незаконно <w>минировать</w> гражданские районы во время конфликта."
                            }
                        }
                    ],
                    "synonyms": [
                        "booby-trap"
                    ],
                    "antonyms": [
                        "demine",
                        "clear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устанавливать взрывные мины на участке суши или воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "минировать",
                                "target_lang_sense_clarification": "закладывать мины"
                            },
                            {
                                "target_lang_word": "заминировать",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "b21af3b3-69b4-3f2e-9078-b452bff5568a",
                    "sense_definition": "My; belonging to me (used before a vowel or noun).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Mine</w> eyes have seen the glory of the coming of the Lord.",
                            "target_lang_translations": {
                                "ru": "<w>Мои</w> глаза видели славу пришествия Господня."
                            }
                        },
                        {
                            "text": "I bid you farewell, brother <w>mine</w>.",
                            "target_lang_translations": {
                                "ru": "Прощай, брат <w>мой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "my"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "poetic or literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мой; принадлежащий мне (устаревшая или поэтическая форма)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мой",
                                "target_lang_sense_clarification": "притяжательное местоимение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
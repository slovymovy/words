    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "574c6d9f-4adc-386e-b8c5-cd37e2abe4a7",
                    "sense_definition": "A device used to slow down or stop the motion of a vehicle or wheel, usually by friction.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "stopping_mechanism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She slammed on the <w>brakes</w> when she saw a child run in front of the car.",
                            "target_lang_translations": {
                                "ru": "Она ударила по <w>тормозам</w>, когда увидела ребенка, выбежавшего перед машиной."
                            }
                        },
                        {
                            "text": "You should check your parking <w>brake</w> before leaving the vehicle on a hill.",
                            "target_lang_translations": {
                                "ru": "Вам следует проверить стояночный <w>тормоз</w>, прежде чем оставлять автомобиль на склоне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hit the brakes",
                        "slam on the brakes",
                        "parking brake",
                        "emergency brake",
                        "apply the brakes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство, используемое для замедления или остановки движения транспортного средства или колеса, обычно за счет трения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тормоз",
                                "target_lang_sense_clarification": "механизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8183300-6616-340f-82bd-c8cdfd19bcaf",
                    "sense_definition": "An area of dense undergrowth, bushes, or briers; a thicket.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "nature_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rabbit disappeared quickly into the dense <w>brake</w> of thorns and vines.",
                            "target_lang_translations": {
                                "ru": "Кролик быстро исчез в густых <w>зарослях</w> колючек и лоз."
                            }
                        },
                        {
                            "text": "Hunters struggled to move through the tangled <w>brake</w> deep in the forest.",
                            "target_lang_translations": {
                                "ru": "Охотники с трудом пробирались через сплетенную <w>чащу</w> в глубине леса."
                            }
                        }
                    ],
                    "common_phrases": [

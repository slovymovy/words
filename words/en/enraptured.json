                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The performance <w>enraptured</w> everyone who came to see it.",
                            "target_lang_translations": {

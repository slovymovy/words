{
    "word_family": [
        "spiż"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d8f350a6-bd8b-38c0-a9f4-87760ce1efbb",
                    "sense_definition": "Wykonany ze <w>spiżu</w> (stopu miedzi, cyny i cynku), często używany w kontekście dzwonów, armat lub pomników.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wieży starego kościoła bije ogromny <w>spiżowy</w> dzwon.",
                            "target_lang_translations": {
                                "ru": "На башне старой церкви бьет огромный <w>бронзовый</w> колокол."
                            }
                        },
                        {
                            "text": "W muzeum wojskowym oglądaliśmy zabytkowe <w>spiżowe</w> armaty z czasów napoleońskich.",
                            "target_lang_translations": {
                                "ru": "В военном музее мы осматривали старинные <w>бронзовые</w> пушки наполеоновских времен."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spiżowy dzwon",
                        "spiżowy pomnik",
                        "spiżowa armata"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный из бронзы (сплава меди, олова и цинка), часто используется в контексте колоколов, пушек или памятников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бронзовый",
                                "target_lang_sense_clarification": "о материале"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c09d87f-c124-352f-91db-55a65adeaf83",
                    "sense_definition": "O człowieku, jego charakterze lub woli: bardzo silny, surowy, trwały i nieustępliwy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>spiżowy</w> charakter pozwolił mu przetrwać najtrudniejsze chwile w życiu.",
                            "target_lang_translations": {
                                "ru": "Его <w>несгибаемый</w> характер позволил ему пережить самые трудные моменты в жизни."
                            }
                        },
                        {
                            "text": "Dowódca był znany ze swojej <w>spiżowej</w> woli i żelaznej dyscypliny.",
                            "target_lang_translations": {
                                "ru": "Командир был известен своей <w>непреклонной</w> волей и железной дисциплиной."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieugięty",
                        "silny"
                    ],
                    "antonyms": [
                        "słaby"
                    ],
                    "common_phrases": [
                        "spiżowy charakter",
                        "spiżowa wola"
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке, его характере или воле: очень сильный, суровый, стойкий и непреклонный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несгибаемый",
                                "target_lang_sense_clarification": "о характере"
                            },
                            {
                                "target_lang_word": "непреклонный",
                                "target_lang_sense_clarification": "о воле"
                            },
                            {
                                "target_lang_word": "твердый",
                                "target_lang_sense_clarification": "синоним сильного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32803de1-932d-35ee-bd3e-92308c73b52f",
                    "sense_definition": "O dźwięku lub głosie: głęboki, donośny i metaliczny, przypominający dźwięk dzwonu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "figurative_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aktor miał potężny, <w>spiżowy</w> głos, który bez trudu wypełniał całą salę teatralną.",
                            "target_lang_translations": {
                                "ru": "У актера был мощный, <w>зычный</w> голос, который без труда заполнял весь театральный зал."
                            }
                        },
                        {
                            "text": "Jej <w>spiżowy</w> śmiech rozbrzmiewał echem na korytarzu.",
                            "target_lang_translations": {
                                "ru": "Ее <w>металлический</w> смех эхом разносился по коридору."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spiżowy głos",
                        "spiżowy dźwięk"
                    ],
                    "target_lang_definitions": {
                        "ru": "О звуке или голосе: глубокий, громкий и металлический, напоминающий звон колокола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зычный",
                                "target_lang_sense_clarification": "о громком голосе"
                            },
                            {
                                "target_lang_word": "металлический",
                                "target_lang_sense_clarification": "о тембре звука"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6d6ec429-9789-3853-9d0b-784abb147e72",
                    "sense_definition": "The hidden power believed to control what will happen in the future.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "destiny_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He believed that <w>destiny</w> had brought them together at the right moment.",
                            "target_lang_translations": {
                                "ru": "Он верил, что <w>судьба</w> свела их вместе в нужный момент."
                            }
                        },
                        {
                            "text": "Dara insists that <w>destiny</w> will eventually help her find the perfect partner.",
                            "target_lang_translations": {
                                "ru": "Дара настаивает, что <w>судьба</w> в конечном итоге поможет ей найти идеального партнера."
                            }
                        }
                    ],
                    "synonyms": [
                        "fate",
                        "fortune",
                        "kismet",
                        "predestination"
                    ],
                    "common_phrases": [
                        "control your destiny",
                        "manifest destiny",
                        "tempt destiny",
                        "believe in destiny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скрытая сила, которая, как считается, управляет тем, что произойдет в будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судьба",
                                "target_lang_sense_clarification": "высшая сила"
                            },
                            {
                                "target_lang_word": "рок",
                                "target_lang_sense_clarification": "неизбежность, фатум"
                            },
                            {
                                "target_lang_word": "провидение",
                                "target_lang_sense_clarification": "божественная воля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "312c8a4a-ed83-3d66-995e-a6e07aea8058",
                    "sense_definition": "The events that will necessarily happen to a particular person or thing in the future.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "destiny_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She felt it was her <w>destiny</w> to become a famous doctor and help people.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала, что её <w>предназначение</w> — стать известным врачом и помогать людям."
                            }
                        },
                        {
                            "text": "The soldiers marched forward, ready to face their final <w>destiny</w> on the battlefield.",
                            "target_lang_translations": {
                                "ru": "Солдаты маршировали вперед, готовые встретить свою окончательную <w>участь</w> на поле битвы."
                            }
                        }
                    ],
                    "synonyms": [
                        "fate",
                        "lot",
                        "doom"
                    ],
                    "common_phrases": [
                        "fulfill one's destiny",
                        "shape one's destiny",
                        "face one's destiny"
                    ],
                    "target_lang_definitions": {
                        "ru": "События, которые неизбежно произойдут с конкретным человеком или вещью в будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судьба",
                                "target_lang_sense_clarification": "жизненный путь"
                            },
                            {
                                "target_lang_word": "предназначение",
                                "target_lang_sense_clarification": "миссия, цель"
                            },
                            {
                                "target_lang_word": "участь",
                                "target_lang_sense_clarification": "доля, исход"
                            }
                        ]
                    }
                }
            ]
        },

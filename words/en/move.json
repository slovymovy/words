    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a98ca718-3583-3c3f-a06e-f3286c491d0a",
                    "sense_definition": "To change physical position or go from one place to another.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car started to <w>move</w> slowly down the street.",
                            "target_lang_translations": {
                                "ru": "Машина начала медленно <w>двигаться</w> по улице."
                            }
                        },
                        {
                            "text": "Please <w>move</w> a little so I can see the screen.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>подвинься</w> немного, чтобы я мог видеть экран."
                            }
                        }
                    ],
                    "synonyms": [
                        "go",
                        "travel",
                        "proceed"
                    ],
                    "antonyms": [
                        "stop",
                        "stay",
                        "remain"
                    ],
                    "common_phrases": [
                        "move away",
                        "move along",
                        "move forward"
                    ],
                    "target_lang_definitions": {
                        "ru": "Менять физическое положение или перемещаться из одного места в другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двигаться",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "перемещаться",
                                "target_lang_sense_clarification": "менять местоположение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd132ea9-ec35-3c70-9db3-48efa9aafaff",
                    "sense_definition": "To cause someone or something to change physical position.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you help me <w>move</w> this heavy table?",
                            "target_lang_translations": {
                                "ru": "Ты можешь помочь мне <w>передвинуть</w> этот тяжелый стол?"
                            }
                        },
                        {
                            "text": "The wind <w>moved</w> the leaves across the lawn.",
                            "target_lang_translations": {
                                "ru": "Ветер <w>перемещал</w> листья по газону."
                            }
                        }
                    ],
                    "synonyms": [
                        "shift",
                        "push",
                        "transport",
                        "carry"
                    ],
                    "antonyms": [
                        "fix",
                        "hold"
                    ],
                    "common_phrases": [
                        "move around"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять кого-то или что-то менять физическое положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двигать",
                                "target_lang_sense_clarification": "приводить в движение"
                            },
                            {
                                "target_lang_word": "передвигать",
                                "target_lang_sense_clarification": "перемещать предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d1d3391-9b86-3e4c-a1fd-1ad49bc17061",
                    "sense_definition": "To change one's home or place of business to a different location.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "relocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They plan to <w>move</w> to a bigger house soon.",
                            "target_lang_translations": {
                                "ru": "Они планируют скоро <w>переехать</w> в дом побольше."
                            }
                        },
                        {
                            "text": "I <w>moved</w> to London for a new job last year.",
                            "target_lang_translations": {
                                "ru": "В прошлом году я <w>переехал</w> в Лондон ради новой работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "relocate"
                    ],
                    "common_phrases": [
                        "move house",
                        "move in",
                        "move out",
                        "move away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Менять место жительства или работы на другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переезжать",
                                "target_lang_sense_clarification": "менять жилье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4f17994-67a0-3b58-beda-7507287eea00",
                    "sense_definition": "To change the position of a game piece on a board.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "game_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>moved</w> his pawn forward two squares.",
                            "target_lang_translations": {
                                "ru": "Он <w>передвинул</w> свою пешку на две клетки вперед."
                            }
                        },
                        {
                            "text": "It is against the rules to <w>move</w> diagonally here.",
                            "target_lang_translations": {
                                "ru": "Здесь <w>ходить</w> по диагонали против правил."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Менять положение игровой фигуры на доске."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "делать ход"
                            },
                            {
                                "target_lang_word": "передвигать",
                                "target_lang_sense_clarification": "фигуру"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8ece4af-0453-38f5-ba44-5b9a120f6405",
                    "sense_definition": "To cause strong feelings, especially sadness, sympathy, or tenderness, in someone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sad movie really <w>moved</w> him.",
                            "target_lang_translations": {
                                "ru": "Этот грустный фильм очень его <w>тронул</w>."
                            }
                        },
                        {
                            "text": "She was <w>moved</w> by the kindness of strangers.",
                            "target_lang_translations": {
                                "ru": "Она была <w>тронута</w> добротой незнакомцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "touch",
                        "affect",
                        "stir"
                    ],
                    "common_phrases": [
                        "deeply moved",
                        "moved to tears"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать сильные чувства, особенно грусть, сочувствие или нежность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трогать",
                                "target_lang_sense_clarification": "вызывать эмоции"
                            },
                            {
                                "target_lang_word": "растрогать",
                                "target_lang_sense_clarification": "довести до эмоций"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33487f4b-ffe0-3385-b437-2b25c14bca8e",
                    "sense_definition": "To start taking action or to hurry up.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>move</w> if we want to catch the train.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>поторапливаться</w>, если мы хотим успеть на поезд."
                            }
                        },
                        {
                            "text": "Let's <w>move</w> before it gets dark.",
                            "target_lang_translations": {
                                "ru": "Давай <w>выдвигаться</w>, пока не стемнело."
                            }
                        }
                    ],
                    "synonyms": [
                        "hurry",
                        "act"
                    ],
                    "common_phrases": [
                        "get moving",
                        "move it"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать действовать или поторапливаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шевелиться",
                                "target_lang_sense_clarification": "разг. поторапливаться"
                            },
                            {
                                "target_lang_word": "действовать",
                                "target_lang_sense_clarification": "предпринимать меры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba0d9112-5bf8-35bf-afd5-5973522c292f",
                    "sense_definition": "To make a formal proposal for consideration in a meeting or legal assembly.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "proposal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chairman <w>moved</w> to approve the minutes.",
                            "target_lang_translations": {
                                "ru": "Председатель <w>предложил</w> утвердить протокол."
                            }
                        },
                        {
                            "text": "I <w>move</w> that we adjourn the meeting immediately.",
                            "target_lang_translations": {
                                "ru": "Я <w>вношу предложение</w> немедленно закрыть собрание."
                            }
                        }
                    ],
                    "synonyms": [
                        "propose",
                        "submit"
                    ],
                    "common_phrases": [
                        "move a motion",
                        "move a resolution"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "parliamentary or legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вносить официальное предложение на собрании или юридическом заседании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предлагать",
                                "target_lang_sense_clarification": "вносить предложение"
                            },
                            {
                                "target_lang_word": "вносить предложение",
                                "target_lang_sense_clarification": "официально"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5b84e84d-7929-3376-9b00-4419c247e69a",
                    "sense_definition": "The act of changing physical position or location.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "One wrong <w>move</w> could cause the tower to fall.",
                            "target_lang_translations": {
                                "ru": "Одно неверное <w>движение</w> могло обрушить башню."
                            }
                        },
                        {
                            "text": "We watched the <w>move</w> of the clouds across the sky.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдали за <w>движением</w> облаков по небу."
                            }
                        }
                    ],
                    "synonyms": [
                        "movement",
                        "motion"
                    ],
                    "antonyms": [
                        "stillness"
                    ],
                    "common_phrases": [
                        "on the move",
                        "make a move"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по изменению физического положения или местонахождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "движение",
                                "target_lang_sense_clarification": "физическое перемещение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "742db62d-e63d-354b-a43a-051ccd74f6c6",
                    "sense_definition": "A turn to play or the action of moving a piece in a game.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "game_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is your <w>move</w> now.",
                            "target_lang_translations": {
                                "ru": "Сейчас твой <w>ход</w>."
                            }
                        },
                        {
                            "text": "That was a brilliant chess <w>move</w>.",
                            "target_lang_translations": {
                                "ru": "Это был блестящий шахматный <w>ход</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "turn",
                        "play"
                    ],
                    "common_phrases": [
                        "your move",
                        "next move"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очередь играть или действие по перемещению фигуры в игре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ход",
                                "target_lang_sense_clarification": "в игре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8ab6c97-bf3a-30f2-8aed-2f8c7116e92f",
                    "sense_definition": "An action taken to achieve a goal or a step in a plan.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "strategy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Quitting his job without a backup plan was a risky <w>move</w>.",
                            "target_lang_translations": {
                                "ru": "Увольнение с работы без запасного плана было рискованным <w>шагом</w>."
                            }
                        },
                        {
                            "text": "It was a smart <w>move</w> to invest in that company.",
                            "target_lang_translations": {
                                "ru": "Инвестировать в эту компанию было умным <w>ходом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "step",
                        "action",
                        "tactic",
                        "maneuver"
                    ],
                    "common_phrases": [
                        "smart move",
                        "bad move",
                        "career move",
                        "bold move"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, предпринятое для достижения цели, или шаг в плане."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шаг",
                                "target_lang_sense_clarification": "поступок"
                            },
                            {
                                "target_lang_word": "ход",
                                "target_lang_sense_clarification": "маневр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "460a4f26-92bb-3078-83f9-926b715809af",
                    "sense_definition": "The process of changing one's residence or workplace.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>move</w> to the new apartment took all weekend.",
                            "target_lang_translations": {
                                "ru": "<w>Переезд</w> на новую квартиру занял все выходные."
                            }
                        },
                        {
                            "text": "Helping friends with a <w>move</w> is exhausting work.",
                            "target_lang_translations": {
                                "ru": "Помогать друзьям с <w>переездом</w> — утомительная работа."
                            }
                        }
                    ],
                    "synonyms": [
                        "relocation",
                        "removal"
                    ],
                    "common_phrases": [
                        "the big move"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс смены места жительства или работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переезд",
                                "target_lang_sense_clarification": "смена жилья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e71fc0c-cf58-3a7b-8beb-a54cf377b0d1",
                    "sense_definition": "A specific, practiced physical movement used in sports, dance, or combat.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "specific_maneuver",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She showed off her best dance <w>moves</w> at the party.",
                            "target_lang_translations": {
                                "ru": "Она показала свои лучшие танцевальные <w>движения</w> на вечеринке."
                            }
                        },
                        {
                            "text": "The wrestler won the match with his signature <w>move</w>.",
                            "target_lang_translations": {
                                "ru": "Борец выиграл матч своим коронным <w>приемом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "maneuver"
                    ],
                    "common_phrases": [
                        "dance moves",
                        "signature move"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особое отработанное физическое движение в спорте, танцах или борьбе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "движение",
                                "target_lang_sense_clarification": "в танце"
                            },
                            {
                                "target_lang_word": "прием",
                                "target_lang_sense_clarification": "в борьбе"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                },
                {
                    "sense_id": "d36c663d-0a7e-3494-a636-7a88c171537e",
                    "sense_definition": "De lading of vracht die vervoerd wordt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cargo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er is een speciaal <w>transport</w> onderweg met grote machines.",
                            "target_lang_translations": {
                                "en": "A special <w>transport</w> is on the way with large machines.",
                                "ru": "В пути находится специальный <w>транспорт</w> с крупной техникой."
                            }
                        },
                        {
                            "text": "De politie begeleidde het zware <w>transport</w> over de snelweg.",
                            "target_lang_translations": {
                                "en": "The police escorted the heavy <w>transport</w> along the highway.",
                                "ru": "Полиция сопровождала тяжелый <w>транспорт</w> по шоссе."
                            }
                        }
                    ],
                    "synonyms": [
                        "vervoer",
                        "overbrenging"
                    ],
                    "common_phrases": [
                        "zwaar transport",
                        "speciaal transport",
                        "exceptioneel transport"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f42a51cf-bb80-3823-8cdc-54139c8f6c59",
                    "sense_definition": "A country, group, or person that is formally united with another for a specific purpose, especially in war or politics.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_cooperation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The United Kingdom has been a close <w>ally</w> of the United States for many years."
                        },
                        {
                            "text": "During the war, the country relied heavily on support from its strongest <w>ally</w>."
                        }
                    ],
                    "antonyms": [
                        "enemy",
                        "adversary"
                    ],
                    "common_phrases": [
                        "close ally",
                        "key ally",
                        "wartime ally"
                    ]
                },
                {
                    "sense_id": "7e0788b3-937c-3355-a86d-66f6718805c9",
                    "sense_definition": "A person who helps and supports someone else, especially when others are against them.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_cooperation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She found a loyal <w>ally</w> in her supervisor when she proposed the new project."
                        },
                        {
                            "text": "He needed an <w>ally</w> to help him navigate the complex office politics."
                        }
                    ],
                    "antonyms": [
                        "opponent"
                    ],
                    "common_phrases": [
                        "loyal ally",
                        "powerful ally",
                        "find an ally"
                    ]
                },
                {
                    "sense_id": "9300f7bd-3ecb-3f7c-b7cd-bfdcab5989bc",
                    "sense_definition": "A person who supports a marginalized group, such as the LGBTQ+ community, without being a member of that group.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_cooperation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tries to be an <w>ally</w> by listening to the experiences of minority groups."
                        },
                        {
                            "text": "The organization offers training on how to become a better <w>ally</w> to the LGBTQ+ community."
                        }
                    ],
                    "common_phrases": [
                        "be an ally",
                        "trans ally"
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "425f0f27-a25a-3605-a36b-929ce461153f",
                    "sense_definition": "To join or unite with someone or something in order to support each other or achieve a common goal.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_cooperation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two companies decided to <w>ally</w> with each other to dominate the market."
                        },
                        {
                            "text": "Small nations often <w>ally</w> themselves with larger powers for defense."
                        }
                    ],
                    "synonyms": [
                        "make common cause"
                    ],
                    "common_phrases": [
                        "ally with",
                        "ally oneself with"
                    ]
                }
            ]
        },

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1eb7f3a6-29da-3399-a94c-85479ddbd323",
                    "sense_definition": "Резким движением извлечь, удалить что-либо, закреплённое где-либо (зуб, растение, гвоздь и т. п.).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "forceful_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач решил <вырвать> больной зуб, чтобы он больше не беспокоил пациента.",
                            "target_lang_translations": {
                                "en": "The doctor decided to <pull out> the bad tooth so it would no longer bother the patient."
                            }
                        },
                        {
                            "text": "Мальчик случайно <вырвал> страницу из библиотечной книги, когда быстро её листал.",
                            "target_lang_translations": {
                                "en": "The boy accidentally <tore out> a page from the library book while leafing through it quickly."
                            }
                        }
                    ],
                    "synonyms": [
                        "выдернуть",
                        "выдрать"
                    ],
                    "antonyms": [
                        "всунуть",
                        "засунуть",
                        "впихнуть"
                    ],
                    "common_phrases": [
                        "вырвать зуб",
                        "вырвать страницу",
                        "вырвать с корнем",
                        "вырвать волос"
                    ],
                    "target_lang_definitions": {
                        "en": "To extract or remove something fixed in place (such as a tooth, plant, or nail) with a sharp, forceful movement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pull out",
                                "target_lang_sense_clarification": "general use, e.g., a tooth or nail"
                            },
                            {
                                "target_lang_word": "tear out",
                                "target_lang_sense_clarification": "for paper or pages"
                            },
                            {
                                "target_lang_word": "extract",
                                "target_lang_sense_clarification": "formal or medical context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a241542-6832-3d75-a000-5717324cb3c5",
                    "sense_definition": "Резким рывком отнять, взять что-либо из рук.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "forceful_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вор попытался <вырвать> сумку у прохожей, но она крепко её держала.",
                            "target_lang_translations": {
                                "en": "The thief tried to <snatch> the bag from a passerby, but she held it tightly."
                            }
                        },
                        {
                            "text": "Он хотел <вырвать> письмо у сестры, чтобы никто не смог его прочитать.",
                            "target_lang_translations": {
                                "en": "He wanted to <snatch> the letter from his sister so no one could read it."
                            }
                        }
                    ],
                    "synonyms": [
                        "выхватить",
                        "урвать"
                    ],
                    "antonyms": [
                        "упустить"
                    ],
                    "common_phrases": [
                        "вырвать из рук",
                        "вырвать сумку"
                    ],
                    "target_lang_definitions": {
                        "en": "To take something away from someone's hands with a sharp, sudden jerk."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "snatch",
                                "target_lang_sense_clarification": "quick, sudden grabbing"
                            },
                            {
                                "target_lang_word": "grab",
                                "target_lang_sense_clarification": "forceful taking"
                            },
                            {
                                "target_lang_word": "wrest",
                                "target_lang_sense_clarification": "taking with a struggle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06ef4b83-7b0b-3d4e-9499-7ca6499848da",
                    "sense_definition": "Извергнуть содержимое желудка через рот (обычно используется в безличных конструкциях).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physiological_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После несвежего обеда его <вырвало> прямо на улице.",
                            "target_lang_translations": {
                                "en": "After a stale lunch, he <threw up> right on the street."
                            }
                        },
                        {
                            "text": "Ребёнка <вырвало> в машине из-за сильной жары и укачивания.",
                            "target_lang_translations": {
                                "en": "The child <threw up> in the car due to the intense heat and motion sickness."
                            }
                        }
                    ],
                    "synonyms": [
                        "стошнить",
                        "вытошнить"
                    ],
                    "common_phrases": [
                        "его вырвало",
                        "меня вырвало"
                    ],
                    "target_lang_definitions": {
                        "en": "To expel the contents of the stomach through the mouth (usually used in impersonal constructions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "throw up",
                                "target_lang_sense_clarification": "common, informal"
                            },
                            {
                                "target_lang_word": "vomit",
                                "target_lang_sense_clarification": "medical or formal"
                            },
                            {
                                "target_lang_word": "be sick",
                                "target_lang_sense_clarification": "common in British English"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ac9c8a6-f803-30ce-a82b-4173d7825538",
                    "sense_definition": "Избавить, спасти кого-либо от неприятной ситуации или окружения; изъять часть из целого.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "forceful_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачам удалось <вырвать> пациента из лап смерти в самый последний момент.",
                            "target_lang_translations": {
                                "en": "The doctors managed to <snatch> the patient from the jaws of death at the very last moment."
                            }
                        },
                        {
                            "text": "Нельзя <вырвать> эту фразу из контекста, иначе смысл сказанного исказится.",
                            "target_lang_translations": {
                                "en": "You cannot <take> this phrase out of context, otherwise the meaning of what was said will be distorted."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вырвать из контекста",
                        "вырвать из лап смерти",
                        "вырвать из плена"
                    ],
                    "target_lang_definitions": {
                        "en": "To rescue someone from a dangerous situation or environment; to isolate a part from the whole context."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "snatch",
                                "target_lang_sense_clarification": "figurative, e.g., from death"
                            },
                            {
                                "target_lang_word": "tear out",
                                "target_lang_sense_clarification": "e.g., from context"
                            },
                            {
                                "target_lang_word": "take out",
                                "target_lang_sense_clarification": "e.g., of context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75310fe8-4d50-3595-a830-c53631f3ca15",
                    "sense_definition": "Добиться чего-либо с большим трудом или силой; вынудить дать что-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "forceful_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Команда смогла <вырвать> победу у соперника на последних минутах матча.",
                            "target_lang_translations": {
                                "en": "The team was able to <snatch> victory from the opponent in the final minutes of the match."
                            }
                        },
                        {
                            "text": "Следователь пытался <вырвать> у подозреваемого признание в совершении преступления.",
                            "target_lang_translations": {
                                "en": "The investigator tried to <extract> a confession from the suspect regarding the commission of the crime."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вырвать победу",
                        "вырвать признание",
                        "вырвать согласие"
                    ],
                    "target_lang_definitions": {
                        "en": "To achieve something with great difficulty or force; to force someone to give something (like a confession)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "snatch",
                                "target_lang_sense_clarification": "e.g., a victory"
                            },
                            {
                                "target_lang_word": "wrest",
                                "target_lang_sense_clarification": "to gain with struggle"
                            },
                            {
                                "target_lang_word": "extract",
                                "target_lang_sense_clarification": "e.g., a confession or promise"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7565b2c-4091-3652-a406-c11b11c5b91c",
                    "sense_definition": "В тяжёлой атлетике: поднять штангу одним непрерывным движением от помоста на прямые руки.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "forceful_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спортсмен должен <вырвать> штангу весом двести килограммов для получения золотой медали.",
                            "target_lang_translations": {
                                "en": "The athlete must <snatch> the two-hundred-kilogram barbell to win the gold medal."
                            }
                        },
                        {
                            "text": "Он не смог чисто <вырвать> заявленный вес и выбыл из соревнований.",
                            "target_lang_translations": {
                                "en": "He could not cleanly <snatch> the declared weight and dropped out of the competition."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вырвать штангу",
                        "вырвать вес"
                    ],
                    "target_lang_definitions": {
                        "en": "In weightlifting: to lift the barbell in one continuous motion from the platform to locked arms overhead."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "snatch",
                                "target_lang_sense_clarification": "sports terminology"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
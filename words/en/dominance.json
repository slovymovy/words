{
    "word_family": [
        "dominate",
        "dominant",
        "domination",
        "domineering"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7c313264-2dd4-378c-8298-7c9907828e49",
                    "sense_definition": "The condition of having power, authority, or control over others.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "power_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The empire maintained its <w>dominance</w> over the region for more than three centuries.",
                            "target_lang_translations": {
                                "ru": "Империя сохраняла свое <w>господство</w> над регионом более трех веков."
                            }
                        },
                        {
                            "text": "He tried to assert <w>dominance</w> in the meeting by speaking louder than everyone else.",
                            "target_lang_translations": {
                                "ru": "Он пытался утвердить свое <w>доминирование</w> на собрании, говоря громче всех остальных."
                            }
                        }
                    ],
                    "synonyms": [
                        "supremacy",
                        "ascendancy",
                        "control",
                        "mastery",
                        "superiority",
                        "authority"
                    ],
                    "antonyms": [
                        "submission",
                        "subordination",
                        "weakness",
                        "inferiority"
                    ],
                    "common_phrases": [
                        "struggle for dominance",
                        "assert dominance",
                        "military dominance",
                        "political dominance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние обладания властью, авторитетом или контролем над другими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "господство",
                                "target_lang_sense_clarification": "власть, контроль"
                            },
                            {
                                "target_lang_word": "доминирование",
                                "target_lang_sense_clarification": "превосходство в силе или влиянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8da1fa8c-0305-3801-ac73-bec4f37bd19f",
                    "sense_definition": "The state of being more important, successful, or common than others of the same type.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "power_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company's <w>dominance</w> in the smartphone market has challenged all its major competitors.",
                            "target_lang_translations": {
                                "ru": "<w>Доминирование</w> компании на рынке смартфонов бросило вызов всем ее основным конкурентам."
                            }
                        },
                        {
                            "text": "English has achieved a level of global <w>dominance</w> in the world of international business.",
                            "target_lang_translations": {
                                "ru": "Английский язык достиг уровня глобального <w>господства</w> в мире международного бизнеса."
                            }
                        }
                    ],
                    "synonyms": [
                        "prevalence",
                        "prominence",
                        "supremacy",
                        "preeminence"
                    ],
                    "antonyms": [
                        "insignificance",
                        "obscurity"
                    ],
                    "common_phrases": [
                        "market dominance",
                        "global dominance",
                        "economic dominance",
                        "cultural dominance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, когда кто-то или что-то является более важным, успешным или распространенным, чем другие того же типа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доминирование",
                                "target_lang_sense_clarification": "лидирующее положение на рынке или в сфере"
                            },
                            {
                                "target_lang_word": "преобладание",
                                "target_lang_sense_clarification": "численное или качественное превосходство"
                            },
                            {
                                "target_lang_word": "господство",
                                "target_lang_sense_clarification": "полное превосходство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0fa45bd-1fa7-3cab-86ea-57106fc502ef",
                    "sense_definition": "The natural tendency to use one side of the body, such as the right hand or eye, more than the other.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most children show a clear right-hand <w>dominance</w> by the time they start school.",
                            "target_lang_translations": {
                                "ru": "Большинство детей демонстрируют явное <w>доминирование</w> правой руки к моменту начала учебы в школе."
                            }
                        },
                        {
                            "text": "Tests for ocular <w>dominance</w> help determine which eye the brain prefers for visual input.",
                            "target_lang_translations": {
                                "ru": "Тесты на глазное <w>доминирование</w> помогают определить, какой глаз мозг предпочитает для визуального восприятия."
                            }
                        }
                    ],
                    "synonyms": [
                        "laterality"
                    ],
                    "common_phrases": [
                        "hand dominance",
                        "eye dominance",
                        "cerebral dominance"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "biology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественная склонность использовать одну сторону тела (например, правую руку или глаз) больше, чем другую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доминирование",
                                "target_lang_sense_clarification": "преимущественное использование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d92eae07-53a4-3172-ab35-e7a74466ea5e",
                    "sense_definition": "The property of a gene to mask the effect of another gene when both are present.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>dominance</w> of the dark pigment gene ensures that brown eyes are more common than blue.",
                            "target_lang_translations": {
                                "ru": "<w>Доминирование</w> гена темного пигмента гарантирует, что карие глаза встречаются чаще, чем голубые."
                            }
                        },
                        {
                            "text": "Mendel discovered the principles of genetic <w>dominance</w> through his experiments with pea plants.",
                            "target_lang_translations": {
                                "ru": "Мендель открыл принципы генетического <w>доминирования</w> благодаря своим экспериментам с горохом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "incomplete dominance"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genetics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство гена маскировать эффект другого гена при наличии обоих."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доминирование",
                                "target_lang_sense_clarification": "процесс подавления одного гена другим"
                            },
                            {
                                "target_lang_word": "доминантность",
                                "target_lang_sense_clarification": "генетическое свойство"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
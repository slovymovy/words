    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ba15ad90-5b0e-3377-aeb9-193a5b2fd528",
                    "sense_definition": "O porze dnia, ciemności lub ciszy: zaczynać panować, nadchodzić, stawać się widocznym lub odczuwalnym.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_and_phenomena",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lesie szybko <zapadał> zmrok, więc musieliśmy wracać do domu.",
                            "target_lang_translations": {
                                "ru": "В лесу быстро <спускались> сумерки, поэтому нам пришлось возвращаться домой."
                            }
                        },
                        {
                            "text": "Gdy <zapada> noc, wszystkie zwierzęta szukają bezpiecznego schronienia.",
                            "target_lang_translations": {
                                "ru": "Когда <наступает> ночь, все животные ищут безопасное убежище."
                            }
                        }
                    ],
                    "synonyms": [
                        "nadchodzić",
                        "następować",
                        "zachorować"
                    ],
                    "antonyms": [
                        "mijać",
                        "ustępować",
                        "świtać"
                    ],
                    "common_phrases": [
                        "zapada zmrok",
                        "zapada noc",
                        "zapada cisza",
                        "zapada ciemność",
                        "zapada wieczór"
                    ],
                    "target_lang_definitions": {
                        "ru": "О времени суток, темноте или тишине: начинать господствовать, приближаться, становиться видимым или ощутимым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наступать",
                                "target_lang_sense_clarification": "о времени суток, тишине"
                            },
                            {
                                "target_lang_word": "опускаться",
                                "target_lang_sense_clarification": "о темноте, сумерках"
                            },
                            {
                                "target_lang_word": "спускаться",
                                "target_lang_sense_clarification": "о сумерках, ночи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b23db10-3604-30e4-af3e-1befac132d9e",
                    "sense_definition": "Wchodzić głęboko w określony stan fizjologiczny (np. sen) lub utrwalać się w umyśle.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "state_and_mind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego mądre słowa głęboko <zapadały> w pamięć wszystkim słuchaczom.",
                            "target_lang_translations": {
                                "ru": "Его мудрые слова глубоко <западали> в память всем слушателям."
                            }
                        },
                        {
                            "text": "Niedźwiedzie <zapadają> w sen zimowy, aby przetrwać trudne warunki.",
                            "target_lang_translations": {
                                "ru": "Медведи <впадают> в зимнюю спячку, чтобы пережить суровые условия."
                            }
                        }
                    ],
                    "synonyms": [
                        "następować",
                        "zachorować"
                    ],
                    "antonyms": [
                        "budzić się",
                        "wylatywać z głowy"
                    ],
                    "common_phrases": [
                        "zapadać w sen",
                        "zapadać w pamięć",
                        "zapadać w letarg",
                        "zapadać w drzemkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глубоко погружаться в определенное физиологическое состояние (напр., сон) или закрепляться в сознании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "западать",
                                "target_lang_sense_clarification": "в память, в душу"
                            },
                            {
                                "target_lang_word": "погружаться",
                                "target_lang_sense_clarification": "в сон, в раздумья"
                            },
                            {
                                "target_lang_word": "впадать",
                                "target_lang_sense_clarification": "в спячку, в летаргию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d9e48c5-f12f-320c-8fec-12aed805c392",
                    "sense_definition": "O decyzji, wyroku lub postanowieniu: zostać oficjalnie podjętym lub ogłoszonym.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "decisions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej sprawie ostateczny wyrok <zapadał> zazwyczaj po wielu miesiącach.",
                            "target_lang_translations": {
                                "ru": "По этому делу окончательный приговор <выносился> обычно спустя много месяцев."
                            }
                        },
                        {
                            "text": "Ważna decyzja o budowie mostu <zapada> często za zamkniętymi drzwiami.",
                            "target_lang_translations": {
                                "ru": "Важное решение о строительстве моста часто <принимается> за закрытыми дверями."
                            }
                        }
                    ],
                    "synonyms": [
                        "następować"
                    ],
                    "common_phrases": [
                        "zapada decyzja",
                        "zapada wyrok",
                        "klamka zapadła",
                        "zapada postanowienie"
                    ],
                    "target_lang_definitions": {
                        "ru": "О решении, приговоре или постановлении: быть официально принятым или объявленным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приниматься",
                                "target_lang_sense_clarification": "о решении"
                            },
                            {
                                "target_lang_word": "выноситься",
                                "target_lang_sense_clarification": "о приговоре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec58461a-d015-3cbf-918f-5a1bf151f37b",
                    "sense_definition": "Osuwać się w dół, tracić stabilność, zagłębiać się w czymś miękkim lub stawać się wklęsłym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod ciężarem samochodu koła zaczęły <zapadać> w grząskie błoto.",
                            "target_lang_translations": {
                                "ru": "Под тяжестью автомобиля колеса начали <проваливаться> в вязкую грязь."
                            }
                        },
                        {
                            "text": "Stary dach stodoły powoli <zapadał> się do środka pod wpływem wilgoci.",
                            "target_lang_translations": {
                                "ru": "Старая крыша сарая медленно <проваливалась> внутрь под воздействием влаги."
                            }
                        }
                    ],
                    "synonyms": [
                        "osuwać się",
                        "opadać",
                        "zachorować"
                    ],
                    "antonyms": [
                        "wznosić się",
                        "wyłaniać się"
                    ],
                    "common_phrases": [
                        "zapadać się pod ziemię",
                        "zapadać w błoto",
                        "zapadać się w sobie",
                        "policzki zapadają"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опускаться вниз, терять устойчивость, углубляться во что-то мягкое или становиться вогнутым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проваливаться",
                                "target_lang_sense_clarification": "обрушиваться, уходить под землю"
                            },
                            {
                                "target_lang_word": "вязнуть",
                                "target_lang_sense_clarification": "погружаться в грязь"
                            },
                            {
                                "target_lang_word": "вваливаться",
                                "target_lang_sense_clarification": "о щеках, глазах"
                            },
                            {
                                "target_lang_word": "оседать",
                                "target_lang_sense_clarification": "о постройках, крыше"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e50353b3-8850-3613-a1e4-28706a3d5e61",
                    "sense_definition": "Zaczynać chorować na jakąś chorobę, zwykle poważną lub przewlekłą.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W okresie jesiennym wiele osób <zapada> na infekcje górnych dróg oddechowych.",
                            "target_lang_translations": {
                                "ru": "В осенний период многие люди <заболевают> инфекциями верхних дыхательных путей."
                            }
                        },
                        {
                            "text": "Mimo dbania o siebie, nagle <zapadał> na zdrowiu i musiał iść do szpitala.",
                            "target_lang_translations": {
                                "ru": "Несмотря на заботу о себе, он внезапно <занемог> и был вынужден лечь в больницу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zachorować",
                        "zachorować"
                    ],
                    "antonyms": [
                        "zdrowieć"
                    ],
                    "common_phrases": [
                        "zapadać na zdrowiu",
                        "zapadać na grypę",
                        "zapadać na zapalenie płuc"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать болеть какой-либо болезнью, обычно серьезной или хронической."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заболевать",
                                "target_lang_sense_clarification": "становиться больным"
                            },
                            {
                                "target_lang_word": "занемогать",
                                "target_lang_sense_clarification": "устар. чувствовать недомогание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c250f6db-d0ab-3353-9048-c0eaf44d281f",
                    "sense_definition": "O ptakach: lądować w zaroślach na odpoczynek; o ludziach: ukrywać się w bezpiecznym miejscu.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "hiding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Partyzanci musieli <zapadać> w głęboki las, by uniknąć pościgu wroga.",
                            "target_lang_translations": {
                                "ru": "Партизаны должны были <скрываться> в глубоком лесу, чтобы избежать преследования врага."
                            }
                        },
                        {
                            "text": "Zmęczone ptaki zaczęły <zapadać> w trzciny na nocny spoczynek.",
                            "target_lang_translations": {
                                "ru": "Уставшие птицы начали <опускаться> в тростник на ночной отдых."
                            }
                        }
                    ],
                    "synonyms": [
                        "zachorować"
                    ],
                    "common_phrases": [
                        "zapadać w las",
                        "zapadać w bór"
                    ],
                    "target_lang_definitions": {
                        "ru": "О птицах: приземляться в заросли для отдыха; о людях: прятаться в безопасном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрываться",
                                "target_lang_sense_clarification": "прятаться"
                            },
                            {
                                "target_lang_word": "укрываться",
                                "target_lang_sense_clarification": "находить убежище"
                            },
                            {
                                "target_lang_word": "опускаться",
                                "target_lang_sense_clarification": "о птицах"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "55f0b7d6-c670-35d7-a355-15fa3197eb79",
                    "sense_definition": "Filled with intense delight, joy, or fascination.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The audience sat motionless, completely <w>enraptured</w> by the beautiful music.",
                            "target_lang_translations": {
                                "ru": "Зрители сидели неподвижно, совершенно <w>заворожённые</w> прекрасной музыкой."
                            }
                        },
                        {
                            "text": "She listened to the story with an <w>enraptured</w> expression on her face.",
                            "target_lang_translations": {
                                "ru": "Она слушала историю с <w>восхищённым</w> выражением лица."
                            }
                        }
                    ],
                    "synonyms": [
                        "entranced",
                        "spellbound",
                        "rapt"
                    ],
                    "antonyms": [
                        "bored",
                        "unimpressed",
                        "indifferent"
                    ],
                    "common_phrases": [
                        "enraptured by",
                        "enraptured audience",
                        "enraptured gaze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наполненный сильным восторгом, радостью или очарованием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восхищённый",
                                "target_lang_sense_clarification": "испытывающий или выражающий сильное восхищение"
                            },
                            {
                                "target_lang_word": "очарованный",
                                "target_lang_sense_clarification": "находящийся под сильным впечатлением, пленённый чем-либо"
                            },
                            {
                                "target_lang_word": "заворожённый",
                                "target_lang_sense_clarification": "находящийся в состоянии оцепенения от восторга или интереса"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [

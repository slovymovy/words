                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To mistakenly take one thing for another or confuse them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mix up",
                                "target_lang_sense_clarification": "mistake one for another"
                            },
                            {
                                "target_lang_word": "confuse",
                                "target_lang_sense_clarification": "fail to distinguish"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "846c337f-b23d-3d8b-8af5-b56b0674a6ff",
                    "sense_definition": "Нарушить порядок расположения чего-либо, привести в беспорядок.",

            "senses": [
                {
                    "sense_id": "a904e6a8-e8df-369d-9b4c-8ed8a29345bd",
                    "sense_definition": "De groep die meer dan de helft van het totaal vormt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>meerderheid</w> van de stemmen was voor het nieuwe plan.",
                            "target_lang_translations": {
                                "en": "The <w>majority</w> of the votes were in favor of the new plan.",
                                "ru": "<w>Большинство</w> голосов было за новый план."
                            }
                        },
                        {
                            "text": "In deze klas is de <w>meerderheid</w> van de leerlingen het eens met de leraar.",
                            "target_lang_translations": {
                                "en": "In this class, the <w>majority</w> of the students agree with the teacher.",
                                "ru": "В этом классе <w>большинство</w> учеников согласно с учителем."
                            }
                        }
                    ],
                    "synonyms": [
                        "merendeel",
                        "gros"
                    ],
                    "antonyms": [
                        "minderheid"
                    ],
                    "common_phrases": [
                        "absolute meerderheid",
                        "in de meerderheid zijn",
                        "de zwijgende meerderheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The group that forms more than half of the total.",
                        "ru": "Группа, составляющая более половины от общего числа."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "majority",
                                "target_lang_sense_clarification": "more than half of a group"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "большинство",
                                "target_lang_sense_clarification": "большая часть группы или населения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ef43b3f-a4ce-3bbe-ae50-3ea34835325c",
                    "sense_definition": "Het sterker, beter of machtiger zijn dan anderen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij bewees zijn <w>meerderheid</w> in kennis en ervaring tijdens het debat.",
                            "target_lang_translations": {
                                "en": "He proved his <w>superiority</w> in knowledge and experience during the debate.",
                                "ru": "Он доказал свое <w>превосходство</w> в знаниях и опыте во время дебатов."
                            }
                        },
                        {
                            "text": "Het leger moest zich terugtrekken vanwege de <w>meerderheid</w> van de vijand.",
                            "target_lang_translations": {
                                "en": "The army had to retreat due to the <w>superiority</w> of the enemy.",
                                "ru": "Армии пришлось отступить из-за <w>численного перевеса</w> врага."
                            }
                        }
                    ],
                    "synonyms": [
                        "overwicht",
                        "superioriteit"
                    ],
                    "antonyms": [
                        "minderheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of being stronger, better, or more powerful than others.",
                        "ru": "Состояние, когда кто-то сильнее, лучше или могущественнее других."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "superiority",

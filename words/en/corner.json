{
    "word_family": [
        "cornerstone",
        "cornerback",
        "cornerwise"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "542397ff-05dd-3f0b-88e6-81eb406d5434",
                    "sense_definition": "The point where two converging lines meet; an angle, either external or internal.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bumped her knee on the sharp <w>corner</w> of the table.",
                            "target_lang_translations": {
                                "ru": "Она ударилась коленом об острый <w>угол</w> стола."
                            }
                        },
                        {
                            "text": "There is a comfortable chair in the <w>corner</w> of the room.",
                            "target_lang_translations": {
                                "ru": "В <w>углу</w> комнаты стоит удобное кресло."
                            }
                        }
                    ],
                    "synonyms": [
                        "angle"
                    ],
                    "common_phrases": [
                        "in the corner",
                        "sharp corner",
                        "top corner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точка, где сходятся две линии; угол, внешний или внутренний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угол",
                                "target_lang_sense_clarification": "геометрический или в помещении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66f77de1-1d9a-3e9c-bbc4-9d21f249ce66",
                    "sense_definition": "An intersection of two streets; any of the four outer points off the street at that intersection.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will meet you at the <w>corner</w> of Main Street and First Avenue.",
                            "target_lang_translations": {
                                "ru": "Я встречу тебя на <w>углу</w> Мэйн-стрит и Первой авеню."
                            }
                        },
                        {
                            "text": "The bakery on the <w>corner</w> sells delicious bread.",
                            "target_lang_translations": {
                                "ru": "Пекарня на <w>углу</w> продает вкусный хлеб."
                            }
                        }
                    ],
                    "common_phrases": [
                        "street corner",
                        "on the corner",
                        "round the corner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пересечение двух улиц; любое из четырех внешних мест у этого пересечения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угол",
                                "target_lang_sense_clarification": "улицы"
                            },
                            {
                                "target_lang_word": "перекрёсток",
                                "target_lang_sense_clarification": "место пересечения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "005f04c7-e827-3fa9-8558-4e895518140c",
                    "sense_definition": "A corner kick in soccer.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_soccer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The referee awarded a <w>corner</w> to the home team after the save.",
                            "target_lang_translations": {
                                "ru": "Судья назначил <w>угловой</w> в пользу хозяев после сейва."
                            }
                        },
                        {
                            "text": "He headed the ball into the goal directly from the <w>corner</w>.",
                            "target_lang_translations": {
                                "ru": "Он забил мяч головой в ворота прямо с <w>углового</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "take a corner",
                        "win a corner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Угловой удар в футболе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угловой",
                                "target_lang_sense_clarification": "спорт"
                            },
                            {
                                "target_lang_word": "корнер",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9617a88b-210a-36aa-825c-42e6632181d4",
                    "sense_definition": "A secret or secluded place; a remote or out of the way place.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "seclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He found a quiet <w>corner</w> in the library to study for his exam.",
                            "target_lang_translations": {
                                "ru": "Он нашел тихий <w>уголок</w> в библиотеке, чтобы подготовиться к экзамену."
                            }
                        },
                        {
                            "text": "The cat likes to sleep in a warm <w>corner</w> behind the sofa.",
                            "target_lang_translations": {
                                "ru": "Кошка любит спать в теплом <w>уголке</w> за диваном."
                            }
                        }
                    ],
                    "synonyms": [
                        "nook",
                        "recess",
                        "hideaway",
                        "cranny"
                    ],
                    "common_phrases": [
                        "quiet corner",
                        "dark corner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Секретное или уединенное место; отдаленное место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уголок",
                                "target_lang_sense_clarification": "укромное место"
                            },
                            {
                                "target_lang_word": "закуток",
                                "target_lang_sense_clarification": "тесное или укромное место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6721e20-1139-3a01-b6fd-c54cd77f5104",
                    "sense_definition": "An edge or extremity; the part farthest from the center.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "region",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "People traveled from every <w>corner</w> of the globe to attend the festival.",
                            "target_lang_translations": {
                                "ru": "Люди приехали со всех <w>уголков</w> земного шара, чтобы посетить фестиваль."
                            }
                        },
                        {
                            "text": "They searched every <w>corner</w> of the house but could not find the keys.",
                            "target_lang_translations": {
                                "ru": "Они обыскали каждый <w>уголок</w> дома, но не смогли найти ключи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "four corners of the earth",
                        "every corner of the globe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Край или отдаленная часть; часть, наиболее удаленная от центра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уголок",
                                "target_lang_sense_clarification": "часть мира, страны"
                            },
                            {
                                "target_lang_word": "край",
                                "target_lang_sense_clarification": "отдаленная местность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fccb5fef-ac0c-3794-bdec-9f3372fe2cb3",
                    "sense_definition": "An embarrassing situation; a difficulty.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "difficulty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His constant lies have finally put him in a tight <w>corner</w>.",
                            "target_lang_translations": {
                                "ru": "Его постоянная ложь наконец загнала его в <w>тупик</w>."
                            }
                        },
                        {
                            "text": "She felt backed into a <w>corner</w> when they asked for her resignation.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала себя загнанной в <w>угол</w>, когда они потребовали её отставки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tight corner",
                        "in a corner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неловкая ситуация; затруднительное положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тупик",
                                "target_lang_sense_clarification": "безвыходное положение"
                            },
                            {
                                "target_lang_word": "угол",
                                "target_lang_sense_clarification": "в идиомах (загнать в угол)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91f39d55-32d5-3fcd-970c-3e97c3ad31b2",
                    "sense_definition": "The corner of the ring, which is where the boxer rests before and during a fight.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports_boxing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boxer returned to his <w>corner</w> to rest between rounds.",
                            "target_lang_translations": {
                                "ru": "Боксер вернулся в свой <w>угол</w>, чтобы отдохнуть между раундами."
                            }
                        },
                        {
                            "text": "His coach shouted instructions from the <w>corner</w> during the match.",
                            "target_lang_translations": {
                                "ru": "Его тренер выкрикивал инструкции из <w>угла</w> во время боя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in the red corner",
                        "in the blue corner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Угол ринга, где боксер отдыхает до и во время боя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угол",
                                "target_lang_sense_clarification": "ринга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8500eeb-39b1-3d3d-995a-c54d4555ea65",
                    "sense_definition": "A sufficient interest in a salable security or commodity to allow the cornering party to influence prices.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the 1970s, private investors tried to get a <w>corner</w> on the silver market.",
                            "target_lang_translations": {
                                "ru": "В 1970-х частные инвесторы пытались осуществить <w>скупку</w> рынка серебра."
                            }
                        },
                        {
                            "text": "Having a <w>corner</w> on the market is often illegal in modern finance.",
                            "target_lang_translations": {
                                "ru": "Наличие <w>монополии</w> на рынке часто является незаконным в современных финансах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "corner on the market"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достаточный контроль над ценными бумагами или товаром, позволяющий влиять на цены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скупка",
                                "target_lang_sense_clarification": "биржевая"
                            },
                            {
                                "target_lang_word": "монополия",
                                "target_lang_sense_clarification": "контроль рынка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "670f4f1d-9107-3346-800d-86b5f9460385",
                    "sense_definition": "To turn a corner or drive around a curve.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The race car <w>cornered</w> the sharp bend at high speed.",
                            "target_lang_translations": {
                                "ru": "Гоночный автомобиль <w>вошел в</w> крутой <w>поворот</w> на высокой скорости."
                            }
                        },
                        {
                            "text": "He <w>cornered</w> carefully because the road was wet and slippery.",
                            "target_lang_translations": {
                                "ru": "Он <w>поворачивал</w> осторожно, потому что дорога была мокрой и скользкой."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повернуть за угол или проехать поворот."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поворачивать",
                                "target_lang_sense_clarification": "менять направление"
                            },
                            {
                                "target_lang_word": "входить в поворот",
                                "target_lang_sense_clarification": "об автомобиле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "faf45069-25c4-32db-9c54-b6dc573ef14d",
                    "sense_definition": "To drive someone or something into a corner or other confined space.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "trap_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police <w>cornered</w> the thief in a dead-end alley.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>загнала</w> вора <w>в угол</w> в тупиковом переулке."
                            }
                        },
                        {
                            "text": "Our cat <w>cornered</w> a mouse behind the kitchen cabinet.",
                            "target_lang_translations": {
                                "ru": "Наша кошка <w>загнала</w> мышь <w>в угол</w> за кухонным шкафом."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Загнать кого-то или что-то в угол или другое замкнутое пространство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загонять в угол",
                                "target_lang_sense_clarification": "физически"
                            },
                            {
                                "target_lang_word": "припирать к стене",
                                "target_lang_sense_clarification": "лишать путей отхода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7665af7-9d1a-36d1-8f69-9f3d019a5cbc",
                    "sense_definition": "To trap in a position of great difficulty or hopeless embarrassment.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "trap_metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The reporter <w>cornered</w> the politician with questions about the scandal.",
                            "target_lang_translations": {
                                "ru": "Репортер <w>загнал</w> политика <w>в угол</w> вопросами о скандале."
                            }
                        },
                        {
                            "text": "Do not let them <w>corner</w> you into agreeing to something you dislike.",
                            "target_lang_translations": {
                                "ru": "Не позволяй им <w>загнать</w> тебя <w>в угол</w> и заставить согласиться на то, что тебе не нравится."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поставить в крайне затруднительное или безнадежное положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загонять в угол",
                                "target_lang_sense_clarification": "метафорически"
                            },
                            {
                                "target_lang_word": "ставить в тупик",
                                "target_lang_sense_clarification": "озадачивать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3449fd52-d08c-38db-a362-e689dfa6db38",
                    "sense_definition": "To handle while moving around a corner in a road or otherwise turning.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "handling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That sports car <w>corners</w> incredibly well on winding roads.",
                            "target_lang_translations": {
                                "ru": "Этот спорткар невероятно хорошо <w>входит в повороты</w> на извилистых дорогах."
                            }
                        },
                        {
                            "text": "The heavy truck <w>corners</w> poorly and needs to slow down significantly.",
                            "target_lang_translations": {
                                "ru": "Тяжелый грузовик плохо <w>входит в повороты</w>, и ему нужно значительно снижать скорость."
                            }
                        }
                    ],
                    "common_phrases": [
                        "corner well",
                        "corner poorly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Управляться при движении на повороте (об автомобиле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "входить в поворот",
                                "target_lang_sense_clarification": "держать дорогу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0124fb0-1703-370d-9ac1-290b4ee467f0",
                    "sense_definition": "To get sufficient command of a stock or commodity so as to be able to manipulate its price.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is extremely hard to <w>corner</w> the petroleum market because there are so many players.",
                            "target_lang_translations": {
                                "ru": "Крайне сложно <w>монополизировать</w> рынок нефти, потому что там так много игроков."
                            }
                        },
                        {
                            "text": "The wealthy investors attempted to <w>corner</w> the market on wheat.",
                            "target_lang_translations": {
                                "ru": "Богатые инвесторы попытались <w>скупить</w> рынок пшеницы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "corner the market"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить достаточный контроль над акциями или товаром для манипулирования ценой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скупать",
                                "target_lang_sense_clarification": "для монополии"
                            },
                            {
                                "target_lang_word": "монополизировать",
                                "target_lang_sense_clarification": "рынок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "8dcca0d0-95da-34e5-9851-083931637d23",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Corner</w> has been working at the firm for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Корнер</w> работает в фирме уже двадцать лет."
                            }
                        },
                        {
                            "text": "I met a woman named Sarah <w>Corner</w> at the conference.",
                            "target_lang_translations": {
                                "ru": "Я встретил женщину по имени Сара <w>Корнер</w> на конференции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "A surname."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Корнер",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "34ef27c0-db71-3b9c-9883-c71fc912d6c9",
                    "sense_definition": "Spoken by service staff such as waiters when walking around a corner, to warn other staff and prevent a collision.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "warning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The waiter shouted \"<w>Corner</w>!\" as he carried the heavy tray into the kitchen.",
                            "target_lang_translations": {
                                "ru": "Официант крикнул: «<w>Угол</w>!», когда нес тяжелый поднос на кухню."
                            }
                        },
                        {
                            "text": "You should always say \"<w>Corner</w>!\" when moving through the busy restaurant.",
                            "target_lang_translations": {
                                "ru": "Вам следует всегда говорить «<w>Угол</w>!», когда вы передвигаетесь по оживленному ресторану."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Произносится обслуживающим персоналом при повороте за угол, чтобы предупредить столкновение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Угол!",
                                "target_lang_sense_clarification": "возглас"
                            },
                            {
                                "target_lang_word": "Осторожно!",
                                "target_lang_sense_clarification": "предупреждение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
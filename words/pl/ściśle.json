                            "target_lang_translations": {
                                "ru": "Следует <w>точно</w> определить цель наших исследований."
                            }
                        }
                    ],
                    "synonyms": [
                        "dokładnie",
                        "precyzyjnie"
                    ],
                    "antonyms": [

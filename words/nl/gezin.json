                    ],
                    "common_phrases": [
                        "een groot gezin",
                        "een jong gezin",
                        "gezin stichten",
                        "hoofd van het gezin"
                    ],
                    "target_lang_definitions": {
                        "en": "A household consisting of one or more parents and their children."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "family",
                                "target_lang_sense_clarification": "nuclear family unit"
                            }
                        ]

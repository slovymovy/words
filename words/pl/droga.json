        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "efc5f97d-5fbc-35cb-b8c9-8ec71c2ab7fd",
                    "sense_definition": "Pas terenu przeznaczony do ruchu pojazdów, ludzi i zwierząt, często utwardzony.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_infrastructure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta <droga> prowadzi prosto do centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Эта <дорога> ведёт прямо в центр города."
                            }
                        },
                        {
                            "text": "Musimy uważać, bo <droga> jest bardzo śliska po deszczu.",
                            "target_lang_translations": {
                                "ru": "Мы должны быть осторожны, так как <дорога> очень скользкая после дождя."
                            }
                        }
                    ],
                    "synonyms": [
                        "trasa",
                        "ścieżka",
                        "tor",
                        "marszruta",
                        "trasa"
                    ],
                    "antonyms": [
                        "bezdroże"
                    ],
                    "common_phrases": [
                        "polna droga",
                        "droga główna",
                        "przejść przez drogę",
                        "ustąpić drogi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полоса земли, предназначенная для движения транспорта, людей и животных, часто с твёрдым покрытием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дорога",
                                "target_lang_sense_clarification": "полоса для движения"
                            },
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "маршрут, направление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97c0e0ce-7b14-3f69-a2ae-5bf025e65a0c",
                    "sense_definition": "Czynność przemieszczania się z jednego miejsca do drugiego; podróż.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała <droga> nad morze zajęła nam pięć godzin.",
                            "target_lang_translations": {
                                "ru": "Вся <дорога> к морю заняла у нас пять часов."
                            }
                        },
                        {
                            "text": "Życzę ci szerokiej <drogi> i bezpiecznego powrotu do domu.",
                            "target_lang_translations": {
                                "ru": "Желаю тебе счастливого <пути> и безопасного возвращения домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "podróż",
                        "wędrówka"
                    ],
                    "common_phrases": [
                        "być w drodze",
                        "ruszać w drogę",
                        "szerokiej drogi",
                        "zmęczony drogą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по перемещению из одного места в другое; путешествие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дорога",
                                "target_lang_sense_clarification": "путешествие, поездка"
                            },
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "процесс перемещения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4aa34763-a35c-36cc-814c-ad510015ab81",
                    "sense_definition": "Sposób postępowania lub przebieg zdarzeń prowadzący do osiągnięcia określonego celu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "method_metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciężka praca to jedyna pewna <droga> do sukcesu.",
                            "target_lang_translations": {
                                "ru": "Тяжёлый труд — это единственный верный <путь> к успеху."
                            }
                        },
                        {
                            "text": "Musimy znaleźć inną <drogę> wyjścia z tej trudnej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Мы должны найти другой <путь> выхода из этой сложной ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "wędrówka"
                    ],
                    "common_phrases": [
                        "droga do celu",
                        "na dobrej drodze",
                        "droga służbowa",
                        "droga sądowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ действий или последовательность событий, ведущие к достижению определённой цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "способ достижения цели"
                            },
                            {
                                "target_lang_word": "дорога",
                                "target_lang_sense_clarification": "направление развития"

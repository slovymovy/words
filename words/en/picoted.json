            "senses": [
                {
                    "sense_id": "d7d97492-6250-350d-a47f-2d82fbfa87e6",
                    "sense_definition": "Simple past and past participle of <w>picot</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>picoted</w> the hem of the dress to give it a fancy finish.",
                            "target_lang_translations": {

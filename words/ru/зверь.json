                        }
                    ],
                    "common_phrases": [
                        "смотреть зверем",
                        "лютый зверь",
                        "настоящий зверь"
                    ],
                    "target_lang_definitions": {
                        "en": "A cruel, ferocious, or very rude person."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beast",
                                "target_lang_sense_clarification": "a cruel or violent person"
                            },
                            {
                                "target_lang_word": "animal",
                                "target_lang_sense_clarification": "an uncivilized or brutish person"
                            },
                            {
                                "target_lang_word": "brute",
                                "target_lang_sense_clarification": "a savagely violent person"
                            }
                        ]

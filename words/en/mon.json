    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9d3d386e-7b37-3852-8e3e-9024f00a8726",
                    "sense_definition": "Abbreviation of <w>Monday</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shop is open from <w>Mon</w> to Fri every week.",
                            "target_lang_translations": {
                                "ru": "Магазин открыт с <w>пн.</w> по пт. каждую неделю."
                            }
                        },
                        {
                            "text": "Please submit your report by next <w>Mon</w> morning.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отправьте отчет к утру следующего <w>понедельника</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on Mon",
                        "next Mon"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от слова \"понедельник\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пн.",
                                "target_lang_sense_clarification": "сокращение"
                            },
                            {
                                "target_lang_word": "пон.",
                                "target_lang_sense_clarification": "сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a9f07c0-8695-300a-af57-20d346cd62e2",
                    "sense_definition": "A member of a people living primarily in the Mon State of Myanmar (Burma) and in Thailand.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mon_ethnicity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Mon</w> have a long history and rich culture in Southeast Asia.",
                            "target_lang_translations": {
                                "ru": "<w>Моны</w> имеют долгую историю и богатую культуру в Юго-Восточной Азии."
                            }
                        },
                        {
                            "text": "Many <w>Mon</w> migrated to Thailand over the centuries.",
                            "target_lang_translations": {
                                "ru": "Многие <w>моны</w> мигрировали в Таиланд на протяжении веков."
                            }
                        }
                    ],
                    "synonyms": [
                        "Talaing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель народа мон, проживающего преимущественно в Мьянме и Таиланде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "моны",
                                "target_lang_sense_clarification": "народ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e1d3b5f-de8b-3db6-8b7b-8862414e2d67",
                    "sense_definition": "A creature in a video game, usually one which is captured, trained, and used in battles.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I spent hours leveling up my favorite <w>mon</w> for the tournament.",
                            "target_lang_translations": {
                                "ru": "Я потратил часы на прокачку своего любимого <w>мона</w> для турнира."
                            }
                        },
                        {
                            "text": "Each <w>mon</w> has unique abilities and weaknesses.",
                            "target_lang_translations": {
                                "ru": "У каждого <w>мона</w> есть уникальные способности и слабости."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gaming"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Существо в видеоигре, которое обычно ловят, тренируют и используют в битвах (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мон",
                                "target_lang_sense_clarification": "игровой сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "380062a2-38ec-374f-bc57-2b4791a21fc7",
                    "sense_definition": "Man; a form of address used in dialects such as Jamaican English or Northern English.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hey <w>mon</w>, everything is going to be alright.",
                            "target_lang_translations": {
                                "ru": "Эй, <w>чувак</w>, всё будет хорошо."
                            }
                        },
                        {
                            "text": "He is a good <w>mon</w> who always helps his neighbors.",
                            "target_lang_translations": {
                                "ru": "Он хороший <w>человек</w>, который всегда помогает соседям."
                            }
                        }
                    ],
                    "common_phrases": [
                        "yeah mon"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Jamaican, Northern English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина; форма обращения в диалектах, таких как ямайский английский."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувак",
                                "target_lang_sense_clarification": "обращение"
                            },
                            {
                                "target_lang_word": "человек",
                                "target_lang_sense_clarification": "мужчина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef45daaa-053c-307a-b39b-49a3ac4943cd",
                    "sense_definition": "The badge or emblem of a Japanese family, typically circular and consisting of stylized forms from nature.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "japanese_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The samurai wore a kimono embroidered with his family's <w>mon</w>.",
                            "target_lang_translations": {
                                "ru": "Самурай носил кимоно с вышитым на нем фамильным <w>моном</w>."
                            }
                        },
                        {
                            "text": "You can identify the clan by the design of the <w>mon</w> on their banner.",
                            "target_lang_translations": {
                                "ru": "Вы можете определить клан по дизайну <w>мона</w> на их знамени."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Эмблема или герб японского клана, обычно круглой формы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мон",
                                "target_lang_sense_clarification": "геральдика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91dab91f-112c-300d-b321-b8f4813200b2",
                    "sense_definition": "The former currency of Japan used until 1870.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "japanese_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The merchant charged five <w>mon</w> for the bag of rice.",
                            "target_lang_translations": {
                                "ru": "Торговец запросил пять <w>монов</w> за мешок риса."
                            }
                        },
                        {
                            "text": "Old coins denominated in <w>mon</w> are now collectors' items.",
                            "target_lang_translations": {
                                "ru": "Старинные монеты номиналом в <w>мон</w> сейчас являются предметами коллекционирования."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": "historical currency"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная японская денежная единица, использовавшаяся до 1870 года."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мон",
                                "target_lang_sense_clarification": "валюта"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7920a454-520e-389f-91c2-9c7e3f91c1cd",
                    "sense_definition": "A diminutive of the female given name <w>Monica</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Hey <w>Mon</w>, do you want to grab some coffee later?",
                            "target_lang_translations": {
                                "ru": "Эй, <w>Мон</w>, не хочешь выпить кофе попозже?"
                            }
                        },
                        {
                            "text": "I asked <w>Mon</w> if she had seen my old dress.",
                            "target_lang_translations": {
                                "ru": "Я спросил <w>Мон</w>, не видела ли она мое старое платье."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма женского имени Моника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мон",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ceca1933-d2dd-3748-a135-8a4a603a7411",
                    "sense_definition": "The Austroasiatic language spoken by the Mon people.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mon_ethnicity",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "She is studying <w>Mon</w> to understand the ancient inscriptions.",
                            "target_lang_translations": {
                                "ru": "Она изучает <w>монский язык</w>, чтобы понимать древние надписи."
                            }
                        },
                        {
                            "text": "There are several dialects of <w>Mon</w> spoken in the region.",
                            "target_lang_translations": {
                                "ru": "В регионе существует несколько диалектов <w>монского</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Австроазиатский язык, на котором говорит народ мон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монский язык",
                                "target_lang_sense_clarification": "язык"
                            },
                            {
                                "target_lang_word": "монский",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "e91c7e30-37ab-38f9-86ad-c838024cfce3",
                    "sense_definition": "Come on; used as an encouragement, challenge, or part of a chant.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "'<w>Mon</w> the team!' shouted the fans from the stands.",
                            "target_lang_translations": {
                                "ru": "'<w>Давай</w>, команда!' — кричали болельщики с трибун."
                            }
                        },
                        {
                            "text": "<w>Mon</w> then, let's see if you can do it.",
                            "target_lang_translations": {
                                "ru": "<w>Давай</w> же, посмотрим, сможешь ли ты это сделать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mon then"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Scottish, Irish"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "\"Давай\"; используется как поощрение, вызов или часть кричалки (диалектное сокращение от \"come on\")."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "давай",
                                "target_lang_sense_clarification": "побуждение"
                            },
                            {
                                "target_lang_word": "вперед",
                                "target_lang_sense_clarification": "возглас"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "13061009-2f17-3f3e-982f-b1304a6cc633",
                    "sense_definition": "A small device, usually made of metal or plastic, used for holding things together or attaching one object to another.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fastening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Use this <w>clip</w> to keep the papers organized.",
                            "target_lang_translations": {
                                "ru": "Используй эту <w>скрепку</w>, чтобы привести бумаги в порядок."
                            }
                        },
                        {
                            "text": "She put a silver <w>clip</w> in her hair to hold it back.",
                            "target_lang_translations": {
                                "ru": "Она заколола волосы серебряным <w>зажимом</w>, чтобы убрать их назад."
                            }
                        }
                    ],
                    "synonyms": [
                        "clip",
                        "clip",
                        "clip"
                    ],
                    "common_phrases": [
                        "paper clip",
                        "hair clip",
                        "bulldog clip",
                        "binder clip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое приспособление, обычно из металла или пластика, используемое для скрепления вещей или прикрепления одного предмета к другому."
                    },
                    "translations": {

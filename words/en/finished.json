                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I won't be <w>finished</w> until just before lunch.",
                            "target_lang_translations": {
                                "ru": "Я не <w>закончу</w> до самого обеда."
                            }
                        },
                        {
                            "text": "The program was finally <w>finished</w> after three hours.",
                            "target_lang_translations": {
                                "ru": "Программа была наконец <w>завершена</w> спустя три часа."
                            }
                        }
                    ],
                    "synonyms": [
                        "done",
                        "completed",
                        "concluded",
                        "ended",
                        "over",
                        "complete"
                    ],
                    "antonyms": [

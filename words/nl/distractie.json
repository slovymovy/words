    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "22c316f4-a86f-31be-8dcc-1ed3110485f7",
                    "sense_definition": "Een gebeurtenis of activiteit die de gedachten op iets anders richt, vaak ter ontspanning.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een week hard werken was het feestje een welkome <w>distractie</w>.",
                            "target_lang_translations": {
                                "en": "After a week of hard work, the party was a welcome <w>distraction</w>.",
                                "ru": "После недели тяжелой работы вечеринка стала желанным <w>развлечением</w>."
                            }
                        },
                        {
                            "text": "Hij zocht wat <w>distractie</w> in het lezen van een spannend boek.",
                            "target_lang_translations": {
                                "en": "He sought some <w>diversion</w> in reading an exciting book.",
                                "ru": "Он искал <w>отвлечения</w> в чтении захватывающей книги."
                            }
                        }
                    ],
                    "synonyms": [
                        "afleiding",
                        "verzetje",
                        "ontspanning"
                    ],
                    "common_phrases": [
                        "zoeken naar distractie",
                        "een welkome distractie"
                    ],
                    "target_lang_definitions": {
                        "en": "An event or activity that directs thoughts elsewhere, often for relaxation.",
                        "ru": "Событие или занятие, которое переключает мысли на что-то другое, часто для расслабления."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "distraction",
                                "target_lang_sense_clarification": "something that diverts attention or entertains"
                            },
                            {
                                "target_lang_word": "diversion",
                                "target_lang_sense_clarification": "activity that relaxes or entertains"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "развлечение",
                                "target_lang_sense_clarification": "приятное времяпрепровождение"
                            },
                            {
                                "target_lang_word": "отвлечение",
                                "target_lang_sense_clarification": "смена фокуса внимания"
                            },
                            {
                                "target_lang_word": "отдушина",
                                "target_lang_sense_clarification": "средство успокоения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7ce9df3-299f-3e1d-ab3e-7a97f23c1263",
                    "sense_definition": "Een toestand van geestelijke afwezigheid waardoor men geen aandacht heeft voor de directe omgeving.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In een moment van <w>distractie</w> vergat hij zijn sleutels mee te nemen.",
                            "target_lang_translations": {
                                "en": "In a moment of <w>distraction</w>, he forgot to take his keys.",
                                "ru": "В момент <w>рассеянности</w> он забыл взять с собой ключи."
                            }
                        },
                        {
                            "text": "Haar fout was duidelijk het gevolg van tijdelijke <w>distractie</w>.",
                            "target_lang_translations": {
                                "en": "Her mistake was clearly the result of temporary <w>absent-mindedness</w>.",
                                "ru": "Её ошибка явно была следствием временной <w>рассеянности</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verstrooidheid",
                        "onoplettendheid",
                        "afwezigheid"
                    ],
                    "antonyms": [
                        "aandacht",
                        "oplettendheid",
                        "concentratie"
                    ],
                    "common_phrases": [
                        "moment van distractie"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of mental absence resulting in a lack of attention to the immediate surroundings.",
                        "ru": "Состояние мысленного отсутствия, приводящее к невнимательности к окружающему."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "absent-mindedness",
                                "target_lang_sense_clarification": "state of being lost in thought"
                            },
                            {
                                "target_lang_word": "distraction",
                                "target_lang_sense_clarification": "lack of concentration"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рассеянность",
                                "target_lang_sense_clarification": "отсутствие собранности"
                            },
                            {
                                "target_lang_word": "невнимательность",
                                "target_lang_sense_clarification": "отсутствие внимания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c6bde8b-94a1-3cb8-ab6e-d9a86cb320b6",
                    "sense_definition": "De overdracht van een deel van de opbrengst of gelden.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De notariële akte maakte melding van de <w>distractie</w> van de kosten.",
                            "target_lang_translations": {
                                "en": "The notarial deed mentioned the <w>diversion</w> of the costs.",
                                "ru": "В нотариальном акте упоминалось об <w>отчислении</w> расходов."
                            }
                        },
                        {
                            "text": "Er vond een <w>distractie</w> van de winst plaats ten gunste van de schuldeisers.",
                            "target_lang_translations": {
                                "en": "A <w>diversion</w> of the profits took place in favor of the creditors.",
                                "ru": "Произошло <w>изъятие</w> прибыли в пользу кредиторов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The transfer of a part of proceeds or funds.",
                        "ru": "Передача или удержание части доходов или денежных средств (юридический термин)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "diversion",
                                "target_lang_sense_clarification": "redirecting of funds"
                            },
                            {
                                "target_lang_word": "allocation",
                                "target_lang_sense_clarification": "assignment of funds"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "изъятие",
                                "target_lang_sense_clarification": "отчуждение средств"
                            },
                            {
                                "target_lang_word": "отчисление",
                                "target_lang_sense_clarification": "перевод части суммы"
                            },
                            {
                                "target_lang_word": "отвлечение",
                                "target_lang_sense_clarification": "использование средств на другие цели"
                            }
                        ]
                    }
                }
            ]
        }
    ]

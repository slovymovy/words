    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "43987e02-74ed-3f37-aeeb-0ca979e2c60a",
                    "sense_definition": "Внутреннее нравственное достоинство человека, доблесть, честность, благородство души и чистая совесть.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "moral_dignity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для настоящего офицера защита своей страны всегда была делом <w>чести</w> и совести.",
                            "target_lang_translations": {
                                "en": "For a true officer, defending his country has always been a matter of <w>honour</w> and conscience."
                            }
                        },
                        {
                            "text": "Он дал слово <w>чести</w>, что вернёт долг в указанный срок, и сдержал своё обещание.",
                            "target_lang_translations": {
                                "en": "He gave his word of <w>honour</w> that he would repay the debt on time, and he kept his promise."
                            }
                        }
                    ],
                    "synonyms": [
                        "благородство",
                        "доблесть",
                        "честность",
                        "порядочность"
                    ],
                    "antonyms": [
                        "позор",
                        "бесчестье",
                        "бесчестие",
                        "порочность"
                    ],
                    "common_phrases": [
                        "дело чести",
                        "слово чести",
                        "беречь честь",
                        "честь и совесть"
                    ],
                    "target_lang_definitions": {
                        "en": "Internal moral dignity, valour, honesty, nobility of soul, and a clear conscience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "honour",
                                "target_lang_sense_clarification": "moral integrity and principles"
                            },
                            {
                                "target_lang_word": "integrity",
                                "target_lang_sense_clarification": "quality of being honest and having strong moral principles"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04c03fc4-be87-3a65-9a5a-66867abb2339",
                    "sense_definition": "Общественное уважение, почёт, хорошая репутация и доброе имя.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "moral_dignity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В <w>честь</w> победителей олимпиады был устроен грандиозный праздник на главной площади города.",
                            "target_lang_translations": {
                                "en": "A grand celebration was held in the city's main square in <w>honour</w> of the Olympic winners."
                            }
                        },
                        {
                            "text": "Директор лично оказал нам великую <w>честь</w>, посетив наше скромное мероприятие.",
                            "target_lang_translations": {
                                "en": "The director did us a great <w>honour</w> by personally visiting our humble event."
                            }
                        }
                    ],
                    "synonyms": [
                        "почёт",
                        "уважение",
                        "почтение",
                        "доброе имя",
                        "репутация"
                    ],
                    "antonyms": [
                        "позор",
                        "неуважение",
                        "презрение"
                    ],
                    "common_phrases": [
                        "в честь",
                        "оказать честь",
                        "много чести",
                        "к чести сказать"
                    ],
                    "target_lang_definitions": {
                        "en": "Public respect, esteem, good reputation, and a good name."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "honour",
                                "target_lang_sense_clarification": "public esteem or recognition"
                            },
                            {
                                "target_lang_word": "privilege",
                                "target_lang_sense_clarification": "a special right or favour conferred"
                            },
                            {
                                "target_lang_word": "credit",
                                "target_lang_sense_clarification": "source of pride or approval"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2423b2b-d26b-37df-bee1-96ea199e40ba",
                    "sense_definition": "Внешнее проявление уважения, воинское приветствие (прикладывание руки к головному убору).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military_gesture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодой солдат вытянулся по стойке смирно и отдал <w>честь</w> проходящему мимо генералу.",
                            "target_lang_translations": {
                                "en": "The young soldier stood at attention and gave a <w>salute</w> to the general passing by."
                            }
                        },
                        {
                            "text": "При встрече все военнослужащие обязаны отдавать друг другу воинскую <w>честь</w>.",
                            "target_lang_translations": {
                                "en": "Upon meeting, all military personnel are obliged to give a military <w>salute</w> to each other."
                            }
                        }
                    ],
                    "synonyms": [
                        "знаки почёта"
                    ],
                    "common_phrases": [
                        "отдать честь",
                        "воинская честь"
                    ],
                    "target_lang_definitions": {
                        "en": "An external display of respect, a military salute (raising the hand to the headdress)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "salute",
                                "target_lang_sense_clarification": "military gesture of respect"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "069ca004-1f8b-39ba-80e1-8f65d42943e4",
                    "sense_definition": "Официально-почтительное обращение к судье во время судебного заседания.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "title",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "Прошу слова, Ваша <w>честь</w>, чтобы пояснить суду важные обстоятельства этого дела.",
                            "target_lang_translations": {
                                "en": "I ask for the floor, Your <w>Honour</w>, to explain important circumstances of this case to the court."
                            }
                        },
                        {
                            "text": "Адвокат встал и обратился к судье с уважительной фразой «Ваша <w>честь</w>».",
                            "target_lang_translations": {
                                "en": "The lawyer stood up and addressed the judge with the respectful phrase \"Your <w>Honour</w>\"."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Ваша честь"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "address"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An official, respectful form of address to a judge during court proceedings."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Honour",
                                "target_lang_sense_clarification": "title of address for a judge"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87f22e49-fa8a-34ce-a004-8bc3b6355949",
                    "sense_definition": "Целомудрие, непорочность и нравственная чистота (обычно по отношению к женщине).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "moral_dignity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старинных романах рыцари часто сражались на турнирах за <w>честь</w> прекрасных дам.",
                            "target_lang_translations": {
                                "en": "In old novels, knights often fought in tournaments for the <w>honour</w> of beautiful ladies."
                            }
                        },
                        {
                            "text": "Она строго берегла свою девичью <w>честь</w> и не позволяла никому лишнего.",
                            "target_lang_translations": {
                                "en": "She strictly guarded her maiden <w>honour</w> and allowed no liberties."
                            }
                        }
                    ],
                    "antonyms": [
                        "порочность"
                    ],
                    "common_phrases": [
                        "девичья честь",
                        "покушение на честь"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "conceptually"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Chastity, innocence, and moral purity (usually regarding a woman)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "honour",
                                "target_lang_sense_clarification": "chastity or purity"
                            },
                            {
                                "target_lang_word": "virtue",
                                "target_lang_sense_clarification": "moral excellence or chastity"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3aca9c6a-2e40-35b5-a362-9010d02c21ad",
                    "sense_definition": "Думать, полагать, считать кого-либо или что-либо кем-то или чем-то.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "to_consider",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы привыкли <w>честь</w> его своим близким другом и самым надежным соратником.",
                            "target_lang_translations": {
                                "en": "We are used to <w>considering</w> him our close friend and most reliable ally."
                            }
                        },
                        {
                            "text": "Не стоит <w>честь</w> себя умнее других, когда речь идет о таких сложных вопросах.",
                            "target_lang_translations": {
                                "en": "One should not <w>consider</w> oneself smarter than others when it comes to such complex issues."
                            }
                        }
                    ],
                    "synonyms": [
                        "считать",
                        "полагать",
                        "почитать"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "literary or dialectal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To think, suppose, or consider someone or something to be someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "consider",
                                "target_lang_sense_clarification": "to regard as"
                            },
                            {
                                "target_lang_word": "deem",
                                "target_lang_sense_clarification": "to judge or consider"
                            },
                            {
                                "target_lang_word": "count",
                                "target_lang_sense_clarification": "to consider or regard"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "853b7d2a-208a-3a30-b16c-cb15c8408b55",
                    "sense_definition": "Воспринимать письменную речь, разбирать написанное.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "to_read",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старец любил <w>честь</w> древние книги при тусклом свете одинокой лучины.",
                            "target_lang_translations": {
                                "en": "The elder loved to <w>read</w> ancient books by the dim light of a lonely splinter."
                            }
                        },
                        {
                            "text": "Ему было очень трудно <w>честь</w> эту старую рукопись из-за неразборчивого почерка.",
                            "target_lang_translations": {
                                "en": "It was very difficult for him to <w>read</w> this old manuscript due to the illegible handwriting."
                            }
                        }
                    ],
                    "synonyms": [
                        "читать"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To perceive written speech, to decipher what is written."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "read",
                                "target_lang_sense_clarification": "to decipher text"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
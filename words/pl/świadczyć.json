    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "492b83ea-29f6-313c-8768-eb0eb2761e16",
                    "sense_definition": "Stanowić dowód czegoś, wskazywać na istnienie jakiegoś faktu, cechy lub sytuacji; być znakiem czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "indicate_prove",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego zachowanie <świadczy> o braku kultury osobistej.",
                            "target_lang_translations": {
                                "ru": "Его поведение <свидетельствует> об отсутствии личной культуры."
                            }
                        },
                        {
                            "text": "To, że przyszliście tak licznie, <świadczy> o waszym zaangażowaniu w projekt.",
                            "target_lang_translations": {
                                "ru": "То, что вы пришли в таком количестве, <свидетельствует> о вашей вовлеченности в проект."
                            }
                        },
                        {
                            "text": "Czyste ulice i zadbane parki dobrze <świadczą> o władzach tego miasta.",
                            "target_lang_translations": {
                                "ru": "Чистые улицы и ухоженные парки хорошо <говорят> о властях этого города."
                            }
                        }
                    ],
                    "common_phrases": [
                        "świadczyć o czymś",
                        "dobrze o kimś świadczyć",
                        "źle o kimś świadczyć",
                        "świadczyć o prawdzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служить доказательством чего-либо, указывать на существование какого-либо факта или ситуации; быть признаком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свидетельствовать",
                                "target_lang_sense_clarification": "указывать на что-то, служить подтверждением"
                            },
                            {
                                "target_lang_word": "говорить",
                                "target_lang_sense_clarification": "в переносном смысле: указывать, характеризовать кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96b1c2e1-0651-3781-95b2-ac084b158320",
                    "sense_definition": "Wykonywać usługi, pracę lub udzielać pomocy na czyjąś rzecz, zazwyczaj w sposób sformalizowany lub zawodowy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "provide_services",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza firma <świadczy> usługi transportowe na terenie całego kraju.",
                            "target_lang_translations": {
                                "ru": "Наша компания <оказывает> транспортные услуги на территории всей страны."
                            }
                        },
                        {
                            "text": "Lekarze mają obowiązek <świadczyć> pomoc medyczną każdemu potrzebującemu pacjentowi.",
                            "target_lang_translations": {
                                "ru": "Врачи обязаны <оказывать> медицинскую помощь каждому нуждающемуся пациенту."
                            }
                        }
                    ],
                    "common_phrases": [
                        "świadczyć usługi",
                        "świadczyć pomoc",
                        "świadczyć pracę",
                        "świadczyć usługi drogą elektroniczną"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять работу или предоставлять помощь кому-либо, обычно на профессиональной или официальной основе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оказывать",
                                "target_lang_sense_clarification": "предоставлять (услуги, помощь, поддержку)"
                            },
                            {
                                "target_lang_word": "предоставлять",
                                "target_lang_sense_clarification": "давать возможность пользоваться услугами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86ec4395-6ecb-331b-b4e0-b85b6c00c5d0",
                    "sense_definition": "Występować w roli świadka, składając oficjalne zeznania w sądzie lub w urzędzie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_testify",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna zgodził się <świadczyć> przeciwko mafii w zamian za ochronę policyjną.",
                            "target_lang_translations": {
                                "ru": "Мужчина согласился <свидетельствовать> против мафии в обмен на полицейскую защиту."
                            }
                        },
                        {
                            "text": "Będę <świadczyć> w sądzie na korzyść mojego przyjaciela, ponieważ jest niewinny.",
                            "target_lang_translations": {
                                "ru": "Я буду <свидетельствовать> в суде в пользу моего друга, так как он невиновен."
                            }
                        }
                    ],
                    "synonyms": [
                        "zeznawać"
                    ],
                    "common_phrases": [
                        "świadczyć w sądzie",
                        "świadczyć przeciwko komuś",
                        "świadczyć na korzyść",
                        "świadczyć pod przysięgą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступать в роли свидетеля, давая официальные показания в суде или следственных органах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свидетельствовать",
                                "target_lang_sense_clarification": "выступать свидетелем"
                            },
                            {
                                "target_lang_word": "давать показания",
                                "target_lang_sense_clarification": "сообщать сведения следствию или суду"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
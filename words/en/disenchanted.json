    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "97a8bb1f-f00e-3f61-b38f-168cf43d953f",
                    "sense_definition": "Disappointed; having lost belief or enthusiasm through bad experience.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_of_mind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many voters have become <w>disenchanted</w> with the current government policies.",
                            "target_lang_translations": {
                                "ru": "Многие избиратели <w>разочаровались</w> в текущей политике правительства."
                            }
                        },
                        {
                            "text": "She grew <w>disenchanted</w> with her career after realizing there was no room for growth.",
                            "target_lang_translations": {
                                "ru": "Она <w>разочаровалась</w> в своей карьере, поняв, что там нет возможностей для роста."
                            }
                        }
                    ],
                    "synonyms": [
                        "disillusioned",
                        "dissatisfied",
                        "cynical"
                    ],
                    "antonyms": [
                        "enthusiastic",
                        "hopeful",
                        "enchanted"
                    ],
                    "common_phrases": [
                        "disenchanted with",
                        "become disenchanted",
                        "grow disenchanted",
                        "disenchanted voters"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разочарованный; утративший веру или энтузиазм из-за негативного опыта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разочарованный",
                                "target_lang_sense_clarification": "утративший веру во что-либо"
                            },
                            {
                                "target_lang_word": "утративший иллюзии",
                                "target_lang_sense_clarification": "переставший верить в идеалы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb38b1f5-4efa-3685-bf4a-971717adf366",
                    "sense_definition": "Having had a magical spell or enchantment removed; no longer enchanted.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "magical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>disenchanted</w> prince suddenly returned to his human form.",
                            "target_lang_translations": {
                                "ru": "<w>Расколдованный</w> принц внезапно вернул свой человеческий облик."
                            }
                        },
                        {
                            "text": "Once the wizard broke the spell, the <w>disenchanted</w> forest became quiet and ordinary again.",
                            "target_lang_translations": {
                                "ru": "Как только волшебник снял заклятие, <w>расколдованный</w> лес снова стал тихим и обычным."
                            }
                        }
                    ],
                    "antonyms": [
                        "enchanted",
                        "bewitched",
                        "spellbound"
                    ],
                    "target_lang_definitions": {
                        "ru": "Освобождённый от магических чар или заклятия; больше не заколдованный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расколдованный",
                                "target_lang_sense_clarification": "освобождённый от магии"
                            },
                            {
                                "target_lang_word": "освобождённый от чар",
                                "target_lang_sense_clarification": "снятие магического воздействия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8272057d-96fd-3b38-a275-622e936aac39",
                    "sense_definition": "Simple past and past participle of <w>disenchant</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The harsh reality of the industry quickly <w>disenchanted</w> the young actor.",
                            "target_lang_translations": {
                                "ru": "Суровая реальность индустрии быстро <w>разочаровала</w> молодого актера."
                            }
                        },
                        {
                            "text": "He had <w>disenchanted</w> his followers by failing to keep his promises.",
                            "target_lang_translations": {
                                "ru": "Он <w>разочаровал</w> своих последователей, не сдержав своих обещаний."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола disenchant (разочаровывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разочаровал",
                                "target_lang_sense_clarification": "лишил иллюзий или надежды"
                            },
                            {
                                "target_lang_word": "лишил чар",
                                "target_lang_sense_clarification": "снял магическое заклятие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
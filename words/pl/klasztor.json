                    "sense_id": "aaff40c8-4dd5-3c1c-8ad6-49a7606a985c",
                    "sense_definition": "Budynek lub zespół budynków stanowiący siedzibę zakonu (mnichów lub mniszek).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Turyści chętnie odwiedzają ten zabytkowy <w>klasztor</w> położony na wzgórzu.",
                            "target_lang_translations": {
                                "ru": "Туристы охотно посещают этот старинный <w>монастырь</w>, расположенный на холме."
                            }
                        },
                        {
                            "text": "Wysokie mury otaczają <w>klasztor</w>, zapewniając mnichom spokój i ciszę.",
                            "target_lang_translations": {
                                "ru": "Высокие стены окружают <w>монастырь</w>, обеспечивая монахам покой и тишину."
                            }
                        }
                    ],
                    "synonyms": [
                        "opactwo",
                        "dom zakonny",
                        "monaster",
                        "monastyr"
                    ],
                    "common_phrases": [

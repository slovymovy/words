    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5d44b5f6-d1d5-3e6a-8ca9-3343cc00de83",
                    "sense_definition": "Taki, który stracił dawną świetność, majątek, znaczenie lub dobrą kondycję (odnosi się do osób, instytucji lub stanów abstrakcyjnych).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten niegdyś sławny, a obecnie <w>podupadły</w> artysta rzadko pokazuje się publicznie.",
                            "target_lang_translations": {
                                "ru": "Этот некогда знаменитый, а ныне <w>опустившийся</w> художник редко появляется на публике."
                            }
                        },
                        {
                            "text": "Jego <w>podupadły</w> na zdrowiu ojciec wymagał stałej i troskliwej opieki lekarskiej.",
                            "target_lang_translations": {
                                "ru": "Его <w>сдавший</w> здоровьем отец требовал постоянного и заботливого медицинского ухода."
                            }
                        }
                    ],
                    "synonyms": [
                        "zubożały",
                        "zbiedniały",
                        "niezamożny"
                    ],
                    "common_phrases": [
                        "podupadły na zdrowiu",
                        "podupadły majątek",
                        "podupadły moralnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Утративший былое величие, богатство, значение или хорошее состояние (о людях, учреждениях или абстрактных понятиях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пошатнувшийся",
                                "target_lang_sense_clarification": "о здоровье, положении или авторитете"
                            },
                            {
                                "target_lang_word": "опустившийся",
                                "target_lang_sense_clarification": "о человеке, потерявшем социальный или моральный облик"
                            },
                            {
                                "target_lang_word": "пришедший в упадок",
                                "target_lang_sense_clarification": "об общем состоянии дел, учреждениях или хозяйстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "921f3ef7-5ce1-3e7d-ab08-af979b96ed95",
                    "sense_definition": "Taki, który jest w złym stanie technicznym, zniszczony lub zaniedbany (odnosi się głównie do budynków i miejsc).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Inwestorzy kupili stary, <w>podupadły</w> dworek i postanowili przywrócić mu dawny blask.",
                            "target_lang_translations": {
                                "ru": "Инвесторы купили старую, <w>обветшалую</w> усадьбу и решили вернуть ей былой блеск."
                            }
                        },
                        {
                            "text": "Mijaliśmy kolejny <w>podupadły</w> budynek, w którym nikt nie mieszkał od wielu lat.",
                            "target_lang_translations": {
                                "ru": "Мы проезжали мимо очередного <w>запущенного</w> здания, в котором никто не жил уже много лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrujnowany",
                        "zaniedbany"
                    ],
                    "common_phrases": [
                        "podupadły dom",
                        "podupadły budynek",
                        "podupadły ogród"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в плохом техническом состоянии, разрушенный или запущенный (о зданиях и местах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обветшалый",
                                "target_lang_sense_clarification": "о зданиях, пришедших в негодность от времени"
                            },
                            {
                                "target_lang_word": "запущенный",
                                "target_lang_sense_clarification": "о местах, лишенных ухода"
                            },
                            {
                                "target_lang_word": "ветхий",
                                "target_lang_sense_clarification": "старый и разрушающийся"
                            }
                        ]
                    }
                }
            ]
        },

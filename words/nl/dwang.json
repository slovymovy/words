                        "obsessie",
                        "manie"
                    ],
                    "common_phrases": [
                        "innerlijke dwang"
                    ],
                    "target_lang_definitions": {
                        "en": "A strong, often pathological inner urge to perform certain actions repeatedly.",
                        "ru": "Сильная, часто болезненная внутренняя потребность совершать определенные действия снова и снова."
                    },

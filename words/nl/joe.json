    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "70b9fc47-77bd-30f4-8ee3-452078670117",
                    "sense_definition": "Een informele groet die wordt gebruikt bij het weggaan.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "greeting_farewell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was erg gezellig, tot de volgende keer, <w>joe</w>!",
                            "target_lang_translations": {
                                "ru": "Было очень здорово, до следующего раза, <w>пока</w>!",
                                "en": "It was really nice, see you next time, <w>bye</w>!"
                            }
                        },
                        {
                            "text": "Ik ga er vandoor, <w>joe</w>!",
                            "target_lang_translations": {
                                "ru": "Я пошел, <w>давай</w>!",
                                "en": "I'm off, <w>see ya</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "doei",
                        "dag",
                        "aju",
                        "later"
                    ],
                    "antonyms": [
                        "hallo",
                        "hoi"
                    ],
                    "common_phrases": [
                        "nou joe",
                        "joe joe"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informeel"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное прощание, используемое при уходе.",
                        "en": "An informal greeting used when leaving."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пока",
                                "target_lang_sense_clarification": "прощание"
                            },
                            {
                                "target_lang_word": "давай",
                                "target_lang_sense_clarification": "разг. прощание"
                            },
                            {
                                "target_lang_word": "счастливо",
                                "target_lang_sense_clarification": "пожелание при прощании"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "bye",
                                "target_lang_sense_clarification": "informal farewell"
                            },
                            {
                                "target_lang_word": "see ya",
                                "target_lang_sense_clarification": "colloquial farewell"
                            },
                            {
                                "target_lang_word": "later",
                                "target_lang_sense_clarification": "very informal farewell"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdd12b15-dac5-3536-99a2-40bc9d010128",
                    "sense_definition": "Een informele uitroep om instemming of bevestiging aan te geven.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik bel je vanavond nog even over de details. — <w>Joe</w>, is goed.",
                            "target_lang_translations": {
                                "ru": "Я позвоню тебе сегодня вечером насчет деталей. — <w>Ага</w>, хорошо.",
                                "en": "I'll call you tonight about the details. — <w>Okay</w>, sounds good."
                            }
                        },
                        {
                            "text": "Wil jij de deur even achter je dichttrekken? — <w>Joe</w>!",
                            "target_lang_translations": {
                                "ru": "Закроешь за собой дверь? — <w>Ага</w>!",
                                "en": "Could you pull the door shut behind you? — <w>Sure</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "oké",
                        "prima",
                        "top",
                        "is goed"
                    ],
                    "antonyms": [
                        "nee"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informeel"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное восклицание для выражения согласия или подтверждения.",
                        "en": "An informal exclamation to indicate agreement or confirmation."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ага",
                                "target_lang_sense_clarification": "подтверждение"
                            },
                            {
                                "target_lang_word": "хорошо",
                                "target_lang_sense_clarification": "согласие"
                            },
                            {
                                "target_lang_word": "ладно",
                                "target_lang_sense_clarification": "согласие"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "okay",
                                "target_lang_sense_clarification": "agreement"
                            },
                            {

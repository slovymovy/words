{
    "word_family": [
        "rist",
        "ritsen"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1b4405c0-5de7-3314-9b87-a20e8aa01aa3",
                    "sense_definition": "Tot een streng of bos samenbinden, met name van veldvruchten zoals uien of vlas.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_bind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boeren <w>risten</w> de uien direct na de oogst om ze te laten drogen.",
                            "target_lang_translations": {
                                "en": "The farmers <w>strung</w> the onions immediately after the harvest to let them dry.",
                                "ru": "Фермеры <w>вязали</w> лук в пучки сразу после сбора урожая, чтобы просушить его."
                            }
                        },
                        {
                            "text": "Het is een oud ambacht om vlas netjes te <w>risten</w>.",
                            "target_lang_translations": {
                                "en": "It is an old craft to neatly <w>bunch</w> flax.",
                                "ru": "Аккуратно <w>вязать</w> лен в снопы — это старинное ремесло."
                            }
                        }
                    ],
                    "synonyms": [
                        "rijgen",
                        "bundelen"
                    ],
                    "common_phrases": [
                        "uien risten",
                        "vlas risten"
                    ],
                    "target_lang_definitions": {
                        "en": "To bind together into a string or bunch, especially crops like onions or flax.",
                        "ru": "Связывать в пучки или плести в косы (обычно о луке, льне и т.п.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to string",
                                "target_lang_sense_clarification": "to thread or tie items together on a string"
                            },
                            {
                                "target_lang_word": "to bunch",
                                "target_lang_sense_clarification": "to tie together in a cluster"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вязать",
                                "target_lang_sense_clarification": "в пучки или снопы"
                            },
                            {
                                "target_lang_word": "плести",
                                "target_lang_sense_clarification": "в косы (о луке)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d88d020-c0da-3732-8d0a-3230429bde0a",
                    "sense_definition": "Iets van een steel, tak of streng afhalen, zoals bessen of bladeren.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_strip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor we de jam kunnen maken, moeten we eerst de aalbessen <w>risten</w>.",
                            "target_lang_translations": {
                                "en": "Before we can make the jam, we first have to <w>strip</w> the redcurrants.",
                                "ru": "Прежде чем варить варенье, мы должны сначала <w>обобрать</w> красную смородину."
                            }
                        },
                        {
                            "text": "Zij <w>riste</w> de dorre bladeren van de tak.",
                            "target_lang_translations": {
                                "en": "She <w>stripped</w> the withered leaves from the branch.",
                                "ru": "Она <w>оборвала</w> сухие листья с ветки."
                            }
                        }
                    ],
                    "synonyms": [
                        "ritsen",
                        "afhalen",
                        "stropen"
                    ],
                    "common_phrases": [
                        "bessen risten"
                    ],
                    "target_lang_definitions": {
                        "en": "To remove something from a stalk, branch, or string, such as berries or leaves.",
                        "ru": "Обирать, снимать что-либо со стебля или ветки (например, ягоды или листья)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to strip",
                                "target_lang_sense_clarification": "to remove fruit or leaves from a stem"
                            },
                            {
                                "target_lang_word": "to pick off",
                                "target_lang_sense_clarification": "to remove individual items from a larger whole"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обирать",
                                "target_lang_sense_clarification": "ягоды с ветки"
                            },
                            {
                                "target_lang_word": "обрывать",
                                "target_lang_sense_clarification": "листья или плоды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2704958-779e-3c64-bd11-a3c5b4089369",
                    "sense_definition": "Meervoud verleden tijd van <w>rissen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>risten</w> gisteren de oude posters van de muur.",
                            "target_lang_translations": {
                                "en": "We <w>ripped</w> the old posters off the wall yesterday.",
                                "ru": "Вчера мы <w>срывали</w> старые плакаты со стены."
                            }
                        },
                        {
                            "text": "Zij <w>risten</w> zich haastig door de menigte.",
                            "target_lang_translations": {
                                "en": "They <w>rushed</w> hastily through the crowd.",
                                "ru": "Они поспешно <w>продирались</w> сквозь толпу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Past tense plural of 'rissen' (to rip off or to rush).",
                        "ru": "Форма прошедшего времени во множественном числе от глагола rissen (срывать, продираться)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ripped",
                                "target_lang_sense_clarification": "past tense of to rip/tear off"
                            },
                            {
                                "target_lang_word": "rushed",
                                "target_lang_sense_clarification": "past tense of to move quickly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "срывали",
                                "target_lang_sense_clarification": "резко отрывали"
                            },
                            {
                                "target_lang_word": "продирались",
                                "target_lang_sense_clarification": "сквозь толпу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8d8542ed-490d-37a0-bd86-d440384be88a",
                    "sense_definition": "Meervoud van <w>rist</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aan de zoldering hingen lange <w>risten</w> gedroogde knoflook.",
                            "target_lang_translations": {
                                "en": "Long <w>strings</w> of dried garlic hung from the ceiling.",
                                "ru": "С потолка свисали длинные <w>связки</w> сушеного чеснока."
                            }
                        },
                        {
                            "text": "De marktkoopman had prachtige <w>risten</w> uien uitgestald.",
                            "target_lang_translations": {
                                "en": "The market vendor had displayed beautiful <w>strings</w> of onions.",
                                "ru": "Торговец на рынке выставил прекрасные <w>плетёнки</w> лука."
                            }
                        }
                    ],
                    "common_phrases": [
                        "risten uien",
                        "risten knoflook"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of 'rist' (a string or bunch of items).",
                        "ru": "Множественное число от rist (связка, гроздь, вереница)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strings",
                                "target_lang_sense_clarification": "connected series of objects (e.g., onions)"
                            },
                            {
                                "target_lang_word": "bunches",
                                "target_lang_sense_clarification": "clusters of items tied together"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "связки",
                                "target_lang_sense_clarification": "лука или чеснока"
                            },
                            {
                                "target_lang_word": "гирлянды",
                                "target_lang_sense_clarification": "нанизанные предметы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
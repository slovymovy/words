                {
                    "sense_id": "1c81ddca-2293-358e-bf8b-a5a2d6688eec",
                    "sense_definition": "Stawać się posiadaczem czegoś, co zostało przekazane przez kogoś innego (np. prezentu, wiadomości, pieniędzy).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "receiving_possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na każde urodziny <dostaję> od babci ciepłe skarpety.",
                            "target_lang_translations": {
                                "ru": "На каждый день рождения я <получаю> от бабушки тёплые носки."
                            }
                        },
                        {
                            "text": "Czy <dostajesz> powiadomienia z banku na swój telefon?",
                            "target_lang_translations": {
                                "ru": "Ты <получаешь> уведомления из банка на свой телефон?"
                            }
                        },
                        {
                            "text": "W zeszłym miesiącu <dostałem> podwyżkę w pracy.",
                            "target_lang_translations": {
                                "ru": "В прошлом месяце я <получил> прибавку к зарплате."
                            }
                        }
                    ],
                    "synonyms": [
                        "otrzymywać",
                        "otrzymać"
                    ],
                    "antonyms": [
                        "tracić",
                        "wydostawać się",
                        "wydostawać się"
                    ],
                    "common_phrases": [

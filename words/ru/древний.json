            "senses": [
                {
                    "sense_id": "5676e6d9-17c1-33be-8863-7acae159c6e7",
                    "sense_definition": "Относящийся к отдалённому прошлому, к начальному периоду истории.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "История <w>Древней</w> Греции изучается во всех школах.",
                            "target_lang_translations": {
                                "en": "The history of <w>Ancient</w> Greece is studied in all schools."
                            }
                        },
                        {
                            "text": "<w>Древние</w> люди жили в пещерах и охотились на диких зверей.",
                            "target_lang_translations": {
                                "en": "<w>Ancient</w> people lived in caves and hunted wild animals."
                            }
                        }
                    ],
                    "synonyms": [
                        "архаичный"
                    ],
                    "antonyms": [
                        "современный"
                    ],
                    "common_phrases": [
                        "Древний мир",
                        "Древняя Греция",
                        "Древний Рим",
                        "с древних времён"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the distant past, especially to the early period of history."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ancient",
                                "target_lang_sense_clarification": "relating to early history, e.g. Ancient Greece"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70a2e501-bb7c-3abd-8b8d-1cdc7c9ea72d",
                    "sense_definition": "Существующий очень долгое время, возникший много лет назад; очень старый.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "age",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В городской библиотеке хранится очень <w>древняя</w> книга.",
                            "target_lang_translations": {
                                "en": "A very <w>ancient</w> book is kept in the city library."
                            }
                        },
                        {
                            "text": "На скамейке у дома сидел <w>древний</w> старик.",
                            "target_lang_translations": {
                                "en": "An <w>ancient</w> old man was sitting on the bench by the house."
                            }
                        }
                    ],
                    "synonyms": [
                        "архаичный"
                    ],
                    "antonyms": [
                        "новый"
                    ],
                    "common_phrases": [
                        "древний старик",
                        "древний город",
                        "древний род"
                    ],
                    "target_lang_definitions": {
                        "en": "Existing for a very long time; extremely old."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ancient",
                                "target_lang_sense_clarification": "having existed for a very long time"
                            },
                            {
                                "target_lang_word": "very old",
                                "target_lang_sense_clarification": "general description of great age"
                            }
                        ]
                    }
                }
            ]

                    "sense_id": "93a3d758-b31f-3884-bf0f-513d2dfaff30",
                    "sense_definition": "Mający nierówną, szorstką powierzchnię, często pokryty guzkami lub wypukłościami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary dąb rosnący w ogrodzie miał grubą i <w>kostropatą</w> korę.",
                            "target_lang_translations": {
                                "ru": "У старого дуба, растущего в саду, была толстая и <w>корявая</w> кора."
                            }
                        },
                        {
                            "text": "Podczas spaceru po lesie znalazłem solidny, <w>kostropaty</w> kij, którym podpierałem się w drodze.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки по лесу я нашел прочную <w>сучковатую</w> палку, на которую опирался по дороге."
                            }
                        }
                    ],
                    "synonyms": [
                        "chropowaty",
                        "szorstki",
                        "nierówny",
                        "chropawy"
                    ],
                    "antonyms": [
                        "gładki"
                    ],
                    "common_phrases": [
                        "kostropata powierzchnia",
                        "kostropata kora",
                        "kostropaty kij",
                        "kostropata skóra"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий неровную, шероховатую поверхность, часто покрытый бугорками или выступами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корявый",

                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Переносить или перевозить что-то тяжелое или громоздкое на расстояние, часто с трудом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тащить",
                                "target_lang_sense_clarification": "нести с усилием"
                            },
                            {
                                "target_lang_word": "везти",
                                "target_lang_sense_clarification": "транспортировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a5c8614-aa8a-3297-9560-46a81499c405",
                    "sense_definition": "To take someone or something away, especially roughly, involuntarily, or for disposal.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The protesters were <w>carted</w> off to the police station.",
                            "target_lang_translations": {
                                "ru": "Протестующих <w>увезли</w> в полицейский участок."
                            }
                        },
                        {
                            "text": "All the broken furniture was <w>carted</w> away to the dump yesterday.",
                            "target_lang_translations": {
                                "ru": "Всю сломанную мебель вчера <w>вывезли</w> на свалку."
                            }
                        }
                    ],
                    "synonyms": [
                        "haul off",
                        "take away",
                        "remove"
                    ],
                    "common_phrases": [
                        "cart off",
                        "cart away"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Увозить кого-то или что-то, особенно грубо, против воли или для утилизации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увозить",
                                "target_lang_sense_clarification": "забирать силой или удалять"
                            },
                            {
                                "target_lang_word": "вывозить",
                                "target_lang_sense_clarification": "удалять мусор или груз"
                            }
                        ]
                    }
                }
            ]

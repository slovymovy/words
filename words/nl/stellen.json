            "senses": [
                {
                    "sense_id": "aadfb181-f9d7-3898-9316-615712afa385",
                    "sense_definition": "Een vraag formuleren en uitspreken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar <w>stelde</w> een moeilijke vraag aan de leerling.",
                            "target_lang_translations": {
                                "en": "The teacher <w>asked</w> the student a difficult question.",
                                "ru": "Учитель <w>задал</w> ученику трудный вопрос."
                            }
                        },
                        {
                            "text": "Mag ik u misschien een persoonlijke vraag <w>stellen</w>?",
                            "target_lang_translations": {
                                "en": "May I <w>ask</w> you a personal question?",
                                "ru": "Могу я <w>задать</w> вам личный вопрос?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "een vraag stellen",
                        "vragen stellen"
                    ],
                    "target_lang_definitions": {
                        "en": "To formulate and express a question.",
                        "ru": "Формулировать и произносить вопрос."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "most common translation for questions"
                            },
                            {
                                "target_lang_word": "put",
                                "target_lang_sense_clarification": "to pose a question to someone"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "задавать",
                                "target_lang_sense_clarification": "вопрос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff66d5fb-28a6-3b3f-8c74-8f2359e600d1",
                    "sense_definition": "Iets of iemand in een bepaalde toestand, positie of situatie brengen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "positioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze onverwachte ontwikkeling <w>stelt</w> ons voor een moeilijk dilemma.",
                            "target_lang_translations": {
                                "en": "This unexpected development <w>presents</w> us with a difficult dilemma.",
                                "ru": "Это неожиданное развитие событий <w>ставит</w> нас перед сложной дилеммой."
                            }
                        },
                        {
                            "text": "Hij <w>stelde</w> de machine onmiddellijk in werking.",
                            "target_lang_translations": {
                                "en": "He immediately <w>put</w> the machine into operation.",
                                "ru": "Он немедленно <w>привел</w> машину в действие."
                            }
                        }
                    ],
                    "synonyms": [
                        "plaatsen"
                    ],
                    "common_phrases": [
                        "in staat stellen",
                        "op prijs stellen",
                        "in werking stellen",
                        "op de hoogte stellen"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring something or someone into a certain state, position, or situation.",
                        "ru": "Приводить что-либо или кого-либо в определенное состояние, положение или ситуацию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put",
                                "target_lang_sense_clarification": "to place in a situation or state"
                            },
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "to cause to be in a certain condition (e.g., in motion)"
                            },
                            {
                                "target_lang_word": "present",
                                "target_lang_sense_clarification": "to introduce a problem or dilemma"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ставить",
                                "target_lang_sense_clarification": "помещать в положение/ситуацию"
                            },
                            {
                                "target_lang_word": "приводить",
                                "target_lang_sense_clarification": "в состояние (напр. в действие)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "817263cc-02a8-371a-bebe-9d5535768717",
                    "sense_definition": "Aannemen dat iets waar is, vaak om een hypothetische situatie te schetsen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "imagination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Stel</w> dat we morgen de loterij winnen.",
                            "target_lang_translations": {
                                "en": "<w>Suppose</w> that we win the lottery tomorrow.",
                                "ru": "<w>Представь</w>, что завтра мы выиграем в лотерею."
                            }
                        },
                        {
                            "text": "<w>Stel</w> je voor dat je overal naartoe kon reizen.",
                            "target_lang_translations": {
                                "en": "<w>Imagine</w> being able to travel anywhere.",
                                "ru": "<w>Представь</w> себе, что ты мог бы поехать куда угодно."
                            }

                {
                    "sense_id": "4aea71f6-8f74-3c30-a666-9aadeb16011f",
                    "sense_definition": "Een massale, vaak gewelddadige poging om het heersende gezag omver te werpen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bevolking kwam massaal in <w>opstand</w> tegen de dictator.",
                            "target_lang_translations": {
                                "en": "The population rose up in mass <w>revolt</w> against the dictator."
                            }
                        },
                        {
                            "text": "Het leger wist de gewapende <w>opstand</w> na drie dagen neer te slaan.",
                            "target_lang_translations": {
                                "en": "The army managed to crush the armed <w>uprising</w> after three days."
                            }
                        }
                    ],
                    "synonyms": [
                        "rebellie",
                        "oproer"
                    ],
                    "common_phrases": [
                        "in opstand komen",
                        "een opstand onderdrukken",
                        "gewapende opstand"
                    ],
                    "target_lang_definitions": {

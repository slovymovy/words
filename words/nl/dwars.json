        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "792eabdf-4f44-356e-8f2f-f9310282d31b",
                    "sense_definition": "Recht door iets heen of van de ene kant naar de andere kant.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Dwars</w> door de sloot lag een omgevallen boomstam waar we overheen klommen.",
                            "target_lang_translations": {
                                "ru": "<w>Поперек</w> канавы лежал упавший ствол дерева, через который мы перелезли.",
                                "en": "<w>Straight across</w> the ditch lay a fallen tree trunk that we climbed over."
                            }
                        },
                        {
                            "text": "We besloten niet om te lopen, maar liepen <w>dwars</w> over de akker naar huis.",
                            "target_lang_translations": {
                                "ru": "Мы решили не идти в обход, а пошли <w>напрямик</w> через поле домой.",
                                "en": "We decided not to walk around, but walked <w>straight across</w> the field home."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dwars door",
                        "dwars over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прямо сквозь что-то или с одной стороны на другую.",
                        "en": "Straight through something or from one side to the other."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напрямик",
                                "target_lang_sense_clarification": "о движении"
                            },
                            {
                                "target_lang_word": "напролом",
                                "target_lang_sense_clarification": "через препятствия"
                            },
                            {
                                "target_lang_word": "поперек",
                                "target_lang_sense_clarification": "пересекая что-либо"
                            }
                        ],
                        "en": [

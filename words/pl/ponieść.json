            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b9ce889f-ef5d-3aa4-af43-1f4dfe9096ca",
                    "sense_definition": "Wziąć coś lub kogoś i przemieścić w inne miejsce, trzymając w rękach lub na sobie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy mógłbyś mi pomóc i <w>ponieść</w> te ciężkie zakupy do samochodu?",
                            "target_lang_translations": {
                                "ru": "Ты не мог бы помочь мне <w>донести</w> эти тяжелые покупки до машины?"
                            }
                        },
                        {
                            "text": "Zmęczony ojciec postanowił <w>ponieść</w> swoją małą córkę na barana.",
                            "target_lang_translations": {
                                "ru": "Уставший отец решил <w>понести</w> свою маленькую дочь на плечах."
                            }
                        }
                    ],
                    "synonyms": [
                        "zanieść"
                    ],
                    "common_phrases": [
                        "ponieść walizkę",
                        "ponieść dziecko",
                        "ponieść bagaż"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взять что-то или кого-то и переместить в другое место, держа в руках или на себе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понести",
                                "target_lang_sense_clarification": "взять и нести"
                            },
                            {
                                "target_lang_word": "донести",
                                "target_lang_sense_clarification": "помочь донести до места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c638cec-6bf1-39ca-a9e7-94337f431515",
                    "sense_definition": "Doświadczyć czegoś negatywnego, na przykład straty, kosztów, kary lub porażki, w wyniku pewnych zdarzeń.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma musiała <w>ponieść</w> ogromne koszty związane z naprawą uszkodzonych maszyn.",
                            "target_lang_translations": {
                                "ru": "Компания должна была <w>понести</w> огромные расходы, связанные с ремонтом поврежденных машин."
                            }
                        },
                        {
                            "text": "Jeśli złamiesz prawo, musisz liczyć się z tym, że możesz <w>ponieść</w> surowe konsekwencje.",
                            "target_lang_translations": {
                                "ru": "Если ты нарушишь закон, ты должен считаться с тем, что можешь <w>понести</w> суровые последствия."
                            }
                        }
                    ],
                    "synonyms": [
                        "doznać",
                        "doświadczyć"
                    ],
                    "common_phrases": [
                        "ponieść konsekwencje",
                        "ponieść koszty",
                        "ponieść stratę",
                        "ponieść porażkę",
                        "ponieść śmierć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытать что-то негативное, например, убытки, расходы, наказание или поражение, в результате определенных событий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понести",
                                "target_lang_sense_clarification": "расходы, наказание, утрату"
                            },
                            {
                                "target_lang_word": "потерпеть",
                                "target_lang_sense_clarification": "поражение, неудачу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b938bb1f-357b-358a-8c3f-15bc8e4ff501",
                    "sense_definition": "Sprawić, że ktoś traci panowanie nad sobą pod wpływem silnych emocji lub siły zewnętrznej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotions_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas kłótni Marka <w>poniosły</w> nerwy i powiedział o kilka słów za dużo.",
                            "target_lang_translations": {
                                "ru": "Во время ссоры у Марка <w>сдали</w> нервы, и он сказал на пару слов больше, чем следовало."
                            }
                        },
                        {
                            "text": "Młodego pisarza <w>poniosła</w> fantazja, gdy tworzył zakończenie swojej nowej powieści.",
                            "target_lang_translations": {
                                "ru": "Молодого писателя <w>увлекла</w> фантазия, когда он создавал концовку своего нового романа."
                            }
                        }
                    ],
                    "synonyms": [
                        "porwać",
                        "ogarnąć",
                        "owładnąć"
                    ],
                    "common_phrases": [
                        "poniosły go nerwy",
                        "poniosła go fantazja",
                        "koń go poniósł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то потерять самообладание под влиянием сильных эмоций или внешней силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понести",
                                "target_lang_sense_clarification": "о потере контроля, напр. «его понесло»"
                            },
                            {
                                "target_lang_word": "увлечь",
                                "target_lang_sense_clarification": "о мыслях, фантазии"
                            }
                        ]
                    }
                }
            ]
        }
    ]

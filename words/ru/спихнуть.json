    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3e063179-1f29-319d-878e-526e8d491667",
                    "sense_definition": "Сдвинуть с места или сбросить вниз, применяя физическую силу (толкая).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_push",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы с трудом смогли <спихнуть> тяжелую лодку в воду.",
                            "target_lang_translations": {
                                "en": "We barely managed to <push> the heavy boat into the water."
                            }
                        },
                        {
                            "text": "Кот забрался на полку и случайно <спихнул> вазу на пол.",
                            "target_lang_translations": {
                                "en": "The cat climbed onto the shelf and accidentally <knocked> the vase onto the floor."
                            }
                        }
                    ],
                    "synonyms": [
                        "столкнуть"
                    ],
                    "common_phrases": [
                        "спихнуть в воду",
                        "спихнуть с дороги",
                        "спихнуть в кювет"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies a rough or casual action"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To move something from its place or throw it down using physical force (pushing)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "push off",
                                "target_lang_sense_clarification": "physical object"
                            },
                            {
                                "target_lang_word": "shove off",
                                "target_lang_sense_clarification": "implies effort or roughness"
                            },
                            {
                                "target_lang_word": "knock off",
                                "target_lang_sense_clarification": "often accidental or quick"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5537b9ff-3c86-355a-a2c6-05ee96a5ae6f",
                    "sense_definition": "Переложить свои обязанности, работу, ответственность или вину на другого человека.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_rid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник пытается <спихнуть> всю бумажную работу на стажёра.",
                            "target_lang_translations": {
                                "en": "The boss is trying to <dump> all the paperwork on the intern."
                            }
                        },
                        {
                            "text": "Он испугался наказания и решил <спихнуть> вину на своего друга.",
                            "target_lang_translations": {
                                "en": "He was afraid of punishment and decided to <shift> the blame onto his friend."
                            }
                        }
                    ],
                    "common_phrases": [
                        "спихнуть ответственность",
                        "спихнуть вину",
                        "спихнуть работу",
                        "спихнуть дела"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To shift one's duties, work, responsibility, or blame onto another person."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dump",
                                "target_lang_sense_clarification": "work or duties (colloquial)"
                            },
                            {
                                "target_lang_word": "shift",
                                "target_lang_sense_clarification": "blame or responsibility"
                            },
                            {
                                "target_lang_word": "offload",
                                "target_lang_sense_clarification": "tasks or burdens"
                            },
                            {
                                "target_lang_word": "foist",
                                "target_lang_sense_clarification": "something unwanted upon someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d547a472-f7db-3f3e-bd92-3fc42aecb58c",
                    "sense_definition": "Избавиться от чего-либо ненужного (часто продать) или отправить кого-либо куда-то, чтобы не мешал.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_rid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему удалось выгодно <спихнуть> старую машину перед покупкой новой.",
                            "target_lang_translations": {
                                "en": "He managed to profitably <unload> his old car before buying a new one."
                            }
                        },
                        {
                            "text": "Родители на всё лето <спихнули> детей в деревню к бабушке.",
                            "target_lang_translations": {
                                "en": "The parents <dumped> the kids in the village with their grandmother for the whole summer."
                            }
                        }
                    ],
                    "common_phrases": [
                        "спихнуть товар",
                        "спихнуть старьё",
                        "спихнуть с рук"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To get rid of something unnecessary (often by selling) or send someone away so they do not interfere."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get rid of",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "unload",
                                "target_lang_sense_clarification": "sell something unwanted"
                            },
                            {
                                "target_lang_word": "dump",
                                "target_lang_sense_clarification": "leave someone somewhere (colloquial)"
                            },
                            {
                                "target_lang_word": "pawn off",
                                "target_lang_sense_clarification": "pass something on deceptively"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cf66c96-c727-3a47-975d-e414c407b54a",
                    "sense_definition": "Вынудить кого-либо уйти с занимаемой должности или места; сместить.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_rid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодой карьерист мечтал <спихнуть> директора и занять его кресло.",
                            "target_lang_translations": {
                                "en": "The young careerist dreamed of <ousting> the director and taking his chair."
                            }
                        },
                        {
                            "text": "Его давно хотели <спихнуть> с поста председателя из-за плохих результатов.",
                            "target_lang_translations": {
                                "en": "They had wanted to <oust> him from the chairman's post for a long time due to poor results."
                            }
                        }
                    ],
                    "common_phrases": [
                        "спихнуть с должности",
                        "спихнуть с места"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To force someone to leave a held position or place; to displace."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oust",
                                "target_lang_sense_clarification": "from a position or job"
                            },
                            {
                                "target_lang_word": "push out",
                                "target_lang_sense_clarification": "force to leave"
                            },
                            {
                                "target_lang_word": "unseat",
                                "target_lang_sense_clarification": "remove from office"
                            }
                        ]
                    }
                }
            ]
        }
    ]

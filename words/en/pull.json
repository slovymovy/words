                },
                {
                    "sense_id": "033ae4c0-10ac-3767-b4c4-5d45167a5bf6",
                    "sense_definition": "An injury caused by straining a muscle or ligament.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "injury_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The athlete is recovering from a hamstring <w>pull</w>.",
                            "target_lang_translations": {
                                "ru": "Спортсмен восстанавливается после <w>растяжения</w> подколенного сухожилия."
                            }
                        },
                        {
                            "text": "He suffered a painful muscle <w>pull</w> during the game.",
                            "target_lang_translations": {
                                "ru": "Он получил болезненное <w>растяжение</w> мышцы во время игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "strain",
                        "sprain"
                    ],
                    "common_phrases": [
                        "muscle pull",
                        "hamstring pull"
                    ],
                    "target_lang_definitions": {
                        "ru": "Травма, вызванная чрезмерным растяжением мышцы."
                    },
                    "translations": {

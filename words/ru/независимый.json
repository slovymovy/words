    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ecb72493-952f-3f34-b893-2c8dac44929a",
                    "sense_definition": "Не находящийся в чьей-либо власти или подчинении, обладающий суверенитетом (о государстве, организации).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "autonomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждое <w>независимое</w> государство имеет свой флаг и гимн.",
                            "target_lang_translations": {
                                "en": "Every <w>independent</w> state has its own flag and anthem."
                            }
                        },
                        {
                            "text": "Эта страна стала <w>независимой</w> в прошлом веке.",
                            "target_lang_translations": {
                                "en": "This country became <w>independent</w> in the last century."
                            }
                        }
                    ],
                    "synonyms": [
                        "самостоятельный",
                        "суверенный",
                        "свободный"
                    ],
                    "antonyms": [
                        "зависимый",
                        "подчинённый"
                    ],
                    "common_phrases": [
                        "независимое государство",
                        "независимая страна",
                        "независимая политика"
                    ],
                    "target_lang_definitions": {
                        "en": "Not under someone else's power or control, possessing sovereignty (referring to a state or organization)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "independent",
                                "target_lang_sense_clarification": "sovereign, self-governing"
                            },
                            {
                                "target_lang_word": "sovereign",
                                "target_lang_sense_clarification": "state or country"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b822eb80-cec2-30b6-b9b9-b900b8115601",
                    "sense_definition": "Не связанный обязательствами с кем-либо, беспристрастный и объективный (об эксперте, судье, СМИ).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "autonomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для оценки ущерба был приглашён <w>независимый</w> эксперт.",
                            "target_lang_translations": {
                                "en": "An <w>independent</w> expert was invited to assess the damage."
                            }
                        },
                        {
                            "text": "Журналисты провели собственное <w>независимое</w> расследование.",
                            "target_lang_translations": {
                                "en": "The journalists conducted their own <w>independent</w> investigation."
                            }
                        }
                    ],
                    "synonyms": [
                        "объективный",
                        "беспристрастный",
                        "непредвзятый"
                    ],
                    "antonyms": [
                        "зависимый"
                    ],
                    "common_phrases": [
                        "независимый эксперт",
                        "независимое расследование",
                        "независимое мнение",
                        "независимый источник"
                    ],
                    "target_lang_definitions": {
                        "en": "Not bound by obligations to anyone, impartial and objective (referring to an expert, judge, or media)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "independent",
                                "target_lang_sense_clarification": "impartial, unbiased"
                            },
                            {
                                "target_lang_word": "impartial",
                                "target_lang_sense_clarification": "neutral"
                            },
                            {
                                "target_lang_word": "unbiased",
                                "target_lang_sense_clarification": "objective"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63a64f06-cbf3-37f3-bcff-d9955ec9484a",
                    "sense_definition": "Самостоятельный в своих поступках и суждениях, не поддающийся чужому влиянию (о человеке и его характере).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "autonomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У неё очень <w>независимый</w> характер, она всё решает сама.",
                            "target_lang_translations": {
                                "en": "She has a very <w>independent</w> character; she decides everything herself."
                            }
                        },
                        {
                            "text": "Он бросил на нас <w>независимый</w> взгляд и вышел из комнаты.",
                            "target_lang_translations": {
                                "en": "He cast an <w>independent</w> look at us and left the room."
                            }
                        }
                    ],
                    "synonyms": [
                        "самостоятельный",
                        "вольный",
                        "свободолюбивый"
                    ],
                    "antonyms": [
                        "зависимый"
                    ],
                    "common_phrases": [
                        "независимый характер",
                        "независимый вид",
                        "независимый человек"
                    ],
                    "target_lang_definitions": {
                        "en": "Self-reliant in actions and judgments, not yielding to outside influence (referring to a person and their character)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "independent",
                                "target_lang_sense_clarification": "self-reliant"
                            },
                            {
                                "target_lang_word": "self-sufficient",
                                "target_lang_sense_clarification": "needing no help"
                            },
                            {
                                "target_lang_word": "free-spirited",
                                "target_lang_sense_clarification": "character"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3851fc34-f22e-3735-8fed-b4647118c11b",
                    "sense_definition": "Не связанный с чем-либо, существующий или происходящий отдельно, обособленно.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дом имеет <w>независимое</w> отопление, что очень удобно зимой.",
                            "target_lang_translations": {
                                "en": "The house has <w>independent</w> heating, which is very convenient in winter."
                            }
                        },
                        {
                            "text": "Эти два события абсолютно <w>независимы</w> друг от друга.",
                            "target_lang_translations": {
                                "en": "These two events are absolutely <w>independent</w> of each other."
                            }
                        }
                    ],
                    "synonyms": [
                        "отдельный",
                        "обособленный",
                        "автономный"
                    ],
                    "antonyms": [
                        "зависимый",
                        "зави́симый"
                    ],
                    "common_phrases": [
                        "независимые переменные",
                        "независимое питание",
                        "независимо от"
                    ],
                    "target_lang_definitions": {
                        "en": "Not connected with something, existing or occurring separately or in isolation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "independent",
                                "target_lang_sense_clarification": "separate, unconnected"
                            },
                            {
                                "target_lang_word": "separate",
                                "target_lang_sense_clarification": "distinct"
                            },
                            {
                                "target_lang_word": "standalone",
                                "target_lang_sense_clarification": "systems or devices"
                            }
                        ]
                    }
                }
            ]
        }

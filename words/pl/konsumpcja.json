{
    "word_family": [
        "konsumować",
        "konsument",
        "konsumpcyjny"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cab8d36a-7a6c-3840-b7c5-a95191a1b6b5",
                    "sense_definition": "Nabywanie i zużywanie dóbr lub usług w celu zaspokojenia potrzeb życiowych.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "economic_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wzrost gospodarczy kraju jest w dużej mierze napędzany przez <w>konsumpcję</w> wewnętrzną.",
                            "target_lang_translations": {
                                "ru": "Экономический рост страны в значительной степени стимулируется внутренним <w>потреблением</w>."
                            }
                        },
                        {
                            "text": "Nadmierna <w>konsumpcja</w> surowców naturalnych prowadzi do degradacji środowiska.",
                            "target_lang_translations": {
                                "ru": "Чрезмерное <w>потребление</w> природных ресурсов ведет к деградации окружающей среды."
                            }
                        }
                    ],
                    "synonyms": [
                        "spożycie",
                        "suchoty",
                        "gruźlica"
                    ],
                    "common_phrases": [
                        "wzrost konsumpcji",
                        "poziom konsumpcji",
                        "konsumpcja dóbr",
                        "model konsumpcji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приобретение и использование товаров или услуг для удовлетворения жизненных потребностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потребление",
                                "target_lang_sense_clarification": "использование товаров и услуг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a54c1d43-3f76-343a-9fa2-cc36e3c33d6b",
                    "sense_definition": "Czynność jedzenia i picia; spożywanie pokarmów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "food_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarze ostrzegają, że nadmierna <w>konsumpcja</w> alkoholu szkodzi zdrowiu.",
                            "target_lang_translations": {
                                "ru": "Врачи предупреждают, что чрезмерное <w>употребление</w> алкоголя вредит здоровью."
                            }
                        },
                        {
                            "text": "Produkt jest zdatny do <w>konsumpcji</w> tylko przez trzy dni od otwarcia.",
                            "target_lang_translations": {
                                "ru": "Продукт пригоден к <w>употреблению</w> только в течение трех дней после вскрытия."
                            }
                        }
                    ],
                    "synonyms": [
                        "spożycie"
                    ],
                    "common_phrases": [
                        "konsumpcja alkoholu",
                        "zdatny do konsumpcji",
                        "przeznaczony do konsumpcji"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "słowo bardziej formalne niż 'jedzenie'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по приему пищи и питья; употребление продуктов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "употребление",
                                "target_lang_sense_clarification": "прием пищи или напитков"
                            },
                            {
                                "target_lang_word": "потребление",
                                "target_lang_sense_clarification": "формально о еде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f49aa17-52f2-34f2-9349-2e91aae871f0",
                    "sense_definition": "Przestarzała nazwa przewlekłej choroby zakaźnej, obecnie znanej jako gruźlica.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "medical_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dziewiętnastowiecznych powieściach bohaterowie często umierali na <w>konsumpcję</w>.",
                            "target_lang_translations": {
                                "ru": "В романах девятнадцатого века герои часто умирали от <w>чахотки</w>."
                            }
                        },
                        {
                            "text": "Jego blada twarz i ciągły kaszel wskazywały na zaawansowaną <w>konsumpcję</w>.",
                            "target_lang_translations": {
                                "ru": "Его бледное лицо и постоянный кашель указывали на запущенную <w>чахотку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gruźlica",
                        "suchoty"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "określenie historyczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название хронического инфекционного заболевания, ныне известного как туберкулез."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чахотка",
                                "target_lang_sense_clarification": "устаревшее название болезни"
                            }
                        ]
                    }
                }
            ]
        }
    ]

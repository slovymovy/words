{
    "word_family": [
        "rot",
        "rotten",
        "rotter",
        "rottenness"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "85f531e6-72de-35ee-8b0c-35af0850f08e",
                    "sense_definition": "Present participle and gerund of <w>rot</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The smell of <w>rotting</w> food filled the kitchen.",
                            "target_lang_translations": {
                                "ru": "Запах <w>гниющей</w> еды наполнил кухню."
                            }
                        },
                        {
                            "text": "The old wooden fence is slowly <w>rotting</w> away in the rain.",
                            "target_lang_translations": {
                                "ru": "Старый деревянный забор медленно <w>гниет</w> под дождем."
                            }
                        }
                    ],
                    "synonyms": [
                        "decaying",
                        "decomposing",
                        "spoiling"
                    ],
                    "common_phrases": [
                        "rotting away",
                        "rotting flesh",
                        "rotting wood",
                        "rotting vegetation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола \"гнить\"; описывает процесс порчи органики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гниющий",
                                "target_lang_sense_clarification": "причастие; находящийся в процессе гниения"
                            },
                            {
                                "target_lang_word": "гнить",
                                "target_lang_sense_clarification": "глагол; разрушаться под действием бактерий"
                            },
                            {
                                "target_lang_word": "разлагающийся",
                                "target_lang_sense_clarification": "синоним; распадающийся на части"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7ebbc243-277d-39ff-8ec0-1ded9ce6c38d",
                    "sense_definition": "The natural process by which organic matter decays or decomposes.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Excessive moisture accelerates the <w>rotting</w> of the timber beams.",
                            "target_lang_translations": {
                                "ru": "Избыточная влага ускоряет <w>гниение</w> деревянных балок."
                            }
                        },
                        {
                            "text": "The <w>rotting</w> of fallen leaves contributes nutrients to the forest floor.",
                            "target_lang_translations": {
                                "ru": "<w>Гниение</w> опавших листьев обогащает лесную почву питательными веществами."
                            }
                        }
                    ],
                    "synonyms": [
                        "decay",
                        "decomposition",
                        "putrefaction"
                    ],
                    "antonyms": [
                        "preservation"
                    ],
                    "common_phrases": [
                        "dry rotting",
                        "root rotting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественный процесс, при котором органическая материя разлагается или портится."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гниение",
                                "target_lang_sense_clarification": "процесс распада органических веществ"
                            },
                            {
                                "target_lang_word": "разложение",
                                "target_lang_sense_clarification": "распад сложных веществ на более простые"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20b8ceba-87da-3a2a-9ca7-cb534f5a2075",
                    "sense_definition": "Material that has decayed or decomposed.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gardener cleared the slimy <w>rottings</w> from the bottom of the compost bin.",
                            "target_lang_translations": {
                                "ru": "Садовник убрал склизкую <w>гниль</w> со дна компостного ящика."
                            }
                        },
                        {
                            "text": "He swept away the vegetable <w>rottings</w> that had accumulated near the drain.",
                            "target_lang_translations": {
                                "ru": "Он смел растительные <w>гниющие остатки</w>, скопившиеся у водостока."
                            }
                        }
                    ],
                    "synonyms": [
                        "compost",
                        "refuse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Материал, который сгнил или разложился; гниющие остатки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гниль",
                                "target_lang_sense_clarification": "сгнившая масса"
                            },
                            {
                                "target_lang_word": "гниющие остатки",
                                "target_lang_sense_clarification": "остатки разложившихся продуктов"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
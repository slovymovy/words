{
    "word_family": [
        "thou",
        "thee",
        "thine",
        "thyself"
    ],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "16c1b65c-8249-370a-a963-76a93a105db8",
                    "sense_definition": "The possessive form of <w>thou</w>; belonging to you (singular).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Love <w>thy</w> neighbor as thyself.",
                            "target_lang_translations": {
                                "ru": "Возлюби ближнего <w>твоего</w>, как самого себя."
                            }
                        },
                        {
                            "text": "To <w>thy</w> own self be true.",
                            "target_lang_translations": {
                                "ru": "Будь верен <w>своему</w> 'я'."
                            }
                        }
                    ],
                    "synonyms": [
                        "thine"
                    ],
                    "common_phrases": [
                        "love thy neighbor",
                        "honor thy father",
                        "with all thy heart"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "standard in older texts, replaced by 'your' in modern usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Притяжательная форма местоимения <w>thou</w>; принадлежащий тебе (единственное число)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "твой",
                                "target_lang_sense_clarification": "принадлежащий тебе (устар./поэт.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "a83eee3f-ed1b-3d0c-8aae-8abb0aef574a",
                    "sense_definition": "Used in the phrase 'for thy', an alternative form of <w>forthy</w> meaning because or therefore.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He stayed behind, for <w>thy</w> he was ill.",
                            "target_lang_translations": {
                                "ru": "Он остался, <w>ибо</w> был болен."
                            }
                        },
                        {
                            "text": "She spoke the truth, for <w>thy</w> I trusted her.",
                            "target_lang_translations": {
                                "ru": "Она говорила правду, <w>поэтому</w> я поверил ей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "for thy"
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": "found only in Middle English or very archaic texts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в словосочетании 'for thy', альтернативной форме <w>forthy</w>, означающей 'потому что' или 'следовательно'."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }
    ]
}
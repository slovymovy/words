    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "97303771-16c0-3b32-8598-418185f24cf6",
                    "sense_definition": "Ogół gatunków roślin występujących na określonym obszarze geograficznym lub w danym środowisku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "biology_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wysokich Tatrach występują piękne i rzadkie okazy górskiej <w>flory</w>.",
                            "target_lang_translations": {
                                "ru": "В высоких Татрах встречаются красивые и редкие экземпляры горной <w>флоры</w>."
                            }
                        },
                        {
                            "text": "Lokalna <w>flora</w> i fauna ucierpiały w wyniku wielkiego pożaru lasu.",
                            "target_lang_translations": {
                                "ru": "Местная <w>флора</w> и фауна пострадали в результате сильного лесного пожара."
                            }
                        }
                    ],
                    "synonyms": [
                        "roślinność",
                        "szata roślinna"
                    ],
                    "antonyms": [
                        "fauna"
                    ],
                    "common_phrases": [
                        "flora i fauna",
                        "flora bakteryjna",
                        "flora jelitowa",
                        "ochrona flory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность видов растений, произрастающих на определённой географической территории или в данной среде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "флора",
                                "target_lang_sense_clarification": "растительный мир"
                            },
                            {
                                "target_lang_word": "растительность",
                                "target_lang_sense_clarification": "совокупность растений"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "79a47044-99e8-308d-adee-c5b2541a7c3f",
                    "sense_definition": "Żeńskie imię pochodzenia łacińskiego, oznaczające kwiat.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "person_name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Moja ciocia <w>Flora</w> zawsze przynosi nam pyszne ciasto na święta.",
                            "target_lang_translations": {
                                "ru": "Моя тётя <w>Флора</w> всегда приносит нам вкусный пирог на праздники."
                            }
                        },
                        {
                            "text": "Na liście zaproszonych gości znalazła się również kuzynka <w>Flora</w>.",
                            "target_lang_translations": {
                                "ru": "В списке приглашённых гостей оказалась также кузина <w>Флора</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Florka",
                        "Florcia",
                        "Floreczka",
                        "Florusia",
                        "Florunia"
                    ],
                    "common_phrases": [
                        "imieniny Flory",
                        "pani Flora"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя латинского происхождения, означающее «цветок»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Флора",
                                "target_lang_sense_clarification": "женское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4d378a1-5346-3f09-a9b8-18bbe0329be8",
                    "sense_definition": "W mitologii rzymskiej bogini kwiatów, wiosny i urodzaju.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mythology_group",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Starożytni Rzymianie oddawali cześć bogini <w>Florze</w> podczas wiosennych świąt.",
                            "target_lang_translations": {
                                "ru": "Древние римляне почитали богиню <w>Флору</w> во время весенних праздников."
                            }
                        },
                        {
                            "text": "Na słynnym obrazie renesansowego malarza <w>Flora</w> sypie kwiatami z kosza.",
                            "target_lang_translations": {
                                "ru": "На знаменитой картине художника эпохи Возрождения <w>Флора</w> сыплет цветами из корзины."
                            }
                        }
                    ],
                    "synonyms": [
                        "Chloris"
                    ],
                    "common_phrases": [
                        "świątynia Flory",
                        "kult Flory"
                    ],
                    "target_lang_definitions": {
                        "ru": "В римской мифологии богиня цветов, весны и плодородия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Флора",
                                "target_lang_sense_clarification": "мифология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1df71e1e-092e-38b5-aaf2-77f1c34078c1",
                    "sense_definition": "Duża planetoida krążąca w pasie głównym między orbitami Marsa i Jowisza.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "astronomy_group",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "<w>Flora</w> porusza się w obszarze między orbitami Marsa i Jowisza.",
                            "target_lang_translations": {
                                "ru": "<w>Флора</w> движется в области между орбитами Марса и Юпитера."
                            }
                        },
                        {
                            "text": "Astronomowie obserwują, jak <w>Flora</w> przemieszcza się na tle gwiazd.",
                            "target_lang_translations": {
                                "ru": "Астрономы наблюдают, как <w>Флора</w> перемещается на фоне звёзд."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупный астероид, вращающийся в главном поясе между орбитами Марса и Юпитера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Флора",
                                "target_lang_sense_clarification": "астрономия"
                            }
                        ]
                    }
                }
            ]
        }

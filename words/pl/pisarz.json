                    "sense_id": "29a1a998-10a9-3a96-912c-f511d2e126f0",
                    "sense_definition": "Osoba, która tworzy utwory literackie, zwłaszcza prozą.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "literature_creator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój ulubiony <w>pisarz</w> wydał wczoraj nową powieść kryminalną.",
                            "target_lang_translations": {
                                "ru": "Мой любимый <w>писатель</w> вчера выпустил новый детективный роман."
                            }
                        },
                        {
                            "text": "Spotkanie autorskie z tym <w>pisarzem</w> przyciągnęło tłumy czytelników.",
                            "target_lang_translations": {
                                "ru": "Творческая встреча с этим <w>писателем</w> привлекла толпы читателей."
                            }
                        }
                    ],
                    "synonyms": [
                        "literat",
                        "prozaik"
                    ],
                    "common_phrases": [
                        "znany pisarz",
                        "współczesny pisarz",
                        "pisarz science-fiction",
                        "wybitny pisarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который создает литературные произведения, особенно прозу."

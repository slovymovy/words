{
    "word_family": [
        "zowat",
        "watje",
        "ietwat"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "dce6db5c-3ac4-3075-99ff-ecbbfa548683",
                    "sense_definition": "Vragend voornaamwoord dat gebruikt wordt om te informeren naar een zaak, gebeurtenis of inhoud.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "interrogative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Wat</w> is jouw favoriete kleur?",
                            "target_lang_translations": {
                                "en": "<w>What</w> is your favorite color?",
                                "ru": "<w>Какой</w> твой любимый цвет?"
                            }
                        },
                        {
                            "text": "<w>Wat</w> gaan we vanavond eten?",
                            "target_lang_translations": {
                                "en": "<w>What</w> are we going to eat tonight?",
                                "ru": "<w>Что</w> мы будем есть сегодня вечером?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "wat is dit",
                        "wat dan ook",
                        "weet je wat"
                    ],
                    "target_lang_definitions": {
                        "en": "Interrogative pronoun used to inquire about a thing, event, or content.",
                        "ru": "Вопросительное местоимение, используемое для получения информации о предмете, событии или содержании."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "what",
                                "target_lang_sense_clarification": "used to ask questions"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "вопросительное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c548112f-c2f2-34d2-b7ae-d51f7fc99c1a",
                    "sense_definition": "Duidt een onbepaalde zaak of hoeveelheid aan; synoniem voor 'iets'.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "indefinite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb je <w>wat</w> te drinken voor mij?",
                            "target_lang_translations": {
                                "en": "Do you have <w>something</w> to drink for me?",
                                "ru": "У тебя есть <w>что-нибудь</w> попить для меня?"
                            }
                        },
                        {
                            "text": "Er moet hier <w>wat</w> veranderen.",
                            "target_lang_translations": {
                                "en": "<w>Something</w> has to change here.",
                                "ru": "Здесь должно <w>что-то</w> измениться."
                            }
                        }
                    ],
                    "synonyms": [
                        "ietwat",
                        "ietwat"
                    ],
                    "antonyms": [
                        "niets"
                    ],
                    "common_phrases": [
                        "net wat",
                        "heel wat"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates an indefinite thing or quantity; synonym for 'something'.",
                        "ru": "Указывает на неопределенный предмет или количество; синоним слова «что-то»."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "something",
                                "target_lang_sense_clarification": "unspecified thing"
                            },
                            {
                                "target_lang_word": "anything",
                                "target_lang_sense_clarification": "in questions or negatives"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "что-то",
                                "target_lang_sense_clarification": "неопределенное"
                            },
                            {
                                "target_lang_word": "что-нибудь",
                                "target_lang_sense_clarification": "в вопросах или просьбах"
                            },
                            {
                                "target_lang_word": "кое-что",
                                "target_lang_sense_clarification": "конкретное, но неназванное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d629ba65-c9ae-303c-976d-98259a744005",
                    "sense_definition": "Betrekkelijk voornaamwoord dat verwijst naar onbepaalde woorden zoals 'alles', 'iets', 'niets' of 'datgene'.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is alles <w>wat</w> ik erover weet.",
                            "target_lang_translations": {
                                "en": "That is everything <w>that</w> I know about it.",
                                "ru": "Это всё, <w>что</w> я об этом знаю."
                            }
                        },
                        {
                            "text": "Er is niets <w>wat</w> hij niet kan maken.",
                            "target_lang_translations": {
                                "en": "There is nothing <w>that</w> he cannot make.",
                                "ru": "Нет ничего, <w>чего</w> он не мог бы сделать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "alles wat",
                        "datgene wat"
                    ],
                    "target_lang_definitions": {
                        "en": "Relative pronoun referring to indefinite words like 'everything', 'something', 'nothing', or 'that which'.",
                        "ru": "Относительное местоимение, которое ссылается на неопределенные слова, такие как «всё», «что-то», «ничего» или «то»."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "relative pronoun after indefinite antecedents"
                            },
                            {
                                "target_lang_word": "which",
                                "target_lang_sense_clarification": "relative pronoun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "союзное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ddb7799e-3d9d-31c8-8c4b-936ee8ab171b",
                    "sense_definition": "Betrekkelijk voornaamwoord dat terugverwijst naar de volledige voorgaande zin.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kwam veel te laat, <w>wat</w> ik erg vervelend vond.",
                            "target_lang_translations": {
                                "en": "He arrived much too late, <w>which</w> I found very annoying.",
                                "ru": "Он пришел слишком поздно, <w>что</w> мне очень не понравилось."
                            }
                        },
                        {
                            "text": "Ze is geslaagd voor haar examen, <w>wat</w> gevierd moet worden.",
                            "target_lang_translations": {
                                "en": "She passed her exam, <w>which</w> must be celebrated.",
                                "ru": "Она сдала экзамен, <w>что</w> нужно отпраздновать."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Relative pronoun referring back to the entire preceding clause.",
                        "ru": "Относительное местоимение, которое отсылает ко всему предыдущему предложению."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "which",
                                "target_lang_sense_clarification": "referring to a previous statement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "союзное слово"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "bb9287cb-02b1-3dde-90ee-68193c670dc0",
                    "sense_definition": "Een onbepaalde, meestal kleine hoeveelheid.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil graag <w>wat</w> water drinken.",
                            "target_lang_translations": {
                                "en": "I would like to drink <w>some</w> water.",
                                "ru": "Я бы хотел выпить <w>немного</w> воды."
                            }
                        },
                        {
                            "text": "Hij heeft <w>wat</w> appels gekocht op de markt.",
                            "target_lang_translations": {
                                "en": "He bought <w>some</w> apples at the market.",
                                "ru": "Он купил <w>несколько</w> яблок на рынке."
                            }
                        }
                    ],
                    "synonyms": [
                        "een beetje",
                        "een tikkeltje"
                    ],
                    "antonyms": [
                        "veel"
                    ],
                    "common_phrases": [
                        "wat geld",
                        "wat tijd"
                    ],
                    "target_lang_definitions": {
                        "en": "An indefinite, usually small quantity.",
                        "ru": "Неопределенное, обычно небольшое количество."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "some",
                                "target_lang_sense_clarification": "unspecified amount"
                            },
                            {
                                "target_lang_word": "a little",
                                "target_lang_sense_clarification": "small amount"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "немного",
                                "target_lang_sense_clarification": "о количестве"
                            },
                            {
                                "target_lang_word": "несколько",
                                "target_lang_sense_clarification": "об исчисляемых предметах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "607e0343-2053-3b22-bc6c-72b62d575af3",
                    "sense_definition": "Gebruikt in een uitroep om verbazing of emotie uit te drukken over een zelfstandig naamwoord.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "exclamatory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Wat</w> een mooi huis is dit!",
                            "target_lang_translations": {
                                "en": "<w>What</w> a beautiful house this is!",
                                "ru": "<w>Какой</w> красивый это дом!"
                            }
                        },
                        {
                            "text": "<w>Wat</w> een onzin vertelt hij toch.",
                            "target_lang_translations": {
                                "en": "<w>What</w> nonsense he is talking.",
                                "ru": "<w>Какую</w> чушь он несет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wat een"
                    ],
                    "target_lang_definitions": {
                        "en": "Used in an exclamation to express surprise or emotion regarding a noun.",
                        "ru": "Используется в восклицаниях для выражения удивления или эмоций по поводу существительного."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "what",
                                "target_lang_sense_clarification": "exclamation with nouns"
                            },
                            {
                                "target_lang_word": "what a",
                                "target_lang_sense_clarification": "exclamation with singular countable nouns"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "какой",
                                "target_lang_sense_clarification": "восклицание"
                            },
                            {
                                "target_lang_word": "что за",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f3e2c42f-e7c5-3b00-82d3-d51a1ce736e4",
                    "sense_definition": "Gebruikt om een eigenschap of emotie te versterken in een uitroep; hoe.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "exclamatory_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Wat</w> leuk dat je er bent!",
                            "target_lang_translations": {
                                "en": "<w>How</w> nice that you are here!",
                                "ru": "<w>Как</w> здорово, что ты здесь!"
                            }
                        },
                        {
                            "text": "<w>Wat</w> ziet zij er goed uit vandaag.",
                            "target_lang_translations": {
                                "en": "<w>How</w> good she looks today.",
                                "ru": "<w>Как</w> хорошо она сегодня выглядит."
                            }
                        }
                    ],
                    "synonyms": [
                        "een beetje",
                        "een tikkeltje"
                    ],
                    "common_phrases": [
                        "wat leuk",
                        "wat jammer",
                        "wat goed"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to emphasize a quality or emotion in an exclamation; how.",
                        "ru": "Используется для усиления качества или эмоции в восклицании; как."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "how",
                                "target_lang_sense_clarification": "exclamation with adjectives"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "усиление эмоции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3629998-3842-326a-857f-80752a7a40c8",
                    "sense_definition": "In lichte mate; niet volledig.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij voelt zich vandaag <w>wat</w> ziekjes.",
                            "target_lang_translations": {
                                "en": "He feels <w>somewhat</w> sick today.",
                                "ru": "Он чувствует себя сегодня <w>немного</w> приболевшим."
                            }
                        },
                        {
                            "text": "De soep is <w>wat</w> flauw van smaak.",
                            "target_lang_translations": {
                                "en": "The soup tastes <w>a bit</w> bland.",
                                "ru": "Суп <w>немного</w> пресноват."
                            }
                        }
                    ],
                    "synonyms": [
                        "een beetje",
                        "een tikkeltje",
                        "ietwat"
                    ],
                    "common_phrases": [
                        "wat later",
                        "wat beter"
                    ],
                    "target_lang_definitions": {
                        "en": "To a slight degree; not completely.",
                        "ru": "В некоторой степени; не полностью."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "somewhat",
                                "target_lang_sense_clarification": "to some extent"
                            },
                            {
                                "target_lang_word": "a bit",
                                "target_lang_sense_clarification": "informal degree"
                            },
                            {
                                "target_lang_word": "a little",
                                "target_lang_sense_clarification": "small degree"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "немного",
                                "target_lang_sense_clarification": "степень"
                            },
                            {
                                "target_lang_word": "слегка",
                                "target_lang_sense_clarification": "степень"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
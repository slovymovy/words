    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a7c1a4cc-471f-31fe-b03f-b07e1762520b",
                    "sense_definition": "Organizm żyjący na innym organizmie lub wewnątrz niego i czerpiący z niego pożywienie, często wyrządzając szkody.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Badania wykazały, że w jelitach pacjenta zagnieździł się rzadki <parazyt>.",
                            "target_lang_translations": {
                                "ru": "Исследования показали, что в кишечнике пациента поселился редкий <w>паразит</w>."
                            }
                        },
                        {
                            "text": "Ten mikroskopijny <parazyt> wywołuje groźną chorobę u zwierząt hodowlanych.",
                            "target_lang_translations": {
                                "ru": "Этот микроскопический <w>паразит</w> вызывает опасное заболевание у сельскохозяйственных животных."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasożyt"
                    ],
                    "antonyms": [
                        "żywiciel"
                    ],
                    "common_phrases": [
                        "parazyt zewnętrzny",
                        "parazyt wewnętrzny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организм, живущий на другом организме или внутри него и питающийся за его счет, часто причиняя вред."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паразит",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85367e5e-e432-3470-9736-4dce1034607a",
                    "sense_definition": "Człowiek, który żyje z pracy innych ludzi, wykorzystuje ich i nie daje nic w zamian.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Społeczeństwo nie powinno tolerować takiego <parazyta>, który unika uczciwej pracy.",
                            "target_lang_translations": {
                                "ru": "Общество не должно терпеть такого <w>паразита</w>, который избегает честного труда."
                            }
                        },
                        {
                            "text": "Nie bądź <parazytem> i zacznij w końcu sam zarabiać na swoje utrzymanie.",
                            "target_lang_translations": {
                                "ru": "Не будь <w>паразитом</w> и начни наконец сам зарабатывать себе на жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasożyt",
                        "naciągacz"
                    ],
                    "common_phrases": [
                        "społeczny parazyt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который живет за счет труда других людей, использует их и ничего не дает взамен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паразит",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "тунеядец",
                                "target_lang_sense_clarification": "человек, живущий за чужой счет"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
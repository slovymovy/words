                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To a higher organization or to the management"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "higher up",
                                "target_lang_sense_clarification": "to superiors"
                            },
                            {
                                "target_lang_word": "to the top",
                                "target_lang_sense_clarification": "to management"
                            }
                        ]

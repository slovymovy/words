            "senses": [
                {
                    "sense_id": "a3eae1a0-1e0e-32a4-b3f2-c12b55e5f950",
                    "sense_definition": "Dawna srebrna moneta o wartości sześciu groszy, bita w Polsce i na Litwie od XVI do XVIII wieku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "coins",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kolekcjoner zapłacił wysoką cenę za rzadki <w>szóstak</w> z czasów Zygmunta III Wazy.",
                            "target_lang_translations": {
                                "ru": "Коллекционер заплатил высокую цену за редкий <w>шостак</w> времен Сигизмунда III Вазы."
                            }
                        },
                        {
                            "text": "W starej skrzyni znaleziono jeden srebrny <w>szóstak</w> i kilka miedzianych monet.",
                            "target_lang_translations": {
                                "ru": "В старом сундуке нашли один серебряный <w>шостак</w> и несколько медных монет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "srebrny szóstak",
                        "koronny szóstak"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical coin"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная серебряная монета достоинством в шесть грошей, чеканившаяся в Польше и Литве с XVI по XVIII век."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шостак",
                                "target_lang_sense_clarification": "старинная монета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2401f7bf-40b0-3114-acbe-4fad58b3947f",
                    "sense_definition": "Jeleń lub kozioł sarny mający poroże z sześcioma odnogami.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "hunting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myśliwy przez lornetkę obserwował pasącego się na polanie <w>szóstaka</w>.",
                            "target_lang_translations": {
                                "ru": "Охотник в бинокль наблюдал за пасущимся на поляне <w>самцом-шестёркой</w>."
                            }
                        },
                        {
                            "text": "To był piękny, regularny <w>szóstak</w>, którego poroże robiło duże wrażenie.",
                            "target_lang_translations": {
                                "ru": "Это был красивый, правильный <w>самец-шестёрка</w>, чьи рога производили большое впечатление."
                            }
                        }
                    ],
                    "common_phrases": [
                        "regularny szóstak",
                        "mocny szóstak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Олень или самец косули, имеющий рога с шестью отростками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шестёрка",
                                "target_lang_sense_clarification": "охотничий жаргон: животное с рогами на 6 отростков"
                            },
                            {
                                "target_lang_word": "самец с шестью отростками",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        },

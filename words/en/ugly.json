                            },
                            {
                                "target_lang_word": "гнусный",
                                "target_lang_sense_clarification": "морально отвратительный"
                            },
                            {
                                "target_lang_word": "подлый",
                                "target_lang_sense_clarification": "низкий, безнравственный"
                            },
                            {
                                "target_lang_word": "некрасивый",
                                "target_lang_sense_clarification": "перен. непорядочный"
                            }
                        ]
                    }
                }
            ]
        }

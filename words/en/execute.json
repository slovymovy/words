{
    "word_family": [
        "execution",
        "executive",
        "executor",
        "executable",
        "executioner"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7323ba6c-1cad-316a-8c2e-232458053f1c",
                    "sense_definition": "To carry out or put a plan, order, or course of action into effect.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>execute</w> our plan carefully to succeed.",
                            "target_lang_translations": {
                                "ru": "Нам нужно тщательно <w>выполнить</w> наш план, чтобы добиться успеха."
                            }
                        },
                        {
                            "text": "The soldiers were trained to <w>execute</w> orders immediately.",
                            "target_lang_translations": {
                                "ru": "Солдаты были обучены немедленно <w>исполнять</w> приказы."
                            }
                        }
                    ],
                    "synonyms": [
                        "carry out",
                        "do"
                    ],
                    "common_phrases": [
                        "execute a plan",
                        "execute an order",
                        "execute a strategy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять или приводить в действие план, приказ или порядок действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выполнять",
                                "target_lang_sense_clarification": "осуществлять, делать"
                            },
                            {
                                "target_lang_word": "исполнять",
                                "target_lang_sense_clarification": "приводить в исполнение (приказ, желание)"
                            },
                            {
                                "target_lang_word": "осуществлять",
                                "target_lang_sense_clarification": "претворять в жизнь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2b7786b-2e29-3b87-9a66-85b812ac8b8d",
                    "sense_definition": "To kill someone, especially as a legal punishment for a serious crime.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "kill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government threatened to <w>execute</w> the spies if they were caught.",
                            "target_lang_translations": {
                                "ru": "Правительство угрожало <w>казнить</w> шпионов, если их поймают."
                            }
                        },
                        {
                            "text": "They plan to <w>execute</w> the prisoner at dawn.",
                            "target_lang_translations": {
                                "ru": "Они планируют <w>казнить</w> заключенного на рассвете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "execute a prisoner",
                        "execute a criminal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убить кого-либо, особенно в качестве законного наказания за тяжкое преступление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "казнить",
                                "target_lang_sense_clarification": "подвергать смертной казни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "492a4201-be7d-378a-a440-466921678612",
                    "sense_definition": "To start, launch, or run a computer program or command.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Click the icon to <w>execute</w> the program on your computer.",
                            "target_lang_translations": {
                                "ru": "Нажмите на значок, чтобы <w>запустить</w> программу на вашем компьютере."
                            }
                        },
                        {
                            "text": "The server failed to <w>execute</w> the requested script.",
                            "target_lang_translations": {
                                "ru": "Серверу не удалось <w>выполнить</w> запрошенный скрипт."
                            }
                        }
                    ],
                    "synonyms": [
                        "start",
                        "launch",
                        "run",
                        "open"
                    ],
                    "common_phrases": [
                        "execute a program",
                        "execute a command",
                        "execute a script"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запустить или начать выполнение компьютерной программы или команды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выполнять",
                                "target_lang_sense_clarification": "команду или скрипт"
                            },
                            {
                                "target_lang_word": "запускать",
                                "target_lang_sense_clarification": "программу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79e1f2dc-4f2f-371a-a421-3dd014db2ccf",
                    "sense_definition": "To perform a difficult action, movement, or piece of art with skill.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gymnast managed to <w>execute</w> a perfect landing.",
                            "target_lang_translations": {
                                "ru": "Гимнастке удалось <w>выполнить</w> идеальное приземление."
                            }
                        },
                        {
                            "text": "It is difficult to <w>execute</w> this piece of music without practice.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>исполнить</w> это музыкальное произведение без практики."
                            }
                        }
                    ],
                    "synonyms": [
                        "perform"
                    ],
                    "common_phrases": [
                        "execute a turn",
                        "execute a move",
                        "execute a jump"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнить сложное действие, движение или художественное произведение с мастерством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исполнять",
                                "target_lang_sense_clarification": "произведение искусства, музыку, роль"
                            },
                            {
                                "target_lang_word": "выполнять",
                                "target_lang_sense_clarification": "технический элемент, трюк"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8253bf65-6f11-3be4-8b13-d56c5898653a",
                    "sense_definition": "To make a legal document valid by signing it.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The partners met to <w>execute</w> the contract for the new business.",
                            "target_lang_translations": {
                                "ru": "Партнеры встретились, чтобы <w>подписать</w> контракт для нового бизнеса."
                            }
                        },
                        {
                            "text": "You must <w>execute</w> the will in the presence of witnesses.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>оформить</w> завещание в присутствии свидетелей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "execute a contract",
                        "execute a will",
                        "execute a deed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придать юридическую силу документу путем его подписания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подписывать",
                                "target_lang_sense_clarification": "ставить подпись на документе"
                            },
                            {
                                "target_lang_word": "оформлять",
                                "target_lang_sense_clarification": "составлять документ надлежащим образом"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
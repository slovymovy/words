    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e69597bf-94a2-3fec-a216-0dd594abc127",
                    "sense_definition": "Bezpowrotnie stracić coś cennego (np. zasoby, czas, pieniądze), używając tego w sposób nieprzemyślany, nieefektywny lub bezcelowy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "waste_resources",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie chcę <w>zmarnować</w> całego dnia na oglądanie telewizji.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <w>потратить впустую</w> весь день на просмотр телевизора."
                            }
                        },
                        {
                            "text": "Kupił mnóstwo niepotrzebnych rzeczy i w ten sposób <zmarnował> wszystkie oszczędności.",
                            "target_lang_translations": {
                                "ru": "Он купил множество ненужных вещей и таким образом <w>растратил</w> все сбережения."
                            }
                        }
                    ],
                    "synonyms": [
                        "stracić",
                        "roztrwonić",
                        "przepuścić"
                    ],
                    "antonyms": [
                        "zaoszczędzić",
                        "wykorzystać",
                        "zyskać"
                    ],
                    "common_phrases": [
                        "zmarnować czas",
                        "zmarnować pieniądze",
                        "zmarnować okazję",
                        "zmarnować szansę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Безвозвратно потерять что-то ценное (например, ресурсы, время, деньги), используя это необдуманно, неэффективно или бесцельно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потратить впустую",
                                "target_lang_sense_clarification": "время, усилия"
                            },
                            {
                                "target_lang_word": "растратить",
                                "target_lang_sense_clarification": "деньги, ресурсы"
                            },
                            {
                                "target_lang_word": "упустить",
                                "target_lang_sense_clarification": "шанс, возможность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe04ff66-21dd-39ff-9b3d-22d3c85c57c5",
                    "sense_definition": "Doprowadzić do zniszczenia lub niewykorzystania czyjegoś potencjału, zdolności lub dorobku; sprawić, że coś przepada.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "ruin_potential",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel ostrzegał go, żeby nie <zmarnował> swojego wielkiego talentu muzycznego.",
                            "target_lang_translations": {
                                "ru": "Учитель предупреждал его, чтобы он не <w>растратил</w> свой великий музыкальный талант."
                            }
                        },
                        {
                            "text": "Przez złe decyzje i brak dyscypliny <zmarnował> sobie życie.",
                            "target_lang_translations": {
                                "ru": "Из-за плохих решений и отсутствия дисциплины он <w>загубил</w> себе жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaprzepaścić",
                        "zniszczyć",
                        "zepsuć"
                    ],
                    "antonyms": [
                        "rozwinąć",
                        "pomnożyć",
                        "uratować"
                    ],
                    "common_phrases": [
                        "zmarnować talent",
                        "zmarnować życie",
                        "zmarnować zdrowie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести к разрушению или неиспользованию чьего-либо потенциала, способностей или достижений; позволить чему-то пропасть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загубить",
                                "target_lang_sense_clarification": "талант, жизнь"
                            },
                            {
                                "target_lang_word": "испортить",
                                "target_lang_sense_clarification": "жизнь, судьбу"
                            },
                            {
                                "target_lang_word": "растратить",
                                "target_lang_sense_clarification": "потенциал, способности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
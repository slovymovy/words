            "senses": [
                {
                    "sense_id": "0f2a17c9-8649-3d77-90af-5023ddc88a6b",
                    "sense_definition": "Having the physical characteristics or appearance typical of a man; masculine.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "manly_qualities",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spoke in a deep, <w>manly</w> voice that echoed through the room.",
                            "target_lang_translations": {
                                "ru": "Он говорил низким, <w>мужественным</w> голосом, который эхом разносился по комнате."
                            }
                        },
                        {
                            "text": "The actor was chosen for his rugged and <w>manly</w> appearance.",
                            "target_lang_translations": {
                                "ru": "Актёра выбрали за его суровую и <w>мужественную</w> внешность."
                            }

{
    "word_family": [
        "voorstel",
        "voorstelling",
        "voorstelbaar",
        "onvoorstelbaar"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fdf2cf54-01bf-32ec-9906-aed051c47070",
                    "sense_definition": "Iemand aan een ander presenteren door de naam te noemen, of de eigen naam noemen bij een eerste ontmoeting.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "introduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik mijn nieuwe collega aan je <w>voorstellen</w>?",
                            "target_lang_translations": {
                                "en": "May I <w>introduce</w> my new colleague to you?",
                                "ru": "Могу я <w>представить</w> тебе моего нового коллегу?"
                            }
                        },
                        {
                            "text": "Hij <w>stelde</w> zich netjes <w>voor</w> aan de ouders van zijn vriendin.",
                            "target_lang_translations": {
                                "en": "He politely <w>introduced</w> himself to his girlfriend's parents.",
                                "ru": "Он вежливо <w>представился</w> родителям своей девушки."
                            }
                        }
                    ],
                    "synonyms": [
                        "introduceren",
                        "bekendmaken"
                    ],
                    "common_phrases": [
                        "zich voorstellen",
                        "iemand voorstellen aan"
                    ],
                    "target_lang_definitions": {
                        "en": "To present someone to another person by name, or to state one's own name at a first meeting.",
                        "ru": "Представить кого-то другому человеку, назвав имя, или назвать свое имя при первой встрече."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "introduce",
                                "target_lang_sense_clarification": "make someone known by name"
                            },
                            {
                                "target_lang_word": "present",
                                "target_lang_sense_clarification": "introduce formally"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "представлять",
                                "target_lang_sense_clarification": "знакомить кого-то"
                            },
                            {
                                "target_lang_word": "представляться",
                                "target_lang_sense_clarification": "называть себя"
                            },
                            {
                                "target_lang_word": "знакомить",
                                "target_lang_sense_clarification": "сводить людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3e5f594-626a-3191-82ac-685fa2c7d859",
                    "sense_definition": "Een idee, plan of suggestie naar voren brengen ter overweging.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "proposal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De werkgroep zal morgen een nieuw beleid <w>voorstellen</w> aan het management.",
                            "target_lang_translations": {
                                "en": "The working group will <w>propose</w> a new policy to management tomorrow.",
                                "ru": "Завтра рабочая группа <w>предложит</w> руководству новую политику."
                            }
                        },
                        {
                            "text": "Ik <w>stel</w> <w>voor</w> dat we even pauze nemen en daarna verdergaan.",
                            "target_lang_translations": {
                                "en": "I <w>suggest</w> that we take a break and continue afterwards.",
                                "ru": "Я <w>предлагаю</w> сделать небольшой перерыв и затем продолжить."
                            }
                        }
                    ],
                    "synonyms": [
                        "opperen",
                        "suggereren",
                        "voordragen"
                    ],
                    "antonyms": [
                        "verwerpen",
                        "afwijzen"
                    ],
                    "common_phrases": [
                        "een oplossing voorstellen",
                        "een plan voorstellen"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring forward an idea, plan, or suggestion for consideration.",
                        "ru": "Выдвинуть идею, план или предложение на рассмотрение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "propose",
                                "target_lang_sense_clarification": "put forward a plan"
                            },
                            {
                                "target_lang_word": "suggest",
                                "target_lang_sense_clarification": "offer an idea"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "предлагать",
                                "target_lang_sense_clarification": "выдвигать идею"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9b9b20d-c703-341f-910c-dd9803090f91",
                    "sense_definition": "Zich in gedachten een beeld vormen van iets dat er niet is of nog moet gebeuren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "imagination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan me niet <w>voorstellen</w> hoe moeilijk die situatie voor jou moet zijn.",
                            "target_lang_translations": {
                                "en": "I cannot <w>imagine</w> how difficult that situation must be for you.",
                                "ru": "Я не могу себе <w>представить</w>, насколько трудной должна быть для тебя эта ситуация."
                            }
                        },
                        {
                            "text": "Stel je <w>voor</w> dat we de loterij zouden winnen.",
                            "target_lang_translations": {
                                "en": "<w>Imagine</w> if we were to win the lottery.",
                                "ru": "<w>Представь</w> себе, что мы выиграли бы в лотерею."
                            }
                        }
                    ],
                    "synonyms": [
                        "inbeelden",
                        "indenken"
                    ],
                    "common_phrases": [
                        "zich kunnen voorstellen",
                        "stel je voor"
                    ],
                    "target_lang_definitions": {
                        "en": "To form a mental image of something that is not present or has not yet happened.",
                        "ru": "Создать мысленный образ чего-то, чего нет или что еще должно произойти."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "imagine",
                                "target_lang_sense_clarification": "form a mental image"
                            },
                            {
                                "target_lang_word": "picture",
                                "target_lang_sense_clarification": "visualize"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "представлять",
                                "target_lang_sense_clarification": "создавать образ в уме"
                            },
                            {
                                "target_lang_word": "воображать",
                                "target_lang_sense_clarification": "фантазировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f87627d0-45d5-3d41-bd63-e8cbea5f8cbe",
                    "sense_definition": "Iets uitbeelden, betekenen of weergeven, bijvoorbeeld in een kunstwerk of toneelstuk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit schilderij <w>stelt</w> een landschap in de winter <w>voor</w>.",
                            "target_lang_translations": {
                                "en": "This painting <w>depicts</w> a landscape in winter.",
                                "ru": "Эта картина <w>изображает</w> зимний пейзаж."
                            }
                        },
                        {
                            "text": "De acteur <w>stelt</w> een koning <w>voor</w> die zijn macht verliest.",
                            "target_lang_translations": {
                                "en": "The actor <w>portrays</w> a king who is losing his power.",
                                "ru": "Актер <w>изображает</w> короля, который теряет свою власть."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitbeelden",
                        "weergeven",
                        "representeren"
                    ],
                    "target_lang_definitions": {
                        "en": "To depict, signify, or represent something, for example in a work of art or a play.",
                        "ru": "Изображать, означать или отображать что-либо, например, в произведении искусства или пьесе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "represent",
                                "target_lang_sense_clarification": "stand for or correspond to"
                            },
                            {
                                "target_lang_word": "depict",
                                "target_lang_sense_clarification": "show in art"
                            },
                            {
                                "target_lang_word": "portray",
                                "target_lang_sense_clarification": "play a role"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "изображать",
                                "target_lang_sense_clarification": "показывать в искусстве"
                            },
                            {
                                "target_lang_word": "представлять собой",
                                "target_lang_sense_clarification": "являться чем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05774876-efbc-36f3-9412-ac5f2f1eede7",
                    "sense_definition": "Betekenis of waarde hebben (vaak gebruikt in ontkenningen om aan te geven dat iets onbelangrijk is).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het probleem <w>stelt</w> eigenlijk niet zoveel <w>voor</w> als we dachten.",
                            "target_lang_translations": {
                                "en": "The problem actually doesn't <w>amount to</w> as much as we thought.",
                                "ru": "Проблема на самом деле не так много из себя <w>представляет</w>, как мы думали."
                            }
                        },
                        {
                            "text": "Zijn bijdrage aan het project <w>stelde</w> helaas weinig <w>voor</w>.",
                            "target_lang_translations": {
                                "en": "His contribution to the project unfortunately <w>meant</w> little.",
                                "ru": "Его вклад в проект, к сожалению, мало что из себя <w>представлял</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "betekenen",
                        "inhouden"
                    ],
                    "common_phrases": [
                        "niet veel voorstellen",
                        "weinig voorstellen"
                    ],
                    "target_lang_definitions": {
                        "en": "To have meaning or importance (often used in negative contexts to indicate unimportance).",
                        "ru": "Иметь значение или ценность (часто используется в отрицаниях, чтобы показать, что что-то неважно)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "amount to",
                                "target_lang_sense_clarification": "be equivalent to in value"
                            },
                            {
                                "target_lang_word": "mean",
                                "target_lang_sense_clarification": "have importance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "представлять из себя",
                                "target_lang_sense_clarification": "иметь значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8bffa5c3-f750-3730-b488-d0a8e1f69c9a",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>voorstel</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De commissie heeft alle ingediende <w>voorstellen</w> zorgvuldig bekeken.",
                            "target_lang_translations": {
                                "en": "The committee has carefully reviewed all submitted <w>proposals</w>.",
                                "ru": "Комиссия тщательно рассмотрела все поданные <w>предложения</w>."
                            }
                        },
                        {
                            "text": "Heb jij nog goede <w>voorstellen</w> voor onze vakantiebestemming?",
                            "target_lang_translations": {
                                "en": "Do you have any good <w>suggestions</w> for our holiday destination?",
                                "ru": "У тебя есть еще хорошие <w>предложения</w> для места нашего отпуска?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of the noun 'voorstel' (proposal).",
                        "ru": "Множественное число существительного 'voorstel' (предложение)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "proposals",
                                "target_lang_sense_clarification": "plans or offers"
                            },
                            {
                                "target_lang_word": "suggestions",
                                "target_lang_sense_clarification": "ideas put forward"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "предложения",
                                "target_lang_sense_clarification": "идеи, планы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
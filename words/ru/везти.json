    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c309ca18-3693-3c99-ab78-a9726817d68e",
                    "sense_definition": "Перемещать кого-либо или что-либо с помощью транспортного средства в определённом направлении.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автобус <везёт> пассажиров в центр города.",
                            "target_lang_translations": {
                                "en": "The bus is <carrying> passengers to the city center."
                            }
                        },
                        {
                            "text": "Мы <везли> старый диван на дачу на машине.",
                            "target_lang_translations": {
                                "en": "We were <transporting> the old sofa to the summer house by car."
                            }
                        }
                    ],
                    "synonyms": [
                        "транспортировать"
                    ],
                    "common_phrases": [
                        "везти домой",
                        "везти багаж",
                        "везти детей"
                    ],
                    "target_lang_definitions": {
                        "en": "To move someone or something using a vehicle in a specific direction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "to transport passengers or goods"
                            },
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "to convey someone in a vehicle"
                            },
                            {
                                "target_lang_word": "transport",
                                "target_lang_sense_clarification": "to move goods or people from one place to another"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8964e43-bbb6-3d86-80da-728a7ec86b62",
                    "sense_definition": "Складываться удачно, благоприятно (об обстоятельствах); сопутствовать (об удаче) — обычно используется в безличной форме.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Паше и Саше сегодня совсем не <везёт>.",
                            "target_lang_translations": {
                                "en": "Pasha and Sasha are not <lucky> at all today."
                            }
                        },
                        {
                            "text": "Говорят, что новичкам всегда <везёт>.",
                            "target_lang_translations": {
                                "en": "They say that beginners are always <lucky>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "мне везёт",
                        "ему везёт",
                        "дуракам везёт"
                    ],
                    "target_lang_definitions": {
                        "en": "To turn out well or favorably (about circumstances); to have luck — usually used in the impersonal form."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be lucky",
                                "target_lang_sense_clarification": "used in impersonal constructions (e.g., 'I am lucky')"
                            }
                        ]
                    }
                }
            ]
        }
    ]

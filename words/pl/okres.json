            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa877622-d1ae-3029-ae43-e4734807bae2",
                    "sense_definition": "Pewien przedział czasu, w którym coś trwa, dzieje się lub rozwija.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę odpocząć przez krótki <w>okres</w>, żeby nabrać sił do dalszej pracy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно отдохнуть короткий <w>период</w> времени, чтобы набраться сил для дальнейшей работы."
                            }
                        },
                        {
                            "text": "Mój <w>okres</w> próbny w nowej firmie kończy się w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Мой испытательный <w>срок</w> в новой фирме заканчивается на следующей неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "czas",
                        "pora"
                    ],
                    "common_phrases": [
                        "okres próbny",
                        "okres czasu",
                        "okres ważności",
                        "w tym okresie",
                        "krótki okres"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный промежуток времени, в течение которого что-то длится, происходит или развивается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "период",
                                "target_lang_sense_clarification": "промежуток времени"
                            },
                            {
                                "target_lang_word": "срок",
                                "target_lang_sense_clarification": "установленное время, напр. испытательный срок"
                            },
                            {
                                "target_lang_word": "время",
                                "target_lang_sense_clarification": "длительность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a05f9ec2-51bd-3733-b25e-b9f983a9cb3a",
                    "sense_definition": "Fizjologiczne krwawienie u kobiet występujące cyklicznie; miesiączka.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ona nie poszła dzisiaj na basen, ponieważ ma <w>okres</w>.",
                            "target_lang_translations": {
                                "ru": "Она не пошла сегодня в бассейн, так как у нее <w>месячные</w>."
                            }
                        },
                        {
                            "text": "Jeśli spóźnia ci się <w>okres</w>, powinnaś zrobić test ciążowy.",
                            "target_lang_translations": {
                                "ru": "Если у тебя задержка <w>месячных</w>, тебе следует сделать тест на беременность."
                            }
                        }
                    ],
                    "synonyms": [
                        "miesiączka",
                        "menstruacja"
                    ],
                    "common_phrases": [
                        "mieć okres",
                        "bolesny okres",
                        "dostać okresu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физиологическое кровотечение у женщин, происходящее циклически; менструация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "месячные",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "менструация",
                                "target_lang_sense_clarification": "медицинское"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30f2cc58-e74e-39a1-ad01-26e31a675fae",
                    "sense_definition": "Ważny etap w historii, dziejach Ziemi lub życiu człowieka, wyróżniający się specyficznymi cechami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To był bardzo burzliwy <w>okres</w> w powojennej historii naszego kraju.",
                            "target_lang_translations": {
                                "ru": "Это был очень бурный <w>период</w> в послевоенной истории нашей страны."
                            }
                        },
                        {
                            "text": "Młodzież często przechodzi trudny <w>okres</w> dojrzewania.",
                            "target_lang_translations": {
                                "ru": "Молодежь часто переживает трудный <w>период</w> полового созревания."
                            }
                        }
                    ],
                    "synonyms": [
                        "epoka",
                        "czas"
                    ],
                    "common_phrases": [
                        "okres dojrzewania",
                        "okres międzywojenny",
                        "okres kredy",
                        "okres powojenny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Важный этап в истории, истории Земли или жизни человека, отличающийся специфическими чертами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "период",
                                "target_lang_sense_clarification": "этап развития"
                            },
                            {
                                "target_lang_word": "эпоха",
                                "target_lang_sense_clarification": "значительный исторический отрезок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71ba2a84-ad36-3041-a5ba-23a04308bd7d",
                    "sense_definition": "Czas potrzebny na wykonanie jednego pełnego cyklu powtarzającego się zjawiska fizycznego lub astronomicznego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "science_cycle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Okres</w> obrotu Ziemi wokół własnej osi wynosi jedną dobę.",
                            "target_lang_translations": {
                                "ru": "<w>Период</w> вращения Земли вокруг собственной оси составляет одни сутки."
                            }
                        },
                        {
                            "text": "Naukowcy badają <w>okres</w> połowicznego rozpadu tego pierwiastka promieniotwórczego.",
                            "target_lang_translations": {
                                "ru": "Ученые исследуют <w>период</w> полураспада этого радиоактивного элемента."
                            }
                        }
                    ],
                    "common_phrases": [

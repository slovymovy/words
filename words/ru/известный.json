{
    "word_family": [
        "известность",
        "неизвестный",
        "общеизвестный",
        "малоизвестный",
        "безызвестный",
        "известие",
        "известить",
        "весть"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d3668787-7021-3d0e-af9b-56e84a8fdc02",
                    "sense_definition": "Такой, о котором все знают, пользующийся широкой славой или популярностью.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "fame",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот <известный> певец скоро приедет в наш город с концертом.",
                            "target_lang_translations": {
                                "en": "This <famous> singer will soon come to our city for a concert."
                            }
                        },
                        {
                            "text": "Пушкин — самый <известный> русский поэт во всём мире.",
                            "target_lang_translations": {
                                "en": "Pushkin is the most <famous> Russian poet in the whole world."
                            }
                        }
                    ],
                    "synonyms": [
                        "знаменитый"
                    ],
                    "antonyms": [
                        "безвестный"
                    ],
                    "common_phrases": [
                        "известный писатель",
                        "известный актёр",
                        "широко известный"
                    ],
                    "target_lang_definitions": {
                        "en": "Widely recognized, famous, or popular."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "famous",
                                "target_lang_sense_clarification": "widely known and celebrated"
                            },
                            {
                                "target_lang_word": "well-known",
                                "target_lang_sense_clarification": "recognized by many people"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f89f233f-7031-3f04-b0da-450d6d985d84",
                    "sense_definition": "Такой, о котором есть информация или сведения; знакомый.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне давно <известен> этот интересный факт из истории.",
                            "target_lang_translations": {
                                "en": "This interesting fact from history has long been <known> to me."
                            }
                        },
                        {
                            "text": "Адрес нового магазина нам пока не <известен>.",
                            "target_lang_translations": {
                                "en": "The address of the new store is not yet <known> to us."
                            }
                        }
                    ],
                    "synonyms": [
                        "знакомый"
                    ],
                    "common_phrases": [
                        "всем известно",
                        "стало известно",
                        "хорошо известно"
                    ],
                    "target_lang_definitions": {
                        "en": "Known to exist or recognized; having information available about."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "known",
                                "target_lang_sense_clarification": "familiar or recognized; information exists about it"
                            },
                            {
                                "target_lang_word": "familiar",
                                "target_lang_sense_clarification": "well-known from long or close association"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e146217-0178-3dc4-bfb6-33a333a0c471",
                    "sense_definition": "Определённый, некоторый (часто не называемый прямо или подразумеваемый).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы не пошли на эту встречу по <известным> причинам.",
                            "target_lang_translations": {
                                "en": "We didn't go to this meeting for <certain> reasons."
                            }
                        },
                        {
                            "text": "У него есть <известный> опыт в этом сложном деле.",
                            "target_lang_translations": {
                                "en": "He has a <certain> experience in this complex matter."
                            }
                        }
                    ],
                    "synonyms": [
                        "определённый"
                    ],
                    "common_phrases": [
                        "в известном смысле",
                        "в известной степени",
                        "по известным причинам"
                    ],
                    "target_lang_definitions": {
                        "en": "Specific or particular, but not explicitly named or described."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "certain",
                                "target_lang_sense_clarification": "specific but unspecified"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66873eb3-9297-3d98-acb7-9b6544f2b54d",
                    "sense_definition": "Слывущий кем-либо, имеющий устоявшуюся репутацию (обычно с указанием качества).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "reputation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш сосед — <известный> спорщик, который никогда не уступает.",
                            "target_lang_translations": {
                                "en": "Our neighbor is a <notorious> arguer who never gives in."
                            }
                        },
                        {
                            "text": "Это <известный> мошенник, поэтому будьте с ним осторожны.",
                            "target_lang_translations": {
                                "en": "This is a <notorious> fraudster, so be careful with him."
                            }
                        }
                    ],
                    "common_phrases": [
                        "известный плут",
                        "известный лгун"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a specific reputation, often for a negative quality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "notorious",
                                "target_lang_sense_clarification": "famous or well known, typically for some bad quality or deed"
                            },
                            {
                                "target_lang_word": "well-known",
                                "target_lang_sense_clarification": "recognized as being a specific type of person"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
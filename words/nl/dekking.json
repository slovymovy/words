    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "43eccd3d-3038-3ba6-9fad-6083f41f955d",
                    "sense_definition": "Bescherming of beschutting tegen gevaar, zicht of weersinvloeden.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten zochten snel <w>dekking</w> achter de muur toen het schieten begon.",
                            "target_lang_translations": {
                                "ru": "Солдаты быстро нашли <w>укрытие</w> за стеной, когда началась стрельба."
                            }
                        },
                        {
                            "text": "Bij zwaar onweer kun je het beste <w>dekking</w> zoeken in een stevig gebouw.",
                            "target_lang_translations": {
                                "ru": "Во время сильной грозы лучше всего искать <w>укрытие</w> в прочном здании."
                            }
                        }
                    ],
                    "synonyms": [
                        "beschutting",
                        "bescherming"
                    ],
                    "common_phrases": [
                        "dekking zoeken",
                        "in dekking gaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защита или убежище от опасности, наблюдения или погодных условий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрытие",
                                "target_lang_sense_clarification": "убежище"
                            },
                            {
                                "target_lang_word": "прикрытие",
                                "target_lang_sense_clarification": "защита от огня или наблюдения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41618308-d93f-32ab-ba21-1f6cb9d12545",
                    "sense_definition": "De garantie dat kosten of schade worden vergoed door een verzekering of financiële reserve.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De diefstal van de fiets viel helaas niet onder de <w>dekking</w> van de polis.",
                            "target_lang_translations": {
                                "ru": "К сожалению, кража велосипеда не подпадала под страховое <w>покрытие</w> полиса."
                            }
                        },
                        {
                            "text": "Voor medische kosten in het buitenland heb je vaak een aanvullende <w>dekking</w> nodig.",
                            "target_lang_translations": {
                                "ru": "Для медицинских расходов за границей часто требуется дополнительное <w>покрытие</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "onder de dekking vallen",
                        "financiële dekking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гарантия того, что расходы или ущерб будут возмещены страховкой или финансовым резервом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрытие",
                                "target_lang_sense_clarification": "страховое"
                            },
                            {
                                "target_lang_word": "обеспечение",
                                "target_lang_sense_clarification": "финансовое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe6f9351-4630-3e61-afe3-370472ab2c50",
                    "sense_definition": "Het bewaken van een tegenstander in de sport of het beschermen van een stuk in een spel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdediger gaf zijn tegenstander geen moment rust door strakke <w>dekking</w> toe te passen.",
                            "target_lang_translations": {
                                "ru": "Защитник не давал противнику ни минуты покоя, применяя плотную <w>опеку</w>."
                            }
                        },
                        {
                            "text": "In het schaakspel is een goede <w>dekking</w> van de koning essentieel om niet te verliezen.",
                            "target_lang_translations": {
                                "ru": "В шахматах надежная <w>защита</w> короля необходима, чтобы не проиграть."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdediging",
                        "bewaking"
                    ],
                    "common_phrases": [
                        "rugdekking geven",
                        "mandekking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опека противника в спорте или защита фигуры в игре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опека",
                                "target_lang_sense_clarification": "в спорте"
                            },
                            {
                                "target_lang_word": "защита",
                                "target_lang_sense_clarification": "оборона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17a33192-5602-33e0-b3cf-3dd5386d112a",
                    "sense_definition": "De daad van het paren bij dieren, specifiek in de context van fokken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De fokker registreerde de datum van de <w>dekking</w> nauwkeurig in het logboek.",
                            "target_lang_translations": {
                                "ru": "Заводчик точно записал дату <w>случки</w> в журнал."
                            }
                        },
                        {
                            "text": "Na een succesvolle <w>dekking</w> werd de merrie drachtig.",
                            "target_lang_translations": {
                                "ru": "После успешной <w>случки</w> кобыла забеременела."
                            }
                        }
                    ],
                    "synonyms": [
                        "paring"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс спаривания животных, особенно в контексте разведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "случка",
                                "target_lang_sense_clarification": "животных"
                            },
                            {
                                "target_lang_word": "вязка",
                                "target_lang_sense_clarification": "собак, кошек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2628aef8-621b-3a33-a15c-130405c5915d",
                    "sense_definition": "Het precies op elkaar passen of samenvallen van twee zaken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De twee verschillende verklaringen zijn niet tot <w>dekking</w> te brengen.",
                            "target_lang_translations": {
                                "ru": "Эти два разных объяснения невозможно привести к <w>соответствию</w>."
                            }
                        },
                        {
                            "text": "Er is weinig <w>dekking</w> tussen de theoretische modellen en de praktijkresultaten.",
                            "target_lang_translations": {
                                "ru": "Между теоретическими моделями и практическими результатами мало <w>совпадений</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "overlapping"
                    ],
                    "common_phrases": [
                        "tot dekking brengen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точное совпадение или соответствие двух вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совпадение",
                                "target_lang_sense_clarification": "наложение"
                            },
                            {
                                "target_lang_word": "соответствие",
                                "target_lang_sense_clarification": "согласованность"
                            }
                        ]
                    }
                }
            ]
        }

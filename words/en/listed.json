{
    "word_family": [
        "list",
        "listing",
        "unlisted",
        "enlist"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7e8a5825-a5a2-39da-acda-d7dba198d0cd",
                    "sense_definition": "Simple past and past participle of <w>list</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>listed</w> all the ingredients we needed to buy for the cake.",
                            "target_lang_translations": {
                                "ru": "Она <w>перечислила</w> все ингредиенты, которые нам нужно было купить для торта."
                            }
                        },
                        {
                            "text": "The rules are <w>listed</w> clearly on the noticeboard for everyone to see.",
                            "target_lang_translations": {
                                "ru": "Правила четко <w>перечислены</w> на доске объявлений для всеобщего обозрения."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола list (составлять список, перечислять)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перечисленный",
                                "target_lang_sense_clarification": "названный по пунктам"
                            },
                            {
                                "target_lang_word": "внесенный в список",
                                "target_lang_sense_clarification": "добавленный в перечень"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b356b06c-c1ab-3bc3-9186-25d171933493",
                    "sense_definition": "Entered on a list, especially an official one.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please check if your name is <w>listed</w> in the directory.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверьте, <w>указано</w> ли ваше имя в справочнике."
                            }
                        },
                        {
                            "text": "The <w>listed</w> price includes all taxes and service charges.",
                            "target_lang_translations": {
                                "ru": "<w>Указанная</w> цена включает все налоги и сервисные сборы."
                            }
                        }
                    ],
                    "synonyms": [
                        "recorded",
                        "registered",
                        "catalogued"
                    ],
                    "antonyms": [
                        "unlisted"
                    ],
                    "common_phrases": [
                        "listed below",
                        "listed above",
                        "listed number"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внесенный в список, особенно в официальный документ или каталог."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "указанный",
                                "target_lang_sense_clarification": "приведенный в списке или документе"
                            },
                            {
                                "target_lang_word": "внесенный в список",
                                "target_lang_sense_clarification": "зарегистрированный в перечне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a27782d2-42d4-3589-b0c2-a21a5ca13241",
                    "sense_definition": "Having its shares traded on a stock exchange.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Investing in a <w>listed</w> company offers more transparency than private equity.",
                            "target_lang_translations": {
                                "ru": "Инвестирование в <w>котируемую на бирже</w> компанию обеспечивает большую прозрачность, чем частный капитал."
                            }
                        },
                        {
                            "text": "The corporation became a publicly <w>listed</w> entity last year.",
                            "target_lang_translations": {
                                "ru": "В прошлом году корпорация стала публично <w>котируемой</w> организацией."
                            }
                        }
                    ],
                    "synonyms": [
                        "public",
                        "quoted"
                    ],
                    "antonyms": [
                        "private",
                        "unlisted"
                    ],
                    "common_phrases": [
                        "listed company",
                        "publicly listed",
                        "listed shares"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий акции, которые торгуются на фондовой бирже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "котируемый на бирже",
                                "target_lang_sense_clarification": "о компании или ценных бумагах"
                            },
                            {
                                "target_lang_word": "публичный",
                                "target_lang_sense_clarification": "о компании, акции которой доступны для покупки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a98c49e-3b4f-378a-a98a-bd4109214761",
                    "sense_definition": "Designated as of special architectural or historical interest and protected from demolition or alteration.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They live in a Grade II <w>listed</w> building that requires special permission for repairs.",
                            "target_lang_translations": {
                                "ru": "Они живут в здании, <w>внесенном в список памятников архитектуры</w> второй категории, для ремонта которого требуется специальное разрешение."
                            }
                        },
                        {
                            "text": "The council protects <w>listed</w> properties to preserve the town's heritage.",
                            "target_lang_translations": {
                                "ru": "Совет защищает <w>охраняемые</w> объекты недвижимости, чтобы сохранить наследие города."
                            }
                        }
                    ],
                    "synonyms": [
                        "protected",
                        "preserved"
                    ],
                    "common_phrases": [
                        "listed building",
                        "listed property"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Признанный объектом особого архитектурного или исторического интереса и охраняемый государством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охраняемый государством",
                                "target_lang_sense_clarification": "о памятниках архитектуры"
                            },
                            {
                                "target_lang_word": "внесенный в реестр памятников",
                                "target_lang_sense_clarification": "официальный статус исторического здания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
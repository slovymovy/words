{
    "word_family": [
        "odbywać",
        "odbycie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e8135584-007b-3eab-b226-09bb03a5a2a7",
                    "sense_definition": "Zostać przeprowadzonym, mieć miejsce w określonym czasie i przestrzeni (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "event_occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczorajsze spotkanie <odbyło się> w bardzo miłej atmosferze.",
                            "target_lang_translations": {
                                "ru": "Вчерашняя встреча <прошла> в очень приятной атмосфере."
                            }
                        },
                        {
                            "text": "Koncert <odbędzie się> zgodnie z planem, mimo padającego deszczu.",
                            "target_lang_translations": {
                                "ru": "Концерт <состоится> по плану, несмотря на дождь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odbyć się bez przeszkód",
                        "odbyć się zgodnie z planem",
                        "odbyć się w terminie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Произойти, иметь место в определенное время и месте (используется с возвратной частицей)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состояться",
                                "target_lang_sense_clarification": "о событии, встрече"
                            },
                            {
                                "target_lang_word": "пройти",
                                "target_lang_sense_clarification": "о мероприятии, процессе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cb30e47-c8fe-3dca-abd8-e0f87b1bc508",
                    "sense_definition": "Zrealizować zaplanowaną czynność, przejść przez wymagany okres lub proces (np. szkolenie, karę, podróż).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "activity_completion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy nowy pracownik musi <odbyć> obowiązkowe szkolenie z bezpieczeństwa.",
                            "target_lang_translations": {
                                "ru": "Каждый новый сотрудник должен <пройти> обязательный инструктаж по технике безопасности."
                            }
                        },
                        {
                            "text": "Mój dziadek <odbył> długą podróż, aby wrócić do ojczyzny.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка <совершил> долгое путешествие, чтобы вернуться на родину."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrobić",
                        "wykonać",
                        "wypełnić"
                    ],
                    "common_phrases": [
                        "odbyć służbę wojskową",
                        "odbyć karę",
                        "odbyć staż",
                        "odbyć rozmowę",
                        "odbyć podróż"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнить запланированное действие, пройти через определенный процесс или период (обучение, наказание, путешествие)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пройти",
                                "target_lang_sense_clarification": "обучение, стажировку"
                            },
                            {
                                "target_lang_word": "отбыть",
                                "target_lang_sense_clarification": "наказание, службу"
                            },
                            {
                                "target_lang_word": "совершить",
                                "target_lang_sense_clarification": "путешествие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67a8dbcb-ea5b-39ef-bfce-b9ab2c510008",
                    "sense_definition": "Zaspokoić potrzebę fizjologiczną lub seksualną (używane głównie w kontekstach medycznych i prawnych).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physiological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biegły sądowy ustalił, że oskarżony <odbył> stosunek z poszkodowaną.",
                            "target_lang_translations": {
                                "ru": "Судебный эксперт установил, что обвиняемый <совершил> половой акт с потерпевшей."
                            }
                        },
                        {
                            "text": "Pacjent miał problemy, aby samodzielnie <odbyć> potrzebę fizjologiczną.",
                            "target_lang_translations": {
                                "ru": "У пациента были проблемы с тем, чтобы самостоятельно <справить> физиологическую нужду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odbyć stosunek",
                        "odbyć potrzebę"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formalne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Удовлетворить физиологическую или сексуальную потребность (формальный, медицинский или юридический контекст)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совершить",
                                "target_lang_sense_clarification": "половой акт"
                            },
                            {
                                "target_lang_word": "справить",
                                "target_lang_sense_clarification": "естественную нужду"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
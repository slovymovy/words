    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7accb3bd-8a05-3fca-833c-c12184f3aa16",
                    "sense_definition": "A person who waits in a line for something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_waiting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The enthusiastic <w>queuer</w> arrived hours before the store opened to get the best deal.",
                            "target_lang_translations": {
                                "ru": "Полный энтузиазма <w>стоящий в очереди</w> прибыл за несколько часов до открытия магазина, чтобы совершить самую выгодную покупку."
                            }
                        },
                        {
                            "text": "Each <w>queuer</w> was given a ticket to ensure they kept their place in line.",
                            "target_lang_translations": {
                                "ru": "Каждому <w>стоящему в очереди</w> выдали талон, чтобы гарантировать сохранение места."
                            }
                        }
                    ],
                    "common_phrases": [
                        "patient queuer",
                        "early queuer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который ждет в очереди за чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоящий в очереди",
                                "target_lang_sense_clarification": "человек, ожидающий в живой очереди"
                            },
                            {
                                "target_lang_word": "очередник",
                                "target_lang_sense_clarification": "человек, числящийся в списке очередности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "238031e9-ade3-38a7-a4a5-5ee233477fda",
                    "sense_definition": "A technical process or component that organizes data items into a sequence.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "computing_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The background <w>queuer</w> manages the flow of tasks sent to the printer.",
                            "target_lang_translations": {
                                "ru": "Фоновый <w>постановщик в очередь</w> управляет потоком задач, отправляемых на принтер."
                            }
                        },
                        {
                            "text": "Developers fixed a bug in the <w>queuer</w> that caused messages to be lost.",
                            "target_lang_translations": {
                                "ru": "Разработчики исправили ошибку в <w>постановщике в очередь</w>, из-за которой терялись сообщения."
                            }
                        }
                    ],
                    "antonyms": [
                        "dequeuer"
                    ],
                    "common_phrases": [
                        "job queuer",
                        "message queuer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Технический процесс или компонент, организующий элементы данных в последовательность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постановщик в очередь",
                                "target_lang_sense_clarification": "компонент, добавляющий задачи в очередь"
                            },
                            {
                                "target_lang_word": "диспетчер очереди",
                                "target_lang_sense_clarification": "программа управления очередностью"
                            }
                        ]
                    }
                }
            ]
        }
    ]

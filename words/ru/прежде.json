    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9828f9a6-b713-3a07-9521-10a512a6923a",
                    "sense_definition": "Некоторое время назад, в прошлом.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_past",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Здесь <w>прежде</w> был густой и тёмный лес.",
                            "target_lang_translations": {
                                "en": "There was a thick and dark forest here <w>before</w>."
                            }
                        },
                        {
                            "text": "Всё в доме осталось, как <w>прежде</w>.",
                            "target_lang_translations": {
                                "en": "Everything in the house remained as <w>before</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "раньше",
                        "в прошлом",
                        "некогда"
                    ],
                    "antonyms": [
                        "потом",
                        "после"
                    ],
                    "common_phrases": [
                        "как прежде"
                    ],
                    "target_lang_definitions": {
                        "en": "Some time ago, in the past."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "before",
                                "target_lang_sense_clarification": "at an earlier time"
                            },
                            {
                                "target_lang_word": "previously",
                                "target_lang_sense_clarification": "formal; in the past"
                            },
                            {
                                "target_lang_word": "formerly",
                                "target_lang_sense_clarification": "referring to a past state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c3d22f5-7feb-3e44-a17c-69d2fcff4b2d",
                    "sense_definition": "Сперва, сначала (перед тем как сделать что-либо другое).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Прежде</w> подумай, а потом уже отвечай.",
                            "target_lang_translations": {
                                "en": "Think <w>first</w>, and only then answer."
                            }
                        },
                        {
                            "text": "Нужно <w>прежде</w> закончить работу, чтобы отдыхать.",
                            "target_lang_translations": {
                                "en": "You need to finish work <w>first</w> to rest."
                            }
                        }
                    ],
                    "synonyms": [
                        "сначала",
                        "сперва"
                    ],
                    "antonyms": [
                        "потом",
                        "затем"
                    ],
                    "target_lang_definitions": {
                        "en": "First, at first (before doing something else)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "first",
                                "target_lang_sense_clarification": "before doing anything else"
                            },
                            {
                                "target_lang_word": "beforehand",
                                "target_lang_sense_clarification": "in advance"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "435614c5-7557-3b2a-b364-940a4908117c",
                    "sense_definition": "Указывает на предшествование чему-либо во времени; перед, до.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "preposition_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гости собрались <w>прежде</w> назначенного времени.",
                            "target_lang_translations": {
                                "en": "The guests gathered <w>before</w> the appointed time."
                            }
                        },
                        {
                            "text": "Не стоит радоваться успеху <w>прежде</w> времени.",
                            "target_lang_translations": {
                                "en": "One should not rejoice at success <w>before</w> the time."
                            }
                        }
                    ],
                    "synonyms": [
                        "перед",
                        "до"
                    ],
                    "antonyms": [
                        "после"
                    ],
                    "common_phrases": [
                        "прежде всего",
                        "прежде времени"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates precedence in time; before, prior to."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "before",
                                "target_lang_sense_clarification": "earlier than"
                            },
                            {
                                "target_lang_word": "prior to",
                                "target_lang_sense_clarification": "formal; earlier than"
                            }
                        ]
                    }
                }
            ]
        }
    ]

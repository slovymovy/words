                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выжидал",
                                "target_lang_sense_clarification": "прошедшее время от 'выжидать'"
                            },
                            {
                                "target_lang_word": "ждал",
                                "target_lang_sense_clarification": "прошедшее время от 'ждать'"
                            }

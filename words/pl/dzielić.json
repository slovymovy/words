    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ea10396f-7154-3886-ad68-fd91a80a88b8",
                    "sense_definition": "Rozdzielać jedną całość na mniejsze części lub fragmenty.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "division_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Matka sprawiedliwie <dzieliła> chleb na kawałki dla wszystkich dzieci.",
                            "target_lang_translations": {
                                "ru": "Мать справедливо <делила> хлеб на куски для всех детей."
                            }
                        },
                        {
                            "text": "Musimy <podzielić> ten tort urodzinowy na dwanaście równych porcji.",
                            "target_lang_translations": {
                                "ru": "Мы должны <разделить> этот праздничный торт на двенадцать равных порций."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozdzielać",
                        "kroić"
                    ],
                    "antonyms": [
                        "łączyć",
                        "scalać"
                    ],
                    "common_phrases": [
                        "dzielić na pół",
                        "dzielić na części",
                        "dzielić na grupy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разделять одно целое на более мелкие части или фрагменты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делить",
                                "target_lang_sense_clarification": "на части"
                            },
                            {
                                "target_lang_word": "разделять",
                                "target_lang_sense_clarification": "распределять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8d4deb9-95f6-3eea-8282-0d5e369b5e14",
                    "sense_definition": "Dawać innym część tego, co się posiada (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sharing_giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Warto <dzielić> się zabawkami z młodszym rodzeństwem podczas zabawy.",
                            "target_lang_translations": {
                                "ru": "Стоит <делиться> игрушками с младшими братьями и сестрами во время игры."
                            }
                        },
                        {
                            "text": "Zawsze chętnie <dzielę> się moimi notatkami ze studiów z innymi.",
                            "target_lang_translations": {
                                "ru": "Я всегда охотно <делюсь> своими конспектами лекций с другими."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dzielić się opłatkiem",
                        "dzielić się jedzeniem",
                        "dzielić się wrażeniami"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать другим часть того, что имеешь (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делиться",
                                "target_lang_sense_clarification": "имуществом, едой, новостями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b7ad332-52fa-328a-af3f-f92818208402",
                    "sense_definition": "Wykonywać działanie matematyczne polegające na sprawdzeniu, ile razy jedna liczba mieści się w drugiej.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "division_math",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczycielka matematyki uczyła nas, jak <dzielić> duże liczby w pamięci.",
                            "target_lang_translations": {
                                "ru": "Учительница математики учила нас, как <делить> большие числа в уме."
                            }
                        },
                        {
                            "text": "Dzieci w szkole podstawowej uczą się dodawać, odejmować, mnożyć i <dzielić>.",
                            "target_lang_translations": {
                                "ru": "Дети в начальной школе учатся складывать, вычитать, умножать и <делить>."
                            }
                        }
                    ],
                    "antonyms": [
                        "mnożyć"
                    ],
                    "common_phrases": [
                        "dzielić przez dwa",
                        "dzielić liczby",
                        "dzielić bez reszty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять математическое действие деления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делить",
                                "target_lang_sense_clarification": "числа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc4db784-0638-3977-85f9-0094c757e4d1",
                    "sense_definition": "Przeżywać coś wspólnie z kimś lub korzystać z czegoś razem z inną osobą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sharing_experience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez cały rok studiów <dzieliłem> mały pokój w akademiku z kolegą.",
                            "target_lang_translations": {
                                "ru": "Весь учебный год я <делил> маленькую комнату в общежитии с другом."
                            }
                        },
                        {
                            "text": "Moi rodzice <dzielą> wspólną pasję do dalekich podróży i górskich wycieczek.",
                            "target_lang_translations": {
                                "ru": "Мои родители <разделяют> общую страсть к дальним путешествиям и походам в горы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dzielić pokój",
                        "dzielić pasję",
                        "dzielić życie",
                        "dzielić los"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать что-то совместно или переживать что-то вместе с кем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делить",
                                "target_lang_sense_clarification": "комнату, жизнь"
                            },
                            {
                                "target_lang_word": "разделять",
                                "target_lang_sense_clarification": "мнение, увлечение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a89df6a5-8329-3f04-a7f8-063e9991f010",
                    "sense_definition": "Stanowić barierę lub przyczynę braku zgody między ludźmi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "separation_conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te dwie siostry <dzieli> ogromna różnica wieku oraz zupełnie inny charakter.",
                            "target_lang_translations": {
                                "ru": "Этих двух сестер <разделяет> огромная разница в возрасте и совершенно разный характер."
                            }
                        },
                        {
                            "text": "Polityka niestety często <dzieli> nawet najlepszych przyjaciół przy wspólnym stole.",
                            "target_lang_translations": {
                                "ru": "Политика, к сожалению, часто <разделяет> даже лучших друзей за общим столом."
                            }
                        }
                    ],
                    "synonyms": [
                        "różnić",
                        "separować"
                    ],
                    "antonyms": [
                        "łączyć"
                    ],
                    "common_phrases": [
                        "dzielić ludzi",
                        "dzieli ich przepaść",
                        "dzieli ich wszystko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служить барьером или причиной разногласий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разделять",
                                "target_lang_sense_clarification": "создавать дистанцию"
                            },
                            {
                                "target_lang_word": "разъединять",
                                "target_lang_sense_clarification": "разлучать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
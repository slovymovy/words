    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7cc636d4-f5f8-3b26-9fd4-6ed94724a45f",
                    "sense_definition": "A public declaration of principles, policies, or intentions, especially that of a political party.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The political party released its election <w>manifesto</w> promising significant tax reforms.",
                            "target_lang_translations": {
                                "ru": "Политическая партия выпустила свой предвыборный <w>манифест</w>, обещая значительные налоговые реформы."
                            }
                        },
                        {
                            "text": "The artist wrote a <w>manifesto</w> to explain the philosophy behind the new movement.",
                            "target_lang_translations": {
                                "ru": "Художник написал <w>манифест</w>, чтобы объяснить философию нового движения."
                            }
                        }
                    ],
                    "synonyms": [
                        "declaration",
                        "proclamation",
                        "platform",
                        "statement"
                    ],
                    "common_phrases": [
                        "political manifesto",
                        "election manifesto",
                        "party manifesto",
                        "publish a manifesto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публичное объявление принципов, политики или намерений, особенно со стороны политической партии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "манифест",
                                "target_lang_sense_clarification": "публичное письменное объявление принципов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6a1c719f-79d3-3ba4-b1e4-2f0ce971bf9a",
                    "sense_definition": "To issue a manifesto.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The radical group <w>manifestoed</w> their demands to the public before taking action.",
                            "target_lang_translations": {
                                "ru": "Радикальная группа <w>обнародовала</w> свои требования общественности перед тем, как начать действовать."
                            }
                        },
                        {
                            "text": "He <w>manifestoed</w> his opposition to the new laws in a lengthy document.",
                            "target_lang_translations": {
                                "ru": "Он <w>провозгласил</w> свою оппозицию новым законам в длинном документе."
                            }
                        }
                    ],
                    "synonyms": [
                        "declare",
                        "proclaim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать или публиковать манифест; провозглашать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "издавать манифест",
                                "target_lang_sense_clarification": "публиковать программный документ"
                            },
                            {
                                "target_lang_word": "провозглашать",
                                "target_lang_sense_clarification": "торжественно объявлять"
                            }
                        ]
                    }
                }
            ]
        }

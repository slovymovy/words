    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "52a0bd68-9737-3c6e-abb2-503aedf2e23f",
                    "sense_definition": "A group of words that contains a subject and a verb, which may form a complete sentence or be a part of one.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the sentence \"I went home because I was tired,\" the second part is a dependent <w>clause</w>.",
                            "target_lang_translations": {
                                "ru": "В предложении «Я пошел домой, потому что устал» вторая часть — это зависимое <w>предложение</w>."
                            }
                        },
                        {
                            "text": "A relative <w>clause</w> usually gives more information about a noun in the sentence.",
                            "target_lang_translations": {
                                "ru": "Относительное придаточное <w>предложение</w> обычно дает дополнительную информацию о существительном в предложении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "main clause",
                        "dependent clause",
                        "relative clause",
                        "subordinate clause"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа слов, содержащая подлежащее и сказуемое, которая может составлять полное предложение или быть его частью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предложение",
                                "target_lang_sense_clarification": "грамматическая единица"
                            },
                            {
                                "target_lang_word": "придаточное предложение",
                                "target_lang_sense_clarification": "зависимая часть сложного предложения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a3a669f-6c67-3d53-be77-994b0a214c90",
                    "sense_definition": "A distinct section, provision, or stipulation in a legal document, contract, or will.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should read every <w>clause</w> in the contract before you sign it.",
                            "target_lang_translations": {
                                "ru": "Вам следует прочитать каждый <w>пункт</w> договора, прежде чем подписывать его."
                            }
                        },
                        {
                            "text": "The confidentiality <w>clause</w> prevents employees from sharing company secrets with competitors.",
                            "target_lang_translations": {
                                "ru": "<w>Пункт</w> о конфиденциальности запрещает сотрудникам делиться секретами компании с конкурентами."
                            }
                        }
                    ],
                    "synonyms": [
                        "provision",
                        "stipulation",
                        "article",
                        "section",
                        "condition"
                    ],
                    "common_phrases": [
                        "penalty clause",
                        "escape clause",
                        "confidentiality clause",
                        "contract clause"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный раздел, положение или условие в юридическом документе, договоре или завещании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пункт",
                                "target_lang_sense_clarification": "договора или документа"
                            },
                            {
                                "target_lang_word": "оговорка",
                                "target_lang_sense_clarification": "особое условие"
                            },
                            {
                                "target_lang_word": "статья",
                                "target_lang_sense_clarification": "закона или соглашения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2eacbea6-06dc-3bf0-898c-1986eb7113c4",
                    "sense_definition": "A specific component or condition within a logical statement or computer query.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "logic_computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The query failed because there was a syntax error in the WHERE <w>clause</w>.",
                            "target_lang_translations": {
                                "ru": "Запрос не удался из-за синтаксической ошибки в <w>предложении</w> WHERE."
                            }
                        },
                        {
                            "text": "Each <w>clause</w> in the logical argument must be evaluated to determine the final truth value.",
                            "target_lang_translations": {
                                "ru": "Каждое <w>выражение</w> в логическом аргументе должно быть оценено для определения итогового значения истинности."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"

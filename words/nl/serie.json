    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4f6fdad8-c25a-3170-9e23-b8ef26d5f48d",
                    "sense_definition": "Een aantal delen die samen een geheel vormen, vaak in een vaste volgorde, zoals afleveringen van een televisieprogramma.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kijk elke avond naar een spannende <w>serie</w> op televisie.",
                            "target_lang_translations": {
                                "en": "I watch an exciting <w>series</w> on television every evening.",
                                "ru": "Я каждый вечер смотрю захватывающий <w>сериал</w> по телевизору."
                            }
                        },
                        {
                            "text": "Er wordt weer een nieuwe <w>serie</w> kinderpostzegels uitgegeven.",
                            "target_lang_translations": {
                                "en": "A new <w>series</w> of children's stamps is being issued again.",
                                "ru": "Снова выпускается новая <w>серия</w> детских почтовых марок."
                            }
                        }
                    ],
                    "synonyms": [
                        "reeks"
                    ],
                    "common_phrases": [
                        "een nieuwe serie",
                        "een spannende serie",
                        "de hele serie"
                    ],
                    "target_lang_definitions": {
                        "en": "A number of parts forming a whole, often in a fixed order, such as episodes of a television program.",
                        "ru": "Ряд частей, составляющих единое целое, часто в определенном порядке, например, эпизоды телепрограммы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "series",
                                "target_lang_sense_clarification": "television program"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сериал",
                                "target_lang_sense_clarification": "телевизионный"
                            },
                            {
                                "target_lang_word": "серия",
                                "target_lang_sense_clarification": "марок, книг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c63158b1-18ed-390c-bb21-8f1f59f10077",
                    "sense_definition": "Een rij van dingen of gebeurtenissen die direct na elkaar komen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door een <w>serie</w> fouten liep het project grote vertraging op.",
                            "target_lang_translations": {
                                "en": "Due to a <w>series</w> of errors, the project suffered major delays.",
                                "ru": "Из-за <w>череды</w> ошибок проект сильно задержался."
                            }
                        },
                        {
                            "text": "De keeper stopte een hele <w>serie</w> schoten tijdens de wedstrijd.",
                            "target_lang_translations": {
                                "en": "The goalkeeper saved a whole <w>series</w> of shots during the match.",
                                "ru": "Вратарь отразил целую <w>серию</w> ударов во время матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "reeks",
                        "rij"
                    ],
                    "common_phrases": [
                        "een serie gebeurtenissen",
                        "een hele serie"
                    ],
                    "target_lang_definitions": {
                        "en": "A row of things or events that come directly after one another.",
                        "ru": "Ряд вещей или событий, происходящих непосредственно друг за другом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "series",
                                "target_lang_sense_clarification": "sequence of events"
                            }
                        ],
                        "ru": [

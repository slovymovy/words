{
    "word_family": [
        "patterned",
        "patterning",
        "patternless"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "81e22065-1c81-3e88-818f-bf8a3a7a6a56",
                    "sense_definition": "A repeated decorative design on a surface, such as on fabric, paper, or pottery.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "decorative_design",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She chose a dress with a beautiful floral <w>pattern</w>.",
                            "target_lang_translations": {
                                "ru": "Она выбрала платье с красивым цветочным <w>узором</w>."
                            }
                        },
                        {
                            "text": "The wallpaper has a complex geometric <w>pattern</w> in blue and gold.",
                            "target_lang_translations": {
                                "ru": "На обоях сложный геометрический <w>узор</w> в синих и золотых тонах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "floral pattern",
                        "geometric pattern",
                        "check pattern",
                        "colorful pattern"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повторяющийся декоративный рисунок на поверхности, например, на ткани, бумаге или керамике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "узор",
                                "target_lang_sense_clarification": "рисунок из линий и красок"
                            },
                            {
                                "target_lang_word": "рисунок",
                                "target_lang_sense_clarification": "изображение или узор"
                            },
                            {
                                "target_lang_word": "орнамент",
                                "target_lang_sense_clarification": "художественное украшение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de0ca977-f84c-3f8d-9f7b-2385a6d30a23",
                    "sense_definition": "A regular and intelligible sequence of events, actions, or behaviors that can be observed.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sequence_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor asked me questions about my sleep <w>pattern</w>.",
                            "target_lang_translations": {
                                "ru": "Врач задал мне вопросы о моем <w>режиме</w> сна."
                            }
                        },
                        {
                            "text": "Police noticed a disturbing <w>pattern</w> of behavior in the suspect's history.",
                            "target_lang_translations": {
                                "ru": "Полиция заметила тревожную <w>модель</w> поведения в прошлом подозреваемого."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pattern of behavior",
                        "sleep pattern",
                        "weather pattern",
                        "follow a pattern"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулярная и понятная последовательность событий, действий или поведения, которую можно наблюдать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закономерность",
                                "target_lang_sense_clarification": "регулярность, повторяемость"
                            },
                            {
                                "target_lang_word": "модель",
                                "target_lang_sense_clarification": "схема поведения или событий"
                            },
                            {
                                "target_lang_word": "паттерн",
                                "target_lang_sense_clarification": "стереотипная реакция или структура"
                            },
                            {
                                "target_lang_word": "схема",
                                "target_lang_sense_clarification": "порядок протекания процесса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c807505f-3c94-3ad8-958a-16bb9ba84047",
                    "sense_definition": "A paper or cardboard template used as a guide to cut fabric for making clothes.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "template_model",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother bought a new <w>pattern</w> to knit a sweater.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка купила новую <w>схему</w> для вязания свитера."
                            }
                        },
                        {
                            "text": "You must pin the <w>pattern</w> to the cloth before you start cutting.",
                            "target_lang_translations": {
                                "ru": "Нужно приколоть <w>выкройку</w> к ткани, прежде чем начинать резать."
                            }
                        }
                    ],
                    "synonyms": [
                        "template",
                        "stencil",
                        "model"
                    ],
                    "common_phrases": [
                        "knitting pattern",
                        "sewing pattern",
                        "cut a pattern"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бумажный или картонный шаблон, используемый как руководство для раскроя ткани при изготовлении одежды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выкройка",
                                "target_lang_sense_clarification": "шаблон для раскроя ткани"
                            },
                            {
                                "target_lang_word": "лекало",
                                "target_lang_sense_clarification": "инструмент для разметки кривых линий"
                            },
                            {
                                "target_lang_word": "шаблон",
                                "target_lang_sense_clarification": "образец для изготовления изделий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd09f91b-3f45-3966-bcd1-97bfbebce960",
                    "sense_definition": "A standard solution to a common problem in software design or architecture.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_design",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Engineers should study this design <w>pattern</w> to improve the system's efficiency.",
                            "target_lang_translations": {
                                "ru": "Инженерам следует изучить этот <w>шаблон</w> проектирования, чтобы повысить эффективность системы."
                            }
                        },
                        {
                            "text": "Using a recognized <w>pattern</w> makes the code easier to maintain.",
                            "target_lang_translations": {
                                "ru": "Использование общепризнанного <w>паттерна</w> облегчает поддержку кода."
                            }
                        }
                    ],
                    "antonyms": [
                        "antipattern"
                    ],
                    "common_phrases": [
                        "design pattern"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стандартное решение распространенной проблемы в проектировании программного обеспечения или архитектуре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шаблон",
                                "target_lang_sense_clarification": "шаблон проектирования"
                            },
                            {
                                "target_lang_word": "паттерн",
                                "target_lang_sense_clarification": "профессиональный термин в IT"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c093574e-4f8b-32e0-8830-5df259b50a3a",
                    "sense_definition": "A person or thing that is a perfect example to be imitated.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "exemplar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was considered a <w>pattern</w> of virtue by all who knew him.",
                            "target_lang_translations": {
                                "ru": "Все, кто его знал, считали его <w>образцом</w> добродетели."
                            }
                        },
                        {
                            "text": "The ancient temple served as a <w>pattern</w> for many later buildings.",
                            "target_lang_translations": {
                                "ru": "Древний храм послужил <w>образцом</w> для многих поздних зданий."
                            }
                        }
                    ],
                    "synonyms": [
                        "model",
                        "original"
                    ],
                    "common_phrases": [
                        "pattern of virtue"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или вещь, являющиеся идеальным примером для подражания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "образец",
                                "target_lang_sense_clarification": "то, чему следует подражать"
                            },
                            {
                                "target_lang_word": "пример",
                                "target_lang_sense_clarification": "наглядный случай или образец"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4d79e36e-b2e3-3616-bf55-4ba0c49a4c6b",
                    "sense_definition": "To design or create something based on a specific model or example.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "template_model",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new government building was <w>patterned</w> after a Greek temple.",
                            "target_lang_translations": {
                                "ru": "Новое правительственное здание было <w>построено по образцу</w> греческого храма."
                            }
                        },
                        {
                            "text": "They <w>patterned</w> their educational system on the successful methods used in Finland.",
                            "target_lang_translations": {
                                "ru": "Они <w>создали</w> свою систему образования <w>по образцу</w> успешных методов, используемых в Финляндии."
                            }
                        }
                    ],
                    "synonyms": [
                        "model"
                    ],
                    "common_phrases": [
                        "patterned after",
                        "patterned on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проектировать или создавать что-либо, основываясь на конкретной модели или примере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делать по образцу",
                                "target_lang_sense_clarification": "создавать на основе модели"
                            },
                            {
                                "target_lang_word": "копировать",
                                "target_lang_sense_clarification": "воспроизводить что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b118c0f3-8280-3798-b8d6-3a760b64c63e",
                    "sense_definition": "To decorate something with a repeated design.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "decorative_design",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Frost had <w>patterned</w> the windowpane with delicate white shapes.",
                            "target_lang_translations": {
                                "ru": "Мороз <w>украсил узорами</w> оконное стекло, нанеся тонкие белые фигуры."
                            }
                        },
                        {
                            "text": "The artist <w>patterned</w> the ceramic bowl with blue and green lines.",
                            "target_lang_translations": {
                                "ru": "Художник <w>расписал узорами</w> керамическую чашу синими и зелеными линиями."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшать что-либо повторяющимся рисунком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "украшать узором",
                                "target_lang_sense_clarification": "наносить декоративный рисунок"
                            },
                            {
                                "target_lang_word": "расписывать",
                                "target_lang_sense_clarification": "покрывать росписью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "900a06b0-22dd-3938-87b1-54c58622e313",
                    "sense_definition": "To arrange, organize, or fix a situation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_fix",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't worry about the tickets, I will <w>pattern</w> it for us.",
                            "target_lang_translations": {
                                "ru": "Не волнуйся о билетах, я всё <w>устрою</w> для нас."
                            }
                        },
                        {
                            "text": "He needs to <w>pattern</w> up his life before he gets into real trouble.",
                            "target_lang_translations": {
                                "ru": "Ему нужно <w>наладить</w> свою жизнь, пока он не попал в настоящую беду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pattern up"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Multicultural London English"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "UK"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устраивать, организовывать или исправлять ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устраивать",
                                "target_lang_sense_clarification": "организовывать что-либо"
                            },
                            {
                                "target_lang_word": "разруливать",
                                "target_lang_sense_clarification": "сленг: решать проблему"
                            },
                            {
                                "target_lang_word": "налаживать",
                                "target_lang_sense_clarification": "приводить в порядок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5cc763a9-6520-3435-a44e-67d7cb90272a",
                    "sense_definition": "Acting as a perfect example or ideal type of something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "exemplar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the novel, she was described as a <w>pattern</w> wife who never complained.",
                            "target_lang_translations": {
                                "ru": "В романе она была описана как <w>примерная</w> жена, которая никогда не жаловалась."
                            }
                        },
                        {
                            "text": "He tried to be a <w>pattern</w> student, but he often forgot his homework.",
                            "target_lang_translations": {
                                "ru": "Он пытался быть <w>образцовым</w> учеником, но часто забывал домашнюю работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "model"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступающий в качестве идеального примера или типа чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "примерный",
                                "target_lang_sense_clarification": "служащий примером"
                            },
                            {
                                "target_lang_word": "образцовый",
                                "target_lang_sense_clarification": "идеальный, достойный подражания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
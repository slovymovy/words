                        "en": [
                            {
                                "target_lang_word": "stand",
                                "target_lang_sense_clarification": "be located (of objects)"
                            },
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "location"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36b3b9f4-60a8-38ab-8243-d34301ead667",
                    "sense_definition": "Geschreven zijn; vermeld worden in een tekst.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de krant <staat> een interessant artikel over het weer.",
                            "target_lang_translations": {
                                "ru": "В газете <написана> интересная статья о погоде.",
                                "en": "There <is> an interesting article about the weather in the newspaper."
                            }
                        },
                        {
                            "text": "Wat <staat> er eigenlijk op dat bord geschreven?",
                            "target_lang_translations": {
                                "ru": "Что, собственно, <написано> на этой доске?",
                                "en": "What is actually <written> on that sign?"
                            }

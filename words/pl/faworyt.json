            "senses": [
                {
                    "sense_id": "3e89be85-49ae-356d-b11b-02c2e73bdc3d",
                    "sense_definition": "Osoba, drużyna lub zwierzę, które ma największe szanse na zwycięstwo w zawodach lub konkursie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_winner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jurek okazuje się być <w>faworytem</w> w tym wyścigu.",
                            "target_lang_translations": {
                                "ru": "Юрек оказывается <w>фаворитом</w> в этой гонке."
                            }
                        },
                        {
                            "text": "Brazylia jest głównym <w>faworytem</w> tegorocznych mistrzostw świata.",
                            "target_lang_translations": {
                                "ru": "Бразилия является главным <w>фаворитом</w> чемпионата мира этого года."
                            }
                        }
                    ],
                    "synonyms": [
                        "bak"
                    ],
                    "antonyms": [
                        "outsider"
                    ],
                    "common_phrases": [

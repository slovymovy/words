                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The patient required immediate <w>operative</w> intervention to repair the damage.",
                            "target_lang_translations": {
                                "ru": "Пациенту требовалось немедленное <w>оперативное</w> вмешательство, чтобы устранить повреждения."
                            }

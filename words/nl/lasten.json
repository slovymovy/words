            "pos": "noun",
            "senses": [
                {
                    "sense_id": "94b9fc4f-a28d-3459-9b11-4a604a37fa70",
                    "sense_definition": "Meervoud van <w>last</w>; vaak gebruikt om verplichte financiële uitgaven of zware verantwoordelijkheden aan te duiden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "financial_burden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Onze vaste <w>lasten</w> zijn deze maand hoger dan verwacht door de energierekening.",
                            "target_lang_translations": {
                                "ru": "Наши постоянные <w>расходы</w> в этом месяце выше ожидаемого из-за счета за электроэнергию."
                            }
                        },
                        {
                            "text": "De regering wil de <w>lasten</w> voor kleine ondernemers verlagen.",
                            "target_lang_translations": {
                                "ru": "Правительство хочет снизить <w>нагрузку</w> на малых предпринимателей."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitgaven",
                        "kosten",
                        "verplichtingen",
                        "onkosten"
                    ],
                    "antonyms": [
                        "baten",
                        "inkomsten"
                    ],
                    "common_phrases": [
                        "vaste lasten",
                        "lasten en baten",
                        "lasten verlichten",
                        "sociale lasten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от <w>last</w>; часто используется для обозначения обязательных финансовых расходов или тяжелой ответственности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расходы",
                                "target_lang_sense_clarification": "финансовые затраты"
                            },
                            {
                                "target_lang_word": "бремя",
                                "target_lang_sense_clarification": "тяжелая ответственность или нагрузка"
                            },
                            {
                                "target_lang_word": "издержки",
                                "target_lang_sense_clarification": "затраты, расходы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e90d4438-c0e0-32e6-b40c-9e10979274a0",
                    "sense_definition": "Vervoeging van <w>lassen</w>: onvoltooid verleden tijd meervoud.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arbeiders <w>lasten</w> de metalen platen gisteren aan elkaar.",
                            "target_lang_translations": {
                                "ru": "Рабочие вчера <w>сваривали</w> металлические пластины друг с другом."
                            }
                        },
                        {
                            "text": "Wij <w>lasten</w> de kapotte onderdelen zodat de machine weer werkte.",
                            "target_lang_translations": {
                                "ru": "Мы <w>сварили</w> сломанные детали, чтобы машина снова заработала."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола <w>lassen</w>: прошедшее время, множественное число."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сваривать",
                                "target_lang_sense_clarification": "соединять металл сваркой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd949e3f-992d-382f-acfc-e412241a961f",
                    "sense_definition": "Iemand of iets met een vracht of opdracht bezwaren.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "burden_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude verhalen <w>lasten</w> reizigers hun dieren met zware goederen.",
                            "target_lang_translations": {
                                "ru": "В старых рассказах путешественники <w>нагружали</w> своих животных тяжелыми товарами."
                            }
                        },
                        {
                            "text": "Zij <w>lasten</w> hem met de zware taak om het nieuws te brengen.",
                            "target_lang_translations": {
                                "ru": "Они <w>обременили</w> его тяжелой задачей сообщить новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "belasten",
                        "bezwaren",
                        "beladen"
                    ],
                    "antonyms": [

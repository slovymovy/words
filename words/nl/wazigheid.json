{
    "word_family": [
        "wazig",
        "waas"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "161e8b89-c0b0-3127-9f79-9e7699834294",
                    "sense_definition": "De toestand van slecht zichtbaar zijn, alsof er een nevel voor hangt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "lack_of_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>wazigheid</w> van de foto maakte het onmogelijk om de gezichten te herkennen.",
                            "target_lang_translations": {
                                "en": "The <w>blurriness</w> of the photo made it impossible to recognize the faces.",
                                "ru": "<w>Размытость</w> фотографии сделала невозможным распознавание лиц."
                            }
                        },
                        {
                            "text": "Door de <w>wazigheid</w> in de verte konden we de bergen niet goed zien.",
                            "target_lang_translations": {
                                "en": "Due to the <w>haziness</w> in the distance, we could not see the mountains well.",
                                "ru": "Из-за <w>туманности</w> вдалеке мы не могли хорошо разглядеть горы."
                            }
                        }
                    ],
                    "synonyms": [
                        "onscherpte"
                    ],
                    "antonyms": [
                        "scherpte",
                        "helderheid"
                    ],
                    "common_phrases": [
                        "wazigheid van het beeld",
                        "optische wazigheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of being poorly visible, as if covered by a mist or out of focus.",
                        "ru": "Состояние плохой видимости или отсутствия резкости, словно сквозь дымку."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blurriness",
                                "target_lang_sense_clarification": "lack of visual focus or sharpness"
                            },
                            {
                                "target_lang_word": "haziness",
                                "target_lang_sense_clarification": "obscured by mist or lack of clarity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "размытость",
                                "target_lang_sense_clarification": "визуальная нечеткость изображения"
                            },
                            {
                                "target_lang_word": "нечеткость",
                                "target_lang_sense_clarification": "отсутствие резкости контуров"
                            },
                            {
                                "target_lang_word": "туманность",
                                "target_lang_sense_clarification": "плохая видимость из-за дымки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8dba12f4-a034-3ad0-ac05-1599bc7dc1d3",
                    "sense_definition": "Het onduidelijk zijn als gevolg van vage uitingen of gebrek aan precisie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "lack_of_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er was veel kritiek op de <w>wazigheid</w> van de nieuwe beleidsplannen.",
                            "target_lang_translations": {
                                "en": "There was much criticism regarding the <w>vagueness</w> of the new policy plans.",
                                "ru": "Было много критики по поводу <w>расплывчатости</w> новых политических планов."
                            }
                        },
                        {
                            "text": "De <w>wazigheid</w> in zijn antwoorden wekte argwaan bij de commissie.",
                            "target_lang_translations": {
                                "en": "The <w>vagueness</w> in his answers aroused suspicion among the committee.",
                                "ru": "<w>Туманность</w> в его ответах вызвала подозрения у комиссии."
                            }
                        }
                    ],
                    "antonyms": [
                        "duidelijkheid",
                        "concreetheid"
                    ],
                    "common_phrases": [
                        "wazigheid van de plannen",
                        "wazigheid in de communicatie"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality of being unclear due to vague expressions or lack of precision.",
                        "ru": "Неясность или отсутствие конкретики в высказываниях, планах или мыслях."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vagueness",
                                "target_lang_sense_clarification": "lack of precision or detail in communication"
                            },
                            {
                                "target_lang_word": "fuzziness",
                                "target_lang_sense_clarification": "lack of clear definition or logic"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "расплывчатость",
                                "target_lang_sense_clarification": "отсутствие точности и конкретики"
                            },
                            {
                                "target_lang_word": "туманность",
                                "target_lang_sense_clarification": "загадочность или непонятность смысла"
                            },
                            {
                                "target_lang_word": "неясность",
                                "target_lang_sense_clarification": "общее отсутствие понимания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
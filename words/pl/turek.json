    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5f64a35b-9321-3ddc-aacf-9887b1d0a965",
                    "sense_definition": "Obywatel Turcji lub mężczyzna narodowości tureckiej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "nationality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad jest <w>Turkiem</w> i często opowiada mi o swoim kraju.",
                            "target_lang_translations": {
                                "ru": "Мой сосед — <w>турок</w>, и он часто рассказывает мне о своей стране."
                            }
                        },
                        {
                            "text": "Wczoraj poznałem bardzo sympatycznego <w>Turka</w>, który przyjechał do Polski na studia.",
                            "target_lang_translations": {
                                "ru": "Вчера я познакомился с очень приятным <w>турком</w>, который приехал в Польшу на учебу."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гражданин Турции или мужчина турецкой национальности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "турок",
                                "target_lang_sense_clarification": "национальность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bc1189d-fe08-3317-aeb6-a46fb361a516",
                    "sense_definition": "Potoczna nazwa jadalnego grzyba leśnego, płachetki kołpakowatej.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fungi",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lesie znaleźliśmy dorodnego <w>turka</w> o jasnym kapeluszu.",
                            "target_lang_translations": {
                                "ru": "В лесу мы нашли крупный <w>колпак кольчатый</w> со светлой шляпкой."
                            }
                        },
                        {
                            "text": "Moja babcia robi pyszną zupę z marynowanych <w>turków</w>.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка готовит вкусный суп из маринованных <w>колпаков</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "płachetka kołpakowata",
                        "niemka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название съедобного лесного гриба, колпака кольчатого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колпак кольчатый",
                                "target_lang_sense_clarification": "гриб"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4262d855-7b20-3fdc-8e15-a4a8903e3ceb",
                    "sense_definition": "Miasto powiatowe w środkowej Polsce, w województwie wielkopolskim.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Moi dziadkowie mieszkają w <w>Turku</w> od wielu lat.",
                            "target_lang_translations": {
                                "ru": "Мои дедушка и бабушка живут в <w>Туреке</w> уже много лет."
                            }
                        },
                        {
                            "text": "Muszę pojechać do <w>Turka</w>, aby załatwić sprawy w urzędzie.",
                            "target_lang_translations": {
                                "ru": "Мне нужно поехать в <w>Турек</w>, чтобы уладить дела в учреждении."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Районный город в центральной Польше, в Великопольском воеводстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Турек",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd13bad0-5d93-3342-82ff-f48a53115a5d",
                    "sense_definition": "Polskie nazwisko.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Turek</w> jest naszym nowym nauczycielem matematyki.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Турек</w> — наш новый учитель математики."
                            }
                        },
                        {
                            "text": "Nie ma dzisiaj w pracy pani <w>Turek</w>, ponieważ wzięła urlop.",
                            "target_lang_translations": {
                                "ru": "Сегодня на работе нет госпожи <w>Турек</w>, так как она взяла отпуск."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Турек",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }

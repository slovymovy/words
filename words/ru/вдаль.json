    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d09571e9-718f-3875-a0dc-038d152ac539",
                    "sense_definition": "На далёкое расстояние, в направлении от наблюдателя; далеко вперёд.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он стоял на берегу и долго смотрел <w>вдаль</w>.",
                            "target_lang_translations": {
                                "en": "He stood on the shore and looked <w>into the distance</w> for a long time."
                            }
                        },
                        {
                            "text": "Извилистая дорога уходила <w>вдаль</w>, исчезая за горизонтом.",
                            "target_lang_translations": {
                                "en": "The winding road stretched <w>into the distance</w>, disappearing beyond the horizon."
                            }
                        }
                    ],
                    "synonyms": [
                        "далеко"
                    ],
                    "common_phrases": [
                        "смотреть вдаль",
                        "уходить вдаль",
                        "глядеть вдаль",
                        "устремить взгляд вдаль"
                    ],
                    "target_lang_definitions": {
                        "en": "To a great distance, away from the observer; far ahead in space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "into the distance",
                                "target_lang_sense_clarification": "direction away from the observer"
                            },
                            {
                                "target_lang_word": "far away",
                                "target_lang_sense_clarification": "to a remote location"
                            },
                            {
                                "target_lang_word": "far off",
                                "target_lang_sense_clarification": "at a considerable distance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b52ebdd5-3626-3316-82fb-f07e330cc6c7",
                    "sense_definition": "В отдалённое будущее (переносное значение).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_future",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему представилась уходящая <w>вдаль</w> вереница дней и недель.",
                            "target_lang_translations": {
                                "en": "He imagined a succession of days and weeks stretching <w>far ahead</w>."
                            }
                        },
                        {
                            "text": "Мы старались не загадывать <w>вдаль</w> и решать проблемы по мере их поступления.",
                            "target_lang_translations": {
                                "en": "We tried not to look too <w>far ahead</w> and to solve problems as they arose."
                            }
                        }
                    ],
                    "common_phrases": [
                        "загадывать вдаль",
                        "мысли уносят вдаль"
                    ],
                    "target_lang_definitions": {
                        "en": "Into the distant future (figurative)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "far ahead",
                                "target_lang_sense_clarification": "regarding time or future plans"
                            },
                            {
                                "target_lang_word": "into the future",
                                "target_lang_sense_clarification": "figurative direction in time"
                            }
                        ]
                    }
                }
            ]
        }

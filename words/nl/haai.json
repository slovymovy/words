    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8783b3e7-63c3-3701-ae60-5492a3845744",
                    "sense_definition": "Een grote, vleesetende vis met een kraakbeenachtig skelet, scherpe tanden en een kenmerkende rugvin.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het duiken zagen we een kleine <haai> zwemmen.",
                            "target_lang_translations": {
                                "en": "While diving, we saw a small <w>shark</w> swimming."
                            }
                        },
                        {
                            "text": "De witte <haai> is een van de bekendste roofdieren in de oceaan.",
                            "target_lang_translations": {
                                "en": "The great white <w>shark</w> is one of the most famous predators in the ocean."
                            }
                        }
                    ],
                    "synonyms": [
                        "roofvis"
                    ],
                    "common_phrases": [
                        "naar de haaien",
                        "witte haai",
                        "dode haai"
                    ],
                    "target_lang_definitions": {
                        "en": "A large, carnivorous fish with a cartilaginous skeleton, sharp teeth, and a distinctive dorsal fin."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shark",
                                "target_lang_sense_clarification": "predatory fish"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "670dcf12-9567-3418-8055-7ea49ab6728e",
                    "sense_definition": "Een meedogenloos of hebzuchtig persoon die ten koste van anderen winst probeert te maken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pas op voor die <haai>, hij probeert je bedrijf over te nemen voor een prikkie.",
                            "target_lang_translations": {
                                "en": "Watch out for that <w>shark</w>, he's trying to take over your company for a pittance."
                            }
                        },
                        {
                            "text": "In de zakenwereld moet je oppassen voor elke <haai> die op geld uit is.",
                            "target_lang_translations": {
                                "en": "In the business world, you have to watch out for every <w>shark</w> who is out for money."
                            }
                        }
                    ],
                    "synonyms": [
                        "geldwolf",
                        "woekeraar"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used metaphorically"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A ruthless or greedy person who tries to make a profit at the expense of others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shark",
                                "target_lang_sense_clarification": "ruthless person"
                            },
                            {
                                "target_lang_word": "loan shark",
                                "target_lang_sense_clarification": "usurer"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d965b4ac-c2f6-3af6-a903-c2e5fcebf97c",
                    "sense_definition": "Een bocht of draai, bijvoorbeeld in een weg of waterloop.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De smalle weg maakt hier een scherpe <haai> naar links.",
                            "target_lang_translations": {
                                "en": "The narrow road makes a sharp <w>turn</w> to the left here."
                            }
                        },
                        {
                            "text": "Door de <haai> in de rivier stroomt het water hier sneller.",
                            "target_lang_translations": {
                                "en": "Due to the <w>bend</w> in the river, the water flows faster here."
                            }
                        }
                    ],
                    "synonyms": [
                        "bocht",
                        "draai"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Southern Dutch / Flemish"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A bend or turn, for example in a road or watercourse."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bend",
                                "target_lang_sense_clarification": "curve in a road or river"
                            },
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "change of direction"
                            }
                        ]
                    }
                }
            ]
        },
        {

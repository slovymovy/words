            "senses": [
                {
                    "sense_id": "b316478e-d9d2-3495-bddc-56a3b39c8a88",
                    "sense_definition": "Simple past and past participle of <w>grant</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was <w>granted</w> a patent on his invention.",
                            "target_lang_translations": {
                                "ru": "Ему был <w>выдан</w> патент на его изобретение."
                            }
                        },
                        {
                            "text": "The scholarship is <w>granted</w> to students with excellent grades.",
                            "target_lang_translations": {
                                "ru": "Стипендия <w>предоставляется</w> студентам с отличными оценками."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past and past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола <w>grant</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выданный",
                                "target_lang_sense_clarification": "о документе, патенте или разрешении"
                            },
                            {
                                "target_lang_word": "предоставленный",
                                "target_lang_sense_clarification": "о праве, стипендии или возможности"

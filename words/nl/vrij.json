        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4b6098d2-120c-3158-a3c7-face21491f6d",
                    "sense_definition": "niet hoeven werken of naar school hoeven; vrije tijd hebbend",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_off",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben morgen een dagje <w>vrij</w> van mijn werk.",
                            "target_lang_translations": {
                                "ru": "У меня завтра <w>выходной</w> на работе."
                            }
                        },
                        {
                            "text": "De kinderen zijn 's middags <w>vrij</w> en spelen buiten.",
                            "target_lang_translations": {
                                "ru": "Днем дети <w>свободны</w> и играют на улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "onbelemmerd"
                    ],
                    "common_phrases": [
                        "vrij zijn",
                        "vrij hebben",
                        "een dagje vrij"
                    ],
                    "target_lang_definitions": {
                        "ru": "не нужно работать или идти в школу; имеющий свободное время"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свободный",
                                "target_lang_sense_clarification": "о времени или человеке"
                            },
                            {
                                "target_lang_word": "выходной",
                                "target_lang_sense_clarification": "о дне отдыха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8707547b-cac7-397a-97fc-c6e63ab04781",
                    "sense_definition": "niet bezet door iemand of iets anders; beschikbaar voor gebruik",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "available",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Is deze stoel naast u nog <w>vrij</w>?",
                            "target_lang_translations": {
                                "ru": "Этот стул рядом с вами еще <w>свободен</w>?"
                            }
                        },
                        {
                            "text": "Er was geen enkele parkeerplaats meer <w>vrij</w> in de straat.",
                            "target_lang_translations": {
                                "ru": "На улице больше не было ни одного <w>свободного</w> парковочного места."
                            }
                        }
                    ],
                    "synonyms": [
                        "behoorlijk",
                        "redelijk",
                        "tamelijk"
                    ],
                    "antonyms": [
                        "belemmerd",
                        "gehinderd"
                    ],
                    "common_phrases": [
                        "is deze plaats vrij",
                        "de weg is vrij"
                    ],
                    "target_lang_definitions": {
                        "ru": "не занятый кем-то или чем-то другим; доступный для использования"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свободный",
                                "target_lang_sense_clarification": "не занятый (место, стул)"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5d05c513-c3f0-3f0b-a083-036a9a53a8e5",
                    "sense_definition": "Материал, изготовленный из нитей или волокон на станке или другим способом, используемый для шитья одежды и других изделий.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "textile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама купила красивую <ткань>, чтобы сшить себе летнее платье.",
                            "target_lang_translations": {
                                "en": "Mom bought some beautiful <fabric> to sew herself a summer dress."
                            }
                        },
                        {
                            "text": "Эта <ткань> сделана из натурального хлопка и очень приятна на ощупь.",
                            "target_lang_translations": {
                                "en": "This <fabric> is made of natural cotton and is very pleasant to the touch."
                            }
                        }
                    ],
                    "synonyms": [
                        "материал",
                        "материя"
                    ],
                    "common_phrases": [
                        "шёлковая ткань",
                        "хлопчатобумажная ткань",
                        "мягкая ткань",
                        "отрез ткани"
                    ],
                    "target_lang_definitions": {
                        "en": "Material made of threads or fibers, used for making clothes and other items."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fabric",
                                "target_lang_sense_clarification": "textile material"
                            },
                            {
                                "target_lang_word": "material",
                                "target_lang_sense_clarification": "cloth for making clothes"
                            },
                            {
                                "target_lang_word": "cloth",
                                "target_lang_sense_clarification": "woven material"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c540ee50-4a3f-3ea6-919f-f090a3049adb",
                    "sense_definition": "Группа клеток в живом организме, имеющих одинаковое строение и выполняющих одну функцию.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мышечная <ткань> позволяет человеку и животным активно двигаться.",
                            "target_lang_translations": {
                                "en": "Muscle <tissue> allows humans and animals to move actively."
                            }
                        },
                        {
                            "text": "Врач сказал, что повреждённая <ткань> быстро восстановится после лечения.",
                            "target_lang_translations": {
                                "en": "The doctor said that the damaged <tissue> would recover quickly after treatment."
                            }
                        }
                    ],
                    "common_phrases": [
                        "живая ткань",
                        "нервная ткань",
                        "мышечная ткань",
                        "соединительная ткань"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of cells in a living organism that have a similar structure and perform a specific function."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tissue",
                                "target_lang_sense_clarification": "biological material in organisms"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28afe73e-0346-3c76-b7d6-780465fc3975",
                    "sense_definition": "Внутренняя структура, содержание или основа чего-либо (обычно художественного произведения или последовательности событий).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автор искусно вплетает реальные исторические факты в <ткань> своего романа.",
                            "target_lang_translations": {
                                "en": "The author skillfully weaves real historical facts into the <fabric> of his novel."
                            }
                        },
                        {
                            "text": "Одно неожиданное событие нарушило привычную <ткань> нашей повседневной жизни.",
                            "target_lang_translations": {
                                "en": "One unexpected event disrupted the familiar <fabric> of our daily life."
                            }
                        }
                    ],
                    "synonyms": [
                        "канва"
                    ],
                    "common_phrases": [
                        "ткань повествования",
                        "ткань рассказа",
                        "ткань жизни"
                    ],
                    "target_lang_definitions": {
                        "en": "The internal structure, content, or basis of something (usually a literary work or sequence of events)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fabric",
                                "target_lang_sense_clarification": "underlying structure or framework"
                            },
                            {
                                "target_lang_word": "texture",
                                "target_lang_sense_clarification": "structure or substance of a work"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
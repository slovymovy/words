                    "sense_id": "f04e0abd-b119-338f-9770-964f03d2fa67",
                    "sense_definition": "In a normal way that is not artificial or forced.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tried to act <w>naturally</w> when the teacher entered the room.",
                            "target_lang_translations": {
                                "ru": "Он старался вести себя <w>естественно</w>, когда учитель вошёл в комнату."
                            }
                        },
                        {
                            "text": "The plants grow <w>naturally</w> in this climate without any help.",
                            "target_lang_translations": {
                                "ru": "Растения растут <w>естественно</w> в этом климате без какой-либо помощи."
                            }
                        }
                    ],
                    "synonyms": [
                        "unaffectedly",
                        "genuinely",
                        "normally"
                    ],
                    "antonyms": [
                        "artificially",
                        "unnaturally"
                    ],
                    "common_phrases": [
                        "act naturally",
                        "behave naturally",
                        "occur naturally"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественным образом, без притворства или напряжения."
                    },
                    "translations": {

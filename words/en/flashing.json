    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "38e7a757-9553-361d-b655-696a8204f689",
                    "sense_definition": "Present participle and gerund of <w>flash</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police car drove past with its blue lights <w>flashing</w>.",
                            "target_lang_translations": {
                                "ru": "Полицейская машина проехала мимо с <w>мигающими</w> синими огнями."
                            }
                        },
                        {
                            "text": "He was <w>flashing</w> a torch around the dark room to find the switch.",
                            "target_lang_translations": {
                                "ru": "Он <w>светил</w> фонариком по темной комнате, чтобы найти выключатель."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола flash."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мигающий",
                                "target_lang_sense_clarification": "включающийся и выключающийся (о свете)"
                            },
                            {
                                "target_lang_word": "сверкающий",
                                "target_lang_sense_clarification": "ярко блестящий"
                            },
                            {
                                "target_lang_word": "вспыхивающий",
                                "target_lang_sense_clarification": "внезапно загорающийся"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d66b7206-698c-35ce-ae98-bd7c325f2b45",
                    "sense_definition": "The act of a light turning on and off repeatedly and quickly.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "light_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The incessant <w>flashing</w> of the neon sign kept the neighborhood awake.",
                            "target_lang_translations": {
                                "ru": "Непрекращающееся <w>мигание</w> неоновой вывески не давало спать всей округе."
                            }
                        },
                        {
                            "text": "We noticed the <w>flashing</w> of a warning signal in the distance.",
                            "target_lang_translations": {
                                "ru": "Мы заметили <w>мигание</w> предупреждающего сигнала вдалеке."
                            }
                        }
                    ],
                    "synonyms": [
                        "blinking",
                        "twinkling"
                    ],
                    "common_phrases": [
                        "light flashing",
                        "constant flashing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, при котором свет многократно и быстро включается и выключается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мигание",
                                "target_lang_sense_clarification": "периодическое включение и выключение света"
                            },
                            {
                                "target_lang_word": "мерцание",
                                "target_lang_sense_clarification": "неровный, дрожащий свет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cccf4f1-a4f0-383b-806d-a45462ea9e7a",
                    "sense_definition": "A strip of metal or other material used to cover joints or edges, especially on a roof, to prevent water from entering.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The roofer installed lead <w>flashing</w> around the chimney to stop the leak.",
                            "target_lang_translations": {
                                "ru": "Кровельщик установил свинцовый <w>фартук</w> вокруг дымохода, чтобы остановить течь."
                            }
                        },
                        {
                            "text": "Water damage occurred because the <w>flashing</w> had rusted through.",
                            "target_lang_translations": {
                                "ru": "Ущерб от воды возник из-за того, что <w>защитная планка</w> проржавела насквозь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "roof flashing",
                        "lead flashing",
                        "copper flashing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полоса металла или другого материала, используемая для покрытия стыков или краев, особенно на крыше, для предотвращения попадания воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фартук",
                                "target_lang_sense_clarification": "защитное покрытие вокруг дымохода или на стыках кровли"
                            },
                            {
                                "target_lang_word": "оклад",
                                "target_lang_sense_clarification": "гидроизоляционное обрамление"
                            },
                            {
                                "target_lang_word": "нащельник",
                                "target_lang_sense_clarification": "планка, закрывающая щель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a34ffcd9-0ba5-3335-991c-b91acfb1c9a5",
                    "sense_definition": "The act of briefly exposing one's naked body in public.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "indecent_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was arrested for <w>flashing</w> in the park yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера его арестовали за <w>публичное обнажение</w> в парке."
                            }
                        },
                        {
                            "text": "The incident involved <w>flashing</w> during a live television broadcast.",
                            "target_lang_translations": {
                                "ru": "Инцидент был связан с <w>обнажением</w> во время прямого телеэфира."
                            }
                        }
                    ],
                    "synonyms": [
                        "exposure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт кратковременного обнажения тела в общественном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публичное обнажение",
                                "target_lang_sense_clarification": "демонстрация наготы"
                            },
                            {
                                "target_lang_word": "эксгибиционизм",
                                "target_lang_sense_clarification": "психологический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc23dce7-6643-349f-93aa-5e6dc1061a82",
                    "sense_definition": "A sudden blazing or bursting, such as of fire or water.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "light_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden <w>flashing</w> of the fireworks illuminated the night sky.",
                            "target_lang_translations": {
                                "ru": "Внезапное <w>сверкание</w> фейерверков озарило ночное небо."
                            }
                        },
                        {
                            "text": "There was a <w>flashing</w> of sparks as the metal struck the stone.",
                            "target_lang_translations": {
                                "ru": "Когда металл ударился о камень, произошла <w>вспышка</w> искр."
                            }
                        }
                    ],
                    "synonyms": [
                        "bursting",
                        "blazing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное возгорание или выброс, например, огня или воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вспышка",
                                "target_lang_sense_clarification": "резкое появление света или огня"
                            },
                            {
                                "target_lang_word": "сверкание",
                                "target_lang_sense_clarification": "яркий блеск"
                            }
                        ]
                    }
                }
            ]
        }
    ]

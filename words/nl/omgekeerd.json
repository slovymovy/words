            "senses": [
                {
                    "sense_id": "e730d9e4-4bc6-384d-aa93-6317cc8c9312",
                    "sense_definition": "In tegengestelde richting, volgorde of werking; tegenovergesteld aan wat normaal of verwacht is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_reverse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb de stappen in <w>omgekeerde</w> volgorde uitgevoerd.",
                            "target_lang_translations": {
                                "en": "I performed the steps in <w>reverse</w> order.",
                                "ru": "Я выполнил шаги в <w>обратном</w> порядке."
                            }
                        },
                        {
                            "text": "Het lijkt soms wel de <w>omgekeerde</w> wereld.",
                            "target_lang_translations": {
                                "en": "Sometimes it seems like the <w>upside-down</w> world.",
                                "ru": "Иногда это кажется <w>перевёрнутым</w> миром."
                            }
                        }
                    ],
                    "synonyms": [
                        "tegengesteld",
                        "tegenovergesteld"
                    ],
                    "common_phrases": [
                        "in omgekeerde volgorde",
                        "de omgekeerde wereld",
                        "in omgekeerde richting"
                    ],
                    "target_lang_definitions": {
                        "en": "In the opposite direction, order, or function; contrary to what is normal or expected.",
                        "ru": "Противоположный по направлению, порядку или действию; обратный нормальному или ожидаемому."
                    },
                    "translations": {

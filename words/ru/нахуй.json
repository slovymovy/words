    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9e0e0e6a-a208-3b44-b4c9-c5e7a20af8bd",
                    "sense_definition": "Грубое вопросительное слово, означающее «зачем», «для чего» или «с какой целью».",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "why_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я совершенно не понимаю, <нахуй> мы вообще сюда приехали.",
                            "target_lang_translations": {
                                "en": "I don't understand <why the fuck> we even came here."
                            }
                        },
                        {
                            "text": "Если тебе это не интересно, то <нахуй> надо этим заниматься.",
                            "target_lang_translations": {
                                "en": "If you're not interested, then <why the fuck> bother doing it."
                            }
                        }
                    ],
                    "synonyms": [
                        "нахуя",
                        "зачем",
                        "для чего",
                        "к чему",
                        "нах"
                    ],
                    "common_phrases": [
                        "нахуй надо",
                        "нахуй это"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "obscene"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A rude interrogative word meaning \"why\", \"what for\", or \"for what purpose\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "why the fuck",
                                "target_lang_sense_clarification": "emphatic, rude interrogative"
                            },
                            {
                                "target_lang_word": "what the fuck for",
                                "target_lang_sense_clarification": "emphatic, rude interrogative regarding purpose"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59ab4fb8-7a75-3f7a-bf2a-149873af9a55",
                    "sense_definition": "Разговорный вариант написания выражения <w>на хуй</w>, используемый для грубого требования уйти или выражения сильного раздражения.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "direction_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Когда он начал кричать, я просто послал его <нахуй> и ушёл.",
                            "target_lang_translations": {
                                "en": "When he started shouting, I just told him to <fuck off> and left."
                            }
                        },
                        {
                            "text": "Да пошло оно всё <нахуй>, я больше не буду это терпеть.",
                            "target_lang_translations": {
                                "en": "Let it all go <to hell>, I'm not going to tolerate this anymore."
                            }
                        }
                    ],
                    "common_phrases": [
                        "иди нахуй",
                        "пошел нахуй"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "variant of на хуй"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "obscene"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial spelling of the expression \"на хуй\", used to rudely demand someone to leave or to express strong irritation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fuck off",
                                "target_lang_sense_clarification": "used in phrases telling someone to leave"
                            },
                            {
                                "target_lang_word": "to hell",
                                "target_lang_sense_clarification": "used in phrases expressing dismissal or frustration"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    ],
                    "synonyms": [
                        "get angry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Злиться, приходить в состояние гнева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "злиться",
                                "target_lang_sense_clarification": "испытывать злость"
                            },
                            {
                                "target_lang_word": "сердиться",
                                "target_lang_sense_clarification": "быть в раздражении"
                            },
                            {
                                "target_lang_word": "гневаться",
                                "target_lang_sense_clarification": "испытывать гнев"
                            }
                        ]
                    }
                }

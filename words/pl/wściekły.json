            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ca024ea3-767f-3fe6-b008-2ca7c654c78f",
                    "sense_definition": "Bardzo zdenerwowany, odczuwający silny gniew lub furię.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion_anger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój ojciec był <w>wściekły</w>, gdy dowiedział się o rozbitym samochodzie.",
                            "target_lang_translations": {
                                "ru": "Мой отец был <w>взбешён</w>, когда узнал о разбитой машине."
                            }
                        },
                        {
                            "text": "Jestem na ciebie <w>wściekły</w> za to, że nie dotrzymałeś słowa.",
                            "target_lang_translations": {
                                "ru": "Я ужасно <w>злой</w> на тебя за то, что ты не сдержал слово."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozgniewany",
                        "rozzłoszczony",
                        "wzburzony",
                        "rozwścieczony",
                        "wkurzony",
                        "gniewny",
                        "cięty"
                    ],
                    "antonyms": [
                        "spokojny",
                        "łagodny"
                    ],
                    "common_phrases": [
                        "wściekły jak osa",
                        "wściekły na kogoś",
                        "wściekły tłum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень расстроенный, испытывающий сильный гнев или ярость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разъярённый",
                                "target_lang_sense_clarification": "охваченный яростью"
                            },
                            {
                                "target_lang_word": "взбешённый",
                                "target_lang_sense_clarification": "крайне раздраженный, в состоянии бешенства"
                            },
                            {
                                "target_lang_word": "злой",
                                "target_lang_sense_clarification": "испытывающий злость (разговорное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bf2f1ed-f38d-31da-89fa-94cd75ded8e7",
                    "sense_definition": "Taki, który jest gwałtowny, intensywny lub odbywa się z dużą siłą.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Narzucili tak <w>wściekłe</w> tempo biegu, że wielu zawodników zostało w tyle.",
                            "target_lang_translations": {
                                "ru": "Они задали такой <w>бешеный</w> темп бега, что многие спортсмены остались позади."
                            }
                        },
                        {
                            "text": "Za oknem wiał <w>wściekły</w> wiatr, który łamał gałęzie drzew.",
                            "target_lang_translations": {
                                "ru": "За окном дул <w>неистовый</w> ветер, который ломал ветки деревьев."
                            }
                        }
                    ],
                    "synonyms": [
                        "intensywny",
                        "mocny"
                    ],
                    "antonyms": [
                        "łagodny",
                        "spokojny"
                    ],
                    "common_phrases": [
                        "wściekłe tempo",
                        "wściekły atak",
                        "wściekły wiatr"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который является бурным, интенсивным или происходит с большой силой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бешеный",
                                "target_lang_sense_clarification": "о скорости, темпе или ритме"
                            },
                            {
                                "target_lang_word": "неистовый",
                                "target_lang_sense_clarification": "о стихии, ветре или атаке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4fe20117-c075-3f83-874d-acf8513b0adf",
                    "sense_definition": "O zwierzęciu: chory na wściekliznę (groźną chorobę zakaźną).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "health_animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lesie znaleziono lisa, który prawdopodobnie był <w>wściekły</w>.",
                            "target_lang_translations": {
                                "ru": "В лесу нашли лису, которая, вероятно, была <w>бешеной</w>."
                            }
                        },
                        {
                            "text": "Każdy <w>wściekły</w> pies stanowi śmiertelne zagrożenie dla ludzi.",
                            "target_lang_translations": {
                                "ru": "Каждая <w>бешеная</w> собака представляет смертельную угрозу для людей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wściekły pies",
                        "wściekły lis",
                        "wściekłe zwierzę"
                    ],
                    "target_lang_definitions": {
                        "ru": "О животном: больной бешенством (опасным инфекционным заболеванием)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бешеный",
                                "target_lang_sense_clarification": "зараженный бешенством"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a98458c7-36e5-3109-abb2-0ae348915d84",
                    "sense_definition": "O kolorze: bardzo jaskrawy, intensywny, rzucający się w oczy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "color_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej sukienka miała <w>wściekły</w> różowy kolor, który było widać z daleka.",
                            "target_lang_translations": {
                                "ru": "У её платья был <w>ядовито</w>-розовый цвет, который было видно издалека."
                            }
                        },
                        {
                            "text": "Nie lubię, gdy ściany w pokoju są pomalowane na tak <w>wściekłe</w> barwy.",
                            "target_lang_translations": {
                                "ru": "Я не люблю, когда стены в комнате покрашены в такие <w>кричащие</w> цвета."
                            }
                        }
                    ],
                    "synonyms": [
                        "intensywny"
                    ],
                    "common_phrases": [
                        "wściekły róż",
                        "wściekły kolor",
                        "wściekła zieleń"
                    ],
                    "target_lang_definitions": {
                        "ru": "О цвете: очень яркий, интенсивный, бросающийся в глаза."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ядовитый",
                                "target_lang_sense_clarification": "о неестественно ярком цвете"
                            },
                            {
                                "target_lang_word": "кричащий",
                                "target_lang_sense_clarification": "о вызывающе ярком цвете"
                            }
                        ]
                    }
                }
            ]
        },
        {

                {
                    "sense_id": "2cd6e592-5803-3313-ae1a-466e3039e493",
                    "sense_definition": "wykonywanie melodii głosem; rzeczownik odczasownikowy od <w>śpiewać</w>",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "music_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej głośne <w>śpiewanie</w> pod prysznicem obudziło wszystkich domowników.",
                            "target_lang_translations": {
                                "ru": "Её громкое <w>пение</w> в душе разбудило всех домашних."
                            }
                        },
                        {
                            "text": "Wspólne <w>śpiewanie</w> kolęd to ważna część naszych świąt.",
                            "target_lang_translations": {
                                "ru": "Совместное <w>пение</w> колядок — важная часть наших праздников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wspólne śpiewanie",
                        "nauka śpiewania",
                        "lekcje śpiewania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исполнение мелодии голосом; отглагольное существительное от <w>śpiewać</w>"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пение",
                                "target_lang_sense_clarification": "исполнение музыки голосом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0820d80-85c0-39df-9c57-42667dd748e5",
                    "sense_definition": "wydawanie dźwięków przypominających melodię przez ptaki lub inne zwierzęta",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "music_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiosenne <w>śpiewanie</w> ptaków jest bardzo przyjemne dla ucha.",
                            "target_lang_translations": {
                                "ru": "Весеннее <w>пение</w> птиц очень приятно для слуха."
                            }
                        },
                        {
                            "text": "Niezwykłe <w>śpiewanie</w> wielorybów fascynuje naukowców od lat.",
                            "target_lang_translations": {
                                "ru": "Необычное <w>пение</w> китов годами восхищает учёных."
                            }
                        }
                    ],
                    "common_phrases": [
                        "śpiewanie ptaków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавание звуков, напоминающих мелодию, птицами или другими животными"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пение",
                                "target_lang_sense_clarification": "звуки птиц или животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb03617f-9220-365c-91a5-a098e1fd417e",
                    "sense_definition": "wydawanie tajemnic lub donoszenie na kogoś policji",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego nagłe <w>śpiewanie</w> na komendzie pogrążyło całą grupę przestępczą.",
                            "target_lang_translations": {
                                "ru": "Его внезапное <w>стукачество</w> в отделении полиции потопило всю преступную группу."
                            }
                        },
                        {
                            "text": "To nie było przypadkowe słowo, lecz celowe <w>śpiewanie</w> na kolegów.",
                            "target_lang_translations": {
                                "ru": "Это было не случайное слово, а намеренное <w>стукачество</w> на коллег."
                            }
                        }
                    ],
                    "synonyms": [
                        "sypanie",
                        "wydawanie",
                        "wkopywanie"
                    ],
                    "common_phrases": [
                        "zacząć śpiewanie"
                    ],
                    "traits": [
                        {

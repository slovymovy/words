            "pos": "verb",
            "senses": [
                {
                    "sense_id": "937c6122-89da-3fe9-9b03-d564505797e6",
                    "sense_definition": "Kennis hebben van iets; op de hoogte zijn van feiten of situaties.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kennis_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>weet</w> precies hoe laat de trein naar Amsterdam vertrekt.",
                            "target_lang_translations": {
                                "en": "I <w>know</w> exactly what time the train to Amsterdam leaves.",
                                "ru": "Я точно <w>знаю</w>, во сколько отправляется поезд в Амстердам."
                            }
                        },
                        {
                            "text": "Hij <w>weet</w> veel over de geschiedenis van dit land.",
                            "target_lang_translations": {
                                "en": "He <w>knows</w> a lot about the history of this country.",
                                "ru": "Он много <w>знает</w> об истории этой страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "beseffen"
                    ],
                    "common_phrases": [
                        "zeker weten",
                        "van wanten weten",
                        "god mag het weten"
                    ],
                    "target_lang_definitions": {
                        "en": "To have knowledge of something; to be aware of facts or situations.",
                        "ru": "Знать что-либо; обладать информацией о фактах или ситуациях."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "know",
                                "target_lang_sense_clarification": "to possess knowledge or information"
                            },
                            {
                                "target_lang_word": "be aware",
                                "target_lang_sense_clarification": "to be conscious of a fact"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "знать",
                                "target_lang_sense_clarification": "обладать сведениями"
                            },
                            {
                                "target_lang_word": "быть в курсе",
                                "target_lang_sense_clarification": "быть осведомленным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9795dd1f-59d4-3cce-9e3d-32d62eb93c82",
                    "sense_definition": "Zich iets herinneren; iets nog in het geheugen hebben.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "kennis_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>weet</w> nog goed hoe koud het vorig jaar in de winter was.",
                            "target_lang_translations": {
                                "en": "I still <w>remember</w> well how cold it was last winter.",
                                "ru": "Я ещё хорошо <w>помню</w>, как холодно было прошлой зимой."
                            }
                        },
                        {
                            "text": "<w>Weet</w> jij nog waar we de auto hebben geparkeerd?",
                            "target_lang_translations": {
                                "en": "Do you <w>remember</w> where we parked the car?",
                                "ru": "Ты ещё <w>помнишь</w>, где мы припарковали машину?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zich herinneren",
                        "onthouden"
                    ],
                    "antonyms": [
                        "vergeten"
                    ],
                    "common_phrases": [
                        "nog weten",
                        "niet meer weten"
                    ],
                    "target_lang_definitions": {
                        "en": "To remember something; to still have something in memory.",
                        "ru": "Помнить что-либо; сохранять информацию в памяти."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remember",
                                "target_lang_sense_clarification": "to recall from memory"
                            },
                            {
                                "target_lang_word": "recall",
                                "target_lang_sense_clarification": "to bring back to mind"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "помнить",
                                "target_lang_sense_clarification": "сохранять в памяти"
                            },
                            {
                                "target_lang_word": "припоминать",
                                "target_lang_sense_clarification": "восстанавливать в памяти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62115059-4e6f-3419-8aa7-00396641a102",
                    "sense_definition": "Iemand informatie geven; vertellen wat men ergens van vindt of wat de plannen zijn.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "kennis_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je me zo snel mogelijk laten <w>weten</w> of je morgen komt?",
                            "target_lang_translations": {
                                "en": "Can you let me <w>know</w> as soon as possible if you are coming tomorrow?",
                                "ru": "Можешь как можно скорее дать мне <w>знать</w>, придёшь ли ты завтра?"
                            }
                        },
                        {
                            "text": "De organisatie laat in een reactie <w>weten</w> dat ze teleurgesteld zijn.",
                            "target_lang_translations": {
                                "en": "The organization lets it be <w>known</w> in a response that they are disappointed.",
                                "ru": "В своём ответе организация <w>сообщает</w>, что они разочарованы."
                            }
                        }
                    ],
                    "synonyms": [
                        "meedelen",
                        "informeren",
                        "berichten"
                    ],
                    "common_phrases": [
                        "laten weten",
                        "iemand iets laten weten"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "gebruikt in de constructie 'laten weten'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To give someone information; to tell what one thinks or plans.",
                        "ru": "Сообщать кому-либо информацию; давать знать о планах или мнении."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let know",
                                "target_lang_sense_clarification": "to inform someone"
                            },
                            {
                                "target_lang_word": "inform",
                                "target_lang_sense_clarification": "to give notice or information"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сообщать",
                                "target_lang_sense_clarification": "передавать сведения"
                            },
                            {
                                "target_lang_word": "давать знать",
                                "target_lang_sense_clarification": "информировать"
                            },
                            {
                                "target_lang_word": "извещать",
                                "target_lang_sense_clarification": "уведомлять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af473edc-fa42-3235-82a5-510b70547412",
                    "sense_definition": "In staat zijn om iets te doen; erin slagen iets te bereiken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vermogen_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ploeg <w>wist</w> de wedstrijd in de laatste minuut toch nog te winnen.",
                            "target_lang_translations": {
                                "en": "The team still <w>managed</w> to win the match in the last minute.",
                                "ru": "Команде всё же <w>удалось</w> выиграть матч на последней минуте."
                            }
                        },
                        {
                            "text": "Ondanks de drukte <w>wisten</w> we een goede parkeerplek te vinden.",
                            "target_lang_translations": {
                                "en": "Despite the crowds, we <w>managed</w> to find a good parking spot.",
                                "ru": "Несмотря на суматоху, мы <w>сумели</w> найти хорошее парковочное место."
                            }
                        }
                    ],
                    "synonyms": [
                        "kunnen",
                        "slagen in"
                    ],
                    "common_phrases": [
                        "weten te vinden",
                        "zich weten te redden"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wordt gebruikt met 'te'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be able to do something; to succeed in achieving something.",
                        "ru": "Суметь что-то сделать; добиться успеха в достижении цели."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "manage",
                                "target_lang_sense_clarification": "to succeed in doing something"
                            },
                            {
                                "target_lang_word": "succeed",
                                "target_lang_sense_clarification": "to achieve a goal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "суметь",
                                "target_lang_sense_clarification": "справиться с действием"
                            },
                            {
                                "target_lang_word": "удаваться",
                                "target_lang_sense_clarification": "получаться сделать что-то"

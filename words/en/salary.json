                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "солёный",
                                "target_lang_sense_clarification": "имеющий вкус соли"
                            },
                            {
                                "target_lang_word": "солевой",
                                "target_lang_sense_clarification": "содержащий соль или состоящий из нее"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "common_phrases": [
                        "возобновить подписку",
                        "возобновить договор",
                        "возобновить абонемент"
                    ],
                    "target_lang_definitions": {
                        "en": "To extend the validity period of a document, agreement, or subscription."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "renew",
                                "target_lang_sense_clarification": "to extend the period of validity or loan"

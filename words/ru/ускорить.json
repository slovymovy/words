    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "728a95e7-8985-399a-ae3b-80190ff0e873",
                    "sense_definition": "Сделать более быстрым; увеличить скорость движения или действия",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "speed_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <ускорить> шаг, чтобы успеть на последний автобус.",
                            "target_lang_translations": {
                                "en": "We decided to <w>quicken</w> our pace to catch the last bus."
                            }
                        },
                        {
                            "text": "Водитель нажал на педаль газа и резко <ускорил> движение автомобиля.",
                            "target_lang_translations": {
                                "en": "The driver pressed the gas pedal and sharply <w>accelerated</w> the car."
                            }
                        }
                    ],
                    "synonyms": [
                        "убыстрить"
                    ],
                    "antonyms": [
                        "замедлить"
                    ],
                    "common_phrases": [
                        "ускорить шаг",
                        "ускорить темп",
                        "ускорить ход"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something faster; to increase the speed of movement or action"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "speed up",
                                "target_lang_sense_clarification": "general term for increasing speed"
                            },
                            {
                                "target_lang_word": "accelerate",
                                "target_lang_sense_clarification": "often used for vehicles or physics"
                            },
                            {
                                "target_lang_word": "quicken",
                                "target_lang_sense_clarification": "often used for steps, pulse, or breath"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d10ddfa7-4752-3537-b8f1-3ed606c38a15",
                    "sense_definition": "Приблизить наступление чего-либо; сделать так, чтобы событие произошло раньше",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Свежий воздух и покой помогли <ускорить> моё выздоровление.",
                            "target_lang_translations": {
                                "en": "Fresh air and rest helped <w>speed up</w> my recovery."
                            }
                        },
                        {
                            "text": "Новые технологии позволили значительно <ускорить> производственный процесс.",
                            "target_lang_translations": {
                                "en": "New technologies allowed us to significantly <w>accelerate</w> the production process."
                            }
                        }
                    ],
                    "synonyms": [
                        "прибли́зить"
                    ],
                    "antonyms": [
                        "замедлить"
                    ],
                    "common_phrases": [
                        "ускорить процесс",
                        "ускорить выздоровление",
                        "ускорить решение"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause something to happen sooner or be accomplished more quickly"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expedite",
                                "target_lang_sense_clarification": "formal, often regarding processes or paperwork"
                            },
                            {
                                "target_lang_word": "hasten",
                                "target_lang_sense_clarification": "to make something happen sooner than expected"
                            },
                            {
                                "target_lang_word": "accelerate",
                                "target_lang_sense_clarification": "to increase the rate of development or progress"
                            }
                        ]
                    }
                }
            ]
        }
    ]

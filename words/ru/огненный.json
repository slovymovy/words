            "senses": [
                {
                    "sense_id": "277eca72-659f-311d-be57-df00c7d05d5a",
                    "sense_definition": "Связанный с огнём, состоящий из огня или вызванный им.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "literal_fire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В небе внезапно вспыхнул яркий <w>огненный</w> шар.",
                            "target_lang_translations": {
                                "en": "A bright <w>fiery</w> ball suddenly flashed in the sky."
                            }
                        },
                        {
                            "text": "Легенда рассказывает про страшный <w>огненный</w> дождь, который уничтожил город.",
                            "target_lang_translations": {
                                "en": "The legend tells of a terrible <w>fiery</w> rain that destroyed the city."
                            }
                        }
                    ],
                    "synonyms": [
                        "огневой"
                    ],
                    "antonyms": [
                        "водяной"
                    ],
                    "common_phrases": [
                        "огненный шар",
                        "огненный дождь",
                        "огненная стихия",
                        "огненная стена"
                    ],
                    "target_lang_definitions": {
                        "en": "Related to fire, consisting of fire, or caused by it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fiery",
                                "target_lang_sense_clarification": "literal meaning"
                            },
                            {
                                "target_lang_word": "of fire",
                                "target_lang_sense_clarification": "descriptive"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7efa9d5f-3452-34bb-93bf-76267b95f08e",
                    "sense_definition": "Имеющий цвет огня; ярко-красный или оранжевый.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы долго любовались, как <w>огненный</w> закат окрашивает море.",
                            "target_lang_translations": {
                                "en": "We admired for a long time how the <w>fiery</w> sunset colored the sea."
                            }
                        },
                        {
                            "text": "Её <w>огненные</w> волосы сразу привлекали внимание в толпе.",
                            "target_lang_translations": {
                                "en": "Her <w>fiery</w> hair immediately attracted attention in the crowd."
                            }
                        }
                    ],
                    "common_phrases": [
                        "огненный закат",
                        "огненные волосы",
                        "огненный цвет"
                    ],
                    "target_lang_definitions": {
                        "en": "Having the color of fire; bright red or orange."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fiery",
                                "target_lang_sense_clarification": "color"
                            },
                            {
                                "target_lang_word": "flame-colored",
                                "target_lang_sense_clarification": "specific shade"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1367d1de-9309-36f5-bb8e-94c0d85c2b34",
                    "sense_definition": "Полный страсти, энергии и сильных эмоций; пылкий.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <w>огненная</w> речь вдохновила всех присутствующих на подвиги.",
                            "target_lang_translations": {
                                "en": "His <w>fiery</w> speech inspired everyone present to heroic deeds."
                            }
                        },
                        {
                            "text": "Артисты исполнили быстрый и <w>огненный</w> танец.",
                            "target_lang_translations": {
                                "en": "The artists performed a fast and <w>fiery</w> dance."
                            }
                        }
                    ],
                    "synonyms": [
                        "пламенный",
                        "зажигательный"
                    ],
                    "antonyms": [
                        "ледяной"

{
    "word_family": [
        "begrip",
        "begrijpelijk",
        "onbegrijpelijk",
        "begripvol"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "58243aac-de5e-3a24-af79-262b74ada15c",
                    "sense_definition": "De betekenis van iets weten of de samenhang van iets inzien.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "mental_grasp",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>begrijp</w> niet wat je bedoelt.",
                            "target_lang_translations": {
                                "en": "I don't <w>understand</w> what you mean.",
                                "ru": "Я не <w>понимаю</w>, что ты имеешь в виду."
                            }
                        },
                        {
                            "text": "Na de uitleg <w>begreep</w> de student de opdracht direct.",
                            "target_lang_translations": {
                                "en": "After the explanation, the student immediately <w>understood</w> the assignment.",
                                "ru": "После объяснения студент сразу <w>понял</w> задание."
                            }
                        }
                    ],
                    "synonyms": [
                        "snappen",
                        "vatten",
                        "verstaan"
                    ],
                    "common_phrases": [
                        "verkeerd begrijpen",
                        "goed begrijpen",
                        "niet te begrijpen"
                    ],
                    "target_lang_definitions": {
                        "en": "To know the meaning of something or understand how it works.",
                        "ru": "Знать значение чего-либо или видеть взаимосвязь вещей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "understand",
                                "target_lang_sense_clarification": "to know the meaning of"
                            },
                            {
                                "target_lang_word": "grasp",
                                "target_lang_sense_clarification": "to understand mentally"
                            },
                            {
                                "target_lang_word": "comprehend",
                                "target_lang_sense_clarification": "to grasp fully"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "улавливать смысл"
                            },
                            {
                                "target_lang_word": "осознавать",
                                "target_lang_sense_clarification": "постигать умом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6768b68f-e8ca-3b19-bdeb-6fbb6d680699",
                    "sense_definition": "Zich kunnen inleven in wat iemand anders voelt, denkt of wil.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_grasp",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan heel goed <w>begrijpen</w> dat je teleurgesteld bent.",
                            "target_lang_translations": {
                                "en": "I can fully <w>understand</w> that you are disappointed.",
                                "ru": "Я очень хорошо <w>понимаю</w>, что ты разочарован."
                            }
                        },
                        {
                            "text": "Zij <w>begrijpen</w> elkaar zonder woorden.",
                            "target_lang_translations": {
                                "en": "They <w>understand</w> each other without words.",
                                "ru": "Они <w>понимают</w> друг друга без слов."
                            }
                        }
                    ],
                    "synonyms": [
                        "snappen",
                        "verstaan",
                        "vatten"
                    ],
                    "common_phrases": [
                        "iemand begrijpen",
                        "elkaar begrijpen"
                    ],
                    "target_lang_definitions": {
                        "en": "To empathize with someone's feelings, thoughts, or intentions.",
                        "ru": "Быть способным войти в положение другого, понять чувства или мысли."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "understand",
                                "target_lang_sense_clarification": "to empathize with"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "сопереживать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aecf41dc-e3a0-3268-878f-7a98a52241c3",
                    "sense_definition": "Als onderdeel bevatten of insluiten, vaak gebruikt met betrekking tot kosten of totalen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "containment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De servicekosten zijn bij de prijs <w>begrepen</w>.",
                            "target_lang_translations": {
                                "en": "The service costs are <w>included</w> in the price.",
                                "ru": "Стоимость обслуживания <w>включена</w> в цену."
                            }
                        },
                        {
                            "text": "Alle maaltijden zijn in dit arrangement <w>begrepen</w>.",
                            "target_lang_translations": {
                                "en": "All meals are <w>included</w> in this package.",
                                "ru": "Все приемы пищи <w>включены</w> в этот пакет услуг."
                            }
                        }
                    ],
                    "synonyms": [
                        "omvatten",
                        "bevatten",
                        "bevatten"
                    ],
                    "common_phrases": [
                        "in de prijs begrepen",
                        "daaronder begrepen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To include or contain as part of a total, especially regarding costs.",
                        "ru": "Содержать в себе или включать как составную часть (обычно о расходах)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "include",
                                "target_lang_sense_clarification": "to contain as part of a whole"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "включать",
                                "target_lang_sense_clarification": "содержать в себе"
                            },
                            {
                                "target_lang_word": "входить",
                                "target_lang_sense_clarification": "быть частью"
                            }
                        ]
                    }
                }
            ]
        }
    ]

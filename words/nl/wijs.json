                },
                {
                    "sense_id": "d5095dc7-3308-3ecb-b800-41297b82ea22",
                    "sense_definition": "Een melodie of een reeks muzikale tonen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "music",
                    "name_type": "no",

                {
                    "sense_id": "f5dd9a2a-5b5e-3b05-af79-36efa9096c18",
                    "sense_definition": "Op geen enkele plaats.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn tas is helaas <w>nergens</w> meer te vinden.",
                            "target_lang_translations": {
                                "en": "Unfortunately, my bag is <w>nowhere</w> to be found anymore.",
                                "ru": "К сожалению, мою сумку больше <w>нигде</w> не найти."
                            }
                        },
                        {
                            "text": "In het hele huis was <w>nergens</w> een pen te bekennen.",
                            "target_lang_translations": {
                                "en": "In the whole house, a pen was <w>nowhere</w> to be seen.",
                                "ru": "Во всём доме <w>нигде</w> не было видно ручки."
                            }
                        }
                    ],
                    "antonyms": [
                        "ergens"
                    ],
                    "common_phrases": [
                        "nergens te bekennen",
                        "nergens te vinden"
                    ],
                    "target_lang_definitions": {
                        "en": "At no place; not anywhere.",
                        "ru": "Ни в каком месте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nowhere",
                                "target_lang_sense_clarification": "location"
                            },
                            {
                                "target_lang_word": "not ... anywhere",
                                "target_lang_sense_clarification": "alternative construction"
                            }
                        ],
                        "ru": [

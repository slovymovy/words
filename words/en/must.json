        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "226e1e16-b996-3722-9596-60c4869e92de",
                    "sense_definition": "Used to indicate that something is necessary, required, or very important to do.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You <w>must</w> finish your homework before you can watch TV.",
                            "target_lang_translations": {
                                "ru": "Ты <w>должен</w> закончить домашнюю работу, прежде чем сможешь смотреть телевизор."
                            }
                        },
                        {
                            "text": "We <w>must</w> go now or we will be late for the train.",
                            "target_lang_translations": {
                                "ru": "Мы <w>должны</w> идти сейчас, иначе опоздаем на поезд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "must go",
                        "must do",
                        "must see",
                        "must have"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на то, что действие является необходимым, обязательным или очень важным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "должен",
                                "target_lang_sense_clarification": "обязанность"
                            },

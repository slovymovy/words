    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1950a24b-ca6b-372d-bc9c-ca5fb678fe53",
                    "sense_definition": "Iemand of iets op een sluwe manier beïnvloeden of misleiden om een eigen doel te bereiken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politicus probeerde de publieke opinie te <w>manipuleren</w> met valse beloftes.",
                            "target_lang_translations": {
                                "en": "The politician tried to <w>manipulate</w> public opinion with false promises.",
                                "ru": "Политик пытался <w>манипулировать</w> общественным мнением с помощью ложных обещаний."
                            }
                        },
                        {
                            "text": "Het bedrijf heeft de cijfers <w>gemanipuleerd</w> om de winst hoger te laten lijken.",
                            "target_lang_translations": {
                                "en": "The company <w>manipulated</w> the figures to make the profit appear higher.",
                                "ru": "Компания <w>подтасовала</w> цифры, чтобы прибыль казалась выше."
                            }
                        }
                    ],
                    "synonyms": [
                        "beïnvloeden",
                        "bespelen",
                        "misleiden"
                    ],
                    "common_phrases": [
                        "de uitslag manipuleren",
                        "iemand emotioneel manipuleren",
                        "de markt manipuleren",
                        "de cijfers manipuleren"
                    ],
                    "target_lang_definitions": {
                        "en": "To influence or mislead someone or something in a cunning way to achieve one's own goal.",
                        "ru": "Влиять на кого-то или что-то хитрым способом или вводить в заблуждение для достижения собственной цели."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "manipulate",
                                "target_lang_sense_clarification": "influence unfairly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "манипулировать",
                                "target_lang_sense_clarification": "воздействовать на людей или общественное мнение"
                            },
                            {
                                "target_lang_word": "подтасовывать",
                                "target_lang_sense_clarification": "искажать данные или результаты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e9af5d2-acac-396e-b11b-9fb9f244dec3",
                    "sense_definition": "Iets vakkundig hanteren, bewerken of aanpassen, vaak met de hand of instrumenten.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wetenschappers kunnen tegenwoordig het DNA van planten <w>manipuleren</w>.",
                            "target_lang_translations": {
                                "en": "Scientists can nowadays <w>manipulate</w> the DNA of plants.",
                                "ru": "Ученые сегодня могут <w>модифицировать</w> ДНК растений."
                            }
                        },
                        {
                            "text": "Met dit computerprogramma kun je eenvoudig een foto <w>manipuleren</w>.",
                            "target_lang_translations": {
                                "en": "With this computer program you can easily <w>manipulate</w> a photo.",
                                "ru": "С помощью этой компьютерной программы можно легко <w>обрабатывать</w> фотографию."
                            }
                        }
                    ],
                    "synonyms": [
                        "hanteren",
                        "bewerken",
                        "aanpassen"
                    ],
                    "common_phrases": [
                        "genetisch manipuleren",
                        "gegevens manipuleren",
                        "variabelen manipuleren"
                    ],
                    "target_lang_definitions": {
                        "en": "To handle, edit, or adjust something skillfully, often by hand or instruments.",
                        "ru": "Умело обращаться с чем-либо, обрабатывать или изменять, часто вручную или с помощью инструментов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "manipulate",
                                "target_lang_sense_clarification": "alter or handle skillfully"
                            },
                            {
                                "target_lang_word": "handle",
                                "target_lang_sense_clarification": "manage with hands"
                            }
                        ],
                        "ru": [
                            {

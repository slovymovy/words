            "senses": [
                {
                    "sense_id": "751af5cb-825e-32fd-abd1-b38e9eeebee8",
                    "sense_definition": "A surname originating as a patronymic, meaning \"son of Robin\".",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "name_person",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Robinson</w> has been our neighbor for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Робинсон</w> — наш сосед уже более двадцати лет."
                            }
                        },
                        {
                            "text": "The <w>Robinson</w> family is hosting a barbecue this weekend.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Робинсонов</w> устраивает барбекю в эти выходные."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Robinson Crusoe",
                        "Swiss Family Robinson",
                        "Mrs. Robinson"
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "originating as a patronymic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, происходящая от отчества и означающая «сын Робина»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Робинсон",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c321fee1-7825-3e16-8af9-4e0f0bd09147",
                    "sense_definition": "A male given name transferred from the surname.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "name_person",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "They named their son <w>Robinson</w> to honor his grandmother's maiden name.",
                            "target_lang_translations": {
                                "ru": "Они назвали сына <w>Робинсоном</w> в честь девичьей фамилии бабушки."
                            }
                        },
                        {
                            "text": "<w>Robinson</w> Jeffers was a well-known American poet.",
                            "target_lang_translations": {
                                "ru": "<w>Робинсон</w> Джефферс был известным американским поэтом."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя, образованное от фамилии."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "abab32b0-6d00-3655-9424-5f6a5d6b26ac",
                    "sense_definition": "Bieg na krótkim dystansie, w którym zawodnik biegnie z maksymalną możliwą prędkością.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sport_race",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Usain Bolt wygrał finałowy <w>sprint</w> na sto metrów z dużą przewagą.",
                            "target_lang_translations": {
                                "ru": "Усэйн Болт выиграл финальный <w>спринт</w> на сто метров с большим отрывом."
                            }
                        },
                        {
                            "text": "Trener kazał nam ćwiczyć <w>sprint</w>, aby poprawić naszą szybkość startową.",
                            "target_lang_translations": {
                                "ru": "Тренер велел нам тренировать <w>спринт</w>, чтобы улучшить нашу стартовую скорость."
                            }
                        }
                    ],
                    "antonyms": [
                        "maraton",
                        "długi dystans"
                    ],
                    "common_phrases": [
                        "sprint na sto metrów",
                        "finałowy sprint",
                        "krótki sprint"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бег на короткую дистанцию, в котором спортсмен бежит с максимальной возможной скоростью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спринт",
                                "target_lang_sense_clarification": "спорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b68414c9-e9db-3a0d-80a8-0fb4e85ab910",
                    "sense_definition": "Bardzo szybki bieg lub gwałtowny zryw, zwykle na krótkim odcinku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sport_race",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodziej rzucił się w <w>sprint</w>, gdy tylko zobaczył nadchodzący radiowóz.",
                            "target_lang_translations": {
                                "ru": "Вор сделал резкий <w>рывок</w>, как только увидел приближающуюся патрульную машину."
                            }
                        },
                        {
                            "text": "Mój poranny <w>sprint</w> do autobusu zakończył się sukcesem i zdążyłem do pracy.",
                            "target_lang_translations": {
                                "ru": "Мой утренний <w>спринт</w> до автобуса завершился успехом, и я успел на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zryw",
                        "pęd"
                    ],
                    "antonyms": [
                        "trucht",
                        "spacer"
                    ],
                    "common_phrases": [
                        "rzucić się w sprint",
                        "ostry sprint"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень быстрый бег или резкий рывок, обычно на коротком отрезке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рывок",
                                "target_lang_sense_clarification": "резкое движение"
                            },
                            {
                                "target_lang_word": "спринт",
                                "target_lang_sense_clarification": "быстрый бег"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "069a8aa3-c8de-3850-8b71-4155e8629628",
                    "sense_definition": "W metodykach zwinnych (np. Scrum) krótki, określony przedział czasu przeznaczony na wykonanie zaplanowanej części pracy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz zespół programistyczny właśnie rozpoczął dwutygodniowy <w>sprint</w> nad nową funkcją aplikacji.",
                            "target_lang_translations": {
                                "ru": "Наша команда разработчиков только что начала двухнедельный <w>спринт</w> по работе над новой функцией приложения."
                            }
                        },
                        {
                            "text": "Podczas spotkania podsumowującego omówiliśmy wszystkie zadania ukończone w tym <w>sprincie</w>.",
                            "target_lang_translations": {
                                "ru": "На итоговом совещании мы обсудили все задачи, выполненные в этом <w>спринте</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "iteracja"
                    ],
                    "common_phrases": [
                        "planowanie sprintu",
                        "cel sprintu",
                        "koniec sprintu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "branżowe (IT, zarządzanie)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В гибких методологиях (например, Scrum) короткий, определенный интервал времени, отведенный на выполнение запланированной части работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спринт",
                                "target_lang_sense_clarification": "IT, управление проектами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdee8205-2a08-3018-9420-cc3e1c3c8181",
                    "sense_definition": "Ucieczka z lekcji szkolnych bez usprawiedliwienia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała klasa umówiła się na <w>sprint</w> z ostatniej lekcji matematyki.",
                            "target_lang_translations": {
                                "ru": "Весь класс договорился совершить <w>побег</w> с последнего урока математики."
                            }
                        },
                        {
                            "text": "Dyrektor wezwał rodziców, ponieważ ich syn zorganizował grupowy <w>sprint</w> ze szkoły.",
                            "target_lang_translations": {
                                "ru": "Директор вызвал родителей, так как их сын организовал групповой <w>побег</w> из школы."
                            }
                        }
                    ],
                    "synonyms": [

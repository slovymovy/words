{
    "word_family": [
        "synchronize",
        "synchronization",
        "synchronous",
        "asynchronous",
        "synchrony",
        "synchronizer"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "21d8fb1e-14f7-3311-bd81-c9291daed820",
                    "sense_definition": "To cause two or more things to happen at the same time, or to update data across different devices so they contain the same information.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_sync",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>sync</w> my calendar with my phone so I don't miss any appointments.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>синхронизировать</w> свой календарь с телефоном, чтобы не пропустить ни одной встречи."
                            }
                        },
                        {
                            "text": "The dancers <w>synced</w> their movements perfectly to the rhythm of the music.",
                            "target_lang_translations": {
                                "ru": "Танцоры идеально <w>синхронизировали</w> свои движения с ритмом музыки."
                            }
                        }
                    ],
                    "synonyms": [
                        "synchronize",
                        "coordinate",
                        "match"
                    ],
                    "antonyms": [
                        "desynchronize"
                    ],
                    "common_phrases": [
                        "sync up",
                        "sync with",
                        "auto sync"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить два или более события происходить одновременно или обновить данные на разных устройствах, чтобы они содержали одинаковую информацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "синхронизировать",
                                "target_lang_sense_clarification": "обновлять данные или согласовывать действия"
                            },
                            {
                                "target_lang_word": "согласовывать",
                                "target_lang_sense_clarification": "приводить в соответствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06c367b3-a3f3-30d6-82b6-fadaaabcbb95",
                    "sense_definition": "To force data held in temporary memory to be written to a permanent storage disk to prevent data loss.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The system administrator manually <w>synced</w> the drives before rebooting the server.",
                            "target_lang_translations": {
                                "ru": "Системный администратор вручную <w>синхронизировал</w> диски перед перезагрузкой сервера."
                            }
                        },
                        {
                            "text": "It is important to <w>sync</w> pending changes to the disk to ensure data integrity.",
                            "target_lang_translations": {
                                "ru": "Важно <w>синхронизировать</w> ожидающие изменения с диском, чтобы обеспечить целостность данных."
                            }
                        }
                    ],
                    "synonyms": [
                        "flush"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical jargon used in computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принудительно записать данные из временной памяти на постоянный диск для предотвращения потери данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "синхронизировать",
                                "target_lang_sense_clarification": "записывать буфер на диск"
                            },
                            {
                                "target_lang_word": "сбрасывать",
                                "target_lang_sense_clarification": "технический жаргон"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "132ee82f-abf1-3321-833f-4a6f0c899563",
                    "sense_definition": "The state of working together, happening at the same time, or matching exactly; a short form of <w>synchronization</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_sync",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The audio is out of <w>sync</w> with the video, which makes the movie hard to watch.",
                            "target_lang_translations": {
                                "ru": "Звук идет в <w>рассинхрон</w> с видео, из-за чего фильм трудно смотреть."
                            }
                        },
                        {
                            "text": "We need to work in <w>sync</w> to finish this project before the deadline.",
                            "target_lang_translations": {
                                "ru": "Нам нужно работать <w>синхронно</w>, чтобы закончить этот проект до дедлайна."
                            }
                        }
                    ],
                    "synonyms": [
                        "synchronization",
                        "harmony",
                        "accord"
                    ],
                    "common_phrases": [
                        "in sync",
                        "out of sync",
                        "lip sync"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "clipping of synchronization"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние совместной работы, одновременного действия или точного совпадения; сокращение от «синхронизация»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "синхронизация",
                                "target_lang_sense_clarification": "состояние согласованности"
                            },
                            {
                                "target_lang_word": "синхрон",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "268bfd1d-d355-3c49-95d8-28d95a563d2e",
                    "sense_definition": "A license that allows a piece of music to be synchronized with visual media, such as a film, TV show, or advertisement.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band earned a significant amount of money from a <w>sync</w> in a popular commercial.",
                            "target_lang_translations": {
                                "ru": "Группа заработала значительную сумму на <w>синхронизации</w> в популярной рекламе."
                            }
                        },
                        {
                            "text": "Securing a <w>sync</w> deal can be a major breakthrough for independent artists.",
                            "target_lang_translations": {
                                "ru": "Заключение сделки по <w>синхронизации</w> может стать большим прорывом для независимых артистов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sync license",
                        "sync deal"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "industry term in music business"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицензия, позволяющая синхронизировать музыкальное произведение с визуальным медиа, например, фильмом, телешоу или рекламой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "синхронизация",
                                "target_lang_sense_clarification": "лицензия на использование музыки"
                            },
                            {
                                "target_lang_word": "синк",
                                "target_lang_sense_clarification": "профессиональный сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
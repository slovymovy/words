                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угольная мелочь",
                                "target_lang_sense_clarification": "вид топлива"
                            },
                            {
                                "target_lang_word": "штыб",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "367cb77a-0b7e-3841-8b34-e3d35055df32",
                    "sense_definition": "In a way that is unusual, unexpected, or difficult to understand.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Daniel was walking <w>strangely</w> because he had twisted his ankle.",
                            "target_lang_translations": {
                                "ru": "Дэниел шёл <w>странно</w>, потому что подвернул лодыжку."
                            }
                        },
                        {
                            "text": "Though I had never been to the city before, everything looked <w>strangely</w> familiar.",
                            "target_lang_translations": {
                                "ru": "Хотя я никогда раньше не был в этом городе, всё выглядело <w>странно</w> знакомым."
                            }
                        }
                    ],
                    "synonyms": [
                        "oddly",
                        "weirdly",
                        "peculiarly",
                        "uncannily",
                        "curiously",
                        "eccentrically"
                    ],
                    "antonyms": [
                        "normally",
                        "mundanely"
                    ],
                    "common_phrases": [
                        "strangely familiar",
                        "acting strangely",
                        "behave strangely",
                        "strangely quiet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Необычным, неожиданным или непонятным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "странно",
                                "target_lang_sense_clarification": "необычно, удивительно"
                            },
                            {
                                "target_lang_word": "необычно",
                                "target_lang_sense_clarification": "не так, как принято"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27550ba9-6d68-3331-a923-fd303354b7d3",
                    "sense_definition": "Used to express that a fact or situation is surprising or unexpected.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Strangely</w>, the dog did not bark when the visitor entered the house.",
                            "target_lang_translations": {
                                "ru": "<w>Странно</w>, но собака не залаяла, когда гость вошёл в дом."
                            }
                        },
                        {
                            "text": "<w>Strangely</w> enough, we both decided to wear the same color today.",
                            "target_lang_translations": {
                                "ru": "<w>Как ни странно</w>, мы оба решили надеть сегодня одежду одного цвета."
                            }
                        }
                    ],
                    "synonyms": [
                        "oddly",
                        "curiously"
                    ],
                    "common_phrases": [
                        "strangely enough"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения удивления по поводу какого-либо факта или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "странно",
                                "target_lang_sense_clarification": "вводное слово, выражающее удивление"
                            },
                            {
                                "target_lang_word": "как ни странно",
                                "target_lang_sense_clarification": "удивительно, но факт"
                            }
                        ]
                    }
                }
            ]
        }

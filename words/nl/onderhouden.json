                        "voortzetten"
                    ],
                    "antonyms": [
                        "verbreken",
                        "beëindigen"
                    ],
                    "common_phrases": [
                        "contact onderhouden",
                        "relatie onderhouden",
                        "vriendschap onderhouden"

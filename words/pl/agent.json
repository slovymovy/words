    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4da8de26-25e0-3322-964b-bc2229257f31",
                    "sense_definition": "Pracownik wywiadu, służb specjalnych lub policji, zajmujący się zdobywaniem tajnych informacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W filmie główny bohater jest tajnym <w>agentem</w>, który ratuje świat przed zagładą.",
                            "target_lang_translations": {
                                "ru": "В фильме главный герой является тайным <w>агентом</w>, который спасает мир от гибели."
                            }
                        },
                        {
                            "text": "Został zdemaskowany jako wrogi <w>agent</w> obcego wywiadu.",
                            "target_lang_translations": {
                                "ru": "Он был разоблачен как вражеский <w>агент</w> иностранной разведки."
                            }
                        }
                    ],
                    "synonyms": [
                        "szpieg",
                        "wywiadowca",
                        "tajniak"
                    ],
                    "common_phrases": [
                        "tajny agent",
                        "agent wywiadu",
                        "podwójny agent",
                        "agent specjalny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сотрудник разведки, спецслужб или полиции, занимающийся сбором секретной информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "агент",
                                "target_lang_sense_clarification": "разведки / спецслужб"
                            },
                            {
                                "target_lang_word": "шпион",
                                "target_lang_sense_clarification": "разведчик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "987c5c61-c693-31e7-9a9d-32af9b01d3d8",
                    "sense_definition": "Osoba lub firma działająca w imieniu innej firmy lub osoby, pośrednicząca w transakcjach handlowych lub usługowych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>agent</w> ubezpieczeniowy przygotował dla mnie bardzo korzystną ofertę polisy.",
                            "target_lang_translations": {
                                "ru": "Мой страховой <w>агент</w> подготовил для меня очень выгодное предложение по полису."
                            }
                        },
                        {
                            "text": "Szukamy nowego mieszkania, więc umówiliśmy się na spotkanie z <w>agentem</w> nieruchomości.",
                            "target_lang_translations": {
                                "ru": "Мы ищем новую квартиру, поэтому договорились о встрече с <w>агентом</w> по недвижимости."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedstawiciel",
                        "ajent"
                    ],
                    "common_phrases": [
                        "agent ubezpieczeniowy",
                        "agent nieruchomości",
                        "agent literacki",
                        "agent handlowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или фирма, действующая от имени другой фирмы или лица, посредник в торговых или сервисных сделках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "агент",
                                "target_lang_sense_clarification": "представитель / посредник"
                            },
                            {
                                "target_lang_word": "представитель",
                                "target_lang_sense_clarification": "уполномоченное лицо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5606e965-cda6-39d9-9d31-99b937f3d76b",
                    "sense_definition": "Osoba zachowująca się w sposób zaskakujący, zabawny, sprytny lub nietypowy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ale z ciebie <w>agent</w>, znowu wymyśliłeś coś szalonego!",
                            "target_lang_translations": {
                                "ru": "Ну ты и <w>кадр</w>, опять придумал что-то безумное!"
                            }
                        },
                        {
                            "text": "Ten nowy pracownik to niezły <w>agent</w>, wszyscy się z niego śmieją.",
                            "target_lang_translations": {
                                "ru": "Этот новый сотрудник — тот еще <w>артист</w>, все над ним смеются."
                            }
                        }
                    ],
                    "common_phrases": [
                        "niezły agent"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane potocznie do określenia kogoś o specyficznym charakterze"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, ведущий себя удивительным, забавным, хитрым или необычным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кадр",
                                "target_lang_sense_clarification": "разг. о странном человеке"
                            },
                            {
                                "target_lang_word": "артист",
                                "target_lang_sense_clarification": "разг. ирон. о человеке"
                            },
                            {
                                "target_lang_word": "тип",
                                "target_lang_sense_clarification": "разг. о человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f3f45b5-9f45-3877-af39-1692dbb29c44",
                    "sense_definition": "Program komputerowy działający autonomicznie w celu wykonania określonego zadania w imieniu użytkownika lub innego programu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Inteligentny <w>agent</w> oprogramowania automatycznie sortuje moją pocztę elektroniczną.",
                            "target_lang_translations": {
                                "ru": "Интеллектуальный программный <w>агент</w> автоматически сортирует мою электронную почту."
                            }
                        },
                        {
                            "text": "System wykorzystuje wiele <w>agentów</w> do zarządzania ruchem w sieci komputerowej.",
                            "target_lang_translations": {
                                "ru": "Система использует множество <w>агентов</w> для управления трафиком в компьютерной сети."
                            }
                        }
                    ],
                    "common_phrases": [
                        "agent inteligentny",
                        "agent mobilny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерная программа, действующая автономно для выполнения определенной задачи от имени пользователя или другой программы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "агент",
                                "target_lang_sense_clarification": "программа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c241d94a-d36c-3639-8186-f53e6a7fcd1f",
                    "sense_definition": "Czynnik chemiczny, biologiczny lub fizyczny wywołujący określoną reakcję lub proces.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten wirus to niebezpieczny <w>agent</w> biologiczny, który szybko się rozprzestrzenia.",
                            "target_lang_translations": {
                                "ru": "Этот вирус — опасный биологический <w>агент</w>, который быстро распространяется."
                            }
                        },
                        {
                            "text": "W laboratorium badano wpływ nowego <w>agenta</w> chemicznego na strukturę metalu.",
                            "target_lang_translations": {
                                "ru": "В лаборатории исследовали влияние нового химического <w>агента</w> на структуру металла."
                            }
                        }
                    ],
                    "synonyms": [
                        "czynnik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Химический, биологический или физический фактор, вызывающий определенную реакцию или процесс."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "агент",
                                "target_lang_sense_clarification": "вещество / фактор"
                            },
                            {
                                "target_lang_word": "фактор",
                                "target_lang_sense_clarification": "причина процесса"
                            }
                        ]
                    }
                }
            ]
        }

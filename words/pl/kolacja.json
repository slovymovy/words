                            },
                            {
                                "target_lang_word": "сличение",
                                "target_lang_sense_clarification": "сравнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85cbec82-7842-3c7e-9319-17520f98be3b",

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "08d80905-516e-3912-a1c3-2359194a5876",
                    "sense_definition": "Te sterk afgekoeld, meestal verwijzend naar een lichaamstemperatuur die gevaarlijk laag is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_cold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zwemmer raakte ernstig <w>onderkoeld</w> in het ijskoude water.",
                            "target_lang_translations": {
                                "ru": "Пловец получил сильное <w>переохлаждение</w> в ледяной воде.",
                                "en": "The swimmer became severely <w>hypothermic</w> in the ice-cold water."
                            }
                        },
                        {
                            "text": "Zonder beschermende kleding kun je in de bergen snel <w>onderkoeld</w> raken.",
                            "target_lang_translations": {
                                "ru": "Без защитной одежды в горах можно быстро <w>переохладиться</w>.",
                                "en": "Without protective clothing, you can quickly become <w>hypothermic</w> in the mountains."
                            }
                        }
                    ],
                    "synonyms": [
                        "hypotherm"
                    ],
                    "antonyms": [
                        "oververhit"
                    ],
                    "common_phrases": [
                        "onderkoeld raken",
                        "zwaar onderkoeld"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переохлажденный; состояние, при котором температура тела падает до опасно низкого уровня.",
                        "en": "Excessively cooled, usually referring to a dangerously low body temperature."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переохлаждённый",
                                "target_lang_sense_clarification": "о температуре тела"
                            },
                            {
                                "target_lang_word": "получивший переохлаждение",
                                "target_lang_sense_clarification": "медицинское состояние"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "hypothermic",
                                "target_lang_sense_clarification": "medical condition"
                            },
                            {
                                "target_lang_word": "suffering from hypothermia",
                                "target_lang_sense_clarification": "state of health"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c2dbd82-738a-3e2a-8b1a-296bec67c7bc",
                    "sense_definition": "Zonder zichtbare emotie; koel, zakelijk of droog in gedrag of uiting.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_cold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>onderkoelde</w> humor wordt niet door iedereen begrepen.",
                            "target_lang_translations": {
                                "ru": "Его <w>сухой</w> юмор понимают не все.",
                                "en": "His <w>deadpan</w> humor is not understood by everyone."
                            }
                        },
                        {
                            "text": "De politicus gaf een <w>onderkoelde</w> reactie op de beschuldigingen.",
                            "target_lang_translations": {
                                "ru": "Политик дал <w>сдержанный</w> ответ на обвинения.",
                                "en": "The politician gave a <w>cool</w> reaction to the allegations."
                            }
                        }
                    ],
                    "synonyms": [
                        "koel",
                        "afstandelijk",
                        "onbewogen",
                        "nuchter"
                    ],
                    "antonyms": [
                        "emotioneel",
                        "gepassioneerd",
                        "uitbundig"
                    ],
                    "common_phrases": [
                        "onderkoelde humor",
                        "een onderkoelde reactie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без видимых эмоций; сдержанный, сухой или холодный в поведении или высказываниях.",
                        "en": "Without visible emotion; cool, businesslike, or dry in behavior or expression."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдержанный",
                                "target_lang_sense_clarification": "об эмоциях или реакции"
                            },
                            {
                                "target_lang_word": "сухой",
                                "target_lang_sense_clarification": "о юморе или тоне"
                            },
                            {
                                "target_lang_word": "холодный",
                                "target_lang_sense_clarification": "об отношении"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "deadpan",
                                "target_lang_sense_clarification": "regarding humor or delivery"
                            },
                            {
                                "target_lang_word": "dry",
                                "target_lang_sense_clarification": "regarding wit or tone"
                            },
                            {
                                "target_lang_word": "cool",
                                "target_lang_sense_clarification": "regarding attitude or reaction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03011961-e232-3e19-ab68-4a188ab88b64",
                    "sense_definition": "Van een vloeistof: afgekoeld tot onder het vriespunt zonder in vaste vorm over te gaan.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "scientific_cold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Onderkoeld</w> water kan plotseling bevriezen als het in contact komt met ijs.",
                            "target_lang_translations": {
                                "ru": "<w>Переохлаждённая</w> вода может внезапно замерзнуть при контакте со льдом.",
                                "en": "<w>Supercooled</w> water can suddenly freeze if it comes into contact with ice."
                            }
                        },
                        {
                            "text": "In het laboratorium werken we met een <w>onderkoelde</w> vloeistof.",
                            "target_lang_translations": {
                                "ru": "В лаборатории мы работаем с <w>переохлаждённой</w> жидкостью.",
                                "en": "In the laboratory, we are working with a <w>supercooled</w> liquid."
                            }
                        }
                    ],
                    "common_phrases": [
                        "onderkoelde vloeistof",
                        "onderkoelde regen"
                    ],
                    "target_lang_definitions": {
                        "ru": "О жидкости: охлажденный ниже точки замерзания, но остающийся в жидком состоянии.",
                        "en": "Of a liquid: cooled below the freezing point without turning into a solid."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переохлаждённый",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "supercooled",
                                "target_lang_sense_clarification": "physics/chemistry term"
                            }
                        ]
                    }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "42610e77-5a36-3d5a-bb63-eb3f32d3ca08",
                    "sense_definition": "Светло-синий цвет, оттенок голубого (обычно о цвете неба или моря).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "color_shade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Над нашими головами сияла чистая небесная <w>лазурь</w>.",
                            "target_lang_translations": {
                                "en": "Pure heavenly <w>azure</w> shone above our heads."
                            }
                        },
                        {
                            "text": "В ясный солнечный день морская <w>лазурь</w> сливается с горизонтом.",
                            "target_lang_translations": {
                                "en": "On a clear sunny day, the marine <w>azure</w> merges with the horizon."
                            }
                        }
                    ],
                    "synonyms": [
                        "синева",
                        "голубизна"
                    ],
                    "common_phrases": [
                        "небесная лазурь",
                        "морская лазурь"
                    ],
                    "target_lang_definitions": {
                        "en": "A light blue color, a shade of blue (usually referring to the color of the sky or sea)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "azure",
                                "target_lang_sense_clarification": "color of the sky or sea"
                            },
                            {
                                "target_lang_word": "blue",
                                "target_lang_sense_clarification": "poetic reference to the sky or sea"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b475ecd-028a-344d-841c-1f56a31d09bb",
                    "sense_definition": "Краска или пигмент светло-синего цвета.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "pigment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Художник выдавил на палитру немного <w>лазури</w> для написания неба.",
                            "target_lang_translations": {
                                "en": "The artist squeezed a little <w>azure</w> onto the palette to paint the sky."
                            }
                        },
                        {
                            "text": "Берлинская <w>лазурь</w> используется в живописи и печати.",
                            "target_lang_translations": {
                                "en": "Prussian <w>blue</w> is used in painting and printing."
                            }
                        }
                    ],
                    "common_phrases": [
                        "берлинская лазурь"
                    ],
                    "target_lang_definitions": {
                        "en": "A paint or pigment of a light blue color."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "azure",
                                "target_lang_sense_clarification": "pigment name"
                            },
                            {
                                "target_lang_word": "blue",
                                "target_lang_sense_clarification": "in specific compounds like Prussian blue"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3ba20233-14ab-3e29-990d-05a8c9ad70d2",
                    "sense_definition": "Żywe stworzenie, które nie jest człowiekiem ani rośliną, zazwyczaj posiadające zdolność poruszania się i odczuwania",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moje ulubione <w>zwierzę</w> to pies, ponieważ jest wiernym przyjacielem.",
                            "target_lang_translations": {
                                "ru": "Моё любимое <w>животное</w> — это собака, потому что она верный друг."
                            }
                        },
                        {
                            "text": "W tym lesie można spotkać niejedno dzikie <w>zwierzę</w>.",
                            "target_lang_translations": {
                                "ru": "В этом лесу можно встретить не одно дикое <w>животное</w>."
                            }
                        },
                        {
                            "text": "Każde <w>zwierzę</w> domowe wymaga opieki i odpowiedzialności ze strony właściciela.",
                            "target_lang_translations": {
                                "ru": "Каждое домашнее <w>животное</w> требует заботы и ответственности со стороны владельца."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwierzak",
                        "zwierz"
                    ],
                    "antonyms": [
                        "człowiek",
                        "roślina"
                    ],
                    "common_phrases": [
                        "dzikie zwierzę",
                        "zwierzę domowe",
                        "zwierzę hodowlane",
                        "świat zwierząt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Живое существо, не являющееся ни человеком, ни растением, обычно обладающее способностью двигаться и чувствовать"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "животное",
                                "target_lang_sense_clarification": "общее название"
                            },
                            {
                                "target_lang_word": "зверь",
                                "target_lang_sense_clarification": "дикое животное, млекопитающее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ede85d7f-51b6-318d-8153-17cbf73752d1",
                    "sense_definition": "Człowiek zachowujący się w sposób brutalny, prymitywny, agresywny lub pozbawiony kultury",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "animal_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy wpadał w szał, zmieniał się w dzikie <w>zwierzę</w>.",
                            "target_lang_translations": {
                                "ru": "Когда он впадал в ярость, он превращался в дикого <w>зверя</w>."
                            }
                        },
                        {
                            "text": "Nie bądź taki agresywny, nie zachowuj się jak <w>zwierzę</w>!",
                            "target_lang_translations": {
                                "ru": "Не будь таким агрессивным, не веди себя как <w>животное</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "zwierz"
                    ],
                    "common_phrases": [
                        "zachowywać się jak zwierzę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, ведущий себя грубо, примитивно, агрессивно или бескультурно"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "животное",
                                "target_lang_sense_clarification": "о грубом, бескультурном человеке"
                            },
                            {
                                "target_lang_word": "зверь",
                                "target_lang_sense_clarification": "о жестоком, свирепом человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25270be3-6709-3cd6-a9f9-9d795a17d56f",
                    "sense_definition": "Wielokomórkowy organizm cudzożywny; w systematyce biologicznej kategoria obejmująca także ludzi",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z biologicznego punktu widzenia człowiek to także <w>zwierzę</w>, należące do ssaków.",
                            "target_lang_translations": {
                                "ru": "С биологической точки зрения человек — это тоже <w>животное</w>, относящееся к млекопитающим."
                            }
                        },
                        {
                            "text": "Gąbki to prymitywne <w>zwierzęta</w> żyjące w środowisku wodnym.",
                            "target_lang_translations": {
                                "ru": "Губки — это примитивные <w>животные</w>, обитающие в водной среде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "królestwo zwierząt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Многоклеточный гетеротрофный организм; в биологической систематике категория, включающая также людей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "животное",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {

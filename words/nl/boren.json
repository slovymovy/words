    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a3cdd679-8a8a-39aa-ad78-68faecb39b7f",
                    "sense_definition": "Met een draaiend werktuig een rond gat in materiaal maken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "drill_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>boorde</w> een diep gat in de muur voor de plug.",
                            "target_lang_translations": {
                                "ru": "Он <w>просверлил</w> глубокое отверстие в стене под дюбель.",
                                "en": "He <w>drilled</w> a deep hole in the wall for the wall plug."
                            }
                        },
                        {
                            "text": "Het grote bedrijf <w>boort</w> naar olie in de oceaan.",
                            "target_lang_translations": {
                                "ru": "Крупная компания <w>бурит</w> дно океана в поисках нефти.",
                                "en": "The large company <w>drills</w> for oil in the ocean."
                            }
                        }
                    ],
                    "antonyms": [
                        "dichten"
                    ],
                    "common_phrases": [
                        "een gat boren",
                        "naar olie boren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать круглое отверстие в материале при помощи вращающегося инструмента.",
                        "en": "To make a round hole in material using a rotating tool."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сверлить",
                                "target_lang_sense_clarification": "делать отверстия"
                            },
                            {
                                "target_lang_word": "бурить",
                                "target_lang_sense_clarification": "скважины, землю"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "drill",
                                "target_lang_sense_clarification": "make a hole"
                            },
                            {
                                "target_lang_word": "bore",
                                "target_lang_sense_clarification": "make a hole or tunnel"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4680fc43-ce01-3210-8127-814f9b8e49a1",
                    "sense_definition": "Iemand of iets zeer indringend en strak aankijken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "stare_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Haar felle blik <w>boorde</w> zich in de ogen van de leugenaar.",
                            "target_lang_translations": {
                                "ru": "Её яростный взгляд <w>впился</w> в глаза лжеца.",
                                "en": "Her fierce gaze <w>bored</w> into the eyes of the liar."
                            }
                        },
                        {
                            "text": "De rechercheur <w>boorde</w> zijn ogen in de verdachte tijdens het verhoor.",
                            "target_lang_translations": {
                                "ru": "Следователь <w>сверлил</w> подозреваемого взглядом во время допроса.",
                                "en": "The detective <w>bored</w> his eyes into the suspect during the interrogation."
                            }
                        }
                    ],
                    "synonyms": [
                        "priemen"
                    ],
                    "common_phrases": [
                        "zich boren in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смотреть на кого-либо или что-либо очень пристально и пронзительно.",
                        "en": "To look at someone or something very penetratingly and intensely."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сверлить",
                                "target_lang_sense_clarification": "взглядом"
                            },
                            {
                                "target_lang_word": "пронзать",
                                "target_lang_sense_clarification": "взглядом"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "bore",
                                "target_lang_sense_clarification": "with one's eyes"
                            },
                            {
                                "target_lang_word": "pierce",
                                "target_lang_sense_clarification": "with a look"
                            }
                        ]
                    }
                }
            ]
        },
        {

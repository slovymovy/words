{
    "word_family": [
        "masseur",
        "masseuse",
        "massager",
        "massagist"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9c228603-203a-3e80-8ddd-37096d393baf",
                    "sense_definition": "The action of rubbing, kneading, or hitting someone's body to help them relax or relieve pain.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "body_massage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She went to the spa for a relaxing <w>massage</w> after a stressful week.",
                            "target_lang_translations": {
                                "ru": "Она пошла в спа на расслабляющий <w>массаж</w> после напряженной недели."
                            }
                        },
                        {
                            "text": "The athlete needed a deep tissue <w>massage</w> to recover from the injury.",
                            "target_lang_translations": {
                                "ru": "Спортсмену потребовался глубокий <w>массаж</w> тканей для восстановления после травмы."
                            }
                        }
                    ],
                    "synonyms": [
                        "rubdown"
                    ],
                    "common_phrases": [
                        "full-body massage",
                        "back massage",
                        "foot massage",
                        "massage therapy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в растирании, разминании или поколачивании тела для расслабления или снятия боли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "масса́ж",
                                "target_lang_sense_clarification": "процедура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11bbb011-bc45-399d-a124-c3d8b549d11b",
                    "sense_definition": "The action of rubbing or kneading an object or substance to change its texture or shape.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object_massage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Give the dough a gentle <w>massage</w> before you let it rise.",
                            "target_lang_translations": {
                                "ru": "Слегка <w>помните</w> тесто, прежде чем дать ему подняться."
                            }
                        },
                        {
                            "text": "A regular <w>massage</w> with oil keeps the leather soft and supple.",
                            "target_lang_translations": {
                                "ru": "Регулярное <w>втирание</w> масла сохраняет кожу мягкой и эластичной."
                            }
                        }
                    ],
                    "synonyms": [
                        "kneading",
                        "rubbing"
                    ],
                    "common_phrases": [
                        "gentle massage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растирание или разминание объекта или вещества для изменения его текстуры или формы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размина́ние",
                                "target_lang_sense_clarification": "теста, глины"
                            },
                            {
                                "target_lang_word": "втира́ние",
                                "target_lang_sense_clarification": "масла, крема"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "37d7d03e-c420-3ec7-90d9-8dc7dc2ff620",
                    "sense_definition": "To rub and knead a person's body or body part to relieve pain or tension.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "body_massage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He gently <w>massaged</w> her shoulders to help her relax.",
                            "target_lang_translations": {
                                "ru": "Он нежно <w>помассировал</w> ей плечи, чтобы помочь ей расслабиться."
                            }
                        },
                        {
                            "text": "The therapist <w>massaged</w> the runner's legs after the marathon.",
                            "target_lang_translations": {
                                "ru": "Терапевт <w>массировал</w> ноги бегуна после марафона."
                            }
                        }
                    ],
                    "synonyms": [
                        "rub down",
                        "knead"
                    ],
                    "common_phrases": [
                        "massage the back",
                        "massage the neck",
                        "massage gently"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растирать и разминать тело человека или его часть для снятия боли или напряжения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "масси́ровать",
                                "target_lang_sense_clarification": "делать массаж"
                            },
                            {
                                "target_lang_word": "де́лать масса́ж",
                                "target_lang_sense_clarification": "выполнять процедуру"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d08a4d77-2aba-3c13-baf6-facd0692b343",
                    "sense_definition": "To rub or knead a substance or object, often to soften it.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object_massage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>massage</w> the kale with olive oil to make it tender.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>помять</w> капусту кейл с оливковым маслом, чтобы она стала мягкой."
                            }
                        },
                        {
                            "text": "She <w>massaged</w> the conditioner into her hair for better absorption.",
                            "target_lang_translations": {
                                "ru": "Она <w>втерла</w> кондиционер в волосы для лучшего впитывания."
                            }
                        }
                    ],
                    "synonyms": [
                        "knead",
                        "rub"
                    ],
                    "common_phrases": [
                        "massage the scalp",
                        "massage the mixture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растирать или разминать вещество или объект, часто чтобы размягчить его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размина́ть",
                                "target_lang_sense_clarification": "делать мягким"
                            },
                            {
                                "target_lang_word": "втира́ть",
                                "target_lang_sense_clarification": "крем, масло"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "447e118b-cc51-3afa-a9d1-600e47762123",
                    "sense_definition": "To manipulate data or figures to make them appear more favorable or acceptable.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accountant tried to <w>massage</w> the numbers to hide the company's losses.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер пытался <w>подтасовать</w> цифры, чтобы скрыть убытки компании."
                            }
                        },
                        {
                            "text": "They decided to <w>massage</w> the report to make the results look more promising.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>приукрасить</w> отчет, чтобы результаты выглядели более многообещающими."
                            }
                        }
                    ],
                    "synonyms": [
                        "manipulate",
                        "doctor",
                        "alter"
                    ],
                    "common_phrases": [
                        "massage the figures",
                        "massage the data",
                        "massage the numbers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Манипулировать данными или цифрами, чтобы представить их в более выгодном свете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подтасо́вывать",
                                "target_lang_sense_clarification": "факты, цифры"
                            },
                            {
                                "target_lang_word": "манипули́ровать",
                                "target_lang_sense_clarification": "данными"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
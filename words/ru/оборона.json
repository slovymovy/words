    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7e54ef37-0654-3e02-845e-e056f7be1158",
                    "sense_definition": "Действие по значению глагола <w>оборонять</w>; защита своей территории или позиций от нападения противника.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Героическая <оборона> крепости вошла в учебники истории.",
                            "target_lang_translations": {
                                "en": "The heroic <defense> of the fortress went down in history books."
                            }
                        },
                        {
                            "text": "Министерство обороны отвечает за безопасность государственных границ.",
                            "target_lang_translations": {
                                "en": "The Ministry of <Defense> is responsible for the security of state borders."
                            }
                        },
                        {
                            "text": "Оборона Ленинграда продолжалась почти девятьсот дней.",
                            "target_lang_translations": {
                                "en": "The <Defense> of Leningrad lasted almost nine hundred days."
                            }
                        }
                    ],
                    "synonyms": [
                        "защита"
                    ],
                    "antonyms": [
                        "нападение",
                        "агрессия"
                    ],
                    "common_phrases": [
                        "держать оборону",
                        "линия обороны",
                        "министерство обороны",
                        "занять оборону"
                    ],
                    "target_lang_definitions": {
                        "en": "The action of defending one's territory or positions from an enemy attack."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "defense",
                                "target_lang_sense_clarification": "action of defending against attack"
                            },
                            {
                                "target_lang_word": "protection",
                                "target_lang_sense_clarification": "general safety or security"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b566ae91-730e-382c-bef6-9415183d5853",
                    "sense_definition": "Совокупность военных средств, укреплений и войск для отпора врагу; в спорте — часть команды, защищающая свои ворота.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наши войска смогли прорвать <оборону> противника на южном фронте.",
                            "target_lang_translations": {
                                "en": "Our troops were able to break through the enemy <defense> on the southern front."
                            }
                        },
                        {
                            "text": "Футбольная команда соперника ушла в глухую <оборону>.",
                            "target_lang_translations": {
                                "en": "The opposing football team went into deep <defense>."
                            }
                        },
                        {
                            "text": "Португальцы старались дорожить мячом во время атаки и очень строго играли в <обороне>.",
                            "target_lang_translations": {
                                "en": "The Portuguese tried to hold onto the ball during the attack and played very strictly in <defense>."
                            }
                        }
                    ],
                    "synonyms": [
                        "защита"
                    ],
                    "antonyms": [
                        "нападение"
                    ],
                    "common_phrases": [
                        "прорвать оборону",
                        "играть в обороне",
                        "укреплять оборону",
                        "глухая оборона"
                    ],
                    "target_lang_definitions": {
                        "en": "Military fortifications and troops used to repel an enemy; in sports, the players protecting the goal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "defense",
                                "target_lang_sense_clarification": "military structures or sports position"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "ee16058c-79e8-3028-9614-d752b57ec7e6",
                    "sense_definition": "Een extra rol van een bepaald materiaal, zoals papier of tape, die bewaard wordt om een lege rol te vervangen.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kijk even in de kast of er nog een <w>reserverol</w> toiletpapier ligt.",
                            "target_lang_translations": {
                                "en": "Just check the cupboard to see if there is a <w>spare roll</w> of toilet paper left."
                            }
                        },
                        {
                            "text": "Het is handig om altijd een <w>reserverol</w> plakband in de lade te hebben.",
                            "target_lang_translations": {
                                "en": "It is handy to always have a <w>spare roll</w> of tape in the drawer."
                            }
                        }
                    ],
                    "common_phrases": [
                        "reserverol toiletpapier",
                        "een reserverol pakken"
                    ],
                    "target_lang_definitions": {
                        "en": "An extra roll of material, such as paper or tape, kept to replace an empty one."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spare roll",
                                "target_lang_sense_clarification": "replacement object"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18b2ec1c-be1d-3f5b-9ac9-3a63c49b5e7f",
                    "sense_definition": "De taak of positie van iemand die als vervanger dient en niet tot de vaste kern of basisopstelling behoort.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De doelman moest zich dit seizoen tevreden stellen met een <w>reserverol</w>.",
                            "target_lang_translations": {
                                "en": "The goalkeeper had to settle for a <w>reserve role</w> this season."
                            }
                        },
                        {
                            "text": "Hij vond het moeilijk om zijn <w>reserverol</w> te accepteren na jarenlang aanvoerder te zijn geweest.",
                            "target_lang_translations": {
                                "en": "He found it difficult to accept his <w>backup role</w> after having been captain for years."
                            }
                        }
                    ],
                    "synonyms": [
                        "reservefunctie"
                    ],
                    "antonyms": [
                        "basisplaats",
                        "hoofdrol"
                    ],
                    "common_phrases": [
                        "een reserverol vervullen",
                        "zich schikken in een reserverol"
                    ],
                    "target_lang_definitions": {
                        "en": "The position or task of someone who serves as a substitute and is not part of the core team or starting lineup."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reserve role",
                                "target_lang_sense_clarification": "position as a substitute"
                            },
                            {
                                "target_lang_word": "backup role",
                                "target_lang_sense_clarification": "secondary position"
                            }
                        ]
                    }
                }
            ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2ea5a231-cc0d-364b-a540-a27874db9353",
                    "sense_definition": "Het proces van rondgaan of circuleren binnen een groep of systeem, vaak gezegd van geld, geruchten of vloeistoffen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zijn valse bankbiljetten in <w>omloop</w> gebracht.",
                            "target_lang_translations": {
                                "en": "Counterfeit banknotes have been put into <w>circulation</w>.",
                                "ru": "Были выпущены в <w>обращение</w> фальшивые банкноты."
                            }
                        },
                        {
                            "text": "Het gerucht dat in <w>omloop</w> is, blijkt niet op waarheid gebaseerd.",
                            "target_lang_translations": {
                                "en": "The rumor currently in <w>circulation</w> turns out not to be based on truth.",
                                "ru": "Слух, который сейчас в <w>ходу</w>, оказывается, не основан на правде."
                            }
                        }
                    ],
                    "synonyms": [
                        "circulatie",
                        "roulatie"
                    ],
                    "common_phrases": [
                        "in omloop zijn",
                        "in omloop brengen"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of going around or circulating within a group or system, often said of money, rumors, or fluids.",
                        "ru": "Процесс циркуляции или распространения внутри группы или системы, часто относится к деньгам, слухам или жидкостям."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "circulation",
                                "target_lang_sense_clarification": "movement of money, information, or fluids"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обращение",
                                "target_lang_sense_clarification": "деньги, товары"
                            },
                            {
                                "target_lang_word": "циркуляция",
                                "target_lang_sense_clarification": "жидкости, воздух, общее значение"
                            },
                            {
                                "target_lang_word": "хождение",
                                "target_lang_sense_clarification": "слухи, деньги (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0604021d-8cc4-3460-b87a-a54f65db3d26",
                    "sense_definition": "Een wedstrijdparcours dat, vaak meerdere keren, wordt afgelegd, met name in de wielersport.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De renners vonden de <w>omloop</w> erg zwaar door de vele kasseien.",
                            "target_lang_translations": {
                                "en": "The riders found the <w>circuit</w> very tough due to the many cobblestones.",
                                "ru": "Гонщики сочли <w>трассу</w> очень тяжелой из-за большого количества брусчатки."
                            }
                        },
                        {
                            "text": "Langs de hele <w>omloop</w> stonden toeschouwers te juichen.",
                            "target_lang_translations": {
                                "en": "Spectators were cheering along the entire <w>course</w>.",
                                "ru": "Вдоль всей <w>трассы</w> стояли и ликовали зрители."
                            }
                        }
                    ],
                    "synonyms": [
                        "parcours",
                        "circuit",
                        "ronde"
                    ],
                    "common_phrases": [
                        "zware omloop"
                    ],
                    "target_lang_definitions": {
                        "en": "A race course or route that is covered, often multiple times, especially in cycling.",
                        "ru": "Гоночная трасса или маршрут соревнований, который часто проходят несколько раз, особенно в велоспорте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "circuit",
                                "target_lang_sense_clarification": "sports course"
                            },
                            {
                                "target_lang_word": "course",
                                "target_lang_sense_clarification": "route for a race"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "трасса",
                                "target_lang_sense_clarification": "спортивная"
                            },
                            {
                                "target_lang_word": "маршрут",
                                "target_lang_sense_clarification": "путь следования"
                            },
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "этап гонки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbce0c34-f28b-3cff-ad7f-4547e608d288",
                    "sense_definition": "De beweging of baan van een hemellichaam rondom een ander hemellichaam.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aarde maakt jaarlijks een volledige <w>omloop</w> om de zon.",
                            "target_lang_translations": {
                                "en": "The earth makes a full <w>orbit</w> around the sun every year.",
                                "ru": "Земля ежегодно совершает полный <w>оборот</w> вокруг Солнца."
                            }
                        },
                        {
                            "text": "De satelliet werd in een stabiele <w>omloop</w> gebracht.",
                            "target_lang_translations": {
                                "en": "The satellite was put into a stable <w>orbit</w>.",
                                "ru": "Спутник был выведен на стабильную <w>орбиту</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "omloopbaan",
                        "omwenteling",
                        "baan"
                    ],
                    "target_lang_definitions": {
                        "en": "The movement or path of a celestial body around another celestial body.",
                        "ru": "Движение или траектория небесного тела вокруг другого небесного тела."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "orbit",
                                "target_lang_sense_clarification": "astronomical path"
                            },
                            {
                                "target_lang_word": "revolution",
                                "target_lang_sense_clarification": "single complete turn"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "оборот",
                                "target_lang_sense_clarification": "полный круг вращения"
                            },
                            {
                                "target_lang_word": "орбита",
                                "target_lang_sense_clarification": "траектория движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06037e78-3416-38f7-a486-3f73cb28ad17",
                    "sense_definition": "Een rondlopende gang of galerij, bijvoorbeeld in een kerk, toren of theater.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vanaf de <w>omloop</w> van de kerktoren heb je een prachtig uitzicht over de stad.",
                            "target_lang_translations": {
                                "en": "From the <w>gallery</w> of the church tower, you have a beautiful view of the city.",
                                "ru": "С <w>галереи</w> церковной башни открывается прекрасный вид на город."
                            }
                        },
                        {
                            "text": "We liepen door de smalle <w>omloop</w> boven in het gebouw.",
                            "target_lang_translations": {
                                "en": "We walked through the narrow <w>walkway</w> at the top of the building.",
                                "ru": "Мы шли по узкому <w>обходу</w> наверху здания."
                            }
                        }
                    ],
                    "synonyms": [
                        "omgang",
                        "galerij"
                    ],
                    "target_lang_definitions": {
                        "en": "A circular corridor or gallery, for example in a church, tower, or theater.",
                        "ru": "Круговой проход или галерея, например, в церкви, башне или театре."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gallery",
                                "target_lang_sense_clarification": "walkway in a building/tower"
                            },
                            {
                                "target_lang_word": "walkway",
                                "target_lang_sense_clarification": "passage for walking"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "галерея",
                                "target_lang_sense_clarification": "архитектурная"
                            },
                            {
                                "target_lang_word": "обход",
                                "target_lang_sense_clarification": "проход вокруг чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0e73a76-76fb-36c2-8fe9-89413b440121",
                    "sense_definition": "Een pijnlijke ontsteking van de huid rondom de nagel.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dokter behandelde de pijnlijke <w>omloop</w> aan zijn duim.",
                            "target_lang_translations": {
                                "en": "The doctor treated the painful <w>whitlow</w> on his thumb.",
                                "ru": "Врач лечил болезненный <w>панариций</w> на его большом пальце."
                            }
                        },
                        {
                            "text": "Door het bijten van nagels kan er gemakkelijk een <w>omloop</w> ontstaan.",
                            "target_lang_translations": {
                                "en": "Nail biting can easily cause a <w>whitlow</w>.",
                                "ru": "Из-за кусания ногтей может легко возникнуть <w>панариций</w>."
                            }
                        }
                    ],
                    "synonyms": [

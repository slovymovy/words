{
    "word_family": [
        "beside"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "ccb3d4bd-f13b-32c2-9fd2-cc72c1126d75",
                    "sense_definition": "In addition to someone or something; as well as.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sem_addition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We have plenty of food <w>besides</w> the bread and cheese.",
                            "target_lang_translations": {
                                "ru": "У нас полно еды <w>кроме</w> хлеба и сыра."
                            }
                        },
                        {
                            "text": "<w>Besides</w> this, there are several other reasons to stay home.",
                            "target_lang_translations": {
                                "ru": "<w>Кроме</w> того, есть несколько других причин остаться дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "in addition to",
                        "on top of",
                        "beyond"
                    ],
                    "common_phrases": [
                        "besides this",
                        "besides that"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вдобавок к кому-либо или чему-либо; наряду с."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроме",
                                "target_lang_sense_clarification": "в значении «вдобавок к»"
                            },
                            {
                                "target_lang_word": "помимо",
                                "target_lang_sense_clarification": "наряду с чем-либо"
                            },
                            {
                                "target_lang_word": "сверх",
                                "target_lang_sense_clarification": "в дополнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "931a6e8b-08f9-3150-b199-0755bc339844",
                    "sense_definition": "Other than; except for.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sem_exception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I did not tell anyone <w>besides</w> my brother.",
                            "target_lang_translations": {
                                "ru": "Я не сказал никому, <w>кроме</w> моего брата."
                            }
                        },
                        {
                            "text": "There is nothing in the box <w>besides</w> old papers.",
                            "target_lang_translations": {
                                "ru": "В коробке нет ничего, <w>кроме</w> старых бумаг."
                            }
                        }
                    ],
                    "synonyms": [
                        "except",
                        "save for",
                        "barring"
                    ],
                    "common_phrases": [
                        "anyone besides",
                        "anything besides",
                        "nothing besides"
                    ],
                    "target_lang_definitions": {
                        "ru": "За исключением; не считая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроме",
                                "target_lang_sense_clarification": "за исключением"
                            },
                            {
                                "target_lang_word": "за исключением",
                                "target_lang_sense_clarification": "формальное"
                            },
                            {
                                "target_lang_word": "исключая",
                                "target_lang_sense_clarification": "не считая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68db9b0e-2c56-3426-bcea-870014b198ec",
                    "sense_definition": "Next to; at the side of.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sem_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They walked <w>besides</w> the river in the early morning.",
                            "target_lang_translations": {
                                "ru": "Они гуляли <w>возле</w> реки ранним утром."
                            }
                        },
                        {
                            "text": "He sat down <w>besides</w> his friend to rest.",
                            "target_lang_translations": {
                                "ru": "Он сел <w>рядом с</w> другом, чтобы отдохнуть."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "standard usage is 'beside'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рядом с; возле (устаревшее использование)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рядом с",
                                "target_lang_sense_clarification": "возле"
                            },
                            {
                                "target_lang_word": "возле",
                                "target_lang_sense_clarification": "около"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "57354830-a4c5-31a8-aace-5f2e6050b051",
                    "sense_definition": "Used to introduce an additional point that reinforces the previous one; moreover.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sem_addition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do not want to go out tonight; <w>besides</w>, it is raining heavily.",
                            "target_lang_translations": {
                                "ru": "Я не хочу никуда идти сегодня вечером; <w>к тому же</w>, идет сильный дождь."
                            }
                        },
                        {
                            "text": "It is too expensive, and <w>besides</w>, I do not really need it.",
                            "target_lang_translations": {
                                "ru": "Это слишком дорого, и <w>кроме того</w>, мне это не особо нужно."
                            }
                        }
                    ],
                    "synonyms": [
                        "in addition to",
                        "on top of",
                        "beyond"
                    ],
                    "common_phrases": [
                        "and besides"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения дополнительного довода; кроме того."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроме того",
                                "target_lang_sense_clarification": "вводное слово"
                            },
                            {
                                "target_lang_word": "к тому же",
                                "target_lang_sense_clarification": "усиливает предыдущий довод"
                            },
                            {
                                "target_lang_word": "более того",
                                "target_lang_sense_clarification": "для усиления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4408860c-5ab7-3d1f-a27c-1a02973a4ef9",
                    "sense_definition": "As well; in addition.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sem_addition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She gave me some advice and money <w>besides</w>.",
                            "target_lang_translations": {
                                "ru": "Она дала мне совет и денег <w>вдобавок</w>."
                            }
                        },
                        {
                            "text": "The house is large and beautiful, and cheap <w>besides</w>.",
                            "target_lang_translations": {
                                "ru": "Дом большой и красивый, и <w>к тому же</w> дешевый."
                            }
                        }
                    ],
                    "synonyms": [
                        "in addition to",
                        "on top of",
                        "beyond"
                    ],
                    "common_phrases": [
                        "and more besides"
                    ],
                    "target_lang_definitions": {
                        "ru": "Также; вдобавок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вдобавок",
                                "target_lang_sense_clarification": "сверх того"
                            },
                            {
                                "target_lang_word": "к тому же",
                                "target_lang_sense_clarification": "также"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "967f728d-0aa9-3baa-a482-aae01f194206",
                    "sense_definition": "In any other place or way; otherwise.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sem_exception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have looked here, but nowhere <w>besides</w>.",
                            "target_lang_translations": {
                                "ru": "Я искал здесь, но нигде <w>больше</w>."
                            }
                        },
                        {
                            "text": "She has visited France but nowhere <w>besides</w> in Europe.",
                            "target_lang_translations": {
                                "ru": "Она посетила Францию, но нигде <w>больше</w> в Европе."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В другом месте; иначе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "больше",
                                "target_lang_sense_clarification": "в сочетании с отрицанием (нигде больше)"
                            },
                            {
                                "target_lang_word": "кроме",
                                "target_lang_sense_clarification": "помимо этого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "904b7255-2e17-305f-bd10-495aa015ad2f",
                    "sense_definition": "Off the mark; to the side.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sem_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The arrow flew <w>besides</w> and missed the target.",
                            "target_lang_translations": {
                                "ru": "Стрела пролетела <w>мимо</w> и не попала в цель."
                            }
                        },
                        {
                            "text": "His guess was <w>besides</w> the truth.",
                            "target_lang_translations": {
                                "ru": "Его догадка была <w>далека от</w> истины."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мимо; в сторону от цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мимо",
                                "target_lang_sense_clarification": "не попадая в цель"
                            },
                            {
                                "target_lang_word": "далеко от",
                                "target_lang_sense_clarification": "в переносном смысле"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
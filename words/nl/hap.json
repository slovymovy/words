            "senses": [
                {
                    "sense_id": "d46c271b-5cfd-3454-a782-dcdc6ff5ad8a",
                    "sense_definition": "Een hoeveelheid voedsel die men in één keer afbijt of in de mond neemt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "bite_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hongerige hond nam een grote <w>hap</w> van het vlees.",
                            "target_lang_translations": {
                                "en": "The hungry dog took a big <w>bite</w> of the meat.",
                                "ru": "Голодная собака откусила большой <w>кусок</w> мяса."
                            }
                        },
                        {
                            "text": "Mag ik een <w>hap</w> van jouw broodje proeven?",
                            "target_lang_translations": {
                                "en": "Can I taste a <w>bite</w> of your sandwich?",
                                "ru": "Можно мне попробовать <w>кусочек</w> твоего бутерброда?"
                            }
                        }
                    ],
                    "synonyms": [
                        "beet"
                    ],
                    "common_phrases": [
                        "een hap nemen",
                        "in één hap",
                        "een hap lucht"
                    ],
                    "target_lang_definitions": {
                        "en": "A quantity of food bitten off or taken into the mouth at once.",
                        "ru": "Количество еды, которое откусывают или кладут в рот за один раз."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bite",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "171b90e1-0fb8-3018-8e38-8eb3640670cd",
                    "sense_definition": "To decide to do something at a later time than originally planned; to put off.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "put_off",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to <w>delay</w> the meeting until next week because the manager was sick."
                        },
                        {
                            "text": "They <w>delayed</w> making a decision until they had more information."
                        }
                    ],
                    "synonyms": [
                        "postpone",
                        "put off",
                        "defer",
                        "adjourn",
                        "suspend"
                    ],
                    "common_phrases": [
                        "delay a decision",
                        "delay the start",
                        "delay action"
                    ]
                },
                {
                    "sense_id": "afa25dcf-a83b-34c6-b8c3-c2e381d402bd",
                    "sense_definition": "To make someone or something late; to slow down the progress of something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "hinder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train was <w>delayed</w> by heavy snow on the tracks."
                        },
                        {
                            "text": "Heavy traffic <w>delayed</w> us for over an hour on our way to the airport."
                        }
                    ],
                    "synonyms": [
                        "hold up",
                        "detain",
                        "impede",
                        "slow down"
                    ],
                    "common_phrases": [
                        "delayed by traffic",
                        "delayed by weather",
                        "sorry for delaying"
                    ]
                },
                {
                    "sense_id": "d5140184-60eb-3b6b-881e-d09074caa5d4",
                    "sense_definition": "To wait or hesitate before acting.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hesitate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't <w>delay</w>; apply for the job today before it is too late!"
                        },
                        {
                            "text": "He <w>delayed</w> too long and missed the opportunity to buy the house."
                        }
                    ],
                    "synonyms": [
                        "hesitate",
                        "wait",
                        "linger"
                    ],
                    "common_phrases": [
                        "do not delay",
                        "without delaying"
                    ]
                }
            ]
        },

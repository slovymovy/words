    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "20ec1c94-15a5-313c-ae01-df8d48d0e1c7",
                    "sense_definition": "An adult male human.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "male_human",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tall <w>man</w> in the suit opened the door for us.",
                            "target_lang_translations": {
                                "ru": "Высокий <w>мужчина</w> в костюме открыл нам дверь."
                            }
                        },
                        {
                            "text": "He is a kind <w>man</w> who always helps his neighbors.",
                            "target_lang_translations": {
                                "ru": "Он добрый <w>человек</w>, который всегда помогает своим соседям."
                            }
                        }
                    ],
                    "synonyms": [
                        "guy",
                        "fellow",
                        "gentleman",
                        "male"
                    ],
                    "antonyms": [
                        "woman"
                    ],
                    "common_phrases": [
                        "old man",
                        "best man",
                        "family man",
                        "young man"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взрослый человек мужского пола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мужчина",
                                "target_lang_sense_clarification": "человек мужского пола"
                            },
                            {
                                "target_lang_word": "мужик",
                                "target_lang_sense_clarification": "разговорное, часто просторечное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63c061f4-4387-3650-9cab-634bdf1e1c36",
                    "sense_definition": "A term of familiar address used for a person, usually a male.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "male_human",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hey <w>man</w>, how are you doing today?",
                            "target_lang_translations": {
                                "ru": "Эй, <w>мужик</w>, как у тебя дела сегодня?"
                            }
                        },
                        {
                            "text": "Thanks for your help, <w>man</w>.",
                            "target_lang_translations": {
                                "ru": "Спасибо за помощь, <w>мужик</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dude",
                        "mate",
                        "buddy",
                        "bro",
                        "pal"
                    ],
                    "common_phrases": [
                        "hey man",
                        "thanks man"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дружеское обращение к мужчине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мужик",
                                "target_lang_sense_clarification": "фамильярное обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec9fb116-2241-30d8-b56d-59887e8a8738",
                    "sense_definition": "A husband or a male romantic partner.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "male_human",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The priest pronounced them <w>man</w> and wife.",
                            "target_lang_translations": {
                                "ru": "Священник объявил их <w>мужем</w> и женой."
                            }
                        },
                        {
                            "text": "She said she would stand by her <w>man</w> no matter what happens.",
                            "target_lang_translations": {
                                "ru": "Она сказала, что будет поддерживать своего <w>мужчину</w>, что бы ни случилось."
                            }
                        }
                    ],
                    "synonyms": [
                        "husband",
                        "partner",
                        "boyfriend"
                    ],
                    "common_phrases": [
                        "man and wife",
                        "my man"
                    ],
                    "target_lang_definitions": {
                        "ru": "Муж или романтический партнёр."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "муж",
                                "target_lang_sense_clarification": "супруг"
                            },
                            {
                                "target_lang_word": "мужчина",
                                "target_lang_sense_clarification": "возлюбленный, партнёр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1086ffb1-4ba8-31c9-9959-e27b48a006ec",
                    "sense_definition": "A human being regardless of gender or sex; a person.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "human_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "No <w>man</w> is an island, entire of itself.",
                            "target_lang_translations": {
                                "ru": "Ни один <w>человек</w> — не остров, сам по себе."
                            }
                        },
                        {
                            "text": "It was a situation where it was every <w>man</w> for himself.",
                            "target_lang_translations": {
                                "ru": "Это была ситуация, когда каждый <w>человек</w> был сам за себя."
                            }
                        }
                    ],
                    "synonyms": [
                        "person",
                        "human",
                        "individual"
                    ],
                    "common_phrases": [
                        "no man is an island",
                        "every man for himself"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек независимо от пола; личность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "человек",
                                "target_lang_sense_clarification": "личность, человеческое существо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3039ddd7-8993-350e-b266-b450fc6af213",
                    "sense_definition": "All human beings collectively; the human race.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "human_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The invention of the wheel changed the history of <w>man</w>.",
                            "target_lang_translations": {
                                "ru": "Изобретение колеса изменило историю <w>человечества</w>."
                            }
                        },
                        {
                            "text": "Pollution poses a serious threat to <w>man</w> and nature alike.",
                            "target_lang_translations": {
                                "ru": "Загрязнение представляет серьезную угрозу как для <w>человека</w>, так и для природы."
                            }
                        }
                    ],
                    "synonyms": [
                        "mankind",
                        "humanity",
                        "humankind"
                    ],
                    "common_phrases": [
                        "history of man",
                        "fall of man"
                    ],
                    "target_lang_definitions": {
                        "ru": "Все люди в совокупности; человеческий род."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "человек",
                                "target_lang_sense_clarification": "человеческий род"
                            },
                            {
                                "target_lang_word": "люди",
                                "target_lang_sense_clarification": "человечество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94d782d4-a0df-33fa-b1d3-cb8dd17b9241",
                    "sense_definition": "An adult male possessing qualities considered masculine, such as strength or courage.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "male_human",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He told his son to be a <w>man</w> and accept responsibility for his mistake.",
                            "target_lang_translations": {
                                "ru": "Он сказал сыну быть <w>мужчиной</w> и взять на себя ответственность за свою ошибку."
                            }
                        },
                        {
                            "text": "Are you <w>man</w> enough to admit that you were wrong?",
                            "target_lang_translations": {
                                "ru": "Ты достаточно <w>мужчина</w>, чтобы признать, что был неправ?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "be a man",
                        "man enough",
                        "make a man of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взрослый мужчина, обладающий мужскими качествами, такими как сила или смелость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мужчина",
                                "target_lang_sense_clarification": "настоящий мужчина"
                            },
                            {
                                "target_lang_word": "мужик",
                                "target_lang_sense_clarification": "разговорное, сильный мужчина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f0ae8ac-e5e4-39a8-b66e-c3faaed7632d",
                    "sense_definition": "A piece or token used in board games like chess or checkers.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "game_piece",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He moved his white <w>man</w> forward to capture the opponent's piece.",
                            "target_lang_translations": {
                                "ru": "Он подвинул свою белую <w>фигуру</w> вперед, чтобы взять фигуру противника."
                            }
                        },
                        {
                            "text": "At the end of the game, only three black <w>men</w> remained on the board.",
                            "target_lang_translations": {
                                "ru": "В конце игры на доске осталось только три черные <w>фигуры</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "piece",
                        "token"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фишка или фигура в настольных играх, таких как шахматы или шашки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фигура",
                                "target_lang_sense_clarification": "в шахматах"
                            }
                        ]
                    }
                }
            ]
        },
        {

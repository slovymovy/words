{
    "word_family": [
        "routinely",
        "routinize"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1e88545b-9ae7-30ce-ab6f-5ff00574fbf0",
                    "sense_definition": "A fixed course of action or standard procedure followed regularly.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "procedure_habit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My morning <w>routine</w> includes drinking coffee and reading the news.",
                            "target_lang_translations": {
                                "ru": "Мой утренний <w>распорядок</w> включает кофе и чтение новостей."
                            }
                        },
                        {
                            "text": "It is important to establish a daily <w>routine</w> for your studies.",
                            "target_lang_translations": {
                                "ru": "Важно установить ежедневный <w>режим</w> для учебы."
                            }
                        }
                    ],
                    "synonyms": [
                        "procedure",
                        "habit",
                        "ritual"
                    ],
                    "common_phrases": [
                        "daily routine",
                        "morning routine",
                        "stick to a routine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Установленный порядок действий или режим, соблюдаемый регулярно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распорядок",
                                "target_lang_sense_clarification": "установленный порядок в организации дел или времени"
                            },
                            {
                                "target_lang_word": "режим",
                                "target_lang_sense_clarification": "точно установленный порядок жизни или занятий"
                            },
                            {
                                "target_lang_word": "порядок",
                                "target_lang_sense_clarification": "последовательность или ход чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed674ceb-2ad5-35f4-a1bd-f74cdf4789f2",
                    "sense_definition": "A set of normal procedures, often performed mechanically or without thought, leading to a feeling of monotony.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "procedure_habit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She felt tired of the endless <w>routine</w> of housework and cleaning.",
                            "target_lang_translations": {
                                "ru": "Она устала от бесконечной домашней <w>рутины</w> и уборки."
                            }
                        },
                        {
                            "text": "His life had become a boring <w>routine</w> with no surprises.",
                            "target_lang_translations": {
                                "ru": "Его жизнь превратилась в скучную <w>рутину</w> без сюрпризов."
                            }
                        }
                    ],
                    "synonyms": [
                        "rut"
                    ],
                    "common_phrases": [
                        "dull routine",
                        "settle into a routine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор привычных действий, часто выполняемых механически, вызывающий чувство однообразия и скуки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рутина",
                                "target_lang_sense_clarification": "механическая привычка, однообразие"
                            },
                            {
                                "target_lang_word": "обыденность",
                                "target_lang_sense_clarification": "заурядное, привычное течение жизни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0f96e12-f5b3-39e8-b18e-c9eaa92634a9",
                    "sense_definition": "A rehearsed sequence of actions, movements, or jokes in a performance.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The comedian practiced his new <w>routine</w> before the show.",
                            "target_lang_translations": {
                                "ru": "Комик репетировал свой новый <w>номер</w> перед шоу."
                            }
                        },
                        {
                            "text": "The skaters performed a flawless dance <w>routine</w> on the ice.",
                            "target_lang_translations": {
                                "ru": "Фигуристы исполнили безупречный танцевальный <w>номер</w> на льду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dance routine",
                        "comedy routine",
                        "gymnastics routine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отрепетированная последовательность действий, движений или шуток в выступлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "номер",
                                "target_lang_sense_clarification": "отдельное исполнение в программе концерта или цирка"
                            },
                            {
                                "target_lang_word": "программа",
                                "target_lang_sense_clarification": "совокупность номеров или действий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50f4f86d-b755-3447-b58d-1fe01b2e0f2d",
                    "sense_definition": "A set of instructions in a computer program designed to perform a specific task.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The programmer wrote a <w>routine</w> to sort the database automatically.",
                            "target_lang_translations": {
                                "ru": "Программист написал <w>подпрограмму</w> для автоматической сортировки базы данных."
                            }
                        },
                        {
                            "text": "This software <w>routine</w> checks for errors every time you save.",
                            "target_lang_translations": {
                                "ru": "Эта программная <w>процедура</w> проверяет наличие ошибок при каждом сохранении."
                            }
                        }
                    ],
                    "synonyms": [
                        "subroutine",
                        "function",
                        "procedure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор инструкций в компьютерной программе, предназначенный для выполнения определенной задачи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подпрограмма",
                                "target_lang_sense_clarification": "часть программы для решения вспомогательной задачи"
                            },
                            {
                                "target_lang_word": "процедура",
                                "target_lang_sense_clarification": "блок кода, выполняющий специфическую задачу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a3686e1c-517c-3d9d-a76e-a23affccf670",
                    "sense_definition": "Done regularly as part of a standard procedure; habitual.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "adj_standard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot performed a <w>routine</w> check of the instruments before takeoff.",
                            "target_lang_translations": {
                                "ru": "Пилот выполнил <w>плановую</w> проверку приборов перед взлетом."
                            }
                        },
                        {
                            "text": "It was just a <w>routine</w> meeting with nothing special to discuss.",
                            "target_lang_translations": {
                                "ru": "Это была просто <w>обычная</w> встреча, на которой не обсуждалось ничего особенного."
                            }
                        }
                    ],
                    "common_phrases": [
                        "routine check",
                        "routine maintenance",
                        "routine inspection"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполняемый регулярно как часть стандартной процедуры; плановый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плановый",
                                "target_lang_sense_clarification": "совершаемый по плану или графику"
                            },
                            {
                                "target_lang_word": "рутинный",
                                "target_lang_sense_clarification": "обычный, являющийся частью практики"
                            },
                            {
                                "target_lang_word": "обычный",
                                "target_lang_sense_clarification": "ничем не выделяющийся, привычный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c24f5d5-fce4-32f2-bcf3-4c84012583b3",
                    "sense_definition": "Ordinary and unexciting, with nothing to distinguish it from others.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_standard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team secured a <w>routine</w> victory against their opponents.",
                            "target_lang_translations": {
                                "ru": "Команда одержала <w>дежурную</w> победу над соперниками."
                            }
                        },
                        {
                            "text": "He described the surgery as <w>routine</w> and very low risk.",
                            "target_lang_translations": {
                                "ru": "Он описал операцию как <w>рядовую</w> и с очень низким риском."
                            }
                        }
                    ],
                    "common_phrases": [
                        "routine victory",
                        "routine job"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обыденный и неинтересный, ничем не выделяющийся среди других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заурядный",
                                "target_lang_sense_clarification": "посредственный, ничем не примечательный"
                            },
                            {
                                "target_lang_word": "рядовой",
                                "target_lang_sense_clarification": "обыкновенный, типичный"
                            },
                            {
                                "target_lang_word": "дежурный",
                                "target_lang_sense_clarification": "формальный, шаблонный (о победе, фразе)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
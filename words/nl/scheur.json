                    "sense_id": "fa21a7ed-9967-3e65-a1d8-23518e3821b8",
                    "sense_definition": "Een opening of barst die ontstaat wanneer materiaal kapotgaat of uit elkaar getrokken wordt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zat een lelijke <w>scheur</w> in zijn nieuwe jas.",
                            "target_lang_translations": {
                                "ru": "На его новой куртке была уродливая <w>дыра</w>.",
                                "en": "There was an ugly <w>tear</w> in his new coat."
                            }
                        },
                        {
                            "text": "Door de aardbeving ontstond er een diepe <w>scheur</w> in de weg.",
                            "target_lang_translations": {
                                "ru": "Из-за землетрясения на дороге образовалась глубокая <w>трещина</w>.",
                                "en": "The earthquake caused a deep <w>crack</w> in the road."
                            }
                        }
                    ],
                    "synonyms": [
                        "barst",
                        "spleet",
                        "kloof"
                    ],
                    "common_phrases": [
                        "een scheur in de muur",
                        "een scheur in je broek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отверстие или трещина, возникающая, когда материал ломается или разрывается.",
                        "en": "An opening or crack that appears when material breaks or is pulled apart."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трещина",
                                "target_lang_sense_clarification": "в твердых материалах (стены, земля)"
                            },
                            {
                                "target_lang_word": "разрыв",
                                "target_lang_sense_clarification": "в мягких материалах (ткань, бумага)"
                            },
                            {
                                "target_lang_word": "дыра",
                                "target_lang_sense_clarification": "разговорное, часто об одежде"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "tear",
                                "target_lang_sense_clarification": "in soft materials (fabric, paper)"
                            },
                            {
                                "target_lang_word": "crack",
                                "target_lang_sense_clarification": "in hard materials (walls, ground)"
                            },
                            {
                                "target_lang_word": "rip",

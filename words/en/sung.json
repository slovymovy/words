            "senses": [
                {
                    "sense_id": "2c117bed-f3bd-320b-b38e-b64f1f4d0e46",
                    "sense_definition": "The past participle of <w>sing</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has <w>sung</w> in the church choir for over ten years.",
                            "target_lang_translations": {
                                "ru": "Она <w>пела</w> в церковном хоре более десяти лет."
                            }
                        },
                        {
                            "text": "The national anthem was <w>sung</w> by a famous opera star before the game.",
                            "target_lang_translations": {
                                "ru": "Национальный гимн был <w>исполнен</w> известной оперной звездой перед игрой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "have sung",
                        "beautifully sung",
                        "sung by",
                        "well sung"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие прошедшего времени от глагола sing (петь)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спетый",
                                "target_lang_sense_clarification": "причастие"
                            },
                            {
                                "target_lang_word": "пел",
                                "target_lang_sense_clarification": "форма прошедшего времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f21ea9a-70a3-39e1-8c78-dd289a31b88f",
                    "sense_definition": "The simple past tense of <w>sing</w> (standard English uses 'sang').",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>sung</w> a quiet melody to himself while he worked in the garden.",
                            "target_lang_translations": {
                                "ru": "Он <w>напевал</w> тихую мелодию про себя, пока работал в саду."
                            }
                        },
                        {
                            "text": "They <w>sung</w> together around the campfire until the sun came up.",
                            "target_lang_translations": {
                                "ru": "Они <w>пели</w> вместе у костра, пока не взошло солнце."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Standard English uses 'sang' for the simple past."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время глагола sing (в стандартном английском — sang)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пел",
                                "target_lang_sense_clarification": "прошедшее время"
                            }
                        ]
                    }
                }
            ]
        },

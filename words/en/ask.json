    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ade9093a-5a3d-3d6b-9493-659a332a94a2",
                    "sense_definition": "To put a question to someone in order to get information or an answer.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "inquire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>asked</w> the teacher about the homework.",
                            "target_lang_translations": {
                                "ru": "Я <w>спросил</w> учителя о домашнем задании."
                            }
                        },
                        {
                            "text": "If you don't know the way, you should <w>ask</w> for directions.",
                            "target_lang_translations": {
                                "ru": "Если ты не знаешь дороги, тебе следует <w>спросить</w> направление."
                            }
                        }
                    ],
                    "synonyms": [
                        "request",
                        "request",
                        "request"
                    ],
                    "antonyms": [
                        "answer",
                        "reply"
                    ],
                    "common_phrases": [
                        "ask a question",
                        "ask about",
                        "ask around"
                    ],
                    "target_lang_definitions": {
                        "ru": "Задавать вопрос кому-либо с целью получения информации или ответа."
                    },
                    "translations": {

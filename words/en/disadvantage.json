{
    "word_family": [
        "advantage",
        "advantageous",
        "disadvantageous",
        "advantageously",
        "disadvantageously"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "22ce2ccd-17d6-3bc1-ac08-3bdac9378636",
                    "sense_definition": "An unfavorable circumstance or feature that causes problems or reduces the chances of success.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "negative_factor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The main <w>disadvantage</w> of living in the city is the noise.",
                            "target_lang_translations": {
                                "ru": "Главный <w>недостаток</w> жизни в городе — это шум."
                            }
                        },
                        {
                            "text": "One <w>disadvantage</w> of this plan is the high cost involved.",
                            "target_lang_translations": {
                                "ru": "Один <w>минус</w> этого плана — связанные с ним высокие расходы."
                            }
                        }
                    ],
                    "synonyms": [
                        "drawback",
                        "downside",
                        "con"
                    ],
                    "antonyms": [
                        "advantage"
                    ],
                    "common_phrases": [
                        "main disadvantage",
                        "big disadvantage",
                        "distinct disadvantage",
                        "major disadvantage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неблагоприятное обстоятельство или особенность, создающая проблемы или снижающая шансы на успех."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недостаток",
                                "target_lang_sense_clarification": "слабая сторона, изъян"
                            },
                            {
                                "target_lang_word": "минус",
                                "target_lang_sense_clarification": "отрицательная сторона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24ba1aa0-9208-3e05-b9fc-66150a0ed4bb",
                    "sense_definition": "A situation or condition that places someone in an inferior position compared to others.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "negative_factor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His lack of experience put him at a <w>disadvantage</w> during the interview.",
                            "target_lang_translations": {
                                "ru": "Отсутствие опыта поставило его в <w>невыгодное положение</w> во время собеседования."
                            }
                        },
                        {
                            "text": "The home team played at a <w>disadvantage</w> after the red card.",
                            "target_lang_translations": {
                                "ru": "Хозяева поля играли в <w>невыгодном положении</w> после красной карточки."
                            }
                        }
                    ],
                    "synonyms": [
                        "weakness",
                        "malefit",
                        "afterdeal"
                    ],
                    "antonyms": [
                        "advantage"
                    ],
                    "common_phrases": [
                        "at a disadvantage",
                        "serious disadvantage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация или условие, ставящее кого-либо в худшее положение по сравнению с другими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невыгодное положение",
                                "target_lang_sense_clarification": "ситуация, дающая меньше шансов на успех"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2b813a67-579e-35a2-822c-a87d2a4a5fa9",
                    "sense_definition": "To put someone or something in a less favorable position or condition.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new tax system may <w>disadvantage</w> low-income families.",
                            "target_lang_translations": {
                                "ru": "Новая налоговая система может <w>поставить в невыгодное положение</w> малообеспеченные семьи."
                            }
                        },
                        {
                            "text": "We must ensure that the rules do not <w>disadvantage</w> any participants.",
                            "target_lang_translations": {
                                "ru": "Мы должны убедиться, что правила не <w>ущемляют</w> никого из участников."
                            }
                        }
                    ],
                    "synonyms": [
                        "tell against",
                        "con"
                    ],
                    "antonyms": [
                        "advantage"
                    ],
                    "common_phrases": [
                        "unfairly disadvantage",
                        "disadvantage the poor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ставить кого-либо или что-либо в менее благоприятное положение или условия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ставить в невыгодное положение",
                                "target_lang_sense_clarification": "создавать худшие условия"
                            },
                            {
                                "target_lang_word": "ущемлять",
                                "target_lang_sense_clarification": "ограничивать в правах или возможностях"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
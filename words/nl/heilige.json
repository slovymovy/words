    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6a227595-d738-3fd8-9e1a-6383d95b79fa",
                    "sense_definition": "Verbogen vorm van <w>heilig</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In India worden koeien vaak beschouwd als <w>heilige</w> dieren.",
                            "target_lang_translations": {
                                "en": "In India, cows are often considered <w>sacred</w> animals.",
                                "ru": "В Индии коров часто считают <w>священными</w> животными."
                            }
                        },
                        {
                            "text": "Zij lazen elke avond een stukje uit de <w>heilige</w> geschriften.",
                            "target_lang_translations": {
                                "en": "Every evening they read a passage from the <w>holy</w> scriptures.",
                                "ru": "Каждый вечер они читали отрывок из <w>священных</w> писаний."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de heilige graal",
                        "de heilige geest",
                        "heilige grond"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "adjective inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Inflected form of 'heilig' (holy/sacred).",
                        "ru": "Склоняемая форма прилагательного 'heilig' (святой, священный)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "holy",
                                "target_lang_sense_clarification": "general religious or spiritual significance"
                            },
                            {
                                "target_lang_word": "sacred",
                                "target_lang_sense_clarification": "dedicated to a deity or religious purpose"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "священный",
                                "target_lang_sense_clarification": "посвященный Богу, культовый"
                            },
                            {
                                "target_lang_word": "святой",
                                "target_lang_sense_clarification": "обладающий божественной благодатью"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "af2b88da-b9c3-325f-bae7-89fd52c7df93",
                    "sense_definition": "Een persoon die door een religie, vaak de katholieke kerk, officieel is erkend als iemand die een bijzonder godvruchtig leven heeft geleid en vereerd wordt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_figure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De paus verklaarde de non tot <w>heilige</w> na een lang proces van onderzoek.",
                            "target_lang_translations": {
                                "en": "The pope declared the nun a <w>saint</w> after a long process of investigation.",
                                "ru": "Папа провозгласил монахиню <w>святой</w> после долгого процесса расследования."
                            }
                        },
                        {
                            "text": "Veel gelovigen bidden tot een specifieke <w>heilige</w> voor bescherming of genezing.",
                            "target_lang_translations": {
                                "en": "Many believers pray to a specific <w>saint</w> for protection or healing.",
                                "ru": "Многие верующие молятся конкретному <w>святому</w> о защите или исцелении."
                            }
                        }
                    ],
                    "synonyms": [
                        "sint"
                    ],
                    "antonyms": [
                        "zondaar"
                    ],
                    "common_phrases": [
                        "de heilige maagd",
                        "een patroonheilige"
                    ],
                    "target_lang_definitions": {
                        "en": "A person officially recognized by a religion, often the Catholic Church, as having lived a particularly godly life and who is venerated.",
                        "ru": "Человек, официально признанный религией (часто католической церковью) как проживший особо благочестивую жизнь и почитаемый верующими."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "saint",
                                "target_lang_sense_clarification": "canonized religious figure"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "святой",
                                "target_lang_sense_clarification": "канонизированный праведник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91df3c26-424e-3b6d-9f11-8518ca454b61",
                    "sense_definition": "Iemand die zich voorbeeldig gedraagt, zeer braaf is of geen fouten lijkt te maken (vaak ironisch of figuurlijk gebruikt).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "virtuous_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doe niet alsof je een <w>heilige</w> bent, want iedereen maakt wel eens fouten.",
                            "target_lang_translations": {
                                "en": "Don't act like you're a <w>saint</w>, because everyone makes mistakes sometimes.",
                                "ru": "Не строй из себя <w>святого</w>, ведь все иногда совершают ошибки."
                            }
                        },
                        {
                            "text": "Mijn broer gedraagt zich als een <w>heilige</w> zodra onze ouders de kamer binnenkomen.",
                            "target_lang_translations": {
                                "en": "My brother behaves like a <w>saint</w> as soon as our parents enter the room.",
                                "ru": "Мой брат ведет себя как <w>святой</w>, как только наши родители входят в комнату."
                            }
                        }
                    ],
                    "synonyms": [
                        "engel",
                        "braverik"
                    ],
                    "antonyms": [
                        "duivel",
                        "schurk"
                    ],
                    "common_phrases": [
                        "geen heilige zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who behaves exemplarily, is very well-behaved, or seems to make no mistakes (often used ironically or figuratively).",
                        "ru": "Тот, кто ведет себя безупречно, очень послушен или кажется не совершающим ошибок (часто используется иронично или в переносном смысле)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "saint",
                                "target_lang_sense_clarification": "virtuous person (figurative)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "святой",
                                "target_lang_sense_clarification": "праведник (иронично)"
                            },
                            {
                                "target_lang_word": "святоша",
                                "target_lang_sense_clarification": "лицемерный праведник"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07661da5-99cc-312f-9425-0ac8e568789c",
                    "sense_definition": "Aanvoegende wijs van <w>heiligen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moge God uw inspanningen zegenen en uw werk <w>heilige</w>.",
                            "target_lang_translations": {
                                "en": "May God bless your efforts and <w>sanctify</w> your work.",
                                "ru": "Да благословит Бог ваши усилия и <w>освятит</w> ваш труд."
                            }
                        },
                        {
                            "text": "Het doel <w>heilige</w> de middelen, zo redeneerde de generaal.",
                            "target_lang_translations": {
                                "en": "The end <w>sanctifies</w> the means, the general reasoned.",
                                "ru": "Цель <w>оправдывает</w> средства, так рассуждал генерал."
                            }
                        }
                    ],

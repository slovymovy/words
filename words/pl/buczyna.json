            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c5e28c19-fe27-30ee-a821-68dd1aac88eb",
                    "sense_definition": "Las, w którym dominującym gatunkiem drzewa jest buk.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "forest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jesienią karpacka <w>buczyna</w> zachwyca turystów paletą złotych i czerwonych barw.",
                            "target_lang_translations": {
                                "ru": "Осенью карпатский <w>буковый лес</w> восхищает туристов палитрой золотых и красных красок."
                            }
                        },
                        {
                            "text": "Podczas spaceru przez starą <w>buczynę</w> czuliśmy przyjemny chłód i zapach liści.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки через старый <w>буковый лес</w> мы чувствовали приятную прохладу и запах листьев."
                            }
                        }
                    ],
                    "synonyms": [
                        "bukowina"
                    ],
                    "common_phrases": [
                        "kwaśna buczyna",
                        "żyzna buczyna",
                        "karpacka buczyna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лес, в котором преобладающей породой деревьев является бук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "буковый лес",
                                "target_lang_sense_clarification": "лес"
                            },
                            {
                                "target_lang_word": "бучняк",
                                "target_lang_sense_clarification": "лес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6975c14f-184c-3ae4-9ad4-4e8d940ce9af",
                    "sense_definition": "Drewno pozyskane z drzewa bukowego, używane w stolarstwie i jako opał.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stolarz polecił nam meble wykonane z twardej <w>buczyny</w> ze względu na ich trwałość.",
                            "target_lang_translations": {
                                "ru": "Столяр порекомендовал нам мебель, изготовленную из твердого <w>бука</w>, из-за её долговечности."
                            }
                        },
                        {
                            "text": "Do wędzenia ryb często używa się suchej <w>buczyny</w>, która nadaje specyficzny aromat.",
                            "target_lang_translations": {
                                "ru": "Для копчения рыбы часто используют сухую <w>буковую древесину</w>, которая придает специфический аромат."
                            }
                        }
                    ],
                    "common_phrases": [
                        "twarda buczyna",
                        "sucha buczyna",
                        "meble z buczyny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Древесина, полученная из бука, используемая в столярном деле и как топливо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бук",
                                "target_lang_sense_clarification": "материал"
                            },
                            {
                                "target_lang_word": "буковая древесина",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a343c6bd-1cd3-328c-b9e8-ba1b1369aa93",
                    "sense_definition": "Owoce buka w postaci trójgraniastych orzeszków.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fruit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziki w lesie chętnie żywią się opadłą <w>buczyną</w> zagrzebaną w liściach.",
                            "target_lang_translations": {
                                "ru": "Кабаны в лесу охотно питаются опавшими <w>буковыми орешками</w>, зарытыми в листве."
                            }
                        },
                        {
                            "text": "Dawniej zbierano <w>buczynę</w>, aby tłoczyć z niej olej jadalny.",
                            "target_lang_translations": {
                                "ru": "Раньше собирали <w>буковые орешки</w>, чтобы выжимать из них пищевое масло."
                            }
                        }
                    ],
                    "synonyms": [
                        "bukiew",
                        "orzeszek bukowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоды бука в виде трехгранных орешков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "буковые орешки",
                                "target_lang_sense_clarification": "плоды"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7f713e74-fc40-36b2-8787-e40e139ed6d2",
                    "sense_definition": "Собрать свои вещи в сумки или чемоданы, готовясь к поездке или отъезду.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "preparation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы успели <w>уложиться</w> за час до выхода из дома.",
                            "target_lang_translations": {
                                "en": "We managed to <w>pack</w> in an hour before leaving the house."
                            }
                        },
                        {
                            "text": "Перед отпуском она долго не могла <w>уложиться</w>, выбирая одежду.",
                            "target_lang_translations": {
                                "en": "Before the vacation, she took a long time to <w>pack</w> while choosing clothes."
                            }
                        }
                    ],
                    "synonyms": [
                        "собраться",
                        "упаковаться"
                    ],
                    "antonyms": [
                        "распаковаться"
                    ],
                    "common_phrases": [
                        "быстро уложиться",
                        "помочь уложиться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implied by usage context and input data"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To pack one's belongings into bags or suitcases in preparation for a trip."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pack",
                                "target_lang_sense_clarification": "to put things into luggage"
                            },
                            {
                                "target_lang_word": "get packed",
                                "target_lang_sense_clarification": "to finish packing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8985a1b1-c044-3646-8762-ebbabbe1981c",
                    "sense_definition": "Успеть выполнить действие или ограничить себя в пределах определённого времени, бюджета или нормы.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "limits",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Строители обещали <w>уложиться</w> в график и сдать объект вовремя.",
                            "target_lang_translations": {
                                "en": "The builders promised to <w>meet</w> the schedule and hand over the facility on time."
                            }
                        },
                        {
                            "text": "Нам нужно <w>уложиться</w> в бюджет, чтобы не занимать деньги.",
                            "target_lang_translations": {
                                "en": "We need to <w>stay within</w> the budget so as not to borrow money."
                            }
                        }
                    ],
                    "synonyms": [
                        "успеть"
                    ],
                    "antonyms": [
                        "опоздать"
                    ],
                    "common_phrases": [
                        "уложиться в срок",
                        "уложиться в бюджет",
                        "уложиться в график",
                        "уложиться в норматив"
                    ],
                    "target_lang_definitions": {
                        "en": "To manage to complete an action or restrict oneself within specific limits of time, budget, or norms."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "meet",
                                "target_lang_sense_clarification": "a deadline or schedule"
                            },
                            {
                                "target_lang_word": "stay within",
                                "target_lang_sense_clarification": "a budget or limit"
                            },
                            {
                                "target_lang_word": "keep within",
                                "target_lang_sense_clarification": "limits or timeframes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1669b31-ac1b-3e80-b041-d45a83e5cc4a",
                    "sense_definition": "Полностью поместиться внутри чего-либо, занять соответствующее место (обычно о вещах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Все книги удачно <w>уложились</w> в одну большую коробку.",
                            "target_lang_translations": {
                                "en": "All the books <w>fit</w> successfully into one large box."
                            }
                        },
                        {
                            "text": "Одежда не <w>уложилась</w> в рюкзак, поэтому пришлось взять пакет.",
                            "target_lang_translations": {
                                "en": "The clothes didn't <w>fit</w> into the backpack, so I had to take a bag."
                            }
                        }
                    ],
                    "synonyms": [
                        "поместиться",
                        "уместиться",
                        "войти"
                    ],
                    "common_phrases": [
                        "уложиться в чемодан",
                        "уложиться в коробку"
                    ],
                    "target_lang_definitions": {
                        "en": "To fit completely inside a container or occupy a specific space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fit",
                                "target_lang_sense_clarification": "physically inside something"
                            },
                            {
                                "target_lang_word": "go in",
                                "target_lang_sense_clarification": "to fit into a space"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
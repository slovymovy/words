    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e443330d-96aa-3a54-957f-82eb642b2717",
                    "sense_definition": "Present participle and gerund of <w>protest</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thousands of people are <w>protesting</w> against the new government policies in the city center.",
                            "target_lang_translations": {
                                "ru": "Тысячи людей <w>протестуют</w> против новой политики правительства в центре города."
                            }
                        },
                        {
                            "text": "She kept <w>protesting</w> her innocence even after the evidence was presented.",
                            "target_lang_translations": {
                                "ru": "Она продолжала <w>настаивать</w> на своей невиновности даже после того, как были представлены доказательства."
                            }
                        }
                    ],
                    "synonyms": [
                        "objecting",
                        "demonstrating",
                        "complaining"
                    ],
                    "antonyms": [
                        "accepting",
                        "agreeing"
                    ],
                    "common_phrases": [
                        "protesting against",
                        "protesting the decision",
                        "protesting loudly"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола protest (протестовать, возражать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протестовать",
                                "target_lang_sense_clarification": "выражать решительное несогласие, участвовать в демонстрации"
                            },
                            {
                                "target_lang_word": "возражать",
                                "target_lang_sense_clarification": "высказывать несогласие с кем-либо или чем-либо"
                            },
                            {
                                "target_lang_word": "настаивать",
                                "target_lang_sense_clarification": "твердо утверждать что-либо (например, свою невиновность)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c607dc7d-cd9c-3dcf-90bc-7f9749036af0",
                    "sense_definition": "The act of making a solemn declaration or objection; a protestation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Despite his loud <w>protestings</w>, the guard refused to let him enter the building.",
                            "target_lang_translations": {
                                "ru": "Несмотря на его громкие <w>возражения</w>, охранник отказался пустить его в здание."
                            }
                        },
                        {
                            "text": "Her tearful <w>protestings</w> of loyalty were met with skepticism by the council.",
                            "target_lang_translations": {
                                "ru": "Её слёзные <w>заверения</w> в верности были встречены советом со скептицизмом."
                            }
                        }
                    ],
                    "synonyms": [
                        "protestation",
                        "declaration",
                        "objection"
                    ],
                    "antonyms": [
                        "acceptance",
                        "acquiescence"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт торжественного заявления или возражения; протестация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возражение",
                                "target_lang_sense_clarification": "выражение несогласия"
                            },
                            {
                                "target_lang_word": "заверение",
                                "target_lang_sense_clarification": "торжественное утверждение или обещание"
                            },
                            {
                                "target_lang_word": "протест",
                                "target_lang_sense_clarification": "решительное заявление о несогласии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
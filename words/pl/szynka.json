    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5a51f45a-b0fc-3190-9770-74116f7ba3a7",
                    "sense_definition": "Wędlina wyrabiana z tylnej części tuszy wieprzowej (lub rzadziej innego zwierzęcia), zazwyczaj wędzona, parzona lub gotowana.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "meat_culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na śniadanie często jem kanapki z serem i <w>szynką</w>.",
                            "target_lang_translations": {
                                "ru": "На завтрак я часто ем бутерброды с сыром и <w>ветчиной</w>."
                            }
                        },
                        {
                            "text": "W sklepie mięsnym poprosiłem o dziesięć plasterków <w>szynki</w> gotowanej.",
                            "target_lang_translations": {
                                "ru": "В мясном магазине я попросил десять ломтиков вареной <w>ветчины</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kanapka z szynką",
                        "szynka gotowana",
                        "szynka wędzona",
                        "szynka parmeńska",
                        "jajecznica na szynce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мясное изделие из задней части свиной туши (реже другого животного), обычно копченое, вареное или запеченное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ветчина",
                                "target_lang_sense_clarification": "готовый мясной продукт"
                            },
                            {
                                "target_lang_word": "окорок",
                                "target_lang_sense_clarification": "копченое или вареное мясо на кости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5ad9ec5-a986-3488-9394-a646c3a4a050",
                    "sense_definition": "Tylna część półtuszy wieprzowej w stanie surowym, używana jako mięso do pieczenia lub surowiec do produkcji wędlin.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "meat_culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na świąteczny obiad mama przygotowała pieczoną <w>szynkę</w> w ziołach.",
                            "target_lang_translations": {
                                "ru": "На праздничный обед мама приготовила запеченный <w>окорок</w> в травах."
                            }
                        },
                        {
                            "text": "Rzeźnik wykroił piękny kawałek surowej <w>szynki</w> na pieczeń.",
                            "target_lang_translations": {
                                "ru": "Мясник вырезал красивый кусок сырого <w>окорока</w> для жаркого."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pieczona szynka",
                        "szynka wieprzowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Задняя часть свиной туши в сыром виде, используемая для запекания или производства мясных изделий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окорок",
                                "target_lang_sense_clarification": "часть туши"
                            },
                            {
                                "target_lang_word": "свиной окорок",
                                "target_lang_sense_clarification": "сырое мясо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51c47433-7374-3e2c-873a-01c073f3255f",
                    "sense_definition": "Niewielka <w>szyna</w>; zdrobnienie używane w kontekście technicznym lub medycznym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "diminutive_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz usztywnił złamany palec pacjenta za pomocą małej <w>szynki</w>.",
                            "target_lang_translations": {
                                "ru": "Врач зафиксировал сломанный палец пациента с помощью маленькой <w>шины</w>."
                            }
                        },
                        {
                            "text": "Modelarz precyzyjnie przykleił każdą <w>szynkę</w> do makiety torowiska.",
                            "target_lang_translations": {
                                "ru": "Моделист аккуратно приклеил каждый <w>рельс</w> к макету железнодорожного пути."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая шина или рельс; уменьшительная форма, используемая в техническом или медицинском контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шина",
                                "target_lang_sense_clarification": "медицина"
                            },
                            {
                                "target_lang_word": "рельс",
                                "target_lang_sense_clarification": "техника, моделирование"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
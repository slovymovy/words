    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "741094f1-31c3-3830-82c1-ee62b77e5593",
                    "sense_definition": "Восклицание, выражающее похвалу, одобрение или восхищение чьим-либо успехом (часто в театре или на концерте).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "praise_exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зрители встали со своих мест и громко закричали «<w>браво</w>».",
                            "target_lang_translations": {
                                "en": "The audience stood up from their seats and loudly shouted \"<w>bravo</w>\"."
                            }
                        },
                        {
                            "text": "Ты отлично справился с этим сложным заданием, <w>браво</w>!",
                            "target_lang_translations": {
                                "en": "You handled this difficult task perfectly, <w>bravo</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "молодцы",
                        "здорово"
                    ],
                    "antonyms": [
                        "долой"
                    ],
                    "common_phrases": [
                        "кричать браво",
                        "бис и браво"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation expressing praise, approval, or admiration for someone's success (often in a theater or at a concert)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bravo",
                                "target_lang_sense_clarification": "exclamation of praise"
                            },
                            {
                                "target_lang_word": "well done",
                                "target_lang_sense_clarification": "general praise for an achievement"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "cf9e5a77-cbc2-3ead-ba1e-342cd5d2f2aa",
                    "sense_definition": "Наречие к слову <w>бравый</w>; действие, совершаемое молодцевато, бодро или с показной смелостью.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bravely_adverb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Несмотря на усталость, солдат <w>браво</w> шагал в строю.",
                            "target_lang_translations": {
                                "en": "Despite fatigue, the soldier marched <w>gallantly</w> in formation."
                            }
                        },
                        {
                            "text": "Он <w>браво</w> ответил на сложный вопрос экзаменатора.",
                            "target_lang_translations": {
                                "en": "He answered the examiner's difficult question <w>boldly</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "храбро",
                        "отважно"
                    ],
                    "antonyms": [
                        "трусливо"
                    ],
                    "common_phrases": [
                        "держаться браво",
                        "выглядеть браво"
                    ],
                    "target_lang_definitions": {
                        "en": "An action performed in a gallant, spirited, or showily brave manner."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gallantly",
                                "target_lang_sense_clarification": "in a spirited or dashing manner"
                            },
                            {
                                "target_lang_word": "bravely",
                                "target_lang_sense_clarification": "in a courageous manner"
                            },
                            {
                                "target_lang_word": "boldly",
                                "target_lang_sense_clarification": "with confidence or daring"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "826a6dbb-99e0-38b0-a484-87f2c7dfe1b9",
                    "sense_definition": "Наёмный убийца или вооружённый телохранитель в Италии XVI–XVII веков.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_assassin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В тёмном переулке его поджидал нанятый врагами <w>браво</w>.",
                            "target_lang_translations": {
                                "en": "A <w>bravo</w> hired by enemies was waiting for him in the dark alley."
                            }
                        },
                        {
                            "text": "Знатный вельможа никогда не выходил из дома без сопровождения своих верных <w>браво</w>.",
                            "target_lang_translations": {
                                "en": "The noble grandee never left the house without the accompaniment of his loyal <w>bravos</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "убийца",
                        "наймит"
                    ],
                    "traits": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e829167d-be14-36f5-bbca-1abead022a90",
                    "sense_definition": "Een officiële overeenkomst, zoals een contract, abonnement of dienstverband, beëindigen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga mijn abonnement op de sportschool <w>opzeggen</w> omdat ik er nooit naartoe ga.",
                            "target_lang_translations": {
                                "ru": "Я собираюсь <w>отменить</w> свой абонемент в спортзал, потому что я никогда туда не хожу.",
                                "en": "I am going to <w>cancel</w> my gym subscription because I never go there."
                            }
                        },
                        {
                            "text": "Hij heeft zijn baan <w>opgezegd</w> om een lange reis rond de wereld te maken.",
                            "target_lang_translations": {
                                "ru": "Он <w>уволился</w> с работы, чтобы отправиться в долгое кругосветное путешествие.",
                                "en": "He <w>quit</w> his job to go on a long trip around the world."
                            }
                        }
                    ],
                    "synonyms": [
                        "beëindigen",
                        "annuleren"
                    ],
                    "antonyms": [
                        "verlengen",
                        "aangaan"
                    ],
                    "common_phrases": [
                        "een abonnement opzeggen",
                        "de huur opzeggen",
                        "het contract opzeggen",
                        "zijn baan opzeggen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расторгнуть или прекратить действие официального соглашения, такого как контракт, подписка или трудовой договор.",
                        "en": "To terminate or end an official agreement, such as a contract, subscription, or employment."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расторгнуть",
                                "target_lang_sense_clarification": "о контракте или договоре"
                            },
                            {
                                "target_lang_word": "отменить",
                                "target_lang_sense_clarification": "о подписке или услуге"
                            },
                            {
                                "target_lang_word": "аннулировать",
                                "target_lang_sense_clarification": "формально прекратить действие"
                            },
                            {
                                "target_lang_word": "уволиться",
                                "target_lang_sense_clarification": "с работы (zijn baan opzeggen)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "cancel",
                                "target_lang_sense_clarification": "a subscription or service"
                            },
                            {
                                "target_lang_word": "terminate",
                                "target_lang_sense_clarification": "a contract or agreement"
                            },
                            {
                                "target_lang_word": "quit",
                                "target_lang_sense_clarification": "a job (informal)"
                            },
                            {
                                "target_lang_word": "give notice",
                                "target_lang_sense_clarification": "for rent or employment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4a698fd-22e3-3e61-926c-6d5c0b708031",
                    "sense_definition": "Iets wat men uit het hoofd geleerd heeft hardop uitspreken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leerling moest voor de hele klas een lang gedicht <w>opzeggen</w>.",
                            "target_lang_translations": {
                                "ru": "Ученик должен был <w>прочитать наизусть</w> длинное стихотворение перед всем классом.",
                                "en": "The student had to <w>recite</w> a long poem in front of the whole class."
                            }
                        },
                        {
                            "text": "Kun jij het alfabet heel snel achterstevoren <w>opzeggen</w>?",
                            "target_lang_translations": {
                                "ru": "Ты можешь очень быстро <w>произнести</w> алфавит задом наперед?",
                                "en": "Can you <w>recite</w> the alphabet backwards very quickly?"
                            }
                        }
                    ],
                    "synonyms": [
                        "voordragen",
                        "reciteren"
                    ],
                    "common_phrases": [
                        "een gedicht opzeggen",
                        "een versje opzeggen",
                        "de tafel van tien opzeggen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произнести вслух то, что было выучено наизусть.",
                        "en": "To speak aloud something that has been learned by heart."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "читать наизусть",
                                "target_lang_sense_clarification": "стихи или текст"
                            },

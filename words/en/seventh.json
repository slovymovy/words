            "senses": [
                {
                    "sense_id": "faba3c1f-365f-37fd-84fb-2dca2dcf1667",
                    "sense_definition": "The ordinal form of the number <w>seven</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ordinal_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This is the <w>seventh</w> time I have called you today.",
                            "target_lang_translations": {
                                "ru": "Это уже <w>седьмой</w> раз, когда я звоню тебе сегодня."
                            }
                        },
                        {
                            "text": "They live on the <w>seventh</w> floor of the apartment building.",
                            "target_lang_translations": {
                                "ru": "Они живут на <w>седьмом</w> этаже многоквартирного дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "7th",
                        "VII"
                    ],
                    "common_phrases": [
                        "seventh heaven",
                        "seventh grade",
                        "seventh son",
                        "seventh day"
                    ],
                    "target_lang_definitions": {
                        "ru": "Порядковое числительное, соответствующее числу семь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "седьмой",
                                "target_lang_sense_clarification": "порядковый номер"
                            }
                        ]
                    }
                }
            ]
        },
        {

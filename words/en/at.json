                    "common_phrases": [
                        "look at",
                        "smile at",
                        "shout at",
                        "point at",
                        "throw at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания цели или направления действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "направление взгляда или действия на объект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6759ba0-abca-3783-942f-9fa553a70700",
                    "sense_definition": "Used to indicate a state, condition, or continuous activity.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two countries have been <w>at</w> war for several years.",

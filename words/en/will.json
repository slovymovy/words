{
    "word_family": [
        "willing",
        "willingness",
        "willful",
        "willfully",
        "unwilling"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "626cb396-aa44-33ae-94b5-508b9f158f58",
                    "sense_definition": "Used to express the future tense, indicating that something is expected to happen or will be true.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "future_aux",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>will</w> call you as soon as I arrive at the station.",
                            "target_lang_translations": {
                                "ru": "Я <w>позвоню</w> тебе, как только приеду на вокзал."
                            }
                        },
                        {
                            "text": "The weather forecast says it <w>will</w> rain tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Прогноз погоды говорит, что завтра утром <w>пойдет</w> дождь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "will be",
                        "will do",
                        "will not",
                        "will see"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для образования будущего времени, указывая на то, что что-то произойдет или будет истинным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "вспомогательный глагол для образования будущего времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ade64501-5a39-3f63-af68-1be9cb38d8d1",
                    "sense_definition": "Used to ask someone to do something, or to state that one is willing to do something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "volition_aux",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Will</w> you help me carry these heavy bags upstairs?",
                            "target_lang_translations": {
                                "ru": "Ты <w>поможешь</w> мне отнести эти тяжелые сумки наверх?"
                            }
                        },
                        {
                            "text": "I <w>will</w> wash the dishes if you cook dinner.",
                            "target_lang_translations": {
                                "ru": "Я <w>помою</w> посуду, если ты приготовишь ужин."
                            }
                        }
                    ],
                    "common_phrases": [
                        "will you",
                        "I will"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для просьбы сделать что-либо или для выражения готовности сделать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "выражение желания или готовности"
                            },
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "формальное выражение воли или желания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84989df5-4889-3bf7-b6cd-db8c7df475f5",
                    "sense_definition": "Used to express what is believed to be true or probable in the present moment.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "probability_aux",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That <w>will</w> be the postman ringing the doorbell now.",
                            "target_lang_translations": {
                                "ru": "Это, <w>должно быть</w>, почтальон звонит в дверь."
                            }
                        },
                        {
                            "text": "She <w>will</w> be at work by this time of the day.",
                            "target_lang_translations": {
                                "ru": "Она, <w>наверное</w>, на работе в это время дня."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения того, что считается истинным или вероятным в данный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "должно быть",
                                "target_lang_sense_clarification": "выражение уверенного предположения"
                            },
                            {
                                "target_lang_word": "наверное",
                                "target_lang_sense_clarification": "выражение вероятности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9a6b5ae-07ee-3da4-8d87-4a297698c7cc",
                    "sense_definition": "Used to describe characteristic behavior or something that happens habitually.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "habitual_aux",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Boys <w>will</w> be boys, so do not worry too much about their rough play.",
                            "target_lang_translations": {
                                "ru": "Мальчишки <w>есть</w> мальчишки, так что не волнуйся слишком сильно из-за их грубых игр."
                            }
                        },
                        {
                            "text": "He <w>will</w> often sit in the garden for hours reading a book.",
                            "target_lang_translations": {
                                "ru": "Он часто <w>сидит</w> в саду часами, читая книгу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "accidents will happen",
                        "boys will be boys"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для описания характерного поведения или того, что происходит обычно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иметь обыкновение",
                                "target_lang_sense_clarification": "о повторяющемся действии"
                            },
                            {
                                "target_lang_word": "любить",
                                "target_lang_sense_clarification": "в значении склонности к действию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59d14b0f-8d2d-38c7-8f10-41302c5bb3b6",
                    "sense_definition": "To be able to or have the capacity to hold or fit something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "capacity_aux",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This jar <w>will</w> hold exactly one liter of water.",
                            "target_lang_translations": {
                                "ru": "Эта банка <w>вмещает</w> ровно один литр воды."
                            }
                        },
                        {
                            "text": "Unfortunately, only one of these gloves <w>will</w> actually fit over my hand.",
                            "target_lang_translations": {
                                "ru": "К сожалению, только одна из этих перчаток действительно <w>налезет</w> мне на руку."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть способным вместить что-либо или подходить по размеру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вмещать",
                                "target_lang_sense_clarification": "быть способным содержать в себе"
                            },
                            {
                                "target_lang_word": "подходить",
                                "target_lang_sense_clarification": "быть впору, соответствовать размеру"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a8bf7067-85f5-3de9-a931-300f262ba72c",
                    "sense_definition": "A legal document stating how a person's property should be distributed after their death.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "legal_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to write her <w>will</w> to ensure her children were provided for.",
                            "target_lang_translations": {
                                "ru": "Она решила написать <w>завещание</w>, чтобы обеспечить своих детей."
                            }
                        },
                        {
                            "text": "The lawyer read the <w>will</w> to the family after the funeral.",
                            "target_lang_translations": {
                                "ru": "Адвокат прочитал <w>завещание</w> семье после похорон."
                            }
                        }
                    ],
                    "synonyms": [
                        "testament",
                        "last will"
                    ],
                    "common_phrases": [
                        "leave a will",
                        "in his will",
                        "living will"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридический документ, указывающий, как имущество человека должно быть распределено после его смерти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завещание",
                                "target_lang_sense_clarification": "документ о наследстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1c379a7-fd3c-3fe7-91c5-6af2f027aaf0",
                    "sense_definition": "Strong determination and firmness of purpose to achieve something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "determination_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He showed great <w>will</w> to recover from his injury so quickly.",
                            "target_lang_translations": {
                                "ru": "Он проявил огромную <w>волю</w> к выздоровлению после травмы."
                            }
                        },
                        {
                            "text": "Success often depends more on <w>will</w> than on talent alone.",
                            "target_lang_translations": {
                                "ru": "Успех часто зависит больше от <w>воли</w>, чем от одного лишь таланта."
                            }
                        }
                    ],
                    "synonyms": [
                        "determination",
                        "resolve",
                        "firmness",
                        "resoluteness"
                    ],
                    "common_phrases": [
                        "will to live",
                        "strength of will",
                        "iron will"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильная решимость и твердость в достижении цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воля",
                                "target_lang_sense_clarification": "целеустремленность, сила характера"
                            },
                            {
                                "target_lang_word": "сила воли",
                                "target_lang_sense_clarification": "способность к самоконтролю"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86a77896-6865-3b9c-a72b-7f61b632fe36",
                    "sense_definition": "The mental power used to make conscious choices and decisions.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "volition_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was forced to go to the meeting against his <w>will</w>.",
                            "target_lang_translations": {
                                "ru": "Его заставили пойти на встречу против его <w>воли</w>."
                            }
                        },
                        {
                            "text": "Many philosophers debate whether humans truly possess free <w>will</w>.",
                            "target_lang_translations": {
                                "ru": "Многие философы спорят, обладают ли люди действительно свободной <w>волей</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "free will",
                        "against one's will",
                        "at will"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ментальная способность делать осознанный выбор и принимать решения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воля",
                                "target_lang_sense_clarification": "свобода выбора, сознательное управление собой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5491210d-6810-3213-9983-cf4b95bd3d9f",
                    "sense_definition": "A desire, wish, or command, often from someone in authority.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "desire_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers had to submit to the <w>will</w> of their commander.",
                            "target_lang_translations": {
                                "ru": "Солдаты должны были подчиниться <w>воле</w> своего командира."
                            }
                        },
                        {
                            "text": "It was the <w>will</w> of the people that the law be changed.",
                            "target_lang_translations": {
                                "ru": "Это была <w>воля</w> народа, чтобы закон был изменен."
                            }
                        }
                    ],
                    "common_phrases": [
                        "will of God",
                        "will of the people"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Желание или приказ, часто исходящий от кого-то, обладающего властью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воля",
                                "target_lang_sense_clarification": "желание, веление"
                            },
                            {
                                "target_lang_word": "желание",
                                "target_lang_sense_clarification": "стремление, хотение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "78968a46-04ea-3b34-bd2b-325d249c3f27",
                    "sense_definition": "A common diminutive of the male given name William.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "name_given",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My friend <w>Will</w> is coming to the party tonight.",
                            "target_lang_translations": {
                                "ru": "Мой друг <w>Уилл</w> придет сегодня на вечеринку."
                            }
                        },
                        {
                            "text": "His full name is William, but everyone calls him <w>Will</w>.",
                            "target_lang_translations": {
                                "ru": "Его полное имя Уильям, но все зовут его <w>Уилл</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенное уменьшительное от мужского имени Уильям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Уилл",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a21b7f3-3cda-33f0-ac35-080f4f99cff1",
                    "sense_definition": "A surname.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "name_surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Will</w> has been our neighbor for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Уилл</w> был нашим соседом двадцать лет."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Уилл",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f6ab6cbb-36f5-3f76-ab77-e54fb7e8de76",
                    "sense_definition": "To try to make something happen by using one's mental power or determination.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "force_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>willed</w> herself to stay awake despite being exhausted.",
                            "target_lang_translations": {
                                "ru": "Она <w>заставила</w> себя не спать, несмотря на усталость."
                            }
                        },
                        {
                            "text": "The fans were <w>willing</w> their team to score a goal in the final minute.",
                            "target_lang_translations": {
                                "ru": "Болельщики страстно <w>желали</w>, чтобы их команда забила гол на последней минуте."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пытаться заставить что-то произойти, используя силу мысли или решимость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заставлять",
                                "target_lang_sense_clarification": "принуждать усилием воли"
                            },
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "сильно хотеть исполнения чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5ab3168-ddf3-323f-b190-9e664b911e35",
                    "sense_definition": "To legally give property to someone after one's death.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "bequeath_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>willed</w> his entire stamp collection to the local museum.",
                            "target_lang_translations": {
                                "ru": "Он <w>завещал</w> всю свою коллекцию марок местному музею."
                            }
                        },
                        {
                            "text": "She <w>willed</w> the house to her eldest daughter.",
                            "target_lang_translations": {
                                "ru": "Она <w>завещала</w> дом своей старшей дочери."
                            }
                        }
                    ],
                    "synonyms": [
                        "bequeath",
                        "leave"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридически передавать имущество кому-либо после смерти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завещать",
                                "target_lang_sense_clarification": "оставлять наследство"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "04fc1389-5b70-3a47-8d8e-031749f70405",
                    "sense_definition": "Najwyżej położona lub najbardziej wysunięta część jakiegoś przedmiotu, na przykład drzewa.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

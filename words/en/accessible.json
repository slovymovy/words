{
    "word_family": [
        "access",
        "accessibility",
        "inaccessible",
        "inaccessibility",
        "accessibly"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b37af978-4d58-367b-b274-02a3e8d951ed",
                    "sense_definition": "Easy to reach, enter, or approach.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The remote village is only <w>accessible</w> by boat during the summer.",
                            "target_lang_translations": {
                                "ru": "Эта отдаленная деревня <w>доступна</w> только на лодке в летнее время."
                            }
                        },
                        {
                            "text": "We chose a hotel that is easily <w>accessible</w> from the main train station.",
                            "target_lang_translations": {
                                "ru": "Мы выбрали отель, который легко <w>доступен</w> от главного железнодорожного вокзала."
                            }
                        }
                    ],
                    "synonyms": [
                        "approachable"
                    ],
                    "antonyms": [
                        "inaccessible"
                    ],
                    "common_phrases": [
                        "easily accessible",
                        "accessible by car",
                        "accessible location",
                        "accessible to the public"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкий для достижения, входа или приближения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "досту́пный",
                                "target_lang_sense_clarification": "о месте или объекте"
                            },
                            {
                                "target_lang_word": "достижи́мый",
                                "target_lang_sense_clarification": "до которого можно добраться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e41c0ef-89dd-3aba-97ca-4a8d08179844",
                    "sense_definition": "Built or designed to be usable by people with disabilities.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new library entrance is fully <w>accessible</w> to wheelchair users.",
                            "target_lang_translations": {
                                "ru": "Новый вход в библиотеку полностью <w>доступен</w> для пользователей инвалидных колясок."
                            }
                        },
                        {
                            "text": "They are working to make the company website <w>accessible</w> for visually impaired people.",
                            "target_lang_translations": {
                                "ru": "Они работают над тем, чтобы сделать сайт компании <w>доступным</w> для людей с нарушениями зрения."
                            }
                        }
                    ],
                    "antonyms": [
                        "inaccessible"
                    ],
                    "common_phrases": [
                        "wheelchair accessible",
                        "accessible bathroom",
                        "accessible facilities",
                        "accessible design"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приспособленный для использования людьми с ограниченными возможностями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "досту́пный",
                                "target_lang_sense_clarification": "безбарьерный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "930526b8-6b3c-3552-88f2-416e8cbac1f0",
                    "sense_definition": "Able to be obtained, used, or seen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "These confidential documents are not <w>accessible</w> to the general public.",
                            "target_lang_translations": {
                                "ru": "Эти конфиденциальные документы не <w>доступны</w> широкой публике."
                            }
                        },
                        {
                            "text": "Information about the project is readily <w>accessible</w> on the internet.",
                            "target_lang_translations": {
                                "ru": "Информация о проекте легко <w>доступна</w> в интернете."
                            }
                        }
                    ],
                    "antonyms": [
                        "inaccessible"
                    ],
                    "common_phrases": [
                        "readily accessible",
                        "freely accessible",
                        "accessible information",
                        "accessible data"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который можно получить, использовать или увидеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "досту́пный",
                                "target_lang_sense_clarification": "имеющийся в наличии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66de9cae-1bb2-321b-b471-0e8910eea171",
                    "sense_definition": "Easy to understand or appreciate.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor wrote a book to make physics <w>accessible</w> to ordinary readers.",
                            "target_lang_translations": {
                                "ru": "Профессор написал книгу, чтобы сделать физику <w>доступной</w> для обычных читателей."
                            }
                        },
                        {
                            "text": "Her poetry is complex but still <w>accessible</w> enough for students to enjoy.",
                            "target_lang_translations": {
                                "ru": "Ее поэзия сложна, но все же достаточно <w>понятна</w>, чтобы студенты могли получить от нее удовольствие."
                            }
                        }
                    ],
                    "synonyms": [
                        "approachable"
                    ],
                    "antonyms": [
                        "inaccessible"
                    ],
                    "common_phrases": [
                        "accessible language",
                        "accessible style",
                        "accessible art",
                        "accessible to beginners"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкий для понимания или восприятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "досту́пный",
                                "target_lang_sense_clarification": "простой для понимания"
                            },
                            {
                                "target_lang_word": "поня́тный",
                                "target_lang_sense_clarification": "ясный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd386699-cff1-39e0-a49c-eae6211536e6",
                    "sense_definition": "Friendly and easy to talk to.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Despite his fame, the actor remained <w>accessible</w> and kind to his fans.",
                            "target_lang_translations": {
                                "ru": "Несмотря на свою славу, актер оставался <w>доступным</w> и добрым к своим поклонникам."
                            }
                        },
                        {
                            "text": "She is a very <w>accessible</w> manager who always listens to her staff.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>открытый</w> менеджер, который всегда выслушивает своих сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "approachable",
                        "easy-going",
                        "friendly",
                        "welcoming"
                    ],
                    "common_phrases": [
                        "accessible personality",
                        "accessible manner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дружелюбный и легкий в общении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "досту́пный",
                                "target_lang_sense_clarification": "открытый в общении"
                            },
                            {
                                "target_lang_word": "демократи́чный",
                                "target_lang_sense_clarification": "простой в обращении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "858c93ec-0518-3203-9e54-83262ecaf48f",
                    "sense_definition": "Open to being influenced or affected by something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was so angry that he was not <w>accessible</w> to reason.",
                            "target_lang_translations": {
                                "ru": "Он был так зол, что был не <w>доступен</w> доводам рассудка."
                            }
                        },
                        {
                            "text": "A mind <w>accessible</w> to new ideas is essential for personal growth.",
                            "target_lang_translations": {
                                "ru": "Разум, <w>восприимчивый</w> к новым идеям, необходим для личностного роста."
                            }
                        }
                    ],
                    "common_phrases": [
                        "accessible to reason",
                        "accessible to pity"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Открытый для влияния или воздействия чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восприи́мчивый",
                                "target_lang_sense_clarification": "к идеям или доводам"
                            },
                            {
                                "target_lang_word": "поддаю́щийся",
                                "target_lang_sense_clarification": "влиянию"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
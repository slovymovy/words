            "senses": [
                {
                    "sense_id": "ded31016-a68a-3a72-b34b-6cb0005ebbb2",
                    "sense_definition": "A Spanish surname, most notably associated with Francisco Franco, the dictator of Spain from 1939 to 1975.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "name_surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "The dictatorship of General <w>Franco</w> had a lasting impact on Spanish society.",
                            "target_lang_translations": {
                                "ru": "Диктатура генерала <w>Франко</w> оказала длительное влияние на испанское общество."
                            }
                        },
                        {
                            "text": "Historians have written extensively about the policies implemented by <w>Franco</w>.",
                            "target_lang_translations": {
                                "ru": "Историки много писали о политике, проводимой <w>Франко</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "Spanish origin"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Испанская фамилия, наиболее известная благодаря Франсиско Франко, диктатору Испании с 1939 по 1975 год."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Франко",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67bec865-15c5-3e46-8c1b-506fd52ae10a",
                    "sense_definition": "A male given name of Italian or Spanish origin.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "name_given",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My neighbor <w>Franco</w> helped me fix my car yesterday.",
                            "target_lang_translations": {
                                "ru": "Мой сосед <w>Франко</w> вчера помог мне починить машину."
                            }
                        },
                        {
                            "text": "They decided to name their new baby <w>Franco</w>.",
                            "target_lang_translations": {
                                "ru": "Они решили назвать своего новорожденного ребенка <w>Франко</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя итальянского или испанского происхождения."
                    },
                    "translations": {

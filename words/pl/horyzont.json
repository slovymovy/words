                    }
                },
                {
                    "sense_id": "8c3a0f81-3879-397e-9e7c-2e932937305a",
                    "sense_definition": "Nowe możliwości, perspektywy lub plany na przyszłość (zwykle w liczbie mnogiej).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_scope",
                    "name_type": "no",
                    "examples": [
                        {

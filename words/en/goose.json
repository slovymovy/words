                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Children played the game of <w>goose</w> in the parlor.",
                            "target_lang_translations": {
                                "ru": "Дети играли в <w>гусёк</w> в гостиной."
                            }

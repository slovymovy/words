                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The composer experimented with the <w>relationship</w> between major and minor keys in this piece.",
                            "target_lang_translations": {
                                "ru": "Композитор экспериментировал с <w>соотношением</w> мажорных и минорных тональностей в этом произведении."
                            }
                        },
                        {
                            "text": "Understanding the <w>relationship</w> of notes in a scale is fundamental to music theory.",
                            "target_lang_translations": {
                                "ru": "Понимание <w>взаимосвязи</w> нот в гамме фундаментально для теории музыки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tonal relationship",
                        "harmonic relationship"
                    ],
                    "target_lang_definitions": {
                        "ru": "Степень близости или гармонической связи между тональностями, аккордами и звуками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соотношение",
                                "target_lang_sense_clarification": "гармоническая связь"
                            },
                            {
                                "target_lang_word": "родство",
                                "target_lang_sense_clarification": "близость тональностей"
                            }
                        ]
                    }
                }
            ]
        }
    ]

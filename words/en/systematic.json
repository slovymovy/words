                {
                    "sense_id": "ec9f91b0-2dff-3c8b-b91f-33695c1e33bf",
                    "sense_definition": "Carried out according to a planned, ordered procedure.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "methodical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team conducted a <w>systematic</w> search of the entire building.",
                            "target_lang_translations": {
                                "ru": "Команда провела <w>методичный</w> обыск всего здания."
                            }

{
    "word_family": [
        "słoiczek",
        "słój",
        "słoikowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "df6beee9-cc24-3904-a075-869b7b96aee8",
                    "sense_definition": "Szklany, najczęściej walcowaty pojemnik z zakrętką, służący do przechowywania żywności.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na półce w spiżarni stoi duży <w>słoik</w> z domowym kompotem.",
                            "target_lang_translations": {
                                "ru": "На полке в кладовой стоит большая <w>банка</w> с домашним компотом."
                            }
                        },
                        {
                            "text": "Muszę kupić nowy <w>słoik</w> majonezu do sałatki jarzynowej.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую <w>банку</w> майонеза для овощного салата."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) krałza, krauza"
                    ],
                    "common_phrases": [
                        "słoik dżemu",
                        "słoik ogórków",
                        "odkręcić słoik",
                        "pusty słoik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стеклянный, чаще всего цилиндрический сосуд с крышкой, служащий для хранения продуктов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "банка",
                                "target_lang_sense_clarification": "стеклянная ёмкость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a0bf72f-696d-30bf-a05d-91ced9716d07",
                    "sense_definition": "Ilość produktu, która mieści się w jednym słoiku.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "measure_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do przygotowania tego ciasta potrzebujesz pół <w>słoika</w> miodu.",
                            "target_lang_translations": {
                                "ru": "Для приготовления этого пирога тебе понадобится пол-<w>банки</w> мёда."
                            }
                        },
                        {
                            "text": "Dzieci zjadły cały <w>słoik</w> czekoladowego kremu w jeden dzień.",
                            "target_lang_translations": {
                                "ru": "Дети съели целую <w>банку</w> шоколадного крема за один день."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pół słoika",
                        "cały słoik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество продукта, которое помещается в одной банке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "банка",
                                "target_lang_sense_clarification": "мера объёма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13d50dda-5159-3a4a-9ed7-8f617648318b",
                    "sense_definition": "Potoczne określenie osoby przyjezdnej, pracującej w Warszawie, która na weekendy wraca do rodzinnej miejscowości i przywozi stamtąd jedzenie w słoikach.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu moich znajomych to tak zwane <w>słoiki</w>, którzy ciężko pracują na swój sukces w stolicy.",
                            "target_lang_translations": {
                                "ru": "Многие мои знакомые — так называемые <w>приезжие</w>, которые тяжело работают ради своего успеха в столице."
                            }
                        },
                        {
                            "text": "W piątkowe popołudnia wylotówki z miasta są zablokowane, bo <w>słoiki</w> wracają do domów.",
                            "target_lang_translations": {
                                "ru": "В пятницу после обеда выезды из города заблокированы, так как <w>приезжие</w> возвращаются домой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "warszawskie słoiki",
                        "być słoikiem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często lekceważące"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "miejski"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название приезжего, работающего в Варшаве, который на выходные возвращается в родной город и привозит оттуда еду в банках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приезжий",
                                "target_lang_sense_clarification": "человек из провинции"
                            },
                            {
                                "target_lang_word": "провинциал",
                                "target_lang_sense_clarification": "житель провинции"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
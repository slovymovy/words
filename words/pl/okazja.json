{
    "word_family": [
        "okazyjny",
        "okazyjnie",
        "okazyjka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d21aee97-579e-3b7a-aa0a-54f436540907",
                    "sense_definition": "Sprzyjające warunki lub zbieg okoliczności, który pozwala na realizację jakiegoś zamierzenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "circumstance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To doskonała <w>okazja</w>, aby nauczyć się czegoś nowego.",
                            "target_lang_translations": {
                                "ru": "Это отличная <w>возможность</w>, чтобы научиться чему-то новому."
                            }
                        },
                        {
                            "text": "Jeśli będziesz w mieście, wpadnij do nas przy <w>okazji</w>.",
                            "target_lang_translations": {
                                "ru": "Если будешь в городе, заходи к нам при <w>случае</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "szansa",
                        "możliwość",
                        "sposobność",
                        "ewentualność"
                    ],
                    "common_phrases": [
                        "przy okazji",
                        "mieć okazję",
                        "korzystać z okazji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Благоприятные условия или стечение обстоятельств, позволяющие осуществить задуманное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возможность",
                                "target_lang_sense_clarification": "благоприятная ситуация"
                            },
                            {
                                "target_lang_word": "случай",
                                "target_lang_sense_clarification": "стечение обстоятельств"
                            },
                            {
                                "target_lang_word": "шанс",
                                "target_lang_sense_clarification": "вероятность успеха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "276ee639-00e8-3859-8784-a0012e002800",
                    "sense_definition": "Ważne wydarzenie, uroczystość lub święto, często stanowiące powód do świętowania.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z <w>okazji</w> twoich urodzin życzę ci wszystkiego najlepszego.",
                            "target_lang_translations": {
                                "ru": "По <w>случаю</w> твоего дня рождения желаю тебе всего наилучшего."
                            }
                        },
                        {
                            "text": "To wyjątkowa <w>okazja</w>, więc założę mój najlepszy garnitur.",
                            "target_lang_translations": {
                                "ru": "Это особый <w>случай</w>, поэтому я надену свой лучший костюм."
                            }
                        }
                    ],
                    "common_phrases": [
                        "z okazji",
                        "na tę okazję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Важное событие, торжество или праздник, часто являющиеся поводом для празднования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "случай",
                                "target_lang_sense_clarification": "событие, повод"
                            },
                            {
                                "target_lang_word": "событие",
                                "target_lang_sense_clarification": "важное происшествие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58f4f2ff-1404-38b0-a54b-1507d2ce0c04",
                    "sense_definition": "Przedmiot kupiony po bardzo korzystnej, niskiej cenie lub wyjątkowo dobra oferta handlowa.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "circumstance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem ten telewizor za połowę ceny, to była prawdziwa <w>okazja</w>.",
                            "target_lang_translations": {
                                "ru": "Я купил этот телевизор за полцены, это была настоящая <w>находка</w>."
                            }
                        },
                        {
                            "text": "W tym sklepie często trafia się jakaś <w>okazja</w> na markowe buty.",
                            "target_lang_translations": {
                                "ru": "В этом магазине часто попадается какая-нибудь <w>выгодная покупка</w> брендовой обуви."
                            }
                        }
                    ],
                    "synonyms": [
                        "gratka"
                    ],
                    "common_phrases": [
                        "prawdziwa okazja",
                        "łowca okazji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, купленный по очень выгодной, низкой цене, или исключительно выгодное торговое предложение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выгодная покупка",
                                "target_lang_sense_clarification": "удачное приобретение"
                            },
                            {
                                "target_lang_word": "находка",
                                "target_lang_sense_clarification": "перен. удачная покупка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1434839b-55c2-348f-9ef5-e356b117bea4",
                    "sense_definition": "Przypadkowy środek transportu jadący w pożądanym kierunku, którym można się zabrać.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie miałem pieniędzy na bilet, więc wróciłem do domu <w>okazją</w>.",
                            "target_lang_translations": {
                                "ru": "У меня не было денег на билет, поэтому я вернулся домой на <w>попутке</w>."
                            }
                        },
                        {
                            "text": "Podróżowanie <w>okazją</w> jest popularne wśród studentów.",
                            "target_lang_translations": {
                                "ru": "Путешествия на <w>попутках</w> популярны среди студентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "autostop"
                    ],
                    "common_phrases": [
                        "jechać okazją"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common usage regarding hitchhiking"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Попутный транспорт, идущий в нужном направлении, на котором можно доехать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попутка",
                                "target_lang_sense_clarification": "разг. попутная машина"
                            },
                            {
                                "target_lang_word": "оказия",
                                "target_lang_sense_clarification": "разг. попутный случай/транспорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c94d3441-2abd-3da6-9199-ebbb2dcecbcf",
                    "sense_definition": "Powód, przyczyna lub pretekst do podjęcia jakiegoś działania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każda <w>okazja</w> jest dobra, żeby uniknąć niechcianej pracy.",
                            "target_lang_translations": {
                                "ru": "Любой <w>повод</w> хорош, чтобы избежать нежелательной работы."
                            }
                        },
                        {
                            "text": "Szukał tylko <w>okazji</w>, by wywołać awanturę.",
                            "target_lang_translations": {
                                "ru": "Он искал лишь <w>повода</w>, чтобы затеять скандал."
                            }
                        }
                    ],
                    "synonyms": [
                        "powód",
                        "przyczyna",
                        "pretekst"
                    ],
                    "common_phrases": [
                        "szukać okazji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повод, причина или предлог для совершения какого-либо действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повод",
                                "target_lang_sense_clarification": "причина"
                            },
                            {
                                "target_lang_word": "предлог",
                                "target_lang_sense_clarification": "отговорка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "607a21bb-1bd4-3943-9610-de1e37f5168b",
                    "sense_definition": "Dawne określenie na bitwę lub krwawą potyczkę zbrojną.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "battle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerze ruszyli w pole, gdzie czekała ich krwawa <w>okazja</w>.",
                            "target_lang_translations": {
                                "ru": "Рыцари двинулись в поле, где их ждала кровавая <w>битва</w>."
                            }
                        },
                        {
                            "text": "Stare kroniki opisują wielką <w>okazję</w> z wojskami nieprzyjaciela.",
                            "target_lang_translations": {
                                "ru": "Старые хроники описывают великое <w>сражение</w> с войсками неприятеля."
                            }
                        }
                    ],
                    "synonyms": [
                        "bitwa",
                        "potyczka"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical meaning"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название битвы или кровавого вооруженного столкновения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "битва",
                                "target_lang_sense_clarification": "сражение"
                            },
                            {
                                "target_lang_word": "сражение",
                                "target_lang_sense_clarification": "военное столкновение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
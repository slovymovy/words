    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7e8f84b9-1a92-3239-858b-79bc55bf7b70",
                    "sense_definition": "Een personage uitbeelden in een film, toneelstuk of televisieprogramma.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "performance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij wil later graag in bekende films <w>acteren</w>.",
                            "target_lang_translations": {
                                "en": "She would like to <w>act</w> in famous films later.",
                                "ru": "В будущем она хочет <w>сниматься</w> в известных фильмах."
                            }
                        },
                        {
                            "text": "De hoofdrolspeler heeft in dit stuk geweldig <w>geacteerd</w>.",
                            "target_lang_translations": {
                                "en": "The lead actor <w>acted</w> magnificently in this play.",
                                "ru": "Исполнитель главной роли великолепно <w>сыграл</w> в этой пьесе."
                            }
                        }
                    ],
                    "synonyms": [
                        "toneelspelen",
                        "spelen",
                        "optreden"
                    ],
                    "common_phrases": [
                        "in een film acteren",
                        "goed kunnen acteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To portray a character in a film, play, or television program.",
                        "ru": "Исполнять роль персонажа в фильме, пьесе или телепрограмме."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "act",
                                "target_lang_sense_clarification": "perform a fictional role"
                            },
                            {
                                "target_lang_word": "perform",
                                "target_lang_sense_clarification": "present an artistic work"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "играть",
                                "target_lang_sense_clarification": "исполнять роль"
                            },
                            {
                                "target_lang_word": "сниматься",
                                "target_lang_sense_clarification": "в кино"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a35a2fb-670b-3dcf-9fdd-d96e28235f1a",
                    "sense_definition": "Doen alsof; gedrag vertonen dat niet echt is om anderen te misleiden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>acteerde</w> dat hij verrast was door het nieuws, maar hij wist het al.",
                            "target_lang_translations": {
                                "en": "He <w>pretended</w> to be surprised by the news, but he already knew.",
                                "ru": "Он <w>притворился</w>, что удивлен новостям, но он уже знал это."
                            }
                        },
                        {
                            "text": "Je hoeft niet te <w>acteren</w>, ik weet dat je het niet meent.",
                            "target_lang_translations": {
                                "en": "You don't have to <w>act</w>, I know you don't mean it.",
                                "ru": "Тебе не нужно <w>играть</w>, я знаю, что ты это не всерьез."
                            }
                        }
                    ],
                    "synonyms": [
                        "veinzen",
                        "doen alsof",
                        "simuleren"
                    ],
                    "common_phrases": [
                        "stoppen met acteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To pretend; to display behavior that is not real in order to deceive others.",
                        "ru": "Притворяться; вести себя неестественно с целью обмана."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pretend",
                                "target_lang_sense_clarification": "make believe"
                            },
                            {
                                "target_lang_word": "act",
                                "target_lang_sense_clarification": "behave artificially"
                            },
                            {
                                "target_lang_word": "feign",
                                "target_lang_sense_clarification": "fake a feeling or state"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "притворяться",
                                "target_lang_sense_clarification": "делать вид"
                            },
                            {
                                "target_lang_word": "играть",
                                "target_lang_sense_clarification": "изображать чувства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1b7d57f-c8fe-3a03-a8ac-ec55ef07ba00",
                    "sense_definition": "Daadkrachtig handelen of functioneren in een bepaalde rol of situatie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf moet snel <w>acteren</w> op de veranderende markt.",
                            "target_lang_translations": {
                                "en": "The company must <w>act</w> quickly in response to the changing market.",
                                "ru": "Компания должна быстро <w>действовать</w> на меняющемся рынке."
                            }
                        },
                        {
                            "text": "De verdediger kan in noodgevallen ook op het middenveld <w>acteren</w>.",
                            "target_lang_translations": {
                                "en": "In emergencies, the defender can also <w>operate</w> in the midfield.",
                                "ru": "Защитник в случае необходимости может также <w>выступать</w> в полузащите."
                            }
                        }
                    ],
                    "synonyms": [
                        "opereren",
                        "handelen",
                        "functioneren",
                        "optreden"
                    ],
                    "antonyms": [
                        "afwachten"
                    ],
                    "common_phrases": [
                        "op hoog niveau acteren",
                        "snel moeten acteren"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To take decisive action or function in a specific role or situation.",
                        "ru": "Решительно действовать или выполнять функции в определенной роли."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "act",
                                "target_lang_sense_clarification": "take action"
                            },
                            {
                                "target_lang_word": "operate",
                                "target_lang_sense_clarification": "function or work"
                            },
                            {
                                "target_lang_word": "function",
                                "target_lang_sense_clarification": "fulfill a role"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "действовать",
                                "target_lang_sense_clarification": "принимать меры"
                            },
                            {
                                "target_lang_word": "выступать",
                                "target_lang_sense_clarification": "функционировать"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "smoothly",
        "smoothness",
        "smoothen",
        "smoothie"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b2312b71-31e6-3274-aab9-226bedee22a9",
                    "sense_definition": "Having a flat, even surface that is not rough, bumpy, or scratched.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baby has very <w>smooth</w> skin that is soft to the touch.",
                            "target_lang_translations": {
                                "ru": "У ребенка очень <w>гладкая</w> кожа, мягкая на ощупь."
                            }
                        },
                        {
                            "text": "She polished the wooden table until it was completely <w>smooth</w>.",
                            "target_lang_translations": {
                                "ru": "Она полировала деревянный стол, пока он не стал совершенно <w>гладким</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "even",
                        "flat",
                        "level",
                        "silky",
                        "sleek",
                        "velvety"
                    ],
                    "antonyms": [
                        "rough",
                        "uneven",
                        "bumpy",
                        "coarse",
                        "textured"
                    ],
                    "common_phrases": [
                        "smooth skin",
                        "smooth surface",
                        "smooth texture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий плоскую, ровную поверхность, не шероховатый, не бугристый и не поцарапанный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гладкий",
                                "target_lang_sense_clarification": "не шероховатый"
                            },
                            {
                                "target_lang_word": "ровный",
                                "target_lang_sense_clarification": "плоский, без выступов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42914ce5-dcf2-3dd0-bb4f-3a928b1bcf47",
                    "sense_definition": "Happening without any difficulties, problems, or delays.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hope for a <w>smooth</w> transition to the new computer system.",
                            "target_lang_translations": {
                                "ru": "Мы надеемся на <w>гладкий</w> переход на новую компьютерную систему."
                            }
                        },
                        {
                            "text": "The flight was <w>smooth</w> and we arrived on time without issues.",
                            "target_lang_translations": {
                                "ru": "Полет прошел <w>спокойно</w>, и мы прибыли вовремя без проблем."
                            }
                        }
                    ],
                    "synonyms": [
                        "fluid",
                        "slick"
                    ],
                    "antonyms": [
                        "rough"
                    ],
                    "common_phrases": [
                        "smooth sailing",
                        "smooth transition",
                        "smooth operation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий без каких-либо трудностей, проблем или задержек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гладкий",
                                "target_lang_sense_clarification": "проходящий без проблем"
                            },
                            {
                                "target_lang_word": "беспрепятственный",
                                "target_lang_sense_clarification": "не встречающий преград"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0321f62c-00a4-3ff1-b4cd-4bcae2b1415e",
                    "sense_definition": "Moving in an even, continuous, and gentle way without sudden stops or jolts.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car gives a very <w>smooth</w> ride even on old country roads.",
                            "target_lang_translations": {
                                "ru": "У машины очень <w>плавный</w> ход даже на старых проселочных дорогах."
                            }
                        },
                        {
                            "text": "The pilot made a <w>smooth</w> landing despite the strong wind.",
                            "target_lang_translations": {
                                "ru": "Пилот совершил <w>мягкую</w> посадку, несмотря на сильный ветер."
                            }
                        }
                    ],
                    "antonyms": [
                        "rough",
                        "bumpy"
                    ],
                    "common_phrases": [
                        "smooth ride",
                        "smooth flow",
                        "smooth flight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Движущийся ровно, непрерывно и мягко, без внезапных остановок или толчков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плавный",
                                "target_lang_sense_clarification": "о движении"
                            },
                            {
                                "target_lang_word": "мягкий",
                                "target_lang_sense_clarification": "без тряски"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f87f53a5-ff2a-392c-8b10-7e526192831e",
                    "sense_definition": "Having a creamy texture or pleasant flavor that is not lumpy or bitter.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "taste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mix the flour and milk until you have a <w>smooth</w> paste.",
                            "target_lang_translations": {
                                "ru": "Смешивайте муку и молоко до получения <w>однородной</w> массы."
                            }
                        },
                        {
                            "text": "This coffee has a rich and <w>smooth</w> flavor that is easy to drink.",
                            "target_lang_translations": {
                                "ru": "У этого кофе насыщенный и <w>мягкий</w> вкус, который приятно пить."
                            }
                        }
                    ],
                    "synonyms": [
                        "velvety",
                        "silky"
                    ],
                    "antonyms": [
                        "coarse"
                    ],
                    "common_phrases": [
                        "smooth consistency",
                        "smooth flavor",
                        "smooth paste"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий кремообразную консистенцию или приятный вкус, без комков или горечи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "однородный",
                                "target_lang_sense_clarification": "о консистенции"
                            },
                            {
                                "target_lang_word": "мягкий",
                                "target_lang_sense_clarification": "о вкусе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56f8e27e-b29f-33d1-b706-b8cc18b6ff9a",
                    "sense_definition": "Charming, confident, and polite, often in a way that is intended to persuade people.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a <w>smooth</w> talker who can sell anything to anyone.",
                            "target_lang_translations": {
                                "ru": "Он <w>красноречивый</w> болтун, который может продать что угодно кому угодно."
                            }
                        },
                        {
                            "text": "The manager was very <w>smooth</w> and handled the complaint easily.",
                            "target_lang_translations": {
                                "ru": "Менеджер был очень <w>обходительным</w> и легко разобрался с жалобой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "smooth talker",
                        "smooth operator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обаятельный, уверенный в себе и вежливый, часто с целью убедить людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обходительный",
                                "target_lang_sense_clarification": "вежливый и приятный в общении"
                            },
                            {
                                "target_lang_word": "вкрадчивый",
                                "target_lang_sense_clarification": "умеющий войти в доверие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e83fd533-d945-3ffb-b928-068e01ca1a9f",
                    "sense_definition": "To make a surface flat and even by removing roughness or wrinkles.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She used an iron to <w>smooth</w> the wrinkles in her shirt.",
                            "target_lang_translations": {
                                "ru": "Она использовала утюг, чтобы <w>разгладить</w> складки на рубашке."
                            }
                        },
                        {
                            "text": "He tried to <w>smooth</w> down his hair before the interview.",
                            "target_lang_translations": {
                                "ru": "Он попытался <w>пригладить</w> волосы перед собеседованием."
                            }
                        }
                    ],
                    "synonyms": [
                        "smoothen"
                    ],
                    "common_phrases": [
                        "smooth out",
                        "smooth down"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать поверхность плоской и ровной, устраняя шероховатости или складки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разглаживать",
                                "target_lang_sense_clarification": "убирать складки"
                            },
                            {
                                "target_lang_word": "сглаживать",
                                "target_lang_sense_clarification": "делать ровным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afdeb8ee-1595-3691-9b99-c285ae6256a0",
                    "sense_definition": "To remove difficulties or problems from a situation to make it easier.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team worked hard to <w>smooth</w> the way for the new project.",
                            "target_lang_translations": {
                                "ru": "Команда усердно работала, чтобы <w>расчистить</w> путь для нового проекта."
                            }
                        },
                        {
                            "text": "He tried to <w>smooth</w> things over after the argument.",
                            "target_lang_translations": {
                                "ru": "Он попытался <w>сгладить</w> ситуацию после ссоры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "smooth the way",
                        "smooth things over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устранять трудности или проблемы из ситуации, чтобы облегчить её."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сглаживать",
                                "target_lang_sense_clarification": "смягчать конфликт"
                            },
                            {
                                "target_lang_word": "улаживать",
                                "target_lang_sense_clarification": "решать проблемы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "44475711-c25a-3e70-bee9-5e4d20095726",
                    "sense_definition": "In a smooth manner; without problems or roughness.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The course of true love never did run <w>smooth</w>.",
                            "target_lang_translations": {
                                "ru": "Путь истинной любви никогда не бывал <w>гладким</w>."
                            }
                        },
                        {
                            "text": "The engine runs <w>smooth</w> now that it has been fixed.",
                            "target_lang_translations": {
                                "ru": "Двигатель работает <w>ровно</w> теперь, когда его починили."
                            }
                        }
                    ],
                    "common_phrases": [
                        "run smooth"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used as an adverb in informal speech or fixed phrases"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Плавно; без проблем или шероховатостей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гладко",
                                "target_lang_sense_clarification": "без проблем"
                            },
                            {
                                "target_lang_word": "ровно",
                                "target_lang_sense_clarification": "без перебоев"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3a283048-0f34-37e9-8ed8-f7b9f67ce2a6",
                    "sense_definition": "The smooth part of something, or a smooth state.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You have to take the rough with the <w>smooth</w> in this job.",
                            "target_lang_translations": {
                                "ru": "На этой работе нужно принимать и горести, и <w>радости</w>."
                            }
                        },
                        {
                            "text": "She felt the <w>smooth</w> of the polished wood under her hand.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала <w>гладкость</w> полированного дерева под рукой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rough with the smooth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гладкая часть чего-либо или спокойное состояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гладкость",
                                "target_lang_sense_clarification": "свойство поверхности"
                            },
                            {
                                "target_lang_word": "приятное",
                                "target_lang_sense_clarification": "в идиомах о хороших моментах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
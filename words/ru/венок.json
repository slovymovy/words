    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "566742cc-fe3c-3d79-9d86-b35d99a2afe0",
                    "sense_definition": "Украшение в виде кольца из сплетённых цветов, листьев или веток, которое обычно носят на голове.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "wreath_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом девочки любили плести <w>венок</w> из жёлтых одуванчиков.",
                            "target_lang_translations": {
                                "en": "In summer, the girls loved to weave a <w>wreath</w> of yellow dandelions."
                            }
                        },
                        {
                            "text": "На праздник Ивана Купала принято пускать <w>венок</w> по воде.",
                            "target_lang_translations": {
                                "en": "On the Ivan Kupala holiday, it is customary to float a <w>wreath</w> on the water."
                            }
                        }
                    ],
                    "synonyms": [
                        "венец"
                    ],
                    "common_phrases": [
                        "плести венок",
                        "венок из цветов",
                        "венок из одуванчиков",
                        "пускать венок"
                    ],
                    "target_lang_definitions": {
                        "en": "An ornament shaped like a ring, made of woven flowers, leaves, or branches, usually worn on the head."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wreath",
                                "target_lang_sense_clarification": "woven flowers worn on the head"
                            },
                            {
                                "target_lang_word": "garland",
                                "target_lang_sense_clarification": "floral decoration"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68e95841-b04d-3c08-af5c-c62997acba6b",
                    "sense_definition": "Композиция из цветов и веток в форме круга или овала, которую возлагают на могилу или к памятнику в знак памяти.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "wreath_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Президент торжественно возложил <w>венок</w> к могиле Неизвестного Солдата.",
                            "target_lang_translations": {
                                "en": "The President solemnly laid a <w>wreath</w> at the Tomb of the Unknown Soldier."
                            }
                        },
                        {
                            "text": "Родственники принесли на кладбище большой траурный <w>венок</w>.",
                            "target_lang_translations": {
                                "en": "The relatives brought a large funeral <w>wreath</w> to the cemetery."
                            }
                        }
                    ],
                    "common_phrases": [
                        "возложить венок",
                        "траурный венок",
                        "похоронный венок"
                    ],
                    "target_lang_definitions": {
                        "en": "A composition of flowers and branches in a circular or oval shape, placed on a grave or monument in memory of someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wreath",
                                "target_lang_sense_clarification": "funeral or memorial tribute"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77f84aa8-e7c6-3d90-bcf0-bc5156df18d5",
                    "sense_definition": "Форма родительного или винительного падежа множественного числа существительного <w>венка</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На ногах пожилой женщины было видно множество вздувшихся <w>венок</w>.",
                            "target_lang_translations": {
                                "en": "Many swollen <w>small veins</w> were visible on the elderly woman's legs."
                            }
                        },
                        {
                            "text": "Врач внимательно осмотрел сетку мелких <w>венок</w> на руке пациента.",
                            "target_lang_translations": {
                                "en": "The doctor carefully examined the network of tiny <w>veins</w> on the patient's arm."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive/accusative plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The genitive or accusative plural form of the noun <w>венка</w> (small vein)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "small vein",
                                "target_lang_sense_clarification": "diminutive form"
                            },
                            {
                                "target_lang_word": "vein",
                                "target_lang_sense_clarification": "blood vessel"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
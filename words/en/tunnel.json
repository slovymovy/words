{
    "word_family": [
        "tunneller",
        "tunnelling"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "48876723-e9a5-315d-9f14-6c08dcca3026",
                    "sense_definition": "An underground or underwater passage for people, trains, or vehicles.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_passage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train went into the dark <w>tunnel</w> and the lights flickered.",
                            "target_lang_translations": {
                                "ru": "Поезд въехал в темный <w>тоннель</w>, и свет замерцал."
                            }
                        },
                        {
                            "text": "Engineers had to build a long <w>tunnel</w> through the mountain to connect the two cities.",
                            "target_lang_translations": {
                                "ru": "Инженерам пришлось построить длинный <w>тоннель</w> сквозь гору, чтобы соединить два города."
                            }
                        }
                    ],
                    "synonyms": [
                        "undermine"
                    ],
                    "common_phrases": [
                        "railway tunnel",
                        "light at the end of the tunnel",
                        "wind tunnel",
                        "long tunnel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подземный или подводный проход для людей, поездов или транспортных средств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тоннель",
                                "target_lang_sense_clarification": "сооружение"
                            },
                            {
                                "target_lang_word": "туннель",
                                "target_lang_sense_clarification": "вариант написания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e53c2d85-121f-3cfe-b2b5-726c4b054bf7",
                    "sense_definition": "A hole or passage in the ground made by an animal.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_burrow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rabbit disappeared into its <w>tunnel</w> to escape the fox.",
                            "target_lang_translations": {
                                "ru": "Кролик скрылся в своей <w>норе</w>, чтобы убежать от лисы."
                            }
                        },
                        {
                            "text": "We found a small <w>tunnel</w> in the garden where a mole had been digging.",
                            "target_lang_translations": {
                                "ru": "Мы нашли в саду маленький <w>подземный ход</w>, где копал крот."
                            }
                        }
                    ],
                    "synonyms": [
                        "undermine"
                    ],
                    "common_phrases": [
                        "rabbit tunnel",
                        "dig a tunnel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отверстие или проход в земле, сделанный животным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нора",
                                "target_lang_sense_clarification": "жилище животного"
                            },
                            {
                                "target_lang_word": "подземный ход",
                                "target_lang_sense_clarification": "прорытый путь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4110cf54-8c14-3d3a-9b4c-180761101d5e",
                    "sense_definition": "A method in computing for sending data securely or through a blocked network by wrapping it in another protocol.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The VPN created a secure <w>tunnel</w> for our internet traffic.",
                            "target_lang_translations": {
                                "ru": "VPN создал безопасный <w>туннель</w> для нашего интернет-трафика."
                            }
                        },
                        {
                            "text": "Hackers used a <w>tunnel</w> to bypass the firewall restrictions.",
                            "target_lang_translations": {
                                "ru": "Хакеры использовали <w>туннель</w>, чтобы обойти ограничения брандмауэра."
                            }
                        }
                    ],
                    "common_phrases": [
                        "encrypted tunnel",
                        "secure tunnel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Метод в информатике для безопасной отправки данных или обхода блокировок сети путем их упаковки в другой протокол."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туннель",
                                "target_lang_sense_clarification": "канал связи"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f1a1bb09-e039-3820-a373-c49faef3c813",
                    "sense_definition": "To dig or make a passage through or under something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_dig",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prisoners planned to <w>tunnel</w> under the wall to escape.",
                            "target_lang_translations": {
                                "ru": "Заключенные планировали <w>сделать подкоп</w> под стеной, чтобы сбежать."
                            }
                        },
                        {
                            "text": "Moles <w>tunnel</w> through the soil in search of food.",
                            "target_lang_translations": {
                                "ru": "Кроты <w>роют ходы</w> в почве в поисках пищи."
                            }
                        }
                    ],
                    "synonyms": [
                        "undermine"
                    ],
                    "common_phrases": [
                        "tunnel through",
                        "tunnel under",
                        "tunnel out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рыть или прокладывать проход сквозь или под чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыть туннель",
                                "target_lang_sense_clarification": "создавать проход"
                            },
                            {
                                "target_lang_word": "делать подкоп",
                                "target_lang_sense_clarification": "копать под чем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c55367a5-64c2-3936-8ffe-3f19b64ac2fb",
                    "sense_definition": "To transmit data through a network by encapsulating it within another protocol.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>tunnel</w> the connection through a proxy server.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>туннелировать</w> соединение через прокси-сервер."
                            }
                        },
                        {
                            "text": "The software allows users to <w>tunnel</w> their web traffic securely.",
                            "target_lang_translations": {
                                "ru": "Программа позволяет пользователям безопасно <w>туннелировать</w> свой веб-трафик."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать данные через сеть, инкапсулируя их внутри другого протокола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туннелировать",
                                "target_lang_sense_clarification": "инкапсулировать трафик"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
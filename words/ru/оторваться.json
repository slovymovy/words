            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1cbd5e75-2a6d-3367-96da-2ef227ac7550",
                    "sense_definition": "Отделиться, отпасть от чего-либо (обычно в результате натяжения, износа или повреждения).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня на пальто случайно <оторвалась> пуговица."
                        },
                        {
                            "text": "Когда я открывал дверь, ручка не выдержала и <оторвалась>."
                        }
                    ],
                    "synonyms": [
                        "отвалиться",
                        "отделиться"
                    ],
                    "antonyms": [
                        "прикрепиться",
                        "держаться"
                    ],
                    "common_phrases": [
                        "пуговица оторвалась",
                        "оторваться с мясом",
                        "легко оторваться"
                    ]
                },
                {
                    "sense_id": "17d1f7b0-e06d-3574-90b8-b052a7aa7a46",
                    "sense_definition": "Перестать смотреть на кого-либо или заниматься чем-либо; переключить внимание.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Книга была настолько захватывающей, что я не мог <оторваться> ни на минуту."
                        },
                        {
                            "text": "Ему пришлось с трудом <оторваться> от работы, чтобы ответить на телефонный звонок."
                        }
                    ],
                    "synonyms": [
                        "отвлечься"
                    ],
                    "antonyms": [
                        "сосредоточиться",
                        "уставиться"
                    ],
                    "common_phrases": [
                        "не оторваться",
                        "оторваться от дел",
                        "оторваться от книги",
                        "с трудом оторваться"
                    ]
                },
                {
                    "sense_id": "8480ebcc-0bc8-3296-94d0-945848dbbf97",
                    "sense_definition": "Уйти вперёд, увеличить расстояние от преследователей или соперников (в спорте или погоне).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "distance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бегун смог <оторваться> от основной группы спортсменов на финишной прямой."
                        },
                        {
                            "text": "Машине удалось <оторваться> от погони и скрыться в переулках."

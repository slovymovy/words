    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8ce9bb86-f422-3e21-9075-2b0af36dbc2e",
                    "sense_definition": "Of or pertaining to a mother; having the characteristics of a mother; motherly.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "motherly_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His weakness seemed to bring out her strong <w>maternal</w> instincts.",
                            "target_lang_translations": {
                                "ru": "Его слабость, казалось, пробудила в ней сильные <w>материнские</w> инстинкты."
                            }
                        },
                        {
                            "text": "The nurse provided excellent <w>maternal</w> care to the women in the ward.",
                            "target_lang_translations": {
                                "ru": "Медсестра обеспечила женщинам в палате превосходный <w>материнский</w> уход."
                            }
                        }
                    ],
                    "synonyms": [
                        "motherly"
                    ],
                    "antonyms": [
                        "paternal",
                        "fatherly"
                    ],
                    "common_phrases": [
                        "maternal instinct",
                        "maternal care",
                        "maternal love"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к матери; обладающий качествами матери; свойственный матери."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "материнский",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56370c59-79b4-36bd-aff0-d2fb33a8f409",
                    "sense_definition": "Related through the mother, or her side of the family.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "kinship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toby is my <w>maternal</w> uncle, the brother of my mother.",
                            "target_lang_translations": {
                                "ru": "Тоби — мой дядя <w>по материнской линии</w>, брат моей мамы."
                            }
                        },
                        {
                            "text": "She inherited the green eyes from her <w>maternal</w> grandmother.",
                            "target_lang_translations": {
                                "ru": "Она унаследовала зеленые глаза от бабушки <w>по материнской линии</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "paternal"
                    ],
                    "common_phrases": [
                        "maternal uncle",
                        "maternal grandfather",
                        "maternal grandmother",
                        "maternal side"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный родством через мать или её сторону семьи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по материнской линии",
                                "target_lang_sense_clarification": "о родстве"
                            },
                            {
                                "target_lang_word": "с материнской стороны",
                                "target_lang_sense_clarification": "о родственниках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4715a715-d3e0-35ef-8246-6a48bd72e26f",
                    "sense_definition": "Derived from the mother as opposed to the fetus during pregnancy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doctors must carefully monitor <w>maternal</w> health throughout the pregnancy.",
                            "target_lang_translations": {
                                "ru": "Врачи должны тщательно следить за <w>материнским</w> здоровьем на протяжении всей беременности."
                            }
                        },
                        {
                            "text": "The test distinguishes between <w>maternal</w> cells and those of the unborn baby.",
                            "target_lang_translations": {
                                "ru": "Тест различает <w>материнские</w> клетки и клетки нерожденного ребенка."
                            }
                        }
                    ],
                    "antonyms": [
                        "fetal",
                        "foetal"
                    ],
                    "common_phrases": [
                        "maternal health",
                        "maternal mortality",
                        "maternal cells"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к организму матери (в отличие от плода) во время беременности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "материнский",
                                "target_lang_sense_clarification": "биологический/медицинский термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [

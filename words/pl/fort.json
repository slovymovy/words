{
    "word_family": [
        "forteca",
        "fortyfikacja",
        "fortyfikować",
        "forteczny"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "01bf1dfd-ac79-323b-84e7-82e3494b3a32",
                    "sense_definition": "Samodzielna budowla o charakterze obronnym, przystosowana do walki okrężnej, często wchodząca w skład większej twierdzy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "defensive_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przypuszczono zmasowany atak na <fort> północny.",
                            "target_lang_translations": {
                                "ru": "Был предпринят массированный налет на северный <форт>."
                            }
                        },
                        {
                            "text": "Turyści chętnie zwiedzają stary <fort> położony na wzgórzu.",
                            "target_lang_translations": {
                                "ru": "Туристы охотно посещают старый <форт>, расположенный на холме."
                            }
                        }
                    ],
                    "synonyms": [
                        "twierdza"
                    ],
                    "common_phrases": [
                        "zdobyć fort",
                        "załoga fortu",
                        "bronić fortu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельное оборонительное сооружение, приспособленное к круговой обороне, часто входящее в состав крепости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "форт",
                                "target_lang_sense_clarification": "военное укрепление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2878871c-1bcf-3b44-854b-ab75f64621e5",
                    "sense_definition": "Ciągle, bezustannie, wciąż (używane w gwarach).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "continuously",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko <fort> płakało i nie mogło się uspokoić.",
                            "target_lang_translations": {
                                "ru": "Ребенок <постоянно> плакал и не мог успокоиться."
                            }
                        },
                        {
                            "text": "On <fort> gada o swoich problemach.",
                            "target_lang_translations": {
                                "ru": "Он <без умолку> болтает о своих проблемах."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciągle"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "używane m.in. w gwarze łowickiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянно, непрерывно, всё время (употребляется в диалектах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постоянно",
                                "target_lang_sense_clarification": "непрерывно"
                            },
                            {
                                "target_lang_word": "всё время",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "0cb2fddb-92b6-3a3f-9c02-af7db7e0ac1e",
                    "sense_definition": "Okrzyk nakazujący komuś natychmiastowe oddalenie się.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "go_away",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Fort> stąd, nie chcę cię więcej widzieć!",
                            "target_lang_translations": {
                                "ru": "<Вон> отсюда, я не хочу тебя больше видеть!"
                            }
                        },
                        {
                            "text": "Zabieraj się <fort> z mojego domu!",
                            "target_lang_translations": {
                                "ru": "Убирайся <прочь> из моего дома!"
                            }
                        }
                    ],
                    "synonyms": [
                        "precz"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "rzadko używane współcześnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, требующий, чтобы кто-то немедленно ушел; прочь, вон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прочь",
                                "target_lang_sense_clarification": "приказ уйти"
                            },
                            {
                                "target_lang_word": "вон",
                                "target_lang_sense_clarification": "грубый приказ"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
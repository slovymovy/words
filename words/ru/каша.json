            "senses": [
                {
                    "sense_id": "8c0492cf-a783-3e1c-a5e0-87d11f93622a",
                    "sense_definition": "Густое кушанье из крупы, сваренной на воде или молоке.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На завтрак дети часто едят овсяную <w>кашу</w>.",
                            "target_lang_translations": {
                                "en": "Children often eat oatmeal <w>porridge</w> for breakfast."
                            }
                        },
                        {
                            "text": "Гречневая <w>каша</w> с молоком очень полезна для здоровья.",
                            "target_lang_translations": {
                                "en": "Buckwheat <w>porridge</w> with milk is very healthy."
                            }
                        }
                    ],
                    "common_phrases": [
                        "манная каша",
                        "гречневая каша",
                        "овсяная каша",
                        "варить кашу"
                    ],
                    "target_lang_definitions": {
                        "en": "A thick dish made of grains boiled in water or milk."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "porridge",
                                "target_lang_sense_clarification": "general term for boiled grain dishes"
                            },
                            {
                                "target_lang_word": "kasha",
                                "target_lang_sense_clarification": "specifically buckwheat or roasted grain porridge"
                            },
                            {
                                "target_lang_word": "oatmeal",
                                "target_lang_sense_clarification": "porridge made specifically from oats"
                            },
                            {
                                "target_lang_word": "mush",
                                "target_lang_sense_clarification": "thick, soft porridge, often cornmeal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69cd0347-1816-3cd8-a9b3-a2231b6d58b1",
                    "sense_definition": "Беспорядок, путаница, отсутствие ясности в мыслях или делах.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "confusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После бессонной ночи у меня в голове полная <w>каша</w>.",
                            "target_lang_translations": {
                                "en": "After a sleepless night, my head is a complete <w>mess</w>."
                            }
                        },
                        {
                            "text": "В документах была такая <w>каша</w>, что мы ничего не нашли.",
                            "target_lang_translations": {
                                "en": "There was such a <w>muddle</w> in the documents that we found nothing."
                            }
                        }
                    ],
                    "synonyms": [
                        "беспорядок",
                        "неразбериха",
                        "путаница"
                    ],
                    "antonyms": [
                        "порядок",
                        "ясность",
                        "упорядоченность"
                    ],
                    "common_phrases": [
                        "каша в голове",
                        "полная каша"
                    ],
                    "traits": [

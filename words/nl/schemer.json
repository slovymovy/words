    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bbfce56b-e981-35c5-9ee9-555f095216c4",
                    "sense_definition": "De periode van verminderend licht tussen dag en nacht; het halfdonker.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bleven gezellig buiten zitten in de <w>schemer</w>.",
                            "target_lang_translations": {
                                "en": "We stayed sitting outside pleasantly in the <w>dusk</w>.",
                                "ru": "Мы уютно сидели на улице в <w>сумерках</w>."
                            }
                        },
                        {
                            "text": "Door de <w>schemer</w> kon ik de weg niet goed meer zien.",
                            "target_lang_translations": {
                                "en": "Because of the <w>twilight</w>, I could no longer see the road well.",
                                "ru": "Из-за <w>сумерек</w> я больше не мог хорошо разглядеть дорогу."
                            }
                        }
                    ],
                    "synonyms": [
                        "schemering",
                        "halfduister",
                        "tweeduister"
                    ],
                    "antonyms": [
                        "daglicht",
                        "duisternis"
                    ],
                    "common_phrases": [
                        "in de schemer"
                    ],
                    "target_lang_definitions": {
                        "en": "The period of diminishing light between day and night; the semi-darkness.",
                        "ru": "Период угасающего света между днем и ночью; полумрак."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dusk",
                                "target_lang_sense_clarification": "The darker stage of twilight, just before night"
                            },
                            {
                                "target_lang_word": "twilight",
                                "target_lang_sense_clarification": "The soft light from the sky when the sun is below the horizon"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сумерки",
                                "target_lang_sense_clarification": "Время суток между заходом солнца и наступлением ночи"
                            },
                            {
                                "target_lang_word": "полумрак",
                                "target_lang_sense_clarification": "Слабое освещение, неполная темнота"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cc1cb246-4854-3318-90e4-1e9404119283",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>schemeren</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>schemer</w> tussen waken en slapen.",
                            "target_lang_translations": {
                                "en": "I <w>drift</w> between waking and sleeping.",
                                "ru": "Я <w>блуждаю</w> между сном и явью."
                            }
                        },
                        {
                            "text": "In dit gedicht <w>schemer</w> ik als een geest door het huis.",
                            "target_lang_translations": {
                                "en": "In this poem I <w>glimmer</w> like a ghost through the house.",
                                "ru": "В этом стихотворении я <w>мелькаю</w> как призрак по дому."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "stem of the verb"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of <w>schemeren</w>.",
                        "ru": "Первое лицо единственного числа настоящего времени от глагола <w>schemeren</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "glimmer",
                                "target_lang_sense_clarification": "To shine faintly or unsteadily"
                            },
                            {
                                "target_lang_word": "drift",
                                "target_lang_sense_clarification": "To be in an indeterminate state (figurative)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мерцать",
                                "target_lang_sense_clarification": "Светиться слабым, неровным светом"
                            },
                            {
                                "target_lang_word": "блуждать",
                                "target_lang_sense_clarification": "Находиться в неопределенном состоянии (переносно)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
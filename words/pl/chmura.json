            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3517ad5e-376c-367d-ab2e-8589e1c5d119",
                    "sense_definition": "Widoczne skupisko kropel wody lub kryształków lodu zawieszone w atmosferze, często przynoszące deszcz lub śnieg.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "suspension_in_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na niebie pojawiły się nagle deszczowe <w>chmury</w>.",
                            "target_lang_translations": {
                                "ru": "На небе внезапно появились дождевые <w>тучи</w>."
                            }
                        },
                        {
                            "text": "Samolot wzbił się wysoko ponad białe <w>chmury</w>.",
                            "target_lang_translations": {
                                "ru": "Самолет взмыл высоко над белыми <w>облаками</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "obłok"
                    ],
                    "common_phrases": [
                        "bujać w chmurach",
                        "czarne chmury",
                        "chmura burzowa",
                        "bez chmur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Видимое скопление капель воды или ледяных кристаллов в атмосфере, часто приносящее дождь или снег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туча",
                                "target_lang_sense_clarification": "дождевая или грозовая"
                            },
                            {
                                "target_lang_word": "облако",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8af53ac6-8380-3ff3-82f7-121dec55269e",
                    "sense_definition": "Sieć serwerów internetowych umożliwiająca przechowywanie danych i korzystanie z usług bez instalowania ich na własnym komputerze.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza firma pracuje nad nową usługą, umożliwiającą przechowywanie danych w <w>chmurze</w>.",
                            "target_lang_translations": {
                                "ru": "Наша компания работает над новой услугой, позволяющей хранить данные в <w>облаке</w>."
                            }
                        },
                        {
                            "text": "Wszystkie zdjęcia z wakacji trzymam bezpiecznie w <w>chmurze</w>.",
                            "target_lang_translations": {
                                "ru": "Все фотографии с отпуска я безопасно храню в <w>облаке</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "w chmurze",
                        "dane w chmurze",
                        "obliczenia w chmurze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сеть интернет-серверов, позволяющая хранить данные и использовать сервисы без установки на собственный компьютер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облако",
                                "target_lang_sense_clarification": "информатика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8fee50c-94d9-3b18-be46-2e8679333cc3",
                    "sense_definition": "Gęsta masa dymu, pyłu lub owadów unosząca się w powietrzu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "suspension_in_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wybuchu wulkanu nad miastem uniosła się wielka <w>chmura</w> pyłu.",
                            "target_lang_translations": {
                                "ru": "После извержения вулкана над городом поднялось огромное <w>облако</w> пыли."
                            }
                        },
                        {
                            "text": "Stary samochód zostawił za sobą śmierdzącą <w>chmurę</w> spalin.",
                            "target_lang_translations": {
                                "ru": "Старый автомобиль оставил за собой вонючее <w>облако</w> выхлопных газов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chmura dymu",
                        "chmura pyłu",
                        "chmura spalin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плотная масса дыма, пыли или насекомых, парящая в воздухе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облако",
                                "target_lang_sense_clarification": "дыма или пыли"
                            },
                            {
                                "target_lang_word": "туча",
                                "target_lang_sense_clarification": "насекомых или очень плотная масса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b845c24-f266-340a-bb0f-7a8c43c82033",
                    "sense_definition": "Dozorca lub stróż pilnujący porządku w budynku.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz osiedlowy <w>chmura</w> zawsze zamyka bramę o dziesiątej wieczorem.",
                            "target_lang_translations": {
                                "ru": "Наш местный <w>дворник</w> всегда закрывает ворота в десять вечера."
                            }
                        },
                        {
                            "text": "Dzieci uciekały z podwórka, gdy tylko zobaczyły, że idzie <w>chmura</w>.",
                            "target_lang_translations": {
                                "ru": "Дети убегали со двора, как только видели, что идет <w>дворник</w>."
                            }
                        }
                    ],
                    "traits": [
                        {

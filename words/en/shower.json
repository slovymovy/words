{
    "word_family": [
        "showery",
        "rainshower",
        "thundershower"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "46b714e2-1e6f-3055-ba12-ade5413329c9",
                    "sense_definition": "An act of washing the body under a spray of water.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "hygiene_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to take a quick <w>shower</w> before we go out.",
                            "target_lang_translations": {
                                "ru": "Мне нужно быстро принять <w>душ</w> перед выходом."
                            }
                        },
                        {
                            "text": "A cold <w>shower</w> is very refreshing in the summer.",
                            "target_lang_translations": {
                                "ru": "Холодный <w>душ</w> очень освежает летом."
                            }
                        }
                    ],
                    "synonyms": [
                        "shower bath"
                    ],
                    "common_phrases": [
                        "take a shower",
                        "have a shower",
                        "cold shower",
                        "hot shower"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс мытья тела под струями воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "душ",
                                "target_lang_sense_clarification": "процесс мытья"
                            },
                            {
                                "target_lang_word": "мытьё",
                                "target_lang_sense_clarification": "гигиеническая процедура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc35a6bd-9c39-3c31-bd1f-b419ef4682e3",
                    "sense_definition": "A brief period of rain, snow, or hail.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The weather forecast predicts scattered <w>showers</w> this afternoon.",
                            "target_lang_translations": {
                                "ru": "Прогноз погоды обещает местами <w>кратковременные дожди</w> сегодня днем."
                            }
                        },
                        {
                            "text": "We got caught in a sudden <w>shower</w> without an umbrella.",
                            "target_lang_translations": {
                                "ru": "Мы попали под внезапный <w>ливень</w> без зонта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "scattered showers",
                        "heavy showers",
                        "rain shower",
                        "snow shower"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кратковременный дождь, снег или град."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ливень",
                                "target_lang_sense_clarification": "сильный дождь"
                            },
                            {
                                "target_lang_word": "кратковременный дождь",
                                "target_lang_sense_clarification": "метеорологическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26293592-2433-3245-a10f-6d7db2521aaf",
                    "sense_definition": "A fixture or apparatus that sprays water for bathing.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hygiene_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>shower</w> in the hotel bathroom has excellent water pressure.",
                            "target_lang_translations": {
                                "ru": "В <w>душе</w> в ванной отеля отличный напор воды."
                            }
                        },
                        {
                            "text": "Please clean the <w>shower</w> after you use it.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, помой <w>душевую кабину</w> после использования."
                            }
                        }
                    ],
                    "synonyms": [
                        "shower bath"
                    ],
                    "common_phrases": [
                        "shower head",
                        "shower curtain",
                        "shower stall",
                        "electric shower"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сантехническое устройство для мытья тела струями воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "душ",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "душевая кабина",
                                "target_lang_sense_clarification": "помещение/кабина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69bc9b22-e7e8-39e1-ade0-3440bc1d93e9",
                    "sense_definition": "A party held to give gifts to a person for a special occasion, such as a wedding or a birth.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She invited all her friends to her baby <w>shower</w> next weekend.",
                            "target_lang_translations": {
                                "ru": "Она пригласила всех подруг на <w>вечеринку в честь будущего ребенка</w> в следующие выходные."
                            }
                        },
                        {
                            "text": "We are planning a surprise bridal <w>shower</w> for my sister.",
                            "target_lang_translations": {
                                "ru": "Мы планируем сюрприз-вечеринку (<w>девичник</w>) для моей сестры перед свадьбой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "baby shower",
                        "bridal shower",
                        "wedding shower"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "originally North American"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вечеринка, устраиваемая для вручения подарков по особому случаю (обычно перед свадьбой или рождением ребенка)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вечеринка",
                                "target_lang_sense_clarification": "праздник с подарками"
                            },
                            {
                                "target_lang_word": "девичник",
                                "target_lang_sense_clarification": "перед свадьбой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8916c3bf-75cd-3d27-9d93-1f2f065b80b8",
                    "sense_definition": "A large number of things falling or moving together.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abundance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We stayed up late to watch the meteor <w>shower</w> in the night sky.",
                            "target_lang_translations": {
                                "ru": "Мы не спали допоздна, чтобы посмотреть на метеорный <w>дождь</w> в ночном небе."
                            }
                        },
                        {
                            "text": "A <w>shower</w> of sparks flew from the grinding wheel.",
                            "target_lang_translations": {
                                "ru": "<w>Сноп</w> искр вылетел из-под шлифовального круга."
                            }
                        }
                    ],
                    "common_phrases": [
                        "meteor shower",
                        "shower of sparks",
                        "shower of arrows"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество чего-либо, падающего или движущегося вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "обилие движущихся объектов"
                            },
                            {
                                "target_lang_word": "град",
                                "target_lang_sense_clarification": "перен. множество падающих объектов"
                            },
                            {
                                "target_lang_word": "сноп",
                                "target_lang_sense_clarification": "искр или света"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60a9ad43-1f09-3824-86dd-51a589bc0afc",
                    "sense_definition": "A group of people perceived as incompetent or worthless.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "pejorative_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new management team is an absolute <w>shower</w>.",
                            "target_lang_translations": {
                                "ru": "Новое руководство — это просто какое-то <w>сборище</w>."
                            }
                        },
                        {
                            "text": "I cannot believe we have to listen to this <w>shower</w> of fools.",
                            "target_lang_translations": {
                                "ru": "Не могу поверить, что нам приходится слушать это <w>сборище</w> дураков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "what a shower",
                        "shower of idiots"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "derogatory"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, воспринимаемых как некомпетентные или никчемные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сборище",
                                "target_lang_sense_clarification": "пренебрежительно"
                            },
                            {
                                "target_lang_word": "шайка",
                                "target_lang_sense_clarification": "разг. группа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9eac530-97f4-3eed-933f-543f86693be5",
                    "sense_definition": "A penis that is relatively large when flaccid but does not grow much when erect.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He joked about being a <w>shower</w> rather than a grower.",
                            "target_lang_translations": {
                                "ru": "Он пошутил, что он <w>мясной</w>, а не кровяной."
                            }
                        },
                        {
                            "text": "The discussion about whether men are growers or <w>showers</w> was quite awkward.",
                            "target_lang_translations": {
                                "ru": "Обсуждение того, являются ли мужчины «кровяными» или «<w>мясными</w>», было довольно неловким."
                            }
                        }
                    ],
                    "antonyms": [
                        "grower"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип пениса, который имеет большой размер в спокойном состоянии, но не сильно увеличивается при эрекции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мясной",
                                "target_lang_sense_clarification": "сленг: тип пениса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a60aa58-7cf4-3d75-895b-1c599b38d519",
                    "sense_definition": "One who shows or exhibits something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "agent_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>shower</w> of the document must verify their identity first.",
                            "target_lang_translations": {
                                "ru": "<w>Предъявитель</w> документа должен сначала подтвердить свою личность."
                            }
                        },
                        {
                            "text": "That calf is going to be a great <w>shower</w> at the agricultural fair.",
                            "target_lang_translations": {
                                "ru": "Этот теленок будет отличным <w>выставочным экземпляром</w> на сельскохозяйственной ярмарке."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто показывает или демонстрирует что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демонстратор",
                                "target_lang_sense_clarification": "тот, кто показывает"
                            },
                            {
                                "target_lang_word": "предъявитель",
                                "target_lang_sense_clarification": "документов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "97e98812-9550-3837-8f66-e750fe6e9d2d",
                    "sense_definition": "To wash oneself using a shower.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hygiene_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I usually <w>shower</w> in the morning to wake myself up.",
                            "target_lang_translations": {
                                "ru": "Я обычно <w>принимаю душ</w> по утрам, чтобы проснуться."
                            }
                        },
                        {
                            "text": "You should <w>shower</w> after working out at the gym.",
                            "target_lang_translations": {
                                "ru": "Тебе стоит <w>помыться</w> после тренировки в спортзале."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shower daily",
                        "shower and change"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мыться под душем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать душ",
                                "target_lang_sense_clarification": "совершать гигиеническую процедуру"
                            },
                            {
                                "target_lang_word": "мыться",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b9e7722-82b2-3df8-b402-ed2fd84b175b",
                    "sense_definition": "To give a large amount of something to someone generously.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "bestow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her grandparents always <w>shower</w> her with expensive gifts.",
                            "target_lang_translations": {
                                "ru": "Бабушка с дедушкой всегда <w>осыпают</w> ее дорогими подарками."
                            }
                        },
                        {
                            "text": "The critics <w>showered</w> the new movie with praise.",
                            "target_lang_translations": {
                                "ru": "Критики <w>осыпали</w> новый фильм похвалами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shower with gifts",
                        "shower with praise",
                        "shower with love",
                        "shower with affection"
                    ],
                    "target_lang_definitions": {
                        "ru": "Щедро давать кому-либо большое количество чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осыпать",
                                "target_lang_sense_clarification": "дарить щедро"
                            },
                            {
                                "target_lang_word": "заваливать",
                                "target_lang_sense_clarification": "давать в избытке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "601e745b-6cf1-37af-9f86-6eeabc3edc6e",
                    "sense_definition": "To fall or cause to fall in large quantities like rain.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "scatter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Confetti <w>showered</w> down on the parade as it passed by.",
                            "target_lang_translations": {
                                "ru": "Конфетти <w>посыпалось</w> на парад, когда он проходил мимо."
                            }
                        },
                        {
                            "text": "The explosion <w>showered</w> the street with broken glass.",
                            "target_lang_translations": {
                                "ru": "Взрыв <w>осыпал</w> улицу битым стеклом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shower down",
                        "shower sparks"
                    ],
                    "target_lang_definitions": {
                        "ru": "Падать или заставлять падать в больших количествах, подобно дождю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осыпать",
                                "target_lang_sense_clarification": "покрывать падающим"
                            },
                            {
                                "target_lang_word": "сыпаться",
                                "target_lang_sense_clarification": "падать множеством"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            {
                                "target_lang_word": "паркет",
                                "target_lang_sense_clarification": "в бальных танцах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d47397d-d962-35bd-b0ac-704bf97a6e41",
                    "sense_definition": "Sala operacyjna giełdy papierów wartościowych lub w przenośni sama giełda i odbywający się na niej handel.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "stock_exchange",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiadomość o fuzji banków wywołała poruszenie na warszawskim <w>parkiecie</w>.",
                            "target_lang_translations": {
                                "ru": "Новость о слиянии банков вызвала переполох на варшавской <w>бирже</w>."
                            }
                        },
                        {
                            "text": "Spółka planuje swój debiut na <w>parkiecie</w> w przyszłym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Компания планирует свой дебют на <w>бирже</w> в следующем месяце."
                            }
                        }
                    ],
                    "synonyms": [
                        "giełda"

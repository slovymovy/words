            "senses": [
                {
                    "sense_id": "4359b4c3-da29-30d5-94a4-0b52b01b2f24",
                    "sense_definition": "Gemoedelijk praten over onbelangrijke dingen",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De buren stonden urenlang bij het hek te <w>revelen</w> over het wisselvallige weer.",
                            "target_lang_translations": {
                                "en": "The neighbors stood by the fence for hours <w>chattering</w> about the changeable weather.",
                                "ru": "Соседи часами стояли у забора и <судачили> о переменчивой погоде."
                            }
                        },
                        {
                            "text": "We hebben de hele avond gezellig zitten <w>revelen</w> bij het haardvuur.",
                            "target_lang_translations": {
                                "en": "We sat by the fireplace all evening pleasantly <w>chatting</w>.",
                                "ru": "Мы весь вечер уютно сидели и <болтали> у камина."
                            }
                        }
                    ],
                    "synonyms": [
                        "kletsen",
                        "babbelen"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "wordt in modern Nederlands minder vaak gebruikt"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To talk pleasantly about trivial matters",
                        "ru": "Приятно болтать о незначительных вещах; вести непринужденную беседу"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chatter",
                                "target_lang_sense_clarification": "to talk informally about unimportant matters"
                            },
                            {
                                "target_lang_word": "babble",
                                "target_lang_sense_clarification": "to talk rapidly and continuously in a foolish, excited, or incomprehensible way"
                            },
                            {
                                "target_lang_word": "natter",
                                "target_lang_sense_clarification": "to talk casually about unimportant topics"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "болтать",
                                "target_lang_sense_clarification": "разговаривать непринужденно"
                            },
                            {
                                "target_lang_word": "судачить",
                                "target_lang_sense_clarification": "вести досужие разговоры (устар./разг.)"
                            },
                            {
                                "target_lang_word": "балагурить",
                                "target_lang_sense_clarification": "весело болтать, шутить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25343073-f594-3c36-b734-3d09764ef07a",
                    "sense_definition": "Onzin praten of wartaal uitslaan",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de hoge koorts begon de zieke man wartaal te <w>revelen</w>.",
                            "target_lang_translations": {
                                "en": "Due to the high fever, the sick man started to <w>rave</w>.",
                                "ru": "Из-за сильного жара больной начал <бредить>."
                            }
                        },
                        {
                            "text": "Luister niet naar hem, hij <w>revelt</w> maar wat zonder na te denken.",
                            "target_lang_translations": {
                                "en": "Don't listen to him, he is just <w>rambling</w> on without thinking.",
                                "ru": "Не слушай его, он просто <мелет вздор> не подумав."
                            }
                        }
                    ],
                    "synonyms": [
                        "bazelen",
                        "raaskallen",
                        "wauwelen",
                        "leuteren",
                        "beuzelen"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "voornamelijk literair of verouderd"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To talk nonsense or speak incoherently",
                        "ru": "Говорить ерунду, бессмыслицу или бредить"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rave",
                                "target_lang_sense_clarification": "to talk wildly or incoherently, as if in delirium"
                            },
                            {
                                "target_lang_word": "ramble",
                                "target_lang_sense_clarification": "to talk or write at length in a confused or inconsequential way"
                            },
                            {
                                "target_lang_word": "drivel",
                                "target_lang_sense_clarification": "to talk nonsense"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бредить",
                                "target_lang_sense_clarification": "говорить бессвязно (из-за болезни)"
                            },
                            {
                                "target_lang_word": "нести чушь",
                                "target_lang_sense_clarification": "говорить глупости"
                            },
                            {
                                "target_lang_word": "пустословить",
                                "target_lang_sense_clarification": "говорить попусту"
                            }
                        ]
                    }
                }
            ]
        }

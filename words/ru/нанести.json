    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0e9ac5c2-51d0-3ab6-bba6-b0d56b3570f6",
                    "sense_definition": "Причинить, сделать что-либо неприятное (обиду, вред, ущерб) или совершить физическое воздействие (удар).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Боксёр сумел <нанести> решающий удар в последнем раунде.",
                            "target_lang_translations": {
                                "en": "The boxer managed to <land> a decisive blow in the last round."
                            }
                        },
                        {
                            "text": "Сильный шторм может <нанести> серьёзный ущерб прибрежным городам.",
                            "target_lang_translations": {
                                "en": "A strong storm can <cause> serious damage to coastal cities."
                            }
                        }
                    ],
                    "synonyms": [
                        "принести",
                        "принести"
                    ],
                    "common_phrases": [
                        "нанести удар",
                        "нанести вред",
                        "нанести ущерб",
                        "нанести обиду",
                        "нанести поражение"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause something unpleasant (such as harm, damage, or an insult) or to deliver a physical blow."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inflict",
                                "target_lang_sense_clarification": "damage, harm, or defeat"
                            },
                            {
                                "target_lang_word": "cause",
                                "target_lang_sense_clarification": "damage or harm"
                            },
                            {
                                "target_lang_word": "deal",
                                "target_lang_sense_clarification": "a blow or strike"
                            },
                            {
                                "target_lang_word": "land",
                                "target_lang_sense_clarification": "a blow (in sports/fighting)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9f2c5f5-a25d-3a1c-a32b-50864d98b62e",
                    "sense_definition": "Покрыть поверхность слоем чего-либо (мази, краски, лака, косметики).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "surface_application",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед выходом на солнце нужно <нанести> защитный крем на лицо.",
                            "target_lang_translations": {
                                "en": "Before going out in the sun, you need to <apply> protective cream to your face."
                            }
                        },
                        {
                            "text": "Художник решил <нанести> ещё один слой краски на холст.",
                            "target_lang_translations": {
                                "en": "The artist decided to <apply> another layer of paint to the canvas."
                            }
                        }
                    ],
                    "antonyms": [
                        "унести",
                        "вынести"
                    ],
                    "common_phrases": [
                        "нанести крем",
                        "нанести макияж",
                        "нанести слой",
                        "нанести краску"
                    ],
                    "target_lang_definitions": {
                        "en": "To put a layer of a substance (like paint, cream, or makeup) onto a surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "apply",
                                "target_lang_sense_clarification": "cream, paint, makeup"
                            },
                            {
                                "target_lang_word": "put on",
                                "target_lang_sense_clarification": "makeup, cream"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd6b2ced-7113-3ebb-a64c-0f6e865d58fa",
                    "sense_definition": "Отметить или изобразить что-либо на карте, схеме или чертеже.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "marking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Путешественники забыли <нанести> на карту новый маршрут.",
                            "target_lang_translations": {
                                "en": "The travelers forgot to <mark> the new route on the map."
                            }
                        },
                        {
                            "text": "Архитектор должен точно <нанести> все размеры на чертёж.",
                            "target_lang_translations": {
                                "en": "The architect must accurately <mark> all dimensions on the drawing."
                            }
                        }
                    ],
                    "common_phrases": [
                        "нанести на карту",
                        "нанести маршрут",
                        "нанести разметку"
                    ],
                    "target_lang_definitions": {
                        "en": "To mark the position of something or draw something on a map, chart, or plan."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mark",
                                "target_lang_sense_clarification": "on a map or drawing"
                            },
                            {
                                "target_lang_word": "plot",
                                "target_lang_sense_clarification": "a route or course"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10575508-9b54-305e-85ba-bf244c15cd3f",
                    "sense_definition": "Принести или натащить большое количество чего-либо (грязи, снега, песка) ветром, водой или на обуви.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гости не сняли обувь и успели <нанести> грязи в коридор.",
                            "target_lang_translations": {
                                "en": "The guests didn't take off their shoes and managed to <track> dirt into the hallway."
                            }
                        },
                        {
                            "text": "Сильный ветер может <нанести> много песка на открытую террасу.",
                            "target_lang_translations": {
                                "en": "A strong wind can <blow> a lot of sand onto the open terrace."
                            }
                        }
                    ],
                    "synonyms": [
                        "принести"
                    ],
                    "antonyms": [
                        "вынести",
                        "унести"
                    ],
                    "common_phrases": [
                        "нанести грязи",
                        "нанести песка",
                        "нанести снега"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring in or accumulate substances like dirt, sand, or snow (often by wind, water, or on shoes)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "track in",
                                "target_lang_sense_clarification": "dirt or mud on shoes"
                            },
                            {
                                "target_lang_word": "blow in",
                                "target_lang_sense_clarification": "sand or snow by wind"
                            },
                            {
                                "target_lang_word": "bring in",
                                "target_lang_sense_clarification": "general accumulation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c37d437-b72c-38fc-ae5a-7af7fd6e59c4",
                    "sense_definition": "Совершить официальный или вежливый визит (устойчивое выражение).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы планируем <нанести> визит нашим партнёрам на следующей неделе.",
                            "target_lang_translations": {
                                "en": "We plan to <pay> a visit to our partners next week."
                            }
                        },
                        {
                            "text": "Посол должен <нанести> официальный визит главе государства.",
                            "target_lang_translations": {
                                "en": "The ambassador must <pay> an official visit to the head of state."
                            }
                        }
                    ],
                    "common_phrases": [

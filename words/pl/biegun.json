    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "554fca39-6b78-361c-9fe6-b21c0393c062",
                    "sense_definition": "Jeden z dwóch punktów na powierzchni ciała niebieskiego (np. Ziemi), przez które przechodzi jego oś obrotu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "axis_point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wyprawa na <w>biegun</w> północny trwała wiele miesięcy.",
                            "target_lang_translations": {
                                "ru": "Экспедиция на Северный <w>полюс</w> длилась много месяцев."
                            }
                        },
                        {
                            "text": "Na <w>biegunie</w> panują ekstremalnie niskie temperatury.",
                            "target_lang_translations": {
                                "ru": "На <w>полюсе</w> царят экстремально низкие температуры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "biegun północny",
                        "biegun południowy",
                        "zdobyć biegun"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из двух точек пересечения оси вращения небесного тела (например, Земли) с его поверхностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полюс",
                                "target_lang_sense_clarification": "географический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61312c27-581b-34df-951e-367c9a439f60",
                    "sense_definition": "Miejsce w urządzeniu elektrycznym (np. baterii), przez które prąd wpływa lub wypływa; zacisk.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "axis_point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz sprawdzić, gdzie bateria ma <w>biegun</w> dodatni.",
                            "target_lang_translations": {
                                "ru": "Ты должен проверить, где у батарейки положительный <w>полюс</w>."
                            }
                        },
                        {
                            "text": "Podłączyłeś przewody do niewłaściwych <w>biegunów</w> akumulatora.",
                            "target_lang_translations": {
                                "ru": "Ты подключил провода к неправильным <w>полюсам</w> аккумулятора."
                            }
                        }
                    ],
                    "common_phrases": [
                        "biegun dodatni",
                        "biegun ujemny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место в электрическом устройстве (например, батарее), через которое входит или выходит ток; клемма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полюс",
                                "target_lang_sense_clarification": "электрический"
                            },
                            {
                                "target_lang_word": "клемма",
                                "target_lang_sense_clarification": "зажим"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e03d359d-ce83-3688-bc96-c86c8d873e6c",
                    "sense_definition": "Punkt, w którym natężenie pola magnetycznego jest największe.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "axis_point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Igła kompasu zawsze wskazuje magnetyczny <w>biegun</w> północny.",
                            "target_lang_translations": {
                                "ru": "Стрелка компаса всегда указывает на северный магнитный <w>полюс</w>."
                            }
                        },
                        {
                            "text": "Dwa takie same <w>bieguny</w> magnesu się odpychają.",
                            "target_lang_translations": {
                                "ru": "Два одинаковых <w>полюса</w> магнита отталкиваются."
                            }
                        }
                    ],
                    "common_phrases": [
                        "biegun magnetyczny",
                        "jednoimienne bieguny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точка, в которой магнитное поле обладает наибольшей силой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полюс",
                                "target_lang_sense_clarification": "магнитный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "785950d6-ba87-3764-b978-b74b939fd10e",
                    "sense_definition": "Wygięta płoza przymocowana do spodu mebla lub zabawki, umożliwiająca kołysanie się.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "rocker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko bezpiecznie bujało się na koniu na <w>biegunach</w>.",
                            "target_lang_translations": {
                                "ru": "Ребенок безопасно качался на лошадке на <w>полозьях</w>."
                            }
                        },
                        {
                            "text": "Stary fotel na <w>biegunach</w> skrzypiał przy każdym ruchu.",
                            "target_lang_translations": {
                                "ru": "Старое кресло на <w>полозьях</w> скрипело при каждом движении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "koń na biegunach",
                        "fotel na biegunach",
                        "sanki na biegunach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изогнутая деталь (полоз), прикрепленная к основанию мебели или игрушки для раскачивания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полоз",
                                "target_lang_sense_clarification": "деталь качалки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ab15668-13ca-3699-bb53-0a01ce35432c",
                    "sense_definition": "Skrajne, całkowicie przeciwne stanowisko, sytuacja lub miejsce w jakiejś hierarchii.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "axis_point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasze poglądy na gospodarkę leżą na dwóch różnych <w>biegunach</w>.",
                            "target_lang_translations": {
                                "ru": "Наши взгляды на экономику находятся на двух разных <w>полюсах</w>."
                            }
                        },
                        {
                            "text": "Bogactwo i nędza to dwa <w>bieguny</w> życia w tym mieście.",
                            "target_lang_translations": {
                                "ru": "Богатство и нищета — это два <w>полюса</w> жизни в этом городе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "na przeciwległym biegunie",
                        "dwa bieguny",
                        "skrajny biegun"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крайняя, полностью противоположная позиция, ситуация или место в какой-либо иерархии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полюс",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3373ed0f-e680-378c-b35c-96c496420cea",
                    "sense_definition": "Szybki koń wierzchowy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "horse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz spiął swego <w>bieguna</w> ostrogami i ruszył do galopu.",
                            "target_lang_translations": {
                                "ru": "Рыцарь пришпорил своего <w>скакуна</w> и пустил в галоп."
                            }
                        },
                        {
                            "text": "Królewski posłaniec zajechał na zamek na spienionym <w>biegunie</w>.",
                            "target_lang_translations": {
                                "ru": "Королевский гонец приехал в замок на взмыленном <w>скакуне</w>."
                            }
                        }
                    ],
                    "traits": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7f2367e2-508f-3ec1-9836-97215587e25f",
                    "sense_definition": "A vehicle used for traveling on water, such as for fishing, transport, or sport.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "water_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We crossed the calm lake in a small wooden <w>boat</w>.",
                            "target_lang_translations": {
                                "ru": "Мы пересекли спокойное озеро на маленькой деревянной <w>лодке</w>."
                            }
                        },
                        {
                            "text": "The fishermen returned to the harbor in their fishing <w>boat</w>.",
                            "target_lang_translations": {
                                "ru": "Рыбаки вернулись в гавань на своей рыбацкой <w>лодке</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ship",
                        "vessel",
                        "craft"
                    ],
                    "common_phrases": [
                        "fishing boat",
                        "by boat",
                        "small boat",
                        "rowing boat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Транспортное средство для передвижения по воде, используемое для рыбалки, перевозок или спорта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лодка",
                                "target_lang_sense_clarification": "небольшое судно"
                            },
                            {
                                "target_lang_word": "судно",
                                "target_lang_sense_clarification": "общее название"
                            },
                            {
                                "target_lang_word": "шлюпка",
                                "target_lang_sense_clarification": "судовая лодка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0b81dc3-4bff-3ae4-b44f-d5d28e24f206",
                    "sense_definition": "A dish or container shaped like a boat, typically used for serving sauce or gravy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please pass the gravy <w>boat</w> so I can add some sauce to my meat.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, передай <w>соусник</w>, чтобы я мог добавить соус к мясу."
                            }
                        },
                        {
                            "text": "She placed a ceramic <w>boat</w> filled with dressing on the table.",
                            "target_lang_translations": {
                                "ru": "Она поставила на стол керамический <w>соусник</w>, наполненный заправкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gravy boat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Посуда вытянутой формы, напоминающая лодку, обычно используемая для подачи соуса или подливки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соусник",
                                "target_lang_sense_clarification": "посуда для соуса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0840b77c-7001-388a-a203-40525e2536d6",
                    "sense_definition": "A very large and heavy car, often considered old-fashioned or wasteful.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "car_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He drives a massive old <w>boat</w> that consumes a lot of fuel.",
                            "target_lang_translations": {
                                "ru": "Он водит огромную старую <w>баржу</w>, которая потребляет много топлива."
                            }
                        },
                        {
                            "text": "Parking that huge <w>boat</w> in the city center is almost impossible.",
                            "target_lang_translations": {
                                "ru": "Припарковать этот огромный <w>корабль</w> в центре города почти невозможно."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "referring to a large car"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень большая и тяжелая машина, часто считающаяся старомодной или неэкономичной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баржа",
                                "target_lang_sense_clarification": "сленг, о большой машине"
                            },
                            {
                                "target_lang_word": "корабль",
                                "target_lang_sense_clarification": "сленг, о большой машине"
                            }
                        ]
                    }
                }
            ]
        },
        {

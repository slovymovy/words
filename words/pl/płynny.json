                            {
                                "target_lang_word": "бесперебойный",
                                "target_lang_sense_clarification": "о процессе или движении транспорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54b07295-3fa8-3d84-ac86-aac21b9e7cc2",
                    "sense_definition": "Zmienny, niestały, trudny do ścisłego i jednoznacznego określenia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "uncertainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Granica między przyjaźnią a miłością bywa czasem bardzo <w>płynna</w>.",
                            "target_lang_translations": {
                                "ru": "Граница между дружбой и любовью бывает порой очень <w>зыбкой</w>."
                            }
                        },
                        {
                            "text": "Sytuacja na giełdzie jest <w>płynna</w> i trudno przewidzieć ceny akcji.",
                            "target_lang_translations": {
                                "ru": "Ситуация на бирже <w>нестабильна</w>, и трудно предсказать цены акций."
                            }

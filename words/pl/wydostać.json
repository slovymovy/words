{
    "word_family": [
        "wydostawać",
        "wydostanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0556ab23-2485-3118-a593-18359a44caf2",
                    "sense_definition": "Opuścić zamknięte miejsce, trudną sytuację lub obszar, w którym było się uwięzionym (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_exit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy jak najszybciej <wydostać> się z tego zatłoczonego budynku.",
                            "target_lang_translations": {
                                "ru": "Мы должны как можно скорее <выбраться> из этого переполненного здания."
                            }
                        },
                        {
                            "text": "Po godzinie walki z zamkiem udało mu się <wydostać> z pokoju.",
                            "target_lang_translations": {
                                "ru": "После часа борьбы с замком ему удалось <выбраться> из комнаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "uciec",
                        "wyjść",
                        "uwolnić się"
                    ],
                    "antonyms": [
                        "utknąć",
                        "zostać"
                    ],
                    "common_phrases": [
                        "wydostać się na wolność",
                        "wydostać się z kłopotów",
                        "wydostać się z pułapki",
                        "wydostać się z miasta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покинуть закрытое место, трудную ситуацию или территорию, где кто-то был заперт (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбраться",
                                "target_lang_sense_clarification": "покинуть место или сложную ситуацию"
                            },
                            {
                                "target_lang_word": "вырваться",
                                "target_lang_sense_clarification": "освободиться с усилием или резко"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99f04c65-9e50-3513-be1f-d87572b559a8",
                    "sense_definition": "Wyciągnąć kogoś lub coś z jakiegoś miejsca, często z trudem lub przy użyciu siły.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transitive_extraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ratownicy zdołali <wydostać> rannych pasażerów z wraku samochodu.",
                            "target_lang_translations": {
                                "ru": "Спасатели смогли <вытащить> раненых пассажиров из обломков автомобиля."
                            }
                        },
                        {
                            "text": "Próbuję <wydostać> drzazgę z palca, ale głęboko weszła.",
                            "target_lang_translations": {
                                "ru": "Я пытаюсь <вытащить> занозу из пальца, но она глубоко засела."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyciągnąć",
                        "wyjąć",
                        "wydobyć"
                    ],
                    "antonyms": [
                        "włożyć",
                        "schować"
                    ],
                    "common_phrases": [
                        "wydostać kogoś z więzienia",
                        "wydostać coś z kieszeni",
                        "wydostać kogoś z opresji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вытащить кого-то или что-то из какого-либо места, часто с трудом или применяя силу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытащить",
                                "target_lang_sense_clarification": "извлечь физический объект или человека"
                            },
                            {
                                "target_lang_word": "извлечь",
                                "target_lang_sense_clarification": "достать что-то, находящееся внутри"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16c23331-78d4-3a7b-a15a-eacfb4b18d1a",
                    "sense_definition": "Uzyskać coś od kogoś, na przykład informację, obietnicę lub pieniądze, często stosując nacisk lub podstęp.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "transitive_extraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policjant próbował <wydostać> od podejrzanego prawdę o kradzieży.",
                            "target_lang_translations": {
                                "ru": "Полицейский пытался <вытянуть> из подозреваемого правду о краже."
                            }
                        },
                        {
                            "text": "Nie uda ci się <wydostać> ode mnie żadnych dodatkowych pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Тебе не удастся <вытянуть> из меня никаких дополнительных денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyciągnąć",
                        "uzyskać",
                        "wymusić"
                    ],
                    "common_phrases": [
                        "wydostać od kogoś prawdę",
                        "wydostać zeznania",
                        "wydostać informacje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить что-то от кого-то, например информацию или деньги, часто применяя давление или хитрость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытянуть",
                                "target_lang_sense_clarification": "получить сведения или деньги (разг.)"
                            },
                            {
                                "target_lang_word": "выудить",
                                "target_lang_sense_clarification": "хитростью узнать информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a0186b4-279f-3b0f-ac7c-4c1b904a1ae8",
                    "sense_definition": "O substancji, gazie, świetle lub dźwięku: przeniknąć na zewnątrz przez otwór lub nieszczelność (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "reflexive_exit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niebezpieczny gaz może <wydostać> się z uszkodzonej rury.",
                            "target_lang_translations": {
                                "ru": "Опасный газ может <вырваться> из поврежденной трубы."
                            }
                        },
                        {
                            "text": "Przez szparę w drzwiach zdołało <wydostać> się trochę światła.",
                            "target_lang_translations": {
                                "ru": "Сквозь щель в двери смогло <просочиться> немного света."
                            }
                        }
                    ],
                    "synonyms": [
                        "ulecieć",
                        "wypłynąć",
                        "przeniknąć"
                    ],
                    "common_phrases": [
                        "wydostać się na zewnątrz",
                        "wydostać się do atmosfery"
                    ],
                    "target_lang_definitions": {
                        "ru": "О веществе, газе, свете или звуке: проникнуть наружу через отверстие или неплотность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вырваться",
                                "target_lang_sense_clarification": "о газе или жидкости под давлением"
                            },
                            {
                                "target_lang_word": "просочиться",
                                "target_lang_sense_clarification": "о свете, жидкости или газе (медленно)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
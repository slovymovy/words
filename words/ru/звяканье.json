    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "76c9881e-c713-3ffe-8d22-d13d50c38284",
                    "sense_definition": "Действие по глаголу <w>звякать</w>; издавание высокого, отрывистого металлического звука при ударе предметов друг о друга.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clinking_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его постоянное <w>звяканье</w> ключами в кармане начинало раздражать окружающих.",
                            "target_lang_translations": {
                                "en": "His constant <w>jingling</w> of keys in his pocket was starting to annoy those around him."
                            }
                        },
                        {
                            "text": "Из кухни доносилось тихое <w>звяканье</w> посудой, означавшее, что ужин почти готов.",
                            "target_lang_translations": {
                                "en": "A quiet <w>clinking</w> of dishes came from the kitchen, meaning dinner was almost ready."
                            }
                        }
                    ],
                    "synonyms": [
                        "звякание"
                    ],
                    "common_phrases": [
                        "звяканье посудой",
                        "звяканье ключами",
                        "тихое звяканье"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "чаще используется в разговорной речи вместо формы «звякание»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The act of making a high-pitched, abrupt metallic sound by striking objects together."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clinking",
                                "target_lang_sense_clarification": "action of making a sharp metallic or glass sound"
                            },
                            {
                                "target_lang_word": "jingling",
                                "target_lang_sense_clarification": "action of making a light ringing sound, typically with keys or coins"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d2187c4-436a-3460-a21d-d03dbba679d1",
                    "sense_definition": "Высокий, отрывистый металлический или стеклянный звук, возникающий при таком действии.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clinking_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В ночной тишине раздалось громкое <w>звяканье</w> разбитого стекла.",
                            "target_lang_translations": {
                                "en": "In the silence of the night, the loud <w>clinking</w> of broken glass rang out."
                            }
                        },
                        {
                            "text": "Характерное металлическое <w>звяканье</w> шпор сопровождало каждый шаг офицера.",
                            "target_lang_translations": {
                                "en": "The characteristic metallic <w>jingling</w> of spurs accompanied the officer's every step."
                            }
                        }
                    ],
                    "synonyms": [
                        "звякание",
                        "звяк"
                    ],
                    "common_phrases": [
                        "металлическое звяканье",
                        "звяканье шпор",
                        "отчетливое звяканье"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "вариант слова «звякание»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A high-pitched, abrupt metallic or glass sound produced by such an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clink",
                                "target_lang_sense_clarification": "a short, sharp metallic sound"
                            },
                            {
                                "target_lang_word": "jingle",
                                "target_lang_sense_clarification": "a light ringing sound"
                            },
                            {
                                "target_lang_word": "tinkle",
                                "target_lang_sense_clarification": "a very light, high-pitched ringing sound"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ddbbbf1c-29aa-3192-84b4-93a6246b0ebd",
                    "sense_definition": "Een plantaardige of kunstmatige stof die een sterke, specifieke geur of smaak geeft.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kok voegde een krachtig <w>aromaat</w> toe aan de soep om de smaak te versterken.",
                            "target_lang_translations": {
                                "ru": "Повар добавил в суп сильный <w>ароматизатор</w>, чтобы усилить вкус."
                            }
                        },
                        {
                            "text": "In dit parfum wordt een synthetisch <w>aromaat</w> gebruikt dat naar rozen ruikt.",
                            "target_lang_translations": {
                                "ru": "В этих духах используется синтетическое <w>ароматическое вещество</w>, которое пахнет розами."
                            }
                        }
                    ],
                    "synonyms": [
                        "geurstof",
                        "smaakstof"
                    ],
                    "common_phrases": [
                        "natuurlijk aromaat",
                        "synthetisch aromaat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растительное или искусственное вещество, которое придает сильный, специфический запах или вкус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ароматизатор",
                                "target_lang_sense_clarification": "вещество, придающее вкус или запах"
                            },
                            {
                                "target_lang_word": "ароматическое вещество",
                                "target_lang_sense_clarification": "субстанция с выраженным ароматом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b745cb9d-9fd8-3da5-a6ea-b809a6844313",
                    "sense_definition": "Een organische chemische verbinding met een stabiele ringstructuur, zoals benzeen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Benzeen is het bekendste voorbeeld van een <w>aromaat</w> in de organische chemie.",
                            "target_lang_translations": {
                                "ru": "Бензол — самый известный пример <w>ароматического соединения</w> в органической химии."
                            }
                        },
                        {
                            "text": "Tijdens het experiment onderzochten de studenten de reactiviteit van het <w>aromaat</w>.",
                            "target_lang_translations": {
                                "ru": "Во время эксперимента студенты исследовали реакционную способность <w>ароматического соединения</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "aromatische verbinding"
                    ],
                    "common_phrases": [
                        "polycyclisch aromaat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Органическое химическое соединение со стабильной кольцевой структурой, например, бензол."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ароматическое соединение",
                                "target_lang_sense_clarification": "класс химических веществ"
                            },
                            {
                                "target_lang_word": "арен",
                                "target_lang_sense_clarification": "ароматический углеводород"
                            }
                        ]
                    }
                }
            ]
        }
    ]

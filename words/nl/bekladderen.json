                    "sense_id": "03b099e1-40d0-3654-81f4-6c3c1637c562",
                    "sense_definition": "Iets vies maken of beschadigen door er verf, inkt of een andere stof op te smeren, vaak als vorm van vandalisme.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vandalen hebben de muren van het station met graffiti <w>bekladderd</w>.",
                            "target_lang_translations": {
                                "en": "The vandals <w>defaced</w> the station walls with graffiti.",
                                "ru": "Вандалы <w>размалевали</w> стены станции граффити."
                            }
                        },
                        {
                            "text": "Het is streng verboden om openbare gebouwen te <w>bekladderen</w> met verf.",
                            "target_lang_translations": {
                                "en": "It is strictly forbidden to <w>deface</w> public buildings with paint.",
                                "ru": "Строго запрещено <w>пачкать</w> общественные здания краской."
                            }
                        }
                    ],
                    "synonyms": [
                        "besmeuren",
                        "bevuilen",
                        "bekladden"
                    ],
                    "antonyms": [
                        "schoonmaken",
                        "reinigen"
                    ],
                    "common_phrases": [
                        "een muur bekladderen",
                        "met verf bekladderen",
                        "met graffiti bekladderen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something dirty or damage it by smearing paint, ink, or another substance on it, often as a form of vandalism.",
                        "ru": "Делать что-то грязным или повреждать, нанося краску, чернила или другое вещество, часто как акт вандализма."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deface",
                                "target_lang_sense_clarification": "damage the appearance of something, especially by drawing or writing on it"
                            },
                            {
                                "target_lang_word": "daub",
                                "target_lang_sense_clarification": "coat or smear with a substance like paint or mud"
                            },
                            {
                                "target_lang_word": "smear",
                                "target_lang_sense_clarification": "spread a substance messily over a surface"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пачкать",

{
    "word_family": [
        "swoboda",
        "swobodny",
        "oswobodzenie",
        "oswobodziciel"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4d41d211-af4f-3f97-881a-e29fc1334ca4",
                    "sense_definition": "Sprawić, że ktoś lub coś odzyskuje wolność; usunąć to, co krępuje ruchy lub ogranicza swobodę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "wolnosc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Armia zdołała ostatecznie <w>oswobodzić</w> okupowane miasto.",
                            "target_lang_translations": {
                                "ru": "Армия сумела окончательно <w>освободить</w> оккупированный город."
                            }
                        },
                        {
                            "text": "Musisz ostrożnie <w>oswobodzić</w> ptaka z sieci, żeby nie uszkodzić mu skrzydeł.",
                            "target_lang_translations": {
                                "ru": "Ты должен осторожно <w>освободить</w> птицу из сети, чтобы не повредить ей крылья."
                            }
                        },
                        {
                            "text": "Ratownik pomógł <w>oswobodzić</w> rannych z wraku samochodu.",
                            "target_lang_translations": {
                                "ru": "Спасатель помог <w>высвободить</w> раненых из обломков автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "uwalniać",
                        "wyzwalać",
                        "uwalniać",
                        "wyzwalać"
                    ],
                    "antonyms": [
                        "uwięzić",
                        "zniewolić"
                    ],
                    "common_phrases": [
                        "oswobodzić zakładników",
                        "oswobodzić kraj",
                        "oswobodzić z więzów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать так, чтобы кто-то или что-то обрели свободу; устранить то, что стесняет движения или ограничивает свободу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освободить",
                                "target_lang_sense_clarification": "дать свободу, избавить от плена или оккупации"
                            },
                            {
                                "target_lang_word": "высвободить",
                                "target_lang_sense_clarification": "извлечь или выпутать из того, что удерживает физически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46c14b16-bc53-3cd3-8467-dee98f2a6e03",
                    "sense_definition": "Wydostać się z uścisku, więzów lub trudnej sytuacji; odzyskać swobodę ruchów (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "wolnosc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ofiara ataku zdołała <w>oswobodzić</w> się z rąk napastnika i uciec.",
                            "target_lang_translations": {
                                "ru": "Жертве нападения удалось <w>вырваться</w> из рук нападавшего и убежать."
                            }
                        },
                        {
                            "text": "Pies szarpał się tak długo, aż udało mu się <w>oswobodzić</w> ze smyczy.",
                            "target_lang_translations": {
                                "ru": "Собака рвалась до тех пор, пока ей не удалось <w>освободиться</w> от поводка."
                            }
                        }
                    ],
                    "synonyms": [
                        "uwalniać",
                        "wyzwalać"
                    ],
                    "antonyms": [
                        "uwięzić",
                        "dać się złapać"
                    ],
                    "common_phrases": [
                        "oswobodzić się z uścisku",
                        "oswobodzić się z nałogu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вырваться из захвата, пут или трудной ситуации; вернуть себе свободу движений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освободиться",
                                "target_lang_sense_clarification": "стать свободным, избавиться от оков или захвата"
                            },
                            {
                                "target_lang_word": "вырваться",
                                "target_lang_sense_clarification": "с усилием освободить себя из рук или тесного окружения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
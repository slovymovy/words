{
    "word_family": [
        "glutek",
        "glutowaty",
        "glucić"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fb82dd47-bfa5-30a3-8646-0a7c695afe16",
                    "sense_definition": "Potoczne określenie śluzu wydzielanego przez błonę śluzową nosa lub jego zaschniętego kawałka.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mucus",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko wytarło <w>gluta</w> w rękaw swojej nowej kurtki.",
                            "target_lang_translations": {
                                "ru": "Ребенок вытер <w>соплю</w> о рукав своей новой куртки."
                            }
                        },
                        {
                            "text": "Masz wielkiego <w>gluta</w> pod nosem, weź chusteczkę.",
                            "target_lang_translations": {
                                "ru": "У тебя огромная <w>сопля</w> под носом, возьми платок."
                            }
                        }
                    ],
                    "synonyms": [
                        "smark",
                        "gil",
                        "koza",
                        "babol",
                        "śpik"
                    ],
                    "common_phrases": [
                        "mieć gluta",
                        "wiszący glut",
                        "zielony glut"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название носовой слизи или её засохшего кусочка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопля",
                                "target_lang_sense_clarification": "жидкая слизь"
                            },
                            {
                                "target_lang_word": "козявка",
                                "target_lang_sense_clarification": "засохшая слизь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1dc80302-832a-3318-a5db-bbec0f973129",
                    "sense_definition": "Gęsta, lepka i ciągnąca się masa o nieokreślonym kształcie i konsystencji.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z pękniętej rury wyciekał na podłogę jakiś czarny <w>glut</w>.",
                            "target_lang_translations": {
                                "ru": "Из лопнувшей трубы на пол вытекала какая-то черная <w>жижа</w>."
                            }
                        },
                        {
                            "text": "Ten deser nie wyszedł i wygląda jak ohydny <w>glut</w>.",
                            "target_lang_translations": {
                                "ru": "Этот десерт не получился и выглядит как отвратительная <w>слизь</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lepki glut",
                        "wstrętny glut"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Густая, липкая и тягучая масса неопределенной формы и консистенции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слизь",
                                "target_lang_sense_clarification": "тягучая масса"
                            },
                            {
                                "target_lang_word": "жижа",
                                "target_lang_sense_clarification": "густая жидкость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2753e698-4108-37c1-bc1c-e7325fb2e80d",
                    "sense_definition": "Małe dziecko, o którym mówi się z niechęcią, lekceważeniem lub żartobliwie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mam ochoty pilnować twojego <w>gluta</w> przez cały wieczór.",
                            "target_lang_translations": {
                                "ru": "У меня нет желания присматривать за твоим <w>сопляком</w> весь вечер."
                            }
                        },
                        {
                            "text": "Po placu zabaw biegały same wrzeszczące <w>gluty</w>.",
                            "target_lang_translations": {
                                "ru": "По детской площадке бегали одни визжащие <w>сопляки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mały glut"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький ребенок, о котором говорят с неприязнью или шутливо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопляк",
                                "target_lang_sense_clarification": "пренебрежительно"
                            },
                            {
                                "target_lang_word": "спиногрыз",
                                "target_lang_sense_clarification": "шутливо/грубо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9847dee-6074-3563-9460-3173501ef50e",
                    "sense_definition": "Dawna amunicja w postaci drobnego śrutu ołowianego lub kawałków żelaza.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze w pośpiechu ładowali broń <w>glutem</w> zamiast kulami.",
                            "target_lang_translations": {
                                "ru": "Солдаты в спешке заряжали оружие <w>картечью</w> вместо пуль."
                            }
                        },
                        {
                            "text": "Wystrzelony z garłacza <w>glut</w> raził cel na bliskim dystansie.",
                            "target_lang_translations": {
                                "ru": "Выстреленный из мушкетона <w>секанец</w> поражал цель на близком расстоянии."
                            }
                        }
                    ],
                    "synonyms": [
                        "siekaniec"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинный вид боеприпасов в виде мелкой свинцовой дроби или рубленых кусочков железа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "картечь",
                                "target_lang_sense_clarification": "вид боеприпаса"
                            },
                            {
                                "target_lang_word": "секанец",
                                "target_lang_sense_clarification": "рубленый свинец"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
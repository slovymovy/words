    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6ff257a6-d7f7-3907-a549-6b77164dea7a",
                    "sense_definition": "A small lever on a gun that is pulled by the finger to fire it.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldier kept his finger on the <w>trigger</w> while watching the door."
                        },
                        {
                            "text": "He aimed carefully at the target and slowly pulled the <w>trigger</w>."
                        }
                    ],
                    "common_phrases": [
                        "pull the trigger",
                        "squeeze the trigger",
                        "finger on the trigger"
                    ]
                },
                {
                    "sense_id": "2f44f3c4-ac4f-3298-b75c-bf43bb24056a",
                    "sense_definition": "An event or situation that causes something else to start or happen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stress is often a major <w>trigger</w> for severe headaches."
                        },
                        {
                            "text": "The court ruling was the <w>trigger</w> for protests across the country."
                        }
                    ],
                    "synonyms": [
                        "cause"
                    ],
                    "common_phrases": [
                        "trigger for",
                        "main trigger",
                        "common trigger"
                    ]
                },
                {
                    "sense_id": "2dd16a9e-07db-3a20-b7bb-f086e2ee9e0e",
                    "sense_definition": "A stimulus, such as a smell or sound, that causes a person to relive a past trauma or feel strong distress.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Loud noises can be a <w>trigger</w> for veterans suffering from PTSD."
                        },
                        {
                            "text": "She learned to identify every emotional <w>trigger</w> that caused her panic attacks."
                        }
                    ],
                    "common_phrases": [
                        "emotional trigger",
                        "trigger warning"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in psychology or internet culture"
                        }
                    ]
                },
                {
                    "sense_id": "2233ce60-1c28-3038-9705-df601e5cf289",
                    "sense_definition": "A procedure in a database that automatically executes when a specific action, like an update or insertion, occurs.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The database administrator set up a <w>trigger</w> to log all changes to the user table."
                        },
                        {
                            "text": "This <w>trigger</w> ensures that data integrity is maintained after every deletion."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "computing"
                        }
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9511d3df-8494-32e6-ba9e-02738fcdab04",
                    "sense_definition": "To make something happen suddenly or cause a process to start.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy rain <w>triggered</w> a massive landslide in the valley."
                        },
                        {
                            "text": "His controversial remarks <w>triggered</w> a fierce debate among the audience."
                        }
                    ],
                    "synonyms": [
                        "cause",
                        "precipitate",
                        "initiate",
                        "set off"
                    ],
                    "common_phrases": [
                        "trigger a reaction",
                        "trigger a crisis",
                        "trigger an alarm"
                    ]
                },
                {
                    "sense_id": "c042194c-da0e-3000-8723-e40b3a44c902",
                    "sense_definition": "To activate a device or system, or to become active.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Smoke from the kitchen <w>triggered</w> the fire alarm immediately."
                        },
                        {
                            "text": "The security camera <w>triggers</w> whenever it detects movement."
                        }
                    ],
                    "synonyms": [
                        "activate",
                        "set off"
                    ]
                },
                {
                    "sense_id": "37517c02-8af1-34a7-8db3-2832bf8b9695",
                    "sense_definition": "To cause a strong negative emotional reaction in someone, often by reminding them of past trauma.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Violent scenes in movies can <w>trigger</w> painful memories for some viewers."
                        },
                        {
                            "text": "He gets <w>triggered</w> easily when people talk about politics."
                        }
                    ],
                    "synonyms": [
                        "push someone's buttons"
                    ],
                    "common_phrases": [
                        "get triggered",
                        "easily triggered"
                    ]
                },
                {
                    "sense_id": "5e8b7815-7ca0-3c0e-b65f-67ccaaa9cbd5",
                    "sense_definition": "To detonate an explosive device or fire a weapon.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The spies used a mobile phone signal to <w>trigger</w> the bomb."
                        },
                        {
                            "text": "A remote control was used to <w>trigger</w> the explosion from a safe distance."
                        }
                    ],
                    "synonyms": [
                        "detonate",
                        "fire"
                    ]
                }
            ]
        }
    ]
}
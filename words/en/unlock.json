{
    "word_family": [
        "lock",
        "locker",
        "locket",
        "locksmith",
        "padlock",
        "deadlock",
        "interlock",
        "unlockable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c055ca04-0e9b-3ac6-9a92-fa04793ff041",
                    "sense_definition": "To undo or open a lock or something locked, usually by turning a key or using a combination.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to find my keys to <w>unlock</w> the front door.",
                            "target_lang_translations": {
                                "ru": "Мне нужно найти ключи, чтобы <w>открыть</w> входную дверь."
                            }
                        },
                        {
                            "text": "She <w>unlocked</w> the suitcase and began to unpack her clothes.",
                            "target_lang_translations": {
                                "ru": "Она <w>открыла</w> чемодан и начала распаковывать вещи."
                            }
                        }
                    ],
                    "antonyms": [
                        "lock"
                    ],
                    "common_phrases": [
                        "unlock the door",
                        "unlock the gate",
                        "unlock the car"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открыть замок или что-то запертое, обычно поворачивая ключ или используя комбинацию цифр."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "наиболее общее и употребительное слово"
                            },
                            {
                                "target_lang_word": "отпирать",
                                "target_lang_sense_clarification": "снимать замок или засов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3f842b2-c0f8-3023-b792-13ae876d2300",
                    "sense_definition": "To gain access to a device, software feature, or digital content, often by entering a password or meeting specific requirements.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "digital_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must enter a four-digit code to <w>unlock</w> the tablet.",
                            "target_lang_translations": {
                                "ru": "Вы должны ввести четырехзначный код, чтобы <w>разблокировать</w> планшет."
                            }
                        },
                        {
                            "text": "Players can <w>unlock</w> new levels by collecting enough stars.",
                            "target_lang_translations": {
                                "ru": "Игроки могут <w>открывать</w> новые уровни, собирая достаточно звезд."
                            }
                        }
                    ],
                    "antonyms": [
                        "lock"
                    ],
                    "common_phrases": [
                        "unlock the phone",
                        "unlock a feature",
                        "unlock the screen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить доступ к устройству, функции программы или цифровому контенту, часто вводя пароль или выполняя определенные условия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разблокировать",
                                "target_lang_sense_clarification": "снять блокировку с устройства или экрана"
                            },
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "сделать доступным (уровень, функцию)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8756a47-f46f-3aa3-a9b4-f7632bd805cd",
                    "sense_definition": "To disclose, release, or reveal previously unknown knowledge, potential, or resources.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_reveal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Reading books can help <w>unlock</w> the imagination.",
                            "target_lang_translations": {
                                "ru": "Чтение книг может помочь <w>раскрыть</w> воображение."
                            }
                        },
                        {
                            "text": "The scientists hope their research will <w>unlock</w> the secrets of the universe.",
                            "target_lang_translations": {
                                "ru": "Ученые надеются, что их исследование <w>раскроет</w> тайны вселенной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "unlock potential",
                        "unlock the mystery",
                        "unlock the secret"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнаружить, высвободить или выявить ранее неизвестные знания, потенциал или ресурсы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрывать",
                                "target_lang_sense_clarification": "выявить потенциал, тайну или секрет"
                            },
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "обнаружить что-то новое"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cb95de4b-ae38-3ee0-a48d-78e79fb86e2a",
                    "sense_definition": "An initially hidden feature, character, or item in a video game that becomes available after achieving a specific goal.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gaming_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new weapon is a rare <w>unlock</w> available only to top players.",
                            "target_lang_translations": {
                                "ru": "Новое оружие — это редкий <w>открываемый предмет</w>, доступный только лучшим игрокам."
                            }
                        },
                        {
                            "text": "He checked the menu to see which <w>unlocks</w> he had earned so far.",
                            "target_lang_translations": {
                                "ru": "Он проверил меню, чтобы узнать, какие <w>разблокированные предметы</w> он уже заработал."
                            }
                        }
                    ],
                    "synonyms": [
                        "unlockable"
                    ],
                    "common_phrases": [
                        "weapon unlock",
                        "character unlock",
                        "level unlock"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изначально скрытая функция, персонаж или предмет в видеоигре, которые становятся доступными после достижения определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разблокируемый контент",
                                "target_lang_sense_clarification": "общий термин для открываемых элементов"
                            },
                            {
                                "target_lang_word": "открываемый предмет",
                                "target_lang_sense_clarification": "конкретный объект или бонус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9975184a-d725-33b0-8f73-ed2198c95345",
                    "sense_definition": "The act of unlocking something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "act_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The automatic <w>unlock</w> of the security doors occurs at 8 AM sharp.",
                            "target_lang_translations": {
                                "ru": "Автоматическая <w>разблокировка</w> защитных дверей происходит ровно в 8 утра."
                            }
                        },
                        {
                            "text": "After the manual <w>unlock</w>, the guards inspected the cells.",
                            "target_lang_translations": {
                                "ru": "После ручного <w>отпирания</w> охранники осмотрели камеры."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по открыванию или разблокировке чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разблокировка",
                                "target_lang_sense_clarification": "процесс снятия блокировки (технический)"
                            },
                            {
                                "target_lang_word": "отпирание",
                                "target_lang_sense_clarification": "процесс открывания замка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
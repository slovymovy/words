{
    "word_family": [
        "uitbuiten",
        "buitmaken",
        "oorlogsbuit",
        "roofbuit"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6a27d81a-81fa-3e10-b49d-5043884b7ebe",
                    "sense_definition": "Goederen die zijn verkregen door diefstal, plundering of verovering.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "captured_goods",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dieven verdeelden de <w>buit</w> eerlijk na de kraak.",
                            "target_lang_translations": {
                                "en": "The thieves divided the <w>loot</w> fairly after the heist.",
                                "ru": "Воры честно поделили <w>добычу</w> после взлома."
                            }
                        },
                        {
                            "text": "Het leger nam veel waardevolle <w>buit</w> mee uit de veroverde stad.",
                            "target_lang_translations": {
                                "en": "The army took much valuable <w>loot</w> from the conquered city.",
                                "ru": "Армия вывезла много ценных <w>трофеев</w> из захваченного города."
                            }
                        }
                    ],
                    "synonyms": [
                        "roofgoed",
                        "vangst"
                    ],
                    "common_phrases": [
                        "de buit verdelen",
                        "de buit binnenhalen"
                    ],
                    "target_lang_definitions": {
                        "en": "Goods obtained by theft, looting, or conquest.",
                        "ru": "Товары или ценности, полученные путем кражи, грабежа или завоевания."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "loot",
                                "target_lang_sense_clarification": "stolen money or valuables"
                            },
                            {
                                "target_lang_word": "booty",
                                "target_lang_sense_clarification": "valuable stolen goods, often associated with pirates"
                            },
                            {
                                "target_lang_word": "spoils",
                                "target_lang_sense_clarification": "goods taken by force, especially in war"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "добыча",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "награбленное",
                                "target_lang_sense_clarification": "украденное"
                            },
                            {
                                "target_lang_word": "трофеи",
                                "target_lang_sense_clarification": "военная добыча"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ec3ba2a-e9d2-38dd-ac98-7c450b939ea3",
                    "sense_definition": "Een dier dat gevangen of gedood is tijdens de jacht.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "captured_goods",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jager kwam trots thuis met zijn <w>buit</w>.",
                            "target_lang_translations": {
                                "en": "The hunter came home proudly with his <w>catch</w>.",
                                "ru": "Охотник гордо вернулся домой со своей <w>добычей</w>."
                            }
                        },
                        {
                            "text": "De kat speelde nog even met zijn <w>buit</w> voor hij hem opat.",
                            "target_lang_translations": {
                                "en": "The cat played with its <w>prey</w> for a while before eating it.",
                                "ru": "Кот еще немного поиграл со своей <w>добычей</w>, прежде чем съесть её."
                            }
                        }
                    ],
                    "synonyms": [
                        "prooi",
                        "vangst"
                    ],
                    "target_lang_definitions": {
                        "en": "An animal caught or killed during a hunt.",
                        "ru": "Животное, пойманное или убитое во время охоты."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "an animal that has been caught, especially fish"
                            },
                            {
                                "target_lang_word": "prey",
                                "target_lang_sense_clarification": "an animal hunted or killed for food"
                            },
                            {
                                "target_lang_word": "kill",
                                "target_lang_sense_clarification": "an animal killed during a hunt"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "добыча",
                                "target_lang_sense_clarification": "на охоте"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "55004add-be54-3b9f-aca7-3db8b453bb41",
                    "sense_definition": "Derde persoon enkelvoud tegenwoordige tijd van <w>buien</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>buit</w> buiten, dus neem een paraplu mee.",
                            "target_lang_translations": {
                                "en": "It <w>is storming</w> outside, so take an umbrella with you.",
                                "ru": "На улице <w>штормит</w>, так что возьми с собой зонт."
                            }
                        },
                        {
                            "text": "Als het zo blijft regenen en <w>buit</w>, gaan we niet wandelen.",
                            "target_lang_translations": {
                                "en": "If it keeps raining and <w>storming</w> like this, we won't go for a walk.",
                                "ru": "Если будет так же лить и <w>штормить</w>, мы не пойдем гулять."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Third person singular present tense of 'buien' (to be showery or squally).",
                        "ru": "Форма глагола buien (идти — о проливном дожде, штормить)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "storms",
                                "target_lang_sense_clarification": "to have bad weather with strong winds"
                            },
                            {
                                "target_lang_word": "is squally",
                                "target_lang_sense_clarification": "to have sudden, sharp storms"
                            }
                        ],
                        "ru": []
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8a947988-e973-3645-8890-e249b51c530a",
                    "sense_definition": "Ломать, портить или уничтожать строения или предметы, превращая их в развалины.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кто вам поручил <w>разрушать</w> старое кладбище?",
                            "target_lang_translations": {
                                "en": "Who ordered you to <w>destroy</w> the old cemetery?"
                            }
                        },
                        {
                            "text": "Сильный ветер начал <w>разрушать</w> крышу дома.",
                            "target_lang_translations": {
                                "en": "A strong wind began to <w>destroy</w> the roof of the house."
                            }
                        }
                    ],
                    "synonyms": [
                        "уничтожать",
                        "ломать",
                        "рушить",
                        "разламывать",
                        "крушить"
                    ],
                    "antonyms": [
                        "создавать",
                        "созидать",
                        "творить"
                    ],
                    "common_phrases": [
                        "разрушать город",
                        "разрушать здание",
                        "разрушать стены",
                        "разрушать до основания"
                    ],
                    "target_lang_definitions": {
                        "en": "To break, damage, or destroy structures or objects, turning them into ruins."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "destroy",
                                "target_lang_sense_clarification": "To damage something so severely that it no longer exists or works"
                            },
                            {
                                "target_lang_word": "demolish",
                                "target_lang_sense_clarification": "To knock down or destroy a building or structure"
                            },
                            {
                                "target_lang_word": "ruin",
                                "target_lang_sense_clarification": "To damage something physically so it loses its value or function"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "525cd1bb-9a22-3e66-beb7-11097a7df561",
                    "sense_definition": "Губить или уничтожать что-либо нематериальное (планы, здоровье, отношения, надежды).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Постоянный стресс может медленно <w>разрушать</w> здоровье человека.",
                            "target_lang_translations": {
                                "en": "Constant stress can slowly <w>destroy</w> a person's health."
                            }
                        },
                        {
                            "text": "Он не хотел <w>разрушать</w> семью из-за глупой ссоры.",
                            "target_lang_translations": {
                                "en": "He didn't want to <w>destroy</w> the family over a silly argument."
                            }
                        }
                    ],
                    "synonyms": [
                        "уничтожать",
                        "рушить",
                        "сокрушать"
                    ],
                    "antonyms": [
                        "создавать",
                        "творить",
                        "созидать"
                    ],
                    "common_phrases": [
                        "разрушать планы",
                        "разрушать здоровье",
                        "разрушать жизнь",
                        "разрушать надежды",
                        "разрушать стереотипы"
                    ],
                    "target_lang_definitions": {
                        "en": "To ruin or destroy something non-material (plans, health, relationships, hopes)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "destroy",
                                "target_lang_sense_clarification": "To put an end to something's existence or effectiveness"
                            },
                            {
                                "target_lang_word": "ruin",
                                "target_lang_sense_clarification": "To spoil or destroy plans, health, or reputation completely"
                            },
                            {
                                "target_lang_word": "wreck",
                                "target_lang_sense_clarification": "To severely damage or destroy something like a life or marriage"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
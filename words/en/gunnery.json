    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3a92363d-34f7-3482-b7cf-8285e63c6be4",
                    "sense_definition": "The science and technical skill of operating, aiming, and firing large guns.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "science_skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The officer studied <w>gunnery</w> to improve the accuracy of the ship's cannons.",
                            "target_lang_translations": {
                                "ru": "Офицер изучал <w>артиллерийское дело</w>, чтобы улучшить точность корабельных орудий."
                            }
                        },
                        {
                            "text": "Modern <w>gunnery</w> relies heavily on computers to calculate targets.",
                            "target_lang_translations": {
                                "ru": "Современная <w>артиллерийская наука</w> сильно зависит от компьютеров для расчета целей."
                            }
                        }
                    ],
                    "synonyms": [
                        "ballistics"
                    ],
                    "common_phrases": [
                        "naval gunnery",
                        "aerial gunnery",
                        "gunnery officer",
                        "gunnery sergeant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наука и технический навык управления, наведения и стрельбы из крупных орудий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "артиллерийское дело",
                                "target_lang_sense_clarification": "наука и практика"
                            },
                            {
                                "target_lang_word": "искусство стрельбы",
                                "target_lang_sense_clarification": "навык"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bddb0b29-afe9-3d94-9da8-eb1960bd8710",
                    "sense_definition": "The act or practice of firing heavy guns or artillery.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers woke up early for their daily <w>gunnery</w> practice.",
                            "target_lang_translations": {
                                "ru": "Солдаты проснулись рано для ежедневных упражнений в <w>стрельбе</w>."
                            }
                        },
                        {
                            "text": "We could hear the loud rumble of distant <w>gunnery</w> throughout the night.",
                            "target_lang_translations": {
                                "ru": "Всю ночь мы слышали громкий гул далекой <w>канонады</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "shelling",
                        "bombardment",
                        "gunfire"
                    ],
                    "common_phrases": [
                        "gunnery practice",
                        "gunnery exercise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или практика стрельбы из тяжелых орудий или артиллерии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "артиллерийская стрельба",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "канонада",
                                "target_lang_sense_clarification": "интенсивная стрельба или её звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb97a393-82d9-35f1-bf91-40b45b30ee2e",
                    "sense_definition": "The design, manufacture, and maintenance of large-caliber guns.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "science_skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Advances in <w>gunnery</w> allowed the navy to build more powerful ships.",
                            "target_lang_translations": {
                                "ru": "Достижения в <w>артиллерийской технике</w> позволили флоту строить более мощные корабли."
                            }
                        },
                        {
                            "text": "He is an expert in the history of 19th-century <w>gunnery</w>.",
                            "target_lang_translations": {
                                "ru": "Он эксперт по истории <w>артиллерии</w> XIX века."
                            }
                        }
                    ],
                    "synonyms": [
                        "ordnance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проектирование, производство и обслуживание крупнокалиберных орудий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "артиллерийская техника",
                                "target_lang_sense_clarification": "оборудование и технологии"
                            },
                            {
                                "target_lang_word": "артиллерия",
                                "target_lang_sense_clarification": "вооружение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "818c370f-3617-301b-ae51-04dfab4e4abb",
                    "sense_definition": "A military location where guns are tested or where personnel are trained to use them.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new recruits were sent to the <w>gunnery</w> for specialized training.",
                            "target_lang_translations": {
                                "ru": "Новобранцев отправили в <w>артиллерийскую школу</w> для прохождения специальной подготовки."
                            }
                        },
                        {
                            "text": "Safety protocols are strictly enforced at the <w>gunnery</w>.",
                            "target_lang_translations": {
                                "ru": "На <w>артиллерийском полигоне</w> строго соблюдаются протоколы безопасности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gunnery school",
                        "gunnery range"
                    ],
                    "target_lang_definitions": {
                        "ru": "Военный объект, где испытываются орудия или где персонал обучается их использованию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "артиллерийский полигон",
                                "target_lang_sense_clarification": "место испытаний"
                            },
                            {
                                "target_lang_word": "артиллерийская школа",
                                "target_lang_sense_clarification": "учебное заведение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

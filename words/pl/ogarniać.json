{
    "word_family": [
        "ogarnięcie",
        "nieogarnięcie",
        "ogarnięty"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7b91d82a-20df-3af9-85e6-dead4705ec59",
                    "sense_definition": "Doprowadzać coś do porządku, sprzątać lub zajmować się czymś, aby przywrócić ład.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_order",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę szybko <w>ogarnąć</w> kuchnię, zanim przyjdą goście.",
                            "target_lang_translations": {
                                "ru": "Мне нужно быстро <w>привести в порядок</w> кухню, пока не пришли гости."
                            }
                        },
                        {
                            "text": "W weekendy zazwyczaj <w>ogarniam</w> cały dom i robię pranie.",
                            "target_lang_translations": {
                                "ru": "По выходным я обычно <w>убираю</w> весь дом и занимаюсь стиркой."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgarniać",
                        "gromadzić",
                        "skupiać"
                    ],
                    "common_phrases": [
                        "ogarniać pokój",
                        "ogarniać bałagan",
                        "ogarniać sprawy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często używane w mowie potocznej zamiast słowa 'sprzątać' lub 'załatwiać'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить что-то в порядок, убирать или заниматься делами, чтобы восстановить чистоту и строй."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приводить в порядок",
                                "target_lang_sense_clarification": "делать уборку или налаживать дела"
                            },
                            {
                                "target_lang_word": "убирать",
                                "target_lang_sense_clarification": "наводить чистоту"
                            },
                            {
                                "target_lang_word": "разгребать",
                                "target_lang_sense_clarification": "разг. справляться с делами или беспорядком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "533ec3e5-69af-3757-8ef6-f9db82fd4c80",
                    "sense_definition": "Rozumieć coś, pojmować sens jakiejś sytuacji lub wiedzy (znaczenie potoczne).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kompletnie nie <w>ogarniam</w> matematyki, to dla mnie czarna magia.",
                            "target_lang_translations": {
                                "ru": "Я совершенно не <w>врубаюсь</w> в математику, для меня это тёмный лес."
                            }
                        },
                        {
                            "text": "Czy ty w ogóle <w>ogarniasz</w>, co się tutaj dzieje?",
                            "target_lang_translations": {
                                "ru": "Ты вообще <w>понимаешь</w>, что тут происходит?"
                            }
                        }
                    ],
                    "synonyms": [
                        "kumać",
                        "czaić",
                        "kapować",
                        "łapać",
                        "jarzyć"
                    ],
                    "common_phrases": [
                        "nie ogarniać tematu",
                        "szybko ogarniać",
                        "wszystko ogarniać"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Понимать что-то, улавливать смысл ситуации или информации (разговорное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "осознавать смысл"
                            },
                            {
                                "target_lang_word": "догонять",
                                "target_lang_sense_clarification": "разг. понимать, соображать"
                            },
                            {
                                "target_lang_word": "врубаться",
                                "target_lang_sense_clarification": "разг. вникать, разбираться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4d73b5e-0d5b-3a39-b940-c6dd737a374f",
                    "sense_definition": "Używane z zaimkiem 'się': uspokajać się, odzyskiwać kontrolę nad sobą lub przygotowywać się do wyjścia (poprawiać wygląd).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_order",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestań płakać i wreszcie <w>się ogarnij</w>, musimy działać.",
                            "target_lang_translations": {
                                "ru": "Перестань плакать и, наконец, <w>возьми себя в руки</w>, нам нужно действовать."
                            }
                        },
                        {
                            "text": "Daj mi chwilę, muszę <w>się ogarnąć</w> przed spotkaniem.",
                            "target_lang_translations": {
                                "ru": "Дай мне минуту, мне нужно <w>привести себя в порядок</w> перед встречей."
                            }
                        }
                    ],
                    "synonyms": [
                        "panować"
                    ],
                    "common_phrases": [
                        "weź się ogarnij",
                        "ogarniać się do wyjścia"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется с местоимением 'się': успокаиваться, брать себя в руки или приводить свой внешний вид в порядок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приводить себя в порядок",
                                "target_lang_sense_clarification": "улучшать внешний вид"
                            },
                            {
                                "target_lang_word": "брать себя в руки",
                                "target_lang_sense_clarification": "успокаиваться, овладевать собой"
                            },
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "готовиться, концентрироваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a16c6391-3782-3761-9424-3939516d65a0",
                    "sense_definition": "O uczuciach, emocjach lub stanach fizycznych: opanowywać kogoś, przejmować nad kimś kontrolę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_surround",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy wszedłem na scenę, nagle <w>ogarnął</w> mnie paraliżujący strach.",
                            "target_lang_translations": {
                                "ru": "Когда я вышел на сцену, меня внезапно <w>охватил</w> парализующий страх."
                            }
                        },
                        {
                            "text": "Wieczorem zawsze <w>ogarnia</w> mnie błogie zmęczenie po pracy.",
                            "target_lang_translations": {
                                "ru": "Вечером меня всегда <w>охватывает</w> приятная усталость после работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "panować",
                        "chwytać",
                        "obejmować"
                    ],
                    "common_phrases": [
                        "ogarnia strach",
                        "ogarnia smutek",
                        "ogarnia senność"
                    ],
                    "target_lang_definitions": {
                        "ru": "О чувствах, эмоциях или физических состояниях: захватывать кого-то, брать над кем-то верх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охватывать",
                                "target_lang_sense_clarification": "о чувствах или состояниях"
                            },
                            {
                                "target_lang_word": "овладевать",
                                "target_lang_sense_clarification": "брать под контроль (об эмоциях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5939b7a6-a144-396b-9679-fa2459752f27",
                    "sense_definition": "Otaczać coś ze wszystkich stron lub rozprzestrzeniać się na jakimś terenie (np. o mroku, ogniu, wzroku).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_surround",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gęsta mgła zaczęła powoli <w>ogarniać</w> całą dolinę.",
                            "target_lang_translations": {
                                "ru": "Густой туман начал медленно <w>окутывать</w> всю долину."
                            }
                        },
                        {
                            "text": "Pożar błyskawicznie <w>ogarniał</w> kolejne piętra starego budynku.",
                            "target_lang_translations": {
                                "ru": "Пожар молниеносно <w>охватывал</w> следующие этажи старого здания."
                            }
                        }
                    ],
                    "synonyms": [
                        "otaczać",
                        "obejmować",
                        "zawierać"
                    ],
                    "common_phrases": [
                        "ogarniać wzrokiem",
                        "mrok ogarnia",
                        "ogień ogarnia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Окружать что-то со всех сторон или распространяться по какой-то территории (например, о темноте, огне, взгляде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охватывать",
                                "target_lang_sense_clarification": "распространяться на что-то"
                            },
                            {
                                "target_lang_word": "окутывать",
                                "target_lang_sense_clarification": "обволакивать (о тумане, дыме)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

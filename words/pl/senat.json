            "senses": [
                {
                    "sense_id": "320d0a08-41be-30a5-9fc5-061c02e13e5c",
                    "sense_definition": "Izba wyższa parlamentu w państwach o systemie dwuizbowym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "political_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj <senat> odrzucił kontrowersyjną ustawę podatkową.",
                            "target_lang_translations": {
                                "ru": "Вчера <w>сенат</w> отклонил спорный налоговый закон."
                            }
                        },
                        {
                            "text": "Burzliwe posiedzenie senatu zakończyło się dopiero nad ranem.",
                            "target_lang_translations": {
                                "ru": "Бурное заседание <w>сената</w> закончилось только под утро."
                            }
                        }
                    ],
                    "synonyms": [
                        "izba wyższa"
                    ],
                    "antonyms": [
                        "izba niższa"
                    ],
                    "common_phrases": [
                        "posiedzenie senatu",
                        "wybory do senatu",
                        "większość w senacie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя палата парламента в государствах с двухпалатной системой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сенат",
                                "target_lang_sense_clarification": "верхняя палата парламента"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7a5338d-d51d-31a9-94cb-8c7570e0bd0e",
                    "sense_definition": "Najwyższy organ kolegialny uczelni wyższej, decydujący o najważniejszych sprawach jej funkcjonowania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "academic_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rektor zwołał nadzwyczajny <senat> w celu omówienia budżetu uczelni.",
                            "target_lang_translations": {
                                "ru": "Ректор созвал чрезвычайный <w>сенат</w> для обсуждения бюджета вуза."
                            }
                        },
                        {
                            "text": "Każda uchwała senatu musi być zgodna ze statutem uniwersytetu.",
                            "target_lang_translations": {
                                "ru": "Каждое постановление <w>сената</w> должно соответствовать уставу университета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "senat uczelni",
                        "uchwała senatu",
                        "członek senatu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высший коллегиальный орган высшего учебного заведения, принимающий решения по важнейшим вопросам его функционирования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сенат",
                                "target_lang_sense_clarification": "в университетах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5339e8e5-cf5b-325c-a4d7-68eca852df6f",
                    "sense_definition": "Organ władzy w systemie politycznym starożytnego Rzymu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starożytny <senat> miał ogromny wpływ na politykę zagraniczną imperium.",
                            "target_lang_translations": {
                                "ru": "Древний <w>сенат</w> имел огромное влияние на внешнюю политику империи."
                            }
                        },
                        {
                            "text": "Cesarz często ignorował rzymski senat i podejmował decyzje samodzielnie.",
                            "target_lang_translations": {
                                "ru": "Император часто игнорировал римский <w>сенат</w> и принимал решения самостоятельно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rzymski senat"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dotyczy kontekstu historycznego"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Орган власти в политической системе Древнего Рима."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сенат",
                                "target_lang_sense_clarification": "в Древнем Риме"
                            }
                        ]
                    }
                }
            ]
        },
        {

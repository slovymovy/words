            "senses": [
                {
                    "sense_id": "fd970171-ff2d-3d6d-af1e-bba4d51c253a",
                    "sense_definition": "Het iets bekijken en waarnemen met als doel iets beter te begrijpen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij deed een uitgebreid <w>onderzoek</w> naar de herkomst van de Afrikaanse taal.",
                            "target_lang_translations": {
                                "en": "He conducted extensive <w>research</w> into the origins of the African language.",
                                "ru": "Он провел обширное <w>исследование</w> происхождения африканского языка."
                            }
                        },
                        {
                            "text": "De politie is een <w>onderzoek</w> gestart naar de exacte toedracht van het incident.",
                            "target_lang_translations": {
                                "en": "The police have started an <w>investigation</w> into the exact circumstances of the incident.",
                                "ru": "Полиция начала <w>расследование</w> точных обстоятельств инцидента."
                            }
                        },
                        {
                            "text": "Ik ging direct op <w>onderzoek</w> uit om te zien wat er aan de hand was.",
                            "target_lang_translations": {
                                "en": "I immediately went to <w>investigate</w> to see what was going on.",
                                "ru": "Я сразу отправился на <w>разведку</w>, чтобы посмотреть, в чем дело."
                            }
                        }
                    ],
                    "synonyms": [
                        "speurwerk"
                    ],
                    "common_phrases": [
                        "onderzoek doen",
                        "wetenschappelijk onderzoek",
                        "op onderzoek uitgaan",
                        "in onderzoek"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of viewing and observing something with the aim of understanding it better.",
                        "ru": "Процесс наблюдения, изучения или анализа чего-либо с целью лучшего понимания или выяснения фактов."
                    },
                    "translations": {
                        "en": [
                            {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "01b9bbd4-79bf-328c-b39c-ea3f630e124d",
                    "sense_definition": "Een afgebakend deel, fase of moeilijkheidsgraad in een computerspel.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "gaming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan dit moeilijke <w>level</w> maar niet halen.",
                            "target_lang_translations": {
                                "ru": "Я никак не могу пройти этот сложный <w>уровень</w>."
                            }
                        },
                        {
                            "text": "In het volgende <w>level</w> krijg je nieuwe vaardigheden.",
                            "target_lang_translations": {
                                "ru": "На следующем <w>уровне</w> ты получишь новые навыки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het volgende level",
                        "level uitspelen",
                        "in een level"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенная часть, этап или уровень сложности в компьютерной игре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "этап игры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f2705a6-1b34-3e8e-ae42-926facc4e634",
                    "sense_definition": "Een getal dat de sterkte, ervaring of rang van een spelpersonage of voorwerp aangeeft.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "gaming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn personage is eindelijk <w>level</w> vijftig geworden.",
                            "target_lang_translations": {
                                "ru": "Мой персонаж наконец достиг пятидесятого <w>уровня</w>."
                            }
                        },
                        {
                            "text": "Je hebt een hoger <w>level</w> nodig om dit wapen te gebruiken.",
                            "target_lang_translations": {
                                "ru": "Тебе нужен более высокий <w>уровень</w>, чтобы использовать это оружие."
                            }
                        }
                    ],
                    "common_phrases": [
                        "level up",
                        "hoog level"
                    ],
                    "target_lang_definitions": {
                        "ru": "Число, указывающее на силу, опыт или ранг игрового персонажа или предмета."

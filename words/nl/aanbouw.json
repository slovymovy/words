                {
                    "sense_id": "dd62b32e-1ac6-3f7c-af58-2aa40999ee05",
                    "sense_definition": "Een ruimte of gebouw dat tegen een bestaand pand is vastgebouwd om het te vergroten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "building_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De slaapkamer komt in de nieuwe <w>aanbouw</w>.",
                            "target_lang_translations": {
                                "ru": "Спальня будет в новой <w>пристройке</w>.",
                                "en": "The bedroom will be in the new <w>extension</w>."
                            }
                        },
                        {
                            "text": "Zij hebben een vergunning aangevraagd voor een <w>aanbouw</w> aan de achterkant van het huis.",
                            "target_lang_translations": {
                                "ru": "Они подали заявление на разрешение на <w>пристройку</w> с задней стороны дома.",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c62197c1-082f-3b70-ba83-21f1e731be30",
                    "sense_definition": "To put something into a liquid for a short time and then take it out again.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "liquid_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>dip</w> your biscuit into your tea if you like.",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>макать</w> печенье в чай, если хочешь."
                            }
                        },
                        {
                            "text": "He <w>dipped</w> the brush into the paint to start working.",
                            "target_lang_translations": {
                                "ru": "Он <w>окунул</w> кисть в краску, чтобы начать работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "didie",
                        "didy",
                        "diddy",
                        "dipe",
                        "diap",
                        "diapie",
                        "diapy",
                        "diapee"
                    ],
                    "common_phrases": [
                        "dip in",
                        "dip into"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить что-то в жидкость на короткое время и затем вынуть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "макать",
                                "target_lang_sense_clarification": "погружать еду или предмет в жидкость"
                            },
                            {
                                "target_lang_word": "окунать",
                                "target_lang_sense_clarification": "погружать целиком или частично"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c85d56f-e931-3100-929f-640bffd85a4b",

{
    "word_family": [
        "tongued",
        "tongueless"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "49df54ee-935a-375c-928d-acffdcb2c069",
                    "sense_definition": "The flexible muscular organ in the mouth used for tasting, swallowing, and speech.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor asked the patient to stick out his <w>tongue</w>.",
                            "target_lang_translations": {
                                "ru": "Врач попросил пациента высунуть <w>язык</w>."
                            }
                        },
                        {
                            "text": "I accidentally bit my <w>tongue</w> while I was eating lunch.",
                            "target_lang_translations": {
                                "ru": "Я случайно прикусил <w>язык</w> во время обеда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bite one's tongue",
                        "stick out one's tongue",
                        "tip of the tongue",
                        "slip of the tongue"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвижный мышечный орган во рту, используемый для определения вкуса, глотания и речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язык",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b55d983-2002-39c3-a00f-5b55c177e757",
                    "sense_definition": "A language, especially a person's native language.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "language",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Although he lives in France, English is his mother <w>tongue</w>.",
                            "target_lang_translations": {
                                "ru": "Хотя он живет во Франции, английский — его родной <w>язык</w>."
                            }
                        },
                        {
                            "text": "She speaks a foreign <w>tongue</w> that I do not understand.",
                            "target_lang_translations": {
                                "ru": "Она говорит на иностранном <w>языке</w>, которого я не понимаю."
                            }
                        }
                    ],
                    "synonyms": [
                        "language"
                    ],
                    "common_phrases": [
                        "mother tongue",
                        "native tongue",
                        "foreign tongue"
                    ],
                    "target_lang_definitions": {
                        "ru": "Средство общения; речь народа или нации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язык",
                                "target_lang_sense_clarification": "речь, средство общения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bae6583-99a9-318b-9b2a-6ae418995ab7",
                    "sense_definition": "The tongue of an animal, usually a cow or ox, prepared and eaten as food.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother often serves cold beef <w>tongue</w> with mustard.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка часто подает холодный говяжий <w>язык</w> с горчицей."
                            }
                        },
                        {
                            "text": "We ordered a sandwich made with pickled <w>tongue</w> at the deli.",
                            "target_lang_translations": {
                                "ru": "Мы заказали в гастрономе сэндвич с маринованным <w>языком</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beef tongue",
                        "pickled tongue"
                    ],
                    "target_lang_definitions": {
                        "ru": "Язык животного, приготовленный как блюдо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язык",
                                "target_lang_sense_clarification": "блюдо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0282b85-ede7-345f-b940-d01e300ac471",
                    "sense_definition": "A manner of speaking, often referring to the style, intent, or attitude of the speaker.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should hold your <w>tongue</w> if you have nothing polite to say.",
                            "target_lang_translations": {
                                "ru": "Тебе следует придержать <w>язык</w>, если не можешь сказать ничего вежливого."
                            }
                        },
                        {
                            "text": "He is known for his sharp <w>tongue</w> and often offends people.",
                            "target_lang_translations": {
                                "ru": "Он известен своим острым <w>языком</w> и часто обижает людей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hold one's tongue",
                        "sharp tongue",
                        "silver tongue",
                        "watch one's tongue"
                    ],
                    "target_lang_definitions": {
                        "ru": "Манера говорить или стиль выражения мыслей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язык",
                                "target_lang_sense_clarification": "манера речи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41fc3304-4494-3d6b-a0e4-fc93fe89bed7",
                    "sense_definition": "The flap of material under the laces of a shoe or boot that protects the foot.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make sure to pull the <w>tongue</w> up before you tie your shoelaces.",
                            "target_lang_translations": {
                                "ru": "Обязательно подтяни <w>язычок</w>, прежде чем завязывать шнурки."
                            }
                        },
                        {
                            "text": "The <w>tongue</w> of my sneaker is wrinkled and feels uncomfortable.",
                            "target_lang_translations": {
                                "ru": "<w>Язычок</w> моего кроссовка смялся, и мне неудобно."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лоскут материала под шнурками обуви."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язычок",
                                "target_lang_sense_clarification": "деталь обуви"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "024e2c55-a5d8-321d-8bb3-b2bc442dc047",
                    "sense_definition": "A long, thin flame rising from a fire.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape_metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bright <w>tongues</w> of fire shot up from the burning roof.",
                            "target_lang_translations": {
                                "ru": "Яркие <w>языки</w> пламени взметнулись вверх от горящей крыши."
                            }
                        },
                        {
                            "text": "A small <w>tongue</w> of flame flickered in the fireplace.",
                            "target_lang_translations": {
                                "ru": "Маленький <w>язычок</w> пламени мерцал в камине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tongues of fire",
                        "tongues of flame"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often literary or descriptive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная, тонкая полоса огня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язык",
                                "target_lang_sense_clarification": "пламени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "407fbe3a-2517-385c-b456-d2da91e9bd0e",
                    "sense_definition": "A long, narrow strip of land projecting into a body of water.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lighthouse was built on a rocky <w>tongue</w> of land.",
                            "target_lang_translations": {
                                "ru": "Маяк был построен на скалистом <w>языке</w> суши."
                            }
                        },
                        {
                            "text": "A narrow <w>tongue</w> of sand stretches out into the blue sea.",
                            "target_lang_translations": {
                                "ru": "Узкая песчаная <w>коса</w> уходит в синее море."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная узкая полоса суши, вдающаяся в воду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коса",
                                "target_lang_sense_clarification": "география"
                            },
                            {
                                "target_lang_word": "язык",
                                "target_lang_sense_clarification": "узкая полоса суши"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "465584cb-ad00-3936-b710-c3185870cbfe",
                    "sense_definition": "The phenomenon of speaking in an unknown language, often during religious worship (glossolalia).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Some members of the congregation began speaking in <w>tongues</w> during the service.",
                            "target_lang_translations": {
                                "ru": "Некоторые прихожане начали говорить на <w>языках</w> во время службы."
                            }
                        },
                        {
                            "text": "The practice of speaking in <w>tongues</w> is common in Pentecostal churches.",
                            "target_lang_translations": {
                                "ru": "Практика говорения на <w>языках</w> распространена в пятидесятнических церквях."
                            }
                        }
                    ],
                    "synonyms": [
                        "speaking in tongues"
                    ],
                    "common_phrases": [
                        "speak in tongues"
                    ],
                    "target_lang_definitions": {
                        "ru": "Феномен говорения на неизвестных языках (глоссолалия)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "языки",
                                "target_lang_sense_clarification": "религиозное понятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "918d035a-9491-390c-af30-dc9de0a8dbfe",
                    "sense_definition": "The swinging metal piece inside a bell that strikes the sides to make sound; a clapper.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The iron <w>tongue</w> of the great bell struck midnight.",
                            "target_lang_translations": {
                                "ru": "Железный <w>язык</w> большого колокола пробил полночь."
                            }
                        },
                        {
                            "text": "When the bell swings, the <w>tongue</w> hits the metal rim.",
                            "target_lang_translations": {
                                "ru": "Когда колокол раскачивается, <w>язык</w> ударяет о металлический обод."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлический стержень внутри колокола, производящий звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язык",
                                "target_lang_sense_clarification": "часть колокола"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f6206b2-fd95-3825-8183-3d13eda37ca0",
                    "sense_definition": "To touch, lick, or manipulate something with the tongue.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cat <w>tongued</w> its fur until it was clean and smooth.",
                            "target_lang_translations": {
                                "ru": "Кошка <w>вылизывала</w> свою шерсть, пока та не стала чистой и гладкой."
                            }
                        },
                        {
                            "text": "He <w>tongued</w> the envelope to seal it before mailing the letter.",
                            "target_lang_translations": {
                                "ru": "Он <w>лизнул</w> конверт, чтобы заклеить его перед отправкой письма."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Трогать или лизать что-либо языком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лизать",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "трогать языком",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8c5c7e3-42cf-3b5e-b8e6-ed1dff1ce6d5",
                    "sense_definition": "To articulate a note on a wind instrument by interrupting the air flow with the tongue.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must learn to <w>tongue</w> rapid notes clearly to play this piece.",
                            "target_lang_translations": {
                                "ru": "Вы должны научиться четко <w>выполнять атаку языком</w> на быстрых нотах, чтобы сыграть это произведение."
                            }
                        },
                        {
                            "text": "The trumpet player <w>tongued</w> the staccato rhythm with precision.",
                            "target_lang_translations": {
                                "ru": "Трубач точно <w>отбивал языком</w> ритм стаккато."
                            }
                        }
                    ],
                    "common_phrases": [
                        "double tongue",
                        "triple tongue"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исполнять ноты на духовом инструменте, прерывая поток воздуха языком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "применять атаку языком",
                                "target_lang_sense_clarification": "музыкальный термин"
                            },
                            {
                                "target_lang_word": "работать языком",
                                "target_lang_sense_clarification": "техника игры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5809be1-b5d8-31fd-9cc9-b0edfba7be39",
                    "sense_definition": "To join two boards together by fitting a projecting edge of one into a groove of the other.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The carpenter <w>tongued</w> the floorboards together for a tight fit.",
                            "target_lang_translations": {
                                "ru": "Плотник <w>соединил в шпунт</w> половицы для плотной подгонки."
                            }
                        },
                        {
                            "text": "These planks are designed to be <w>tongued</w> and grooved easily.",
                            "target_lang_translations": {
                                "ru": "Эти доски предназначены для легкого <w>соединения в шпунт и гребень</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tongue and groove"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединять доски методом «шпунт и гребень»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шпунтовать",
                                "target_lang_sense_clarification": "строительство"
                            },
                            {
                                "target_lang_word": "соединять в шпунт",
                                "target_lang_sense_clarification": "строительство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "f588436d-a3f2-3368-aa6a-34111a9b7ff0",
                    "sense_definition": "A village in the Highland council area of Scotland.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We visited the scenic village of <w>Tongue</w> during our trip to Scotland.",
                            "target_lang_translations": {
                                "ru": "Мы посетили живописную деревню <w>Тонг</w> во время нашей поездки в Шотландию."
                            }
                        },
                        {
                            "text": "The Kyle of <w>Tongue</w> offers beautiful views of the coast.",
                            "target_lang_translations": {
                                "ru": "Кайл-оф-<w>Тонг</w> предлагает прекрасные виды на побережье."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревня в Шотландии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Тонг",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "326fe854-da54-3ddc-a6e9-a105b45aa2f7",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Tongue</w> has lived in this neighborhood for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Тонг</w> живет в этом районе уже двадцать лет."
                            }
                        },
                        {
                            "text": "I met a woman named Sarah <w>Tongue</w> at the conference.",
                            "target_lang_translations": {
                                "ru": "Я встретил женщину по имени Сара <w>Тонг</w> на конференции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Тонг",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

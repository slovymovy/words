    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "85e331fd-266f-39db-812f-c46ceec9b692",
                    "sense_definition": "Pokrywać powierzchnię farbą, lakierem lub innym środkiem barwiącym w celu nadania jej koloru lub ochrony.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <malować> kuchnię w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе нам нужно <покрасить> кухню."
                            }
                        },
                        {
                            "text": "Mój tata <maluje> płot na zielono.",
                            "target_lang_translations": {
                                "ru": "Мой папа <красит> забор в зелёный цвет."
                            }
                        }
                    ],
                    "synonyms": [
                        "farbować",
                        "barwić"
                    ],
                    "antonyms": [
                        "zmywać",
                        "czyścić"
                    ],
                    "common_phrases": [
                        "malować ściany",
                        "malować płot",
                        "malować pokój",
                        "malować na biało"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать поверхность краской, лаком или другим красящим веществом для придания цвета или защиты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красить",
                                "target_lang_sense_clarification": "покрывать краской"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3afcd9e-2c02-3b37-ac9c-c7f8531010d6",
                    "sense_definition": "Tworzyć obraz, rysunek lub dzieło artystyczne za pomocą farb i pędzla.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "artistic_creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra pięknie <maluje> akwarelami.",
                            "target_lang_translations": {
                                "ru": "Моя сестра красиво <рисует> акварелью."
                            }
                        },
                        {
                            "text": "Artysta <maluje> portret słynnej aktorki.",
                            "target_lang_translations": {
                                "ru": "Художник <пишет> портрет известной актрисы."
                            }
                        }
                    ],
                    "synonyms": [
                        "tworzyć"
                    ],
                    "common_phrases": [
                        "malować obraz",
                        "malować pejzaż",
                        "malować portret",
                        "ładnie malować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать картину, рисунок или художественное произведение с помощью красок и кисти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рисовать",
                                "target_lang_sense_clarification": "создавать изображение"
                            },
                            {
                                "target_lang_word": "писать",
                                "target_lang_sense_clarification": "о картинах, портретах (проф.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0df6543f-42c0-34e7-b425-e5b7ddd4cdc5",
                    "sense_definition": "Nakładać kosmetyki na twarz, paznokcie lub włosy w celu upiększenia.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "cosmetics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lubię <malować> paznokcie na czerwono.",
                            "target_lang_translations": {
                                "ru": "Я люблю <красить> ногти в красный цвет."
                            }
                        },
                        {
                            "text": "Fryzjerka <maluje> klientce włosy na blond.",
                            "target_lang_translations": {
                                "ru": "Парикмахер <красит> клиентке волосы в блонд."
                            }
                        }
                    ],
                    "synonyms": [
                        "robić makijaż"
                    ],
                    "antonyms": [
                        "zmywać"
                    ],
                    "common_phrases": [
                        "malować paznokcie",
                        "malować usta",
                        "malować rzęsy",
                        "malować włosy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить косметику на лицо, ногти или волосы для украшения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красить",
                                "target_lang_sense_clarification": "волосы, ногти, губы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8aceab28-40e7-3fdb-84d9-9f90df2dcd68",
                    "sense_definition": "Robić sobie makijaż; nakładać kosmetyki na własną twarz.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "cosmetics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Anna <maluje> się codziennie przed wyjściem do biura.",
                            "target_lang_translations": {
                                "ru": "Анна <красится> каждый день перед выходом в офис."
                            }
                        },
                        {
                            "text": "Nie musisz się <malować>, wyglądasz świetnie naturalnie.",
                            "target_lang_translations": {
                                "ru": "Тебе не нужно <краситься>, ты и так отлично выглядишь."
                            }
                        }
                    ],
                    "synonyms": [
                        "robić makijaż"
                    ],
                    "common_phrases": [
                        "malować się rano",
                        "mocno się malować",
                        "malować się do pracy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать себе макияж; наносить косметику на собственное лицо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "краситься",
                                "target_lang_sense_clarification": "делать макияж себе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "200c62ba-6c19-394b-8494-c380b92315b4",
                    "sense_definition": "Opisywać coś lub kogoś w sposób barwny i sugestywny za pomocą słów.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_description",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autor <maluje> w swojej książce obraz XIX-wiecznej Warszawy.",
                            "target_lang_translations": {
                                "ru": "Автор <рисует> в своей книге образ Варшавы XIX века."
                            }
                        },
                        {
                            "text": "Nie <maluj> wszystkiego w tak ciemnych barwach.",
                            "target_lang_translations": {
                                "ru": "Не <рисуй> всё в таких тёмных красках."
                            }
                        }
                    ],
                    "synonyms": [
                        "opisywać",
                        "przedstawiać",
                        "ukazywać"
                    ],
                    "common_phrases": [
                        "malować słowami",
                        "malować przyszłość",
                        "malować w czarnych barwach"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in phrases like 'malować w czarnych barwach'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывать что-то или кого-то образно и ярко с помощью слов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "описывать",
                                "target_lang_sense_clarification": "рассказывать о чем-то"
                            },
                            {
                                "target_lang_word": "живописать",
                                "target_lang_sense_clarification": "ярко описывать (книжн.)"
                            },
                            {
                                "target_lang_word": "изображать",
                                "target_lang_sense_clarification": "представлять в рассказе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cf531a0-a9b5-3570-81bb-0d792d015187",
                    "sense_definition": "O uczuciach i emocjach: stawać się widocznym na twarzy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na jej twarzy <malowało> się ogromne zaskoczenie.",
                            "target_lang_translations": {
                                "ru": "На её лице <отражалось> огромное удивление."
                            }
                        },
                        {
                            "text": "W jego oczach <malował> się smutek i rezygnacja.",
                            "target_lang_translations": {
                                "ru": "В его глазах <читались> грусть и смирение."
                            }
                        }
                    ],
                    "synonyms": [
                        "uwidaczniać się",
                        "rysować się"
                    ],
                    "common_phrases": [
                        "na twarzy maluje się uśmiech",
                        "w oczach maluje się strach",
                        "maluje się zdziwienie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О чувствах и эмоциях: становиться заметным на лице."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отражаться",
                                "target_lang_sense_clarification": "быть видным (об эмоциях)"
                            },
                            {
                                "target_lang_word": "читаться",
                                "target_lang_sense_clarification": "быть заметным"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
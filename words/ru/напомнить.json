{
    "word_family": [
        "помнить",
        "память",
        "напоминание",
        "запомнить",
        "вспомнить",
        "воспоминание",
        "припомнить",
        "памятный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bc94466a-ba6c-3b89-997d-98cad9fe03bb",
                    "sense_definition": "Указать на необходимость сделать что-либо, чтобы человек не забыл.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, <напомни> мне купить хлеб по дороге домой.",
                            "target_lang_translations": {
                                "en": "Please <remind> me to buy bread on the way home."
                            }
                        },
                        {
                            "text": "Учитель <напомнил> ученикам о домашнем задании на завтра.",
                            "target_lang_translations": {
                                "en": "The teacher <reminded> the students about tomorrow's homework."
                            }
                        }
                    ],
                    "common_phrases": [
                        "напомнить о встрече",
                        "напомнить сделать",
                        "напомнить правила"
                    ],
                    "target_lang_definitions": {
                        "en": "To point out the need to do something so a person does not forget."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remind",
                                "target_lang_sense_clarification": "to tell someone not to forget"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95707cc2-b0cb-3f1d-a86a-623a6270c6ba",
                    "sense_definition": "Заставить вспомнить, пробудить в памяти воспоминание о ком-либо или чём-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта мелодия <напомнила> мне о нашем летнем путешествии.",
                            "target_lang_translations": {
                                "en": "This melody <reminded> me of our summer trip."
                            }
                        },
                        {
                            "text": "Старая фотография <напомнила> нам о счастливых днях.",
                            "target_lang_translations": {
                                "en": "The old photograph <reminded> us of happy days."
                            }
                        }
                    ],
                    "synonyms": [
                        "навеять"
                    ],
                    "common_phrases": [
                        "напомнить о себе",
                        "напомнить прошлое",
                        "напомнить о былом"
                    ],
                    "target_lang_definitions": {
                        "en": "To make someone remember, to awaken a memory of someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remind",
                                "target_lang_sense_clarification": "to evoke a memory"
                            },
                            {
                                "target_lang_word": "bring back",
                                "target_lang_sense_clarification": "to cause to remember memories"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d1e5147-94fb-3d32-9c74-0cf923bb81cc",
                    "sense_definition": "Показаться похожим на кого-либо или что-либо внешностью или качествами.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Твой голос очень <напомнил> мне голос моего отца.",
                            "target_lang_translations": {
                                "en": "Your voice really <reminded> me of my father's voice."
                            }
                        },
                        {
                            "text": "Этот странный дом <напомнил> нам старинный замок.",
                            "target_lang_translations": {
                                "en": "This strange house <reminded> us of an old castle."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сильно напомнить",
                        "чем-то напомнить",
                        "напомнить кого-то"
                    ],
                    "target_lang_definitions": {
                        "en": "To seem similar to someone or something in appearance or qualities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remind",
                                "target_lang_sense_clarification": "to cause to think of due to similarity"
                            },
                            {
                                "target_lang_word": "resemble",
                                "target_lang_sense_clarification": "to look or seem like"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
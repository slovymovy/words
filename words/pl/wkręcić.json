            "senses": [
                {
                    "sense_id": "74d7ed07-d7f4-3943-b8ce-e422b2676962",
                    "sense_definition": "Umieścić przedmiot gwintowany (np. śrubę, żarówkę) w pasującym do niego otworze poprzez kręcenie nim.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić drabinę, żeby <w>wkręcić</w> nową żarówkę w kuchni.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить стремянку, чтобы <w>вкрутить</w> новую лампочку на кухне."
                            }
                        },
                        {
                            "text": "Mój tata pokazał mi, jak prosto <w>wkręcić</w> wkręt w twarde drewno.",
                            "target_lang_translations": {
                                "ru": "Мой папа показал мне, как легко <w>вкрутить</w> шуруп в твердое дерево."
                            }
                        }
                    ],
                    "synonyms": [
                        "przykręcić",
                        "wmontować",
                        "zamocować"
                    ],
                    "antonyms": [
                        "wykręcić",
                        "odkręcić"
                    ],
                    "common_phrases": [

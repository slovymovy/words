    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4f9cccb9-747d-3a3d-9857-d3a1d7a2284c",
                    "sense_definition": "Relating to the name used for the inhabitants of a specific place.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "demonym_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>gentilic</w> adjective for a person from Canada is Canadian.",
                            "target_lang_translations": {
                                "ru": "<w>Катойконимическим</w> прилагательным для человека из Канады является «канадский»."
                            }
                        },
                        {
                            "text": "Linguists study how <w>gentilic</w> forms vary across different languages.",
                            "target_lang_translations": {
                                "ru": "Лингвисты изучают, как <w>катойконимические</w> формы варьируются в разных языках."
                            }

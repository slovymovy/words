{
    "word_family": [
        "rybałcik",
        "rybałtowski"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d9ec9037-e62b-3805-81de-5652a10bd8e6",
                    "sense_definition": "Średniowieczny wędrowny aktor, muzyk lub poeta, wykonujący utwory o charakterze świeckim, często satyrycznym, na dworach lub placach miejskich.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na rynku miejskim pojawił się wesoły <w>rybałt</w>, który zabawiał tłum swoimi opowieściami.",
                            "target_lang_translations": {
                                "ru": "На городской площади появился веселый <w>бродячий комедиант</w>, который развлекал толпу своими рассказами."
                            }
                        },
                        {
                            "text": "Stary <w>rybałt</w> zagrał na lutni pieśń o dawnych bohaterach, wzruszając słuchaczy.",
                            "target_lang_translations": {
                                "ru": "Старый <w>менестрель</w> сыграл на лютне песню о древних героях, растрогав слушателей."
                            }
                        }
                    ],
                    "synonyms": [
                        "minstrel",
                        "trubadur",
                        "grajek",
                        "wesołek"
                    ],
                    "common_phrases": [
                        "wędrowny rybałt",
                        "komedia rybałtowska",
                        "pieśń rybałta"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "określenie historyczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Средневековый бродячий актер, музыкант или поэт, исполняющий светские, часто сатирические произведения при дворах или на городских площадях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бродячий комедиант",
                                "target_lang_sense_clarification": "странствующий актер"
                            },
                            {
                                "target_lang_word": "менестрель",
                                "target_lang_sense_clarification": "средневековый поэт-музыкант"
                            },
                            {
                                "target_lang_word": "шпильман",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fce9cf8c-993a-33e1-b3bc-3241e3ce21af",
                    "sense_definition": "Dawny niższy funkcjonariusz kościelny, zajmujący się śpiewem liturgicznym lub nauczaniem w szkole parafialnej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody <w>rybałt</w> ćwiczył śpiewanie psalmów przed niedzielną uroczystością.",
                            "target_lang_translations": {
                                "ru": "Молодой <w>певчий</w> упражнялся в пении псалмов перед воскресной службой."
                            }
                        },
                        {
                            "text": "W kronikach parafialnych zapisano imię <w>rybałta</w>, który uczył dzieci czytania i śpiewu.",
                            "target_lang_translations": {
                                "ru": "В приходских хрониках записано имя <w>кантора</w>, который учил детей чтению и пению."
                            }
                        }
                    ],
                    "synonyms": [
                        "kantor",
                        "śpiewak kościelny"
                    ],
                    "common_phrases": [
                        "szkolny rybałt",
                        "obowiązki rybałta"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "określenie historyczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В старину: низший церковный служитель, занимающийся литургическим пением или преподаванием в приходской школе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "церковный певчий",
                                "target_lang_sense_clarification": "исполнитель церковных песнопений"
                            },
                            {
                                "target_lang_word": "кантор",
                                "target_lang_sense_clarification": "руководитель церковного хора или певец"
                            },
                            {
                                "target_lang_word": "служка",
                                "target_lang_sense_clarification": "низший церковный служитель"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "43383975-ae4b-323e-9412-0c0ed325e9aa",
                    "sense_definition": "Bevrijd van angst, spanning of zorgen; blij dat iets vervelends voorbij is of niet is gebeurd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij was zichtbaar <w>opgelucht</w> toen ze de goede uitslag van de toets hoorde.",
                            "target_lang_translations": {
                                "en": "She was visibly <w>relieved</w> when she heard the good result of the test."
                            }
                        },
                        {
                            "text": "We kunnen weer <w>opgelucht</w> ademhalen nu het gevaar eindelijk geweken is.",
                            "target_lang_translations": {
                                "en": "We can breathe a sigh of <w>relief</w> again now that the danger has finally passed."
                            }
                        }
                    ],
                    "synonyms": [
                        "gerustgesteld"
                    ],
                    "antonyms": [
                        "bezorgd"
                    ],
                    "common_phrases": [
                        "opgelucht ademhalen",
                        "zichtbaar opgelucht"
                    ],
                    "target_lang_definitions": {
                        "en": "Freed from fear, tension, or worry; glad that something unpleasant is over or did not happen."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "relieved",
                                "target_lang_sense_clarification": "feeling happy that something unpleasant has not happened or has ended"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e266ad2f-74c2-3c4c-89b4-bbef01fc8efb",
                    "sense_definition": "Voltooid deelwoord van <w>opluchten</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het goede nieuws heeft ons allemaal enorm <w>opgelucht</w>.",
                            "target_lang_translations": {
                                "en": "The good news <w>relieved</w> us all enormously."
                            }
                        },
                        {
                            "text": "Het heeft mij <w>opgelucht</w> om er eindelijk met iemand over te praten.",
                            "target_lang_translations": {
                                "en": "It <w>relieved</w> me to finally talk to someone about it."
                            }

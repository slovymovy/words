            "senses": [
                {
                    "sense_id": "6e3ad48f-858e-3166-9f9f-3478b0e8adc9",
                    "sense_definition": "Een handeling waarmee men iemand anders helpt of een plezier doet.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "help_favor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik je om een kleine <w>dienst</w> vragen?",
                            "target_lang_translations": {
                                "ru": "Могу я попросить тебя о маленькой <w>услуге</w>?"
                            }
                        },
                        {
                            "text": "Hij heeft mij een grote <w>dienst</w> bewezen door te helpen met de verhuizing.",
                            "target_lang_translations": {
                                "ru": "Он оказал мне большую <w>услугу</w>, помогая с переездом."
                            }
                        }
                    ],
                    "synonyms": [
                        "gunst",
                        "hulp"
                    ],
                    "common_phrases": [

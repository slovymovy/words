    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f1798b8c-e491-3c27-a572-e1bb277e91ae",
                    "sense_definition": "Испытывать физическую или нравственную боль, мучения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pain_illness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После развода она долго <страдала> от одиночества.",
                            "target_lang_translations": {
                                "en": "After the divorce, she <suffered> from loneliness for a long time."
                            }
                        },
                        {
                            "text": "Раненый солдат ужасно <страдал> от боли в ноге.",
                            "target_lang_translations": {
                                "en": "The wounded soldier <suffered> terribly from the pain in his leg."
                            }
                        }
                    ],
                    "synonyms": [
                        "мучиться"
                    ],
                    "antonyms": [
                        "наслаждаться"
                    ],
                    "common_phrases": [
                        "страдать от боли",
                        "страдать от любви",
                        "страдать от одиночества"
                    ],
                    "target_lang_definitions": {
                        "en": "To experience physical or emotional pain or torment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suffer",
                                "target_lang_sense_clarification": "to feel pain or distress"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c0f8c2b-ea06-31db-87ed-198ca04da830",
                    "sense_definition": "Иметь какую-либо болезнь, хронический недуг.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pain_illness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой дедушка часто <страдает> бессонницей по ночам.",
                            "target_lang_translations": {
                                "en": "My grandfather often <suffers> from insomnia at night."
                            }
                        },
                        {
                            "text": "В детстве этот ребёнок часто <страдал> аллергией.",
                            "target_lang_translations": {
                                "en": "As a child, this child often <suffered> from allergies."
                            }
                        }
                    ],
                    "synonyms": [
                        "болеть"
                    ],
                    "common_phrases": [
                        "страдать бессонницей",
                        "страдать аллергией",
                        "страдать ожирением"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a disease or chronic ailment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suffer",
                                "target_lang_sense_clarification": "to have a medical condition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5077dc8-c7f4-34e2-9823-4b1b757b8383",
                    "sense_definition": "Нести потери, убытки или подвергаться негативному воздействию.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "damage_defect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мирные жители больше всего <страдают> во время войны.",
                            "target_lang_translations": {
                                "en": "Civilians <suffer> the most during war."
                            }
                        },
                        {
                            "text": "Урожай в этом году сильно <страдал> от засухи.",
                            "target_lang_translations": {
                                "en": "The harvest <suffered> greatly from the drought this year."
                            }
                        }
                    ],
                    "common_phrases": [
                        "страдать от наводнения",
                        "репутация страдает",
                        "страдать от войны"
                    ],
                    "target_lang_definitions": {
                        "en": "To sustain losses, damage, or be negatively affected."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suffer",
                                "target_lang_sense_clarification": "to sustain damage or loss"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c58cb11c-2794-3109-84b1-a29455b3b326",
                    "sense_definition": "Быть неудовлетворительным, иметь недостатки (о качестве или свойствах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "damage_defect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за спешки качество работы заметно <страдает>.",
                            "target_lang_translations": {
                                "en": "Due to haste, the quality of the work noticeably <suffers>."
                            }

            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "81d0b50d-a78e-3179-81cf-da72c7152689",
                    "sense_definition": "W ilości dwa razy większej lub z dwukrotnie większą siłą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po uderzeniu w głowę zawodnik zaczął widzieć <w>podwójnie</w>.",
                            "target_lang_translations": {
                                "ru": "После удара по голове у спортсмена начало <w>двоиться</w> в глазах."
                            }
                        },
                        {
                            "text": "Za pracę w dni świąteczne pracodawca płaci nam <w>podwójnie</w>.",
                            "target_lang_translations": {
                                "ru": "За работу в праздничные дни работодатель платит нам <w>вдвойне</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dwukrotnie",
                        "dwa razy",
                        "w dwójnasób",
                        "dubeltowo",
                        "dwakroć"
                    ],
                    "common_phrases": [
                        "widzieć podwójnie",
                        "liczyć się podwójnie",
                        "zyskać podwójnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "В количестве, в два раза большем, или с удвоенной силой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вдвойне",
                                "target_lang_sense_clarification": "в два раза больше или сильнее"
                            },
                            {
                                "target_lang_word": "в два раза",
                                "target_lang_sense_clarification": "об увеличении количества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15cefe95-fd9b-3cec-868e-71800b216bda",
                    "sense_definition": "W sposób dwojaki; biorąc pod uwagę dwa różne aspekty, powody lub punkty widzenia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego tajemnicze zachowanie można interpretować <w>podwójnie</w>.",
                            "target_lang_translations": {
                                "ru": "Его загадочное поведение можно толковать <w>двояко</w>."
                            }
                        },
                        {
                            "text": "Jako lekarz i matka czuła się <w>podwójnie</w> odpowiedzialna za zdrowie dzieci.",
                            "target_lang_translations": {
                                "ru": "Как врач и мать, она чувствовала себя <w>вдвойне</w> ответственной за здоровье детей."
                            }
                        }
                    ],
                    "synonyms": [
                        "dwojako",
                        "dwoiście"
                    ],
                    "common_phrases": [
                        "interpretować podwójnie",
                        "rozumieć podwójnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двояким образом; учитывая два разных аспекта, причины или точки зрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двояко",
                                "target_lang_sense_clarification": "двумя способами, двусмысленно"
                            },
                            {
                                "target_lang_word": "вдвойне",
                                "target_lang_sense_clarification": "в силу двух причин или аспектов"
                            }
                        ]
                    }
                }
            ]
        }

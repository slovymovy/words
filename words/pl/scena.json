                {
                    "sense_id": "1227232a-2617-3592-9391-c793001206cc",
                    "sense_definition": "Podwyższone miejsce w teatrze lub sali widowiskowej, na którym występują aktorzy lub muzycy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "theater_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aktorzy powoli wchodzili na <w>scenę</w>, aby rozpocząć przedstawienie.",
                            "target_lang_translations": {
                                "ru": "Актеры медленно выходили на <w>сцену</w>, чтобы начать представление."
                            }
                        },
                        {
                            "text": "Światła na <w>scenie</w> zgasły, a publiczność zaczęła głośno bić brawo.",
                            "target_lang_translations": {
                                "ru": "Огни на <w>сцене</w> погасли, и публика начала громко аплодировать."
                            }
                        }
                    ],
                    "synonyms": [
                        "estrada"
                    ],
                    "common_phrases": [
                        "występować na scenie",
                        "wejść na scenę",
                        "zejść ze sceny",
                        "grać na scenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвышение в театре или концертном зале, на котором выступают актеры или музыканты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сцена",
                                "target_lang_sense_clarification": "площадка для выступлений"

                },
                {
                    "sense_id": "8a807e87-fc90-3ea6-a461-f1aa1b232287",
                    "sense_definition": "Поступок или действие, предпринятое для достижения какой-либо цели или решения проблемы.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_action",
                    "name_type": "no",
                    "examples": [

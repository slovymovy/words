    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ea1e990f-4cab-3cb2-8b7e-e094249387df",
                    "sense_definition": "The waging of war or armed conflict against an enemy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "war_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The region has been devastated by years of constant <w>warfare</w>.",
                            "target_lang_translations": {
                                "ru": "Регион был опустошен годами непрерывных <w>военных действий</w>."
                            }
                        },
                        {
                            "text": "Leaders met to discuss ways to end the <w>warfare</w> and restore peace.",
                            "target_lang_translations": {
                                "ru": "Лидеры встретились, чтобы обсудить способы прекращения <w>войны</w> и восстановления мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "combat",
                        "fighting",
                        "conflict",
                        "hostilities"
                    ],
                    "antonyms": [
                        "peace"
                    ],
                    "common_phrases": [
                        "engage in warfare",
                        "open warfare",
                        "modern warfare"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ведение войны или вооруженный конфликт против врага."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "военные действия",
                                "target_lang_sense_clarification": "вооруженный конфликт"
                            },
                            {
                                "target_lang_word": "ведение войны",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "война",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f824b54-f526-3f11-befd-05a1edd85e8a",
                    "sense_definition": "Military operations of a particular kind, usually specified by an adjective.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "war_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Guerrilla <w>warfare</w> relies on small mobile units to harass the enemy.",
                            "target_lang_translations": {
                                "ru": "Партизанская <w>война</w> полагается на небольшие мобильные отряды для изматывания врага."
                            }
                        },
                        {
                            "text": "The treaty bans the use of biological and chemical <w>warfare</w>.",
                            "target_lang_translations": {
                                "ru": "Договор запрещает ведение биологической и химической <w>войны</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "guerrilla warfare",
                        "chemical warfare",
                        "biological warfare",
                        "cyber warfare",
                        "trench warfare"
                    ],
                    "target_lang_definitions": {
                        "ru": "Военные операции определенного типа, обычно уточняемые прилагательным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "война",
                                "target_lang_sense_clarification": "в сочетаниях: партизанская, химическая и т.д."
                            },
                            {
                                "target_lang_word": "методы ведения войны",
                                "target_lang_sense_clarification": "способы борьбы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fd12e02d-b28d-36e4-9268-b5fb59f92183",
                    "sense_definition": "To lead a military life; to carry on continual wars.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient clans <w>warfared</w> against their neighbors for centuries.",
                            "target_lang_translations": {
                                "ru": "Древние кланы веками <w>воевали</w> со своими соседями."
                            }
                        },
                        {
                            "text": "He <w>warfared</w> in distant lands before returning home.",
                            "target_lang_translations": {
                                "ru": "Он <w>воевал</w> в дальних землях, прежде чем вернуться домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "battle",
                        "fight",
                        "war"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rarely used as a verb in modern English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести военную жизнь; вести непрерывные войны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воевать",
                                "target_lang_sense_clarification": "устаревшее использование"
                            },
                            {
                                "target_lang_word": "вести войну",
                                "target_lang_sense_clarification": "устаревшее использование"
                            }
                        ]
                    }
                }
            ]
        }
    ]

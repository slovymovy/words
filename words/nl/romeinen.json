            "senses": [
                {
                    "sense_id": "fac5ba1d-fbeb-3175-b5f7-24ad737a6c9b",
                    "sense_definition": "Meervoud van <w>Romein</w>: inwoners van de stad Rome.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "inhabitants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <Romeinen> zijn erg trots op de geschiedenis van hun stad.",
                            "target_lang_translations": {
                                "ru": "<Римляне> очень гордятся историей своего города.",
                                "en": "The <Romans> are very proud of the history of their city."
                            }
                        },
                        {
                            "text": "Veel <Romeinen> verlaten de stad in augustus vanwege de hitte.",
                            "target_lang_translations": {
                                "ru": "Многие <римляне> покидают город в августе из-за жары.",
                                "en": "Many <Romans> leave the city in August because of the heat."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жители города Рим.",
                        "en": "Inhabitants of the city of Rome."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "римляне",
                                "target_lang_sense_clarification": "жители города"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "Romans",
                                "target_lang_sense_clarification": "inhabitants of the city"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0bceb9a-a4d2-3b7d-9708-1bcaae8380da",
                    "sense_definition": "Het volk uit de oudheid dat een groot rijk rond de Middellandse Zee bestuurde.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "historical_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <Romeinen> bouwden stevige wegen die eeuwenlang meegingen.",
                            "target_lang_translations": {
                                "ru": "<Римляне> строили прочные дороги, которые служили веками.",
                                "en": "The <Romans> built sturdy roads that lasted for centuries."
                            }
                        },
                        {
                            "text": "Op school leren kinderen veel over de Grieken en de <Romeinen>.",
                            "target_lang_translations": {
                                "ru": "В школе дети много узнают о греках и <римлянах>.",
                                "en": "At school, children learn a lot about the Greeks and the <Romans>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de oude Romeinen",
                        "Grieken en Romeinen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Древний народ, управлявший огромной империей вокруг Средиземного моря.",
                        "en": "The ancient people who ruled a large empire around the Mediterranean Sea."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "римляне",
                                "target_lang_sense_clarification": "древний народ"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "Romans",
                                "target_lang_sense_clarification": "ancient people"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ffcd059-c727-3665-a1ee-556d81601f2c",
                    "sense_definition": "Meervoud van <w>romein</w>: een lettertype met rechtopstaande tekens.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "typography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In deze tekst worden cursieven en <romeinen> door elkaar gebruikt.",
                            "target_lang_translations": {

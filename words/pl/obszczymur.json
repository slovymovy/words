            "senses": [
                {
                    "sense_id": "01ae3adf-ff87-3288-b98c-9e6e274b0e6f",
                    "sense_definition": "Pijak, zazwyczaj zaniedbany i bezdomny, często przesiadujący pod sklepami z alkoholem",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "pijak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod sklepem monopolowym stał brudny <w>obszczymur</w> i zaczepiał przechodniów o drobne.",
                            "target_lang_translations": {
                                "ru": "У винного магазина стоял грязный <w>забулдыга</w> и клянчил мелочь у прохожих."
                            }
                        },
                        {
                            "text": "Ten stary <w>obszczymur</w> znowu awanturował się o tanie wino.",
                            "target_lang_translations": {
                                "ru": "Этот старый <w>алкаш</w> снова скандалил из-за дешевого вина."
                            }
                        }
                    ],
                    "synonyms": [
                        "zlejbruk",
                        "obszczymurek"
                    ],
                    "common_phrases": [
                        "pijany obszczymur",
                        "śmierdzący obszczymur"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo o wydźwięku pogardliwym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пьяница, обычно неопрятный и бездомный, часто околачивающийся у магазинов с алкоголем"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забулдыга",

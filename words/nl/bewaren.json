    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d14930d2-a273-332e-9eb8-50990ad9b936",
                    "sense_definition": "Iets in bezit houden en niet wegdoen, vaak om het later te gebruiken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt de overgebleven soep twee dagen in de koelkast <w>bewaren</w>.",
                            "target_lang_translations": {
                                "ru": "Оставшийся суп можно два дня <w>хранить</w> в холодильнике."
                            }
                        },
                        {
                            "text": "Mijn oma heeft al mijn tekeningen van vroeger goed <w>bewaard</w>.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка бережно <w>сохранила</w> все мои старые рисунки."
                            }
                        }
                    ],
                    "synonyms": [
                        "houden",
                        "behouden"
                    ],
                    "antonyms": [
                        "kwijtraken",
                        "verliezen",
                        "kwijtraken"
                    ],
                    "common_phrases": [
                        "voor later bewaren",
                        "in de koelkast bewaren",
                        "goed bewaren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Держать что-то у себя, не выбрасывать, часто для использования в будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хранить",
                                "target_lang_sense_clarification": "держать вещь где-либо"
                            },
                            {
                                "target_lang_word": "сохранять",
                                "target_lang_sense_clarification": "сберегать, не давать пропасть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7c11695-4149-39eb-84c9-4d5ac5e95bcf",
                    "sense_definition": "Zorgen dat een bepaalde toestand, sfeer of situatie blijft bestaan.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is belangrijk om in deze moeilijke situatie de rust te <w>bewaren</w>.",
                            "target_lang_translations": {
                                "ru": "В этой сложной ситуации важно <w>сохранять</w> спокойствие."
                            }
                        },
                        {
                            "text": "Met veel moeite wist de fietser zijn evenwicht te <w>bewaren</w>.",
                            "target_lang_translations": {
                                "ru": "С большим трудом велосипедисту удалось <w>сохранить</w> равновесие."
                            }
                        }
                    ],
                    "synonyms": [
                        "behouden",
                        "bewaken"
                    ],
                    "antonyms": [
                        "verliezen"
                    ],
                    "common_phrases": [
                        "de rust bewaren",
                        "het evenwicht bewaren",
                        "de vrede bewaren",
                        "afstand bewaren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поддерживать определенное состояние, атмосферу или ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сохранять",
                                "target_lang_sense_clarification": "поддерживать состояние (спокойствие, равновесие)"
                            },
                            {
                                "target_lang_word": "соблюдать",
                                "target_lang_sense_clarification": "придерживаться чего-либо (тишину, дистанцию)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87c645db-8e9e-30db-a068-b95d23516437",
                    "sense_definition": "Beschermen tegen gevaar of onheil; behoeden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moge God u <w>bewaren</w> op uw lange reis.",
                            "target_lang_translations": {
                                "ru": "Да <w>хранит</w> вас Бог в долгом путешествии."
                            }
                        },
                        {
                            "text": "Hij werd gelukkig voor een grote vergissing <w>bewaard</w>.",
                            "target_lang_translations": {
                                "ru": "К счастью, он был <w>убережен</w> от большой ошибки."
                            }
                        }
                    ],
                    "synonyms": [
                        "behoeden",
                        "bewaken",
                        "behouden"
                    ],
                    "common_phrases": [
                        "God beware u",
                        "bewaren voor"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "vooral in religieuze of plechtige context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Защищать от опасности или беды; оберегать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уберечь",
                                "target_lang_sense_clarification": "защитить от чего-то плохого"
                            },
                            {
                                "target_lang_word": "хранить",
                                "target_lang_sense_clarification": "оберегать (часто в религиозном контексте)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6c816f46-d565-38a6-8328-3493ff4c49da",
                    "sense_definition": "Jedna z części, na które podzielone jest jakieś wydarzenie, rozgrywka sportowa lub wybory, odbywająca się w określonym czasie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sequence_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kandydat na prezydenta wygrał wybory już w pierwszej <w>turze</w>.",
                            "target_lang_translations": {
                                "ru": "Кандидат в президенты выиграл выборы уже в первом <w>туре</w>."
                            }
                        },
                        {
                            "text": "Druga <w>tura</w> negocjacji zakończyła się bez osiągnięcia porozumienia.",
                            "target_lang_translations": {
                                "ru": "Второй <w>раунд</w> переговоров завершился без достижения соглашения."
                            }
                        }
                    ],
                    "synonyms": [
                        "etap",
                        "faza",
                        "runda",
                        "stadium"
                    ],
                    "common_phrases": [
                        "pierwsza tura",
                        "druga tura",
                        "tura wyborów",
                        "kolejna tura"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть мероприятия, спортивного состязания или выборов, проходящая в определенный промежуток времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тур",
                                "target_lang_sense_clarification": "этап соревнований или выборов"
                            },
                            {
                                "target_lang_word": "раунд",
                                "target_lang_sense_clarification": "часть переговоров или боя"
                            },
                            {
                                "target_lang_word": "этап",
                                "target_lang_sense_clarification": "стадия процесса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb07108e-6ea6-31be-8162-34f274f5fc54",
                    "sense_definition": "Grupa osób wykonująca jakąś czynność w wyznaczonym czasie, zmieniająca inną grupę lub następująca po niej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pierwsza <w>tura</w> turystów weszła do muzeum punktualnie o dziewiątej.",
                            "target_lang_translations": {
                                "ru": "Первая <w>группа</w> туристов вошла в музей ровно в девять."
                            }
                        },
                        {
                            "text": "Obiad na stołówce jest wydawany w dwóch <w>turach</w>.",
                            "target_lang_translations": {
                                "ru": "Обед в столовой выдается в две <w>смены</w>."
                            }
                        },
                        {
                            "text": "Kolejne <w>tury</w> uczestników otrzymały pełne wyposażenie.",
                            "target_lang_translations": {
                                "ru": "Следующие <w>партии</w> участников получили полное снаряжение."
                            }
                        }
                    ],
                    "synonyms": [
                        "podróż",
                        "jazda"
                    ],
                    "common_phrases": [
                        "pierwsza tura gości",
                        "w kilku turach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, выполняющая действие в определенное время, сменяя другую группу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смена",
                                "target_lang_sense_clarification": "группа людей, работающих или едящих в одно время"
                            },
                            {
                                "target_lang_word": "партия",
                                "target_lang_sense_clarification": "группа людей или предметов"
                            },
                            {
                                "target_lang_word": "группа",
                                "target_lang_sense_clarification": "совокупность людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfe430cd-53bb-305b-b140-3bf30b9eaca4",
                    "sense_definition": "Dłuższa wędrówka, podróż lub przejazd, często składająca się z kilku etapów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ich górska <w>tura</w> trwała ponad dwa tygodnie i była bardzo wyczerpująca.",
                            "target_lang_translations": {
                                "ru": "Их горный <w>поход</w> длился более двух недель и был очень утомительным."
                            }
                        },
                        {
                            "text": "Zimą organizujemy narciarską <w>turę</w> po okolicznych szczytach.",
                            "target_lang_translations": {
                                "ru": "Зимой мы организуем лыжный <w>тур</w> по окрестным вершинам."
                            }
                        }
                    ],
                    "synonyms": [
                        "podróż",
                        "jazda"
                    ],
                    "common_phrases": [
                        "tura narciarska"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "w znaczeniu ogólnej podróży, obecnie częściej w kontekście turystyki kwalifikowanej np. skiturowej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Длительная прогулка, путешествие или поездка, часто состоящая из нескольких этапов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поход",
                                "target_lang_sense_clarification": "пешее или горное путешествие"
                            },
                            {
                                "target_lang_word": "тур",
                                "target_lang_sense_clarification": "туристическая поездка, ски-тур"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "406c62ef-da20-3f99-b19f-ecd1bb2dc926",
                    "sense_definition": "Forma słowa <w>tur</w> (dopełniacz lub biernik liczby pojedynczej).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W herbie tego miasta można zobaczyć wizerunek <w>tura</w>.",
                            "target_lang_translations": {
                                "ru": "На гербе этого города можно увидеть изображение <w>тура</w>."
                            }
                        },
                        {
                            "text": "Dawni władcy polowali na potężnego <w>tura</w> w puszczach.",
                            "target_lang_translations": {
                                "ru": "Древние правители охотились на могучего <w>тура</w> в пущах."
                            }
                        }
                    ],
                    "traits": [

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2c297d62-901c-33f4-a491-112c48da1ac4",
                    "sense_definition": "At a time that was not long ago; a short while ago.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_past",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <recently> moved to a new apartment in the city center.",
                            "target_lang_translations": {
                                "ru": "Она <недавно> переехала в новую квартиру в центре города."
                            }
                        },
                        {
                            "text": "This is a <recently> published book about the history of art.",
                            "target_lang_translations": {
                                "ru": "Это <недавно> изданная книга об истории искусства."
                            }
                        }
                    ],
                    "synonyms": [
                        "just now",
                        "newly",
                        "freshly"
                    ],
                    "antonyms": [
                        "long ago",
                        "long since"
                    ],
                    "common_phrases": [
                        "recently discovered",
                        "recently married",
                        "most recently",
                        "more recently"
                    ],
                    "target_lang_definitions": {
                        "ru": "В недалеком прошлом; некоторое время назад."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недавно",
                                "target_lang_sense_clarification": "в прошлом, некоторое время назад"
                            },
                            {
                                "target_lang_word": "на днях",
                                "target_lang_sense_clarification": "совсем недавно, несколько дней назад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f42b942c-61d3-396e-9d7e-f37feea8579f",
                    "sense_definition": "During the period of time just before the present.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_past",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I haven't been sleeping very well <recently>.",
                            "target_lang_translations": {
                                "ru": "Я не очень хорошо сплю <в последнее время>."
                            }
                        },
                        {
                            "text": "Until <recently>, they had little interest in buying a new car.",
                            "target_lang_translations": {
                                "ru": "До <недавнего времени> они мало интересовались покупкой новой машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "lately",
                        "of late",
                        "as of late",
                        "latterly"
                    ],
                    "antonyms": [
                        "long ago"
                    ],
                    "common_phrases": [
                        "until recently",
                        "quite recently",
                        "very recently"
                    ],
                    "target_lang_definitions": {
                        "ru": "В течение последнего времени; за последнее время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в последнее время",
                                "target_lang_sense_clarification": "на протяжении недавнего периода"
                            },
                            {
                                "target_lang_word": "недавно",
                                "target_lang_sense_clarification": "в контексте 'до недавнего времени'"
                            }
                        ]
                    }
                }
            ]
        }

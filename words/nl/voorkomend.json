                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени от глагола \"voorkomen\" (предотвращать, препятствовать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предотвращающий",
                                "target_lang_sense_clarification": "не допускающий чего-либо нежелательного"
                            }
                        ]
                    }
                }
            ]
        }
    ]

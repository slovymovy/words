                    "sense_id": "79150abf-6229-3f0e-8e9b-c98238f4bef1",
                    "sense_definition": "A diminutive of the male given name <w>Mitchell</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "I introduced my friend <w>Mitch</w> to the rest of the group.",
                            "target_lang_translations": {
                                "ru": "Я представил своего друга <w>Митча</w> остальной группе."
                            }
                        },
                        {
                            "text": "Uncle <w>Mitch</w> is coming to visit us this weekend.",
                            "target_lang_translations": {
                                "ru": "Дядя <w>Митч</w> приедет навестить нас в эти выходные."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма мужского имени Митчелл."
                    },
                    "translations": {

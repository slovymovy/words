            "senses": [
                {
                    "sense_id": "0915af28-b4f4-39c5-8c37-a983d5964cce",
                    "sense_definition": "Having accepted something unpleasant that one cannot do anything about.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He gave a <w>resigned</w> sigh and started the difficult task all over again.",
                            "target_lang_translations": {
                                "ru": "Он издал <w>покорный</w> вздох и начал сложную задачу заново."
                            }
                        },
                        {
                            "text": "They seemed <w>resigned</w> to the fact that the flight would be cancelled.",
                            "target_lang_translations": {
                                "ru": "Они казались <w>смирившимися</w> с тем фактом, что рейс отменят."
                            }
                        }
                    ],
                    "synonyms": [
                        "acquiescent",
                        "submissive",
                        "stoic"
                    ],
                    "antonyms": [
                        "defiant",
                        "resistant"
                    ],
                    "common_phrases": [
                        "resigned to fate",
                        "resigned sigh",
                        "resigned acceptance",
                        "resigned look"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6cfc84e9-b881-3b4e-90ab-8f01a9650bec",
                    "sense_definition": "A hard earthen substance that can form rocks; especially, such substance when regarded as a building material.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "material_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient castle was built entirely of grey <w>stone</w>.",
                            "target_lang_translations": {
                                "ru": "Древний замок был построен целиком из серого <w>камня</w>."
                            }
                        },
                        {
                            "text": "They decided to build a <w>stone</w> wall around the garden.",
                            "target_lang_translations": {
                                "ru": "Они решили построить <w>каменную</w> стену вокруг сада."
                            }
                        }
                    ],
                    "synonyms": [
                        "rock"
                    ],
                    "common_phrases": [
                        "stone wall",
                        "carved in stone",
                        "heart of stone",
                        "stone age"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердая горная порода или минерал; строительный материал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "камень",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4da7f407-6210-31b7-b76c-ccadae5914ad",
                    "sense_definition": "A small piece of rock or pebble found on the ground.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "material_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He picked up a smooth <w>stone</w> and threw it into the lake.",
                            "target_lang_translations": {
                                "ru": "Он поднял гладкий <w>камень</w> и бросил его в озеро."
                            }
                        },
                        {
                            "text": "I have a small <w>stone</w> in my shoe that is hurting my foot.",
                            "target_lang_translations": {
                                "ru": "У меня в ботинке маленький <w>камешек</w>, который натирает ногу."
                            }
                        }
                    ],
                    "synonyms": [
                        "pebble",
                        "rock"
                    ],
                    "common_phrases": [
                        "throw a stone",
                        "stepping stone",
                        "rolling stone",
                        "leave no stone unturned"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой кусок горной породы, найденный на земле."
                    },
                    "translations": {
                        "ru": [

            "senses": [
                {
                    "sense_id": "bbe54360-dc8b-30b4-b6d8-b4a46fcd367b",
                    "sense_definition": "het verdrietige gevoel alleen te zijn en contact met anderen te missen",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gevoel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>eenzaamheid</w> overviel haar na het verlies van haar partner.",
                            "target_lang_translations": {
                                "ru": "<w>Одиночество</w> охватило её после потери партнера."
                            }
                        },
                        {
                            "text": "Veel mensen ervaren een diep gevoel van <w>eenzaamheid</w> in drukke steden.",
                            "target_lang_translations": {
                                "ru": "Многие люди испытывают глубокое чувство <w>одиночества</w> в шумных городах."
                            }
                        }
                    ],
                    "synonyms": [
                        "verlatenheid",
                        "isolement"
                    ],
                    "antonyms": [
                        "verbondenheid",
                        "gezelschap"
                    ],
                    "common_phrases": [
                        "sociale eenzaamheid",
                        "strijd tegen eenzaamheid",
                        "uit eenzaamheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грустное чувство, возникающее, когда человек находится один и ему не хватает общения с другими"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одиночество",
                                "target_lang_sense_clarification": "чувство покинутости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31b898fe-b6fc-393e-a9b8-c34ce14ec9d5",
                    "sense_definition": "de toestand waarin men zich bevindt als men alleen is, zonder gezelschap",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "toestand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zocht de <w>eenzaamheid</w> van de natuur op om tot rust te komen.",
                            "target_lang_translations": {
                                "ru": "Он искал <w>уединения</w> на природе, чтобы успокоиться."
                            }
                        },
                        {
                            "text": "In de <w>eenzaamheid</w> van zijn werkkamer kon de schrijver zich goed concentreren.",
                            "target_lang_translations": {
                                "ru": "В <w>уединении</w> своего кабинета писатель мог хорошо сосредоточиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "afzondering",
                        "alleen-zijn"
                    ],
                    "antonyms": [
                        "samenzijn"
                    ],
                    "common_phrases": [
                        "in eenzaamheid leven",
                        "totale eenzaamheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, в котором человек находится один, без компании"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уединение",
                                "target_lang_sense_clarification": "изоляция, покой"
                            },
                            {
                                "target_lang_word": "одиночество",
                                "target_lang_sense_clarification": "физическое состояние"
                            }
                        ]
                    }
                }
            ]
        }

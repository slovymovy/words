                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The interval from do to <w>ta</w> creates a specific harmonic tension.",
                            "target_lang_translations": {
                                "ru": "Интервал от до до <w>та</w> создает специфическое гармоническое напряжение."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5694c090-da09-380f-bdb7-f49d5e39fd50",
                    "sense_definition": "Puste plastry pszczele, z których wydobyto miód, przeznaczone zazwyczaj do przetopienia na wosk.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "beekeeping_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pszczelarz zebrał starą <w>woszczynę</w>, aby przetopić ją na czysty wosk.",
                            "target_lang_translations": {
                                "ru": "Пчеловод собрал старые <w>соты</w>, чтобы перетопить их на чистый воск."
                            }
                        },
                        {
                            "text": "W ulu znajdowała się ciemna <w>woszczyna</w>, którą należało wymienić na nową węzę.",
                            "target_lang_translations": {
                                "ru": "В улье находились темные <w>соты</w>, которые нужно было заменить на новую вощину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przetapianie woszczyny",
                        "ciemna woszczyna",
                        "stara woszczyna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пустые пчелиные соты, из которых извлечен мед, обычно предназначенные для переплавки на воск."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соты",
                                "target_lang_sense_clarification": "пчелиные ячейки"
                            },
                            {
                                "target_lang_word": "сушь",
                                "target_lang_sense_clarification": "пустые соты (профессиональный термин)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd56a4a6-7dff-3c1b-8bb7-68cd2a7fd3d2",
                    "sense_definition": "Żółtawa, tłusta substancja wydzielana przez gruczoły w przewodzie słuchowym, chroniąca ucho.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "body_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nadmiar <w>woszczyny</w> może prowadzić do tymczasowego pogorszenia słuchu.",
                            "target_lang_translations": {
                                "ru": "Избыток <w>ушной серы</w> может привести к временному ухудшению слуха."
                            }
                        },
                        {
                            "text": "Lekarz ostrożnie usunął zalegającą <w>woszczynę</w> z ucha pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врач осторожно удалил скопившуюся <w>ушную серу</w> из уха пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "woskowina"
                    ],
                    "common_phrases": [
                        "nadmiar woszczyny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Желтоватое жирное вещество, выделяемое железами в слуховом проходе, защищающее ухо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ушная сера",
                                "target_lang_sense_clarification": "секрет ушных желез"
                            }
                        ]
                    }
                }
            ]
        },
        {

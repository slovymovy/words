    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "927f849e-0483-363a-8588-1479a36d7a33",
                    "sense_definition": "Gwałtowne wyładowanie elektryczne w atmosferze, któremu towarzyszy huk i błysk; uderzenie pioruna.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "weather_phenomenon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas burzy potężny <w>grom</w> uderzył w stare drzewo za domem.",
                            "target_lang_translations": {
                                "ru": "Во время грозы мощный <w>гром</w> ударил в старое дерево за домом."
                            }
                        },
                        {
                            "text": "Huk był tak głośny, że brzmiał jak <w>grom</w> z jasnego nieba.",
                            "target_lang_translations": {
                                "ru": "Грохот был таким сильным, что прозвучал как <w>гром</w> среди ясного неба."
                            }
                        }
                    ],
                    "synonyms": [
                        "piorun"
                    ],
                    "common_phrases": [
                        "jak grom z jasnego nieba",
                        "huk gromu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резкий электрический разряд в атмосфере, сопровождающийся грохотом и вспышкой; удар молнии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гром",
                                "target_lang_sense_clarification": "звук, раскат"
                            },
                            {
                                "target_lang_word": "молния",
                                "target_lang_sense_clarification": "электрический разряд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0e277e5-9acc-3e3e-aa49-212b0433cb5f",
                    "sense_definition": "Ostre, głośne słowa krytyki, wyraz wielkiego gniewu lub potępienia; zwykle używane w liczbie mnogiej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_anger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor ciskał na pracowników <w>gromy</w> za niewykonanie planu sprzedażowego.",
                            "target_lang_translations": {
                                "ru": "Директор метал <w>громы</w> и молнии в сотрудников за невыполнение плана продаж."
                            }
                        },
                        {
                            "text": "Spodziewał się pochwały, a tymczasem spadły na niego same <w>gromy</w>.",
                            "target_lang_translations": {
                                "ru": "Он ожидал похвалы, а вместо этого на него обрушились одни <w>громы</w> и молнии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ciskać gromy"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "stylistic/literary usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Резкие, громкие слова критики, выражение сильного гнева или осуждения; обычно используется во множественном числе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "громы и молнии",
                                "target_lang_sense_clarification": "идиома: гневная критика"
                            },
                            {
                                "target_lang_word": "разнос",
                                "target_lang_sense_clarification": "разг. строгий выговор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74ae0b69-6414-3e50-884e-83c7c6bcf5be",
                    "sense_definition": "Forma gramatyczna (celownik liczby mnogiej) rzeczownika <w>gra</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzięki tym nowym <w>grom</w> planszowym spędzamy więcej czasu z rodziną.",
                            "target_lang_translations": {
                                "ru": "Благодаря этим новым настольным <w>играм</w> мы проводим больше времени с семьей."
                            }
                        },
                        {
                            "text": "Przyglądam się uważnie <w>grom</w> politycznym w naszym kraju.",
                            "target_lang_translations": {
                                "ru": "Я внимательно присматриваюсь к политическим <w>играм</w> в нашей стране."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "dative plural of 'gra'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма (дательный падеж множественного числа) существительного <w>gra</w> (игра)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "играм",
                                "target_lang_sense_clarification": "форма слова 'игра'"
                            }
                        ]
                    }
                }
            ]
        },

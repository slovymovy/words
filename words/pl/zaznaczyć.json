                {
                    "sense_id": "560d4aa0-cc3b-3f0e-b5dd-77f68d8cee14",
                    "sense_definition": "Postawić znak lub sygnał graficzny na jakiejś powierzchni, aby coś wskazać, wybrać lub wyróżnić.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "marking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <zaznaczyć> właściwą odpowiedź w formularzu, stawiając krzyżyk w odpowiedniej kratce.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <отметьте> правильный ответ в бланке, поставив крестик в соответствующей клетке."
                            }
                        },
                        {
                            "text": "Nauczyciel <zaznaczył> wszystkie błędy w moim wypracowaniu czerwonym długopisem.",
                            "target_lang_translations": {
                                "ru": "Учитель <отметил> все ошибки в моем сочинении красной ручкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "oznaczyć",
                        "naznaczyć"
                    ],
                    "common_phrases": [
                        "zaznaczyć na mapie",
                        "zaznaczyć odpowiedź",
                        "zaznaczyć ołówkiem",
                        "zaznaczyć teren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поставить знак или графический сигнал на какой-либо поверхности, чтобы что-то указать, выбрать или выделить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отметить",
                                "target_lang_sense_clarification": "поставить знак или галочку"
                            },
                            {
                                "target_lang_word": "пометить",
                                "target_lang_sense_clarification": "сделать пометку"
                            },
                            {
                                "target_lang_word": "обозначить",
                                "target_lang_sense_clarification": "указать на карте или схеме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8cef601-44ca-3f06-8de4-23dc7c464516",
                    "sense_definition": "Zwrócić uwagę na pewien fakt w rozmowie lub tekście, podkreślając jego wagę lub znaczenie.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym wyraźnie <zaznaczyć>, że nie zgadzam się z podjętą decyzją.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы четко <обозначить>, что не согласен с принятым решением."
                            }
                        },
                        {
                            "text": "Autor artykułu <zaznaczył> rolę edukacji w procesie rozwoju społeczeństwa.",
                            "target_lang_translations": {
                                "ru": "Автор статьи <отметил> роль образования в процессе развития общества."
                            }
                        }
                    ],
                    "synonyms": [
                        "uwydatnić",
                        "wyróżnić",
                        "wyróżnić"
                    ],
                    "common_phrases": [

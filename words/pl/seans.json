                },
                {
                    "sense_id": "892d6126-2771-3d61-8e82-1c5e08ebbe72",
                    "sense_definition": "Spotkanie z terapeutą, hipnotyzerem lub uzdrowicielem poświęcone konkretnym zabiegom lub ćwiczeniom",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent poddał się <w>seansowi</w> hipnozy, aby rzucić palenie.",
                            "target_lang_translations": {
                                "ru": "Пациент прошел <w>сеанс</w> гипноза, чтобы бросить курить."
                            }
                        },
                        {
                            "text": "Regularny <w>seans</w> jogi pomaga w redukcji stresu.",
                            "target_lang_translations": {

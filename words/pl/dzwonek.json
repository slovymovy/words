    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b64d06cf-15ba-3379-93cc-616036b90787",
                    "sense_definition": "Urządzenie sygnalizacyjne, elektryczne lub mechaniczne, używane np. przy drzwiach wejściowych, w szkole lub przy rowerze.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "signal_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ktoś nacisnął <w>dzwonek</w> u drzwi, więc poszedłem otworzyć.",
                            "target_lang_translations": {
                                "ru": "Кто-то нажал на дверной <w>звонок</w>, поэтому я пошел открывать."
                            }
                        },
                        {
                            "text": "Muszę naprawić <w>dzwonek</w> w moim rowerze, bo nie działa.",
                            "target_lang_translations": {
                                "ru": "Мне нужно починить <w>звонок</w> на моем велосипеде, так как он не работает."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dzwonek do drzwi",
                        "dzwonek rowerowy",
                        "szkolny dzwonek",
                        "nacisnąć dzwonek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сигнальное устройство, электрическое или механическое, используемое, например, у входной двери, в школе или на велосипеде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонок",
                                "target_lang_sense_clarification": "устройство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "598673db-574e-3076-a0ef-b0f5e6f0966b",
                    "sense_definition": "Dźwięk wydawany przez urządzenie sygnalizacyjne, często oznaczający początek lub koniec czegoś (np. lekcji).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "signal_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczniowie wybiegli z klasy, gdy tylko usłyszeli <w>dzwonek</w>.",
                            "target_lang_translations": {
                                "ru": "Ученики выбежали из класса, как только услышали <w>звонок</w>."
                            }
                        },
                        {
                            "text": "Ten głośny <w>dzwonek</w> obudził mnie w środku nocy.",
                            "target_lang_translations": {
                                "ru": "Этот громкий <w>звонок</w> разбудил меня посреди ночи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dzwonek na przerwę",
                        "dzwonek na lekcję",
                        "usłyszeć dzwonek",
                        "ostatni dzwonek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, издаваемый сигнальным устройством, часто обозначающий начало или конец чего-либо (например, урока)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонок",
                                "target_lang_sense_clarification": "звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa9dc64f-189e-39a2-a8eb-87f5cfb7c4a6",
                    "sense_definition": "Mały przedmiot w kształcie odwróconego kielicha z sercem w środku, który wydaje dźwięk przy potrząsaniu; zdrobnienie od słowa <w>dzwon</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pasterz zawiesił mały <w>dzwonek</w> na szyi owcy.",
                            "target_lang_translations": {
                                "ru": "Пастух повесил маленький <w>колокольчик</w> на шею овцы."
                            }
                        },
                        {
                            "text": "Święty Mikołaj trzymał w ręku złoty <w>dzwonek</w>.",
                            "target_lang_translations": {
                                "ru": "Санта-Клаус держал в руке золотой <w>колокольчик</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dzwoneczek",
                        "gw. (Górny Śląsk) zwónek"
                    ],
                    "common_phrases": [
                        "złoty dzwonek",
                        "dźwięk dzwonka",
                        "dzwonek u sań"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький предмет в форме перевернутой чаши с язычком внутри, издающий звук при встряхивании; уменьшительное от слова «колокол»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колокольчик",
                                "target_lang_sense_clarification": "маленький колокол"
                            },
                            {
                                "target_lang_word": "бубенчик",
                                "target_lang_sense_clarification": "круглый, с дробинкой внутри"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0317f05-0510-324f-b7d5-2d3841fd55e8",
                    "sense_definition": "Melodia lub sygnał dźwiękowy w telefonie informujący o połączeniu przychodzącym.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "signal_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Masz bardzo zabawny <w>dzwonek</w> w swoim nowym telefonie.",
                            "target_lang_translations": {
                                "ru": "У тебя очень смешной <w>рингтон</w> на новом телефоне."
                            }
                        },
                        {
                            "text": "Zmieniłem <w>dzwonek</w>, bo stary mnie denerwował.",
                            "target_lang_translations": {
                                "ru": "Я сменил <w>рингтон</w>, потому что старый меня раздражал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dzwonek w telefonie",
                        "ustawić dzwonek",
                        "głośny dzwonek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелодия или звуковой сигнал в телефоне, сообщающий о входящем вызове."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рингтон",
                                "target_lang_sense_clarification": "мелодия телефона"
                            },
                            {
                                "target_lang_word": "звонок",
                                "target_lang_sense_clarification": "сигнал вызова"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "001cc970-55b1-3aa0-a0fe-33ce8bf0da7c",
                    "sense_definition": "Roślina o kwiatach przypominających kształtem małe dzwony, często spotykana na łąkach lub w ogrodach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na górskiej łące zakwitł piękny, fioletowy <w>dzwonek</w>.",
                            "target_lang_translations": {
                                "ru": "На горном лугу расцвел красивый фиолетовый <w>колокольчик</w>."
                            }
                        },
                        {
                            "text": "Moja babcia posadziła w ogrodzie niebieskie <w>dzwonki</w>.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка посадила в саду синие <w>колокольчики</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kampanula"
                    ],
                    "common_phrases": [
                        "dzwonek polny",
                        "kwiat dzwonek",
                        "niebieski dzwonek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растение с цветами, напоминающими по форме маленькие колокола, часто встречающееся на лугах или в садах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колокольчик",
                                "target_lang_sense_clarification": "цветок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e455d077-31c4-3d40-a459-4a50ea1d200c",
                    "sense_definition": "Jeden z czterech kolorów w kartach, oznaczony czerwonym rombem.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "card_suit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mam żadnego <w>dzwonka</w>, więc muszę dobrać kartę.",
                            "target_lang_translations": {
                                "ru": "У меня нет ни одной <w>бубны</w>, поэтому мне нужно взять карту."
                            }
                        },
                        {
                            "text": "Dołożył króla <w>dzwonka</w> i wygrał całą partię.",
                            "target_lang_translations": {
                                "ru": "Он положил короля <w>бубен</w> и выиграл всю партию."
                            }
                        }
                    ],
                    "synonyms": [
                        "karo",
                        "szel"
                    ],
                    "antonyms": [
                        "wino",
                        "żołądź",
                        "serce",
                        "czerwień"
                    ],
                    "common_phrases": [
                        "walet dzwonek",
                        "as dzwonek",
                        "król dzwonek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из четырех карточных мастей, обозначаемая красным ромбом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бубны",
                                "target_lang_sense_clarification": "карточная масть"
                            },
                            {
                                "target_lang_word": "бубна",
                                "target_lang_sense_clarification": "карта этой масти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91eb3660-728e-3473-a319-ac0298e37884",
                    "sense_definition": "Instrument muzyczny perkusyjny, składający się z zestawu metalowych płytek lub rurek wydających dźwięki o różnej wysokości.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "instrument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej symfonii bardzo wyraźnie słychać orkiestrowe <w>dzwonki</w>.",
                            "target_lang_translations": {
                                "ru": "В этой симфонии очень отчетливо слышны оркестровые <w>колокольчики</w>."
                            }
                        },
                        {
                            "text": "Dzieci w szkole muzycznej uczą się grać na <w>dzwonkach</w>.",
                            "target_lang_translations": {
                                "ru": "Дети в музыкальной школе учатся играть на <w>колокольчиках</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grać na dzwonkach",
                        "dzwonki chromatyczne",
                        "dzwonki orkiestrowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ударный музыкальный инструмент, состоящий из набора металлических пластинок или трубок, издающих звуки разной высоты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колокольчики",
                                "target_lang_sense_clarification": "музыкальный инструмент"
                            },
                            {
                                "target_lang_word": "металлофон",
                                "target_lang_sense_clarification": "учебный инструмент"
                            }
                        ]
                    }
                }
            ]
        },
        {

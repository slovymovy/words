        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "990b20c6-6b26-36d1-b0d6-b825963dca0e",
                    "sense_definition": "To unfairly influence someone's opinions, decisions, or judgment.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The newspapers attempted to <w>bias</w> the jury before the trial began.",
                            "target_lang_translations": {
                                "ru": "Газеты пытались <w>настроить</w> присяжных до начала суда."
                            }
                        },
                        {
                            "text": "Do not let personal feelings <w>bias</w> your judgment of the situation.",
                            "target_lang_translations": {
                                "ru": "Не позволяйте личным чувствам <w>влиять</w> на ваше суждение о ситуации."
                            }

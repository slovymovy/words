                {
                    "sense_id": "74211564-72f0-379c-8776-6925d50685c5",
                    "sense_definition": "Rodzaj twórczości literackiej, filmowej lub plastycznej, który przedstawia świat i wydarzenia nierealne, często magiczne lub futurystyczne.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W bibliotece dział z <w>fantastyką</w> cieszy się największym zainteresowaniem młodzieży.",
                            "target_lang_translations": {
                                "ru": "В библиотеке отдел <w>фантастики</w> пользуется наибольшим интересом у молодёжи."
                            }
                        },
                        {
                            "text": "Ten reżyser kręci głównie <w>fantastykę</w> pełną kosmitów i statków kosmicznych.",
                            "target_lang_translations": {
                                "ru": "Этот режиссёр снимает в основном <w>фантастику</w>, полную инопланетян и космических кораблей."
                            }
                        }
                    ],
                    "antonyms": [
                        "literatura faktu",
                        "dokument"
                    ],
                    "common_phrases": [
                        "fantastyka naukowa",
                        "polska fantastyka",
                        "miłośnik fantastyki",
                        "literatura i fantastyka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жанр литературы, кино или искусства, изображающий нереальные, часто магические или футуристические миры и события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фантастика",
                                "target_lang_sense_clarification": "жанр искусства"

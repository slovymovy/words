{
    "word_family": [
        "twarzyczka",
        "twarzowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "201aac33-a4bf-3540-82ea-9dae1a7e8788",
                    "sense_definition": "Przednia część głowy człowieka, zawierająca usta, nos i oczy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy_01",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przedszkolanka kazała dzieciom umyć <w>twarze</w>.",
                            "target_lang_translations": {
                                "ru": "Воспитательница велела детям умыть <w>лица</w>."
                            }
                        },
                        {
                            "text": "Jej <w>twarz</w> była blada ze zmęczenia.",
                            "target_lang_translations": {
                                "ru": "Её <w>лицо</w> было бледным от усталости."
                            }
                        }
                    ],
                    "synonyms": [
                        "buzia",
                        "oblicze",
                        "lico",
                        "fizys",
                        "facjata",
                        "gęba"
                    ],
                    "antonyms": [
                        "potylica"
                    ],
                    "common_phrases": [
                        "myć twarz",
                        "wyraz twarzy",
                        "prosto w twarz",
                        "znajoma twarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передняя часть головы человека, на которой расположены рот, нос и глаза."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лицо",
                                "target_lang_sense_clarification": "часть головы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f647a252-f8c6-3e75-bc25-4fc320d97bed",
                    "sense_definition": "Osoba, która powszechnie jest kojarzona z czymś, np. z marką, instytucją lub ideą.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_rep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza Jola stała się ostatnio <w>twarzą</w> pewnego kolorowego magazynu.",
                            "target_lang_translations": {
                                "ru": "Наша Йоля стала недавно <w>лицом</w> одного глянцевого журнала."
                            }
                        },
                        {
                            "text": "Znany piłkarz został nową <w>twarzą</w> kampanii reklamowej.",
                            "target_lang_translations": {
                                "ru": "Известный футболист стал новым <w>лицом</w> рекламной кампании."
                            }
                        }
                    ],
                    "synonyms": [
                        "osoba"
                    ],
                    "common_phrases": [
                        "twarz kampanii",
                        "nowa twarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который публично представляет что-либо, например, бренд, учреждение или идею."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лицо",
                                "target_lang_sense_clarification": "представитель бренда или идеи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b190958e-887b-326a-9464-957a631fe389",
                    "sense_definition": "Specyficzny aspekt, charakter lub wariant czegoś; sposób, w jaki ktoś lub coś się prezentuje.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "aspect_01",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Strach ma wiele <w>twarzy</w>.",
                            "target_lang_translations": {
                                "ru": "У страха много <w>лиц</w>."
                            }
                        },
                        {
                            "text": "Ten polityk w ostatnim wystąpieniu pokazał ludzką <w>twarz</w>.",
                            "target_lang_translations": {
                                "ru": "Этот политик в последнем выступлении показал человеческое <w>лицо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "oblicze"
                    ],
                    "common_phrases": [
                        "ludzka twarz",
                        "wiele twarzy",
                        "prawdziwa twarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфический аспект, характер или вариант чего-либо; то, как кто-то или что-то себя проявляет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лицо",
                                "target_lang_sense_clarification": "сущность, характер"
                            },
                            {
                                "target_lang_word": "облик",
                                "target_lang_sense_clarification": "вид, образ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36d5e45c-fc07-3bdd-a46d-742843d9d9c8",
                    "sense_definition": "Dobra reputacja, godność i szacunek w oczach innych.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "reputation_01",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciał wycofać się z obietnicy, ale musiał zachować <w>twarz</w>.",
                            "target_lang_translations": {
                                "ru": "Он хотел отказаться от обещания, но должен был сохранить <w>лицо</w>."
                            }
                        },
                        {
                            "text": "Przez to kłamstwo dyrektor stracił <w>twarz</w> przed pracownikami.",
                            "target_lang_translations": {
                                "ru": "Из-за этой лжи директор потерял <w>лицо</w> перед сотрудниками."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zachować twarz",
                        "stracić twarz",
                        "ratować twarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошая репутация, достоинство и уважение в глазах окружающих."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лицо",
                                "target_lang_sense_clarification": "репутация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b9e1abd8-7ea7-3db0-a9d4-6918886ebab2",
                    "sense_definition": "Сопровождать кого-либо или что-либо, указывая путь или помогая идти (обычно регулярно, туда и обратно или в разных направлениях).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_guide",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама каждый день <водит> сына в детский сад.",
                            "target_lang_translations": {
                                "en": "Mom <takes> her son to kindergarten every day."
                            }
                        },
                        {
                            "text": "Экскурсовод <водил> туристов по музею целый час.",
                            "target_lang_translations": {
                                "en": "The tour guide <led> the tourists around the museum for a whole hour."
                            }
                        }
                    ],
                    "synonyms": [
                        "вести"
                    ],
                    "common_phrases": [
                        "водить детей в школу",
                        "водить экскурсии",
                        "водить за руку",
                        "водить за нос"
                    ],
                    "target_lang_definitions": {
                        "en": "To accompany or guide someone to a destination, often on foot or regularly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "someone to a place"
                            },
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "guide or conduct"
                            },
                            {
                                "target_lang_word": "guide",
                                "target_lang_sense_clarification": "show the way"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3460fd9e-1ae5-328c-9a69-e2dac2c47c61",
                    "sense_definition": "Управлять транспортным средством (машиной, автобусом и т. д.).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_guide",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат отлично <водит> машину.",
                            "target_lang_translations": {
                                "en": "My brother <drives> a car excellently."
                            }
                        },
                        {
                            "text": "Я пока не умею <водить>, но хочу научиться.",
                            "target_lang_translations": {
                                "en": "I don't know how to <drive> yet, but I want to learn."
                            }
                        }
                    ],
                    "synonyms": [
                        "вести"
                    ],
                    "common_phrases": [
                        "водить машину",
                        "водить автомобиль",
                        "уметь водить",
                        "водить аккуратно"
                    ],
                    "target_lang_definitions": {
                        "en": "To operate and control the direction of a vehicle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "a vehicle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bbd62a4-4efa-3e1c-8c62-63f3aeb44965",
                    "sense_definition": "Двигать чем-либо по поверхности чего-либо (например, рукой, смычком, курсором).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "motion_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он задумчиво <водил> карандашом по бумаге.",
                            "target_lang_translations": {
                                "en": "He thoughtfully <moved> the pencil across the paper."
                            }
                        },
                        {
                            "text": "Не нужно <водить> пальцем по строчкам, когда читаешь книгу.",
                            "target_lang_translations": {
                                "en": "You shouldn't <run> your finger along the lines when reading a book."
                            }
                        }
                    ],
                    "common_phrases": [
                        "водить пальцем",
                        "водить смычком",
                        "водить мышкой",
                        "водить глазами"
                    ],
                    "target_lang_definitions": {
                        "en": "To move an object or body part across a surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "a finger or object over a surface"
                            },
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "an object"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e68ad165-5f6e-37ef-bbda-cda06e3dea91",
                    "sense_definition": "Исполнять главную роль в детских играх (догонять других, искать спрятавшихся).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы играли в прятки, и я <водил> первым.",
                            "target_lang_translations": {
                                "en": "We played hide-and-seek, and I <was \"it\"> first."
                            }
                        },
                        {
                            "text": "В этой игре <водит> тот, кого поймали.",
                            "target_lang_translations": {
                                "en": "In this game, the one who gets caught <is \"it\">."
                            }
                        }
                    ],
                    "common_phrases": [
                        "тебе водить",
                        "кто водит"
                    ],
                    "target_lang_definitions": {
                        "en": "To play the active role (the seeker or chaser) in children's games."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be \"it\"",
                                "target_lang_sense_clarification": "in games like tag or hide-and-seek"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed5e554b-ef1c-3c47-b287-72508813e8b0",
                    "sense_definition": "Поддерживать отношения или совершать круговые движения (обычно используется в устойчивых выражениях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "idiomatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Они давно перестали <водить> дружбу с соседями.",
                            "target_lang_translations": {
                                "en": "They stopped <maintaining> a friendship with the neighbors a long time ago."
                            }
                        },
                        {
                            "text": "Дети взялись за руки и начали <водить> хоровод вокруг ёлки.",
                            "target_lang_translations": {
                                "en": "The children held hands and started to <lead> a round dance around the Christmas tree."
                            }
                        }
                    ],
                    "common_phrases": [
                        "водить дружбу",
                        "водить знакомство",
                        "водить хоровод"
                    ],
                    "target_lang_definitions": {
                        "en": "Used in set phrases regarding maintaining relationships or performing circular dances."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "maintain",
                                "target_lang_sense_clarification": "a friendship or acquaintance"
                            },
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "a round dance"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c1baf323-bb33-3d3f-9428-b1b45ca7f32e",
                    "sense_definition": "Mężczyzna, który oświadczył się kobiecie i planuje z nią ślub.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "relacje",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>narzeczony</w> kupił mi piękny pierścionek zaręczynowy.",
                            "target_lang_translations": {
                                "ru": "Мой <w>жених</w> купил мне красивое помолвочное кольцо."
                            }
                        },
                        {
                            "text": "Jej <w>narzeczony</w> mieszka w innym mieście, więc widują się rzadko.",
                            "target_lang_translations": {
                                "ru": "Её <w>жених</w> живёт в другом городе, поэтому они видятся редко."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mój narzeczony",
                        "były narzeczony",
                        "zostać narzeczonym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, который сделал предложение женщине и планирует вступить с ней в брак."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жених",
                                "target_lang_sense_clarification": "будущий муж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7139fc2a-7e94-3a75-95e3-d626e1ffe2ab",
                    "sense_definition": "Mężczyzna żyjący z kobietą w stałym związku bez ślubu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "relacje",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkają razem od dziesięciu lat, ale to wciąż tylko jej <w>narzeczony</w>.",
                            "target_lang_translations": {
                                "ru": "Они живут вместе десять лет, но он всё ещё только её <w>жених</w>."
                            }
                        },
                        {
                            "text": "Przedstawiła go rodzinie jako swojego <w>narzeczonego</w>, choć nie planują wesela.",
                            "target_lang_translations": {
                                "ru": "Она представила его семье как своего <w>жениха</w>, хотя свадьбу они не планируют."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Śląsk Cieszyński) galan",
                        "gw. (Górny Śląsk) szac"
                    ],
                    "common_phrases": [
                        "wieczny narzeczony"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często eufemistycznie o konkubencie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, живущий с женщиной в постоянных отношениях без регистрации брака."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гражданский муж",
                                "target_lang_sense_clarification": "фактический супруг"
                            },
                            {
                                "target_lang_word": "сожитель",
                                "target_lang_sense_clarification": "партнёр по совместному проживанию"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "44496d7d-9214-370d-9b1d-4000dff4b20d",
                    "sense_definition": "Участок земли, засаженный деревьями, кустарниками и цветами для красоты, отдыха или получения плодов.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "garden_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашем <w>саду</w> каждую весну расцветают яблони и вишни.",
                            "target_lang_translations": {
                                "en": "Apple and cherry trees blossom in our <w>garden</w> every spring."
                            }
                        },
                        {
                            "text": "Мы любим гулять в старом городском <w>саду</w>.",
                            "target_lang_translations": {
                                "en": "We love walking in the old city <w>garden</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "парк",
                        "сквер"
                    ],
                    "common_phrases": [
                        "фруктовый сад",
                        "зимний сад",
                        "ботанический сад",
                        "райский сад"
                    ],
                    "target_lang_definitions": {
                        "en": "A plot of land planted with trees, shrubs, and flowers for beauty, relaxation, or fruit production."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "garden",
                                "target_lang_sense_clarification": "general term for an area with plants"
                            },
                            {
                                "target_lang_word": "orchard",
                                "target_lang_sense_clarification": "specifically for fruit trees"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d326f6f-3579-3a5b-a6ec-6ac7a877a7a3",
                    "sense_definition": "Разговорное сокращение от «детский сад»; учреждение для дневного пребывания и воспитания детей дошкольного возраста.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "kindergarten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой сын уже второй год ходит в <w>сад</w>.",
                            "target_lang_translations": {
                                "en": "My son has been going to <w>kindergarten</w> for two years now."
                            }
                        },
                        {
                            "text": "Нужно успеть забрать ребёнка из <w>сада</w> до шести вечера.",
                            "target_lang_translations": {
                                "en": "I need to pick up the child from <w>kindergarten</w> before six in the evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "де́тский сад"
                    ],
                    "common_phrases": [
                        "ходить в сад",
                        "место в саду"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for детский сад"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial short for 'kindergarten'; an institution for the daily care and education of preschool children."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kindergarten",
                                "target_lang_sense_clarification": "school for young children"
                            },
                            {
                                "target_lang_word": "nursery school",
                                "target_lang_sense_clarification": "preschool education"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "573757f1-2faa-34fd-887a-ec80ae1bcace",
                    "sense_definition": "Земельный участок в садовом товариществе, часто с домом, используемый для отдыха и выращивания растений (дача).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "dacha",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На майские праздники мы всей семьёй поедем в <w>сад</w> жарить шашлыки.",
                            "target_lang_translations": {
                                "en": "For the May holidays, our whole family will go to the <w>summer cottage</w> to barbecue."
                            }
                        },
                        {
                            "text": "Родители всё лето живут в <w>саду</w> и редко приезжают в город.",
                            "target_lang_translations": {
                                "en": "My parents live at the <w>summer cottage</w> all summer and rarely come to the city."
                            }
                        }
                    ],
                    "synonyms": [
                        "СНТ",
                        "СОТ"
                    ],
                    "common_phrases": [
                        "поехать в сад",
                        "домик в саду"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in regions to mean dacha or SNT"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A plot of land in a gardening association, often with a house, used for recreation and growing plants (dacha)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "allotment",
                                "target_lang_sense_clarification": "plot of land rented for growing vegetables"
                            },
                            {
                                "target_lang_word": "summer cottage",
                                "target_lang_sense_clarification": "house used for summer holidays"
                            },
                            {
                                "target_lang_word": "dacha",
                                "target_lang_sense_clarification": "Russian summer house"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebc43ac2-1b30-3f0f-9f51-4e45009a7a4b",
                    "sense_definition": "Участок земли, предназначенный для выращивания овощей; огород.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "vegetable_garden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка с утра работает в <w>саду</w>, пропалывает грядки с морковью.",
                            "target_lang_translations": {
                                "en": "Grandma has been working in the <w>vegetable garden</w> since morning, weeding the carrot beds."
                            }
                        },
                        {
                            "text": "В этом году мы посадили в <w>саду</w> только картошку и лук.",
                            "target_lang_translations": {
                                "en": "This year we planted only potatoes and onions in the <w>vegetable garden</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "огород"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "in some dialects сад implies vegetable garden"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A plot of land intended for growing vegetables; a vegetable garden."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vegetable garden",
                                "target_lang_sense_clarification": "area for growing food"
                            },
                            {
                                "target_lang_word": "kitchen garden",
                                "target_lang_sense_clarification": "garden for household food"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "51aa6879-c7c1-35f9-8307-fffdf061b5c2",
                    "sense_definition": "Предмет одежды в виде длинной полосы ткани или вязания, который носят на шее для тепла или красоты.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В холодную погоду я всегда надеваю тёплый <w>шарф</w>.",
                            "target_lang_translations": {
                                "en": "In cold weather, I always put on a warm <w>scarf</w>."
                            }
                        },
                        {
                            "text": "Бабушка связала мне длинный шерстяной <w>шарф</w>.",
                            "target_lang_translations": {
                                "en": "Grandma knitted me a long wool <w>scarf</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "кашне"
                    ],
                    "common_phrases": [
                        "тёплый шарф",
                        "вязаный шарф",
                        "завязать шарф",
                        "носить шарф"
                    ],
                    "target_lang_definitions": {
                        "en": "An item of clothing consisting of a long strip of fabric or knitted material, worn around the neck for warmth or style."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scarf",
                                "target_lang_sense_clarification": "garment worn around the neck"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4686280-6438-3cbf-b7e2-80df6dec81d0",
                    "sense_definition": "Офицерский пояс или повязка через плечо, служившая знаком воинского звания в старой армии.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "historical_clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Генерал появился на балу в парадном мундире и с серебряным <w>шарфом</w>.",
                            "target_lang_translations": {
                                "en": "The general appeared at the ball in a dress uniform and with a silver <w>sash</w>."
                            }
                        },
                        {
                            "text": "Старинный офицерский <w>шарф</w> хранится в музее как реликвия.",
                            "target_lang_translations": {
                                "en": "An antique officer's <w>sash</w> is kept in the museum as a relic."
                            }
                        }
                    ],
                    "synonyms": [
                        "перевязь",
                        "кушак"
                    ],
                    "common_phrases": [
                        "офицерский шарф",
                        "парадный шарф"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "исторический термин"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An officer's belt or sash worn across the shoulder, serving as a sign of military rank in the old army."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sash",
                                "target_lang_sense_clarification": "military insignia worn across the body"
                            }
                        ]
                    }
                }
            ]
        }
    ]

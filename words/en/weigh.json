    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8dc32bf8-d07f-38af-af3b-4e892506b3a4",
                    "sense_definition": "To have a specific heaviness or mass.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>weigh</w> about seventy kilograms.",
                            "target_lang_translations": {
                                "ru": "Я <w>вешу</w> около семидесяти килограммов."
                            }
                        },
                        {
                            "text": "The suitcase does not <w>weigh</w> much, so it is easy to carry.",
                            "target_lang_translations": {
                                "ru": "Чемодан не много <w>весит</w>, поэтому его легко нести."
                            }
                        }
                    ],
                    "common_phrases": [
                        "weigh a ton"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь определенный вес или массу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "весить",
                                "target_lang_sense_clarification": "to have a certain weight"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbb84ebf-8363-311c-b9f4-0431a04b94e9",
                    "sense_definition": "To measure how heavy someone or something is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nurse <w>weighed</w> the baby during the checkup.",
                            "target_lang_translations": {
                                "ru": "Медсестра <w>взвесила</w> ребенка во время осмотра."
                            }
                        },
                        {
                            "text": "Please <w>weigh</w> the vegetables before you pay for them.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>взвесьте</w> овощи перед тем, как платить за них."
                            }
                        }
                    ],
                    "synonyms": [
                        "measure"
                    ],
                    "common_phrases": [
                        "weigh in",
                        "weigh out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Измерять вес кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взвешивать",
                                "target_lang_sense_clarification": "to measure weight (imperfective)"
                            },
                            {
                                "target_lang_word": "взвесить",
                                "target_lang_sense_clarification": "to measure weight (perfective)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c920da6-7ea1-3106-ab4b-0b36189ed54e",

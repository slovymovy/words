    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "20edff90-dc0c-3d78-b2a1-b713e61ec9c7",
                    "sense_definition": "forma dopełniacza liczby mnogiej rzeczownika <w>kura</w>",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na podwórku babci biegało mnóstwo <kur> i kaczek.",
                            "target_lang_translations": {
                                "ru": "По бабушкиному двору бегало множество <кур> и уток."
                            }
                        },
                        {
                            "text": "W tym kurniku nie ma już żadnych <kur>.",
                            "target_lang_translations": {
                                "ru": "В этом курятнике уже нет никаких <кур>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "форма родительного падежа множественного числа существительного <w>kura</w> (курица)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кур",
                                "target_lang_sense_clarification": "родительный падеж мн. ч. от 'курица'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab476764-ab4a-3d79-a05e-0a5e435fabdd",
                    "sense_definition": "dorosły samiec kury domowej; słowo obecnie rzadziej używane w mowie potocznej, występujące głównie w literaturze, przysłowiach lub stylizacjach",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_male",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy tylko zapiał <kur>, wszyscy we wsi wstali do pracy.",
                            "target_lang_translations": {
                                "ru": "Как только пропел <петух>, все в деревне встали на работу."
                            }
                        },
                        {
                            "text": "Stary <kur> dumnie kroczył po płocie, pilnując swojego stada.",
                            "target_lang_translations": {
                                "ru": "Старый <петух> гордо шагал по забору, охраняя свою стаю."
                            }
                        }
                    ],
                    "synonyms": [
                        "kogut"
                    ],
                    "antonyms": [
                        "kura"
                    ],
                    "common_phrases": [
                        "kur pieje",
                        "złoty kur"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "replaced by 'kogut' in modern speech"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "often found in folklore or literature"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "взрослый самец домашней курицы; слово, ныне редко используемое в разговорной речи, встречающееся в основном в литературе или пословицах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "петух",
                                "target_lang_sense_clarification": "общеупотребительное слово"
                            },
                            {
                                "target_lang_word": "кур",
                                "target_lang_sense_clarification": "устаревшее или поэтическое"
                            },
                            {
                                "target_lang_word": "кочет",
                                "target_lang_sense_clarification": "народно-поэтическое или диалектное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "922cc04f-18f1-391b-a7e3-f816d0deac1c",
                    "sense_definition": "członek dawnego ludu bałtyjskiego zamieszkującego krainę historyczną zwaną Kurlandią (obecnie zachodnia Łotwa)",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "ethnicity",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "Każdy <Kur> był doskonałym żeglarzem i wojownikiem.",
                            "target_lang_translations": {
                                "ru": "Каждый <курш> был превосходным мореплавателем и воином."
                            }
                        },
                        {
                            "text": "Historycy badają język, którym posługiwał się starożytny <Kur>.",
                            "target_lang_translations": {
                                "ru": "Историки изучают язык, на котором говорил древний <курш>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "представитель древнего балтийского народа, населявшего историческую область Курляндию (ныне западная Латвия)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курш",
                                "target_lang_sense_clarification": "представитель народа куршей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6febda15-be04-3dd6-8d34-e38e4944fc5b",
                    "sense_definition": "drapieżna ryba morska z rodziny głowaczowatych, występująca w zimnych wodach północnych",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sieci rybaka zaplątał się rzadki <kur> rogacz.",
                            "target_lang_translations": {
                                "ru": "В сети рыбака запутался редкий <керчак>."
                            }
                        },
                        {
                            "text": "Ten gatunek ryby, znany jako <kur>, żyje przy dnie morza.",
                            "target_lang_translations": {
                                "ru": "Этот вид рыбы, известный как <керчак>, обитает на дне моря."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kur rogacz",
                        "kur diabeł"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical or regional biological name"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "хищная морская рыба семейства рогатковых, обитающая в холодных северных водах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "керчак",
                                "target_lang_sense_clarification": "зоологическое название"
                            },
                            {
                                "target_lang_word": "бычок",
                                "target_lang_sense_clarification": "разговорное название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "92b85084-6007-3dc5-b4ec-c0d16dda43a6",
                    "sense_definition": "rzeka w azjatyckiej części Rosji, która wraz z rzeką Urmi tworzy Tunguskę",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geography",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Rzeka <Kur> przepływa przez dzikie i niedostępne tereny Syberii.",
                            "target_lang_translations": {
                                "ru": "Река <Кур> протекает через дикие и недоступные территории Сибири."
                            }
                        },
                        {
                            "text": "Ekspedycja wyruszyła w górę rzeki <Kur>, aby zbadać lokalną faunę.",
                            "target_lang_translations": {
                                "ru": "Экспедиция отправилась вверх по реке <Кур>, чтобы изучить местную фауну."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rzeka Kur",
                        "dolina rzeki Kur"
                    ],
                    "target_lang_definitions": {
                        "ru": "река в азиатской части России, которая вместе с рекой Урми образует Тунгуску"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кур",
                                "target_lang_sense_clarification": "река"
                            }
                        ]
                    }
                }
            ]
        }
    ]

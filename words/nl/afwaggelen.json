{
    "word_family": [
        "waggelen",
        "waggel",
        "waggelgang"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "35adbe2f-5343-36e6-8298-748d6c880422",
                    "sense_definition": "Op een onzekere, waggelende manier naar beneden gaan.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "movement_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dronken man kwam gevaarlijk de steile trap <w>afwaggelen</w>.",
                            "target_lang_translations": {
                                "ru": "Пьяный мужчина опасно <w>спускался, шатаясь,</w> по крутой лестнице.",
                                "en": "The drunken man came dangerously <w>staggering down</w> the steep stairs."
                            }
                        },
                        {
                            "text": "We zagen de jonge eendjes de oever <w>afwaggelen</w> richting het water.",
                            "target_lang_translations": {
                                "ru": "Мы видели, как утята <w>спускались, переваливаясь,</w> с берега к воде.",
                                "en": "We saw the young ducklings <w>waddle down</w> the bank towards the water."
                            }
                        }
                    ],
                    "synonyms": [
                        "afdalen",
                        "afstrompelen",
                        "afstommelen"
                    ],
                    "common_phrases": [
                        "de trap afwaggelen",
                        "de heuvel afwaggelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спускаться неуверенной, шатающейся или переваливающейся походкой.",
                        "en": "To descend in an unsteady, wobbling, or waddling manner."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спускаться, шатаясь",
                                "target_lang_sense_clarification": "о неустойчивой походке, например, пьяного"
                            },
                            {
                                "target_lang_word": "спускаться, переваливаясь",
                                "target_lang_sense_clarification": "о походке вразвалку, например, уток"
                            },
                            {
                                "target_lang_word": "ковылять вниз",
                                "target_lang_sense_clarification": "спускаться с трудом"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "waddle down",
                                "target_lang_sense_clarification": "moving with short steps and a swaying motion, like a duck"
                            },
                            {
                                "target_lang_word": "stagger down",
                                "target_lang_sense_clarification": "moving unsteadily, often due to intoxication or dizziness"
                            },
                            {
                                "target_lang_word": "totter down",
                                "target_lang_sense_clarification": "moving feebly or unsteadily"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ffb2eb3-3cd6-3cc0-b528-12772f611793",
                    "sense_definition": "Op een onzekere, waggelende manier weggaan of vertrekken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "movement_away",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zijn optreden zag het publiek de clown het podium <w>afwaggelen</w>.",
                            "target_lang_translations": {
                                "ru": "После выступления публика видела, как клоун <w>сходил, переваливаясь,</w> со сцены.",
                                "en": "After his performance, the audience saw the clown <w>waddle off</w> the stage."
                            }
                        },
                        {
                            "text": "De gewonde speler moest het veld <w>afwaggelen</w> onder begeleiding van de arts.",
                            "target_lang_translations": {
                                "ru": "Травмированный игрок был вынужден <w>уковылять</w> с поля в сопровождении врача.",
                                "en": "The injured player had to <w>hobble off</w> the field accompanied by the doctor."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het podium afwaggelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уходить или сходить с чего-либо неуверенной, шатающейся походкой.",
                        "en": "To leave or move off something in an unsteady, wobbling manner."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уковылять",
                                "target_lang_sense_clarification": "уйти с трудом, хромая или шатаясь"
                            },
                            {
                                "target_lang_word": "сойти, шатаясь",
                                "target_lang_sense_clarification": "покинуть возвышение (сцену) или площадку"
                            },
                            {
                                "target_lang_word": "уйти, переваливаясь",
                                "target_lang_sense_clarification": "удалиться походкой вразвалку"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "waddle off",
                                "target_lang_sense_clarification": "leaving with a clumsy, side-to-side gait"
                            },
                            {
                                "target_lang_word": "stagger off",
                                "target_lang_sense_clarification": "leaving unsteadily or dizzily"
                            },
                            {
                                "target_lang_word": "hobble off",
                                "target_lang_sense_clarification": "leaving with difficulty, typically due to injury"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "4caf4ab5-424e-3baf-924e-88b707427a81",
                    "sense_definition": "Een dier of plant dat is ontstaan uit twee verschillende soorten of rassen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "biology_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een muildier is een <w>kruising</w> tussen een paard en een ezel.",
                            "target_lang_translations": {
                                "ru": "Мул — это <w>помесь</w> лошади и осла.",
                                "en": "A mule is a <w>cross</w> between a horse and a donkey."
                            }
                        },
                        {
                            "text": "Deze hond is een <w>kruising</w> en geen rashond.",
                            "target_lang_translations": {
                                "ru": "Эта собака — <w>помесь</w>, а не породистая.",
                                "en": "This dog is a <w>crossbreed</w> and not a purebred."
                            }
                        }
                    ],
                    "synonyms": [
                        "hybride"
                    ],
                    "common_phrases": [
                        "genetische kruising",
                        "bijzondere kruising"
                    ],
                    "target_lang_definitions": {
                        "ru": "Животное или растение, полученное от двух разных видов или пород.",
                        "en": "An animal or plant created from two different species or breeds."

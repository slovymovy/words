                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набрать",
                                "target_lang_sense_clarification": "взять определенное количество (воды, воздуха, номера)"
                            },
                            {
                                "target_lang_word": "зачерпнуть",
                                "target_lang_sense_clarification": "взять, загребая чем-либо (жидкость, сыпучее)"

{
    "word_family": [
        "emergence",
        "emergent"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b42ff694-f231-34d1-a3cb-f7cfff07c94d",
                    "sense_definition": "To appear or come into view from a hidden or enclosed place.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sun <w>emerged</w> from behind the clouds.",
                            "target_lang_translations": {
                                "ru": "Солнце <w>вышло</w> из-за туч."
                            }
                        },
                        {
                            "text": "A dark figure <w>emerged</w> from the shadows.",
                            "target_lang_translations": {
                                "ru": "Тёмная фигура <w>вышла</w> из тени."
                            }
                        }
                    ],
                    "synonyms": [
                        "appear",
                        "come forth"
                    ],
                    "antonyms": [
                        "disappear",
                        "vanish"
                    ],
                    "common_phrases": [
                        "emerge from the shadows",
                        "emerge from the darkness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Появляться в поле зрения или выходить из скрытого или замкнутого пространства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "появляться",
                                "target_lang_sense_clarification": "становиться видимым"
                            },
                            {
                                "target_lang_word": "выходить",
                                "target_lang_sense_clarification": "покидать пределы чего-либо, появляться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0073dafe-4df9-3c64-a832-58a4cf62a05e",
                    "sense_definition": "To come out of a liquid, object, or difficult situation, often surviving it.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The submarine <w>emerged</w> from the ocean.",
                            "target_lang_translations": {
                                "ru": "Подводная лодка <w>всплыла</w> на поверхность океана."
                            }
                        },
                        {
                            "text": "He <w>emerged</w> unscathed from the accident.",
                            "target_lang_translations": {
                                "ru": "Он <w>вышел</w> невредимым из аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "come to light"
                    ],
                    "antonyms": [
                        "submerge"
                    ],
                    "common_phrases": [
                        "emerge unscathed",
                        "emerge victorious",
                        "emerge from the water"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подниматься на поверхность жидкости или выходить из сложной ситуации, преодолевая её."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выходить",
                                "target_lang_sense_clarification": "из трудного положения или состояния"
                            },
                            {
                                "target_lang_word": "всплывать",
                                "target_lang_sense_clarification": "подниматься на поверхность воды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18b4f08f-8843-36a1-b47e-eeca7ebf11d2",
                    "sense_definition": "To become known or revealed after being hidden or secret.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gradually the truth <w>emerged</w>.",
                            "target_lang_translations": {
                                "ru": "Постепенно правда <w>всплыла</w> наружу."
                            }
                        },
                        {
                            "text": "New evidence has <w>emerged</w> regarding the case.",
                            "target_lang_translations": {
                                "ru": "По этому делу <w>появились</w> новые улики."
                            }
                        }
                    ],
                    "synonyms": [
                        "come to light",
                        "transpire"
                    ],
                    "antonyms": [
                        "remain hidden"
                    ],
                    "common_phrases": [
                        "truth emerged",
                        "details emerged",
                        "it emerged that"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться известным или явным; обнаруживаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всплывать",
                                "target_lang_sense_clarification": "становиться явным (о фактах, правде)"
                            },
                            {
                                "target_lang_word": "появляться",
                                "target_lang_sense_clarification": "возникать, становиться известным"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
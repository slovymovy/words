                {
                    "sense_id": "ae0b9eac-6384-38be-b064-2e513593c1c5",
                    "sense_definition": "Plural of <w>person</w>: human beings in general or considered as a group.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There were so many <w>people</w> at the restaurant last night.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером в ресторане было так много <w>людей</w>."
                            }
                        },
                        {
                            "text": "Teachers are <w>people</w> too and they have feelings just like us.",
                            "target_lang_translations": {
                                "ru": "Учителя тоже <w>люди</w>, и у них есть чувства, как и у нас."
                            }
                        },
                        {
                            "text": "Most <w>people</w> enjoy spending time with their friends and family.",
                            "target_lang_translations": {
                                "ru": "Большинству <w>людей</w> нравится проводить время с друзьями и семьей."
                            }

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "83574b12-3830-36fc-b9d7-8f47a3a7c129",
                    "sense_definition": "Идти, распространяться от кого-либо или чего-либо (о запахе, тепле, звуке, свете или информации).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "origin_emanation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От старой печи <исходило> приятное и уютное тепло."
                        },
                        {
                            "text": "Странный шум <исходил> из соседней комнаты, пугая кошку."
                        }
                    ],
                    "synonyms": [
                        "обойти",
                        "обойти",
                        "обойти"
                    ],
                    "common_phrases": [
                        "исходить от человека",
                        "исходить от источника",
                        "исходить паром"
                    ]
                },
                {
                    "sense_id": "4178aa52-b1d6-34f2-ba09-fcad60401c83",
                    "sense_definition": "Брать что-либо за основу для своих действий, решений или рассуждений; руководствоваться чем-либо.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "logical_basis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При планировании бюджета мы будем <исходить> из реальных доходов."
                        },
                        {
                            "text": "Адвокат в своей речи <исходил> из показаний главного свидетеля."
                        }
                    ],
                    "synonyms": [
                        "обойти",
                        "обойти",
                        "обойти"
                    ],
                    "common_phrases": [
                        "исходить из того",
                        "исходить из опыта",
                        "исходить из предположения",
                        "исходя из этого"
                    ]
                },
                {
                    "sense_id": "6a5df3c4-17d8-3e10-9fe6-9acb7f9e234b",
                    "sense_definition": "Побывать во многих местах, обойти большое пространство пешком (обычно совершенный вид).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За один день туристы <исходили> весь исторический центр города."
                        },
                        {
                            "text": "Мы <исходили> всё поле, но так и не нашли потерянные ключи."
                        }
                    ],
                    "synonyms": [
                        "обойти"
                    ],
                    "common_phrases": [
                        "исходить всё поле",
                        "исходить весь город",
                        "исходить все тропинки"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies extensive or tiring walking"
                        }
                    ]
                },
                {
                    "sense_id": "b3ed6260-2e58-3541-9490-318f38d4a50b",
                    "sense_definition": "Сильно истекать чем-либо (обычно кровью, слезами, потом), терять много сил или жидкости.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physiological_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Раненый боец <исходил> кровью, ожидая помощи медиков."
                        },
                        {
                            "text": "Мать <исходила> слезами, провожая сына в дальнюю дорогу."
                        }
                    ],
                    "synonyms": [
                        "обойти",
                        "обойти",
                        "обойти"
                    ],
                    "common_phrases": [
                        "исходить кровью",
                        "исходить слезами",
                        "исходить криком",
                        "исходить потом"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often found in literary contexts"
                        }
                    ]
                },
                {
                    "sense_id": "ac9b30f0-401e-380d-b089-29dc172dbb47",
                    "sense_definition": "Заканчиваться, подходить к концу (о времени, сроке).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Срок ультиматума <исходит> завтра на рассвете."
                        },
                        {
                            "text": "Время ожидания уже <исходит>, а ответа всё нет."
                        }
                    ],
                    "synonyms": [
                        "обойти",
                        "обойти",
                        "обойти"
                    ],
                    "common_phrases": [
                        "срок исходит",
                        "время исходит"
                    ],
                    "traits": [

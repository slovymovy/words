                                "target_lang_sense_clarification": "place of birth"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "колыбель",
                                "target_lang_sense_clarification": "переносное значение: место возникновения"
                            },
                            {
                                "target_lang_word": "исток",
                                "target_lang_sense_clarification": "начало, происхождение"
                            },
                            {
                                "target_lang_word": "родина",
                                "target_lang_sense_clarification": "место рождения"
                            }
                        ]
                    }

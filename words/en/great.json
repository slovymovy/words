                        {
                            "text": "The car won't start. <w>Great</w>!",
                            "target_lang_translations": {
                                "ru": "Машина не заводится. <w>Замечательно</w>!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "oh great"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "sarcastic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется иронически для выражения раздражения, разочарования или сарказма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прекрасно",
                                "target_lang_sense_clarification": "ироничное использование"
                            },
                            {
                                "target_lang_word": "замечательно",

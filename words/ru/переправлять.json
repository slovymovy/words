                        "переправлять оценку",
                        "переправлять текст",
                        "переправлять ошибки"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often implies altering something written specifically"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To make changes to written text, correct mistakes, or alter numbers/letters (often to falsify or fix)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "correct",
                                "target_lang_sense_clarification": "to fix mistakes in text"
                            },
                            {
                                "target_lang_word": "alter",
                                "target_lang_sense_clarification": "to change a number or letter into another (e.g., a grade or date)"
                            }
                        ]
                    }
                }
            ]
        }

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c534b8e9-f27b-315a-8a06-4a0e620affce",
                    "sense_definition": "Обратиться к кому-либо с вежливым предложением сделать что-либо, дать что-либо или разрешить что-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "request_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец <попросил> у дочери прощения за свою ошибку.",
                            "target_lang_translations": {
                                "en": "The father <asked> his daughter for forgiveness for his mistake."
                            }
                        },
                        {
                            "text": "Я хочу <попросить> тебя помочь мне с домашним заданием.",
                            "target_lang_translations": {
                                "en": "I want to <ask> you to help me with my homework."
                            }
                        }
                    ],
                    "antonyms": [
                        "приказать",
                        "потребовать"
                    ],
                    "common_phrases": [
                        "попросить прощения",
                        "попросить помощи",
                        "попросить денег",
                        "попросить разрешения",
                        "попросить совета"
                    ],
                    "target_lang_definitions": {
                        "en": "To address someone with a polite proposal to do something, give something, or allow something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "general request"
                            },
                            {
                                "target_lang_word": "request",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76bd63e8-fc72-3fd5-be61-3afe9f2264cd",
                    "sense_definition": "Вежливо пригласить кого-либо войти, подойти или направиться куда-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "request_invite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Секретарь <попросил> посетителя пройти в кабинет директора.",
                            "target_lang_translations": {
                                "en": "The secretary <asked> the visitor to proceed to the director's office."
                            }
                        },
                        {
                            "text": "Хозяйка дома улыбнулась и <попросила> гостей к столу.",
                            "target_lang_translations": {
                                "en": "The hostess smiled and <asked> the guests to the table."
                            }
                        }
                    ],
                    "synonyms": [
                        "пригласить",
                        "позвать"
                    ],
                    "common_phrases": [
                        "попросить к телефону",
                        "попросить войти",
                        "попросить к столу"
                    ],
                    "target_lang_definitions": {
                        "en": "To politely invite someone to enter, approach, or go somewhere."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "invite to enter or approach"
                            },
                            {
                                "target_lang_word": "invite",
                                "target_lang_sense_clarification": "synonym"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a48374cd-0bb6-34f8-b4b0-b3ff747b387e",
                    "sense_definition": "Строго потребовать прекратить нежелательные действия или слова (часто используется как реакция на грубость или неуместное поведение).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "demand_stop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <попрошу> вас не разговаривать со мной в таком тоне!",
                            "target_lang_translations": {
                                "en": "I must <ask> you not to speak to me in that tone!"
                            }
                        },
                        {
                            "text": "— Я бы <попросил>! — возмутился учитель, услышав шум на задней парте.",
                            "target_lang_translations": {
                                "en": "\"I would <ask> [you to behave]!\" the teacher said indignantly, hearing noise from the back desk."
                            }
                        }
                    ],
                    "synonyms": [
                        "потребовать"
                    ],
                    "common_phrases": [
                        "я попрошу",
                        "я бы попросил"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in phrases like 'я попрошу!'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To strictly demand the cessation of unwanted actions or words (often used as a reaction to rudeness)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "stern demand"
                            },
                            {
                                "target_lang_word": "demand",
                                "target_lang_sense_clarification": "meaning context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9d84b92-b2bb-32dd-b9ef-7d2ae08a0bbc",
                    "sense_definition": "Вежливо, но категорично потребовать уйти, покинуть помещение или освободить должность.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "demand_leave",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если вы продолжите шуметь, охрана <попросит> вас из зала.",
                            "target_lang_translations": {
                                "en": "If you continue making noise, security will <ask> you to leave the hall."
                            }
                        },
                        {
                            "text": "После громкого скандала сотрудника вежливо <попросили> с работы.",
                            "target_lang_translations": {
                                "en": "After the loud scandal, the employee was politely <asked> to leave his job."
                            }
                        }
                    ],
                    "synonyms": [
                        "выпроводить",
                        "уволить"
                    ],
                    "common_phrases": [
                        "попросить на выход",
                        "попросить вон"
                    ],
                    "target_lang_definitions": {
                        "en": "To politely but categorically demand someone to leave a room or vacate a position."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "to leave or resign"
                            },
                            {
                                "target_lang_word": "dismiss",
                                "target_lang_sense_clarification": "employment context"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
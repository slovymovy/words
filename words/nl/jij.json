    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "90eaa066-cd2c-3446-9c03-2f950b20fdfd",
                    "sense_definition": "Je gebruikt dit woord om één persoon aan te spreken (als onderwerp). Het is de benadrukte vorm en wordt gebruikt in informele situaties.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Jij</w> was er gisteren niet bij.",
                            "target_lang_translations": {
                                "en": "<w>You</w> weren't there yesterday."
                            }
                        },
                        {
                            "text": "Ik ga naar de bioscoop, ga <w>jij</w> ook mee?",
                            "target_lang_translations": {
                                "en": "I'm going to the cinema, are <w>you</w> coming too?"
                            }
                        }
                    ],
                    "synonyms": [
                        "je"
                    ],
                    "antonyms": [
                        "u"
                    ],
                    "common_phrases": [
                        "jij en ik",
                        "jij ook",
                        "dat ben jij"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informeel"
                        },
                        {
                            "trait_type": "form",
                            "comment": "benadrukt (stressed)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to address one person (as the subject). It is the stressed form and is used in informal situations."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "you",
                                "target_lang_sense_clarification": "singular, informal subject, stressed"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b66b5ee1-a15d-3f12-85e4-b506d1e542eb",
                    "sense_definition": "Vervoeging van <w>jijen</w>: eerste persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>jij</w> en jou graag als ik met vrienden praat.",
                            "target_lang_translations": {
                                "en": "I like to <w>address</w> people <w>informally</w> when I talk to friends."
                            }
                        },
                        {
                            "text": "Waarom <w>jij</w> je tegen de directeur?",
                            "target_lang_translations": {
                                "en": "Why do you <w>address</w> the director <w>informally</w>?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Conjugation of <w>jijen</w>: first person singular present tense."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "address informally",
                                "target_lang_sense_clarification": "to use the informal pronoun 'jij'"
                            }
                        ]
                    }
                }
            ]
        }
    ]

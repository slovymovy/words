    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fcbfffd5-7d95-34d0-91f6-9a3b18a24e60",
                    "sense_definition": "Niet recht of loodrecht; hellend in een bepaalde richting.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_orientation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De toren van Pisa staat al eeuwenlang <w>schuin</w>.",
                            "target_lang_translations": {
                                "ru": "Пизанская башня уже столетиями стоит <w>под наклоном</w>."
                            }
                        },
                        {
                            "text": "Hij sneed de worst in <w>schuine</w> plakjes voor op de borrelplank.",
                            "target_lang_translations": {
                                "ru": "Он нарезал колбасу <w>косыми</w> ломтиками для закусочной доски."
                            }
                        }
                    ],
                    "synonyms": [
                        "scheef",
                        "hellend"
                    ],
                    "antonyms": [
                        "recht",
                        "loodrecht",
                        "verticaal"
                    ],
                    "common_phrases": [
                        "schuin afsnijden",
                        "een schuine lijn",
                        "schuin dak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не прямой и не перпендикулярный; наклонный в определенном направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "косой",
                                "target_lang_sense_clarification": "о линии, срезе или поверхности"
                            },
                            {
                                "target_lang_word": "наклонный",
                                "target_lang_sense_clarification": "находящийся под углом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cbfadfe-90da-3e48-a971-a39836bdcd49",
                    "sense_definition": "Dubbelzinnig of onfatsoenlijk, vaak met een seksuele toespeling.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het feestje werden er de hele avond <w>schuine</w> moppen verteld.",
                            "target_lang_translations": {
                                "ru": "На вечеринке весь вечер рассказывали <w>сальные</w> анекдоты."
                            }
                        },
                        {
                            "text": "Niet iedereen kon zijn <w>schuine</w> opmerkingen waarderen.",
                            "target_lang_translations": {
                                "ru": "Не все могли оценить его <w>непристойные</w> замечания."
                            }
                        }
                    ],
                    "synonyms": [
                        "schunnig",
                        "dubbelzinnig",
                        "onfatsoenlijk"
                    ],
                    "antonyms": [
                        "fatsoenlijk",
                        "kuis",
                        "netjes"
                    ],
                    "common_phrases": [
                        "een schuine mop",
                        "schuine praat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двусмысленный или непристойный, часто с сексуальным подтекстом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сальный",
                                "target_lang_sense_clarification": "о шутках или анекдотах"
                            },
                            {
                                "target_lang_word": "непристойный",
                                "target_lang_sense_clarification": "грубый, бестактный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9e9b341a-65f2-31a9-a817-095ce18dc279",
                    "sense_definition": "In een diagonale richting; niet recht tegenover of naast iets.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_orientation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze woont <w>schuin</w> tegenover de supermarkt in de hoofdstraat.",
                            "target_lang_translations": {
                                "ru": "Она живет <w>наискосок</w> от супермаркета на главной улице."
                            }
                        },
                        {
                            "text": "Je moet de straat <w>schuin</w> oversteken om bij de ingang te komen.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно перейти улицу <w>по диагонали</w>, чтобы попасть ко входу."
                            }
                        }
                    ],
                    "synonyms": [
                        "diagonaal",
                        "dwars"
                    ],
                    "antonyms": [
                        "recht"
                    ],
                    "common_phrases": [
                        "schuin tegenover",
                        "schuin achter",
                        "schuin oversteken"
                    ],
                    "target_lang_definitions": {
                        "ru": "В диагональном направлении; не прямо напротив или рядом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наискосок",
                                "target_lang_sense_clarification": "по диагонали от чего-либо"
                            },
                            {
                                "target_lang_word": "по диагонали",
                                "target_lang_sense_clarification": "направление движения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "69ed7d8d-5c77-3249-b8be-8436e6dc3bf0",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>schuinen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>schuin</w> de randen van het hout af voor een betere afwerking.",
                            "target_lang_translations": {
                                "ru": "Я <w>скашиваю</w> края древесины для лучшей отделки."
                            }
                        },
                        {
                            "text": "Als ik door het veld loop, <w>schuin</w> ik meestal een stukje af.",
                            "target_lang_translations": {
                                "ru": "Когда я иду через поле, я обычно немного <w>срезаю</w> путь."
                            }

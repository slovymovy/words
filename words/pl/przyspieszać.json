            "pos": "verb",
            "senses": [
                {
                    "sense_id": "02e540d2-4950-37ea-bdac-d17111a20d9d",
                    "sense_definition": "Zwiększać prędkość poruszania się lub tempo wykonywania jakiejś czynności; sprawiać, że coś dzieje się szybciej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "speed_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca zaczął gwałtownie <przyspieszać>, aby zdążyć na zielone światło.",
                            "target_lang_translations": {
                                "ru": "Водитель начал резко <разгоняться>, чтобы успеть на зеленый свет."
                            }
                        },
                        {
                            "text": "Musimy <przyspieszać> tempo pracy, jeśli chcemy skończyć projekt na czas.",
                            "target_lang_translations": {
                                "ru": "Мы должны <ускорять> темп работы, если хотим закончить проект вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpędzać",
                        "ponaglać",
                        "usprawniać"
                    ],
                    "antonyms": [
                        "zwalniać",
                        "hamować",
                        "opóźniać"
                    ],
                    "common_phrases": [
                        "przyspieszać kroku",
                        "przyspieszać tempo",
                        "przyspieszać proces"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличивать скорость движения или темп выполнения действия; заставлять что-либо происходить быстрее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ускорять",
                                "target_lang_sense_clarification": "делать быстрее (процесс, темп)"
                            },
                            {
                                "target_lang_word": "ускоряться",
                                "target_lang_sense_clarification": "набирать скорость, начинать двигаться быстрее"
                            },
                            {
                                "target_lang_word": "разгоняться",
                                "target_lang_sense_clarification": "набирать разгон (о транспорте или беге)"
                            },
                            {
                                "target_lang_word": "прибавлять",
                                "target_lang_sense_clarification": "увеличивать (шаг, скорость, газ)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9204d426-a1df-3e16-acae-5650edd14ded",
                    "sense_definition": "Ustalać wcześniejszy termin jakiegoś wydarzenia; przesuwać coś na wcześniejszą datę lub godzinę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_schedule",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie warto na siłę <przyspieszać> terminu wyjazdu, jeśli nie jesteśmy gotowi.",
                            "target_lang_translations": {
                                "ru": "Не стоит насильно <приближать> дату отъезда, если мы не готовы."
                            }
                        },
                        {
                            "text": "Organizatorzy starają się <przyspieszać> kolejne etapy konkursu.",
                            "target_lang_translations": {
                                "ru": "Организаторы стараются <ускорять> следующие этапы конкурса."
                            }
                        }
                    ],
                    "antonyms": [
                        "opóźniać",
                        "odraczać",
                        "przekładać"
                    ],
                    "common_phrases": [
                        "przyspieszać termin",
                        "przyspieszać wizytę",
                        "przyspieszać datę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Назначать более ранний срок события; переносить что-либо на более раннюю дату или время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приближать",
                                "target_lang_sense_clarification": "переносить на более близкое время (дату, срок)"
                            },
                            {
                                "target_lang_word": "форсировать",
                                "target_lang_sense_clarification": "ускорять ход событий"
                            }
                        ]
                    }
                }
            ]
        }
    ]

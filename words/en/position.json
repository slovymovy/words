                },
                {
                    "sense_id": "cf8e2c47-7fd5-3d87-9321-578d184b3adc",
                    "sense_definition": "A job or role in an organization.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "role_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to apply for a management <w>position</w> at the new company.",

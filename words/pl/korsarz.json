{
    "word_family": [
        "korsarski",
        "korsarstwo"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aa0ce2e4-dc45-3be2-a155-8230e117cedd",
                    "sense_definition": "Pirat działający legalnie na zlecenie władcy lub państwa w celu atakowania statków handlowych wroga w czasie wojny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słynny <korsarz> zgromadził ogromne bogactwa, łupiąc statki nieprzyjaciela.",
                            "target_lang_translations": {
                                "ru": "Знаменитый <w>корсар</w> скопил огромные богатства, грабя вражеские корабли."
                            }
                        },
                        {
                            "text": "Król wydał specjalny list, dzięki któremu kapitan stał się legalnym <korsarzem>.",
                            "target_lang_translations": {
                                "ru": "Король выдал специальную грамоту, благодаря которой капитан стал законным <w>корсаром</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kaper"
                    ],
                    "common_phrases": [
                        "słynny korsarz",
                        "królewski korsarz"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пират, действующий легально по поручению правителя или государства для нападения на торговые суда врага в военное время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корсар",
                                "target_lang_sense_clarification": "человек"
                            },
                            {
                                "target_lang_word": "капер",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b29a68c1-4fcb-3684-aa00-52c2af7d6923",
                    "sense_definition": "Uzbrojony statek prywatny, upoważniony przez państwo do prowadzenia działań wojennych przeciwko flocie handlowej przeciwnika.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na horyzoncie pojawił się wrogi <korsarz> pod obcą banderą.",
                            "target_lang_translations": {
                                "ru": "На горизонте появился вражеский <w>корсар</w> под чужим флагом."
                            }
                        },
                        {
                            "text": "Ten szybki <korsarz> był trudny do dogonienia przez ciężkie okręty wojenne.",
                            "target_lang_translations": {
                                "ru": "Этого быстрого <w>корсара</w> было трудно догнать тяжелым военным кораблям."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вооруженное частное судно, уполномоченное государством вести военные действия против торгового флота противника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корсар",
                                "target_lang_sense_clarification": "судно"
                            },
                            {
                                "target_lang_word": "каперское судно",
                                "target_lang_sense_clarification": "тип корабля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fa70cb7-bf2b-37f4-8d52-c455cb0ac728",
                    "sense_definition": "Rodzaj małego, dwuosobowego jachtu żaglowego, popularnego w szkoleniu i turystyce.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młodzi żeglarze wypłynęli na jezioro małym <korsarzem>.",
                            "target_lang_translations": {
                                "ru": "Молодые яхтсмены вышли в озеро на маленьком «<w>Корсаре</w>»."
                            }
                        },
                        {
                            "text": "W porcie stało kilka odnowionych <korsarzy> gotowych do regat.",
                            "target_lang_translations": {
                                "ru": "В порту стояло несколько обновленных «<w>Корсаров</w>», готовых к регате."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jacht typu korsarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип небольшой двухместной парусной яхты, популярной для обучения и туризма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Корсар",
                                "target_lang_sense_clarification": "класс яхт"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
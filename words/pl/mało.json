    "entries": [
        {
            "pos": "numeral",
            "senses": [
                {
                    "sense_id": "a6647437-f636-308d-a772-64e6b650863e",
                    "sense_definition": "Oznacza niewielką liczbę lub ilość kogoś lub czegoś; przeciwieństwo słowa dużo.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lodówce zostało bardzo <w>mało</w> jedzenia na dzisiejszą kolację.",
                            "target_lang_translations": {
                                "ru": "В холодильнике осталось очень <w>мало</w> еды на сегодняшний ужин."
                            }
                        },
                        {
                            "text": "Na spotkanie przyszło tak <w>mało</w> osób, że musieliśmy je odwołać.",
                            "target_lang_translations": {
                                "ru": "На встречу пришло так <w>мало</w> людей, что нам пришлось её отменить."
                            }
                        }
                    ],
                    "synonyms": [
                        "niewiele",
                        "niedużo",
                        "trochę",
                        "nieco",
                        "odrobinę",
                        "jak na lekarstwo",
                        "tyle co nic",
                        "tyle, co kot napłakał"
                    ],
                    "antonyms": [
                        "dużo",
                        "wiele",
                        "sporo",
                        "multum"
                    ],
                    "common_phrases": [
                        "mało czasu",
                        "mało miejsca",
                        "za mało",
                        "bardzo mało"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает небольшое число или количество кого-либо или чего-либо; противоположность слову \"много\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мало",
                                "target_lang_sense_clarification": "о количестве"
                            },
                            {
                                "target_lang_word": "немного",
                                "target_lang_sense_clarification": "небольшое количество"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "76673fa7-aead-30b6-bc7f-c41e06d42108",
                    "sense_definition": "W niewielkim stopniu lub zakresie; nie wystarczająco (używane do określania przymiotników i przysłówków).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten film był nudny i <w>mało</w> interesujący dla przeciętnego widza.",
                            "target_lang_translations": {
                                "ru": "Этот фильм был скучным и <w>мало</w> интересным для среднего зрителя."
                            }
                        },
                        {
                            "text": "Jestem dzisiaj <w>mało</w> rozmowna, ponieważ bardzo boli mnie głowa.",
                            "target_lang_translations": {
                                "ru": "Я сегодня <w>мало</w> разговорчива, потому что у меня очень болит голова."
                            }
                        }
                    ],
                    "synonyms": [
                        "niewiele",
                        "słabo",
                        "nieco"
                    ],
                    "antonyms": [
                        "bardzo"
                    ],
                    "common_phrases": [
                        "mało ważny",
                        "mało znany",
                        "mało ciekawy",
                        "mało prawdopodobne"
                    ],
                    "target_lang_definitions": {
                        "ru": "В незначительной степени или мере; недостаточно (используется для определения прилагательных и наречий)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мало",
                                "target_lang_sense_clarification": "о степени"
                            },
                            {
                                "target_lang_word": "недостаточно",
                                "target_lang_sense_clarification": "не в полной мере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0062918-0017-3295-8f66-7b18deb6d006",
                    "sense_definition": "Przez krótki czas; niezbyt długo (odnosi się do czasu trwania czynności).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj w nocy <w>mało</w> spałem i teraz jestem bardzo zmęczony.",
                            "target_lang_translations": {
                                "ru": "Вчера ночью я <w>мало</w> спал, и теперь я очень устал."
                            }
                        },
                        {
                            "text": "Mój brat <w>mało</w> przebywa w domu, ponieważ ciągle pracuje.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>мало</w> бывает дома, так как постоянно работает."
                            }
                        }
                    ],
                    "synonyms": [
                        "krótko"
                    ],
                    "antonyms": [
                        "dużo"
                    ],
                    "common_phrases": [
                        "mało spać",
                        "mało bywać"
                    ],
                    "target_lang_definitions": {
                        "ru": "В течение короткого времени; недолго (относится к продолжительности действия)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мало",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "недолго",
                                "target_lang_sense_clarification": "короткое время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17c7a75e-b1f8-34bb-b56f-06ce2e107d9c",
                    "sense_definition": "Słowo używane w wyrażeniach wprowadzających dodatkową, mocniejszą informację; nie tylko to.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conjunction_like",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zgubił portfel, a <w>mało</w> tego, uciekł mu ostatni autobus do domu.",
                            "target_lang_translations": {
                                "ru": "Он потерял кошелёк, а <w>мало</w> того, от него ушёл последний автобус домой."
                            }
                        },
                        {
                            "text": "<w>Mało</w> powiedziane, że był zły na naszą decyzję, on był wściekły.",
                            "target_lang_translations": {
                                "ru": "<w>Мало</w> сказано, что он был зол на наше решение, он был в бешенстве."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mało tego",
                        "mało powiedziane",
                        "mało że"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое в выражениях, вводящих дополнительную, более сильную информацию; не только это."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мало",
                                "target_lang_sense_clarification": "в составе устойчивых выражений"
                            }
                        ]
                    }
                }
            ]
        }
    ]

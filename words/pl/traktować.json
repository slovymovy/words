                    ],
                    "synonyms": [
                        "omawiać",
                        "dotyczyć",
                        "być na (jakiś) temat"
                    ],
                    "common_phrases": [
                        "traktować o czymś",
                        "traktować o problemie",
                        "artykuł traktuje o"

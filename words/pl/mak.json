            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3a0fc5bc-3d05-391e-aaea-65dfe99665da",
                    "sense_definition": "Roślina o delikatnych, zazwyczaj czerwonych płatkach, rosnąca dziko na łąkach i polach lub uprawiana.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "plant_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zielonej łące zakwitł czerwony <w>mak</w>.",
                            "target_lang_translations": {
                                "ru": "На зеленом лугу расцвел красный <w>мак</w>."
                            }
                        },
                        {
                            "text": "W zbożu często można spotkać dzikie <w>maki</w>.",
                            "target_lang_translations": {
                                "ru": "Среди зерновых часто можно встретить дикие <w>маки</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mak dziki"
                    ],
                    "common_phrases": [
                        "czerwony jak mak",
                        "mak polny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растение с нежными, обычно красными лепестками, растущее на лугах и полях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мак",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "213a1258-1b87-3abb-8ee4-eab2ad249847",
                    "sense_definition": "Drobne, ciemne nasiona tej rośliny, wykorzystywane w kuchni do wypieków i potraw.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "plant_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na śniadanie zjadłem świeżą bułkę z <w>makiem</w>.",
                            "target_lang_translations": {
                                "ru": "На завтрак я съел свежую булочку с <w>маком</w>."
                            }
                        },
                        {
                            "text": "Do tego ciasta trzeba dodać dużo słodkiego <w>maku</w>.",
                            "target_lang_translations": {
                                "ru": "В этот пирог нужно добавить много сладкого <w>мака</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bułka z makiem",
                        "ciasto z makiem",
                        "cisza jak makiem zasiał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкие темные семена этого растения, используемые в кулинарии для выпечки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мак",
                                "target_lang_sense_clarification": "семена"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9d6dbb0-f208-3774-a279-2ff90947d820",
                    "sense_definition": "Potoczne określenie restauracji sieci McDonald’s.",

            "senses": [
                {
                    "sense_id": "b838f6c6-7b63-3742-9ec0-227be6a0a6e6",
                    "sense_definition": "Uczucie oczekiwania na to, że wydarzy się coś pożądanego; ufność w pomyślny rezultat.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hope_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam wielką <w>nadzieję</w>, że jutro będzie słoneczna pogoda.",
                            "target_lang_translations": {
                                "ru": "Я очень <w>надеюсь</w>, что завтра будет солнечная погода."
                            }
                        },
                        {
                            "text": "Jego słowa obudziły w niej <w>nadzieję</w> na lepszą przyszłość.",
                            "target_lang_translations": {
                                "ru": "Его слова пробудили в ней <w>надежду</w> на лучшее будущее."
                            }
                        }
                    ],
                    "synonyms": [
                        "otucha"
                    ],
                    "antonyms": [
                        "rezygnacja",
                        "zwątpienie",
                        "beznadzieja"
                    ],
                    "common_phrases": [
                        "mieć nadzieję",
                        "stracić nadzieję",
                        "promyk nadziei",
                        "wbrew nadziei",
                        "robić sobie nadzieję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство ожидания чего-то желаемого; уверенность в благоприятном исходе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надежда",
                                "target_lang_sense_clarification": "чувство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b09375fb-3db4-3ac2-b937-1a7e3fb35d6d",
                    "sense_definition": "Osoba, rzecz lub sytuacja, z którą wiąże się oczekiwania na pomyślny obrót spraw.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "hope_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy lek to ostatnia <w>nadzieja</w> dla ciężko chorych pacjentów.",
                            "target_lang_translations": {
                                "ru": "Это новое лекарство — последняя <w>надежда</w> для тяжелобольных пациентов."
                            }
                        },
                        {
                            "text": "Naszą <w>nadzieją</w> na rozwój firmy są młodzi i zdolni pracownicy.",
                            "target_lang_translations": {
                                "ru": "Нашей <w>надеждой</w> на развитие компании являются молодые и способные сотрудники."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jedyna nadzieja",
                        "ostatnia nadzieja",
                        "pokładać nadzieję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, вещь или ситуация, с которыми связаны ожидания благоприятного исхода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надежда",
                                "target_lang_sense_clarification": "опора"
                            }
                        ]

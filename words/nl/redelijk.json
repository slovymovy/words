                {
                    "sense_id": "e2f7038d-234b-3ac5-9c09-064a2887f327",
                    "sense_definition": "In behoorlijke mate; niet heel erg, maar ook niet weinig.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kan zich <w>redelijk</w> goed verstaanbaar maken in het Engels.",
                            "target_lang_translations": {
                                "en": "He can make himself understood <w>reasonably</w> well in English.",
                                "ru": "Он может <w>довольно</w> хорошо объясняться по-английски."
                            }
                        },
                        {
                            "text": "Het is vandaag <w>redelijk</w> warm voor de tijd van het jaar.",
                            "target_lang_translations": {
                                "en": "It is <w>fairly</w> warm today for the time of year.",
                                "ru": "Сегодня <w>сравнительно</w> тепло для этого времени года."
                            }
                        },
                        {
                            "text": "Ik ben <w>redelijk</w> zeker dat we de juiste weg hebben genomen.",
                            "target_lang_translations": {
                                "en": "I am <w>pretty</w> sure we took the right road.",
                                "ru": "Я <w>вполне</w> уверен, что мы выбрали правильную дорогу."
                            }
                        }
                    ],
                    "synonyms": [
                        "tamelijk",
                        "behoorlijk",
                        "vrij",
                        "nogal"
                    ],
                    "common_phrases": [
                        "redelijk goed",
                        "redelijk snel",
                        "redelijk zeker",
                        "redelijk veel"
                    ],
                    "target_lang_definitions": {
                        "en": "To a moderate or considerable degree; not extremely, but not little.",
                        "ru": "В значительной степени; достаточно, но не чрезмерно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fairly",

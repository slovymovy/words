                    "sense_id": "51e0a44a-43f4-3e85-9043-4086a227f560",
                    "sense_definition": "Voltooid deelwoord van <w>raken</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "participle_raken",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De pijl heeft het doel precies in het midden <w>geraakt</w>.",
                            "target_lang_translations": {
                                "en": "The arrow <w>hit</w> the target exactly in the center.",
                                "ru": "Стрела <w>попала</w> в цель точно в середину."
                            }
                        },
                        {
                            "text": "Er is gelukkig niemand gewond <w>geraakt</w> bij het ongeluk.",
                            "target_lang_translations": {
                                "en": "Fortunately, no one <w>got</w> injured in the accident.",
                                "ru": "К счастью, в аварии никто не <w>пострадал</w>."
                            }
                        }
                    ],

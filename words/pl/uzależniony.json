    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5723080f-7708-3453-9aff-57402552fcde",
                    "sense_definition": "Będący w stanie nałogu; odczuwający silną, trudną do opanowania potrzebę zażywania jakiejś substancji lub wykonywania jakiejś czynności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "addiction_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest <uzależniony> od gier komputerowych i spędza przy nich całe noce.",
                            "target_lang_translations": {
                                "ru": "Мой брат <зависим> от компьютерных игр и проводит за ними целые ночи."
                            }
                        },
                        {
                            "text": "Pacjent jest silnie <uzależniony> od leków przeciwbólowych po wypadku.",
                            "target_lang_translations": {
                                "ru": "Пациент сильно <зависим> от обезболивающих после аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "nałogowiec"
                    ],
                    "antonyms": [
                        "wolny",
                        "nieuzależniony"
                    ],
                    "common_phrases": [
                        "uzależniony od alkoholu",
                        "uzależniony od hazardu",
                        "silnie uzależniony",
                        "psychicznie uzależniony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в состоянии зависимости; испытывающий сильную, трудноконтролируемую потребность в употреблении какого-либо вещества или выполнении какого-либо действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зависимый",
                                "target_lang_sense_clarification": "страдающий от пагубной привычки"
                            },
                            {
                                "target_lang_word": "страдающий зависимостью",
                                "target_lang_sense_clarification": "имеющий аддикцию"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e3695369-6d80-371e-92a8-b843adab50e4",
                    "sense_definition": "Osoba, która popadła w nałóg i nie potrafi się z niego samodzielnie wyzwolić.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "addiction_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Terapia grupowa pomaga <uzależnionym> zrozumieć mechanizmy ich choroby.",
                            "target_lang_translations": {
                                "ru": "Групповая терапия помогает <зависимым> понять механизмы их болезни."
                            }
                        },
                        {
                            "text": "Ośrodek oferuje profesjonalną pomoc dla <uzależnionych> i ich rodzin.",
                            "target_lang_translations": {
                                "ru": "Центр предлагает профессиональную помощь для <зависимых> и их семей."
                            }
                        }
                    ],
                    "synonyms": [
                        "nałogowiec"
                    ],
                    "common_phrases": [
                        "pomoc uzależnionym",
                        "klub anonimowych uzależnionych",
                        "terapia dla uzależnionych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, попавший в зависимость и не способный самостоятельно от неё освободиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зависимый",
                                "target_lang_sense_clarification": "существительное: человек с зависимостью"
                            }
                        ]
                    }
                }
            ]
        },
        {

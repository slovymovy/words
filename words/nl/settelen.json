{
    "word_family": [],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "19e0cfa0-9d24-3831-8812-d105271a54ed",
                    "sense_definition": "Zich op een nieuwe plaats vestigen en zich daar thuis voelen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "settle_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na jaren rondreizen besloot hij zich in een rustig dorp te <w>settelen</w>.",
                            "target_lang_translations": {
                                "ru": "После многих лет путешествий он решил <w>обосноваться</w> в тихой деревне."
                            }
                        },
                        {
                            "text": "Als mensen zich op de maan willen <w>settelen</w>, zijn er speciale voorzieningen nodig.",
                            "target_lang_translations": {
                                "ru": "Если люди хотят <w>поселиться</w> на Луне, потребуются специальные условия."
                            }
                        }
                    ],
                    "synonyms": [
                        "vestigen",
                        "koloniseren"
                    ],
                    "common_phrases": [
                        "zich settelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поселиться на новом месте и почувствовать себя там как дома."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обосноваться",
                                "target_lang_sense_clarification": "устроиться на новом месте основательно"
                            },
                            {
                                "target_lang_word": "поселиться",
                                "target_lang_sense_clarification": "начать жить в определенном месте"
                            },
                            {
                                "target_lang_word": "обжиться",
                                "target_lang_sense_clarification": "привыкнуть к новому месту жительства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a510397b-3e8b-388b-9255-ff0779e7df5d",
                    "sense_definition": "Een stabiel leven opbouwen en zaken permanent regelen, vaak door te trouwen of een huis te kopen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "settle_life",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel dertigers willen zich <w>settelen</w> en aan kinderen beginnen.",
                            "target_lang_translations": {
                                "ru": "Многие тридцатилетние хотят <w>остепениться</w> и завести детей."
                            }
                        },
                        {
                            "text": "Zij is nog niet klaar om zich te <w>settelen</w> en wil eerst carrière maken.",
                            "target_lang_translations": {
                                "ru": "Она еще не готова <w>остепениться</w> и хочет сначала сделать карьеру."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich gaan settelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Построить стабильную жизнь и наладить быт, часто подразумевает вступление в брак или покупку жилья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остепениться",
                                "target_lang_sense_clarification": "стать серьезнее, завести семью"
                            },
                            {
                                "target_lang_word": "осесть",
                                "target_lang_sense_clarification": "прекратить переезды и начать жить постоянно на одном месте"
                            },
                            {
                                "target_lang_word": "устроить свою жизнь",
                                "target_lang_sense_clarification": "наладить быт и семейные отношения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
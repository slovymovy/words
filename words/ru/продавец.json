    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "475d3a54-8393-37f2-bd50-7c6c6ff52af1",
                    "sense_definition": "Работник магазина или торговой точки, который отпускает товары покупателям и принимает оплату.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "job_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вежливый <w>продавец</w> помог мне выбрать свежие фрукты.",
                            "target_lang_translations": {
                                "en": "The polite <w>shop assistant</w> helped me choose fresh fruit."
                            }
                        },
                        {
                            "text": "Если вы не можете найти нужный размер, спросите у <w>продавца</w>.",
                            "target_lang_translations": {
                                "en": "If you cannot find the right size, ask the <w>salesperson</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "покупатель",
                        "клиент"
                    ],
                    "common_phrases": [
                        "вежливый продавец",
                        "работать продавцом",
                        "спросить у продавца",
                        "продавец-консультант"
                    ],
                    "target_lang_definitions": {
                        "en": "A person employed in a shop to sell goods to customers."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shop assistant",
                                "target_lang_sense_clarification": "common in British English"
                            },
                            {
                                "target_lang_word": "sales clerk",
                                "target_lang_sense_clarification": "common in American English"
                            },
                            {
                                "target_lang_word": "salesperson",
                                "target_lang_sense_clarification": "gender-neutral term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bec0b70-2d13-3e9f-895f-451bc5c95848",
                    "sense_definition": "Человек или организация, которые занимаются продажей товаров или услуг; сторона в сделке купли-продажи.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "economic_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "По закону <w>продавец</w> обязан вернуть деньги за бракованный товар.",
                            "target_lang_translations": {
                                "en": "By law, the <w>seller</w> is obliged to refund money for defective goods."
                            }
                        },
                        {
                            "text": "В договоре указаны реквизиты и подписи покупателя и <w>продавца</w>.",
                            "target_lang_translations": {
                                "en": "The contract states the details and signatures of the buyer and the <w>seller</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "поставщик"
                    ],
                    "antonyms": [
                        "покупатель",
                        "клиент"
                    ],
                    "common_phrases": [
                        "права продавца",
                        "ответственность продавца",
                        "подпись продавца"
                    ],
                    "target_lang_definitions": {
                        "en": "A person or business that sells goods; the selling party in a contract."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seller",
                                "target_lang_sense_clarification": "general or legal context"
                            },
                            {
                                "target_lang_word": "vendor",
                                "target_lang_sense_clarification": "formal or commercial context"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    ],
                    "common_phrases": [
                        "и одновременно",
                        "но одновременно"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to connect contradictory or complementary qualities or actions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at the same time",
                                "target_lang_sense_clarification": "indicating co-existing but contrasting traits"

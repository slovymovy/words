    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "65da7e00-7b2c-3ec3-ae6e-3a3c28a76c41",
                    "sense_definition": "Of or relating to a monarch (such as a king or queen) or their family.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "monarchy_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>royal</w> family waved to the crowd from the palace balcony.",
                            "target_lang_translations": {
                                "ru": "<w>Королевская</w> семья помахала толпе с балкона дворца."
                            }
                        },
                        {
                            "text": "She dreams of attending a <w>royal</w> wedding one day.",
                            "target_lang_translations": {
                                "ru": "Она мечтает однажды побывать на <w>королевской</w> свадьбе."
                            }
                        }
                    ],
                    "synonyms": [
                        "kingly",
                        "princely",
                        "monarchical",
                        "regal"
                    ],
                    "antonyms": [
                        "common"
                    ],
                    "common_phrases": [
                        "royal family",
                        "royal palace",
                        "royal blood",
                        "royal navy"
                    ],
                    "target_lang_definitions": {

            "senses": [
                {
                    "sense_id": "1c3663c5-c649-34aa-b230-5e358b8f3859",
                    "sense_definition": "A person who works with you in the same profession or organization.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I went to lunch with a <w>colleague</w> from the marketing department.",
                            "target_lang_translations": {
                                "ru": "Я пошел на обед с <w>коллегой</w> из отдела маркетинга."
                            }
                        },
                        {
                            "text": "She is a trusted <w>colleague</w> who always helps me with my projects.",
                            "target_lang_translations": {
                                "ru": "Она надежная <w>коллега</w>, которая всегда помогает мне с проектами."
                            }
                        }
                    ],
                    "synonyms": [
                        "coworker",
                        "workmate"
                    ],
                    "common_phrases": [

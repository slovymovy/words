                {
                    "sense_id": "abe35b7b-949b-3507-b828-53dd8d582b67",
                    "sense_definition": "Having spines or sharp, pointed outgrowths.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_attributes",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>spined</w> caterpillar can cause a rash if touched with bare hands.",
                            "target_lang_translations": {
                                "ru": "<w>Шипастая</w> гусеница может вызвать сыпь, если прикоснуться к ней голыми руками."
                            }
                        },
                        {
                            "text": "Gardeners should wear gloves when pruning the long-<w>spined</w> rose bushes.",
                            "target_lang_translations": {
                                "ru": "Садовникам следует надевать перчатки при обрезке кустов роз с длинными <w>шипами</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "spiny",
                        "prickly",
                        "thorny"
                    ],
                    "antonyms": [
                        "unspined",
                        "unspiny",
                        "spineless"
                    ],
                    "common_phrases": [
                        "short-spined",
                        "long-spined",
                        "sharp-spined"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий шипы или острые, заостренные выросты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шипастый",
                                "target_lang_sense_clarification": "покрытый шипами"
                            },
                            {
                                "target_lang_word": "колючий",
                                "target_lang_sense_clarification": "имеющий колючки"
                            },
                            {
                                "target_lang_word": "с шипами",
                                "target_lang_sense_clarification": "описывает наличие шипов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e728c5f-a937-3724-86cd-70e255d38f92",
                    "sense_definition": "Having a backbone or vertebral column, often used in combination to describe the quality or type of the spine.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anatomical_attributes",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The biologist examined the fossil of a strong-<w>spined</w> reptile.",
                            "target_lang_translations": {
                                "ru": "Биолог изучил окаменелость рептилии с крепким <w>хребтом</w>."
                            }
                        },
                        {
                            "text": "Critics dismissed him as a weak-<w>spined</w> leader who avoided difficult choices.",
                            "target_lang_translations": {
                                "ru": "Критики отвергли его как лидера со слабым <w>хребтом</w>, который избегал трудных решений."
                            }
                        }
                    ],
                    "synonyms": [
                        "backboned",
                        "vertebrate"
                    ],
                    "antonyms": [
                        "spineless"
                    ],
                    "common_phrases": [
                        "stiff-spined",
                        "weak-spined",
                        "strong-spined"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий позвоночник или хребет; часто используется в сочетаниях для описания свойств позвоночника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с ... хребтом",
                                "target_lang_sense_clarification": "используется в составных прилагательных (напр., с крепким хребтом)"
                            },
                            {
                                "target_lang_word": "позвоночный",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                }
            ]

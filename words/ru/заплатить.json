    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "28158f6c-f778-321b-a4e0-d0a1d58a4be0",
                    "sense_definition": "Отдать деньги или другие ценности в обмен на товар, услугу или в качестве возмещения.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <w>заплатить</w> за этот обед картой.",
                            "target_lang_translations": {
                                "en": "I want to <w>pay</w> for this lunch by card."
                            }
                        },
                        {
                            "text": "За предварительную консультацию я <w>заплатил</w> пятьсот рублей.",
                            "target_lang_translations": {
                                "en": "I <w>paid</w> five hundred rubles for the preliminary consultation."
                            }
                        }
                    ],
                    "synonyms": [
                        "уплати́ть"
                    ],
                    "common_phrases": [
                        "заплатить за проезд",
                        "заплатить наличными",
                        "заплатить картой",
                        "заплатить по счёту",
                        "заплатить налоги"
                    ],
                    "target_lang_definitions": {
                        "en": "To give money in exchange for goods, services, or as reimbursement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay",
                                "target_lang_sense_clarification": "give money"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abf31ca8-b0dd-3e3c-983f-cea86fde04de",
                    "sense_definition": "Понести наказание, ответственность или пожертвовать чем-либо важным ради достижения цели или как следствие ошибки.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За эту глупую ошибку нам придётся дорого <w>заплатить</w>.",
                            "target_lang_translations": {
                                "en": "We will have to <w>pay</w> dearly for this stupid mistake."
                            }
                        },
                        {
                            "text": "Ради победы в войне народ <w>заплатил</w> огромную цену.",
                            "target_lang_translations": {
                                "en": "The people <w>paid</w> a huge price for victory in the war."
                            }
                        }
                    ],
                    "synonyms": [
                        "уплати́ть"
                    ],
                    "common_phrases": [
                        "дорого заплатить",
                        "заплатить жизнью",
                        "заплатить высокую цену",
                        "заплатить сполна"
                    ],
                    "target_lang_definitions": {
                        "en": "To suffer negative consequences or make a sacrifice for a mistake or goal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay",
                                "target_lang_sense_clarification": "suffer consequences"
                            },
                            {
                                "target_lang_word": "pay the price",
                                "target_lang_sense_clarification": "idiomatic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e12be40-9747-30cc-81b1-b5e342f0699d",
                    "sense_definition": "Совершить ответное действие на чей-либо поступок (добром или злом); отплатить.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он решил <w>заплатить</w> обидчику той же монетой.",
                            "target_lang_translations": {
                                "en": "He decided to <w>pay</w> the offender <w>back</w> in his own coin."
                            }
                        },
                        {
                            "text": "Она <w>заплатила</w> ему добром и заботой за его помощь.",
                            "target_lang_translations": {
                                "en": "She <w>repaid</w> him with kindness and care for his help."
                            }
                        }
                    ],
                    "synonyms": [
                        "уплати́ть"
                    ],
                    "common_phrases": [
                        "заплатить той же монетой",
                        "заплатить добром за добро",
                        "заплатить неблагодарностью"
                    ],
                    "target_lang_definitions": {
                        "en": "To return an action or feeling; to retaliate or reward someone for their deeds."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "repay",
                                "target_lang_sense_clarification": "return a favor or injury"
                            },
                            {
                                "target_lang_word": "pay back",
                                "target_lang_sense_clarification": "retaliate or reimburse"
                            }
                        ]
                    }
                }
            ]
        }
    ]

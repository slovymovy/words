            "senses": [
                {
                    "sense_id": "91004ec4-3746-3bc0-988a-bc9e6a73bd40",
                    "sense_definition": "A country in Central Europe, officially known as the Federal Republic of Germany; its capital is Berlin.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "geopolitical_entity",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "<w>Germany</w> has the largest economy in the European Union.",
                            "target_lang_translations": {
                                "ru": "<w>Германия</w> обладает крупнейшей экономикой в Европейском союзе."
                            }
                        },
                        {
                            "text": "Many cars made in <w>Germany</w> are exported around the world.",
                            "target_lang_translations": {
                                "ru": "Многие автомобили, произведенные в <w>Германии</w>, экспортируются по всему миру."
                            }
                        }
                    ],
                    "synonyms": [
                        "Federal Republic of Germany",
                        "FRG",
                        "Deutschland"
                    ],
                    "common_phrases": [
                        "made in Germany",
                        "West Germany",
                        "East Germany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Страна в Центральной Европе, официально известная как Федеративная Республика Германия; столица — Берлин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Германия",
                                "target_lang_sense_clarification": "страна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10e1911c-5693-3a31-8700-6e6df8af95ba",
                    "sense_definition": "The German nation or civilization in Central Europe throughout its history, encompassing various governments and borders over time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "geopolitical_entity",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "In the 19th century, <w>Germany</w> was unified into a single empire.",
                            "target_lang_translations": {
                                "ru": "В XIX веке <w>Германия</w> была объединена в единую империю."
                            }
                        },
                        {
                            "text": "The history of <w>Germany</w> includes periods of both great cultural achievement and devastating war.",
                            "target_lang_translations": {
                                "ru": "История <w>Германии</w> включает периоды как великих культурных достижений, так и разрушительных войн."
                            }
                        }
                    ],
                    "synonyms": [
                        "Germania",
                        "German Reich"
                    ],
                    "common_phrases": [
                        "Nazi Germany",
                        "Imperial Germany"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "historical usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Немецкая нация или цивилизация в Центральной Европе на протяжении всей своей истории, охватывающая различные формы правления и границы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Германия",
                                "target_lang_sense_clarification": "историческое государство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d37de12-6b47-3afb-aa96-0a40b4b24ee0",
                    "sense_definition": "The divided states of the German nation, specifically referring to the period when the country was split into West Germany and East Germany.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geopolitical_entity",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Relations between the two <w>Germanys</w> were often tense during the Cold War.",
                            "target_lang_translations": {
                                "ru": "Отношения между двумя <w>Германиями</w> часто были напряженными во время холодной войны."
                            }
                        },
                        {
                            "text": "Reunification in 1990 brought the two <w>Germanies</w> back together as one nation.",
                            "target_lang_translations": {
                                "ru": "Воссоединение в 1990 году снова объединило две <w>Германии</w> в одну нацию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "two Germanys"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "referring to the 1949-1990 period"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разделенные государства немецкой нации, в частности, в период, когда страна была разделена на Западную и Восточную Германию."
                    },
                    "translations": {

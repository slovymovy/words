            "pos": "verb",
            "senses": [
                {
                    "sense_id": "70ea7fde-f519-3797-a640-40e314efe8f8",
                    "sense_definition": "Zakładać na siebie lub kogoś element ubrania albo dodatki.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze <nakładaj> czapkę, gdy na zewnątrz jest mróz.",
                            "target_lang_translations": {
                                "ru": "Всегда <надевай> шапку, когда на улице мороз."
                            }
                        },
                        {
                            "text": "Muszę <nakładać> okulary do czytania, bo słabo widzę.",
                            "target_lang_translations": {
                                "ru": "Мне приходится <надевать> очки для чтения, так как я плохо вижу."
                            }
                        }
                    ],
                    "synonyms": [
                        "ubierać",
                        "zakładać się",
                        "oblekać"
                    ],
                    "antonyms": [
                        "zdejmować"
                    ],
                    "common_phrases": [
                        "nakładać buty",
                        "nakładać czapkę",
                        "nakładać płaszcz",
                        "nakładać okulary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надевать на себя или кого-то одежду или аксессуары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надевать",
                                "target_lang_sense_clarification": "одежду, очки, головные уборы"
                            },
                            {
                                "target_lang_word": "накидывать",
                                "target_lang_sense_clarification": "быстро набрасывать на плечи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c79f6b81-6b25-331e-bcd6-d249c6e17ede",
                    "sense_definition": "Rozprowadzać warstwę jakiejś substancji na powierzchni czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "application",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wyjściem na plażę należy dokładnie <nakładać> krem z filtrem.",
                            "target_lang_translations": {
                                "ru": "Перед выходом на пляж следует тщательно <наносить> солнцезащитный крем."
                            }
                        },
                        {
                            "text": "Malarz powoli <nakładał> kolejne warstwy farby na płótno.",
                            "target_lang_translations": {
                                "ru": "Художник медленно <накладывал> на холст очередные слои краски."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokrywać",
                        "okładać",
                        "napełniać"
                    ],
                    "common_phrases": [
                        "nakładać krem",
                        "nakładać makijaż",
                        "nakładać farbę",
                        "nakładać maseczkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить слой какого-либо вещества на поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наносить",
                                "target_lang_sense_clarification": "крем, макияж, тонкий слой"
                            },
                            {
                                "target_lang_word": "накладывать",
                                "target_lang_sense_clarification": "толстый слой, грим, повязку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69cd1770-9606-37a1-9787-92acce99b1b4",
                    "sense_definition": "Umieszczać porcję jedzenia na talerzu lub w naczyniu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodyni hojnie <nakładała> gościom bigos na talerze.",
                            "target_lang_translations": {
                                "ru": "Хозяйка щедро <накладывала> гостям бигос в тарелки."
                            }
                        },
                        {
                            "text": "Proszę <nakładać> sobie sałatkę, jest bardzo zdrowa.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <накладывайте> себе салат, он очень полезный."
                            }
                        }
                    ],
                    "synonyms": [
                        "napełniać",
                        "łożyć"
                    ],
                    "common_phrases": [
                        "nakładać zupę",
                        "nakładać obiad",
                        "nakładać łyżką",
                        "nakładać do talerza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Класть порцию еды на тарелку или в посуду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накладывать",
                                "target_lang_sense_clarification": "пищу в тарелку"
                            },
                            {
                                "target_lang_word": "класть",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f7ddea6-f773-3fcd-b92d-feb9c05b6a6b",
                    "sense_definition": "Ustanawiać oficjalny nakaz, obowiązek, opłatę lub karę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "imposition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd może <nakładać> wysokie grzywny za łamanie przepisów.",
                            "target_lang_translations": {
                                "ru": "Суд может <накладывать> высокие штрафы за нарушение правил."
                            }
                        },
                        {
                            "text": "Rząd planuje <nakładać> nowe podatki na towary luksusowe.",
                            "target_lang_translations": {
                                "ru": "Правительство планирует <вводить> новые налоги на предметы роскоши."
                            }
                        }
                    ],
                    "synonyms": [
                        "oblekać"
                    ],
                    "common_phrases": [
                        "nakładać karę",
                        "nakładać podatek",
                        "nakładać sankcje",
                        "nakładać obowiązek"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устанавливать официальное предписание, обязанность, плату или наказание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накладывать",
                                "target_lang_sense_clarification": "штраф, взыскание, запрет"
                            },
                            {
                                "target_lang_word": "возлагать",
                                "target_lang_sense_clarification": "ответственность, обязанности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa3a7351-36ae-3792-820b-f1e4466de11c",
                    "sense_definition": "Dziać się w tym samym czasie lub zajmować to samo miejsce, częściowo się pokrywając (zwykle z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "overlap",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety nasze urlopy się <nakładają> i nie możemy się spotkać.",
                            "target_lang_translations": {
                                "ru": "К сожалению, наши отпуска <накладываются>, и мы не можем встретиться."
                            }
                        },
                        {
                            "text": "Głosy w tle <nakładały> się na słowa aktora, utrudniając zrozumienie.",
                            "target_lang_translations": {
                                "ru": "Голоса на фоне <накладывались> на слова актера, затрудняя понимание."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbiegać się",
                        "pokrywać"
                    ],
                    "common_phrases": [
                        "terminy się nakładają",
                        "głosy się nakładają",
                        "obrazy się nakładają"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить одновременно или занимать одно и то же место, частично перекрываясь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накладываться",
                                "target_lang_sense_clarification": "совпадать по времени или месту"
                            },
                            {
                                "target_lang_word": "совпадать",
                                "target_lang_sense_clarification": "происходить в одно время"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                        "sense_id": "f8aa3be7-8d25-370e-9240-bdee9f176a75",
                        "entry_id": "4105dd98-3494-317e-b384-4455f8056ae2",
                        "glosses": [
                            "Pronunciation spelling of you'll."
                        ],
                        "tags": [
                            "alt-of",

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e703891b-6493-3306-8ea6-db4c9b36e19e",
                    "sense_definition": "Doprowadzić jakąś czynność, pracę lub dzieło do końca; wykonać ostatni etap działania.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "action_completion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <skończyć> ten raport przed jutrzejszym spotkaniem z szefem.",
                            "target_lang_translations": {
                                "ru": "Я должен <закончить> этот отчет перед завтрашней встречей с шефом."
                            }
                        },
                        {
                            "text": "Kiedy wreszcie <skończysz> czytać tę grubą książkę?",
                            "target_lang_translations": {
                                "ru": "Когда ты наконец <закончишь> читать эту толстую книгу?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zakończyć",
                        "sfinalizować",
                        "dokończyć",
                        "wykonać"
                    ],
                    "antonyms": [
                        "zacząć",
                        "rozpocząć",
                        "zainicjować"
                    ],
                    "common_phrases": [
                        "skończyć pracę",
                        "skończyć pisać",
                        "skończyć na dzisiaj",
                        "skończyć zadanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Довести действие, работу или произведение до конца; выполнить последний этап."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зако́нчить",
                                "target_lang_sense_clarification": "завершить дело"
                            },
                            {
                                "target_lang_word": "заверши́ть",
                                "target_lang_sense_clarification": "сделать полностью"
                            },
                            {
                                "target_lang_word": "доде́лать",
                                "target_lang_sense_clarification": "довести до конца"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e453270-3c9c-38d8-9219-1ce65598bf4c",
                    "sense_definition": "Przestać trwać; dobiec do kresu (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "event_termination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety, nasze wakacje już się <skończyły> i musimy wracać do domu.",
                            "target_lang_translations": {
                                "ru": "К сожалению, наши каникулы уже <закончились>, и мы должны возвращаться домой."
                            }
                        },
                        {
                            "text": "Film <skończył> się znacznie później, niż się spodziewaliśmy.",
                            "target_lang_translations": {
                                "ru": "Фильм <закончился> гораздо позже, чем мы ожидали."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakończyć się",
                        "minąć",
                        "ustać",
                        "dobiec końca"
                    ],
                    "antonyms": [
                        "zacząć się",
                        "rozpocząć się"
                    ],
                    "common_phrases": [
                        "wszystko się skończyło",
                        "czas się skończył",
                        "dobrze się skończyć",
                        "lato się skończyło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать длиться; подойти к концу (о времени или событии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зако́нчиться",
                                "target_lang_sense_clarification": "о событии"
                            },
                            {
                                "target_lang_word": "ко́нчиться",
                                "target_lang_sense_clarification": "перестать длиться"
                            },
                            {
                                "target_lang_word": "подойти́ к концу́",
                                "target_lang_sense_clarification": "фразеологизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81685e2f-658b-3aae-befa-fd5d9e8a4103",
                    "sense_definition": "Absolwować szkołę, studia lub kurs; zdobyć wykształcenie na danym etapie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój starszy brat <skończył> studia medyczne w zeszłym roku.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат <окончил> медицинский институт в прошлом году."
                            }
                        },
                        {
                            "text": "Aby zostać prawnikiem, musisz najpierw <skończyć> prawo na uniwersytecie.",
                            "target_lang_translations": {
                                "ru": "Чтобы стать юристом, ты должен сначала <окончить> юридический факультет университета."
                            }
                        }
                    ],
                    "synonyms": [
                        "ukończyć",
                        "wyedukować się"
                    ],
                    "antonyms": [
                        "rzucić",
                        "przerwać"
                    ],
                    "common_phrases": [
                        "skończyć studia",
                        "skończyć szkołę",
                        "skończyć uniwersytet",
                        "skończyć klasę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпуститься из учебного заведения; получить образование."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "око́нчить",
                                "target_lang_sense_clarification": "учебное заведение"
                            },
                            {
                                "target_lang_word": "зако́нчить",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6261da3-c28c-3c4b-9e42-7ff6387783da",
                    "sense_definition": "Zostać całkowicie zużytym; przestać być dostępnym z powodu wyczerpania zapasów (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "resource_depletion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W drukarce <skończył> się tusz, więc nie mogę wydrukować dokumentu.",
                            "target_lang_translations": {
                                "ru": "В принтере <кончились> чернила, поэтому я не могу распечатать документ."
                            }
                        },
                        {
                            "text": "Musimy iść do sklepu, bo <skończyło> się mleko do kawy.",
                            "target_lang_translations": {
                                "ru": "Нам нужно идти в магазин, потому что <кончилось> молоко для кофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyczerpać się",
                        "zużyć się"
                    ],
                    "common_phrases": [

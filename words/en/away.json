                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should throw <w>away</w> that old food.",
                            "target_lang_translations": {
                                "ru": "Тебе следует <w>выбросить</w> эту старую еду."
                            }
                        },
                        {
                            "text": "The rain will wash <w>away</w> the chalk on the sidewalk.",
                            "target_lang_translations": {
                                "ru": "Дождь <w>смоет</w> мел на тротуаре."
                            }
                        }
                    ],
                    "synonyms": [
                        "out-",
                        "off"
                    ],
                    "common_phrases": [
                        "throw away",
                        "give away",
                        "wash away"
                    ],
                    "target_lang_definitions": {
                        "ru": "С целью удалить, выбросить или устранить что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прочь",
                                "target_lang_sense_clarification": "указание на избавление от чего-либо"
                            },
                            {
                                "target_lang_word": "вон",
                                "target_lang_sense_clarification": "разговорное, используется для усиления действия удаления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a5fadad-820b-39fb-9492-c186df438111",

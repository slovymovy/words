            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ce64cd65-a6e2-3ed4-8ca1-f90d3f5b0705",
                    "sense_definition": "Een oud, traditioneel verhaal over goden, helden of wonderlijke wezens, vaak bedoeld om iets te verklaren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verhaal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De Griekse <w>mythe</w> over Hercules is over de hele wereld bekend.",
                            "target_lang_translations": {
                                "en": "The Greek <w>myth</w> about Hercules is known all over the world.",
                                "ru": "Греческий <w>миф</w> о Геркулесе известен во всем мире."
                            }
                        },
                        {
                            "text": "In deze oude <w>mythe</w> wordt uitgelegd hoe de seizoenen zijn ontstaan.",
                            "target_lang_translations": {
                                "en": "This ancient <w>myth</w> explains how the seasons originated.",
                                "ru": "В этом древнем <w>мифе</w> объясняется, как возникли времена года."
                            }
                        }
                    ],
                    "synonyms": [
                        "godenverhaal",
                        "sage",
                        "legende"
                    ],
                    "common_phrases": [
                        "Griekse mythe",
                        "klassieke mythe",
                        "oude mythe"
                    ],
                    "target_lang_definitions": {
                        "en": "An ancient, traditional story about gods, heroes, or supernatural beings, often intended to explain natural events or origins.",
                        "ru": "Древнее предание о богах, героях или чудесных существах, часто объясняющее происхождение явлений."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "myth",
                                "target_lang_sense_clarification": "traditional story"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "миф",
                                "target_lang_sense_clarification": "сказание о богах и героях"
                            },
                            {
                                "target_lang_word": "сказание",
                                "target_lang_sense_clarification": "предание, легенда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ff45ed3-81b4-3579-bc9c-af288fa96626",
                    "sense_definition": "Een idee of verhaal dat veel mensen geloven, maar dat eigenlijk niet waar is.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "onwaarheid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is een <w>mythe</w> dat goudvissen een geheugen van slechts drie seconden hebben.",
                            "target_lang_translations": {
                                "en": "It is a <w>myth</w> that goldfish have a memory of only three seconds.",
                                "ru": "Это <w>миф</w>, что у золотых рыбок память всего три секунды."
                            }
                        },
                        {
                            "text": "De wetenschapper probeerde de hardnekkige <w>mythe</w> met feiten te ontkrachten.",
                            "target_lang_translations": {
                                "en": "The scientist tried to debunk the persistent <w>myth</w> with facts.",
                                "ru": "Ученый пытался опровергнуть этот устойчивый <w>миф</w> с помощью фактов."
                            }
                        }
                    ],
                    "synonyms": [
                        "fabeltje",
                        "verzinsel",
                        "misvatting",
                        "illusie"
                    ],
                    "antonyms": [
                        "feit",
                        "waarheid",
                        "realiteit"
                    ],
                    "common_phrases": [
                        "een mythe ontkrachten",
                        "een hardnekkige mythe",
                        "populaire mythe"
                    ],
                    "target_lang_definitions": {
                        "en": "A widely held but false belief or idea.",
                        "ru": "Идея или история, в которую многие верят, но которая на самом деле не является правдой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "myth",
                                "target_lang_sense_clarification": "false belief"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "миф",
                                "target_lang_sense_clarification": "ложное представление, заблуждение"
                            },
                            {
                                "target_lang_word": "выдумка",
                                "target_lang_sense_clarification": "неправда, фантазия"
                            }
                        ]

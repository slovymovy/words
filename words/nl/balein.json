{
    "word_family": [
        "baleinwalvis"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eb176d67-e2ae-3ad1-bc9e-901fc69ca740",
                    "sense_definition": "Een dun, buigzaam staafje dat dient ter versteviging van voorwerpen zoals een paraplu, korset of kraag.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de harde wind zijn de <w>baleinen</w> van mijn paraplu gebroken.",
                            "target_lang_translations": {
                                "ru": "Из-за сильного ветра сломались <w>спицы</w> моего зонта."
                            }
                        },
                        {
                            "text": "Zij droeg een strak lijfje dat verstevigd was met <w>baleinen</w>.",
                            "target_lang_translations": {
                                "ru": "Она носила тугой корсаж, укрепленный <w>косточками</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "baleinen van een paraplu",
                        "korset met baleinen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкий гибкий стержень, используемый для придания жесткости таким предметам, как зонт, корсет или воротник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спица",
                                "target_lang_sense_clarification": "деталь зонта"
                            },
                            {
                                "target_lang_word": "косточка",
                                "target_lang_sense_clarification": "деталь корсета или воротника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7f5feb2-5c6b-3074-9e0f-96656fc3d299",
                    "sense_definition": "Een hoornachtige plaat in de bek van bepaalde walvissen die dient om voedsel uit het water te filteren, of het materiaal daarvan.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De walvis filtert grote hoeveelheden plankton uit het water met zijn <w>baleinen</w>.",
                            "target_lang_translations": {
                                "ru": "Кит отфильтровывает из воды огромное количество планктона с помощью <w>китового уса</w>."
                            }
                        },
                        {
                            "text": "Vroeger werden veel luxe voorwerpen gemaakt van <w>balein</w>.",
                            "target_lang_translations": {
                                "ru": "Раньше многие предметы роскоши изготавливались из <w>китового уса</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "walvisbeen",
                        "baard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Роговая пластина в пасти усатых китов, служащая для отцеживания пищи из воды, или материал из этих пластин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "китовый ус",
                                "target_lang_sense_clarification": "анатомия и материал"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
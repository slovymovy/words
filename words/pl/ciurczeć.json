    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b52399f9-e6c9-3344-92b1-8b4ca225aa05",
                    "sense_definition": "Płynąć powoli wąskim strumieniem, wydając przy tym cichy, charakterystyczny dźwięk; ciec.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "flow_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z nieszczelnego kranu przez całą noc <w>ciurczała</w> woda.",
                            "target_lang_translations": {
                                "ru": "Из протекающего крана всю ночь <w>журчала</w> вода."
                            }
                        },
                        {
                            "text": "Górski strumyk wesoło <w>ciurczy</w> między kamieniami w dolinie.",
                            "target_lang_translations": {
                                "ru": "Горный ручей весело <w>журчит</w> между камнями в долине."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciurkać",
                        "ciec",
                        "sączyć się"
                    ],
                    "antonyms": [
                        "tryskać",
                        "buchać"
                    ],
                    "common_phrases": [
                        "woda ciurczy",
                        "kran ciurczy",
                        "ciurczeć po szybie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Течь тонкой струйкой, издавая тихий, характерный звук (журчание)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "журчать",
                                "target_lang_sense_clarification": "издавать характерный звук при течении (о воде)"
                            },
                            {
                                "target_lang_word": "струиться",
                                "target_lang_sense_clarification": "течь непрерывной струей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1898042-7430-38c8-8f98-2123236512c8",
                    "sense_definition": "Narzekać, marudzić lub mówić w sposób monotonny i męczący dla słuchacza.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "complain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestań mi tu ciągle <w>ciurczeć</w> nad uchem o swoich problemach.",
                            "target_lang_translations": {
                                "ru": "Перестань постоянно <w>жужжать</w> мне над ухом о своих проблемах."
                            }
                        },
                        {
                            "text": "Stary sąsiad przyszedł i zaczął <w>ciurczeć</w>, że jest za głośno.",
                            "target_lang_translations": {
                                "ru": "Старый сосед пришел и начал <w>бубнить</w>, что слишком громко."
                            }
                        }
                    ],
                    "synonyms": [
                        "marudzić",
                        "zrzędzić",
                        "narzekać",
                        "biadolić"
                    ],
                    "common_phrases": [
                        "ciurczeć nad uchem"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Śląsk Cieszyński"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жаловаться, ныть или говорить монотонно, утомляя слушателя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жужжать",
                                "target_lang_sense_clarification": "надоедливо говорить, часто «над ухом»"
                            },
                            {
                                "target_lang_word": "бубнить",
                                "target_lang_sense_clarification": "монотонно ворчать или говорить неразборчиво"
                            },
                            {
                                "target_lang_word": "ныть",
                                "target_lang_sense_clarification": "надоедливо жаловаться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
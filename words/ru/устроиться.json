    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "888852ca-b2f4-395e-82ab-f4405c4510fb",
                    "sense_definition": "Поступить на работу, быть зачисленным в штат сотрудников.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "job",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После окончания университета он смог быстро <устроиться> на работу в банк.",
                            "target_lang_translations": {
                                "en": "After graduating from university, he was able to quickly <get a job> at a bank."
                            }
                        },
                        {
                            "text": "Анна мечтает <устроиться> дизайнером в крупную международную компанию.",
                            "target_lang_translations": {
                                "en": "Anna dreams of <getting a job> as a designer at a large international company."
                            }
                        }
                    ],
                    "synonyms": [
                        "зачислиться",
                        "пристроиться"
                    ],
                    "common_phrases": [
                        "устроиться на работу",
                        "устроиться менеджером",
                        "хорошо устроиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To start a job, to be hired as an employee."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get a job",
                                "target_lang_sense_clarification": "to find employment"
                            },
                            {
                                "target_lang_word": "get hired",
                                "target_lang_sense_clarification": "to be accepted for a position"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f9785ec-e2ea-327b-b3e0-66180b1adcf0",
                    "sense_definition": "Занять место, сесть или лечь (обычно удобно).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я предлагаю вам <устроиться> поудобнее в кресле и послушать мой рассказ.",
                            "target_lang_translations": {
                                "en": "I suggest you <settle> comfortably in the armchair and listen to my story."
                            }
                        },
                        {
                            "text": "Кошка решила <устроиться> на мягкой подушке и сразу уснула.",
                            "target_lang_translations": {
                                "en": "The cat decided to <settle> on the soft pillow and fell asleep immediately."
                            }
                        }
                    ],
                    "synonyms": [
                        "расположиться",
                        "разместиться",
                        "примоститься",
                        "приткнуться"
                    ],
                    "common_phrases": [
                        "устроиться поудобнее",
                        "устроиться в кресле",
                        "устроиться на ночлег"
                    ],
                    "target_lang_definitions": {
                        "en": "To take a seat or lie down, usually in a comfortable position."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "settle",
                                "target_lang_sense_clarification": "to position oneself comfortably"
                            },
                            {
                                "target_lang_word": "make oneself comfortable",
                                "target_lang_sense_clarification": "idiomatic phrase for sitting/lying comfortably"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f2c0066-74d2-32d3-a279-9186a503abf5",
                    "sense_definition": "Наладить свой быт, обосноваться на новом месте жительства или временного пребывания.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "settle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы успели <устроиться> в гостинице до наступления темноты.",
                            "target_lang_translations": {
                                "en": "The tourists managed to <get settled> in the hotel before dark."
                            }
                        },
                        {
                            "text": "Переехав в другой город, они смогли вполне комфортно <устроиться>.",
                            "target_lang_translations": {
                                "en": "Having moved to another city, they were able to <settle in> quite comfortably."
                            }
                        }
                    ],
                    "synonyms": [
                        "обосноваться",
                        "расположиться",
                        "разместиться"
                    ],
                    "common_phrases": [
                        "устроиться в гостинице",
                        "устроиться с комфортом"
                    ],
                    "target_lang_definitions": {
                        "en": "To organize one's daily life or establish oneself in a new place of residence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "settle in",
                                "target_lang_sense_clarification": "to become established in a new home or place"
                            },
                            {
                                "target_lang_word": "get settled",
                                "target_lang_sense_clarification": "to arrange one's living situation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "964c1dcd-04b8-3178-bc45-b9372783bb81",
                    "sense_definition": "Прийти в порядок, разрешиться благополучно (о делах, обстоятельствах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не волнуйся, со временем всё обязательно <устроится> наилучшим образом.",
                            "target_lang_translations": {
                                "en": "Don't worry, with time everything will definitely <work out> for the best."
                            }
                        },
                        {
                            "text": "Их финансовые дела наконец-то <устроились> после подписания контракта.",
                            "target_lang_translations": {
                                "en": "Their financial affairs finally <got sorted out> after signing the contract."
                            }
                        }
                    ],
                    "synonyms": [
                        "наладиться",
                        "утрястись",
                        "образоваться"
                    ],
                    "common_phrases": [
                        "всё устроится",
                        "дела устроились",
                        "жизнь устроилась"
                    ],
                    "target_lang_definitions": {
                        "en": "To come into order, to be resolved successfully (regarding affairs or circumstances)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "work out",
                                "target_lang_sense_clarification": "to resolve successfully"
                            },
                            {
                                "target_lang_word": "sort itself out",
                                "target_lang_sense_clarification": "to come into order naturally"
                            }
                        ]
                    }
                }
            ]
        }
    ]

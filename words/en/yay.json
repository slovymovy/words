    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "e056df12-6cb9-3b03-a044-7cfbb17b7801",
                    "sense_definition": "An exclamation used to express joy, excitement, or approval.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotion_happiness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Yay</w>! We finally finished the project on time.",
                            "target_lang_translations": {
                                "ru": "<w>Ура</w>! Мы наконец-то закончили проект вовремя."
                            }
                        },
                        {
                            "text": "I got the job I wanted, <w>yay</w>!",
                            "target_lang_translations": {
                                "ru": "Я получил работу, которую хотел, <w>ура</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "hooray",
                        "hurrah",
                        "hurray",
                        "woohoo",
                        "yippee",
                        "wahoo"
                    ],
                    "antonyms": [
                        "dammit"
                    ],
                    "common_phrases": [
                        "yay me",
                        "yay us"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, используемое для выражения радости, восторга или одобрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ура",
                                "target_lang_sense_clarification": "выражение радости"
                            },
                            {
                                "target_lang_word": "класс",
                                "target_lang_sense_clarification": "выражение одобрения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "abe47b4d-2cce-3899-82a1-df42788cc7a8",
                    "sense_definition": "To this extent; used to indicate a specific size or height, usually accompanied by a hand gesture.",

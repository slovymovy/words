{
    "word_family": [
        "regulate",
        "regulator",
        "regulatory",
        "deregulate",
        "deregulation"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "334cd6cf-8416-34ca-a87e-71bde5410f0e",
                    "sense_definition": "An official rule or law made by a government or authority to control how something is done.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "rule_law",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new safety <w>regulations</w> require all workers to wear protective gear.",
                            "target_lang_translations": {
                                "ru": "Новые <w>правила</w> техники безопасности требуют, чтобы все рабочие носили защитное снаряжение."
                            }
                        },
                        {
                            "text": "Companies must comply with strict environmental <w>regulations</w> to avoid fines.",
                            "target_lang_translations": {
                                "ru": "Компании должны соблюдать строгие экологические <w>нормы</w> во избежание штрафов."
                            }
                        }
                    ],
                    "synonyms": [
                        "rule",
                        "law",
                        "directive",
                        "statute",
                        "ruling"
                    ],
                    "common_phrases": [
                        "safety regulations",
                        "government regulations",
                        "strict regulations",
                        "fire regulations"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное правило или закон, установленный властью для контроля порядка действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правило",
                                "target_lang_sense_clarification": "общее требование или норма поведения"
                            },
                            {
                                "target_lang_word": "предписание",
                                "target_lang_sense_clarification": "официальное распоряжение или указание"
                            },
                            {
                                "target_lang_word": "регламент",
                                "target_lang_sense_clarification": "свод правил, регулирующих порядок деятельности"
                            },
                            {
                                "target_lang_word": "устав",
                                "target_lang_sense_clarification": "свод правил организации или военный кодекс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "110f5d1f-57ce-32bf-b9c5-ed4aeb96bab4",
                    "sense_definition": "The act of controlling or managing something, such as a business process or system.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "act_of_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government proposed tighter <w>regulation</w> of the banking industry.",
                            "target_lang_translations": {
                                "ru": "Правительство предложило ужесточить <w>регулирование</w> банковской сферы."
                            }
                        },
                        {
                            "text": "Effective <w>regulation</w> of traffic flow can reduce congestion in the city.",
                            "target_lang_translations": {
                                "ru": "Эффективное <w>регулирование</w> транспортного потока может уменьшить пробки в городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "control",
                        "management",
                        "supervision",
                        "oversight",
                        "governance"
                    ],
                    "antonyms": [
                        "deregulation"
                    ],
                    "common_phrases": [
                        "market regulation",
                        "self-regulation",
                        "financial regulation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс управления или контроля над чем-либо, например, бизнес-процессом или системой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регулирование",
                                "target_lang_sense_clarification": "процесс управления или налаживания"
                            },
                            {
                                "target_lang_word": "контроль",
                                "target_lang_sense_clarification": "надзор или проверка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7deced42-cc7d-314f-99f0-79b6b49dd591",
                    "sense_definition": "A physiological or biological process that maintains a stable internal state within an organism.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "biological_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The kidneys play a vital role in the <w>regulation</w> of blood pressure.",
                            "target_lang_translations": {
                                "ru": "Почки играют жизненно важную роль в <w>регуляции</w> кровяного давления."
                            }
                        },
                        {
                            "text": "Hormones are essential for the <w>regulation</w> of metabolism and growth.",
                            "target_lang_translations": {
                                "ru": "Гормоны необходимы для <w>регуляции</w> обмена веществ и роста."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Физиологический или биологический процесс, поддерживающий стабильное внутреннее состояние организма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регуляция",
                                "target_lang_sense_clarification": "биологический процесс поддержания функций"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a6af0811-0c99-3346-961b-af2a6e37d498",
                    "sense_definition": "Conforming to the official rules or standards, especially regarding size, shape, or uniform.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The referee checked to ensure the ball was of <w>regulation</w> size and weight.",
                            "target_lang_translations": {
                                "ru": "Судья проверил, чтобы мяч был <w>стандартного</w> размера и веса."
                            }
                        },
                        {
                            "text": "All soldiers must appear in full <w>regulation</w> uniform for the inspection.",
                            "target_lang_translations": {
                                "ru": "Все солдаты должны явиться на осмотр в полной <w>уставной</w> форме."
                            }
                        }
                    ],
                    "synonyms": [
                        "standard",
                        "official",
                        "prescribed",
                        "required"
                    ],
                    "antonyms": [
                        "non-regulation"
                    ],
                    "common_phrases": [
                        "regulation size",
                        "regulation uniform",
                        "regulation height"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соответствующий официальным правилам или стандартам, особенно в отношении размера или формы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уставной",
                                "target_lang_sense_clarification": "соответствующий уставу (о форме одежды)"
                            },
                            {
                                "target_lang_word": "стандартный",
                                "target_lang_sense_clarification": "соответствующий установленному стандарту"
                            },
                            {
                                "target_lang_word": "установленный",
                                "target_lang_sense_clarification": "официально утвержденный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
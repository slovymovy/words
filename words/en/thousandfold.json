    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "de2cedff-f7ec-3157-b653-4cb6243bbfda",
                    "sense_definition": "Multiplied by one thousand or repeated a thousand times.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "multiplication_by_1000",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new manufacturing process resulted in a <w>thousandfold</w> increase in daily production.",
                            "target_lang_translations": {
                                "ru": "Новый производственный процесс привел к <w>тысячекратному</w> увеличению ежедневной выработки."
                            }
                        },
                        {
                            "text": "Investors were delighted to see a <w>thousandfold</w> return on their initial capital.",
                            "target_lang_translations": {
                                "ru": "Инвесторы были в восторге, увидев <w>тысячекратную</w> прибыль от своего первоначального капитала."
                            }
                        }
                    ],
                    "common_phrases": [
                        "thousandfold increase",
                        "thousandfold growth",
                        "thousandfold return"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличенный в тысячу раз или повторенный тысячу раз"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тысячекратный",
                                "target_lang_sense_clarification": "об увеличении, росте или повторении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "646fcdec-865b-3da6-98b9-090f18320bf8",
                    "sense_definition": "Having one thousand parts or members.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient text describes a <w>thousandfold</w> army descending from the northern hills.",
                            "target_lang_translations": {
                                "ru": "Древний текст описывает <w>многотысячную</w> армию, спускающуюся с северных холмов."
                            }
                        },
                        {
                            "text": "She gazed at the <w>thousandfold</w> stars visible in the clear desert sky.",
                            "target_lang_translations": {
                                "ru": "Она смотрела на <w>бесчисленные</w> звезды, видимые в ясном небе пустыни."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоящий из тысячи частей или элементов; очень многочисленный"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "многотысячный",
                                "target_lang_sense_clarification": "состоящий из множества единиц"
                            },
                            {
                                "target_lang_word": "бесчисленный",
                                "target_lang_sense_clarification": "в переносном смысле: очень много"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "10de3c37-bb62-3570-a26b-3e449d15fb14",
                    "sense_definition": "By a factor of a thousand.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "multiplication_by_1000",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Since the introduction of the internet, the speed of communication has increased <w>thousandfold</w>.",
                            "target_lang_translations": {
                                "ru": "С момента появления интернета скорость общения возросла <w>тысячекратно</w>."
                            }
                        },
                        {
                            "text": "Our appreciation for his hard work grew <w>thousandfold</w> after seeing the final results.",
                            "target_lang_translations": {
                                "ru": "Наша признательность за его тяжелый труд выросла <w>тысячекратно</w> после того, как мы увидели конечные результаты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "increase thousandfold",
                        "grow thousandfold",
                        "multiply thousandfold"
                    ],
                    "target_lang_definitions": {
                        "ru": "В тысячу раз; в огромной степени"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тысячекратно",

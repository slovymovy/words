                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угол",
                                "target_lang_sense_clarification": "острый выступ предмета"
                            },
                            {
                                "target_lang_word": "край",
                                "target_lang_sense_clarification": "граница поверхности"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a2dfaaf3-ec3a-3448-8b0d-7205687c60e0",
                    "sense_definition": "Nasiona lub strąki tej rośliny używane jako pożywienie, a także potrawa z nich przygotowana.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na obiad ugotujemy dziś <w>fasolę</w>.",
                            "target_lang_translations": {
                                "ru": "Сегодня на обед мы сварим <w>фасоль</w>."
                            }
                        },
                        {
                            "text": "Do tej sałatki najlepiej pasuje czerwona <w>fasola</w> z puszki.",
                            "target_lang_translations": {
                                "ru": "К этому салату лучше всего подходит красная консервированная <w>фасоль</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Kraków) fazola"
                    ],
                    "common_phrases": [
                        "fasola po bretońsku",
                        "fasola szparagowa",
                        "fasola Jaś",
                        "puszka fasoli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Семена или стручки этого растения, употребляемые в пищу, а также блюдо, приготовленное из них."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фасоль",
                                "target_lang_sense_clarification": "пища"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f9d28a9-e9a8-364c-985e-53767d2e08d0",
                    "sense_definition": "Roślina uprawna z rodziny bobowatych, często pnąca, uprawiana dla jadalnych nasion i strąków.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na działce uprawiamy dwie odmiany <w>fasoli</w>.",
                            "target_lang_translations": {
                                "ru": "На даче мы выращиваем два сорта <w>фасоли</w>."
                            }
                        },
                        {
                            "text": "W naszym ogrodzie <w>fasola</w> pnie się po wysokich tyczkach.",
                            "target_lang_translations": {
                                "ru": "В нашем саду <w>фасоль</w> вьется по высоким шестам."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Kraków) fazola"
                    ],
                    "common_phrases": [
                        "uprawa fasoli",
                        "tyczki do fasoli",
                        "zbiór fasoli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Культурное растение семейства бобовых, часто вьющееся, выращиваемое ради съедобных семян и стручков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фасоль",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c491dcb-9e79-36c0-a825-3266b2a76836",
                    "sense_definition": "Człowiek nieporadny, mało zaradny lub naiwny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź taką <w>fasolą</w>, musisz sobie w końcu poradzić.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>недотёпой</w>, ты должен наконец справиться сам."
                            }
                        },
                        {
                            "text": "Ten nowy pracownik to straszna <w>fasola</w>, ciągle coś psuje.",
                            "target_lang_translations": {
                                "ru": "Этот новый сотрудник — ужасный <w>растяпа</w>, постоянно что-то портит."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gwara więzienna lub środowiskowa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неуклюжий, малоприспособленный к жизни или наивный человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недотёпа",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "простак",
                                "target_lang_sense_clarification": "о человеке"
                            }
                        ]
                    }
                }
            ]
        }
    ]

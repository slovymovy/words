                },
                {
                    "sense_id": "e09dd5cd-46e1-38fe-ab81-395f2942e346",
                    "sense_definition": "Able to walk around and not confined to a bed, despite having an injury or illness.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hospital set up a separate area to treat the <w>walking</w> wounded.",
                            "target_lang_translations": {
                                "ru": "Больница выделила отдельную зону для лечения <w>ходячих</w> раненых."
                            }

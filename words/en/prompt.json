{
    "word_family": [
        "prompter",
        "promptly",
        "promptness",
        "promptitude"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "496c93de-9e98-3343-a775-3eef31780862",
                    "sense_definition": "Done quickly, immediately, or without delay.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We received a <w>prompt</w> response to our inquiry.",
                            "target_lang_translations": {
                                "ru": "Мы получили <w>оперативный</w> ответ на наш запрос."
                            }
                        },
                        {
                            "text": "The doctor suggested <w>prompt</w> treatment to avoid complications.",
                            "target_lang_translations": {
                                "ru": "Врач предложил <w>немедленное</w> лечение, чтобы избежать осложнений."
                            }
                        }
                    ],
                    "synonyms": [
                        "quick",
                        "fast",
                        "rapid",
                        "speedy",
                        "immediate"
                    ],
                    "antonyms": [
                        "hesitant",
                        "hesitative",
                        "vacillatory"
                    ],
                    "common_phrases": [
                        "prompt action",
                        "prompt reply",
                        "prompt payment",
                        "prompt response"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный быстро, немедленно или без задержек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быстрый",
                                "target_lang_sense_clarification": "происходящий с большой скоростью"
                            },
                            {
                                "target_lang_word": "оперативный",
                                "target_lang_sense_clarification": "сделанный вовремя и быстро (часто о реакции или ответе)"
                            },
                            {
                                "target_lang_word": "немедленный",
                                "target_lang_sense_clarification": "совершаемый безотлагательно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21fdbb8b-ec89-35fd-8f2c-c91d8a463205",
                    "sense_definition": "Arriving or acting at the correct or expected time; punctual.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please be <w>prompt</w> for the meeting so we can start on time.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, будьте <w>пунктуальны</w> на встрече, чтобы мы могли начать вовремя."
                            }
                        },
                        {
                            "text": "She is always <w>prompt</w> in paying her bills.",
                            "target_lang_translations": {
                                "ru": "Она всегда <w>пунктуальна</w> в оплате счетов."
                            }
                        }
                    ],
                    "synonyms": [
                        "punctual",
                        "on time",
                        "timely"
                    ],
                    "common_phrases": [
                        "be prompt",
                        "prompt arrival"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибывающий или действующий в правильное или ожидаемое время; пунктуальный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пунктуальный",
                                "target_lang_sense_clarification": "делающий всё вовремя"
                            },
                            {
                                "target_lang_word": "точный",
                                "target_lang_sense_clarification": "соблюдающий установленное время"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8dfb6982-1256-3e57-a000-3a6c9b897c11",
                    "sense_definition": "To cause something to happen or to make someone decide to do something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "causation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The news <w>prompted</w> him to call his family immediately.",
                            "target_lang_translations": {
                                "ru": "Новости <w>побудили</w> его немедленно позвонить семье."
                            }
                        },
                        {
                            "text": "The accident <w>prompted</w> a review of the safety procedures.",
                            "target_lang_translations": {
                                "ru": "Авария <w>вызвала</w> пересмотр процедур безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "cause",
                        "trigger",
                        "provoke",
                        "instigate",
                        "motivate",
                        "inspire"
                    ],
                    "common_phrases": [
                        "prompt a response",
                        "prompt an investigation",
                        "prompt a discussion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить что-то произойти или побудить кого-то решить сделать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побуждать",
                                "target_lang_sense_clarification": "склонять к какому-либо действию"
                            },
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "быть причиной возникновения чего-либо"
                            },
                            {
                                "target_lang_word": "подталкивать",
                                "target_lang_sense_clarification": "стимулировать к принятию решения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6c25f49-ce89-3522-88eb-13d02d2fde43",
                    "sense_definition": "To help a speaker or actor remember what to say by telling them the next words.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "assistance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher <w>prompted</w> the student when he forgot the answer.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>подсказал</w> ученику, когда тот забыл ответ."
                            }
                        },
                        {
                            "text": "If the actor forgets his lines, I will <w>prompt</w> him from the side of the stage.",
                            "target_lang_translations": {
                                "ru": "Если актер забудет слова, я <w>подскажу</w> ему из-за кулис."
                            }
                        }
                    ],
                    "synonyms": [
                        "cue",
                        "remind",
                        "assist"
                    ],
                    "common_phrases": [
                        "prompt an actor",
                        "prompt someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помочь оратору или актеру вспомнить, что сказать, подсказав следующие слова."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подсказывать",
                                "target_lang_sense_clarification": "тихо говорить забытые слова"
                            },
                            {
                                "target_lang_word": "суфлировать",
                                "target_lang_sense_clarification": "подсказывать актерам в театре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28223b70-653d-3418-9341-8cba22a9216f",
                    "sense_definition": "To request a user to provide input on a computer or digital system.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The system <w>prompted</w> me to enter a new password.",
                            "target_lang_translations": {
                                "ru": "Система <w>запросила</w> у меня новый пароль."
                            }
                        },
                        {
                            "text": "The app <w>prompted</w> the user to allow location access.",
                            "target_lang_translations": {
                                "ru": "Приложение <w>предложило</w> пользователю разрешить доступ к геолокации."
                            }
                        }
                    ],
                    "synonyms": [
                        "request",
                        "ask"
                    ],
                    "common_phrases": [
                        "prompt for password",
                        "prompt the user"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Запросить у пользователя ввод данных в компьютерной или цифровой системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запрашивать",
                                "target_lang_sense_clarification": "требовать ввода информации"
                            },
                            {
                                "target_lang_word": "предлагать",
                                "target_lang_sense_clarification": "выводить сообщение с просьбой о действии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6a2dc99-f0b6-3f74-9817-2ff120805713",
                    "sense_definition": "To provide instructions or text to an artificial intelligence model to generate a specific result.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>prompted</w> the AI to write a poem about the ocean.",
                            "target_lang_translations": {
                                "ru": "Он <w>дал задание</w> ИИ написать стихотворение об океане."
                            }
                        },
                        {
                            "text": "Users can <w>prompt</w> the model to create images in various styles.",
                            "target_lang_translations": {
                                "ru": "Пользователи могут <w>инструктировать</w> модель создавать изображения в разных стилях."
                            }
                        }
                    ],
                    "synonyms": [
                        "instruct"
                    ],
                    "common_phrases": [
                        "prompt the model",
                        "prompt an AI"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "artificial intelligence"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставить инструкции или текст модели искусственного интеллекта для получения определенного результата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "давать задание",
                                "target_lang_sense_clarification": "отправлять инструкцию нейросети"
                            },
                            {
                                "target_lang_word": "инструктировать",
                                "target_lang_sense_clarification": "задавать параметры генерации"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "961d369a-ce98-309e-83a1-8a6fb245f963",
                    "sense_definition": "A word, signal, or action that reminds someone what to say or do.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "assistance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The actor waited for a <w>prompt</w> from the wings before continuing.",
                            "target_lang_translations": {
                                "ru": "Актер ждал <w>подсказки</w> из кулис, прежде чем продолжить."
                            }
                        },
                        {
                            "text": "The teacher gave the students a writing <w>prompt</w> to start their stories.",
                            "target_lang_translations": {
                                "ru": "Учитель дал ученикам <w>тему</w> для начала их рассказов."
                            }
                        }
                    ],
                    "synonyms": [
                        "cue",
                        "reminder",
                        "hint"
                    ],
                    "common_phrases": [
                        "give a prompt",
                        "writing prompt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, сигнал или действие, напоминающее кому-то, что сказать или сделать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подсказка",
                                "target_lang_sense_clarification": "помощь в том, что нужно сказать или сделать"
                            },
                            {
                                "target_lang_word": "напоминание",
                                "target_lang_sense_clarification": "знак или сигнал, чтобы вспомнить что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbe705df-fb7f-39d2-989a-861086b77e95",
                    "sense_definition": "Textual input or instructions given to an artificial intelligence model to produce a desired output.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I entered a detailed <w>prompt</w> to get the chatbot to summarize the article.",
                            "target_lang_translations": {
                                "ru": "Я ввел подробный <w>промпт</w>, чтобы чат-бот резюмировал статью."
                            }
                        },
                        {
                            "text": "The quality of the AI image depends heavily on the specific <w>prompt</w> used.",
                            "target_lang_translations": {
                                "ru": "Качество изображения ИИ сильно зависит от конкретного используемого <w>запроса</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "instruction",
                        "input"
                    ],
                    "common_phrases": [
                        "enter a prompt",
                        "AI prompt"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "artificial intelligence"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Текстовый ввод или инструкции, даваемые модели искусственного интеллекта для получения желаемого результата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "промпт",
                                "target_lang_sense_clarification": "текстовый запрос к нейросети (профессиональный сленг)"
                            },
                            {
                                "target_lang_word": "запрос",
                                "target_lang_sense_clarification": "инструкция для ИИ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bd7b846-a9f9-37a1-9873-795de856dafb",
                    "sense_definition": "A symbol or message on a computer screen indicating that the system is ready to receive input.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cursor blinked at the command <w>prompt</w>, waiting for instructions.",
                            "target_lang_translations": {
                                "ru": "Курсор мигал в <w>командной строке</w>, ожидая инструкций."
                            }
                        },
                        {
                            "text": "Type your name when the <w>prompt</w> appears on the screen.",
                            "target_lang_translations": {
                                "ru": "Введите свое имя, когда на экране появится <w>приглашение</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "command prompt",
                        "system prompt"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Символ или сообщение на экране компьютера, указывающее, что система готова к приему ввода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приглашение",
                                "target_lang_sense_clarification": "символ готовности системы к вводу (термин)"
                            },
                            {
                                "target_lang_word": "командная строка",
                                "target_lang_sense_clarification": "интерфейс для ввода текстовых команд"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "52b10ce9-b7af-3edc-88ce-3bc6f6f2a8cc",
                    "sense_definition": "A common female given name.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My sister <w>Mary</w> is coming to visit us next week.",
                            "target_lang_translations": {
                                "ru": "Моя сестра <w>Мэри</w> приедет навестить нас на следующей неделе."
                            }
                        },
                        {
                            "text": "There were three girls named <w>Mary</w> in my class at school.",
                            "target_lang_translations": {
                                "ru": "В моем классе в школе было три девочки по имени <w>Мэри</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Mary Jane",
                        "Bloody Mary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенное женское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мэри",
                                "target_lang_sense_clarification": "английское имя"
                            },
                            {
                                "target_lang_word": "Мария",
                                "target_lang_sense_clarification": "русский эквивалент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af0d5d86-397d-3c95-9f56-0e6ee1962b49",
                    "sense_definition": "The mother of Jesus Christ in the Christian religion.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "biblical_figures",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Many churches contain a statue of the Virgin <w>Mary</w> holding the baby Jesus.",
                            "target_lang_translations": {
                                "ru": "Во многих церквях есть статуя Девы <w>Марии</w>, держащей младенца Иисуса."
                            }
                        },
                        {
                            "text": "She prayed to <w>Mary</w> for comfort and protection during the difficult times.",
                            "target_lang_translations": {
                                "ru": "Она молилась <w>Марии</w> об утешении и защите в трудные времена."
                            }
                        }
                    ],
                    "synonyms": [
                        "Virgin Mary",
                        "Madonna",
                        "Our Lady",
                        "Mother of God",
                        "Blessed Virgin"
                    ],
                    "common_phrases": [
                        "Virgin Mary",
                        "Hail Mary",
                        "Mother Mary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мать Иисуса Христа в христианской религии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мария",
                                "target_lang_sense_clarification": "библейский персонаж"
                            },
                            {
                                "target_lang_word": "Богородица",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dde3ab90-5ae9-350c-94a6-352553d5a3c5",
                    "sense_definition": "Mary Magdalene, a prominent disciple of Jesus in the New Testament.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "biblical_figures",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "According to the Bible, <w>Mary</w> Magdalene was the first person to see Jesus after his resurrection.",
                            "target_lang_translations": {
                                "ru": "Согласно Библии, <w>Мария</w> Магдалина была первым человеком, увидевшим Иисуса после его воскресения."
                            }
                        },
                        {
                            "text": "The role of <w>Mary</w> Magdalene in early Christianity has been a subject of much historical study.",
                            "target_lang_translations": {
                                "ru": "Роль <w>Марии</w> Магдалины в раннем христианстве была предметом многих исторических исследований."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мария Магдалина, видная ученица Иисуса в Новом Завете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мария",
                                "target_lang_sense_clarification": "библейский персонаж"
                            }
                        ]

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6c42c769-43bf-3554-90a4-3ca4660dc911",
                    "sense_definition": "Организовать, подготовить и провести какое-либо мероприятие или встречу.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <устроить> небольшую вечеринку для друзей в эту субботу.",
                            "target_lang_translations": {
                                "en": "We decided to <throw> a small party for friends this Saturday."
                            }
                        },
                        {
                            "text": "Коллеги хотят <устроить> сюрприз на день рождения начальника.",
                            "target_lang_translations": {
                                "en": "Colleagues want to <organize> a surprise for the boss's birthday."
                            }
                        }
                    ],
                    "synonyms": [
                        "организовать",
                        "провести"
                    ],
                    "antonyms": [
                        "отменить",
                        "сорвать"
                    ],
                    "common_phrases": [
                        "устроить праздник",
                        "устроить вечеринку",
                        "устроить встречу",
                        "устроить сюрприз"
                    ],
                    "target_lang_definitions": {
                        "en": "To organize, prepare, and conduct an event or meeting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "arrange",
                                "target_lang_sense_clarification": "plan or prepare an event"
                            },
                            {
                                "target_lang_word": "organize",
                                "target_lang_sense_clarification": "coordinate and prepare"
                            },
                            {
                                "target_lang_word": "throw",
                                "target_lang_sense_clarification": "host a party (informal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66b0aed1-2ebf-39dc-bf21-7a50380c1b08",
                    "sense_definition": "Быть удобным, подходящим или приемлемым для кого-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если цена вас <устроит>, мы можем подписать договор прямо сейчас.",
                            "target_lang_translations": {
                                "en": "If the price <suits> you, we can sign the contract right now."
                            }
                        },
                        {
                            "text": "Такое простое решение проблемы вполне <устроит> обе стороны.",
                            "target_lang_translations": {
                                "en": "Such a simple solution to the problem will quite <satisfy> both parties."
                            }
                        }
                    ],
                    "synonyms": [
                        "подойти",
                        "удовлетворить"
                    ],
                    "common_phrases": [
                        "меня это устроит",
                        "вполне устроить",
                        "всех устроить"
                    ],
                    "target_lang_definitions": {
                        "en": "To be convenient, suitable, or acceptable for someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suit",
                                "target_lang_sense_clarification": "be convenient or acceptable"
                            },
                            {
                                "target_lang_word": "satisfy",
                                "target_lang_sense_clarification": "meet requirements or needs"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "542d93e5-4db1-31a3-8b93-a6d383895c6a",
                    "sense_definition": "Совершить какое-либо действие, часто негативное, шумное или привлекающее внимание (скандал, сцену).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пассажир начал громко кричать и грозился <устроить> скандал.",
                            "target_lang_translations": {
                                "en": "The passenger started shouting loudly and threatened to <make> a scene."
                            }
                        },
                        {
                            "text": "Маленький ребёнок устал и может <устроить> истерику в любой момент.",
                            "target_lang_translations": {
                                "en": "The small child is tired and might <throw> a tantrum at any moment."
                            }
                        }
                    ],
                    "synonyms": [
                        "учинить",
                        "закатить"
                    ],
                    "common_phrases": [
                        "устроить скандал",
                        "устроить сцену",
                        "устроить истерику",
                        "устроить разнос"
                    ],
                    "target_lang_definitions": {
                        "en": "To commit an action, often negative, noisy, or attention-grabbing (a scandal, a scene)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "create a scene or scandal"
                            },
                            {
                                "target_lang_word": "throw",
                                "target_lang_sense_clarification": "have a tantrum"
                            },
                            {
                                "target_lang_word": "cause",
                                "target_lang_sense_clarification": "bring about something negative"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59178db2-1843-308f-b28f-b854c2eaf2ee",
                    "sense_definition": "Помочь кому-либо получить место работы, учёбы или временно поселить куда-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец пообещал <устроить> сына на хорошую работу после университета.",
                            "target_lang_translations": {
                                "en": "The father promised to <get> his son a good job after university."
                            }
                        },
                        {
                            "text": "Нам удалось <устроить> ребёнка в лучший детский сад нашего района.",
                            "target_lang_translations": {
                                "en": "We managed to <get> the child into the best kindergarten in our district."
                            }
                        }
                    ],
                    "synonyms": [
                        "пристроить",
                        "определить"
                    ],
                    "common_phrases": [
                        "устроить на работу",
                        "устроить в школу",
                        "устроить в больницу",
                        "устроить в гостиницу"
                    ],
                    "target_lang_definitions": {
                        "en": "To help someone get a job, a place of study, or temporary accommodation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get (someone a job/into)",
                                "target_lang_sense_clarification": "help obtain a position"
                            },
                            {
                                "target_lang_word": "place",
                                "target_lang_sense_clarification": "find a position for"
                            },
                            {
                                "target_lang_word": "set up",
                                "target_lang_sense_clarification": "establish in a position"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d136b902-9680-36a0-98b3-ec9aeb87b66f",
                    "sense_definition": "Наладить, привести в порядок жизненные обстоятельства или быт.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После переезда в новый город им пришлось заново <устроить> свой быт.",
                            "target_lang_translations": {
                                "en": "After moving to a new city, they had to <set up> their household anew."
                            }
                        },
                        {
                            "text": "Она мечтает удачно <устроить> свою личную жизнь и выйти замуж.",
                            "target_lang_translations": {
                                "en": "She dreams of successfully <arranging> her personal life and getting married."
                            }
                        }
                    ],
                    "synonyms": [
                        "наладить",
                        "обустроить"
                    ],
                    "antonyms": [
                        "разрушить",
                        "развалить"
                    ],
                    "common_phrases": [
                        "устроить свою жизнь",
                        "устроить быт",
                        "устроить дела"
                    ],
                    "target_lang_definitions": {
                        "en": "To put in order or establish one's life circumstances or household."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "settle",
                                "target_lang_sense_clarification": "establish order or stability"
                            },
                            {
                                "target_lang_word": "arrange",
                                "target_lang_sense_clarification": "put into order"
                            },
                            {
                                "target_lang_word": "set up",
                                "target_lang_sense_clarification": "establish a household"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
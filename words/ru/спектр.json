                    "common_phrases": [
                        "спектр оператора",
                        "спектр матрицы"
                    ],
                    "target_lang_definitions": {
                        "en": "The set of all eigenvalues of a linear operator in mathematics"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spectrum",
                                "target_lang_sense_clarification": "mathematics"
                            }
                        ]
                    }
                }
            ]
        }

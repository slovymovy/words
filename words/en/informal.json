                },
                {
                    "sense_id": "12f2422f-eb58-3557-9773-044fedad4569",
                    "sense_definition": "Not officially recognized or controlled according to strict rules.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two leaders reached an <w>informal</w> agreement to work together.",
                            "target_lang_translations": {
                                "ru": "Два лидера достигли <w>неофициального</w> соглашения о совместной работе."
                            }

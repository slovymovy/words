    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "795fcb04-c5eb-33c9-9efd-f44489830531",
                    "sense_definition": "Relating to the customs, style, or people of mainland Europe.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "european_style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We enjoyed a light <w>continental</w> breakfast with coffee and croissants at the hotel.",
                            "target_lang_translations": {
                                "ru": "В отеле мы наслаждались легким <w>континентальным</w> завтраком с кофе и круассанами."
                            }
                        },
                        {
                            "text": "She adopted a <w>continental</w> way of dressing after living in Paris for a year.",
                            "target_lang_translations": {
                                "ru": "Прожив год в Париже, она переняла <w>европейскую</w> манеру одеваться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "continental breakfast",
                        "continental quilt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к культуре, стилю или жителям материковой Европы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "континентальный",
                                "target_lang_sense_clarification": "о завтраке, одеяле"
                            },
                            {
                                "target_lang_word": "европейский",
                                "target_lang_sense_clarification": "о стиле, манерах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "840088a8-ea09-38a4-9f99-07514dc0193e",
                    "sense_definition": "Of or relating to the large landmasses of the earth known as continents.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "geographic_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The theory of <w>continental</w> drift explains how landmasses move over millions of years.",
                            "target_lang_translations": {
                                "ru": "Теория <w>континентального</w> дрейфа объясняет, как массивы суши перемещаются на протяжении миллионов лет."
                            }
                        },
                        {
                            "text": "This region experiences a <w>continental</w> climate with hot summers and very cold winters.",
                            "target_lang_translations": {
                                "ru": "Для этого региона характерен <w>континентальный</w> климат с жарким летом и очень холодной зимой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "continental drift",
                        "continental shelf",
                        "continental climate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к крупным массивам суши, известным как континенты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "континентальный",
                                "target_lang_sense_clarification": "геологический, климатический"
                            },
                            {
                                "target_lang_word": "материковый",
                                "target_lang_sense_clarification": "относящийся к материку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53b4970c-f9d8-327f-9a5e-d50148283a51",
                    "sense_definition": "Belonging to the main landmass of a country or region, excluding its islands.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "geographic_mainland",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Shipping goods to <w>continental</w> Europe is faster than sending them to the islands.",
                            "target_lang_translations": {
                                "ru": "Доставка товаров в <w>континентальную</w> Европу происходит быстрее, чем отправка их на острова."
                            }
                        },
                        {
                            "text": "The <w>continental</w> United States consists of forty-eight states located on the mainland.",
                            "target_lang_translations": {
                                "ru": "<w>Континентальная</w> часть США состоит из сорока восьми штатов, расположенных на материке."
                            }
                        }
                    ],
                    "synonyms": [
                        "mainland"
                    ],
                    "common_phrases": [
                        "continental Europe",
                        "continental United States"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий к основной части страны или региона, исключая острова."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "материковый",
                                "target_lang_sense_clarification": "основная часть суши"
                            },
                            {
                                "target_lang_word": "континентальный",
                                "target_lang_sense_clarification": "в геополитическом контексте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5a87f5b-946b-326f-a6bc-834fbbae3b3b",
                    "sense_definition": "Relating to the American colonies during the time of the Revolutionary War.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "historical_us",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Continental</w> Army fought bravely against British forces to gain independence.",
                            "target_lang_translations": {
                                "ru": "<w>Континентальная</w> армия храбро сражалась против британских войск за независимость."
                            }
                        },
                        {
                            "text": "Delegates gathered at the <w>Continental</w> Congress to draft the new laws.",
                            "target_lang_translations": {
                                "ru": "Делегаты собрались на <w>Континентальном</w> конгрессе для разработки новых законов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Continental Congress",
                        "Continental Army"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "United States"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к американским колониям во времена Войны за независимость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "континентальный",
                                "target_lang_sense_clarification": "исторический термин США"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "28cfee7b-e6af-3284-8701-041a0c968cbe",
                    "sense_definition": "A person from the mainland of Europe.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>continentals</w> in the group found the British food quite different from what they were used to.",
                            "target_lang_translations": {
                                "ru": "<w>Европейцы</w> в группе нашли британскую еду довольно отличной от той, к которой они привыкли."
                            }
                        },
                        {
                            "text": "He was clearly a <w>continental</w>, judging by his distinctive accent and fashion style.",
                            "target_lang_translations": {
                                "ru": "Судя по его характерному акценту и стилю одежды, он явно был <w>европейцем</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "European"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек родом из материковой Европы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "европеец",
                                "target_lang_sense_clarification": "житель материковой Европы"
                            },
                            {
                                "target_lang_word": "житель континента",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4922bc04-9fc2-3418-a89c-c315bfeb7727",
                    "sense_definition": "An extremely small amount or value, often used in negative expressions.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "idiom_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do not care a <w>continental</w> about what the neighbors think of my garden.",
                            "target_lang_translations": {
                                "ru": "Меня ни на <w>грош</w> не волнует, что соседи думают о моем саде."
                            }
                        },
                        {
                            "text": "That old broken watch is not worth a <w>continental</w> anymore.",
                            "target_lang_translations": {
                                "ru": "Эти старые сломанные часы больше не стоят и ломаного <w>гроша</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "not worth a continental"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "idiomatic"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезвычайно малое количество или ценность, часто используется в отрицаниях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грош",
                                "target_lang_sense_clarification": "в идиомах о ценности"

                {
                    "sense_id": "866c2379-f603-3002-b8db-856ca5af2c02",
                    "sense_definition": "Iemand die net begint op een bepaalde markt, zoals de woningmarkt of de arbeidsmarkt, of die een eigen bedrijf start.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor <w>starters</w> op de woningmarkt komt er een overgangsregeling.",
                            "target_lang_translations": {
                                "ru": "Для <w>новичков</w> на рынке жилья вводится переходное регулирование."
                            }
                        },
                        {
                            "text": "De bank biedt speciale leningen aan om de beginnende <w>starter</w> te ondersteunen.",
                            "target_lang_translations": {
                                "ru": "Банк предлагает специальные кредиты, чтобы поддержать <w>начинающего предпринимателя</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "toetreder"
                    ],
                    "antonyms": [
                        "doorstromer"
                    ],
                    "common_phrases": [
                        "starter op de woningmarkt",
                        "kansen voor starters"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который только начинает деятельность на определенном рынке (например, жилищном или рынке труда) или открывает свой бизнес."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "новичок",
                                "target_lang_sense_clarification": "на рынке жилья или труда"
                            },
                            {
                                "target_lang_word": "начинающий",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "начинающий предприниматель",
                                "target_lang_sense_clarification": "в бизнесе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5cff126-e030-3f6f-949b-8f2e8a5e8618",

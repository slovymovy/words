{
    "word_family": [],
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "745f0fee-3198-38fd-8a10-77b43ef0e6af",
                    "sense_definition": "Used to indicate a contrast or difference between two facts, ideas, or situations.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "contrast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He loves outdoor activities, <w>whereas</w> his brother prefers reading at home.",
                            "target_lang_translations": {
                                "ru": "Он любит активный отдых, <w>тогда как</w> его брат предпочитает читать дома."
                            }
                        },
                        {
                            "text": "The north of the country is industrial, <w>whereas</w> the south is mostly agricultural.",
                            "target_lang_translations": {
                                "ru": "Север страны — промышленный, <w>в то время как</w> юг — преимущественно сельскохозяйственный."
                            }
                        }
                    ],
                    "synonyms": [
                        "while",
                        "although",
                        "conversely",
                        "but"
                    ],
                    "antonyms": [
                        "similarly",
                        "likewise"
                    ],
                    "common_phrases": [
                        "whereas in fact",
                        "whereas usually"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на противопоставление или различие между двумя фактами, идеями или ситуациями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тогда как",
                                "target_lang_sense_clarification": "выражает явное противопоставление"
                            },
                            {
                                "target_lang_word": "в то время как",
                                "target_lang_sense_clarification": "подчеркивает различие или одновременность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9775a96e-30aa-3ba4-8e97-a49e3c7da2a2",
                    "sense_definition": "Considering the fact that; used formally to introduce a preamble or a statement of fact in legal or official documents.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "preamble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Whereas</w> the defendant has admitted guilt, the court will proceed to sentencing.",
                            "target_lang_translations": {
                                "ru": "<w>Принимая во внимание</w>, что подсудимый признал вину, суд перейдет к вынесению приговора."
                            }
                        },
                        {
                            "text": "<w>Whereas</w> the contract has been fully executed, the project may now commence.",
                            "target_lang_translations": {
                                "ru": "<w>Поскольку</w> контракт был полностью исполнен, проект может начаться."
                            }
                        }
                    ],
                    "synonyms": [
                        "since",
                        "considering",
                        "inasmuch as"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимая во внимание тот факт, что; используется официально для введения преамбулы или констатации факта в юридических документах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимая во внимание",
                                "target_lang_sense_clarification": "стандартная формулировка в преамбулах"
                            },
                            {
                                "target_lang_word": "поскольку",
                                "target_lang_sense_clarification": "формальное обоснование причины"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "484a3adf-ed0c-388f-a7d5-fe8007624ccd",
                    "sense_definition": "A clause or statement in a legal document that begins with the word 'whereas'.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer carefully reviewed the <w>whereases</w> in the preamble of the agreement.",
                            "target_lang_translations": {
                                "ru": "Юрист внимательно изучил <w>пункты преамбулы</w> в соглашении."
                            }
                        },
                        {
                            "text": "The document was filled with complicated <w>whereases</w> that confused the reader.",
                            "target_lang_translations": {
                                "ru": "Документ был наполнен сложными <w>вводными положениями</w>, которые сбивали читателя с толку."
                            }
                        }
                    ],
                    "synonyms": [
                        "preamble",
                        "recital"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пункт или утверждение в юридическом документе, начинающееся со слова 'whereas' (принимая во внимание)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преамбула",
                                "target_lang_sense_clarification": "вводная часть документа"
                            },
                            {
                                "target_lang_word": "вводное положение",
                                "target_lang_sense_clarification": "пункт, описывающий обстоятельства"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1e399c14-4554-3837-b4de-ee4f733ef29f",
                    "sense_definition": "At which place; where.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She returned to the village <w>whereas</w> she was born.",
                            "target_lang_translations": {
                                "ru": "Она вернулась в деревню, <w>где</w> родилась."
                            }
                        },
                        {
                            "text": "They found the hidden path <w>whereas</w> the old tree stood.",
                            "target_lang_translations": {
                                "ru": "Они нашли скрытую тропу <w>там, где</w> стояло старое дерево."
                            }
                        }
                    ],
                    "synonyms": [
                        "where"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "obsolete usage meaning 'where'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В каком месте; где."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "где",
                                "target_lang_sense_clarification": "устаревшее значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
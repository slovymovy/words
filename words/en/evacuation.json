    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b2f50bcc-5d7d-34d7-8f08-aa100f4db587",
                    "sense_definition": "The act of moving people away from a place of danger to a safer location.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "safety_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fire alarm triggered an immediate <w>evacuation</w> of the office building.",
                            "target_lang_translations": {
                                "ru": "Пожарная сигнализация спровоцировала немедленную <w>эвакуацию</w> офисного здания."
                            }
                        },
                        {
                            "text": "Local officials ordered a mass <w>evacuation</w> before the hurricane reached the coast.",
                            "target_lang_translations": {
                                "ru": "Местные власти распорядились о массовой <w>эвакуации</w> до того, как ураган достиг побережья."
                            }
                        }
                    ],
                    "synonyms": [
                        "withdrawal",
                        "removal",
                        "clearance"
                    ],
                    "antonyms": [
                        "occupation"
                    ],
                    "common_phrases": [
                        "emergency evacuation",
                        "mass evacuation",
                        "evacuation plan",
                        "order an evacuation",
                        "evacuation route"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованный вывоз людей из зоны опасности в безопасное место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эвакуация",
                                "target_lang_sense_clarification": "вывоз людей из опасной зоны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be108e8b-cfb6-371c-86fd-d160802a87ed",
                    "sense_definition": "The process of removing the contents from a container or space, often to create a vacuum.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_emptying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pump allows for the rapid <w>evacuation</w> of air from the sealed chamber.",
                            "target_lang_translations": {
                                "ru": "Насос позволяет производить быструю <w>откачку</w> воздуха из герметичной камеры."
                            }
                        },
                        {
                            "text": "Complete <w>evacuation</w> of the tank is necessary before any repairs can begin.",
                            "target_lang_translations": {
                                "ru": "Полное <w>опорожнение</w> резервуара необходимо перед началом любых ремонтных работ."
                            }
                        }
                    ],
                    "synonyms": [
                        "emptying",
                        "draining",
                        "voiding"
                    ],
                    "antonyms": [
                        "filling"
                    ],
                    "common_phrases": [
                        "evacuation of air",
                        "smoke evacuation"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс удаления содержимого из емкости или пространства, часто для создания вакуума."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удаление",
                                "target_lang_sense_clarification": "извлечение содержимого"
                            },
                            {
                                "target_lang_word": "откачка",
                                "target_lang_sense_clarification": "воздуха или газа"
                            },
                            {
                                "target_lang_word": "опорожнение",
                                "target_lang_sense_clarification": "освобождение емкости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5830346f-a26a-3bc0-80ca-c4382d199477",
                    "sense_definition": "The act of discharging waste matter from the body.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "medical_discharge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor asked the patient about the frequency of their bowel <w>evacuation</w>.",

            "senses": [
                {
                    "sense_id": "55226941-50d6-377d-b180-09671ab27f73",
                    "sense_definition": "For a period of time with no fixed or known end.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The construction project has been delayed <w>indefinitely</w> due to a lack of funds.",
                            "target_lang_translations": {
                                "ru": "Строительный проект был отложен <w>на неопределённый срок</w> из-за нехватки средств."
                            }
                        },
                        {
                            "text": "We cannot continue to ignore these environmental issues <w>indefinitely</w>.",
                            "target_lang_translations": {
                                "ru": "Мы не можем продолжать игнорировать эти экологические проблемы <w>бесконечно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "endlessly"
                    ],
                    "antonyms": [
                        "temporarily",
                        "briefly"
                    ],
                    "common_phrases": [
                        "postponed indefinitely",
                        "suspended indefinitely",
                        "closed indefinitely",
                        "continue indefinitely"
                    ],
                    "target_lang_definitions": {
                        "ru": "В течение периода времени, не имеющего фиксированного или известного конца"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на неопределённый срок",
                                "target_lang_sense_clarification": "о времени, отложенных делах"
                            },
                            {
                                "target_lang_word": "бесконечно",
                                "target_lang_sense_clarification": "без конца, очень долго"
                            },
                            {
                                "target_lang_word": "вечно",
                                "target_lang_sense_clarification": "всегда, постоянно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89cb2bb8-3b50-398a-90f0-7c8447387e1e",
                    "sense_definition": "In a way that is not precise, clear, or limited.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "manner_vague",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boundaries of the ancient territory were <w>indefinitely</w> described in the old texts.",
                            "target_lang_translations": {
                                "ru": "Границы древней территории были <w>неопределённо</w> описаны в старых текстах."
                            }
                        },
                        {
                            "text": "Mathematicians often deal with <w>indefinitely</w> large numbers in their calculations.",
                            "target_lang_translations": {
                                "ru": "Математики часто имеют дело с <w>бесконечно</w> большими числами в своих вычислениях."
                            }
                        }
                    ],
                    "synonyms": [

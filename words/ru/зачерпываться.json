            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e2fd8e72-2d2d-373a-9915-98b2094e8b46",
                    "sense_definition": "Страдательный залог к глаголу <w>зачерпывать</w>; набираться, извлекаться (о жидкости или сыпучем веществе) с помощью какого-либо сосуда.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "passive_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вода из колодца легко <w>зачерпывается</w> ведром.",
                            "target_lang_translations": {
                                "en": "Water from the well is easily <w>scooped up</w> with a bucket."
                            }
                        },
                        {
                            "text": "Густой раствор <w>зачерпывается</w> мастерком и наносится на стену.",
                            "target_lang_translations": {
                                "en": "Thick mortar is <w>scooped up</w> with a trowel and applied to the wall."
                            }
                        }
                    ],
                    "synonyms": [
                        "набираться"
                    ],
                    "antonyms": [
                        "выливаться",
                        "высыпаться"
                    ],
                    "common_phrases": [
                        "вода зачерпывается",
                        "жидкость зачерпывается"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Passive form of the verb 'scoop up'; to be gathered or extracted (referring to liquid or loose substances) using a vessel."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be scooped up",
                                "target_lang_sense_clarification": "general use"
                            },
                            {
                                "target_lang_word": "be drawn",
                                "target_lang_sense_clarification": "specifically for water from a well"
                            },
                            {
                                "target_lang_word": "be ladled",
                                "target_lang_sense_clarification": "using a ladle or spoon"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d123d7dd-1346-3de4-a325-4b629064b451",
                    "sense_definition": "Попадать, заливаться внутрь чего-либо, набираться через край (о воде, снеге и т. п.).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При сильном крене лодки вода <w>зачерпывается</w> через борт.",
                            "target_lang_translations": {
                                "en": "When the boat lists heavily, water <w>pours in</w> over the side."
                            }
                        },
                        {
                            "text": "Глубокий снег постоянно <w>зачерпывается</w> в мои короткие ботинки.",
                            "target_lang_translations": {
                                "en": "Deep snow constantly <w>gets scooped</w> into my short boots."
                            }
                        }
                    ],
                    "synonyms": [
                        "заливаться",
                        "попадать"
                    ],
                    "common_phrases": [
                        "вода зачерпывается",
                        "снег зачерпывается"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To get inside or pour into something, to accumulate over the edge (referring to water, snow, etc. entering a container like a boat or shoe)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get in",
                                "target_lang_sense_clarification": "enter a container accidentally"
                            },
                            {
                                "target_lang_word": "pour in",
                                "target_lang_sense_clarification": "flow over the edge"
                            },
                            {
                                "target_lang_word": "be scooped in",
                                "target_lang_sense_clarification": "literal translation, entering due to movement"
                            }
                        ]
                    }
                }
            ]
        }
    ]

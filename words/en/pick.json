    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "78f03bca-bd04-3ec9-9bee-2d3328cf8df1",
                    "sense_definition": "To choose or select one or more options from a group.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "selection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>pick</w> a number between one and ten.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>выберите</w> число от одного до десяти."
                            }
                        },
                        {
                            "text": "I will <w>pick</w> the red dress for the party.",
                            "target_lang_translations": {
                                "ru": "Я <w>выберу</w> красное платье для вечеринки."
                            }
                        }
                    ],
                    "synonyms": [
                        "choose",
                        "select",
                        "opt for"
                    ],
                    "antonyms": [
                        "reject",
                        "refuse"
                    ],
                    "common_phrases": [
                        "pick a winner",
                        "pick and choose",
                        "take your pick"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выбрать или отобрать один или несколько вариантов из группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбирать",
                                "target_lang_sense_clarification": "из вариантов"
                            },
                            {
                                "target_lang_word": "выбрать",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "248fa269-e8a1-300a-9921-253c04a468cf",
                    "sense_definition": "To harvest fruit, vegetables, or flowers by removing them from the plant.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "gathering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went to the orchard to <w>pick</w> fresh apples.",
                            "target_lang_translations": {
                                "ru": "Мы пошли во фруктовый сад <w>собирать</w> свежие яблоки."
                            }
                        },
                        {
                            "text": "She likes to <w>pick</w> wild flowers in the spring.",
                            "target_lang_translations": {
                                "ru": "Она любит <w>рвать</w> полевые цветы весной."
                            }
                        }
                    ],
                    "synonyms": [
                        "harvest",
                        "gather",
                        "collect"
                    ],
                    "antonyms": [
                        "plant"
                    ],
                    "common_phrases": [
                        "pick fruit",
                        "pick flowers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собирать фрукты, овощи или цветы, срывая их с растений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "урожай"
                            },
                            {
                                "target_lang_word": "рвать",
                                "target_lang_sense_clarification": "цветы"
                            },
                            {
                                "target_lang_word": "срывать",
                                "target_lang_sense_clarification": "плоды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04c0eba7-428f-37c1-98d1-92bf11444b67",
                    "sense_definition": "To remove something small or unwanted from a surface or space, often using fingers or a pointed instrument.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He used a toothpick to <w>pick</w> the food from his teeth.",
                            "target_lang_translations": {
                                "ru": "Он использовал зубочистку, чтобы <w>выковырять</w> еду из зубов."
                            }
                        },
                        {
                            "text": "The child was told not to <w>pick</w> his nose in public.",
                            "target_lang_translations": {
                                "ru": "Ребенку сказали не <w>ковырять</w> в носу на людях."
                            }
                        }
                    ],
                    "synonyms": [
                        "remove",
                        "clean"
                    ],
                    "common_phrases": [
                        "pick one's nose",
                        "pick one's teeth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалять что-то мелкое или нежелательное с поверхности или из отверстия, часто пальцами или острым инструментом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ковырять",
                                "target_lang_sense_clarification": "в носу, в зубах"
                            },
                            {
                                "target_lang_word": "выковыривать",
                                "target_lang_sense_clarification": "удалять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf656655-2c17-3142-be78-d06aedbecca3",
                    "sense_definition": "To intentionally provoke or start a quarrel or fight.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "provocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tried to <w>pick</w> a fight with the bouncer at the club.",
                            "target_lang_translations": {
                                "ru": "Он пытался <w>затеять</w> драку с вышибалой в клубе."
                            }
                        },
                        {
                            "text": "Don't <w>pick</w> an argument when you know you are wrong.",
                            "target_lang_translations": {
                                "ru": "Не <w>затевай</w> спор, когда знаешь, что неправ."
                            }
                        }
                    ],
                    "synonyms": [
                        "provoke",
                        "instigate",
                        "start"
                    ],
                    "antonyms": [
                        "avoid"
                    ],
                    "common_phrases": [
                        "pick a fight",
                        "pick a quarrel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеренно провоцировать или начинать ссору или драку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затевать",
                                "target_lang_sense_clarification": "ссору, драку"
                            },
                            {
                                "target_lang_word": "провоцировать",
                                "target_lang_sense_clarification": "конфликт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c53d5af-68d4-3bf6-9807-5550b301ef9d",
                    "sense_definition": "To open a lock without using the key, typically by using a wire or special tool.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "unlocking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The thief managed to <w>pick</w> the lock on the back door.",
                            "target_lang_translations": {
                                "ru": "Вору удалось <w>вскрыть</w> замок на задней двери."
                            }
                        },
                        {
                            "text": "I lost my keys and had to learn how to <w>pick</w> the padlock.",
                            "target_lang_translations": {
                                "ru": "Я потерял ключи и мне пришлось научиться <w>вскрывать</w> навесной замок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pick a lock"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открывать замок без ключа, обычно с помощью проволоки или специального инструмента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вскрывать",
                                "target_lang_sense_clarification": "замок"
                            },
                            {
                                "target_lang_word": "открывать отмычкой",
                                "target_lang_sense_clarification": "инструментом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b9ded91-b381-340a-a9b1-389784bda055",
                    "sense_definition": "To play a stringed musical instrument by plucking the strings individually.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He sat on the porch and began to <w>pick</w> a tune on his banjo.",
                            "target_lang_translations": {
                                "ru": "Он сел на крыльцо и начал <w>наигрывать</w> мелодию на своем банджо."
                            }
                        },
                        {
                            "text": "She learned to <w>pick</w> the guitar strings with great speed.",
                            "target_lang_translations": {
                                "ru": "Она научилась <w>перебирать</w> гитарные струны с большой скоростью."
                            }
                        }
                    ],
                    "synonyms": [
                        "pluck",
                        "strum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Играть на струнном музыкальном инструменте, перебирая струны по отдельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перебирать",
                                "target_lang_sense_clarification": "струны"
                            },
                            {
                                "target_lang_word": "наигрывать",
                                "target_lang_sense_clarification": "мелодию"
                            },
                            {
                                "target_lang_word": "щипать",
                                "target_lang_sense_clarification": "струны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b52d972f-652b-3e81-a777-f86af1638ee2",
                    "sense_definition": "To eat food slowly, sparingly, or without appetite.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "eating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wasn't hungry, so she just <w>picked</w> at her dinner.",
                            "target_lang_translations": {
                                "ru": "Она не была голодна, поэтому просто <w>ковырялась</w> в своем ужине."
                            }
                        },
                        {
                            "text": "The bird <w>picked</w> at the crumbs left on the table.",
                            "target_lang_translations": {
                                "ru": "Птица <w>клевала</w> крошки, оставленные на столе."
                            }
                        }
                    ],
                    "synonyms": [
                        "nibble"
                    ],
                    "antonyms": [
                        "gorge",
                        "devour"
                    ],
                    "common_phrases": [
                        "pick at food"
                    ],
                    "target_lang_definitions": {
                        "ru": "Есть медленно, неохотно или без аппетита."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ковыряться",
                                "target_lang_sense_clarification": "в еде"
                            },
                            {
                                "target_lang_word": "клевать",
                                "target_lang_sense_clarification": "о птицах или людях"

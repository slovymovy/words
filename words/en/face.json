    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f5ae6b40-6c96-3f3f-9309-dc7342fd0070",
                    "sense_definition": "The front part of the head of a human or animal, featuring the eyes, nose, and mouth.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a very pretty <w>face</w>.",
                            "target_lang_translations": {
                                "ru": "У неё очень красивое <w>лицо</w>."
                            }
                        },
                        {
                            "text": "He washed his <w>face</w> with cold water every morning.",
                            "target_lang_translations": {
                                "ru": "Каждое утро он умывал <w>лицо</w> холодной водой."
                            }
                        }
                    ],
                    "synonyms": [
                        "visage",
                        "mug",
                        "countenance"
                    ],
                    "common_phrases": [
                        "face to face",
                        "pretty face",
                        "familiar face"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передняя часть головы человека или животного, на которой расположены глаза, нос и рот."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лицо",
                                "target_lang_sense_clarification": "человека"
                            },
                            {
                                "target_lang_word": "морда",
                                "target_lang_sense_clarification": "животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c92c90d-4bba-31f0-ad43-c862cc02faa8",
                    "sense_definition": "A facial expression that shows feelings or emotions.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Why do you have such a sad <w>face</w> today?",
                            "target_lang_translations": {
                                "ru": "Почему у тебя сегодня такое грустное <w>лицо</w>?"
                            }
                        },
                        {
                            "text": "He tried to keep a straight <w>face</w> when he told the joke.",
                            "target_lang_translations": {
                                "ru": "Он старался сохранить серьёзное <w>лицо</w>, когда рассказывал шутку."
                            }
                        }
                    ],
                    "synonyms": [
                        "expression",
                        "look"
                    ],
                    "common_phrases": [
                        "make a face",
                        "straight face",
                        "poker face",
                        "pull a face"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение лица, показывающее чувства или эмоции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выражение лица",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "мина",
                                "target_lang_sense_clarification": "недовольное или притворное выражение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68e20f16-020d-3f69-84f6-d5df56a97727",
                    "sense_definition": "The front or main surface of an object.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "object_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The climbers slowly moved up the north <w>face</w> of the mountain.",
                            "target_lang_translations": {
                                "ru": "Альпинисты медленно поднимались по северному <w>склону</w> горы."
                            }
                        },
                        {
                            "text": "The clock <w>face</w> was large and easy to read from a distance.",
                            "target_lang_translations": {
                                "ru": "<w>Циферблат</w> часов был большим, и его было легко читать издалека."
                            }
                        }
                    ],
                    "synonyms": [
                        "front",
                        "surface",
                        "side"
                    ],
                    "antonyms": [
                        "heel",
                        "heel"
                    ],
                    "common_phrases": [
                        "clock face",
                        "face of the earth",
                        "cliff face"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передняя или главная поверхность предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лицевая сторона",
                                "target_lang_sense_clarification": "предмета, ткани"
                            },
                            {
                                "target_lang_word": "циферблат",
                                "target_lang_sense_clarification": "часов"
                            },
                            {
                                "target_lang_word": "склон",
                                "target_lang_sense_clarification": "горы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18b1bf1c-d3fa-3ebc-bfc6-a87d1e4c233d",
                    "sense_definition": "Good reputation, dignity, or prestige in the eyes of others.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_reputation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He wanted to save <w>face</w> after making such a big mistake.",
                            "target_lang_translations": {
                                "ru": "Он хотел сохранить <w>лицо</w> после такой грубой ошибки."
                            }
                        },
                        {
                            "text": "The company did not want to lose <w>face</w> with its customers.",
                            "target_lang_translations": {
                                "ru": "Компания не хотела потерять <w>лицо</w> перед своими клиентами."
                            }
                        }
                    ],
                    "synonyms": [
                        "reputation",
                        "image",
                        "prestige",
                        "dignity"
                    ],
                    "common_phrases": [
                        "lose face",
                        "save face"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошая репутация, достоинство или престиж в глазах других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лицо",
                                "target_lang_sense_clarification": "репутация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17c07586-426b-353a-83c8-0bc845a01fef",
                    "sense_definition": "A person, often used when referring to people in a group or crowd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_ref",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was nice to see some familiar <w>faces</w> at the party.",
                            "target_lang_translations": {
                                "ru": "Было приятно увидеть знакомые <w>лица</w> на вечеринке."
                            }
                        },
                        {
                            "text": "There are always new <w>faces</w> in the office at the start of the year.",
                            "target_lang_translations": {
                                "ru": "В начале года в офисе всегда появляются новые <w>лица</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "familiar face",
                        "new face"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to refer to people"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек (часто используется при упоминании людей в группе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лицо",
                                "target_lang_sense_clarification": "человек"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "95364a7e-c7a3-3490-8f44-8ab372aa6963",
                    "sense_definition": "To turn or position oneself so that the face or front is looking at something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_orientation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>face</w> the front of the classroom.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>повернитесь лицом</w> к доске."
                            }
                        },
                        {
                            "text": "She turned her chair to <w>face</w> the window.",
                            "target_lang_translations": {
                                "ru": "Она повернула стул, чтобы <w>сидеть лицом</w> к окну."
                            }
                        }
                    ],
                    "synonyms": [
                        "turn to"
                    ],
                    "common_phrases": [
                        "face forward",
                        "face the wall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повернуться или расположиться так, чтобы лицо или передняя часть были обращены к чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повернуться лицом",
                                "target_lang_sense_clarification": "изменить положение"
                            },
                            {
                                "target_lang_word": "стоять лицом",
                                "target_lang_sense_clarification": "находиться напротив"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d792e7c-8e24-32b5-94bb-d466af24ec38",
                    "sense_definition": "To have the front side pointing in a particular direction.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_orientation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our hotel room <w>faces</w> the ocean.",
                            "target_lang_translations": {
                                "ru": "Окна нашего номера <w>выходят</w> на океан."
                            }
                        },
                        {
                            "text": "The house <w>faces</w> south, so it gets a lot of sunlight.",
                            "target_lang_translations": {
                                "ru": "Дом <w>обращён</w> на юг, поэтому в нём много солнечного света."
                            }
                        }
                    ],
                    "synonyms": [
                        "overlook",
                        "look onto"
                    ],
                    "common_phrases": [
                        "face south",
                        "face the street"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь переднюю сторону, направленную в определённую сторону (о зданиях, окнах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выходить (на)",
                                "target_lang_sense_clarification": "об окнах или дверях"
                            },
                            {
                                "target_lang_word": "быть обращённым",
                                "target_lang_sense_clarification": "о здании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58569fc2-1477-3648-bac7-a1f977caba5b",
                    "sense_definition": "To deal with a difficult situation, problem, or truth; to confront.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_confront",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>face</w> your fears if you want to succeed.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>взглянуть в лицо</w> своим страхам, если хочешь добиться успеха."
                            }
                        },
                        {
                            "text": "We are <w>facing</w> a very difficult problem right now.",
                            "target_lang_translations": {
                                "ru": "Сейчас мы <w>сталкиваемся</w> с очень сложной проблемой."
                            }
                        }
                    ],
                    "synonyms": [
                        "confront",
                        "deal with",
                        "encounter",
                        "handle"
                    ],
                    "common_phrases": [
                        "face the facts",
                        "face reality",
                        "face the music",
                        "face a challenge",
                        "let's face it"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь дело с трудной ситуацией, проблемой или правдой; противостоять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сталкиваться",
                                "target_lang_sense_clarification": "с проблемой"
                            },
                            {
                                "target_lang_word": "смотреть в лицо",
                                "target_lang_sense_clarification": "фактам, опасности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fd723fc-ac28-3829-ac0a-11270a250bdc",
                    "sense_definition": "To compete against an opponent in a game or sport.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "competition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team will <w>face</w> their biggest rivals in the final match.",
                            "target_lang_translations": {
                                "ru": "В финальном матче команда <w>встретится</w> со своими главными соперниками."
                            }
                        },
                        {
                            "text": "He has to <w>face</w> a tough opponent in the next round.",
                            "target_lang_translations": {
                                "ru": "В следующем раунде ему предстоит <w>сразиться</w> с сильным противником."
                            }
                        }
                    ],
                    "synonyms": [
                        "play against"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соревноваться с противником в игре или спорте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречаться",
                                "target_lang_sense_clarification": "в поединке"
                            },
                            {
                                "target_lang_word": "сразиться",
                                "target_lang_sense_clarification": "с соперником"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
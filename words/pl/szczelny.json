    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "528fe00d-8d1a-3627-9fc6-163b203deb32",
                    "sense_definition": "Taki, który nie ma szpar ani otworów i nie przepuszcza cieczy, gazu, powietrza lub innych substancji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fizyczna_szczelnosc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem nowy, <w>szczelny</w> pojemnik na zupę, żeby bezpiecznie zabrać ją do pracy.",
                            "target_lang_translations": {
                                "ru": "Я купил новый <w>герметичный</w> контейнер для супа, чтобы безопасно взять его на работу."
                            }
                        },
                        {
                            "text": "Przez <w>szczelne</w> okna do mieszkania nie wpada zimne powietrze ani wilgoć.",
                            "target_lang_translations": {
                                "ru": "Через <w>герметичные</w> окна в квартиру не попадает холодный воздух и влага."
                            }
                        }
                    ],
                    "synonyms": [
                        "hermetyczny",
                        "nieprzepuszczalny"
                    ],
                    "antonyms": [
                        "nieszczelny",
                        "przepuszczalny"
                    ],
                    "common_phrases": [
                        "szczelne okno",
                        "szczelny pojemnik",
                        "szczelne zamknięcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий щелей или отверстий, не пропускающий жидкости, газы, воздух или другие вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "герметичный",
                                "target_lang_sense_clarification": "не пропускающий воздух и жидкости"
                            },
                            {
                                "target_lang_word": "непроницаемый",
                                "target_lang_sense_clarification": "не пропускающий сквозь себя вещества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8209125-20da-3efa-85a9-aa639634d00b",
                    "sense_definition": "Taki, który jest trudny do pokonania lub podważenia; nieposiadający luk ani słabych punktów (np. o granicy, ochronie, systemie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "szczelnosc_barier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja utworzyła <w>szczelny</w> kordon wokół budynku, aby nikt nie mógł uciec.",
                            "target_lang_translations": {
                                "ru": "Полиция создала <w>плотный</w> кордон вокруг здания, чтобы никто не мог сбежать."
                            }
                        },
                        {
                            "text": "System ochrony banku jest tak <w>szczelny</w>, że włamanie wydaje się niemożliwe.",
                            "target_lang_translations": {
                                "ru": "Система охраны банка настолько <w>надежна</w>, что взлом кажется невозможным."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieprzenikalny",
                        "hermetyczny"
                    ],
                    "antonyms": [
                        "nieszczelny"
                    ],
                    "common_phrases": [
                        "szczelna granica",
                        "szczelna ochrona",
                        "szczelna izolacja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудный для преодоления или нарушения; не имеющий уязвимых мест (о границе, охране, системе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непроницаемый",
                                "target_lang_sense_clarification": "недоступный для проникновения"
                            },
                            {
                                "target_lang_word": "надежный",
                                "target_lang_sense_clarification": "о системе защиты или обороне"
                            },
                            {
                                "target_lang_word": "плотный",
                                "target_lang_sense_clarification": "сплошной, без промежутков (напр. о кордоне)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
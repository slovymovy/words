                {
                    "sense_id": "684cc400-6e04-3717-be8e-4500779d91f4",
                    "sense_definition": "Een beschermend wapen dat voor het lichaam wordt gehouden om aanvallen af te weren",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ridder hield zijn <w>schild</w> omhoog om de pijlen tegen te houden.",
                            "target_lang_translations": {
                                "ru": "Рыцарь поднял свой <w>щит</w>, чтобы остановить стрелы."
                            }
                        },
                        {
                            "text": "Op het <w>schild</w> stond het kleurrijke wapen van de familie afgebeeld.",
                            "target_lang_translations": {
                                "ru": "На <w>щите</w> был изображен красочный герб семьи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iets in het schild voeren",
                        "iemand op het schild heffen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защитное вооружение, которое держат перед телом для отражения атак"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щит",
                                "target_lang_sense_clarification": "военное снаряжение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5e62ff4-cabf-32bb-becb-64b65b33178d",
                    "sense_definition": "De harde, beschermende buitenlaag van bepaalde dieren, zoals schildpadden of kevers",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schildpad trok zijn kop snel terug in zijn veilige <w>schild</w>.",
                            "target_lang_translations": {
                                "ru": "Черепаха быстро втянула голову в свой безопасный <w>панцирь</w>."
                            }
                        },
                        {
                            "text": "Het glanzende <w>schild</w> van de kever beschermt hem tegen roofdieren.",
                            "target_lang_translations": {
                                "ru": "Блестящий <w>панцирь</w> жука защищает его от хищников."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердый защитный наружный слой у некоторых животных, таких как черепахи или жуки"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "панцирь",
                                "target_lang_sense_clarification": "твердая оболочка животных"
                            },
                            {
                                "target_lang_word": "щит",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b53a8a8-ce11-3539-ba1c-52b2f55557dc",
                    "sense_definition": "Een bord met een opschrift of afbeelding, vaak aan een gevel bevestigd als aanduiding",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "signage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Boven de ingang van de oude herberg hing een verweerd houten <w>schild</w>.",

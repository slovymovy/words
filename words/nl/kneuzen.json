{
    "word_family": [
        "kneus",
        "kneuzing"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c04ce6b9-52ff-338c-bcc1-3d9ecf85900f",
                    "sense_definition": "Lichamelijk letsel oplopen of veroorzaken door een harde stoot of klap, waarbij de huid niet scheurt maar het weefsel eronder beschadigd raakt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft zijn enkel <w>gekneusd</w> tijdens het voetballen.",
                            "target_lang_translations": {
                                "en": "He <w>bruised</w> his ankle while playing football.",
                                "ru": "Он <w>ушиб</w> лодыжку во время игры в футбол."
                            }
                        },
                        {
                            "text": "Pas op dat je het fruit in de tas niet <w>kneust</w>.",
                            "target_lang_translations": {
                                "en": "Be careful not to <w>bruise</w> the fruit in the bag.",
                                "ru": "Смотри, не <w>помни</w> фрукты в сумке."
                            }
                        }
                    ],
                    "synonyms": [
                        "kwetsen"
                    ],
                    "common_phrases": [
                        "een rib kneuzen",
                        "zwaar gekneusd",
                        "een enkel kneuzen"
                    ],
                    "target_lang_definitions": {
                        "en": "To sustain or cause physical injury by a hard blow, where the skin does not tear but the tissue underneath is damaged; also applies to damaging fruit.",
                        "ru": "Получить или нанести телесное повреждение сильным ударом, при котором кожа не рвется, но повреждаются ткани под ней."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bruise",
                                "target_lang_sense_clarification": "to injure tissue or fruit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ушибить",
                                "target_lang_sense_clarification": "повредить часть тела"
                            },
                            {
                                "target_lang_word": "помять",
                                "target_lang_sense_clarification": "о фруктах или предметах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f51b4256-b784-33d0-b831-0939a9b4ce3d",
                    "sense_definition": "De meervoudsvorm van <w>kneus</w>; verwijst naar plekken op het lichaam die door een stoot beschadigd zijn, of in informele taal naar onhandige personen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de val zat hij onder de schrammen en <w>kneuzen</w>.",
                            "target_lang_translations": {
                                "en": "After the fall, he was covered in scratches and <w>bruises</w>.",
                                "ru": "После падения он был весь в царапинах и <w>синяках</w>."
                            }
                        },
                        {
                            "text": "Die twee <w>kneuzen</w> hebben het hele project in de soep laten lopen.",
                            "target_lang_translations": {
                                "en": "Those two <w>losers</w> messed up the whole project.",
                                "ru": "Эти два <w>неудачника</w> запороли весь проект."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The plural form of 'kneus'; refers to spots on the body damaged by a blow, or in informal language to clumsy or awkward persons.",
                        "ru": "Множественное число от <w>kneus</w>; обозначает поврежденные места на теле или, в неформальном языке, неуклюжих людей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bruises",
                                "target_lang_sense_clarification": "physical injuries"
                            },
                            {
                                "target_lang_word": "losers",
                                "target_lang_sense_clarification": "clumsy or awkward people"
                            },
                            {
                                "target_lang_word": "dorks",
                                "target_lang_sense_clarification": "socially awkward people"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "неудачники",
                                "target_lang_sense_clarification": "о людях"
                            },
                            {
                                "target_lang_word": "ушибы",
                                "target_lang_sense_clarification": "травмы"
                            },
                            {
                                "target_lang_word": "синяки",
                                "target_lang_sense_clarification": "видимые следы травм"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8cddc39a-95dc-382f-b60b-dbe84d169453",
                    "sense_definition": "Способ передвижения быстрым аллюром, при котором тело на мгновение отрывается от земли; не шагом.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы опаздывали на поезд и неслись на вокзал <бегом>.",
                            "target_lang_translations": {
                                "en": "We were late for the train and rushed to the station <at a run>."
                            }
                        },
                        {
                            "text": "Солдаты преодолевали полосу препятствий <бегом>.",
                            "target_lang_translations": {
                                "en": "The soldiers overcame the obstacle course <at a run>."
                            }
                        }
                    ],
                    "synonyms": [
                        "быстро",
                        "стремительно"
                    ],
                    "antonyms": [
                        "шагом",
                        "медленно"
                    ],
                    "common_phrases": [
                        "пуститься бегом",
                        "передвигаться бегом"
                    ],
                    "target_lang_definitions": {
                        "en": "A method of moving at a fast pace where the body momentarily leaves the ground; not walking."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at a run",
                                "target_lang_sense_clarification": "adverbial phrase indicating movement speed"
                            },
                            {
                                "target_lang_word": "running",
                                "target_lang_sense_clarification": "participle describing the manner of movement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56ad4c63-3b0b-3a05-b192-4776a5118319",
                    "sense_definition": "Очень быстро, поспешно, срочно (обычно о выполнении какого-либо дела).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужно <бегом> закончить отчёт и отправить его начальнику.",
                            "target_lang_translations": {
                                "en": "I need to finish the report <on the double> and send it to the boss."
                            }
                        },
                        {
                            "text": "Он <бегом> сбегал в магазин за хлебом перед обедом.",
                            "target_lang_translations": {
                                "en": "He went to the store <on the double> to get bread before lunch."
                            }
                        }
                    ],
                    "synonyms": [
                        "срочно",
                        "спешно",
                        "пулей",
                        "мухой",
                        "молниеносно",
                        "одна нога здесь, другая там"
                    ],
                    "antonyms": [
                        "медленно",
                        "неторопливо",
                        "не спеша",
                        "вразвалочку"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Very quickly, hastily, or urgently (usually referring to getting a task done)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on the double",
                                "target_lang_sense_clarification": "idiom implying urgency or speed"
                            },
                            {
                                "target_lang_word": "in a rush",
                                "target_lang_sense_clarification": "hastily"
                            },
                            {
                                "target_lang_word": "double-quick",
                                "target_lang_sense_clarification": "very quickly"
                            }
                        ]
                    }
                }
            ]
        },
        {

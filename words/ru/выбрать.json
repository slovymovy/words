    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9d74d63d-7c99-3c66-b539-74ada79b9858",
                    "sense_definition": "Определить и взять то, что нужно, нравится или подходит, из нескольких вариантов.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "choice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы должны <выбрать> хороший подарок для мамы.",
                            "target_lang_translations": {
                                "en": "We need to <choose> a good gift for mom."
                            }
                        },
                        {
                            "text": "Ты можешь <выбрать> любой цвет, который тебе нравится.",
                            "target_lang_translations": {
                                "en": "You can <choose> any color you like."
                            }
                        }
                    ],
                    "synonyms": [
                        "подобрать",
                        "отобрать"
                    ],
                    "common_phrases": [
                        "выбрать подарок",
                        "выбрать профессию",
                        "выбрать путь",
                        "выбрать место"
                    ],
                    "target_lang_definitions": {
                        "en": "To identify and take what is needed, liked, or suitable from several options."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "choose",
                                "target_lang_sense_clarification": "general act of making a choice"
                            },
                            {
                                "target_lang_word": "pick",
                                "target_lang_sense_clarification": "often implies a more casual or personal selection"
                            },
                            {
                                "target_lang_word": "select",
                                "target_lang_sense_clarification": "implies careful choice from a specific group"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2d7ab1d-c00e-3aed-b491-b3d588f047ad",
                    "sense_definition": "Назначить кого-либо на должность или присвоить статус путём голосования.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "choice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Граждане пришли на участки, чтобы <выбрать> президента.",
                            "target_lang_translations": {
                                "en": "Citizens came to the polling stations to <elect> a president."
                            }
                        },
                        {
                            "text": "Совет директоров должен <выбрать> нового председателя компании.",
                            "target_lang_translations": {
                                "en": "The board of directors must <elect> a new company chairman."
                            }
                        }
                    ],
                    "synonyms": [
                        "избрать"
                    ],
                    "common_phrases": [
                        "выбрать президента",
                        "выбрать депутата",
                        "выбрать председателя"
                    ],
                    "target_lang_definitions": {
                        "en": "To appoint someone to a position or assign a status by voting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "elect",
                                "target_lang_sense_clarification": "to choose by voting"
                            },
                            {
                                "target_lang_word": "choose",
                                "target_lang_sense_clarification": "general selection for a role"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30154b56-9cbf-3a1e-9f14-ba53df6a3228",
                    "sense_definition": "Найти свободное время или удобный момент для совершения какого-либо действия.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я постараюсь <выбрать> время и зайти к тебе в гости.",
                            "target_lang_translations": {
                                "en": "I will try to <find> the time to come visit you."
                            }
                        },
                        {
                            "text": "Ему удалось <выбрать> удачный момент для важного разговора.",
                            "target_lang_translations": {
                                "en": "He managed to <pick> a good moment for the important conversation."
                            }
                        }
                    ],
                    "synonyms": [
                        "найти",
                        "выкроить",
                        "улучить"
                    ],
                    "common_phrases": [
                        "выбрать время",
                        "выбрать момент",
                        "выбрать минутку",
                        "выбрать час"
                    ],
                    "target_lang_definitions": {
                        "en": "To find free time or a convenient moment to perform an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "find",
                                "target_lang_sense_clarification": "used with time or opportunity"
                            },
                            {
                                "target_lang_word": "pick",
                                "target_lang_sense_clarification": "used with a specific moment or time"
                            },
                            {
                                "target_lang_word": "choose",
                                "target_lang_sense_clarification": "to decide on a specific time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7288a914-4ab4-3112-aff6-3c3a303f27c5",
                    "sense_definition": "Полностью извлечь, достать всё содержимое откуда-либо или поднять наверх (о сетях, якоре).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Матросам приказали срочно <выбрать> якорь и готовиться к отплытию.",
                            "target_lang_translations": {
                                "en": "The sailors were ordered to urgently <weigh> anchor and prepare for departure."
                            }
                        },
                        {
                            "text": "Нам нужно полностью <выбрать> грунт из этой ямы.",
                            "target_lang_translations": {
                                "en": "We need to completely <remove> the soil from this pit."
                            }
                        }
                    ],
                    "synonyms": [
                        "извлечь",
                        "забрать",
                        "изъять"
                    ],
                    "common_phrases": [
                        "выбрать якорь",
                        "выбрать сеть",
                        "выбрать всё дочиста"
                    ],
                    "target_lang_definitions": {
                        "en": "To completely extract contents or pull up (e.g., nets, anchor)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "haul in",
                                "target_lang_sense_clarification": "for nets, ropes, or slack"
                            },
                            {
                                "target_lang_word": "weigh",
                                "target_lang_sense_clarification": "specifically for an anchor"
                            },
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "to take out contents completely"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
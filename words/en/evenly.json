    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "97797df5-ed2b-3361-8e67-6b648cfee861",
                    "sense_definition": "In a fair manner of distribution, giving the same amount or number to each; equally.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "distribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To avoid arguments, he divided the sweets <w>evenly</w> between his two children.",
                            "target_lang_translations": {
                                "ru": "Чтобы избежать споров, он разделил сладости <w>поровну</w> между двумя своими детьми."
                            }
                        },
                        {
                            "text": "The two teams were <w>evenly</w> matched during the final game.",
                            "target_lang_translations": {
                                "ru": "В финальной игре команды были <w>равны</w> по силам."
                            }
                        }
                    ],
                    "synonyms": [
                        "equally",
                        "fairly",
                        "equitably"
                    ],
                    "antonyms": [
                        "unevenly",
                        "unequally",
                        "unfairly"
                    ],
                    "common_phrases": [
                        "divide evenly",
                        "split evenly",
                        "distribute evenly",
                        "evenly matched"
                    ],
                    "target_lang_definitions": {
                        "ru": "Справедливо, раздавая всем одинаковое количество; поровну."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по́ровну",
                                "target_lang_sense_clarification": "на равные части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa96c0c5-a201-3f6c-88e5-bb566dae9e47",
                    "sense_definition": "So as to form a flat, smooth, or uniform surface.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spread the icing <w>evenly</w> over the top of the cake.",
                            "target_lang_translations": {
                                "ru": "<w>Равномерно</w> распределите глазурь по верху торта."
                            }
                        },
                        {
                            "text": "The paint must be applied <w>evenly</w> to avoid streaks on the wall.",
                            "target_lang_translations": {
                                "ru": "Краску необходимо наносить <w>равномерно</w>, чтобы избежать разводов на стене."
                            }
                        }
                    ],
                    "synonyms": [
                        "uniformly",
                        "smoothly",
                        "flatly"
                    ],
                    "antonyms": [
                        "unevenly",
                        "roughly"
                    ],
                    "common_phrases": [
                        "spread evenly",
                        "apply evenly",
                        "spaced evenly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Так, чтобы поверхность была плоской, гладкой или однородной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "равноме́рно",
                                "target_lang_sense_clarification": "одинаково по всей поверхности"
                            },
                            {
                                "target_lang_word": "ро́вно",
                                "target_lang_sense_clarification": "гладко, без неровностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b4d9ffc-f1ab-34bb-a48a-9a739813f8c0",
                    "sense_definition": "In a manner that leaves no remainder when dividing numbers.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mathematics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The number twelve is <w>evenly</w> divisible by two, three, four, and six.",
                            "target_lang_translations": {
                                "ru": "Число двенадцать делится <w>нацело</w> на два, три, четыре и шесть."
                            }
                        },
                        {
                            "text": "We need a total count that can be split <w>evenly</w> into three groups.",
                            "target_lang_translations": {
                                "ru": "Нам нужно общее количество, которое можно разделить <w>нацело</w> на три группы."
                            }
                        }
                    ],
                    "synonyms": [
                        "exactly"
                    ],
                    "common_phrases": [
                        "evenly divisible"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без остатка при делении чисел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на́цело",
                                "target_lang_sense_clarification": "без остатка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4607d70b-3cc3-3419-8d0f-3488399ff47e",
                    "sense_definition": "In terms of or by means of even numbers.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mathematics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The complex pattern relies on <w>evenly</w> counted cycles to maintain synchronization.",
                            "target_lang_translations": {
                                "ru": "Сложный узор опирается на <w>чётное</w> количество циклов для поддержания синхронизации."
                            }
                        },
                        {
                            "text": "The system processes odd and <w>evenly</w> numbered items differently.",
                            "target_lang_translations": {
                                "ru": "Система обрабатывает нечётные и <w>чётные</w> элементы по-разному."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с чётными числами."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }
    ]

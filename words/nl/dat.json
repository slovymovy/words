    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "68dc976c-0116-3301-ae80-8fd77e9101ef",
                    "sense_definition": "Verwijst zelfstandig naar een zaak, situatie of iets dat eerder genoemd is (vaak met nadruk of om onderscheid te maken met 'dit').",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "demonstrative_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb <w>dat</w> nog nooit eerder gezien.",
                            "target_lang_translations": {
                                "en": "I have never seen <w>that</w> before.",
                                "ru": "Я никогда раньше <w>этого</w> не видел."
                            }
                        },
                        {
                            "text": "Wat is <w>dat</w> voor een raar ding?",
                            "target_lang_translations": {
                                "en": "What kind of strange thing is <w>that</w>?",
                                "ru": "Что <w>это</w> за странная штука?"
                            }
                        }
                    ],
                    "synonyms": [
                        "die"
                    ],
                    "antonyms": [
                        "dit"
                    ],
                    "common_phrases": [
                        "dat is",
                        "dat klopt",
                        "dat mag"
                    ],
                    "target_lang_definitions": {
                        "en": "Refers independently to a thing, situation, or idea mentioned earlier or understood from context.",
                        "ru": "Самостоятельно указывает на предмет, ситуацию или что-то, упомянутое ранее (часто с акцентом или для противопоставления 'dit')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "demonstrative pronoun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "это",
                                "target_lang_sense_clarification": "указательное местоимение"
                            },
                            {
                                "target_lang_word": "то",
                                "target_lang_sense_clarification": "указательное местоимение (удалённый объект)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7a8798b-74df-38fb-a318-f869b13f74ac",
                    "sense_definition": "Gebruikt in combinatie met het werkwoord 'zijn' om personen of dingen te identificeren of te benoemen (ongeacht enkelvoud of meervoud).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "identifying_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Dat</w> is mijn broer die daar loopt.",
                            "target_lang_translations": {
                                "en": "<w>That</w> is my brother walking there.",
                                "ru": "<w>Это</w> мой брат там идёт."
                            }
                        },
                        {
                            "text": "<w>Dat</w> zijn de mooiste bloemen uit de tuin.",
                            "target_lang_translations": {
                                "en": "<w>Those</w> are the most beautiful flowers from the garden.",
                                "ru": "<w>Это</w> самые красивые цветы из сада."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dat is",
                        "dat zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "Used with the verb 'to be' to identify people or things.",
                        "ru": "Используется в сочетании с глаголом 'zijn' (быть) для идентификации или называния лиц или предметов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "identifying pronoun (singular)"
                            },
                            {
                                "target_lang_word": "those",
                                "target_lang_sense_clarification": "identifying pronoun (plural context)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "это",
                                "target_lang_sense_clarification": "в конструкции 'это есть'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36e5736f-0c8f-3d2c-a82c-0e879c756a46",
                    "sense_definition": "Verwijst terug naar een onzijdig zelfstandig naamwoord (het-woord) dat eerder in de zin is genoemd.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "relative_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het boek <w>dat</w> op tafel ligt, is van mij.",
                            "target_lang_translations": {
                                "en": "The book <w>that</w> is on the table is mine.",
                                "ru": "Книга, <w>которая</w> лежит на столе, — моя."
                            }
                        },
                        {
                            "text": "Dit is het enige <w>dat</w> ik zeker weet.",
                            "target_lang_translations": {
                                "en": "This is the only thing <w>that</w> I know for sure.",
                                "ru": "Это единственное, <w>что</w> я точно знаю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het meisje dat",
                        "het huis dat"
                    ],
                    "target_lang_definitions": {
                        "en": "Introduces a clause that provides more information about a preceding neuter noun.",
                        "ru": "Отсылает к существительному среднего рода (het-слово), упомянутому ранее в предложении."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "relative pronoun"
                            },
                            {
                                "target_lang_word": "which",
                                "target_lang_sense_clarification": "relative pronoun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "который",
                                "target_lang_sense_clarification": "относительное местоимение"
                            },
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "союзное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "163e96c9-2f4a-343a-b720-b337b3acbeeb",
                    "sense_definition": "Verwijst naar een persoon op een onvriendelijke, afstandelijke of minachtende manier.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "pejorative_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Dat</w> denkt maar dat het alles kan maken!",
                            "target_lang_translations": {
                                "en": "<w>That</w> thinks it can get away with anything!",
                                "ru": "<w>Это</w> ещё думает, что ему всё позволено!"
                            }
                        },
                        {
                            "text": "Moet <w>dat</w> nu echt mee naar het feestje?",
                            "target_lang_translations": {
                                "en": "Does <w>that</w> really have to come along to the party?",
                                "ru": "<w>Это</w> действительно должно идти с нами на вечеринку?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejoratief, onvriendelijk"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Refers to a person in an unfriendly, distant, or dehumanizing way.",
                        "ru": "Указывает на человека в недружелюбной, отстранённой или пренебрежительной манере."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "pejorative reference"
                            },
                            {
                                "target_lang_word": "that one",
                                "target_lang_sense_clarification": "pejorative reference"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "это",
                                "target_lang_sense_clarification": "пренебрежительно о человеке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "c2cdba04-ccb7-357b-a536-a917612b02c1",
                    "sense_definition": "Verbindt een bijzin met de hoofdzin; leidt vaak een mededeling, gedachte, wens of feit in.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "conjunction_subordinate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik denk <w>dat</w> het morgen gaat regenen.",
                            "target_lang_translations": {
                                "en": "I think <w>that</w> it is going to rain tomorrow.",
                                "ru": "Я думаю, <w>что</w> завтра пойдёт дождь."
                            }
                        },
                        {
                            "text": "Hij vertelde me <w>dat</w> hij een nieuwe baan heeft.",
                            "target_lang_translations": {
                                "en": "He told me <w>that</w> he has a new job.",
                                "ru": "Он сказал мне, <w>что</w> у него новая работа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ik denk dat",
                        "ik hoop dat",
                        "het feit dat",
                        "zodat"
                    ],
                    "target_lang_definitions": {
                        "en": "Connects a subordinate clause to the main clause.",
                        "ru": "Связывает придаточное предложение с главным; часто вводит утверждение, мысль, желание или факт."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "conjunction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "изъяснительный союз"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "2dde96c2-e41a-3998-a8ad-49f8caca9267",
                    "sense_definition": "Staat voor een onzijdig zelfstandig naamwoord (het-woord) om dit specifiek aan te wijzen (vaak iets dat verder weg is).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "demonstrative_determiner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil <w>dat</w> boek graag lezen.",
                            "target_lang_translations": {
                                "en": "I would like to read <w>that</w> book.",
                                "ru": "Я хочу прочитать <w>ту</w> книгу."
                            }
                        },
                        {
                            "text": "<w>Dat</w> huis staat al jaren leeg.",
                            "target_lang_translations": {
                                "en": "<w>That</w> house has been empty for years.",
                                "ru": "<w>Тот</w> дом стоит пустым уже много лет."
                            }
                        }
                    ],
                    "antonyms": [
                        "dit"
                    ],
                    "common_phrases": [
                        "dat soort",
                        "dat moment"
                    ],
                    "target_lang_definitions": {
                        "en": "Used before a neuter noun to indicate a specific person or thing.",
                        "ru": "Стоит перед существительным среднего рода для его конкретного указания (часто на то, что находится на расстоянии)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "demonstrative determiner"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тот",
                                "target_lang_sense_clarification": "указательное местоимение (муж. р.)"
                            },
                            {
                                "target_lang_word": "та",
                                "target_lang_sense_clarification": "указательное местоимение (жен. р.)"
                            },
                            {
                                "target_lang_word": "то",
                                "target_lang_sense_clarification": "указательное местоимение (ср. р.)"
                            }
                        ]
                    }
                }
            ]
        },

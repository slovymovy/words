    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ece0249c-0ea8-347b-99db-ccddc9faac1c",
                    "sense_definition": "Mówić lub pisać coś w reakcji na zadane pytanie, lub otrzymaną wiadomość",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Student poprawnie <odpowiada> na pytanie profesora.",
                            "target_lang_translations": {
                                "ru": "Студент правильно <отвечает> на вопрос профессора."
                            }
                        },
                        {
                            "text": "Zawsze staram się szybko <odpowiadać> na służbowe e-maile.",
                            "target_lang_translations": {
                                "ru": "Я всегда стараюсь быстро <отвечать> на рабочие электронные письма."
                            }
                        }
                    ],
                    "synonyms": [
                        "odrzekać",
                        "odpierać"
                    ],
                    "antonyms": [
                        "pytać"
                    ],
                    "common_phrases": [
                        "odpowiadać na pytania",
                        "odpowiadać na list",
                        "odpowiadać twierdząco",
                        "odpowiadać przecząco"
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить или писать что-то в ответ на заданный вопрос или полученное сообщение"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвечать",
                                "target_lang_sense_clarification": "на вопрос, письмо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a095a5c0-96b4-3ce8-be8f-ac620dd170f5",
                    "sense_definition": "Być dla kogoś właściwym, dogodnym lub podobać się komuś",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "suitability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten termin spotkania bardzo mi <odpowiada>.",
                            "target_lang_translations": {
                                "ru": "Это время встречи мне очень <подходит>."
                            }
                        },
                        {
                            "text": "Czy <odpowiada> ci kuchnia włoska na dzisiejszą kolację?",
                            "target_lang_translations": {
                                "ru": "Тебе <подходит> итальянская кухня для сегодняшнего ужина?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "to mi odpowiada",
                        "czy to ci odpowiada"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть подходящим, удобным для кого-то или нравиться кому-то"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подходить",
                                "target_lang_sense_clarification": "быть удобным"
                            },
                            {
                                "target_lang_word": "устраивать",
                                "target_lang_sense_clarification": "быть приемлемым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a84b9e0-8d9b-3701-b289-bf6588b8debd",
                    "sense_definition": "Udzielać ustnych wyjaśnień nauczycielowi w szkole w celu sprawdzenia wiedzy i otrzymania oceny",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzisiaj będę <odpowiadać> z historii przy tablicy.",
                            "target_lang_translations": {
                                "ru": "Сегодня я буду <отвечать> по истории у доски."
                            }
                        },
                        {
                            "text": "Uczeń bardzo się stresował, gdy musiał <odpowiadać> przed całą klasą.",
                            "target_lang_translations": {
                                "ru": "Ученик очень нервничал, когда ему приходилось <отвечать> перед всем классом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odpowiadać przy tablicy",
                        "odpowiadać z matematyki",
                        "odpowiadać na ocenę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устно отвечать учителю на уроке для проверки знаний и получения оценки"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвечать",
                                "target_lang_sense_clarification": "урок, задание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb2dcc20-675b-34bd-9010-9b62587eab1d",
                    "sense_definition": "Ponosić odpowiedzialność lub konsekwencje za kogoś, lub za coś",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "responsibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice <odpowiadają> za bezpieczeństwo i zachowanie swoich dzieci.",
                            "target_lang_translations": {
                                "ru": "Родители <отвечают> за безопасность и поведение своих детей."
                            }
                        },
                        {
                            "text": "Kierownik projektu <odpowiada> przed zarządem za wyniki zespołu.",
                            "target_lang_translations": {
                                "ru": "Руководитель проекта <отвечает> перед правлением за результаты команды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odpowiadać za coś",
                        "odpowiadać karnie",
                        "odpowiadać głową"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нести ответственность или последствия за кого-то или за что-то"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвечать",
                                "target_lang_sense_clarification": "нести ответственность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f17e66d7-63d4-3a6e-b884-6f10dc6a72d2",
                    "sense_definition": "Być zgodnym z czymś, pasować do wzorca lub być równoważnym innej rzeczy",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "correspondence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego rysopis dokładnie <odpowiada> wyglądowi poszukiwanego sprawcy.",
                            "target_lang_translations": {
                                "ru": "Его описание точно <соответствует> внешности разыскиваемого преступника."
                            }
                        },
                        {
                            "text": "Jakość tego produktu nie <odpowiada> jego wysokiej cenie.",
                            "target_lang_translations": {
                                "ru": "Качество этого продукта не <соответствует> его высокой цене."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odpowiadać opisowi",
                        "odpowiadać prawdzie",
                        "odpowiadać normom",
                        "odpowiadać wymaganiom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соответствовать чему-то, совпадать с образцом или быть равноценным"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соответствовать",
                                "target_lang_sense_clarification": "совпадать"
                            },
                            {
                                "target_lang_word": "отвечать",
                                "target_lang_sense_clarification": "требованиям, нормам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e977f583-0bbe-3b9f-a251-f346170ba4c4",
                    "sense_definition": "Reagować na czyjeś działanie, zachowanie lub bodziec",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na miłe powitanie sąsiada zawsze <odpowiadam> szerokim uśmiechem.",
                            "target_lang_translations": {
                                "ru": "На приветливое приветствие соседа я всегда <отвечаю> широкой улыбкой."
                            }
                        },
                        {
                            "text": "Organizm pacjenta dobrze <odpowiada> na zastosowane leczenie.",
                            "target_lang_translations": {
                                "ru": "Организм пациента хорошо <реагирует> на примененное лечение."
                            }
                        }
                    ],
                    "synonyms": [
                        "reagować",
                        "odwzajemniać"
                    ],
                    "common_phrases": [
                        "odpowiadać uśmiechem",
                        "odpowiadać ogniem",
                        "odpowiadać atakiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Реагировать на чье-то действие, поведение или раздражитель"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвечать",
                                "target_lang_sense_clarification": "реагировать"
                            },
                            {
                                "target_lang_word": "реагировать",
                                "target_lang_sense_clarification": "отзываться на воздействие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
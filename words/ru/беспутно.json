    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2734976c-6032-36f9-9e75-8f0a77b2c827",
                    "sense_definition": "Бессмысленно, безрассудно, не имея определённой цели или плана.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <w>беспутно</w> потратил всё своё наследство за один год.",
                            "target_lang_translations": {
                                "en": "He <w>recklessly</w> spent his entire inheritance in one year."
                            }
                        },
                        {
                            "text": "Мы <w>беспутно</w> бродили по улицам города до самого утра.",
                            "target_lang_translations": {
                                "en": "We wandered <w>aimlessly</w> through the city streets until the very morning."
                            }
                        }
                    ],
                    "synonyms": [
                        "бессмысленно",
                        "безрассудно",
                        "бесцельно"
                    ],
                    "antonyms": [
                        "путно"
                    ],
                    "common_phrases": [
                        "беспутно тратить",
                        "беспутно проводить время",
                        "беспутно жить"
                    ],
                    "target_lang_definitions": {
                        "en": "Senselessly, recklessly, without a specific goal or plan."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "aimlessly",
                                "target_lang_sense_clarification": "without a clear purpose or direction"
                            },
                            {
                                "target_lang_word": "recklessly",
                                "target_lang_sense_clarification": "without thinking about the consequences"
                            },
                            {
                                "target_lang_word": "foolishly",
                                "target_lang_sense_clarification": "lacking good sense or judgment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78fbb03b-d8f8-3610-8783-4c7b1ec80a94",
                    "sense_definition": "Разгульно, развратно, нарушая нормы морали и поведения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодой человек начал жить <w>беспутно</w>, забыв о своих обязанностях и семье.",
                            "target_lang_translations": {
                                "en": "The young man began to live <w>dissolutely</w>, forgetting about his duties and family."
                            }
                        },
                        {
                            "text": "Компания вела себя шумно и <w>беспутно</w>, привлекая внимание прохожих.",
                            "target_lang_translations": {
                                "en": "The group behaved noisily and <w>wildly</w>, attracting the attention of passersby."
                            }
                        }
                    ],
                    "synonyms": [
                        "разгульно",
                        "развратно"
                    ],
                    "antonyms": [
                        "порядочно"
                    ],
                    "common_phrases": [
                        "вести себя беспутно",
                        "жить беспутно"
                    ],
                    "target_lang_definitions": {
                        "en": "Dissolutely, licentiously, violating norms of morality and behavior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dissolutely",
                                "target_lang_sense_clarification": "in a way that involves immoral or excessive behavior"
                            },
                            {
                                "target_lang_word": "wildly",
                                "target_lang_sense_clarification": "in an uncontrolled or unrestrained manner"
                            },
                            {
                                "target_lang_word": "licentiously",
                                "target_lang_sense_clarification": "lacking moral discipline, especially in sexual conduct"
                            }
                        ]
                    }
                }
            ]
        }
    ]

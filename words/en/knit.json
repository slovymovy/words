    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3b18bc69-1bd6-3d42-9cf8-0e85ff7d0d90",
                    "sense_definition": "To make clothing or fabric by interlocking loops of wool or yarn with needles or a machine.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "create_fabric",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother taught me how to <w>knit</w> a warm scarf.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка научила меня <w>вязать</w> теплый шарф."
                            }
                        },
                        {
                            "text": "She likes to <w>knit</w> socks for her family during the winter.",
                            "target_lang_translations": {
                                "ru": "Она любит <w>вязать</w> носки для своей семьи зимой."
                            }
                        }
                    ],
                    "antonyms": [
                        "unravel"
                    ],
                    "common_phrases": [
                        "knit a sweater",
                        "knit by hand",
                        "knit a scarf"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать одежду или ткань, переплетая петли шерсти или пряжи спицами или на машине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вязать",
                                "target_lang_sense_clarification": "изготавливать из пряжи"
                            },
                            {
                                "target_lang_word": "связать",
                                "target_lang_sense_clarification": "закончить вязание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98d1137b-c7dd-346f-9d89-9a0a63b3f872",
                    "sense_definition": "To join people or things closely and firmly together.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "unite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shared tragedy <w>knitted</w> the small community together.",
                            "target_lang_translations": {
                                "ru": "Общая трагедия <w>сплотила</w> маленькую общину."
                            }
                        },
                        {
                            "text": "Common interests can help to <w>knit</w> a group of strangers into a team.",
                            "target_lang_translations": {
                                "ru": "Общие интересы могут помочь <w>сплотить</w> группу незнакомцев в команду."
                            }
                        }
                    ],
                    "synonyms": [
                        "unite",
                        "bond",
                        "connect",
                        "join"
                    ],
                    "antonyms": [
                        "separate",
                        "divide"
                    ],
                    "common_phrases": [
                        "knit together",
                        "closely knit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тесно и прочно соединять людей или вещи вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сплачивать",
                                "target_lang_sense_clarification": "объединять людей"
                            },
                            {
                                "target_lang_word": "объединять",
                                "target_lang_sense_clarification": "соединять в целое"
                            },
                            {
                                "target_lang_word": "связывать",
                                "target_lang_sense_clarification": "соединять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc744fa7-6f45-30d3-a6ad-c192128a7107",
                    "sense_definition": "To grow back together and heal, especially referring to a broken bone.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "heal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor said that the broken bone would <w>knit</w> in about six weeks.",
                            "target_lang_translations": {
                                "ru": "Врач сказал, что сломанная кость <w>срастется</w> примерно через шесть недель."
                            }
                        },
                        {
                            "text": "It takes time for a serious fracture to <w>knit</w> completely.",
                            "target_lang_translations": {
                                "ru": "Требуется время, чтобы серьезный перелом полностью <w>сросся</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "heal",
                        "mend",
                        "fuse"
                    ],
                    "antonyms": [
                        "break",
                        "fracture"
                    ],
                    "common_phrases": [
                        "bones knit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Срастаться и заживать, особенно говоря о сломанной кости."
                    },
                    "translations": {

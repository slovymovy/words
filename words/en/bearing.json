    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9d540f65-7abf-3399-96f2-d8f5246a7236",
                    "sense_definition": "Relevance; a relationship or connection.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Personal feelings should have no <w>bearing</w> on your professional decisions.",
                            "target_lang_translations": {
                                "ru": "Личные чувства не должны иметь <w>отношения</w> к вашим профессиональным решениям."
                            }
                        },
                        {
                            "text": "The evidence provided has a direct <w>bearing</w> on the outcome of the trial.",
                            "target_lang_translations": {
                                "ru": "Представленные доказательства имеют прямое <w>отношение</w> к исходу судебного разбирательства."
                            }
                        }
                    ],
                    "synonyms": [
                        "relevance",
                        "connection",
                        "significance",
                        "relation"
                    ],
                    "antonyms": [
                        "irrelevance"
                    ],
                    "common_phrases": [
                        "have a bearing on",
                        "no bearing on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связь, отношение или влияние на что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отношение",
                                "target_lang_sense_clarification": "связь"
                            },
                            {
                                "target_lang_word": "влияние",
                                "target_lang_sense_clarification": "воздействие"
                            },
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "соединение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f0b45a7-b31e-31db-a939-37e51b51219b",
                    "sense_definition": "A mechanical device that supports another part or reduces friction.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mechanical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic had to replace a worn <w>bearing</w> in the engine.",
                            "target_lang_translations": {
                                "ru": "Механику пришлось заменить изношенный <w>подшипник</w> в двигателе."
                            }
                        },
                        {
                            "text": "Without a proper <w>bearing</w>, the wheel will not rotate smoothly.",
                            "target_lang_translations": {
                                "ru": "Без исправного <w>подшипника</w> колесо не будет вращаться плавно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ball bearing",
                        "roller bearing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Механическое устройство, поддерживающее вал или ось и уменьшающее трение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подшипник",
                                "target_lang_sense_clarification": "механизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0b3bc98-93dc-30a6-b675-be34dd9b9786",
                    "sense_definition": "One's understanding of one's orientation or relative position, literally or figuratively.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "orientation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need a moment to look at the map and get my <w>bearings</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужна минута, чтобы посмотреть на карту и определить свое <w>местоположение</w>."
                            }
                        },
                        {
                            "text": "It took him a few weeks to find his <w>bearings</w> in the new office environment.",
                            "target_lang_translations": {
                                "ru": "Ему потребовалось несколько недель, чтобы <w>освоиться</w> в новой офисной обстановке."
                            }
                        }
                    ],
                    "synonyms": [
                        "orientation",
                        "sense of direction"
                    ],
                    "common_phrases": [
                        "get one's bearings",
                        "find one's bearings",
                        "lose one's bearings"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Понимание своего местоположения или ориентация в пространстве (в прямом или переносном смысле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ориентация",
                                "target_lang_sense_clarification": "в пространстве"
                            },
                            {
                                "target_lang_word": "положение",
                                "target_lang_sense_clarification": "местонахождение"
                            },
                            {
                                "target_lang_word": "ориентиры",
                                "target_lang_sense_clarification": "точки отсчета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d859cbab-fca3-31e8-beeb-201c4797e914",
                    "sense_definition": "The horizontal angle between the direction of an object and another object, or between it and that of true north; a heading or direction.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "navigation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain checked the compass to determine the ship's <w>bearing</w>.",
                            "target_lang_translations": {
                                "ru": "Капитан сверился с компасом, чтобы определить <w>курс</w> корабля."
                            }
                        },
                        {
                            "text": "We took a magnetic <w>bearing</w> to ensure we were walking in the right direction.",
                            "target_lang_translations": {
                                "ru": "Мы определили магнитный <w>азимут</w>, чтобы убедиться, что идем в правильном направлении."
                            }
                        }
                    ],
                    "synonyms": [
                        "heading",
                        "direction",
                        "course"
                    ],
                    "common_phrases": [
                        "take a bearing",
                        "compass bearing",
                        "magnetic bearing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Угол между направлением на объект и меридианом; направление движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пеленг",
                                "target_lang_sense_clarification": "морской/авиационный термин"
                            },
                            {
                                "target_lang_word": "азимут",
                                "target_lang_sense_clarification": "геодезический угол"
                            },
                            {
                                "target_lang_word": "курс",
                                "target_lang_sense_clarification": "направление движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "789bbc4b-6ca4-3058-bfe7-88801c68ccc0",
                    "sense_definition": "One's posture, demeanor, or manner.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "demeanor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She walked into the room with a confident and regal <w>bearing</w>.",
                            "target_lang_translations": {
                                "ru": "Она вошла в комнату с уверенной и царственной <w>осанкой</w>."
                            }
                        },
                        {
                            "text": "His military <w>bearing</w> made him stand out among the civilians.",
                            "target_lang_translations": {
                                "ru": "Его военная <w>выправка</w> выделяла его среди гражданских."
                            }
                        }
                    ],
                    "synonyms": [
                        "posture",
                        "carriage",
                        "demeanor",
                        "deportment",
                        "manner"
                    ],
                    "common_phrases": [
                        "regal bearing",
                        "military bearing",
                        "proud bearing"

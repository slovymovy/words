                            },
                            {
                                "target_lang_word": "подсчитывать",
                                "target_lang_sense_clarification": "вести подсчет"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "count",
                                "target_lang_sense_clarification": "determine quantity"
                            }
                        ]

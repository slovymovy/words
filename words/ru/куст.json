    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "daa15191-3dc8-3560-a018-f4ddddd4d88b",
                    "sense_definition": "Растение с древовидными ветвями, не имеющее одного центрального ствола.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "plant_woody",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашем саду расцвёл красивый <w>куст</w> белой сирени.",
                            "target_lang_translations": {
                                "en": "A beautiful white lilac <w>bush</w> blossomed in our garden."
                            }
                        },
                        {
                            "text": "Маленький заяц испугался шума и быстро спрятался под <w>куст</w>.",
                            "target_lang_translations": {
                                "en": "The little hare was frightened by the noise and quickly hid under a <w>bush</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "кустарник"
                    ],
                    "antonyms": [
                        "дерево",
                        "трава"
                    ],
                    "common_phrases": [
                        "куст сирени",
                        "густой куст",
                        "спрятаться за куст",
                        "под кустом"
                    ],
                    "target_lang_definitions": {
                        "en": "A plant with woody branches that does not have a single central trunk."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bush",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "shrub",
                                "target_lang_sense_clarification": "botanical or formal term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ab38379-bded-327e-a2dd-f0a5c07cb225",
                    "sense_definition": "Травянистое растение, у которого множество стеблей растёт из одного корня.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "plant_herbaceous",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Осенью мы выкопали на огороде последний <w>куст</w> картофеля.",
                            "target_lang_translations": {
                                "en": "In autumn, we dug up the last potato <w>plant</w> in the vegetable garden."
                            }
                        },
                        {
                            "text": "На грядке покраснел первый <w>куст</w> сладкой клубники.",
                            "target_lang_translations": {
                                "en": "The first sweet strawberry <w>plant</w> turned red in the garden bed."
                            }
                        }
                    ],
                    "common_phrases": [
                        "куст картофеля",
                        "куст клубники",
                        "куст помидоров"
                    ],
                    "target_lang_definitions": {
                        "en": "A herbaceous plant, usually a crop, with multiple stems growing from a single root."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "plant",
                                "target_lang_sense_clarification": "for crops like potatoes, strawberries, tomatoes"
                            },
                            {
                                "target_lang_word": "bush",
                                "target_lang_sense_clarification": "for berry plants like raspberries"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4078bff1-01b2-3956-ac1e-50b830f8ea16",
                    "sense_definition": "Группа скважин, расположенных на одной площадке (в нефтегазовой промышленности).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "industry_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Инженеры приехали проверить новый <w>куст</w> скважин на месторождении.",
                            "target_lang_translations": {
                                "en": "Engineers arrived to check the new <w>cluster</w> of wells at the field."
                            }
                        },
                        {
                            "text": "Эта <w>кустовая площадка</w> обеспечивает добычу нефти для всего района.",
                            "target_lang_translations": {
                                "en": "This <w>well pad</w> ensures oil production for the entire district."
                            }
                        }
                    ],
                    "synonyms": [
                        "кустовая площадка"
                    ],
                    "common_phrases": [
                        "нефтяной куст",
                        "бурение кустов",
                        "куст скважин"

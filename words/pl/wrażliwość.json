            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bf465bf1-421e-3da5-8154-73aac388726d",
                    "sense_definition": "Zdolność do intensywnego przeżywania emocji, dostrzegania subtelności i reagowania na otoczenie; bycie uczuciowym.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotions_personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego artystyczna <w>wrażliwość</w> pozwala mu tworzyć niezwykle poruszające obrazy.",
                            "target_lang_translations": {
                                "ru": "Его художественная <w>чувствительность</w> позволяет ему создавать невероятно трогательные картины."
                            }
                        },
                        {
                            "text": "Musisz wykazać się dużą <w>wrażliwością</w>, rozmawiając z kimś po tak trudnych przeżyciach.",
                            "target_lang_translations": {
                                "ru": "Ты должен проявить большую <w>чуткость</w>, разговаривая с кем-то после таких тяжелых переживаний."
                            }
                        }
                    ],
                    "synonyms": [
                        "czułość",
                        "delikatność"
                    ],
                    "antonyms": [
                        "niewrażliwość"
                    ],
                    "common_phrases": [
                        "wrażliwość na piękno",
                        "wrażliwość artystyczna",
                        "duża wrażliwość",
                        "wrażliwość społeczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность глубоко переживать эмоции, замечать тонкости и реагировать на окружение; эмоциональность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствительность",
                                "target_lang_sense_clarification": "эмоциональная"
                            },
                            {
                                "target_lang_word": "чуткость",
                                "target_lang_sense_clarification": "эмпатия, деликатность"
                            },
                            {
                                "target_lang_word": "впечатлительность",
                                "target_lang_sense_clarification": "острая реакция на впечатления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d462003f-936b-304c-b935-f038354d39b1",
                    "sense_definition": "Zdolność organizmu, narządu lub zmysłu do reagowania na bodźce zewnętrzne, takie jak dotyk, światło czy temperatura.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "biology_senses",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po zabiegu dentystycznym pacjent może odczuwać zwiększoną <w>wrażliwość</w> na zimne napoje.",
                            "target_lang_translations": {
                                "ru": "После стоматологической процедуры пациент может ощущать повышенную <w>чувствительность</w> к холодным напиткам."
                            }
                        },
                        {
                            "text": "Niektóre zwierzęta nocne mają wyjątkową <w>wrażliwość</w> na światło.",
                            "target_lang_translations": {
                                "ru": "Некоторые ночные животные обладают исключительной <w>чувствительностью</w> к свету."
                            }
                        }
                    ],
                    "antonyms": [
                        "niewrażliwość"
                    ],
                    "common_phrases": [
                        "wrażliwość na ból",
                        "wrażliwość na światło",
                        "próg wrażliwości",
                        "wrażliwość dotykowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность организма, органа или чувства реагировать на внешние раздражители, такие как прикосновение, свет или температура."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствительность",
                                "target_lang_sense_clarification": "физиологическая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0dc5bd80-edc9-3412-a8c1-ed1191975bef",
                    "sense_definition": "Brak odporności na działanie czynników szkodliwych; podatność na zniszczenie, uszkodzenie lub chorobę.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "vulnerability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Główną wadą tego urządzenia jest jego duża <w>wrażliwość</w> na wilgoć i kurz.",
                            "target_lang_translations": {
                                "ru": "Главным недостатком этого устройства является его высокая <w>чувствительность</w> к влаге и пыли."
                            }
                        },
                        {
                            "text": "Lekarze badają <w>wrażliwość</w> bakterii na różne rodzaje antybiotyków.",
                            "target_lang_translations": {
                                "ru": "Врачи исследуют <w>чувствительность</w> бактерий к различным видам антибиотиков."
                            }
                        }
                    ],
                    "antonyms": [
                        "odporność",
                        "wytrzymałość"
                    ],
                    "common_phrases": [
                        "wrażliwość na wilgoć",
                        "wrażliwość na korozję",
                        "wrażliwość na leki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствие устойчивости к вредным факторам; подверженность повреждениям, влиянию или болезням."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствительность",
                                "target_lang_sense_clarification": "реакция на воздействие"
                            },
                            {
                                "target_lang_word": "восприимчивость",
                                "target_lang_sense_clarification": "подверженность"
                            }
                        ]
                    }
                }
            ]
        }

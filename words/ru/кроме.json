{
    "word_family": [
        "кромка",
        "укромный",
        "кромешный"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "b52bd049-d224-39b7-a520-0368768acc9b",
                    "sense_definition": "Указывает на исключение кого-либо или чего-либо из числа упомянутых; за исключением, не считая.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "exclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В комнате никого не было, <w>кроме</w> моего старого друга.",
                            "target_lang_translations": {
                                "en": "There was no one in the room <w>except</w> my old friend."
                            }
                        },
                        {
                            "text": "Я ничего не слышал, <w>кроме</w> шума листьев за окном.",
                            "target_lang_translations": {
                                "en": "I heard nothing <w>but</w> the rustling of leaves outside the window."
                            }
                        }
                    ],
                    "synonyms": [
                        "за исключением",
                        "не считая",
                        "помимо"
                    ],
                    "common_phrases": [
                        "кроме шуток",
                        "ничего кроме",
                        "все кроме"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the exclusion of someone or something from a group or statement; except, apart from."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "except",
                                "target_lang_sense_clarification": "excluding; not including"
                            },
                            {
                                "target_lang_word": "but",
                                "target_lang_sense_clarification": "used after negatives or universals (e.g., 'nothing but', 'everyone but')"
                            },
                            {
                                "target_lang_word": "apart from",
                                "target_lang_sense_clarification": "with the exception of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f31b32b-672d-31e2-9e09-9766bb46eedf",
                    "sense_definition": "Указывает на включение, добавление кого-либо или чего-либо; вдобавок к, наряду с.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "inclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Кроме</w> тетрадей, я купил в магазине ещё и новые ручки.",
                            "target_lang_translations": {
                                "en": "<w>Besides</w> notebooks, I also bought new pens at the store."
                            }
                        },
                        {
                            "text": "<w>Кроме</w> того, нам нужно обсудить планы на завтрашний день.",
                            "target_lang_translations": {
                                "en": "<w>Besides</w> that, we need to discuss plans for tomorrow."
                            }
                        }
                    ],
                    "synonyms": [
                        "помимо",
                        "сверх",
                        "наряду"
                    ],
                    "common_phrases": [
                        "кроме того",
                        "кроме этого"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the inclusion or addition of someone or something; in addition to, besides."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "besides",
                                "target_lang_sense_clarification": "in addition to; as well as"
                            },
                            {
                                "target_lang_word": "in addition to",
                                "target_lang_sense_clarification": "along with; as an extra factor"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
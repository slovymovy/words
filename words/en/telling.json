{
    "word_family": [
        "teller",
        "retelling",
        "foretell",
        "tale"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c67b8da2-d0cc-3e23-9b06-4383f01accbc",
                    "sense_definition": "Revealing hidden information, feelings, or the truth about a situation; bearing significance.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "adj_revealing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her silence was a <w>telling</w> sign that she disagreed with the plan.",
                            "target_lang_translations": {
                                "ru": "Её молчание было <w>красноречивым</w> знаком того, что она не согласна с планом."
                            }
                        },
                        {
                            "text": "The statistics provide a <w>telling</w> insight into the habits of consumers.",
                            "target_lang_translations": {
                                "ru": "Статистика дает <w>показательное</w> представление о привычках потребителей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "a telling sign",
                        "a telling example",
                        "telling detail"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскрывающий скрытую информацию или истину; многозначительный, показательный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красноречивый",
                                "target_lang_sense_clarification": "о знаке, взгляде или детали"
                            },
                            {
                                "target_lang_word": "показательный",
                                "target_lang_sense_clarification": "о примере или статистике"
                            },
                            {
                                "target_lang_word": "говорящий",
                                "target_lang_sense_clarification": "раскрывающий суть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62483819-e52b-3770-8df3-93b8c98efe56",
                    "sense_definition": "Having a strong force or a marked effect; effective and weighty.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_effective",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer made a <w>telling</w> argument that convinced the jury.",
                            "target_lang_translations": {
                                "ru": "Адвокат привел <w>веский</w> довод, который убедил присяжных."
                            }
                        },
                        {
                            "text": "He struck a <w>telling</w> blow against his opponent in the debate.",
                            "target_lang_translations": {
                                "ru": "Он нанес <w>ощутимый</w> удар своему оппоненту в дебатах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "telling blow",
                        "telling effect"
                    ],
                    "target_lang_definitions": {
                        "ru": "Производящий сильное впечатление или имеющий заметный эффект; веский."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "веский",
                                "target_lang_sense_clarification": "об аргументе или доводе"
                            },
                            {
                                "target_lang_word": "ощутимый",
                                "target_lang_sense_clarification": "об ударе или эффекте"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "583c0c0f-861c-34b6-bd58-5b712296c763",
                    "sense_definition": "The present participle and gerund of <w>tell</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>telling</w> the children a bedtime story.",
                            "target_lang_translations": {
                                "ru": "Она <w>рассказывает</w> детям сказку на ночь."
                            }
                        },
                        {
                            "text": "Thank you for <w>telling</w> me the truth about what happened.",
                            "target_lang_translations": {
                                "ru": "Спасибо, что <w>сказал</w> мне правду о том, что случилось."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола tell (говорить, рассказывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассказывать",
                                "target_lang_sense_clarification": "истории, сказки"
                            },
                            {
                                "target_lang_word": "говорить",
                                "target_lang_sense_clarification": "сообщать информацию, правду"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "12717d94-55bc-39f6-953e-7f0f17c6abb6",
                    "sense_definition": "The ability to determine or know something (almost exclusively used in the phrase 'there is no telling').",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_knowing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is no <w>telling</w> when the rain will stop.",
                            "target_lang_translations": {
                                "ru": "Невозможно <w>сказать</w>, когда прекратится дождь."
                            }
                        },
                        {
                            "text": "With the market so volatile, there is no <w>telling</w> what the price will be tomorrow.",
                            "target_lang_translations": {
                                "ru": "При такой нестабильности рынка невозможно <w>предсказать</w>, какой будет цена завтра."
                            }
                        }
                    ],
                    "common_phrases": [
                        "no telling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность определить или узнать что-либо (обычно используется в фразе 'there is no telling')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сказать",
                                "target_lang_sense_clarification": "в значении 'невозможно определить'"
                            },
                            {
                                "target_lang_word": "предсказать",
                                "target_lang_sense_clarification": "о будущем событии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78810789-0efc-3df0-b529-923d5d0d4c76",
                    "sense_definition": "The act of narrating a story or disclosing information.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_narration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>telling</w> of the ancient legend took the whole evening.",
                            "target_lang_translations": {
                                "ru": "<w>Рассказ</w> древней легенды занял весь вечер."
                            }
                        },
                        {
                            "text": "The story improved greatly in the <w>telling</w>.",
                            "target_lang_translations": {
                                "ru": "История стала намного лучше при <w>пересказе</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "story telling",
                        "fortune telling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс рассказывания истории или сообщения информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассказ",
                                "target_lang_sense_clarification": "действие или результат"
                            },
                            {
                                "target_lang_word": "повествование",
                                "target_lang_sense_clarification": "литературный или формальный стиль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15db1e0d-2a01-32c2-9ea5-332511fdb85f",
                    "sense_definition": "The act of counting or numbering items.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_counting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shepherd finished the <w>telling</w> of his sheep before sunset.",
                            "target_lang_translations": {
                                "ru": "Пастух закончил <w>подсчет</w> своих овец до заката."
                            }
                        },
                        {
                            "text": "By the <w>telling</w> of the votes, the new chairman was elected.",
                            "target_lang_translations": {
                                "ru": "По <w>подсчету</w> голосов был избран новый председатель."
                            }
                        }
                    ],
                    "synonyms": [
                        "enumeration",
                        "counting"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт подсчета или перечисления предметов (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подсчет",
                                "target_lang_sense_clarification": "пересчитывание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
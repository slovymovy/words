        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5b00a7db-a220-30b4-aa91-a7b0b6b7b0d6",
                    "sense_definition": "Een voorwerp met een glad oppervlak dat licht weerkaatst en waarin men een beeld van zichzelf of de omgeving kan zien.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "reflection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kijk even in de <w>spiegel</w> om te zien of je haar goed zit.",
                            "target_lang_translations": {
                                "en": "Just look in the <w>mirror</w> to see if your hair looks good.",
                                "ru": "Посмотри в <w>зеркало</w>, чтобы проверить, в порядке ли прическа."
                            }
                        },
                        {
                            "text": "De tandarts gebruikt een klein <w>spiegel</w>tje om naar mijn kiezen te kijken.",
                            "target_lang_translations": {
                                "en": "The dentist uses a small <w>mirror</w> to look at my molars.",
                                "ru": "Стоматолог использует маленькое <w>зеркальце</w>, чтобы осмотреть мои зубы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de spiegel kijken",
                        "een spiegel voorhouden"
                    ],
                    "target_lang_definitions": {
                        "en": "An object with a smooth surface that reflects light and in which one can see an image of oneself or the surroundings.",
                        "ru": "Предмет с гладкой поверхностью, отражающий свет, в котором можно увидеть свое изображение или окружающую обстановку."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mirror",
                                "target_lang_sense_clarification": "reflecting object"
                            },
                            {
                                "target_lang_word": "looking-glass",
                                "target_lang_sense_clarification": "dated or literary"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "зеркало",
                                "target_lang_sense_clarification": "предмет обихода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1084b838-ba55-39e0-bd60-02b5c6f89990",
                    "sense_definition": "De concentratie van een bepaalde stof in een vloeistof, meestal in het bloed.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "concentration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts controleert de <w>spiegel</w> van het medicijn in het bloed.",
                            "target_lang_translations": {
                                "en": "The doctor checks the <w>level</w> of the medicine in the blood.",
                                "ru": "Врач проверяет <w>уровень</w> лекарства в крови."
                            }
                        },
                        {
                            "text": "Door het dieet daalde zijn suiker<w>spiegel</w> aanzienlijk.",
                            "target_lang_translations": {
                                "en": "Due to the diet, his sugar <w>level</w> dropped significantly.",
                                "ru": "Благодаря диете его <w>уровень</w> сахара значительно снизился."
                            }
                        }
                    ],
                    "synonyms": [
                        "concentratie",
                        "gehalte"
                    ],
                    "common_phrases": [
                        "bloedspiegel"
                    ],
                    "target_lang_definitions": {
                        "en": "The concentration of a certain substance in a liquid, usually in the blood.",
                        "ru": "Концентрация определенного вещества в жидкости, обычно в крови."
                    },
                    "translations": {

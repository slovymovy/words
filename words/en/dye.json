    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "034e3b6f-c8ba-3555-9454-cb0ea23891df",
                    "sense_definition": "To change the colour of something, especially hair or cloth, using a special liquid or substance.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You look different. Have you had your hair <w>dyed</w>?",
                            "target_lang_translations": {
                                "ru": "Ты выглядишь иначе. Ты <w>покрасила</w> волосы?"
                            }
                        },
                        {
                            "text": "She decided to <w>dye</w> her white dress a deep blue for the wedding.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>покрасить</w> своё белое платье в тёмно-синий цвет к свадьбе."
                            }
                        }
                    ],
                    "synonyms": [
                        "tint",
                        "stain"
                    ],
                    "common_phrases": [
                        "dye hair",
                        "dye fabric",
                        "dye clothes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменять цвет чего-либо, особенно волос или ткани, с помощью специальной жидкости или вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красить",
                                "target_lang_sense_clarification": "изменять цвет"
                            },
                            {
                                "target_lang_word": "окрашивать",
                                "target_lang_sense_clarification": "придавать цвет"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1f03d96b-c86b-3a18-ba5c-cc6da78f926b",
                    "sense_definition": "A substance used to change the colour of something, such as hair, textile fibers, or food.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a box of permanent hair <w>dye</w> at the pharmacy.",
                            "target_lang_translations": {
                                "ru": "Она купила коробку стойкой <w>краски</w> для волос в аптеке."
                            }
                        },
                        {
                            "text": "The factory uses a natural vegetable <w>dye</w> to color the cotton shirts.",
                            "target_lang_translations": {
                                "ru": "Фабрика использует натуральный растительный <w>краситель</w> для окрашивания хлопковых рубашек."
                            }
                        }
                    ],
                    "synonyms": [
                        "colourant",
                        "stain"
                    ],
                    "common_phrases": [
                        "hair dye",
                        "natural dye",
                        "vegetable dye",
                        "red dye"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество, используемое для изменения цвета чего-либо, например волос, текстильных волокон или еды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "краска",
                                "target_lang_sense_clarification": "вещество для окрашивания"
                            },
                            {
                                "target_lang_word": "краситель",
                                "target_lang_sense_clarification": "химическое вещество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "625ba081-a8cf-398c-9dd0-a24e496b9066",
                    "sense_definition": "Any hue or color produced by or resembling a colouring substance.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sunset cast a brilliant crimson <w>dye</w> across the western sky.",
                            "target_lang_translations": {
                                "ru": "Закат окрасил западную часть неба в ярко-малиновый <w>цвет</w>."
                            }
                        },
                        {
                            "text": "The autumn leaves took on a golden <w>dye</w> before falling.",
                            "target_lang_translations": {
                                "ru": "Осенние листья приобрели золотистый <w>оттенок</w> перед тем, как опасть."
                            }

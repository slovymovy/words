            "senses": [
                {
                    "sense_id": "8a6f1c43-0487-3b1a-8744-ac556d0c1a2c",
                    "sense_definition": "An Eastern European grandmother or elderly woman.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>babushka</w> taught me how to make traditional dumplings for dinner.",
                            "target_lang_translations": {
                                "ru": "Моя <w>бабушка</w> научила меня готовить традиционные пельмени на ужин."
                            }
                        },
                        {
                            "text": "We saw an old <w>babushka</w> sitting on the bench feeding the pigeons.",
                            "target_lang_translations": {
                                "ru": "Мы видели старую <w>бабушку</w>, сидящую на скамейке и кормящую голубей."
                            }
                        }
                    ],
                    "synonyms": [
                        "baba",
                        "babulya"
                    ],
                    "common_phrases": [
                        "old babushka",
                        "my babushka"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in Eastern European contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бабушка или пожилая женщина из Восточной Европы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бабушка",
                                "target_lang_sense_clarification": "мать одного из родителей; пожилая женщина"
                            },
                            {
                                "target_lang_word": "старушка",
                                "target_lang_sense_clarification": "ласковое название пожилой женщины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "402e88cd-b273-3cf3-9458-ed3b82e32a6d",
                    "sense_definition": "A headscarf tied under the chin, traditionally worn by Eastern European women.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She tied a silk <w>babushka</w> under her chin to keep her hair tidy in the wind.",
                            "target_lang_translations": {
                                "ru": "Она повязала шёлковый <w>платок</w> под подбородком, чтобы ветер не растрепал волосы."
                            }
                        },
                        {
                            "text": "The peasant woman wore a floral <w>babushka</w> while working in the garden.",
                            "target_lang_translations": {
                                "ru": "Крестьянка носила цветочный <w>платок</w>, работая в саду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wear a babushka",
                        "floral babushka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "describes a specific style of wearing a scarf"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Головной платок, завязанный под подбородком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платок",
                                "target_lang_sense_clarification": "кусок ткани для повязывания на голову"
                            },
                            {
                                "target_lang_word": "косынка",
                                "target_lang_sense_clarification": "треугольный головной платок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f56eb0b8-a652-3529-85a2-85e774bb9a34",
                    "sense_definition": "A Russian nesting doll, also known as a matryoshka.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tourist bought a painted <w>babushka</w> to take home as a souvenir.",
                            "target_lang_translations": {
                                "ru": "Турист купил расписную <w>матрёшку</w>, чтобы увезти домой в качестве сувенира."
                            }
                        },
                        {
                            "text": "Inside the largest <w>babushka</w> was a slightly smaller wooden figure.",
                            "target_lang_translations": {
                                "ru": "Внутри самой большой <w>матрёшки</w> была деревянная фигурка чуть меньшего размера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wooden babushka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Русская деревянная кукла, вкладывающаяся одна в другую; матрёшка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "матрёшка",
                                "target_lang_sense_clarification": "деревянная расписная кукла"
                            }
                        ]
                    }
                }
            ]
        }
    ]

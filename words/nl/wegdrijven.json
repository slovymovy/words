            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cf37bbb5-0516-3a41-961a-75459790f32a",
                    "sense_definition": "Door wind of waterstroming langzaam van een plaats weggaan zonder zelf te sturen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zonder anker zal de boot langzaam <w>wegdrijven</w>.",
                            "target_lang_translations": {
                                "en": "Without an anchor, the boat will slowly <w>drift away</w>.",
                                "ru": "Без якоря лодка будет медленно <w>уплывать</w>."
                            }
                        },
                        {
                            "text": "We zagen de kleurrijke ballon hoog in de lucht <w>wegdrijven</w>.",
                            "target_lang_translations": {
                                "en": "We saw the colorful balloon <w>float away</w> high in the sky.",
                                "ru": "Мы видели, как разноцветный шар <w>уплывал</w> высоко в небо."
                            }
                        }
                    ],
                    "synonyms": [
                        "afdrijven",
                        "wegdobberen"
                    ],
                    "common_phrases": [
                        "langzaam wegdrijven",
                        "met de stroom wegdrijven",
                        "op zee wegdrijven"
                    ],
                    "target_lang_definitions": {
                        "en": "To slowly move away from a place carried by wind or water currents without steering.",
                        "ru": "Медленно удаляться с места под воздействием ветра или течения, не управляя движением."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drift away",
                                "target_lang_sense_clarification": "movement by current or wind"
                            },
                            {
                                "target_lang_word": "float away",
                                "target_lang_sense_clarification": "movement on surface or in air"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уплывать",
                                "target_lang_sense_clarification": "удаляться по воде или плавно по воздуху"
                            },
                            {
                                "target_lang_word": "дрейфовать",
                                "target_lang_sense_clarification": "перемещаться по ветру или течению (о судне)"
                            },
                            {
                                "target_lang_word": "улетать",
                                "target_lang_sense_clarification": "удаляться по воздуху"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4be2ea28-f39c-3d20-9a4f-71847c1aa601",
                    "sense_definition": "Iemand of iets dwingen om een plaats te verlaten, vaak door kracht of invloed.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer moest de koeien uit de verkeerde wei <w>wegdrijven</w>.",
                            "target_lang_translations": {
                                "en": "The farmer had to <w>drive away</w> the cows from the wrong pasture.",
                                "ru": "Фермеру пришлось <w>выгонять</w> коров с чужого пастбища."
                            }
                        },
                        {
                            "text": "De sterke wind zal de donkere wolken snel <w>wegdrijven</w>.",
                            "target_lang_translations": {
                                "en": "The strong wind will quickly <w>drive away</w> the dark clouds.",
                                "ru": "Сильный ветер быстро <w>разгонит</w> темные тучи."
                            }
                        }
                    ],
                    "synonyms": [
                        "verjagen",
                        "verdrijven"
                    ],
                    "common_phrases": [
                        "de kudde wegdrijven",
                        "wolken wegdrijven",
                        "vijanden wegdrijven"
                    ],
                    "target_lang_definitions": {
                        "en": "To force someone or something to leave a place, often by force or influence.",
                        "ru": "Заставлять кого-то или что-то покинуть место, часто применяя силу или воздействие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drive away",
                                "target_lang_sense_clarification": "force to leave"
                            },
                            {
                                "target_lang_word": "chase away",
                                "target_lang_sense_clarification": "force to flee"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отгонять",
                                "target_lang_sense_clarification": "заставлять отойти (животных, людей)"
                            },
                            {
                                "target_lang_word": "разгонять",
                                "target_lang_sense_clarification": "заставлять рассеяться (облака, толпу)"
                            },
                            {
                                "target_lang_word": "прогонять",
                                "target_lang_sense_clarification": "вынуждать уйти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46de5d4c-0668-32e0-bf9f-dad80cd82cb2",
                    "sense_definition": "Geleidelijk minder worden en verdwijnen, vaak gezegd van gevoelens, gedachten of spanning.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het goede gesprek voelde hij zijn angst langzaam <w>wegdrijven</w>.",
                            "target_lang_translations": {
                                "en": "After the good conversation, he felt his fear slowly <w>drift away</w>.",
                                "ru": "После хорошего разговора он почувствовал, как его страх медленно <w>уходит</w>."
                            }
                        },
                        {
                            "text": "Terwijl ik naar de muziek luisterde, liet ik mijn gedachten <w>wegdrijven</w>.",
                            "target_lang_translations": {
                                "en": "While listening to the music, I let my thoughts <w>drift away</w>.",
                                "ru": "Слушая музыку, я позволил своим мыслям <w>унестись</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "vervagen",
                        "verdwijnen",
                        "oplossen"
                    ],
                    "common_phrases": [
                        "gedachten laten wegdrijven",
                        "zorgen wegdrijven",
                        "pijn wegdrijven"
                    ],
                    "target_lang_definitions": {
                        "en": "To gradually diminish and disappear, often used for feelings, thoughts, or tension.",
                        "ru": "Постепенно ослабевать и исчезать (часто о чувствах, мыслях или напряжении)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drift away",
                                "target_lang_sense_clarification": "figurative disappearance"
                            },
                            {
                                "target_lang_word": "fade away",
                                "target_lang_sense_clarification": "gradual disappearance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рассеиваться",
                                "target_lang_sense_clarification": "исчезать, распадаться"
                            },
                            {
                                "target_lang_word": "улетучиваться",
                                "target_lang_sense_clarification": "быстро или незаметно исчезать"
                            },
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "покидать, переставать ощущаться"
                            }
                        ]
                    }
                }
            ]
        }
